mcmipxSysFilterGroup    OBJECT IDENTIFIER ::= {mcmfilter 19}

mcmipxSysRipFilter OBJECT-TYPE
        SYNTAX   INTEGER {
          inclusive (1),
          exclusive (2)
	  }
        ACCESS   read-only
        STATUS   mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = Indicates whether the ipx rip filter \
                  is inclusive or exclusive;
	   HELP = ;
	   CAPABILITIES = NET_CFG, VPN_CFG;"
         ::= { mcmipxSysFilterGroup 1 }

mcmipxSysSapFilter OBJECT-TYPE
        SYNTAX   INTEGER {
          inclusive (1),
          exclusive (2)
	  }
        ACCESS   read-only
        STATUS   mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = Indicates whether the ipx sap filter \
                  is inclusive or exclusive;
	   HELP = ;
	   CAPABILITIES = NET_CFG, VPN_CFG;"
         ::= { mcmipxSysFilterGroup 2 }

mcmipxSysNetFilter OBJECT-TYPE
        SYNTAX   INTEGER {
          inclusive (1),
          exclusive (2)
	  }
        ACCESS   read-only
        STATUS   mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = Indicates whether the ipx net filter \
                  is inclusive or exclusive;
	   HELP = ;
	   CAPABILITIES = NET_CFG, VPN_CFG;"
         ::= { mcmipxSysFilterGroup 3 }


