	SYNTAX  INTEGER (1..255)
	ACCESS  read-write
	STATUS  mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = The protocol type associated with the \
                  filter entry.;
           HELP = If the protocol type in the IP packet matches \
                  this together with other patterns in the filter \
                  entry then the IP packet is filtered.;
                  packet is filtered;
	   CAPABILITIES = ;"
              ::= {nvmIpFwdPortOutFltrEntry 4}

nvmIpFwdPortOutFltrPortId OBJECT-TYPE
	SYNTAX  INTEGER (1..65535)
	ACCESS  read-write
	STATUS  mandatory
	DESCRIPTION
	  "NAME = ;
           DESC = The TCP/UDP port number associated with the \
                  filter entry.;
           HELP = If the destination TCP/UDP port number in the \
                  IP packet matches this together w/ other patterns \
                  in the filter entry then the IP pkt is filtered.;
	   CAPABILITIES = ;"
              ::= {nvmIpFwdPortOutFltrEntry 5}

