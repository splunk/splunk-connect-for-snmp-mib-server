                 NetNumber,
    	     mcmipxNetFltrSapMask
                 SapType,
             mcmipxNetFltrPort
                 INTEGER,
             mcmipxNetFltrDir
                 INTEGER
         }

mcmipxNetFltrSysInstance OBJECT-TYPE
        SYNTAX   INTEGER
        ACCESS   read-only
        STATUS   obsolete
	DESCRIPTION
	  "NAME = ;
	   DESC = ** OBSOLETE **;
