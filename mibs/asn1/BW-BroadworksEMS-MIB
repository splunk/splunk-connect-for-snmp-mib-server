BW-BroadworksEMS-MIB	DEFINITIONS ::= BEGIN
	IMPORTS                                                                 
		Unsigned32, Integer32, enterprises, OBJECT-TYPE, IpAddress, TimeTicks, Gauge32, NOTIFICATION-TYPE
			FROM SNMPv2-SMI
		DisplayString, DateAndTime, TruthValue
			FROM SNMPv2-TC
		enterprises
			FROM RFC1155-SMI
		DisplayString
			FROM RFC1213-MIB;
			
	RowStatus ::= INTEGER 
		{ active ( 1 ) , 
		  notInService ( 2 ) , 
		  notReady ( 3 ) , 
		  createAndGo ( 4 ) , 
		  createAndWait ( 5 ) , 
		  destroy ( 6 ) }

	org	OBJECT IDENTIFIER
