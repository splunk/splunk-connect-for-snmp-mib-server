           DESC = The DNA for the destination of \
                  the virtual circuit.;
           HELP = ;
	   CAPABILITIES = ;"
	::= {mcmFrLANSpvcSVCEntry 3}

mcmFrLANSpvcSVCDLCI OBJECT-TYPE
	SYNTAX	INTEGER (16..991)
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION
	  "NAME = ;
           DESC = The local DLCI for this SVC. \
                  Valid only when the SVC is \
                  established. ;
           HELP = ;
	   CAPABILITIES = ;"
	::= {mcmFrLANSpvcSVCEntry 4}

mcmFrLANSpvcSVCMaxTxSize OBJECT-TYPE
	SYNTAX	INTEGER (0..4096)
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION
	  "NAME = ;
           DESC = The maximum transmission frame size. \
                  This should be equal to (or greater \
                  than) the Rx size for the corresponding \
                  PVC. Expressed in bytes.; 
           HELP = ;
	   CAPABILITIES = ;"
	::= {mcmFrLANSpvcSVCEntry 5}

mcmFrLANSpvcSVCMaxRxSize OBJECT-TYPE
	SYNTAX	INTEGER (0..4096)
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION
	  "NAME = ;
           DESC = The maximum receivable frame size. \
                  This should be equal to (or less than) \
                  the Tx size for the corresponding PVC. \
                  Expressed in bytes.;
           HELP = ;
	   CAPABILITIES = ;"
	::= {mcmFrLANSpvcSVCEntry 6}

mcmFrLANSpvcSVCMinTxThroughput OBJECT-TYPE
	SYNTAX	INTEGER (0..2560000)
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION
	  "NAME = ;
           DESC = The minimum transmission throughput. \
                  This should be equal to (or greater than) \
                  the Rx CIR for the corresponding PVC. \
                  Expressed in bits/sec.;
           HELP = ;
	   CAPABILITIES = ;"
	::= {mcmFrLANSpvcSVCEntry 7}

