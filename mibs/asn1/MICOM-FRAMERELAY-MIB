	SYNTAX	INTEGER (0..2560000)
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION
	  "NAME = ;
           DESC = The minimum receive throughput.\
                  This should be equal to (or less \
                  than) the Tx CIR for the corresponding \
                  PVC. Expressed in bits/sec.
           HELP = ;
	   CAPABILITIES = ;"
	::= {mcmFrLANSpvcSVCEntry 8}

