        SYNTAX   McmFrDlcmiSVCStatusEntry
        ACCESS   not-accessible
        STATUS   mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = A row or entry in the Frame Relay Dlcmi SVC \
		  status Table. (Operational);
	   HELP = ;
	   CAPABILITIES = ;"
           INDEX { mcmFrDlcmiSVCStatusIfIndex, mcmFrDlcmiSVCStatusDlci }
