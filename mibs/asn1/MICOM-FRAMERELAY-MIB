        SYNTAX   INTEGER (16..991)
        ACCESS   read-only
        STATUS   mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = The local DLCI for this SVC, valid \
		  only when the connection has been \
		  established;
	   HELP = ;
	   CAPABILITIES = ;"
            REFERENCE
               "Draft American National  Standard  T1.618-1991,
               Section 3.3.6"
           ::= { mcmFrAllSVCCircuitEntry 2 }


mcmFrAllSVCCircuitType OBJECT-TYPE
        SYNTAX   INTEGER {
		rfc1490 (1),
		voice (2),
		switched (3),
		rfc1490switched (4)
		}
        ACCESS   read-only
        STATUS   mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = The type of this SVC.;
	   HELP = Valid types include voice, switched or \
		  rfc1490;
	   CAPABILITIES = ;"
::= { mcmFrAllSVCCircuitEntry 3 }


mcmFrAllSVCCircuitDNA OBJECT-TYPE
        SYNTAX   OCTET STRING (SIZE(1..34))
        ACCESS   read-only
        STATUS   mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = DNA of the destination node associated \
		  with the SVC;
	   HELP = ;
	   CAPABILITIES = ;"
            ::= { mcmFrAllSVCCircuitEntry 4 }


mcmFrAllSVCCircuitState OBJECT-TYPE
        SYNTAX   INTEGER {
                 invalid (1),
                 active (2),
                 inactive (3)
                      }
        ACCESS   read-only
        STATUS   mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = Indication for the state of this SVC (active \
		  or inactive).;
	   HELP = Only the configured SVCs \
		  can be in the inactive state (all learned \
		  SVCs will be deleted when the connection \
		  has been released);
	   CAPABILITIES = ;"
            DEFVAL { active }
            ::= { mcmFrAllSVCCircuitEntry 5 }


mcmFrAllSVCCircuitReceivedFECNs OBJECT-TYPE
        SYNTAX   Counter
        ACCESS   read-only
        STATUS   mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = Number of frames received from \
		  the network indicating forward \
		  congestion since this switched \
		  virtual circuit was created;
	   HELP = Number of Frame Relay Packets \
		  received on the corresponding \
		  DLCI with the Forward congestion \
		  bit set;
	   CAPABILITIES = ;"
            REFERENCE
               "Draft American National  Standard  T1.618-1991, Section 3.3.3"
           ::= { mcmFrAllSVCCircuitEntry 6 }



