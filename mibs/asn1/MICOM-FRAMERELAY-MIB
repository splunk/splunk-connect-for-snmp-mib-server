	SYNTAX  McmFrSVCCntrEntry
	ACCESS  not-accessible
	STATUS  obsolete
	DESCRIPTION
	  "NAME = ;
	   DESC = Entry int the SVC Counter table;
	   HELP = ;
	   CAPABILITIES = ;"
	INDEX   { mcmFrSVCCntrIfIndex, mcmFrSVCCntrDNA }
		::= { mcmFrSVCCntrTable 1 }

	McmFrSVCCntrEntry ::= SEQUENCE {
		mcmFrSVCCntrIfIndex 		INTEGER,
		mcmFrSVCCntrDNA 		OCTET STRING,
		mcmFrSVCCntrAction 		INTEGER
	}


mcmFrSVCCntrIfIndex	OBJECT-TYPE
	SYNTAX	INTEGER
	ACCESS	read-write
	STATUS	obsolete
	DESCRIPTION
	  "NAME = ;
	   DESC = Ifindex associated with the SVC \
		  for which the counters are to be\
	          reset;
	   HELP = ;
	   CAPABILITIES = ;"
	::= { mcmFrSVCCntrEntry 1 }

mcmFrSVCCntrDNA	OBJECT-TYPE
	SYNTAX	OCTET STRING (SIZE(3..30)) 
	ACCESS	read-write
	STATUS	obsolete
	DESCRIPTION
	  "NAME = ;
	   DESC = The DNA associated with the SVC \
		  whose counters are to be reset;
	   HELP = ;
	   CAPABILITIES = ;"
	::= { mcmFrSVCCntrEntry 2 }

mcmFrSVCCntrAction OBJECT-TYPE
	SYNTAX  INTEGER { reset(1), other(2) }
	ACCESS	write-only
	STATUS  obsolete
	DESCRIPTION
	  "NAME = ;
	   DESC = Action to take on the SVC \
	          counters (reset);
	   HELP = When set to reset, the SVC \
		  counters should be reset;
	   CAPABILITIES = ;"
	::= { mcmFrSVCCntrEntry 3 }

-- Trap Messages

mcmFrWanLinkUp TRAP-TYPE
	ENTERPRISE	micom-frame-relay
	VARIABLES	{ mcmSysAsciiTimeOfDay, mcmSysIfExtModule, mcmSysIfExtPPA }
	DESCRIPTION
	  "NAME = ;
	   DESC = FR TRAP message: indication of WAN link \
                  up.\
		  Severity Level: INFORMATIVE.;
	   HELP = The Frame Relay WAN LINK is up. \
                  Normal status Indicator.;"
	::= 1

mcmFrWanLinkDown TRAP-TYPE
	ENTERPRISE	micom-frame-relay
	VARIABLES	{ mcmSysAsciiTimeOfDay,mcmSysIfExtModule,mcmSysIfExtPPA }
	DESCRIPTION
	  "NAME = ;
	   DESC = FR TRAP message: indication of WAN link \
                  down.\
		  Severity Level: CRITICAL.;
	   HELP = Loss of Physical connection, or LMI is dropped.\
                  Please check the physical connection; the MPANL \
                  LMI status, and provisioning on Passport. If  \
                  no change, contact Sys Admin., or Field \
                  Personnel.;"
	::= 2

-- End of Trap Messages 

END


