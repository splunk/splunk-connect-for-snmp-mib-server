        SYNTAX   INTEGER (1..4096)
        ACCESS   read-write
        STATUS   mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = The maximum receive frame size in \
		  octets for this PVC tunnel data (sent into \
		  this unit). (Configuration);
	   HELP = ;
	   CAPABILITIES = ;"
	DEFVAL { 1604 }
            ::= { nvmFrTunnelPvcEntry 7 }

