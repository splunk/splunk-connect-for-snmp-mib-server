	SYNTAX	INTEGER (0..2560000)
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION
	  "NAME = ;
           DESC = The maximum transmission throughput.\
                  Always set to the value of \
                   mcmFrLANSpvcSVCMinTxThroughput.;
           HELP = ;
	   CAPABILITIES = ;"
	::= {mcmFrLANSpvcSVCEntry 9}

