		nvmFrSVCMaxRxThroughput			INTEGER, 
		nvmFrSVCTxBurstSize			INTEGER,
		nvmFrSVCRxBurstSize			INTEGER,
		nvmFrSVCExcessTxBurstSize		INTEGER,
		nvmFrSVCExcessRxBurstSize		INTEGER
		}

