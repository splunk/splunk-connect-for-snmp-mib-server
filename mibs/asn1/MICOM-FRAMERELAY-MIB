-- Parallel Frame Relay Virtual Port to Netlink 
--	(Tunnel PVC) Map Table

nvmFrNetlinkMapTable OBJECT-TYPE
	SYNTAX	SEQUENCE OF NvmFrNetlinkMapEntry
	ACCESS	not-accessible
	STATUS	mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = Table for mapping Netlinks (Tunnel PVC ifIndex \
	 	  values) to Virtual Ports. (Configuration);
	   HELP = ;
	   CAPABILITIES = NET_CFG, VPN_DISP;"
	::= {fr-configuration 26}

nvmFrNetlinkMapEntry OBJECT-TYPE
	SYNTAX	NvmFrNetlinkMapEntry
	ACCESS	not-accessible
	STATUS	mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = Entry in the table for mapping Netlinks \
		  (Tunnel PVC ifIndex values) to Virtual \
		  Ports. (Configuration);
	   HELP = ;
	   CAPABILITIES = ;"
	INDEX	{nvmFrNetlinkMapVPIfIndex, 
		nvmFrNetlinkMapNetlinkIfIndex}
	::= {nvmFrNetlinkMapTable 1}

NvmFrNetlinkMapEntry ::= SEQUENCE {
	nvmFrNetlinkMapVPIfIndex	INTEGER,
	nvmFrNetlinkMapNetlinkIfIndex	INTEGER,
	nvmFrNetlinkMapRowStatus	INTEGER
	}

nvmFrNetlinkMapVPIfIndex OBJECT-TYPE
	SYNTAX	INTEGER (1..255)
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = Virtual port ifIndex associated \
		  with this Netlink (Tunnel PVC). \
		  (Configuration);
	   HELP = ;
	   CAPABILITIES = ;"
	::=  { nvmFrNetlinkMapEntry 1}

nvmFrNetlinkMapNetlinkIfIndex OBJECT-TYPE
	SYNTAX	INTEGER (1..255)
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = Netlink (Tunnel PVC) ifIndex associated \
		  with the specified Virtual Port. \
		  (Configuration);
	   HELP = ;
	   CAPABILITIES = ;"
	::=  { nvmFrNetlinkMapEntry 2}

nvmFrNetlinkMapRowStatus OBJECT-TYPE
	SYNTAX	INTEGER {
		add (1),
		delete (2),
		active (3)
		}
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = This object is used to add new rows or delete \
		  existing rows in this Virtual Port to Netlink \
		  Mapping table. (Configuration) ;
	   HELP = ;
	   CAPABILITIES = ;"
	::=  {nvmFrNetlinkMapEntry 3}



  -- The Frame Relay Counter Reset Group

  -- The FR Physical Port Counter Table

mcmFrPhyPortCntrTable OBJECT-TYPE
	SYNTAX  SEQUENCE OF McmFrPhyPortCntrEntry
	ACCESS  not-accessible
	STATUS  obsolete
	DESCRIPTION
	  "NAME = ;
	   DESC = This table has been OBSOLETED for software \
		  releases beginning with Release 3.0. \
		  Table to reset Frame Relay physical \
		  port counters;
	   HELP = ;
	   CAPABILITIES = NET_CFG, VPN_DISP;"
			::= { fr-control 1 }

mcmFrPhyPortCntrEntry OBJECT-TYPE
	SYNTAX  McmFrPhyPortCntrEntry
	ACCESS  not-accessible
	STATUS obsolete
	DESCRIPTION
	  "NAME = ;
	   DESC = Entry in the Frame Relay Physical \
		  port counters table;
	   HELP = ;
	   CAPABILITIES = ;"
	INDEX   { mcmFrPhyPortCntrIfIndex }
		::= { mcmFrPhyPortCntrTable 1 }

	McmFrPhyPortCntrEntry ::= SEQUENCE {
		mcmFrPhyPortCntrIfIndex 	INTEGER,
		mcmFrPhyPortCntrAction 		INTEGER
	}


mcmFrPhyPortCntrIfIndex	OBJECT-TYPE
	SYNTAX	INTEGER
	ACCESS	read-write
	STATUS	obsolete
	DESCRIPTION
	  "NAME = ;
	   DESC = The ifIndex of the physical port whose \
		  counters are to be reset;
	   HELP = ;
	   CAPABILITIES = ;"
	::= { mcmFrPhyPortCntrEntry 1 }

mcmFrPhyPortCntrAction OBJECT-TYPE
	SYNTAX  INTEGER { reset(1), other(2) }
	ACCESS	write-only
	STATUS  obsolete
	DESCRIPTION
	  "NAME = ;
	   DESC = Action to be taken on counters (reset);
	   HELP = When set to reset, the physical port \
		  counters should be zeroed out for the \
		  specified ifIndex;
	   CAPABILITIES = ;"
	::= { mcmFrPhyPortCntrEntry 2 }

