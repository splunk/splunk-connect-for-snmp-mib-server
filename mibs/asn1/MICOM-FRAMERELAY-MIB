	SYNTAX  McmFrPVCCntrEntry
	ACCESS  not-accessible
	STATUS  obsolete
	DESCRIPTION
	  "NAME = ;
	   DESC = Entry in the PVC counter reset \
		  table;
	   HELP = ;
	   CAPABILITIES = ;"
	INDEX   { mcmFrPVCCntrIfIndex, mcmFrPVCCntrDLCI }
		::= { mcmFrPVCCntrTable 1 }

	McmFrPVCCntrEntry ::= SEQUENCE {
		mcmFrPVCCntrIfIndex 		INTEGER,
		mcmFrPVCCntrDLCI 		INTEGER,
		mcmFrPVCCntrAction 		INTEGER
	}


mcmFrPVCCntrIfIndex	OBJECT-TYPE
	SYNTAX	INTEGER
	ACCESS	read-write
	STATUS	obsolete
	DESCRIPTION
	  "NAME = ;
	   DESC = Ifindex associated with the pvc.;
	   HELP = The ifIndex of the physical port \
		  associated with the PVC whose \
		  counters are to be reset;
	   CAPABILITIES = ;"
	::= { mcmFrPVCCntrEntry 1 }

mcmFrPVCCntrDLCI	OBJECT-TYPE
	SYNTAX	INTEGER (17..126975)
	ACCESS	read-write
	STATUS	obsolete
	DESCRIPTION
	  "NAME = ;
	   DESC = The DLCI number of the PVC whose \
		  counters are to be reset;
	   HELP = ;
	   CAPABILITIES = ;"
	::= { mcmFrPVCCntrEntry 2 }

