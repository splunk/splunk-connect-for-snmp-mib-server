	SYNTAX  INTEGER { reset(1), other(2) }
	ACCESS	write-only
	STATUS  obsolete
	DESCRIPTION
	  "NAME = ;
	   DESC = Action to be taken on the PVC \
		  counter (reset);
	   HELP = When set to reset, the PVC Counters \
		  get reset;
	   CAPABILITIES = ;"
	::= { mcmFrPVCCntrEntry 3 }


  -- The FR SVC Counter Table

mcmFrSVCCntrTable OBJECT-TYPE
	SYNTAX  SEQUENCE OF McmFrSVCCntrEntry
	ACCESS  not-accessible
	STATUS  obsolete
	DESCRIPTION
	  "NAME = ;
	   DESC = This table has been OBSOLETED for  software \
		  releases beginning with Release 3.0. \
