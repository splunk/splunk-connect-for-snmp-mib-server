-- Parallel Frame Relay MICOM Virtual Port Extension

nvmFrVirtualPortTable OBJECT-TYPE
	SYNTAX	SEQUENCE OF NvmFrVirtualPortEntry
	ACCESS	not-accessible
	STATUS	mandatory
	DESCRIPTION
	  "NAME = ;
           DESC = Table to provide the mapping between \
                  virtual ports and physical ports \
                  and to define the virtual port \
                  parameters;
           HELP = ;
	   CAPABILITIES = NET_CFG, VPN_DISP;"
	::= {fr-configuration 13}

nvmFrVirtualPortEntry OBJECT-TYPE
	SYNTAX	NvmFrVirtualPortEntry
	ACCESS	not-accessible
	STATUS	mandatory
	DESCRIPTION
	  "NAME = ;
           DESC = Each entry in this table contains \
                  information to define a virtual \
                  port;
           HELP = ;
	   CAPABILITIES = ;"
	INDEX	{nvmFrVirtualPortIfIndex}
	::= {nvmFrVirtualPortTable 1}

NvmFrVirtualPortEntry ::= SEQUENCE {
		nvmFrVirtualPortIfIndex		INTEGER,
		nvmFrVirtualPortPhyPortIfIndex	INTEGER,
		nvmFrVirtualPortPPA 		INTEGER,
		nvmFrVirtualPortMode		INTEGER,
		nvmFrVirtualPortEntryStatus	INTEGER,
		nvmFrVirtualPortProtocolBinding	INTEGER,
		nvmFrVirtualPortNumber		INTEGER
		}

nvmFrVirtualPortIfIndex OBJECT-TYPE
	SYNTAX	INTEGER (1..255)
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION
	  "NAME = Virtual Port Interface Number;
           DESC = IfIndex associated with this port;
           HELP = ;
	   CAPABILITIES = ;"
	::=  {nvmFrVirtualPortEntry 1}

nvmFrVirtualPortPhyPortIfIndex OBJECT-TYPE
	SYNTAX	INTEGER (1..255)
	ACCESS	read-write
	STATUS	obsolete
	DESCRIPTION
	  "NAME = ;
	   DESC = *******NOTE: THIS OBJECT IS OBSOLETE*******  
                  This information is no longer useful for \
                  virtual ports.  The virtual ports no longer \
                  have an underlying network. Only SVC DNAs \
                  and PVC DLCIs are associated with VPs now. \
                  The underlying MPANL will take care of which \
                  link the SVC is to be made on and a Physical \
                  Line IfIndex has been added to the PVCs.  VP \
                  are no longer required to provide the backup \
                  link support (MPANL now does that).\  

                  IfIndex of the Frame Relay physical \
                  port for which the virtual port is \
                  (or will be) linked to;
           HELP = ;
	   CAPABILITIES = ;"
	::=  {nvmFrVirtualPortEntry 2}

nvmFrVirtualPortPPA OBJECT-TYPE
	SYNTAX	INTEGER (0..254)
	ACCESS	read-write
	STATUS	obsolete
	DESCRIPTION
	  "NAME = This object has been OBSOLETED for software \
		  releases beginning with Release 3.0. \
		  Virtual Port PPA Number;
           DESC = Point of Physical attachment for \
                  this port;
           HELP = ;
	   CAPABILITIES = ;"
	::=  {nvmFrVirtualPortEntry 3}

nvmFrVirtualPortMode OBJECT-TYPE
	SYNTAX	INTEGER {
		basic (1),
		easyRouting (2)
		}
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION
	  "NAME = Virtual Port Mode;
           HELP = Mode of the virtual port;
           DESC = Easy Routing requires that data  \
                  will be switched between the \
                  Virtual Circuits on the this \
                  virutal port. Basic mode turns \
                  off the switching mode;
	   CAPABILITIES = ;"
	::=  {nvmFrVirtualPortEntry 4}

nvmFrVirtualPortEntryStatus OBJECT-TYPE
	SYNTAX	INTEGER {
		enable (1),
		disable (2),
		delete (3)
		}
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION
	  "NAME = Virtual Port Mode;
           DESC = Virtual port row status;
           HELP = Indicates whether or not this \
                  entry is valid;
	   CAPABILITIES = ;"
	::=  {nvmFrVirtualPortEntry 5}


nvmFrVirtualPortProtocolBinding OBJECT-TYPE
	SYNTAX	INTEGER {
		ip-configured (1),
		ipx-configured (2),
		ip-and-ipx-configured (3),
		bridge-configured (4),
		ip-and-bridge-configured (5),
		ipx-and-bridge-configured (6),
		ip-and-ipx-and-bridge-configured (7),
		sNA-configured (8),
		x25-configured (9)
		}
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = Bitmap that contains the protocols bound. \
		  to this Virtual Port;
	   HELP = ;
	   CAPABILITIES = ;"
	::=  {nvmFrVirtualPortEntry 6}

nvmFrVirtualPortNumber OBJECT-TYPE
	SYNTAX	INTEGER (0..254)
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = This object specifies the Virtual \
		  Port Number used as an identifier \
		  for a Virtual Port. (Configuration);
	   HELP = ;
	   CAPABILITIES = ;"
	::=  {nvmFrVirtualPortEntry 7}


-- Parallel Frame Relay Associated SVC MIB8

nvmFrSVCMapTable OBJECT-TYPE
	SYNTAX	SEQUENCE OF NvmFrSVCMapEntry
	ACCESS	not-accessible
	STATUS	mandatory
	DESCRIPTION
	  "NAME = ;
           DESC = Table for SVC to VP bindings;
           HELP = This table will give the SVC \
                  DNA address to be bound to the \
                  specified Virtual Port;
	   CAPABILITIES = NET_CFG, VPN_DISP;"
	::= {fr-configuration 14}

nvmFrSVCMapEntry OBJECT-TYPE
	SYNTAX	NvmFrSVCMapEntry
	ACCESS	not-accessible
	STATUS	mandatory
	DESCRIPTION
	  "NAME = ;
           DESC = Entry in the SVC to VP binding \
                  table;
           HELP = SVC/VP binding entry in the SVC \
                  binding table.  Multiple SVCS \
                  (DNAs) may be bound to a single \
                  VP (ifindex);
	   CAPABILITIES = ;"
	INDEX	{nvmFrSVCMapVirtualPortIfIndex, nvmFrSVCMapDNA}
	::= {nvmFrSVCMapTable 1}

NvmFrSVCMapEntry ::= SEQUENCE {
	nvmFrSVCMapVirtualPortIfIndex		INTEGER,
	nvmFrSVCMapDNA				OCTET STRING,
	nvmFrSVCMapMaxTxSize			INTEGER,
	nvmFrSVCMapMaxRxSize			INTEGER,
	nvmFrSVCMapMinTxThroughput		INTEGER,
	nvmFrSVCMapMinRxThroughput		INTEGER,
	nvmFrSVCMapMaxTxThroughput		INTEGER,
	nvmFrSVCMapMaxRxThroughput		INTEGER,
	nvmFrSVCMapTxBurstSize			INTEGER,
	nvmFrSVCMapRxBurstSize			INTEGER,
	nvmFrSVCMapExcessTxBurstSize		INTEGER,
	nvmFrSVCMapExcessRxBurstSize		INTEGER,
	nvmFrSVCMapPriority			INTEGER,
	nvmFrSVCMapEntryStatus			INTEGER,
	nvmFrSVCMapTransferPriority		INTEGER,
	nvmFrSVCMapRowStatus			INTEGER,
	nvmFrSVCMapDiscardPriority		INTEGER,
	nvmFrSVCMapSetupPriority  		INTEGER,
	nvmFrSVCMapHoldingPriority		INTEGER
	}

nvmFrSVCMapVirtualPortIfIndex OBJECT-TYPE
	SYNTAX	INTEGER
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = Virtual Port ifIndex associated \
		  with this DNA;
	   HELP = ;
	   CAPABILITIES = ;"
	::=  {nvmFrSVCMapEntry 1}

nvmFrSVCMapDNA OBJECT-TYPE
	SYNTAX	OCTET STRING (SIZE(1..34)) 
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION
	  "NAME = ;
           DESC = Remote DNA to associate with \
                  the corresponding virtual port;
           HELP = DNA to be mapped to the \
                  corresponding virtual port. \
                  This will be used to create \
                  an SVC to the corresponding \
                  remote DNA;
	   CAPABILITIES = ;"
	::=  {nvmFrSVCMapEntry 2}

nvmFrSVCMapMaxTxSize OBJECT-TYPE
	SYNTAX	INTEGER (0..4096)
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION
	  "NAME = ;
           DESC = Maximum transmit frame size to \
                  negotiate with the remote (in \
                  bytes);
           HELP = This value will override the default values for \
		  the physical interface.  A value \
		  of 0 indicates the networks default \
		  values are to be used. Experessed in \
		  bytes;
	   CAPABILITIES = ;"
	::=  {nvmFrSVCMapEntry 3}

nvmFrSVCMapMaxRxSize OBJECT-TYPE
	SYNTAX	INTEGER (0..4096)
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION
	  "NAME = ;
           DESC = Maximum receive frame size to \
                  negotiate with the remote (in \
                  bytes);
           HELP = This value will override the default values for \
