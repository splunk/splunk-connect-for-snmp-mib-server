	OBJECTS {
		epc8PortName,
		epc8PortState,
		epc8PortSwitchCount}
	STATUS  current
	DESCRIPTION
		"Event is triggered when a port is switched"
	-- 1.3.6.1.4.1.28507.1.0.3
	::= { events 3 }


