	STATUS  current
	DESCRIPTION
		"hygro limits exceeded"
	-- 1.3.6.1.4.1.28507.1.0.12
	::= { events 12 }

epc8BasicGroup OBJECT-GROUP
	OBJECTS {
		epc8portNumber,
		epc8PortName,
		epc8PortState,
		epc8PortSwitchCount,
		epc8TrapCtrl,
		epc8TrapIPAddr,
		epc8TrapIPPort,
		epc8PortStartupMode,
		epc8PortStartupDelay,
		epc8PortRepowerTime,
		epc8Irms,
		epc8TempSensor,
		epc8HygroSensor }
	STATUS  current
	DESCRIPTION
		"Basic objects."
	-- 1.3.6.1.4.1.28507.1.3.1.1
	::= { epc8Groups 1 }

epc8NotificationGroup NOTIFICATION-GROUP
	NOTIFICATIONS {
		epcSwitchEvtPort1,
		epcSwitchEvtPort2,
		epcSwitchEvtPort3,
		epcSwitchEvtPort4,
		epcSwitchEvtPort5,
		epcSwitchEvtPort6,
		epcSwitchEvtPort7,
		epcSwitchEvtPort8,
		epc8TempEvtSen1,
		epc8HygroEvtSen1,
		epc8TempEvtSen2,
		epc8HygroEvtSen2 }
	STATUS  current
	DESCRIPTION
		""
	-- 1.3.6.1.4.1.28507.1.3.1.2
	::= { epc8Groups 2 }

END

