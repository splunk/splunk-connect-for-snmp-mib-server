    SYNTAX     InstanceName
    MAX-ACCESS read-only
    STATUS     current
    DESCRIPTION
	    "Instance name."
    ::= { bizTalkFILESendAdapterEntry 1 }

btfilesaBytesSent OBJECT-TYPE
    SYNTAX     Gauge32
    MAX-ACCESS read-only
    STATUS     current
    DESCRIPTION
	    "Total number of bytes sent by adapter. The
	    counter is incremented only for messages that
	    have been completely written to file system"
    ::= { bizTalkFILESendAdapterEntry 2 }

