	SYNTAX  INTEGER
		{
		bertPatternSyncOther(1),
		bertPatternSyncNoTest(2),
		bertPatternSyncNoSync(3),
		bertPatternSyncInsync(4)
		}
	MAX-ACCESS  read-only
	STATUS  current
	DESCRIPTION
