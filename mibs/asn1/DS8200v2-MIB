	SYNTAX  INTEGER
	MAX-ACCESS  read-only
	STATUS  current
	DESCRIPTION
		"Specifies the far device."
	::= { traplogEntry 3 }

traplogInterfaceIndex             OBJECT-TYPE
	SYNTAX  INTEGER
	MAX-ACCESS  read-only
	STATUS  current
	DESCRIPTION
		"Specifies the interface."
	::= { traplogEntry 4 }
 
traplogTrapNum  OBJECT-TYPE
	SYNTAX  INTEGER
