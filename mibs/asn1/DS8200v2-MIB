	SYNTAX  INTEGER
		{
		serialDteASCAlarmOther(1),
		serialDteASCAlarmNone(2),
		serialDteASCAlarmThresholdExceeded(3)
		}
	MAX-ACCESS  read-only
	STATUS  current
	DESCRIPTION     
		"This status indicates if the ASC Seconds threshold is exceeded."
	::= { serialDteAlarmTableEntry 11 }

