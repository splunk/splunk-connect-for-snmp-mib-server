	::= { ddsNet 2 }

ddsNetAlarmTableEntry   OBJECT-TYPE
	SYNTAX  DdsNetAlarmTableEntry
	MAX-ACCESS  not-accessible
	STATUS  current
	DESCRIPTION
		"An entry in the ddsNetAlarmTable."
	INDEX   { ddsNetAlarmNearIndex, ddsNetAlarmFarIndex, ddsNetAlarmIndex }
	::= { ddsNetAlarmTable 1 }

