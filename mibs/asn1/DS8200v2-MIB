-- itable
-- requests a list of all interfaces on a device

iTable  OBJECT-TYPE
	SYNTAX  SEQUENCE OF ITableEntry
	MAX-ACCESS  not-accessible
	STATUS  current
	DESCRIPTION
		"The interface table."
	::= { itable 1 }

iTableEntry     OBJECT-TYPE
	SYNTAX  ITableEntry
	MAX-ACCESS  not-accessible
	STATUS  current
	DESCRIPTION
