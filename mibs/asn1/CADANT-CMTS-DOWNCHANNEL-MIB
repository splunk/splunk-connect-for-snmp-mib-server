CADANT-CMTS-DOWNCHANNEL-MIB DEFINITIONS ::= BEGIN

IMPORTS
	OBJECT-TYPE, MODULE-IDENTITY, 
	-- do not import BITS,
	Integer32, TimeTicks, Unsigned32, Counter64
		FROM SNMPv2-SMI
	TEXTUAL-CONVENTION, TimeInterval, TruthValue, RowStatus
		FROM SNMPv2-TC
    InterfaceIndex
        FROM IF-MIB
	TenthdBmV
		FROM DOCS-IF-MIB
	cadSpectrum
		FROM CADANT-PRODUCTS-MIB
	CardId
		FROM CADANT-TC;


