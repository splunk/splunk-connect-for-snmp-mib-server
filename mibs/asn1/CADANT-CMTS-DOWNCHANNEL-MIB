cadIfCmtsCardNumber OBJECT-TYPE
        SYNTAX      CardId
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
	    "Card Number associated with this Channel."
        ::= { cadIfDownstreamChannelEntry 19 }

--cadIfCmtsConfigured OBJECT-TYPE
