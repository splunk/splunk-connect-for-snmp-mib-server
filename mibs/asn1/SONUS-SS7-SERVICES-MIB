        sonusSs7TimerT15                Integer32,
        sonusSs7TimerT16                Integer32,
        sonusSs7TimerT17                Integer32,
        sonusSs7TimerT18                Integer32,
        sonusSs7TimerT19                Integer32,
        sonusSs7TimerT20                Integer32,
        sonusSs7TimerT21                Integer32,
