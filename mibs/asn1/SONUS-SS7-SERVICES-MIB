sonusSs7ServicesMIBNotifications OBJECT IDENTIFIER
    ::= { sonusSs7ServicesMIB 2 }

sonusSs7ServicesMIBNotificationsPrefix OBJECT IDENTIFIER
    ::= { sonusSs7ServicesMIBNotifications 0 }

sonusSs7ServicesMIBNotificationsObjects OBJECT IDENTIFIER
    ::= { sonusSs7ServicesMIBNotifications 1 }

sonusSs7NodeServiceType OBJECT-TYPE
    SYNTAX          DisplayString (SIZE(1..6))
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "The type of service supported by this SS7 node connection (ISUP or TCAP)."
    ::= { sonusSs7ServicesMIBNotificationsObjects 1 }

sonusSs7NodeStateChangeReason OBJECT-TYPE
    SYNTAX          DisplayString (SIZE(1..101))
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "The reason why this SS7 node connection has  changed state."
    ::= { sonusSs7ServicesMIBNotificationsObjects 2 }

sonusSs7NodeCurrentState OBJECT-TYPE
    SYNTAX          INTEGER { offline(1), active(2), initializing(3), 
                        failed(4), disabled(5), congested(6) }
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "The current state of this SS7 node connection."
    ::= { sonusSs7ServicesMIBNotificationsObjects 3 }

sonusSs7NodeAffectedPointCode OBJECT-TYPE
    SYNTAX          PointCode
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "The SS7 point code affected by this MTP state change."
    ::= { sonusSs7ServicesMIBNotificationsObjects 4 }

sonusSs7NodeConnectionEstablishedNotification NOTIFICATION-TYPE
    OBJECTS         { sonusSs7NodeName, sonusSs7NodeGatewayAssignment, 
                      sonusSs7NodeServiceType, sonusEventDescription, 
                      sonusEventClass, sonusEventLevel }
    STATUS          current
    DESCRIPTION     
            "This trap indicates that a connection has been established to the
             specified SS7 gateway. Information provided by this trap includes
             the name of the connected SS7 node, the name of the SS7 gateway 
             on which the node resides, and the type of SS7 service that has been
             established (ISUP or TCAP)."
    ::= { sonusSs7ServicesMIBNotificationsPrefix 1 }

sonusSs7NodeConnectionFailedNotification NOTIFICATION-TYPE
    OBJECTS         { sonusSs7NodeName, sonusSs7NodeGatewayAssignment, 
                      sonusSs7NodeServiceType, 
                      sonusSs7NodeStateChangeReason, 
                      sonusEventDescription, sonusEventClass, 
                      sonusEventLevel }
    STATUS          current
    DESCRIPTION     
            "This trap indicates that an established connection to an SS7 gateway
             has failed. Information provided by this trap includes the name of the
             failed SS7 node, the name of the SS7 gateway on which the node resides,
             the type of SS7 service affected by this failure, and the reason why
             the connection has failed."
    ::= { sonusSs7ServicesMIBNotificationsPrefix 2 }

sonusSs7NodeConnectionStateChangeNotification NOTIFICATION-TYPE
    OBJECTS         { sonusSs7NodeName, sonusSs7NodeGatewayAssignment, 
                      sonusSs7NodeServiceType, sonusSs7NodeCurrentState, 
                      sonusSs7NodeStateChangeReason, 
                      sonusEventDescription, sonusEventClass, 
                      sonusEventLevel }
    STATUS          current
    DESCRIPTION     
            "This trap indicates that an established connection to an SS7 gateway
             has changed state. Information provided by this trap includes the name 
             of the affected SS7 node, the name of the SS7 gateway on which the node 
             resides, the type of SS7 service affected by this failure, the new state
             of the node, and the reason for the state change."
    ::= { sonusSs7ServicesMIBNotificationsPrefix 3 }

sonusSs7NodeMtpStateChangeNotification NOTIFICATION-TYPE
    OBJECTS         { sonusSs7NodeName, sonusSs7NodeGatewayAssignment, 
                      sonusSs7NodeServiceType, sonusSs7NodeMtpStatus, 
                      sonusSs7NodeAffectedPointCode, 
                      sonusEventDescription, sonusEventClass, 
                      sonusEventLevel }
    STATUS          current
    DESCRIPTION     
            "This trap indicates that a MTP state change event has been received from the
             SS7 gateway, affecting the specified destination (or remote) point 
             code (DPC). Information provided by this trap includes the name 
             of the affected SS7 node, the name of the SS7 gateway on which the node 
             resides, the updated MTP state, and the DPC affected by this event."
    ::= { sonusSs7ServicesMIBNotificationsPrefix 4 }

END -- end of module SONUS-SS7-SERVICES-MIB.
