Summitd DEFINITIONS ::= BEGIN
    IMPORTS
    enterprises, IpAddress, Gauge
        FROM RFC1155-SMI
    OBJECT-TYPE
        FROM RFC-1212
    DisplayString
        FROM RFC1213-MIB;


    summit-development OBJECT IDENTIFIER ::= { enterprises 23688 }

    summit-Products OBJECT IDENTIFIER ::= { summit-development 1 }

    uni-a OBJECT IDENTIFIER ::= { summit-Products 1 }

    name OBJECT-TYPE
        SYNTAX              DisplayString (SIZE(0..16))
        ACCESS              read-only
        STATUS              mandatory
        DESCRIPTION         "Name of the unit."
        ::= { uni-a 1 }

    condition OBJECT-TYPE
        SYNTAX              INTEGER {
                                ok( 1 ),
                                error( 2 ),
                                warning( 3 )
                            }
        ACCESS              read-only
        STATUS              mandatory
        DESCRIPTION         "Condition of the unit (1-ok,2-Error,3-Warning)."
        ::= { uni-a 2 }

    mannum OBJECT-TYPE
        SYNTAX              INTEGER
        ACCESS              read-only
        STATUS              mandatory
        DESCRIPTION         "Maufacturing number of the unit."
        ::= { uni-a 3 }

    status OBJECT-TYPE
        SYNTAX              DisplayString (SIZE(1..16))
        ACCESS              read-only
        STATUS              mandatory
        DESCRIPTION         "Status of the unit."
        ::= { uni-a 4 }

    rssi OBJECT-TYPE
        SYNTAX              Gauge
        ACCESS              read-only
        STATUS              mandatory
        DESCRIPTION         "Receive strenght indicator in -db (negative level)."
        ::= { uni-a 5 }

    ebno OBJECT-TYPE
        SYNTAX              Gauge
        ACCESS              read-only
        STATUS              mandatory
        DESCRIPTION         "Eb/No signal quality in 0.1 db (72 -> 7.2db)."
        ::= { uni-a 6 }

    berr OBJECT-TYPE
        SYNTAX              Gauge
        ACCESS              read-only
        STATUS              mandatory
        DESCRIPTION         "Bit error rate bit/sec."
        ::= { uni-a 7 }

    temperature OBJECT-TYPE
        SYNTAX              INTEGER
        ACCESS              read-only
        STATUS              mandatory
        DESCRIPTION         "Temperature of the unit ."
        ::= { uni-a 8 }

    maxENspeed OBJECT-TYPE
        SYNTAX              INTEGER
        ACCESS              read-only
        STATUS              mandatory
        DESCRIPTION         "Maximum enabled speed of the radio in Mbit/sec."
        ::= { uni-a 9 }

    currentRadioSpeed OBJECT-TYPE
        SYNTAX              INTEGER
        ACCESS              read-only
        STATUS              mandatory
        DESCRIPTION         "Current speed of the radio in Mbit/sec"
        ::= { uni-a 10 }

    type OBJECT-TYPE
        SYNTAX              DisplayString
        ACCESS              read-only
        STATUS              mandatory
        DESCRIPTION         "Uni type."
        ::= { uni-a 11 }

    txFreq OBJECT-TYPE
        SYNTAX              DisplayString
        ACCESS              read-only
        STATUS              mandatory
        DESCRIPTION         "Frequency of the transmitter in MHz."
        ::= { uni-a 12 }

    rxFreq OBJECT-TYPE
        SYNTAX              DisplayString
        ACCESS              read-only
        STATUS              mandatory
        DESCRIPTION         "Frequency of the receiver in MHz."
        ::= { uni-a 13 }

    txDataSpeed OBJECT-TYPE
        SYNTAX              Gauge
        ACCESS              read-only
        STATUS              mandatory
        DESCRIPTION         "Current data speed of the transmitter in bit/sec."
        ::= { uni-a 14 }

    rxDataSpeed OBJECT-TYPE
        SYNTAX              Gauge
        ACCESS              read-only
        STATUS              mandatory
        DESCRIPTION         "Current data speed of the receiver in bit/sec."
        ::= { uni-a 15 }

    remoteName OBJECT-TYPE
        SYNTAX              DisplayString
        ACCESS              read-only
        STATUS              mandatory
        DESCRIPTION         "Name of the remote Unit."
        ::= { uni-a 16 }

    remoteIPaddress OBJECT-TYPE
        SYNTAX              IpAddress
        ACCESS              read-only
        STATUS              mandatory
        DESCRIPTION         "IP address of the remote unit"
        ::= { uni-a 17 }

    atpc OBJECT-TYPE
        SYNTAX              INTEGER {
                                enabled( 1 ),
                                disabled( 2 )
                            }
        ACCESS              read-only
        STATUS              mandatory
        DESCRIPTION         "ATPC - Automatic Transmit Power Control (1-enabled, 2-disabled)"
        ::= { uni-a 18 }

    txPower OBJECT-TYPE
        SYNTAX              Gauge
        ACCESS              read-only
        STATUS              mandatory
        DESCRIPTION         "Actual Transmit Power [dBm]"
        ::= { uni-a 19 }

    acm OBJECT-TYPE
        SYNTAX              INTEGER {
                                enabled( 1 ),
                                disabled( 2 )
                            }
        ACCESS              read-only
        STATUS              mandatory
        DESCRIPTION         "ACM - Adaptive Coding & Modulation (1-enabled, 2-disabled)"
        ::= { uni-a 20 }

    txModulation OBJECT-TYPE
        SYNTAX              INTEGER {
                                qam-2( 1 ),
                                qam-4( 2 ),
                                qam-8( 3 ),
                                qam-16( 4 ),
                                qam-32( 5 ),
                                qam-64( 6 ),
                                qam-128( 7 ),
                                qam-256( 8 ),
                                qam-512( 9 ),
                                qam-1024( 10 )
                            }
        ACCESS              read-only
        STATUS              mandatory
        DESCRIPTION         "Current Modulation (Tx)"
        ::= { uni-a 21 }

    rxModulation OBJECT-TYPE
        SYNTAX              INTEGER {
                                qam-2( 1 ),
                                qam-4( 2 ),
                                qam-8( 3 ),
                                qam-16( 4 ),
                                qam-32( 5 ),
                                qam-64( 6 ),
                                qam-128( 7 ),
                                qam-256( 8 ),
                                qam-512( 9 ),
                                qam-1024( 10 )
                            }
        ACCESS              read-only
        STATUS              mandatory
        DESCRIPTION         "Current Modulation (Rx)"
        ::= { uni-a 22 }

    txBandwidth OBJECT-TYPE
        SYNTAX              Gauge
        ACCESS              read-only
        STATUS              mandatory
        DESCRIPTION         "Actual Transmit Bandwidth [MHz]"
        ::= { uni-a 23 }

    rxBandwidth OBJECT-TYPE
        SYNTAX              Gauge
        ACCESS              read-only
        STATUS              mandatory
        DESCRIPTION         "Actual Receiver Bandwidth [MHz]"
        ::= { uni-a 24 }

    licenseExpire OBJECT-TYPE
        SYNTAX              Gauge
        ACCESS              read-only
        STATUS              mandatory
        DESCRIPTION         "License expire time (time to blocked radio) [day], 
                            0 Blocked, -1 no Expire time set"
        ::= { uni-a 25 }

    summitd-Common OBJECT IDENTIFIER ::= { summit-development 2 }

    summitd-Experimental OBJECT IDENTIFIER ::= { summit-development 3 }


END

