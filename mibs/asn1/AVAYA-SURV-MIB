-- 1.3.6.1.4.1.6889.2.6.4.8
avSurvIpCodecSetConfig OBJECT IDENTIFIER ::= { avSurvMib 8 }

	-- 1.3.6.1.4.1.6889.2.6.4.8.1
	avSurvIpCodecSetFaxMode OBJECT-TYPE
		SYNTAX INTEGER
			{
				off		(1),
				relay		(2),
				passthru	(3),
				t38		(4)
			}
		MAX-ACCESS read-create
