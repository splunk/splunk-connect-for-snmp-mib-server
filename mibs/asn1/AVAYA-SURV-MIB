			SYNTAX INTEGER
					{
						user		(0),
						network	(1),
						peerMaster	(3),
						peerSlave	(4)
					}
			MAX-ACCESS read-create
			STATUS current 			
			DESCRIPTION 
				"Determines glare handling. Non-QSIG calls: a-network, b-user; 
                             QSIG calls: a-peerMaster, b-peerSlave"
			::= { avSurvBriEntry 3 }

		-- 1.3.6.1.4.1.6889.2.6.4.12.1.4
