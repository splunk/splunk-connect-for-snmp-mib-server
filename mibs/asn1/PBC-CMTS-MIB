		-- 1.3.6.1.4.1.5987.2.2.2.2.1.1
		pbcSpectrumAnalysisRFInputTable OBJECT-TYPE
			SYNTAX SEQUENCE OF PbcSpectrumAnalysisRFInputEntry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"There is an entry in this table for each
				upstream input connector on each DOCSIS module
				in the CMTS.  Each port is uniquely identified within
				the CMTS by its pbcCardIfPortIndex value.
				
				This index object is defined under the generic
				chassis management section of the PBC private
				MIB tree."
			::= { pbcCmtsSpectrumAnalysisObjects 1 }
		
		-- 1.3.6.1.4.1.5987.2.2.2.2.1.1.1
		pbcSpectrumAnalysisRFInputEntry OBJECT-TYPE
			SYNTAX PbcSpectrumAnalysisRFInputEntry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"An entry in the RF monitoring input (port) table."
			INDEX { pbcCardIfPortIndex }
			::= { pbcSpectrumAnalysisRFInputTable 1 }
		
		PbcSpectrumAnalysisRFInputEntry ::=
			SEQUENCE { 
				pbcSpectrumAnalysisEnable
					TruthValue,
				pbcSpectrumAnalysisCmdStatus
					INTEGER,
				pbcSpectrumAnalysisIntegrationLen
					Unsigned32,
				pbcSpectrumAnalysisFreqStep
					INTEGER,
				pbcSpectrumAnalysisFreqMin
					Unsigned32,
				pbcSpectrumAnalysisFreqMax
					Unsigned32,
				pbcSpectrumAnalysisArraySize
					Unsigned32,
				pbcSpectrumAnalysisActiveArray
					Unsigned32
			 }

		-- 1.3.6.1.4.1.5987.2.2.2.2.1.1.1.1
		pbcSpectrumAnalysisEnable OBJECT-TYPE
			SYNTAX TruthValue
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"True ('1') enables spectrum estimation.  This
				should be set only after setting the other
				parameters affecting the scan.  After setting
				this 'true', pbcSpectrumAnalysisCmdStatus will
				reflect initial and, ultimately, final status
				of the sweep attempt."
			DEFVAL { false }
			::= { pbcSpectrumAnalysisRFInputEntry 1 }
		
		-- 1.3.6.1.4.1.5987.2.2.2.2.1.1.1.2
		pbcSpectrumAnalysisCmdStatus OBJECT-TYPE
			SYNTAX INTEGER
				{
				available(1),
				scanInProgress(2),
				complete(3),
				generalFailure(4),
				noResources(5)
				}
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"Result of pbcSpectrumAnalysisEnable command."
			DEFVAL { available }
			::= { pbcSpectrumAnalysisRFInputEntry 2 }
		
		-- 1.3.6.1.4.1.5987.2.2.2.2.1.1.1.3
		pbcSpectrumAnalysisIntegrationLen OBJECT-TYPE
			SYNTAX Unsigned32
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"This specifies the number of estimates over
				which the peak of the noise power is
				calculated."
			DEFVAL { 1 }
			::= { pbcSpectrumAnalysisRFInputEntry 3 }
		
		-- 1.3.6.1.4.1.5987.2.2.2.2.1.1.1.4
		pbcSpectrumAnalysisFreqStep OBJECT-TYPE
			SYNTAX INTEGER
				{
				kHz20(1),
				kHz40(2),
				kHz80(3),
				kHz160(4)
				}
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"The frequency step for scanning which defines
				the frequency resolution.  The choices are:
				20KHz, 40KHz, 80KHz and 160KHz."
			DEFVAL { kHz160 }
			::= { pbcSpectrumAnalysisRFInputEntry 4 }
		
		-- 1.3.6.1.4.1.5987.2.2.2.2.1.1.1.5
		pbcSpectrumAnalysisFreqMin OBJECT-TYPE
			SYNTAX Unsigned32
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"The low end of the frequency range being
				scanned.  The lowest legal value, per both
				the DOCSIS and Euro-DOCSIS standards, is
				5MHz."
			DEFVAL { 5000000 }
			::= { pbcSpectrumAnalysisRFInputEntry 5 }
		
		-- 1.3.6.1.4.1.5987.2.2.2.2.1.1.1.6
		pbcSpectrumAnalysisFreqMax OBJECT-TYPE
			SYNTAX Unsigned32
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"Top end of the frequency range being scanned.
				For DOCSIS, the maximum is 42MHz and for
				Euro-DOCSIS it is 65MHz."
			DEFVAL { 42000000 }
			::= { pbcSpectrumAnalysisRFInputEntry 6 }
		
		-- 1.3.6.1.4.1.5987.2.2.2.2.1.1.1.7
		pbcSpectrumAnalysisArraySize OBJECT-TYPE
			SYNTAX Unsigned32 (2..32)
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"This variable holds/specifies the number of scan
				data tables to be allocated to this particular
				scan instance.  The size of each table and,
				therefore, the total amount of buffer space
				required is determined by the other scan parameters.
				If the agent is unable to allocate that amount of
				space, a status of 'noResources' will be returned
				in pbcSpectrumAnalysisCmdStatus following the
				setting of pbcSpectrumAnalysisEnable.
				
				If successful, this becomes the total number of
				table entries in pbcSpectrumAnalysisArrayIndexTable.
				
				A size of '2' is the absolute minimum to permit the
				agent to fill one table with data points while the
				management application is 'draining' the other."
			::= { pbcSpectrumAnalysisRFInputEntry 7 }
		
		-- 1.3.6.1.4.1.5987.2.2.2.2.1.1.1.8
		pbcSpectrumAnalysisActiveArray OBJECT-TYPE
			SYNTAX Unsigned32
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"Contains the pbcSpectrumAnalysisArrayIndex number
				of the data table currently being filled with
				spectral data points."
			DEFVAL { 0 }
			::= { pbcSpectrumAnalysisRFInputEntry 8 }
		
		-- 1.3.6.1.4.1.5987.2.2.2.2.1.2
