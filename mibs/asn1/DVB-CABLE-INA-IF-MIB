        SYNTAX      Unsigned32 ( 0..4294967295 )
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "Control which fields of the upstream session binding
             parameters that are used."
        REFERENCE
            "ES 200 800 version 2"
        ::= { dvbInaIfReprovDstConSesEntry 1 }

dvbInaIfReprovDstConSesUsNiuClientSrcIpType     OBJECT-TYPE
        SYNTAX      InetAddressType
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "The type of the IP source address of the NIU client."
        ::= { dvbInaIfReprovDstConSesEntry 2 }

dvbInaIfReprovDstConSesUsNiuClientSrcIp         OBJECT-TYPE
        SYNTAX      InetAddress
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
