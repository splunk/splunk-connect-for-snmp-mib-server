apRebootNow OBJECT-TYPE
        SYNTAX      INTEGER  
        {
            not-reboot(0),
            reboot(1)
        }
                    
        ACCESS  read-write
        STATUS      mandatory
        DESCRIPTION
            "Setting this object to true(1) causes the device to reset.
             Reading this object always returns false(2)."
        ::= { wirelessAPManageGroup 1 }

