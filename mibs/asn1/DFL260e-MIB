    STATUS      current
    DESCRIPTION 
        "Module Compliance"

    MODULE      -- this module

        MANDATORY-GROUPS        
                { dfl260eSystemObjectGroup, 
                  dfl260eIPsecObjectGroup, 
                  dfl260eStateCountersGroup, 
