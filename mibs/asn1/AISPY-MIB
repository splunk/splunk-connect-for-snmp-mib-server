--  Title:    aiSPY.MIB
--  Version:  1.21
--  Date:     01/07/99 1:00 pm	
--
--  Copyright @ 1999 Applied Innovation, Inc.  All rights reserved worldwide.
--
--  This MIB defines objects for the aiSPY SNMP agent.
--

AISPY-MIB DEFINITIONS ::= BEGIN

IMPORTS
	TruthValue
		FROM RFC1253-MIB
	DisplayString
		FROM RFC1213-MIB
	OBJECT-TYPE
		FROM RFC-1212
	TRAP-TYPE FROM RFC-1215
	Counter, enterprises
		FROM RFC1155-SMI
	Gauge32, OBJECT-IDENTITY FROM SNMPv2-SMI;

aii OBJECT IDENTIFIER ::= { enterprises  539 }

aiSPY   OBJECT IDENTIFIER ::= { aii  20 }

aiSPYIdent OBJECT IDENTIFIER ::= { aiSPY 1 }

aiSPYIdentManufacturer OBJECT-TYPE
    SYNTAX  DisplayString
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The Monitor Control Unit manufacturer."
    ::= { aiSPYIdent  1 }

aiSPYIdentModel OBJECT-TYPE
    SYNTAX  DisplayString
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The Monitor Control Unit Model designation."
    ::= { aiSPYIdent  2 }

aiSPYIdentSoftwareVersion OBJECT-TYPE
    SYNTAX  DisplayString
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The aiSPY SNMP Agent software version."
    ::= { aiSPYIdent  3 }

aiSPYIdentSpecific OBJECT-TYPE
    SYNTAX  OBJECT IDENTIFIER
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "A reference to MIB definitions specific to the
            particular Monitor Unit being managed. This
            object is used to locate the product-specific MIB
            for this device.

            If this information is not present, its value
            should be the OBJECT IDENTIFIER { 0 0 },
            which is a syntactically valid object identifier,
            and any conformant implementation of ASN.1 and BER
            must be able to generate and recognize this value."
    ::= { aiSPYIdent  4 }

aiSPYSystem OBJECT IDENTIFIER ::= { aiSPY 2 }

aiSPYClock OBJECT-TYPE
    SYNTAX  DisplayString
    ACCESS  read-write
    STATUS  mandatory
    DESCRIPTION
           "The aiSPY internal clock settings.
            Displayed as MM/DD/YY HH:MM:SS"
    ::= { aiSPYSystem  1 }

aiSPYDoorAlarmBypass OBJECT-TYPE
    SYNTAX  INTEGER (-2147483648..2147483647)
    ACCESS  read-write
    STATUS  mandatory
    DESCRIPTION
           "The aiSPY door alarm bypass switch.
            This allows the alarm to be bypassed upon valid keypad access.
            Zero disables this, or use 1 thru 20 for the corresponding
            aiSPY input."
    ::= { aiSPYSystem  2 }

aiSPYKeypad OBJECT IDENTIFIER ::= { aiSPYSystem 3 }

aiSPYKeypadCode1 OBJECT-TYPE
    SYNTAX  DisplayString
    ACCESS  read-write
    STATUS  mandatory
    DESCRIPTION
           "The aiSPY keypad access code for user #1.
            This can be up to six numeric characters."
    ::= { aiSPYKeypad  1 }

aiSPYKeypadName1 OBJECT-TYPE
    SYNTAX  DisplayString
    ACCESS  read-write
    STATUS  mandatory
    DESCRIPTION
           "The aiSPY keypad access name for user #1.
            This can be up to 20 characters."
    ::= { aiSPYKeypad  2 }

