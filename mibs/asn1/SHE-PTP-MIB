				shePtpDefaultDSControlPriority2, shePtpDefaultDSControlDomainNumber }
			STATUS current
			DESCRIPTION 
				"Notification for successful PTP default data set configuration."
			::= { shePtpEvents 2 }

		
		shePtpPortConfigDataModified NOTIFICATION-TYPE
			OBJECTS { shePtpPortDataSetControlTimeStamp, shePtpPortDataSetControlIndex, shePtpPortDataSetControlUnicastNegotiationEnabled, shePtpPortDataSetControlUnicastNegotiationDuration, shePtpPortDataSetControlUnicastLogQueryInterval, 
				shePtpPortDataSetControlVlanId, shePtpPortDataSetControlLogMeanSyncInterval, shePtpPortDataSetControlLogMinMeanDelayReqInterval, shePtpPortDataSetControlLogMeanAnnounceInterval, shePtpPortDataSetControlAnnounceReceiptTimeout, 
				shePtpPortDataSetControlLogMinPdelayReqInterval, shePtpPortDataSetControlAlarmSeverityIndex }
			STATUS current
			DESCRIPTION 
				"Notification for successfull PTP port config data configuration."
			::= { shePtpEvents 3 }

		
		shePtpAcceptableMasterTableEntryCreated NOTIFICATION-TYPE
			OBJECTS { shePtpAcceptableMasterControlTimeStamp, shePtpAcceptableMasterControlIndex, shePtpAcceptableMasterControlProtocolAddress, shePtpAcceptableMasterControlPriority, shePtpAcceptableMasterControlUnicast, 
				shePtpAcceptableMasterControlMinTimingPktSize }
			STATUS current
			DESCRIPTION 
				"Notification for successfull create of PTP acceptable master table entry."
			::= { shePtpEvents 4 }

		
		shePtpAcceptableMasterTableEntryModified NOTIFICATION-TYPE
			OBJECTS { shePtpAcceptableMasterControlTimeStamp, shePtpAcceptableMasterControlIndex, shePtpAcceptableMasterControlProtocolAddress, shePtpAcceptableMasterControlPriority, shePtpAcceptableMasterControlUnicast, 
				shePtpAcceptableMasterControlMinTimingPktSize }
			STATUS current
			DESCRIPTION 
				"Notification for successfull modify of PTP acceptable master table entry."
			::= { shePtpEvents 5 }

		
		shePtpAcceptableMasterTableEntryDeleted NOTIFICATION-TYPE
			OBJECTS { shePtpAcceptableMasterControlTimeStamp, shePtpAcceptableMasterControlIndex }
			STATUS current
			DESCRIPTION 
				"Notification for successfull delete of PTP acceptable master table entry."
			::= { shePtpEvents 6 }

		
		shePtpUnicastDiscoveryMasterTableEntryCreated NOTIFICATION-TYPE
			OBJECTS { shePtpUnicastDiscoveryMasterControlTimeStamp, shePtpUnicastDiscoveryMasterControlPortDsNumber, shePtpUnicastDiscoveryMasterControlIndex, shePtpUnicastDiscoveryMasterControlProtocolAddress }
			STATUS current
			DESCRIPTION 
				"Notification for successfull create of PTP unicast discovery master table entry."
			::= { shePtpEvents 7 }

		
		shePtpUnicastDiscoveryMasterTableEntryModified NOTIFICATION-TYPE
			OBJECTS { shePtpUnicastDiscoveryMasterControlTimeStamp, shePtpUnicastDiscoveryMasterControlPortDsNumber, shePtpUnicastDiscoveryMasterControlIndex, shePtpUnicastDiscoveryMasterControlProtocolAddress }
			STATUS current
			DESCRIPTION 
				"Notification for successfull modify of PTP unicast discovery master table entry."
			::= { shePtpEvents 8 }

		
		shePtpUnicastDiscoveryMasterTableEntryDeleted NOTIFICATION-TYPE
			OBJECTS { shePtpUnicastDiscoveryMasterControlTimeStamp, shePtpUnicastDiscoveryMasterControlPortDsNumber, shePtpUnicastDiscoveryMasterControlIndex }
			STATUS current
			DESCRIPTION 
				"Notification for successfull delete of PTP unicast discovery master table entry."
			::= { shePtpEvents 9 }

		
		shePtpForeignMasterTableChanged NOTIFICATION-TYPE
			OBJECTS { shePtpPortDataSetControlTimeStamp, shePtpForeignMasterDSNumberForeignRecords, shePtpForeignMasterDSClockIdentity, shePtpForeignMasterDSPortNumber, shePtpForeignMasterDSAnnounceMessages, 
				shePtpForeignMasterDSNetworkProtocol, shePtpForeignMasterDSProtocolAddressLength, shePtpForeignMasterDSProtocolAddress }
			STATUS current
			DESCRIPTION 
				"This trap will be send if the foreign master table changes because of new detected masters or a
				alraedy detected entry is lost."
			::= { shePtpEvents 10 }

		
		shePtpAlarms OBJECT IDENTIFIER ::= { shePtpNotifications 2 }

		
		shePtpSynchronizationAlarm NOTIFICATION-TYPE
			OBJECTS { sheAlarmTimeAndDate, sheAlarmReportingSource, sheAlarmReportingSourceType, sheAlarmSeverity }
			STATUS current
			DESCRIPTION 
				"This alarm will be activated if the ptp sync module is enabled and no external sync master
				was found or connection could not be established or was lost."
			::= { shePtpAlarms 1 }

		
		shePtpIpHostInterfaceAlarm NOTIFICATION-TYPE
			OBJECTS { sheAlarmTimeAndDate, sheAlarmReportingSource, sheAlarmReportingSourceType, sheAlarmSeverity }
			STATUS current
			DESCRIPTION 
				"This alarm will be activated:
				1. If the ptp sync module is enabled and no valid IpHostInterface configuration could be detected.
				   -> Configure a valid host interface via sheSystemIpHostTable.
				2. The used interface was reconfigured during ongoing PTP session.
				   -> The PTP module must be restarted.
				"
			::= { shePtpAlarms 2 }

		
		shePtpConformance OBJECT IDENTIFIER ::= { shePtpMIB 100 }

		
