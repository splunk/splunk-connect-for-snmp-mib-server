			unless radius PreAuth is specified."

	::= { sessionConfigEntry 13 }

	sessionConfigPreAuthTimeout	OBJECT-TYPE
		SYNTAX		INTEGER (4..30)
		MAX-ACCESS	read-create
		STATUS		current
		DESCRIPTION
			"The maximum number of seconds allowed for called
			number authentication to complete."

		DEFVAL		{ 10 }
	::= { sessionConfigEntry 14 }

	sessionConfigPreAuthTimeoutAction	OBJECT-TYPE
		SYNTAX		INTEGER {
				    accept(1),
				    reject(2)
				}
		MAX-ACCESS	read-create
		STATUS		current
		DESCRIPTION
			"The action to take when the pre-auth times out
			without authentication."

		DEFVAL		{ accept }
	::= { sessionConfigEntry 15 }

	sessionConfigCheckpointTime	OBJECT-TYPE
		SYNTAX		INTEGER (0..15)
		MAX-ACCESS	read-create
		STATUS		current
		DESCRIPTION
			"The number of minutes between accounting checkpoint
			records. 0 disables checkpoint records."

		DEFVAL		{ 0 }
	::= { sessionConfigEntry 16 }

	sessionConfigTerminateEnable	OBJECT-TYPE
		SYNTAX		INTEGER {
				    disabled(0),
				    enabled(1)
				}
		MAX-ACCESS	read-create
		STATUS		current
		DESCRIPTION
			"Controls whether or not the session can be terminated
			by a remote NMS."

		DEFVAL		{ disabled }
	::= { sessionConfigEntry 17 }

	sessionConfigTerminateSecret	OBJECT-TYPE
		SYNTAX		DisplayString (SIZE (0..32))
		MAX-ACCESS	read-create
		STATUS		current
		DESCRIPTION
