	::= { ipAaaClientPortEntry 3 }

	ipAaaClientPortStartingPort	OBJECT-TYPE
		SYNTAX		INTEGER (1..65535)
		MAX-ACCESS	read-create
		STATUS		current
		DESCRIPTION
			"UDP port number"

		DEFVAL		{ 2048 }
	::= { ipAaaClientPortEntry 11 }

	ipAaaClientPortPortCount	OBJECT-TYPE
		SYNTAX		INTEGER (1..10)
		MAX-ACCESS	read-create
		STATUS		current
		DESCRIPTION
			"count of UDP ports"

		DEFVAL		{ 1 }
	::= { ipAaaClientPortEntry 12 }


	ipAaaRadiusConfigTable	OBJECT-TYPE
