      SYNTAX  INTEGER (1..255)  
      ACCESS  read-only  
      STATUS  mandatory  
      DESCRIPTION  
      "Id of the group to which the port belongs."  
      ::= { lsePortEntry 1 }  
  
lsePortId OBJECT-TYPE  
      SYNTAX  INTEGER (1..255)  
      ACCESS  read-only  
      STATUS  mandatory  
      DESCRIPTION  
      "Number of LanSwitch port in the board."  
      ::= { lsePortEntry 2 }  
  
lsePortPolarity OBJECT-TYPE  
      SYNTAX  INTEGER {  
	on(1),  
	off(2),  
	notSupported(255)  
	}  
      ACCESS read-write  
      STATUS mandatory  
		DESCRIPTION  
      "Setting the value of this attribute to ON activates the automatic port polarity  
      detection mode."  
      ::= { lsePortEntry 3 }  
  
  
  
