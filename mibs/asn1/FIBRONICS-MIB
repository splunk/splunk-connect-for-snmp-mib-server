	SYNTAX  INTEGER { perPort(1), perRing(2), disabled(3),  
			notSupported(255) }  
	ACCESS  read-write  
	STATUS  mandatory  
	DESCRIPTION  
	"Security scheme selected by the user."  
	::= { tokRingEntry 14 }  
  
