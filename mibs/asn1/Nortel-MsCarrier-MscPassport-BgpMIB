MscVrIpBgpImportProvEntry ::= SEQUENCE {
   mscVrIpBgpImportPeerAs
      Unsigned32,
   mscVrIpBgpImportPeerIpAddress
      IpAddress,
   mscVrIpBgpImportOriginAs
      Unsigned32,
