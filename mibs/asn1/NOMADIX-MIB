     localWebImagesTable OBJECT-TYPE
       SYNTAX      SEQUENCE OF LocalWebImagesTableEntry
       MAX-ACCESS      not-accessible
       STATUS      current
       DESCRIPTION
         "Table of local Web image names."
       ::= { localWebImages 1 }

     localWebImagesTableEntry OBJECT-TYPE
       SYNTAX      LocalWebImagesTableEntry
       MAX-ACCESS      not-accessible
       STATUS      current
       DESCRIPTION
         "Local Web Images table entry."
       INDEX       { localWebImagesTableIndex }
       ::= { localWebImagesTable 1 }

     LocalWebImagesTableEntry ::= SEQUENCE {
           localWebImagesTableIndex     Integer32,
           localWebImagesTableName      DisplayString,
           localWebImagesTableStatus    RowStatus
           }

     localWebImagesTableIndex OBJECT-TYPE
       SYNTAX      Integer32
       MAX-ACCESS  read-only
       STATUS      current
       DESCRIPTION
         " Local Web Images Index"
       ::= { localWebImagesTableEntry 1 }

     localWebImagesTableName OBJECT-TYPE
       SYNTAX      DisplayString(SIZE(0..237))
       MAX-ACCESS      read-write
       STATUS      current
       DESCRIPTION
         "Local Web Image entries."
       ::= { localWebImagesTableEntry 2 }

     localWebImagesTableStatus OBJECT-TYPE
       SYNTAX      RowStatus
       MAX-ACCESS      read-write
       STATUS      current
       DESCRIPTION
         "Status of local Web images table entry."
       ::= { localWebImagesTableEntry 3 }
--
--   IPSec
--

   ipsecVpnOn     OBJECT-TYPE
     SYNTAX         INTEGER { disabled(0), enabled(1) }
     MAX-ACCESS     read-write
     STATUS         current
     DESCRIPTION
       " Setting this value to (1) enables IPSec over the network interface.
         Setting it to zero disables it. Valid license is required. When this variable
         is changed, system must be rebooted for the change to take effect. (NOTE: This
         is not to be confused with the MIB variable ipsecOn, which pertains to the
         passthru of IPSec traffic to/from a subscriber host.)"
     ::= { ipsec 1 }

   ipsecPeers    OBJECT IDENTIFIER ::= { ipsec 4 }
   ipsecPolicies    OBJECT IDENTIFIER ::= { ipsec 5 }
   ipsecSaStatus    OBJECT IDENTIFIER ::= { ipsec 6 }

--
-- IPSec PEER TABLE
--
     ipsecPeerTable OBJECT-TYPE
       SYNTAX      SEQUENCE OF IpsecPeerTableEntry
       MAX-ACCESS  not-accessible
       STATUS  current
       DESCRIPTION
          "List of IPSec peers."
       ::= { ipsecPeers 1 }

     ipsecPeerTableEntry OBJECT-TYPE
       SYNTAX      IpsecPeerTableEntry
       MAX-ACCESS  not-accessible
       STATUS  current
       DESCRIPTION
          "Parameters for a specific IPSec peer."
       INDEX  { ipsecPeerIndex }
       ::= { ipsecPeerTable 1 }


     IpsecPeerTableEntry ::=
       SEQUENCE
       {
        ipsecPeerIndex Integer32,
        ipsecPeerIpAddr   IpAddress,
        ipsecPeerAuthMethod    INTEGER,
        ipsecPeerSharedKey     DisplayString,
        ipsecPeerPrivkeyFile   DisplayString,
        ipsecPeerCertFile      DisplayString,
        ipsecPeerEncDesOk INTEGER,
        ipsecPeerEnc3DesOk INTEGER,
        ipsecPeerHashMd5Ok INTEGER,
        ipsecPeerHashShaOk INTEGER,
        ipsecPeerKeyStrength INTEGER,
        ipsecPeerLifetime Integer32,
        ipsecPeerStatus        RowStatus
       }

     ipsecPeerIndex OBJECT-TYPE
       SYNTAX     Integer32
       MAX-ACCESS  read-only
       STATUS     current
       DESCRIPTION
          "IPSec Peer Index"
       ::= { ipsecPeerTableEntry 1 }

     ipsecPeerIpAddr OBJECT-TYPE
       SYNTAX      IpAddress
       MAX-ACCESS  read-write
       STATUS  current
       DESCRIPTION
          "IP Address of the IPSec peer."
       ::= { ipsecPeerTableEntry 2 }

     ipsecPeerAuthMethod   OBJECT-TYPE
       SYNTAX       INTEGER { preSharedKey(0), x509Certificate(1) }
       MAX-ACCESS       read-write
       STATUS       current
       DESCRIPTION
          "Method to be used to authenticate and be authenticated by IPSec peer"
       ::= { ipsecPeerTableEntry 3 }

     ipsecPeerSharedKey   OBJECT-TYPE
       SYNTAX       DisplayString(SIZE(1..128))
       MAX-ACCESS       read-write
       STATUS       current
       DESCRIPTION
          "The key to be used if ipsecPeerAuthMethod is preSharedKey."
       ::= { ipsecPeerTableEntry 4 }

     ipsecPeerPrivkeyFile   OBJECT-TYPE
       SYNTAX       DisplayString(SIZE(1..20))
       MAX-ACCESS       read-write
       STATUS       current
       DESCRIPTION
          "The filename (within the /flash/ipseccrt directory) of the file containing
           the private key to be used for X509 authentication. Only needed if
           ipsecPeerAuthMethod is x509Certificate"
      ::= { ipsecPeerTableEntry 5 }

     ipsecPeerCertFile   OBJECT-TYPE
       SYNTAX       DisplayString(SIZE(1..20))
       MAX-ACCESS       read-write
       STATUS       current
       DESCRIPTION
          "The filename (within the /flash/ipseccrt directory) of the file containing
           the X.509 certificate to be used for X509 authentication. Only needed if
           ipsecPeerAuthMethod is x509Certificate"
      ::= { ipsecPeerTableEntry 6 }

     ipsecPeerEncDesOk   OBJECT-TYPE
       SYNTAX       INTEGER { notAllowed(0), allowed(1) }
       MAX-ACCESS       read-write
       STATUS       current
       DESCRIPTION
          "Indicates whether DES encryption is allowed to be used on the IKE channel
          between the NSE and this peer."
       ::= { ipsecPeerTableEntry 7 }

     ipsecPeerEnc3DesOk   OBJECT-TYPE
       SYNTAX       INTEGER { notAllowed(0), allowed(1) }
       MAX-ACCESS       read-write
       STATUS       current
       DESCRIPTION
          "Indicates whether 3DES encryption is allowed to be used on the IKE channel
          between the NSE and this peer."
       ::= { ipsecPeerTableEntry 8 }

     ipsecPeerHashMd5Ok   OBJECT-TYPE
       SYNTAX       INTEGER { notAllowed(0), allowed(1) }
       MAX-ACCESS       read-write
       STATUS       current
       DESCRIPTION
          "Indicates whether MD5 hashing is allowed to be used on the IKE channel
          between the NSE and this peer."
       ::= { ipsecPeerTableEntry 9 }

     ipsecPeerHashShaOk   OBJECT-TYPE
       SYNTAX       INTEGER { notAllowed(0), allowed(1) }
       MAX-ACCESS       read-write
       STATUS       current
       DESCRIPTION
          "Indicates whether SHA hashing is allowed to be used on the IKE channel
          between the NSE and this peer."
       ::= { ipsecPeerTableEntry 10 }

     ipsecPeerKeyStrength   OBJECT-TYPE
       SYNTAX       INTEGER { key768-bit(0), key1024-bit(1) }
       MAX-ACCESS       read-write
       STATUS       current
       DESCRIPTION
          "The key strength to be used on the IKE channel between the NSE and this peer"
       ::= { ipsecPeerTableEntry 11 }

     ipsecPeerLifetime OBJECT-TYPE
       SYNTAX     Integer32
       MAX-ACCESS  read-write
       STATUS     current
       DESCRIPTION
          "The lifetime (in seconds) of the IKE channel between the NSE and this peer,
          before it needs to be torn down and re-established."
       ::= { ipsecPeerTableEntry 12 }

     ipsecPeerStatus   OBJECT-TYPE
       SYNTAX       RowStatus
       MAX-ACCESS       read-write
       STATUS       current
       DESCRIPTION
          "Status of the IPSec peer table entry."
       ::= { ipsecPeerTableEntry 20}

--
-- IPSec POLICY TABLE
--
     ipsecPolicyTable OBJECT-TYPE
       SYNTAX      SEQUENCE OF IpsecPolicyTableEntry
       MAX-ACCESS  not-accessible
       STATUS  current
       DESCRIPTION
          "List of IPSec security policies."
       ::= { ipsecPolicies 1 }

     ipsecPolicyTableEntry OBJECT-TYPE
       SYNTAX      IpsecPolicyTableEntry
       MAX-ACCESS  not-accessible
       STATUS  current
       DESCRIPTION
          "Parameters for a specific IPSec security policy."
       INDEX  { ipsecPolicyIndex }
       ::= { ipsecPolicyTable 1 }


     IpsecPolicyTableEntry ::=
       SEQUENCE
       {
        ipsecPolicyIndex Integer32,
        ipsecPolicyPeerIp IpAddress,
        ipsecPolicyProtocol INTEGER,
        ipsecPolicyRemIp IpAddress,
        ipsecPolicyRemSubnet IpAddress,
        ipsecPolicyRemPort INTEGER,
        ipsecPolicyUseNetIp INTEGER,
        ipsecPolicyLocIp IpAddress,
        ipsecPolicyLocSubnet IpAddress,
        ipsecPolicyLocPort INTEGER,
        ipsecPolicyType INTEGER,
        ipsecPolicyDirType INTEGER,
        ipsecPolicyEncDesOk INTEGER,
        ipsecPolicyEnc3desOk INTEGER,
        ipsecPolicyEncNullOk INTEGER,
        ipsecPolicyAuthMd5Ok INTEGER,
        ipsecPolicyAuthShaOk INTEGER,
        ipsecPolicyAuthNullOk INTEGER,
        ipsecPolicyPfsStrength INTEGER,
        ipsecPolicyMaxLifetime Integer32,
        ipsecPolicyMaxLifesize Integer32,
        ipsecPolicyAutoRenew INTEGER,
        ipsecPolicyStatus      RowStatus
       }

     ipsecPolicyIndex OBJECT-TYPE
       SYNTAX     Integer32
       MAX-ACCESS  read-only
       STATUS     current
       DESCRIPTION
          "IPSec Policy Index"
       ::= { ipsecPolicyTableEntry 1 }

     ipsecPolicyPeerIp OBJECT-TYPE
       SYNTAX      IpAddress
       MAX-ACCESS  read-write
       STATUS  current
       DESCRIPTION
          "IP Address of the IPSec peer for this policy. There must be an entry in
          the peer table for this peer"
       ::= { ipsecPolicyTableEntry 2 }

     ipsecPolicyProtocol   OBJECT-TYPE
       SYNTAX       INTEGER
       MAX-ACCESS       read-write
       STATUS       current
       DESCRIPTION
          "The IP protocol number (e.g. 6 for TCP) of the protocol this policy pertains
          to. a zero means it pertains to ANY protocol."
       ::= { ipsecPolicyTableEntry 3 }

     ipsecPolicyRemIp OBJECT-TYPE
       SYNTAX      IpAddress
       MAX-ACCESS  read-write
       STATUS  current
       DESCRIPTION
          "IP Address of the remote host/subnet to which this policy pertains."
       ::= { ipsecPolicyTableEntry 4 }

     ipsecPolicyRemSubnet OBJECT-TYPE
       SYNTAX      IpAddress
       MAX-ACCESS  read-write
       STATUS  current
       DESCRIPTION
          "Subnet mask of the remote subnet to which this policy pertains."
       ::= { ipsecPolicyTableEntry 5 }

     ipsecPolicyRemPort   OBJECT-TYPE
       SYNTAX       INTEGER
       MAX-ACCESS       read-write
       STATUS       current
       DESCRIPTION
          "The remote TCP/UDP port number to which this policy pertains. A zero means
          it pertains to ANY port number. This field is only used if the protocol
          number is 6 (TCP) or 17 (UDP)."
       ::= { ipsecPolicyTableEntry 6 }

     ipsecPolicyUseNetIp   OBJECT-TYPE
       SYNTAX       INTEGER { no(0), yes(1) }
       MAX-ACCESS       read-write
       STATUS       current
       DESCRIPTION
          "Indicates whether or not this policy pertains to traffic originated by
          and/or destined to the NSE's network interface IP address. Otherwise it
          would pertain to a device on the subscriber network."
       ::= { ipsecPolicyTableEntry 7 }

     ipsecPolicyLocIp OBJECT-TYPE
       SYNTAX      IpAddress
       MAX-ACCESS  read-write
       STATUS  current
       DESCRIPTION
          "If ipsecPolicyUseNetIp is no, this is the IP Address of the subscriber-side
          host/subnet to which this policy pertains."
       ::= { ipsecPolicyTableEntry 8 }

     ipsecPolicyLocSubnet OBJECT-TYPE
       SYNTAX      IpAddress
       MAX-ACCESS  read-write
       STATUS  current
       DESCRIPTION
          "If ipsecPolicyUseNetIp is no, this is the Subnet mask of the subscriber-side
          subnet to which this policy pertains."
       ::= { ipsecPolicyTableEntry 9 }

     ipsecPolicyLocPort   OBJECT-TYPE
       SYNTAX       INTEGER
       MAX-ACCESS       read-write
       STATUS       current
       DESCRIPTION
          "The local TCP/UDP port number to which this policy pertains. A zero means
          it pertains to ANY port number. This field is only used if the protocol
          number is 6 (TCP) or 17 (UDP)."
       ::= { ipsecPolicyTableEntry 10 }

     ipsecPolicyType   OBJECT-TYPE
       SYNTAX       INTEGER { discard(0), bypass(1), esp(2), ah(3) }
       MAX-ACCESS       read-write
       STATUS       current
       DESCRIPTION
          "The type of IPSec security policy this is, i.e., what type of action to take
          on packets that fit the traffic selectors."
       ::= { ipsecPolicyTableEntry 11 }

     ipsecPolicyDirType   OBJECT-TYPE
       SYNTAX       INTEGER { inOnly(0), outOnly(1), inAndOut(2) }
       MAX-ACCESS       read-write
       STATUS       current
       DESCRIPTION
          "The traffic direction to which this policy pertains. Used only for discard or
          bypass policies (ESP and AH tunnels always pertain to both directions)"
       ::= { ipsecPolicyTableEntry 12 }

     ipsecPolicyEncDesOk   OBJECT-TYPE
       SYNTAX       INTEGER { notAllowed(0), allowed(1) }
       MAX-ACCESS       read-write
       STATUS       current
       DESCRIPTION
          "Indicates whether DES encryption is allowed to be used on the security
          association based on this policy. Pertains only to ESP tunnels."
       ::= { ipsecPolicyTableEntry 13 }

     ipsecPolicyEnc3desOk   OBJECT-TYPE
       SYNTAX       INTEGER { notAllowed(0), allowed(1) }
       MAX-ACCESS       read-write
       STATUS       current
       DESCRIPTION
          "Indicates whether 3DES encryption is allowed to be used on the security
          association based on this policy. Pertains only to ESP tunnels."
       ::= { ipsecPolicyTableEntry 14 }

     ipsecPolicyEncNullOk   OBJECT-TYPE
       SYNTAX       INTEGER { notAllowed(0), allowed(1) }
       MAX-ACCESS       read-write
       STATUS       current
       DESCRIPTION
          "Indicates whether NULL encryption (i.e., not encrypted) is allowed to be used on
          the security association based on this policy. Pertains only to ESP tunnels."
       ::= { ipsecPolicyTableEntry 15 }

     ipsecPolicyAuthMd5Ok   OBJECT-TYPE
       SYNTAX       INTEGER { notAllowed(0), allowed(1) }
       MAX-ACCESS       read-write
       STATUS       current
       DESCRIPTION
          "Indicates whether MD5 authentication is allowed to be used on the security
          association based on this policy. Pertains to both ESP and AH tunnels."
       ::= { ipsecPolicyTableEntry 16 }

     ipsecPolicyAuthShaOk   OBJECT-TYPE
       SYNTAX       INTEGER { notAllowed(0), allowed(1) }
       MAX-ACCESS       read-write
       STATUS       current
       DESCRIPTION
          "Indicates whether SHA authentication is allowed to be used on the security
          association based on this policy. Pertains to both ESP and AH tunnels."
       ::= { ipsecPolicyTableEntry 17 }

     ipsecPolicyAuthNullOk   OBJECT-TYPE
       SYNTAX       INTEGER { notAllowed(0), allowed(1) }
       MAX-ACCESS       read-write
       STATUS       current
       DESCRIPTION
          "Indicates whether no authentication is allowed to be used on the security
          association based on this policy. Pertains only to ESP tunnels."
       ::= { ipsecPolicyTableEntry 18 }

     ipsecPolicyPfsStrength   OBJECT-TYPE
       SYNTAX       INTEGER { pfsNone(0), pfs768(1), pfs1024(2) }
       MAX-ACCESS       read-write
       STATUS       current
       DESCRIPTION
          "Indicates whether perfect forward secrecy is to be used on the security
          association based on this policy, and if so what lever of keying strength to
          use. Pertains to either ESP or AH tunnels."
       ::= { ipsecPolicyTableEntry 19 }

     ipsecPolicyMaxLifetime   OBJECT-TYPE
       SYNTAX       Integer32
       MAX-ACCESS   read-write
       STATUS       current
       DESCRIPTION
          "The maximum lifetime (in seconds) a security association based on this policy
          may stay up before it must either be torn down or renewed. A zero means there
          is no maximum."
       ::= { ipsecPolicyTableEntry 20 }

     ipsecPolicyMaxLifesize   OBJECT-TYPE
       SYNTAX       Integer32
       MAX-ACCESS   read-write
       STATUS       current
       DESCRIPTION
          "The maximum amount of data (in bytes) that may pass thru a tunnel based
          on this policy before it must either be torn down or renewed. A zero means
          there is no maximum."
       ::= { ipsecPolicyTableEntry 21 }

     ipsecPolicyAutoRenew   OBJECT-TYPE
       SYNTAX       INTEGER { no(0), yes(1) }
       MAX-ACCESS   read-write
       STATUS       current
       DESCRIPTION
          "Indicates whether the security association based on this policy should be
          automatically renewed or not. Used only if either ipsecPolicyMaxLifesize or
          ipsecPolicyMaxLifetime is non-zero."
       ::= { ipsecPolicyTableEntry 22 }

     ipsecPolicyStatus   OBJECT-TYPE
       SYNTAX       RowStatus
       MAX-ACCESS       read-write
       STATUS       current
       DESCRIPTION
          "Status of the IPSec policy table entry."
       ::= { ipsecPolicyTableEntry 40}


--
-- IPSec Security Association STATUS TABLE
--
     ipsecSaStatusTable OBJECT-TYPE
       SYNTAX      SEQUENCE OF IpsecSaStatusTableEntry
       MAX-ACCESS  not-accessible
       STATUS  current
       DESCRIPTION
          "List of the status of security associations  corresponding to
          configured IPSec security policies."
       ::= { ipsecSaStatus 1 }

     ipsecSaStatusTableEntry OBJECT-TYPE
       SYNTAX      IpsecSaStatusTableEntry
       MAX-ACCESS  not-accessible
       STATUS  current
       DESCRIPTION
          "Information about the status of an IPSec security association."
       INDEX  { ipsecSaStatusIndex }
       ::= { ipsecSaStatusTable 1 }


     IpsecSaStatusTableEntry ::=
       SEQUENCE
       {
        ipsecSaStatusIndex Integer32,
        ipsecSaStatusState      INTEGER,
        ipsecSaStatusStatus     RowStatus
       }

     ipsecSaStatusIndex OBJECT-TYPE
       SYNTAX     Integer32
       MAX-ACCESS  read-only
       STATUS     current
       DESCRIPTION
          "IPSec Security Association Status Index"
       ::= { ipsecSaStatusTableEntry 1 }

     ipsecSaStatusState   OBJECT-TYPE
       SYNTAX       INTEGER { ipsecUp(0), ipsecDisabled(1), notConfiguredInStack(2),
        noIkeChannelToPeer(3),notNegotiatedWithPeer(4) }
       MAX-ACCESS       read-only
       STATUS       current
       DESCRIPTION
          "Indicates the state ofwhether the security association corresponding to the configured
          security policy is presently up or down."
       ::= { ipsecSaStatusTableEntry 2 }

     ipsecSaStatusStatus   OBJECT-TYPE
       SYNTAX       RowStatus
       MAX-ACCESS       read-write
       STATUS       current
       DESCRIPTION
          "Status of the IPSec security association table entry."
       ::= { ipsecSaStatusTableEntry 20}



END
