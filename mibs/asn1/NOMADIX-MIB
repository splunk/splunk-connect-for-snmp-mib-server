NOMADIX-MIB DEFINITIONS ::= BEGIN

  IMPORTS
     enterprises,
     Counter32,
     Integer32,
     IpAddress,
     OBJECT-TYPE,
--      OBJECT-IDENTITY,
     NOTIFICATION-TYPE,
     MODULE-IDENTITY
         FROM SNMPv2-SMI

     MacAddress, RowStatus
         FROM SNMPv2-TC;

  --
  -- Nomadix enterprise module definition
  --

     nomadix MODULE-IDENTITY
          LAST-UPDATED "200501270000Z"
          ORGANIZATION "Nomadix, Inc"
          CONTACT-INFO "Nomadix, Inc
                        1100 Business Center Circle
                        Suite 100
                        Newbury Park, CA 91320
                        +1 818 597 1500
                        info@nomadix.com"
          DESCRIPTION
          "MIB definition for Nomadix AG-2000w wireless gateway."
          ::= {  enterprises 3309 }


  DisplayString ::= OCTET STRING


