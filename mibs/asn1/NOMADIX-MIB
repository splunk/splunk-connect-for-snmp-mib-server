--
--   Dynamic DNS
--
     dynDnsEnable   OBJECT-TYPE
        SYNTAX       INTEGER { disabled(0), enabled(1) }
        MAX-ACCESS   read-write
        STATUS       current
        DESCRIPTION
        "Set to 0 to disable Dynamic DNS, 1 to enable Dynamic DNS"
        ::= { dyndns 0 }

     dynDnsProtocol OBJECT-TYPE
        SYNTAX       INTEGER { dynDnsOrgProto(0), dynDnsOrgProtoSecure(1) }
        MAX-ACCESS   read-write
        STATUS       current
        DESCRIPTION
        "Set to 0 to set protocol to dyndns.org, or set protocol to 1 to set it to dyndns.org (secure) protocol"
        ::= { dyndns 1 }

     dynDnsServer OBJECT-TYPE
        SYNTAX      DisplayString(SIZE(0..255))
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
        "Server name given by dynamic DNS provider. Please specify only domain name, without http://, '/', etc"
        ::= { dyndns 2 }

     dynDnsPort      OBJECT-TYPE
        SYNTAX       INTEGER { dynDnsPort80(0), dynDnsPort8245(1), dynDnsPort443(2) }
        MAX-ACCESS   read-write
        STATUS       current
        DESCRIPTION
        "Set port on which to connect to dynamic dns provider. NOTE: port 443 may only be chosen if protocol is dynDnsOrgProtoSecure (dyndns.org (secure)). For non-secure version of dyndns.org protocol: set to 0 for port 80; set to 1 for port 8245. For secure version: set port to 443"
        ::= { dyndns 3 }

     dynDnsHostname OBJECT-TYPE
        SYNTAX      DisplayString(SIZE(0..255))
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
        "Hostname which ip address of the NSE will be mapped to. Please specify only domain name, without http://, '/', etc"
        ::= { dyndns 4 }

     dynDnsUsername OBJECT-TYPE
        SYNTAX      DisplayString(SIZE(0..255))
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
        "Username associated with dynamic dns account"
        ::= { dyndns 5 }

     dynDnsPassword OBJECT-TYPE
        SYNTAX      DisplayString(SIZE(0..255))
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
        "Password associated with dynamic dns account."
        ::= { dyndns 6 }

     dynDnsForceUpdate OBJECT-TYPE
        SYNTAX      INTEGER { disabled(0), enabled(1) }
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
        "When set to 1 will perform a forced dynamic DNS update. When set to false will have no effect. Will always return 0 on get. IMPORTANT: some Dynamic DNS Providers (e.g. dyndns.org) consider unnecessary updates (i.e. updates with unchanged IP addresses) abusive. Such updates may result in hostname / username being blocked."
        ::= { dyndns 7 }


