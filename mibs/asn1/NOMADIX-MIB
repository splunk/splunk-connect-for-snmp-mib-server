     -- AAA BILLOPT PLANS
        aaaBillingPlans OBJECT IDENTIFIER ::= { aaaBillingOption 19 }
           aaaBillingPlan0 OBJECT IDENTIFIER ::= { aaaBillingPlans 1 }
