--Table 25 Raw Counters for Static and PNNI Routing Common Statistic Table
--------------------------------------------------------------------------------

  nncRoutingStatsRawCommonTable OBJECT-TYPE
    SYNTAX       SEQUENCE OF NncRoutingStatsRawCommonEntry
    MAX-ACCESS   not-accessible
    STATUS       current
    DESCRIPTION  
    "The nncRoutingStatsRawCommonTable contains objects for monitoring the 
     raw counters for static and PNNI routing common stats."
  ::= { nncRoutingStatsObjects 25 }


  nncRoutingStatsRawCommonEntry OBJECT-TYPE
    SYNTAX      NncRoutingStatsRawCommonEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION  
    "An entry in the nncRoutingStatsRawCommonTable. Each conceptual row
    contains the raw counter for static and PNNI routing common stats."
    INDEX       { nncCallControlGroupNumber }
  ::= { nncRoutingStatsRawCommonTable 1 }


  NncRoutingStatsRawCommonEntry ::=
    SEQUENCE {
      
      nncRoutingStatsRawCommonSuccessRoutedCallsOrigFromLocalSubs    Counter32,
      nncRoutingStatsRawCommonSuccessRtedCallsTransitedViaThisNode	Counter32,
      nncRoutingStatsRawCommonSuccessRoutedCallsTermToLocalSubs		Counter32,
      nncRoutingStatsRawCommonSuccessRoutedLocalCalls	 		Counter32,
      nncRoutingStatsRawCommonCallsOrigFromLocalCalls			Counter32,
      nncRoutingStatsRawCommonCallsTermToLocalCalls			Counter32,
      nncRoutingStatsRawCommonLocalCallAttempts				Counter32,
      nncRoutingStatsRawCommonCallsClearedDueToNoRoutingTabEntry     Counter32,
      nncRoutingStatsRawCommonCrankbacksGeneratedByThisNode		Counter32,
      nncRoutingStatsRawCommonFailedCallsAtLocalSubs			Counter32,
      nncRoutingStatsRawCommonCallsSuccessRerouted		 	Counter32,
      nncRoutingStatsRawCommonCallsFailedInRerouting			Counter32
    }

  


  nncRoutingStatsRawCommonSuccessRoutedCallsOrigFromLocalSubs        	OBJECT-TYPE
    SYNTAX                                          			Counter32
