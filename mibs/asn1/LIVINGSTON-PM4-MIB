-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
-- Lucent Fault Management Group
-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
lucentPM4FaultMgmt	OBJECT IDENTIFIER	::= { lucentPM4Mib 5 }

-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
-- Lucent Fault Isolation Group
-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
lucentPM4FaultMgmtIsolation	OBJECT IDENTIFIER	::= { lucentPM4FaultMgmt 1 }

-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
-- Lucent Chassis Global Fault management table
-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
lucentPM4FaultMgmtChasTrap	OBJECT-TYPE
	SYNTAX	SEQUENCE OF LucentPM4FMChasTrapEntry
	ACCESS	not-accessible
	STATUS	mandatory
	DESCRIPTION
	"Chassis Trap table which indicates one of several Traps. The
	chassis trap table stores the previous history of the traps
	which can be retrieved by the management stations at a later
	time. This object is not-accessible and present for MIB clarity."
	::= { lucentPM4FaultMgmtIsolation 1 }

lucentPM4FMChasTrapEntry	OBJECT-TYPE
	SYNTAX	LucentPM4FMChasTrapEntry
	ACCESS	not-accessible
	STATUS	mandatory
	DESCRIPTION
	"Entry in the chassis Trap table. Each trap is uniquely
	identified by an index. This object is not accessible and
	present for MIB clarity."
	INDEX	{ lucentPM4FMChasTrapIndex }
	::= { lucentPM4FaultMgmtChasTrap 1 }

LucentPM4FMChasTrapEntry ::=
	SEQUENCE {
		lucentPM4FMChasTrapIndex
			INTEGER,
		lucentPM4FMChasBoardID
			INTEGER,
		lucentPM4FMChasTrapUnitType
			PMUnitType,
		lucentPM4FMChasTrapUnitIndex
			INTEGER,
		lucentPM4FMChasTrapStatus
			INTEGER,
		lucentPM4FMChasTrapSeverity
			INTEGER,
		lucentPM4FMChasTrapTimeStamp
			TimeTicks,
		lucentPM4FMChasTrapState
			INTEGER
	}

lucentPM4FMChasTrapIndex	OBJECT-TYPE
	SYNTAX	INTEGER
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION
	"Index into the Trap table on the agent. This table stores the
	previous 500 traps which can be retrieved at any given time."
	::= { lucentPM4FMChasTrapEntry 1 }

lucentPM4FMChasTrapBoardID	OBJECT-TYPE
	SYNTAX	INTEGER
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION
	"Board ID is the board number for which this trap is stored. If
	the trap is for an auxillary device such as a power supply or
	fan, this value is set to zero."
	::= { lucentPM4FMChasTrapEntry 2 }

lucentPM4FMChasTrapUnitType	OBJECT-TYPE
	SYNTAX	PMUnitType
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION
	"Uniquely specifies the unit type for this trap. The unit can be
	a board or any other device in the chassis such as a fan or a power
	supply."
	::= { lucentPM4FMChasTrapEntry 3 }

lucentPM4FMChasTrapUnitIndex	OBJECT-TYPE
	SYNTAX	INTEGER
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION
	"Uniquely specifies the unit index. The unit index is same as the
	ifIndex for T1/E1 interfaces, or the modemIndex for the modems or
	fan or power supply index for fan or power supplies."
	::= { lucentPM4FMChasTrapEntry 4 }

lucentPM4FMChasTrapStatus	OBJECT-TYPE
	SYNTAX	INTEGER {
		other(1),
		online(2),
		offline(3),
		maintenance(4),
		fault(5),
		notinstalled(6)
	}
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION
	"Trap status specifies the associated object in the Trap is online(2), 
	offline(3), maintenance(4) or fault(5)."
	::= { lucentPM4FMChasTrapEntry 5 }

lucentPM4FMChasTrapSeverity	OBJECT-TYPE
	SYNTAX	PMAlarmType
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION
	"Trap severity specifies the severity of the Trap for the associated
	object."
	::= { lucentPM4FMChasTrapEntry 6 }

lucentPM4FMChasTrapTimeStamp	OBJECT-TYPE
	SYNTAX	TimeTicks
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION
	"This object stores the timestamp of this trap."
	::= { lucentPM4FMChasTrapEntry 7 }

lucentPM4FMChasTrapState	OBJECT-TYPE
	SYNTAX	INTEGER {
		trapsent(1),
		ackdue(2),
		acked(3)
	}
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION
	"This object stores the Trap state of this trap."
	::= { lucentPM4FMChasTrapEntry 8 }

-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
-- Lucent Fault management objects
-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
lucentPM4FMBoardIndex	OBJECT-TYPE
	SYNTAX	INTEGER
	ACCESS	not-accessible
	STATUS	mandatory
	DESCRIPTION
	"Board index uniquely specifies the board in the chassis. 
	This object is set to zero for power supplies, fans and
	other auxillary devices. This object is not accessible 
	through Get, Get-Next or Set PDUs. It is sent out as part
	of the Trap."
	::= { lucentPM4FaultMgmtIsolation 2 }

lucentPM4FMUnitIndex	OBJECT-TYPE
	SYNTAX	INTEGER
	ACCESS	not-accessible
	STATUS	mandatory
	DESCRIPTION
	"Unit index uniquely specifies the T1/E1 line, or modem or 
	any device (logical/physical) in the chassis. This object
	is not accessible through Get, Get-Next or Set PDUs. It is
	sent out as part of the Trap."
	::= { lucentPM4FaultMgmtIsolation 3 }

lucentPM4FMUnitType	OBJECT-TYPE
	SYNTAX	PMUnitType
	ACCESS	not-accessible
	STATUS	mandatory
	DESCRIPTION
	"Unit type specifies the T1/E1 line, or modem or any device
 	in the chassis. This object is not accessible through Get,
	Get-Next or Set PDUs. It is sent out as part of the Trap."
	::= { lucentPM4FaultMgmtIsolation 4 }

lucentPM4FMUnitTrapStatus	OBJECT-TYPE
	SYNTAX	INTEGER {
		other(1),
		offline(2),
		online(3),
		failed(4),
		restored(5),
		pwrwarn(6),
		tempwarn(7),
		temphot(8),
		dtrloss(9),
		carrierloss(10),
		renegotiation(11),
		los(12),		-- Loss of Signal
		ais(13),		-- Alarm Indication Signal
		redalarm(14), 	-- Loss of Framing
		yellowalarm(15),	-- Remote Alarm Indication (indicating remote red alarm)
		cv(16),		-- Code Violation
		crc(17),		-- Cyclic Redundancy Check
		bpv(18),		-- Bipolar Violations
		fer(19),		-- Framing Errors
		pll(20),		-- Phase Locked Loop
		es(21),		-- Errored Seconds
		ses(22),		-- Severely Errored Seconds
		sefs(23),		-- Severely Errored Framing Seconds
		uas(24),		-- Unavailable Seconds
		dm(25),		-- Degraded Minutes
		les(26),		-- Line Errored Seconds
		css(27),		-- Controlled Slip Seconds
		bes(28)		-- Bursty Errored Seconds
	}
	ACCESS	not-accessible
	STATUS	mandatory
	DESCRIPTION
	"Trap status specifies the associated object in the Trap. 
	This object is not accessible other than when produced as 
	the result of a trap."
	::= { lucentPM4FaultMgmtIsolation 5 }

lucentPM4FMUnitTrapSeverity	OBJECT-TYPE
	SYNTAX	PMAlarmType
	ACCESS	not-accessible
	STATUS	mandatory
	DESCRIPTION
	"Trap severity specifies the severity of the Trap for the associated
	object. This object is not accessible except when produced as the 
	result of a trap."
	::= { lucentPM4FaultMgmtIsolation 6 }

-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
-- Lucent Alarm Configuration Group
-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
lucentPM4FMTrapConfig	OBJECT IDENTIFIER	::= { lucentPM4FaultMgmt 2 }

-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
-- Lucent Equipment Traps configuration table
-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
lucentPM4FMEqpTrapCfg	OBJECT-TYPE
	SYNTAX	SEQUENCE OF LucentPM4FMEqpTrapCfgEntry
	ACCESS	not-accessible
	STATUS	mandatory
	DESCRIPTION
	"Equipment Trap configuration table configure Traps. The
	objects in this table are used to enable or disable traps
	on a per board/interface/device basis. This object is 
	not-accessible and present for MIB clarity."
	::= { lucentPM4FMTrapConfig 1 }

lucentPM4FMEqpTrapCfgEntry	OBJECT-TYPE
	SYNTAX	LucentPM4FMEqpTrapCfgEntry
	ACCESS	not-accessible
	STATUS	mandatory
	DESCRIPTION
	"Entry in the equipment Trap config table. Each trap is 
	uniquely identified by the board ID, Unit type and unit
	index. For auxillary devices such as power supplies and
	fans, the board index will be zero, the unit index 
	identifies the units and the unit type specifies if the
	unit is a fan, power supplies etc. This object is not 
	accessible and present for MIB clarity."
	INDEX	{ lucentPM4FMEqpBoardIndex,
		  lucentPM4FMEqpUnitType,
		  lucentPM4FMEqpUnitIndex }
	::= { lucentPM4FMEqpTrapCfg 1 }

LucentPM4FMEqpTrapCfgEntry ::=
	SEQUENCE {
		lucentPM4FMEqpBoardIndex
			INTEGER,
		lucentPM4FMEqpUnitType
			PMUnitType,
		lucentPM4FMEqpunitIndex
			INTEGER,
		lucentPM4FMEqpTrapId
			INTEGER,
		lucentPM4FMEqpTrapCtl
			INTEGER,
		lucentPM4FMEqpTrapRepTimer
			INTEGER
	}

lucentPM4FMEqpBoardIndex	OBJECT-TYPE
	SYNTAX	INTEGER
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION
	"Board ID for which the Trap configuration need to apply.
	The board ID is zero if this trap configuration is for an
	auxillary device such as fans or power supplies."
	::= { lucentPM4FMEqpTrapCfgEntry 1 }

lucentPM4FMEqpUnitType	OBJECT-TYPE
	SYNTAX	PMUnitType
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION
	"Unit type alongwith the board index and unit index specifies
	uniquely the device/interface which is being configured."
	::= { lucentPM4FMEqpTrapCfgEntry 2 }

lucentPM4FMEqpUnitIndex	OBJECT-TYPE
	SYNTAX	INTEGER
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION
	"Unit index refers to the interface or sub-device such as a
	modem, serial port etc. For devices such as power supplies
	and fans this object is zero."
	::= { lucentPM4FMEqpTrapCfgEntry 3 }

lucentPM4FMEqpTrapId	OBJECT-TYPE
	SYNTAX	INTEGER {
		boardoffline(1),
		boardonline(2),
		pwrsupfail(3),
		pwrsuprestored(4),
		fanfail(5),
		fanrestored(6),
		boardtempwarn(7),
		boardtempnormal(8),
		boardtoohot(9),
		modemfail(10),
		linedown(11),
		lineup(12),
		linethresh(13),
		boardpwrshutdown(14),
		radiusauthfail(15)
	}
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION
	"Trap ID indicating the trap for which the configuration must
	apply."
	::= { lucentPM4FMEqpTrapCfgEntry 4 }

lucentPM4FMEqpTrapCtl	OBJECT-TYPE
	SYNTAX	INTEGER {
		disable(1),
		enable(2)
	}
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION
	"Trap control which configures the trap off(1) or on(2).
	When the trap is configured as off(1), the trap is not sent
	out to the management station. When configures as on(2),
	the trap is sent to all the management stations configured
	to receive the trap."
	::= { lucentPM4FMEqpTrapCfgEntry 5 }

lucentPM4FMEqpRepTimer	OBJECT-TYPE
	SYNTAX	INTEGER
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION
	"If the trap is to be repeated, this object specifies the time
	in seconds. When this object value is set to 0, it indicates
	the trap is non-repeat trap."
	::= { lucentPM4FMEqpTrapCfgEntry 6 }

-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
-- Lucent T1/E1 Threshold Traps configuration table
-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
lucentPM4FMT1E1ThreshTrapCfg	OBJECT-TYPE
	SYNTAX	SEQUENCE OF LucentPM4FMThreshT1E1TrapCfgEntry
	ACCESS	not-accessible
	STATUS	mandatory
	DESCRIPTION
	"T1/E1 Threshold Trap configuration table to configure the
	thresholds for various T1/E1 traps. This object is 
	not-accessible and present for MIB clarity."
	::= { lucentPM4FMTrapConfig 2 }

lucentPM4FMT1E1ThreshTrapCfgEntry	OBJECT-TYPE
	SYNTAX	LucentPM4FMThreshT1E1TrapCfgEntry
	ACCESS	not-accessible
	STATUS	mandatory
	DESCRIPTION
	"Entry in the T1/E1 threshold trap config table. Each 
	trap is uniquely identified by the board index, unit 
	type and unit index which is the T1/E1 interface number.
	This object is not accessible and present for MIB clarity."
	INDEX	{ lucentPM4FMThreshBoardIndex,
		  lucentPM4FMThreshUnitType,
		  lucentPM4FMThreshUnitIndex }
	::= { lucentPM4FMT1E1ThreshTrapCfg 1 }

LucentPM4FMThreshT1E1TrapCfgEntry ::=
	SEQUENCE {
		lucentPM4FMT1E1ThreshBoardIndex
			INTEGER,
		lucentPM4FMT1E1ThreshUnitType
			INTEGER,
		lucentPM4FMT1E1ThreshUnitIndex
			INTEGER,
		lucentPM4FMT1E1ThreshESs
			Gauge,
		lucentPM4FMT1E1ThreshSESs
			Gauge,
		lucentPM4FMT1E1ThreshSEFSs
			Gauge,
		lucentPM4FMT1E1ThreshUASs
			Gauge,
		lucentPM4FMT1E1ThreshCSSs
			Gauge,
		lucentPM4FMT1E1ThreshPCVs
			Gauge,
		lucentPM4FMT1E1ThreshLESs
			Gauge,
		lucentPM4FMT1E1ThreshBESs
			Gauge,
		lucentPM4FMT1E1ThreshDMs
			Gauge,
		lucentPM4FMT1E1ThreshTrapRepTimer
			INTEGER,
		lucentPM4FMT1E1ThreshTrapAck
			INTEGER
	}

lucentPM4FMT1E1ThreshBoardIndex	OBJECT-TYPE
	SYNTAX	INTEGER
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION
	"Board ID for which the Trap threshold configuration must
	apply. It includes boards 1 - 10 and other devices such as
	power supplies and fans etc."
	::= { lucentPM4FMT1E1ThreshTrapCfgEntry 1 }

lucentPM4FMT1E1ThreshUnitType	OBJECT-TYPE
	SYNTAX	PMUnitType
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION
	"Unit type for which the Trap threshold configuration 
	must be applied."
	::= { lucentPM4FMT1E1ThreshTrapCfgEntry 2 }

lucentPM4FMT1E1ThreshESs	OBJECT-TYPE
	SYNTAX  Gauge
	ACCESS  read-write
	STATUS  mandatory
	DESCRIPTION
	"The threshold for errored seconds on the interface. A trap is
	issued when this set limit is exceeded. When this threshold
	exceeds, the performance of the interface will degrade. A trap
	is generated to notify the adminstrator to take appropriate
	action."
	::= { lucentPM4FMT1E1ThreshTrapCfgEntry 3 }

lucentPM4FMT1E1ThreshSESs	OBJECT-TYPE
	SYNTAX  Gauge
	ACCESS  read-write
	STATUS  mandatory
	DESCRIPTION
	"The threshold for severely errored seconds on the interface.
	A trap is issued when this limit is exceeded. A severely errored
	seconds is a second with 320 or more path code violation error
	events or one or more out of frame defects or detected AIS defect."
	::= { lucentPM4FMT1E1ThreshTrapCfgEntry 4 }

lucentPM4FMT1E1ThreshSEFSs	OBJECT-TYPE
	SYNTAX	Gauge
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION
	"The threshold for severely errored framing seconds.
	A trap is issued when this threshold is exceeded. A severely
	errored framing second is a second with one or more frame
	defects or detected AIS defect."
	::= { lucentPM4FMT1E1ThreshTrapCfgEntry 5 }

lucentPM4FMT1E1ThreshUASs	OBJECT-TYPE
	SYNTAX 	Gauge
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION
	"The threshold for unavailable seconds. A trap is issued 
	when this set threshold is exceeded. Unavailable seconds
	are calculated by counting the number of seconds that the
	interface is unavailable from the onset of 10 SESs. Once
	unavailable, the interface becomes available at the onset
	of 10 contiguous no SESs."
	::= { lucentPM4FMT1E1ThreshTrapCfgEntry 6 }

lucentPM4FMT1E1ThreshCSSs	OBJECT-TYPE
	SYNTAX	Gauge
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION
	"The threshold for controlled slip seconds on the interface.
	A trap is issued when this set threshold is exceeded. A
	controlled slip second is a one-second interval containing
	one or more controlled slips."
	::= { lucentPM4FMT1E1ThreshTrapCfgEntry 7 }

lucentPM4FMT1E1ThreshPCVs	OBJECT-TYPE
	SYNTAX	Gauge
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION
	"The threshold for path code violations on the interface. 
	A trap is issued when this set threshold is exceeded. PCV
	is a frame syncronization bit error in the D4 and E1-noCRC
	format interfaces or a CRC error in the ESF (extended super
	frame) and E1-CRC interface formats."
	::= { lucentPM4FMT1E1ThreshTrapCfgEntry 8 }

lucentPM4FMT1E1ThreshLESs	OBJECT-TYPE
	SYNTAX	Gauge
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION
	"The threshold for line errored seconds on the interface.
	A trap is sent to the manager when this set threshold 
	is exceeded. A line errored second, according to T1M1.3
	is a second in which one or more line code violations
	were detected."
	::= { lucentPM4FMT1E1ThreshTrapCfgEntry 9 }

lucentPM4FMT1E1ThreshBESs	OBJECT-TYPE
	SYNTAX	Gauge
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION
	"The threshold for bursty errored seconds on the interface.
	A trap is sent to the manager when this set threshold 
	is exceeded. A bursty errored second is a second with fewer
	than 320 and more than 1 path code violations."
	::= { lucentPM4FMT1E1ThreshTrapCfgEntry 10 }

lucentPM4FMT1E1ThreshDMs	OBJECT-TYPE
	SYNTAX	Gauge
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION
	"The threshold for degraded minutes on the interface.
	A trap is sent to the manager when this set threshold 
	is exceeded. Degraded minutes are determined by collecting 
	all of the available seconds, after removing any severely 
	errored seconds. The resulting seconds is grouped into
	60 second intervals and if the cumulative errors during
	the seconds present in the group exceeds 1E-6."
	::= { lucentPM4FMT1E1ThreshTrapCfgEntry 11 }

lucentPM4FMT1E1ThreshRepTimer	OBJECT-TYPE
	SYNTAX	INTEGER
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION
	"If the trap is to be repeated, this object specifies the time
	in seconds. When this object value is set to 0, it indicates
	the trap is non-repeat trap."
	::= { lucentPM4FMT1E1ThreshTrapCfgEntry 12 }

lucentPM4FMT1E1ThreshTrapAck	OBJECT-TYPE
	SYNTAX	INTEGER {
		other(1),
		noack(2),
		ack(3)
	}
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION
	"If set to ack(3), clears the trap condition. If the value is
	set to noack(2), leaves the trap condition unchanged."
	::= { lucentPM4FMT1E1ThreshTrapCfgEntry 13 }

-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
-- Lucent Environment Traps configuration table
-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
lucentPM4FMEnvTrapCfg	OBJECT-TYPE
	SYNTAX	SEQUENCE OF LucentPM4FMEnvTrapCfgEntry
	ACCESS	not-accessible
	STATUS	mandatory
	DESCRIPTION
	"Environment Trap configuration table. This table
	enables configuration of voltage, power levels and
	temperature ranges for different units in the chassis.
	This object is not-accessible and present for MIB clarity."
	::= { lucentPM4FMTrapConfig 3 }

lucentPM4FMEnvTrapCfgEntry	OBJECT-TYPE
	SYNTAX	LucentPM4FMEnvTrapCfgEntry
	ACCESS	not-accessible
	STATUS	mandatory
	DESCRIPTION
	"Entry in the environment trap config table. Each trap
	is uniquely identified by the board index, unit type 
	and unit index. This object is not accessible and present
	for MIB clarity."
	INDEX	{ lucentPM4FMEnvBoardID,
		  lucentPM4FMEnvUnitType,
		  lucentPM4FMEnvUnitIndex }
	::= { lucentPM4FMEnvTrapCfg 1 }

LucentPM4FMEnvTrapCfgEntry ::=
	SEQUENCE {
		lucentPM4FMEnvBoardID
			INTEGER,
		lucentPM4FMEnvUnitType
			PMUnitType,
		lucentPM4FMEnvUnitIndex
			INTEGER,
		lucentPM4FMEnvOptUnitTemp
			INTEGER,
		lucentPM4FMEnvUnitTempRange
			INTEGER,
		lucentPM4FMEnvOptUnitPwrLvl
			INTEGER,
		lucentPM4FMEnvUnitPwrRange
			INTEGER,
		lucentPM4FMEnvTrapCtl
			INTEGER,
		lucentPM4FMEnvTrapRepTimer
			INTEGER
	}

lucentPM4FMEnvBoardID	OBJECT-TYPE
	SYNTAX	INTEGER
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION
	"Board ID specifies the board identifier for this trap.
	If the trap configuration is for an auxillary device
	such as a power supply or fan, this object will be set
	to zero. The unit type and the unit index will uniquely
	identify the auxillary devices."
	::= { lucentPM4FMEnvTrapCfgEntry 1 }

lucentPM4FMEnvUnitType	OBJECT-TYPE
	SYNTAX	PMUnitType
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION
	"Unit for which the Trap configuration must to apply."
	::= { lucentPM4FMEnvTrapCfgEntry 2 }

lucentPM4FMEnvUnitIndex	OBJECT-TYPE
	SYNTAX	INTEGER
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION
	"Unit index specifies the interface or sub-unit for 
	this trap (modem or T1/E1 interface etc.). The unit
	type and the unit index will uniquely identify the
	auxillary devices."
	::= { lucentPM4FMEnvTrapCfgEntry 3 }

lucentPM4FMEnvOptUnitTemp	OBJECT-TYPE
	SYNTAX 	INTEGER
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION
	"The optimum temperature for this unit. A trap is 
	generated when the temperature deviates from the 
	specified range. The temperature is specified as 
	an integer in degrees farenheit."
	::= { lucentPM4FMEnvTrapCfgEntry 4 }

lucentPM4FMEnvUnitTempRange	OBJECT-TYPE
	SYNTAX	INTEGER
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION
	"The temperature range above which a trap is generated.
	The temperature must be specified as an integer in degree
	Farenhiet (for example +/- 5 degree Far.)."
	::= { lucentPM4FMEnvTrapCfgEntry 5 }

lucentPM4FMEnvOptUnitPwrLvl	OBJECT-TYPE
	SYNTAX	INTEGER
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION
	"The optimal power level that is appropriate for this unit.
	A trap is generated when the power level fluctuates outside
	the limits set."
	::= { lucentPM4FMEnvTrapCfgEntry 6 }

lucentPM4FMEnvUnitPwrRange	OBJECT-TYPE
	SYNTAX	INTEGER
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION
	"The power range specified in volts. A trap is generated 
	when the power level fluctuates outside the Opt Pwr +/- Range 
	set."
	::= { lucentPM4FMEnvTrapCfgEntry 7 }

lucentPM4FMEnvTrapCtl	OBJECT-TYPE
	SYNTAX	INTEGER {
		disable(1),
		enable(2)
	}
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION
	"The trap control used to turn the environment traps on or off
	for the specified unit(s)."
	::= { lucentPM4FMEnvTrapCfgEntry 8 }

-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
-- Lucent Performance Management Group
-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
lucentPM4PerfMgmt	OBJECT IDENTIFIER	::= { lucentPM4Mib 6 }

-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
-- Lucent T1/E1 Performance Management Current Interval Table
-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
lucentPM4T1E1PerfMgmt	OBJECT IDENTIFIER ::= { lucentPM4PerfMgmt 1 }

lucentPM4T1E1PMCur	OBJECT-TYPE
	SYNTAX	SEQUENCE OF LucentPM4T1E1PMCurEntry
	ACCESS	not-accessible
	STATUS	mandatory
	DESCRIPTION
	"Performance management table representing the performance
	statistics of T1/E1 interfaces in the box. This table represents
	the current 15 mins statistics. This object is not accessible and 
	present for clarity purpose. This table is part of RFC 1406."
	::= { lucentPM4T1E1PerfMgmt 1 }

lucentPM4T1E1PMCurEntry	OBJECT-TYPE
	SYNTAX	LucentPM4T1E1PMCurEntry
	ACCESS	not-accessible
	STATUS	mandatory
	DESCRIPTION
	"Performance management table entries for all the T1/E1 interfaces 
	in the box. This table represents the current 15 mins statistics. 
	This object is not accessible and present for clarity purpose."
	INDEX	{ lucentPM4T1E1PMBoardID,
		  lucentPM4T1E1PMUnitType,
		  lucentPM4T1E1PMLineNum }
	::= { lucentPM4T1E1PMCur 1 }

LucentPM4T1E1PMCurEntry ::=
	SEQUENCE {
		lucentPM4T1E1PMCurBoardID
			INTEGER,
		lucentPM4T1E1PMCurUnitType
			PMUnitType,
		lucentPM4T1E1PMCurLineNum
			INTEGER,
		lucentPM4T1E1PMCurIfIndex
			INTEGER,
		lucentPM4T1E1PMCurESs
			Gauge,
		lucentPM4T1E1PMCurSESs
			Gauge,
		lucentPM4T1E1PMCurSEFs
			Gauge,
		lucentPM4T1E1PMCurUASs
			Gauge,
		lucentPM4T1E1PMCurCSSs
			Gauge,
		lucentPM4T1E1PMCurPCVs
			Gauge,
		lucentPM4T1E1PMCurLESs
			Gauge,
		lucentPM4T1E1PMCurBESs
			Gauge,
		lucentPM4T1E1PMCurDMs
			Gauge,
		lucentPM4T1E1PMCurLCVs
			Gauge
	}

lucentPM4T1E1PMCurBoard	OBJECT-TYPE
	SYNTAX	INTEGER
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION
	"Board number of the interface. The global interface number is 
	computed by using the Most Significant byte of the ifIndex 
	and the Least Significant 2 bytes represents the interface index.
	Byte 3 will represent the unit type which would be a T1 or E1."
	::= { lucentPM4T1E1PMCurEntry 1 }

lucentPM4T1E1PMCurUnitType	OBJECT-TYPE
	SYNTAX	PMUnitType
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION
	"Unit type indicates the type of interface as T1/E1 or T3/E3
	in future. This is part of the interface table ifIndex which
	is constructed with boardID, unit type and unit index."
	::= { lucentPM4T1E1PMCurEntry 2 }

lucentPM4T1E1PMCurLineNum	OBJECT-TYPE
	SYNTAX	INTEGER
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION
	"Line number uniquely identifies the T1/E1 interface on a
	given board."
	::= { lucentPM4T1E1PMCurEntry 3 }

lucentPM4T1E1PMCurIfIndex	OBJECT-TYPE
	SYNTAX	INTEGER
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION
	"Interface table ifIndex cross reference. The global interface 
	number is computed by using the Most Significant byte as the 
	board ID and the Least Significant 2 bytes represents the 
	interface index. The third byte represents the unit type which
	will be a T1 or E1. Thus board 0 interface 3 is represented as 
	0x00050003. The global interface number corresponds to the 
	IfIndex in MIB II."
	::= { lucentPM4T1E1PMCurEntry 4 }

lucentPM4T1E1PMCurESs	OBJECT-TYPE
	SYNTAX	Gauge
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION
	"The number of errored seconds, encountered by the line
	in the current 15 mins interval."
	::= { lucentPM4T1E1PMCurEntry 5 }

lucentPM4T1E1PMCurSESs	OBJECT-TYPE
	SYNTAX	Gauge
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION
	"The number of Severely Errored Seconds encountered by
	the line in the current 15 mins interval."
	::= { lucentPM4T1E1PMCurEntry 6 }

lucentPM4T1E1PMCurSEFSs	OBJECT-TYPE
