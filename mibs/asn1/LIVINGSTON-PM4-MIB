	::= { lucentPM4ModemEntry 16 }

-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
-- The Lucent Ethernet Interface table
-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
lucentPM4CmEther	OBJECT IDENTIFIER ::= { lucentPM4CmInterfaces 4 }

lucentPM4EtherTable OBJECT-TYPE
	SYNTAX  SEQUENCE OF LucentPM4EtherEntry
	ACCESS  not-accessible
	STATUS  mandatory
	DESCRIPTION
	"A list of ethernet interface entries. This object is not
	accessible"
	::= { lucentPM4CmEther 1 }

lucentPM4EtherEntry OBJECT-TYPE
	SYNTAX  LucentPM4EtherEntry
	ACCESS  not-accessible
	STATUS  mandatory
	DESCRIPTION
	"Ethernet interface entry containing objects at the 
	Session/Physical layers."
	INDEX   { lucentPM4EtherBoardIndex,
		    lucentPM4EtherIfType,
		    lucentPM4EtherIndex }
	::= { lucentPM4EtherTable 1 }

LucentPM4EtherEntry ::=
	SEQUENCE {
		lucentPM4EtherBoardIndex
			INTEGER,
		lucentPM4EtherIfType
			PMUnitType,
		lucentPM4EtherIndex
			INTEGER,
		lucentPM4EtherIfIndex
			INTEGER,
		lucentPM4EtherPortName
			DisplayString,
		lucentPM4EtherMacAddr
			PhysAddress,
		lucentPM4EtherIPAddr
			IpAddress,
		lucentPM4EtherIPGateway
			IpAddress,
		lucentPM4EtherPriNameServer
			IpAddress,
		lucentPM4EtherAltNameServer
			IpAddress,
		lucentPM4EtherSubnetMask
			IpAddress,
		lucentPM4EtherInFilter
			DisplayString,
		lucentPM4EtherOutFilter
			DisplayString,
		lucentPM4EtherOptRip
			INTEGER,
		lucentPM4EtherOptSlip
			INTEGER,
		lucentPM4EtherOptEtherDown
			INTEGER,
		lucentPM4EtherOptBcastHigh
			INTEGER,
		lucentPM4EtherOptSnmp
			INTEGER,
		lucentPM4EtherOptNoListen
			INTEGER,
		lucentPM4EtherOptDefaultRip
			INTEGER,
		lucentPM4EtherOptDefaultListen
			INTEGER,
		lucentPM4EtherOptIPFilter
			INTEGER,
		lucentPM4EtherOptDns
			INTEGER,
		lucentPM4EtherOptPmeMsg
			INTEGER,
		lucentPM4EtherOptNoClip
			INTEGER,
		lucentPM4EtherOptEtherIpx
			INTEGER,
		lucentPM4EtherOptNetBIOS
			INTEGER,
		lucentPM4EtherOptAccounting
			INTEGER,
		lucentPM4EtherOptNoPAP
			INTEGER
	}

lucentPM4EtherBoardIndex OBJECT-TYPE
	SYNTAX  INTEGER
	ACCESS  read-only
	STATUS  mandatory
	DESCRIPTION
	"A unique value for each ethernet board. The manager card
	has two ethernet interfaces at present. The ethernet interface
	in slot 4 has a board ID 10 and if there is a manager card in
	slot 5, the board ID for the interface would be 11."
	::= { lucentPM4EtherEntry 1 }

lucentPM4EtherIfType OBJECT-TYPE
	SYNTAX  PMUnitType
	ACCESS  read-only
	STATUS  mandatory
	DESCRIPTION
	"The interface type which together with the board ID and 
	the interface number will uniquely identify the interface."
	::= { lucentPM4EtherEntry 2 }

lucentPM4EtherIndex OBJECT-TYPE
	SYNTAX  INTEGER {
		ether0(1),
		ether1(2),
		other(3)
	}
	ACCESS  read-only
	STATUS  mandatory
	DESCRIPTION
	"A unique value for each ethernet interface. The manager card
	has two ethernet interfaces at present. ether0(1) represents
	10 Base-T interface and ether1(2) specifies the 10/100 Base-T
	auto-sensing ethernet interface."
	::= { lucentPM4EtherEntry 3 }

lucentPM4EtherIfIndex OBJECT-TYPE
	SYNTAX  INTEGER {
		ether0(67436545),
		ether1(168099841)
	}
	ACCESS  read-only
	STATUS  mandatory
	DESCRIPTION
	"IfIndex cross reference value for the ethernet interfaces. 
	The manager card has two ethernet interfaces at present. 
	ether0(67436545) represents 10 Base-T interface which corresponds
	to board 4 and interface 1. The enumerated value ether1(168099841) 
	specifies the 10/100 Base-T auto-sensing ethernet interface
	which corresponds to board 4 and interface 2. We can add the
	standby manager card ethernet interfaces when they are available."
	::= { lucentPM4EtherEntry 4 }

lucentPM4EtherPortName OBJECT-TYPE
	SYNTAX  DisplayString (SIZE(1..8))
	ACCESS  read-write
	STATUS  mandatory
	DESCRIPTION
	"A printable ASCII string specifying the name of the ethernet
	port."
	::= { lucentPM4EtherEntry 5 }

lucentPM4EtherMacAddress OBJECT-TYPE
	SYNTAX  PhysAddress
	ACCESS  read-only
	STATUS  mandatory
	DESCRIPTION
	"Physical address of the interface."
	::= { lucentPM4EtherEntry 6 }

lucentPM4EtherIpAddress OBJECT-TYPE
	SYNTAX  IpAddress
	ACCESS  read-write
	STATUS  mandatory
	DESCRIPTION
	"IP address of the interface."
	::= { lucentPM4EtherEntry 7 }

lucentPM4EtherIpGateway OBJECT-TYPE
	SYNTAX  IpAddress
	ACCESS  read-write
	STATUS  mandatory
	DESCRIPTION
