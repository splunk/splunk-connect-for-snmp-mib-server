-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
-- Lucent Administration Management Group
-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
lucentPM4AcctMgmtComm	OBJECT IDENTIFIER	::= { lucentPM4AcctMgmt 1 }

-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
-- Lucent SNMP Read/Write Community Table
-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
lucentPM4SnmpCommTable	OBJECT-TYPE
	SYNTAX	SEQUENCE OF LucentPM4SnmpCommEntry
	ACCESS	not-accessible
	STATUS	mandatory
	DESCRIPTION
	"The SNMP Community Table. This table contains entries to restrict
	the SNMP get and set operations."
	::= { lucentPM4AcctMgmtComm 1 }

lucentPM4SnmpCommEntry	OBJECT-TYPE
	SYNTAX	LucentPM4SnmpCommEntry
	ACCESS	not-accessible
	STATUS	mandatory
	DESCRIPTION
	"The entries in the community table."
	INDEX	{ lucentPM4SnmpCommName }
	::= { lucentPM4SnmpCommTable 1 }

LucentPM4SnmpCommEntry ::=
	SEQUENCE {
		lucentPM4SnmpCommIndex
			INTEGER,
		lucentPM4SnmpCommName
			DisplayString,
		lucentPM4SnmpCommIpAddr
			IpAddress,
		lucentPM4SnmpCommReadAccess
			INTEGER,
		lucentPM4SnmpCommWriteAccess
			INTEGER,
		lucentPM4SnmpCommTraps
			INTEGER,
		lucentPM4SnmpCommStatus
			INTEGER,
		lucentPM4SnmpCommLastError
			DisplayString
	}

lucentPM4SnmpCommIndex	OBJECT-TYPE
	SYNTAX	INTEGER (1..10)
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION
	"The index of the command in the command table. A MAX of
	10 network management stations must be specified along with
	their community names."
	::= { lucentPM4SnmpCommEntry 1 }

lucentPM4SnmpCommName	OBJECT-TYPE
	SYNTAX	DisplayString (SIZE (1..32))
	ACCESS	not-accessible
	STATUS	mandatory
	DESCRIPTION
	"The name of the SNMP Community for SNMP readers and writers. The size
	of the string is limited to 32 characters. All characters in the string
	must be printable."
	::= { lucentPM4SnmpCommEntry 2 }

lucentPM4SnmpCommIpAddr	OBJECT-TYPE
	SYNTAX	IpAddress
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION
	"The IP Address of the remote community."
	::= { lucentPM4SnmpCommEntry 3 }

lucentPM4SnmpCommReadAccess	OBJECT-TYPE
	SYNTAX	INTEGER {
		disable(1),
		ensable(2)
	}
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION
	"Read access enable or disable for this community. When
	enabled, it allows read-only variable access using this
	community string by the SNMP client."
	::= { lucentPM4SnmpCommEntry 4 }

lucentPM4SnmpCommWriteAccess	OBJECT-TYPE
	SYNTAX	INTEGER {
		disable(1),
		ensable(2)
	}
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION
	"Write access enable or disable for this community. When
	enabled, the agent allows write access to the parameters
	on the agent by the SNMP clients."
	::= { lucentPM4SnmpCommEntry 5 }

lucentPM4SnmpCommTraps	OBJECT-TYPE
	SYNTAX	INTEGER {
		disable(1),
		ensable(2)
	}
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION
	"Traps receiving capability enable or disable for 
	this community. When enabled, the SNMP agent forwards
	the traps generated in the box to this SNMP client."
	::= { lucentPM4SnmpCommEntry 6 }

lucentPM4SnmpCommStatus	OBJECT-TYPE
	SYNTAX	INTEGER {
		normal(1),
		delete(2)
	}
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION
	"The status of the entry for this community. If the
	status is set to normal, it allows requests from this
	SNMP client else it discards the requests from this
	client."
	::= { lucentPM4SnmpCommEntry 7 }

lucentPM4SnmpCommLastError	OBJECT-TYPE
	SYNTAX	DisplayString (SIZE (0..511))
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION
	"If there is an error on a request, this variable may
	contain a message indicating the error."
	::= { lucentPM4SnmpCommEntry 8 }

-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
-- Lucent Administration Management Group
-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
lucentPM4AcctMgmtCallEvent	OBJECT IDENTIFIER	::= { lucentPM4AcctMgmt 2 }

-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
-- Lucent Billing/Accounting Information Table
-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
lucentPM4AMCallEventTable	OBJECT-TYPE
	SYNTAX	SEQUENCE OF LucentPM4AMCallEventEntry
	ACCESS	not-accessible
	STATUS	mandatory
	DESCRIPTION
	"Call accounting table containing a list of call events,
	which may be used for billing purposes."
	::= { lucentPM4AcctMgmtCallEvent 1 }

lucentPM4AMCallEventEntry	OBJECT-TYPE
	SYNTAX	LucentPM4AMCallEventEntry
	ACCESS	not-accessible
	STATUS	mandatory
	DESCRIPTION
	"The entries in the accounting/billing table."
	INDEX	{ lucentPM4AMCEIndex }
	::= { lucentPM4AMCallEventTable 1 }

LucentPM4AMCallEventEntry ::=
	SEQUENCE {
		lucentPM4AMCEIndex
			INTEGER,
		lucentPM4AMCETimeStamp
			INTEGER,
		lucentPM4AMCEType
			INTEGER,
		lucentPM4AMCESvcType
			INTEGER,
		lucentPM4AMCEUName
			DisplayString,
		lucentPM4AMCEModemBoard
			INTEGER,
		lucentPM4AMCEModemID
			INTEGER,
		lucentPM4AMCEModemPort
			DisplayString,
		lucentPM4AMCEDataRate
			INTEGER,
		lucentPM4AMCECallingPartyID
			DisplayString,
		lucentPM4AMCECalledPartyID
			DisplayString,
		lucentPM4AMCEInOctets
			INTEGER,
		lucentPM4AMCEOutOctets
			INTEGER,
		lucentPM4AMCECallCharge
			INTEGER,
		lucentPM4AMCEDisconnReason
			INTEGER
	}

lucentPM4AMCEIndex	OBJECT-TYPE
	SYNTAX	INTEGER
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION
	"Call event index used as an index into the call event table.
	The table stores call events which may be used for billing."
	::= { lucentPM4AMCallEventEntry 1 }

lucentPM4AMCETimeStamp	OBJECT-TYPE
	SYNTAX	INTEGER
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION
	"Time stamp for this event in seconds since the last reboot."
	::= { lucentPM4AMCallEventEntry 2 }

lucentPM4AMCEType	OBJECT-TYPE
	SYNTAX	INTEGER {
		calloriginated(1),
		callanswered(2),
		callcleared(3),
		servicechanged(4),
		namechanged(5),
		baudratechanged(6)
	}
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION
	"Specifies the type of event associated with this entry in the
	call event table."
	::= { lucentPM4AMCallEventEntry 3 }

lucentPM4AMCESvcType	OBJECT-TYPE
	SYNTAX	INTEGER {
		none(1),
		ppp(2),
		slip(3),
		mpp(4),
		x25(5),
		combinet(6),
		frameRelay(7),
		euraw(8),
		euui(9),
		telnet(10),	
		telnetBinary(11),
		rawTcp(12),
		terminalServer(13),
		mp(14),
		virtualConnect(15),
		x25DChannel(16)
	}
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION
	"The type of service provided to the user. This field is
	meaningful if the event type is servicechanged(4), or
	namechanged(5) events. In all other cases, this object
	must return none(1)."
	::= { lucentPM4AMCallEventEntry 4 }

lucentPM4AMCEUName	OBJECT-TYPE
	SYNTAX	DisplayString
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION
	"User name of the dialed in user. This object returns the
	valid user name when the event type is servicechanged(4)
	or namechanged(5). In all other cases, it returns a NULL."
	::= { lucentPM4AMCallEventEntry 5 }

lucentPM4AMCEModemBoard	OBJECT-TYPE
	SYNTAX	INTEGER
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION
	"Board ID of the modem which handled this call. This value
	can be used to diagnose modem related problems (dropping the
	call, retraining too frequently etc.)."
	::= { lucentPM4AMCallEventEntry 6 }

lucentPM4AMCEModemID	OBJECT-TYPE
	SYNTAX	INTEGER
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION
	"Identifies the specific modem on a board which handled this
	call. Can be used to diagnose modem related problems."
	::= { lucentPM4AMCallEventEntry 7 }

lucentPM4AMCEModemPort	OBJECT-TYPE
	SYNTAX	DisplayString
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION
	"A textual string containing the name of the serial
	interface (ie. S0, S1, etc)."
	::= { lucentPM4AMCallEventEntry 8 }

lucentPM4AMCEDataRate	OBJECT-TYPE
	SYNTAX	INTEGER
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION
	"Specifies the speed of this connection. Speed is specified
	as baud rate for modem calls and receive data rate for ISDN
	calls. This object returns a 0 for call answered and call
	cleared events."
	::= { lucentPM4AMCallEventEntry 9 }

lucentPM4AMCECallingPartyID	OBJECT-TYPE
	SYNTAX	DisplayString
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION
	"Calling party ID. This object is valid only for call answered,
	call originated, and call cleared events. For all invalid event
	types, this object is set to NULL."
	::= { lucentPM4AMCallEventEntry 10 }

lucentPM4AMCECalledPartyID	OBJECT-TYPE
	SYNTAX	DisplayString
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION
	"Called party ID. This object is valid only for call answered,
	call originated, and call cleared events. For all invalid event
	types, this object is set to NULL."
	::= { lucentPM4AMCallEventEntry 11 }

lucentPM4AMCEInOctets	OBJECT-TYPE
	SYNTAX	INTEGER
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION
	"Total octets received during this call. This object is cleared
	at the end of each call."
	::= { lucentPM4AMCallEventEntry 12 }

lucentPM4AMCEOutOctets	OBJECT-TYPE
	SYNTAX	INTEGER
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION
	"Total octets sent out during this call. This object is cleared
	at the end of each call."
	::= { lucentPM4AMCallEventEntry 13 }

lucentPM4AMCECallCharge	OBJECT-TYPE
	SYNTAX	INTEGER
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION
	"Call charge for this call. This object is valid only when the
	event is call cleared. For all other events this object is set
	to zero."
	::= { lucentPM4AMCallEventEntry 14 }

lucentPM4AMCEDisconnReason	OBJECT-TYPE
	SYNTAX	INTEGER {
		notApplicable(1),
		unknown(2),
		disconnected(3),
		clidAuthFailed(4),		-- Radius authorization failed
		clidAuthServTimeout(5), 	-- Radius client timeout
		clidAuthRequestCallback(6),
		preT310Timeout(7),		-- disconnect timer timeout
		noModemAvailable(9),		-- all modems assigned to calls
		noModemNoCarrier(10),		-- no carrier detected
		noModemLossCarrier(11),		-- lost carrier during call
		noModemResultCodes(12),
		noModemOpenFailed(13),
		noModemOpenFailedDiag(14),
		tsUserExit(20),			-- user exited the terminal server
		tsIdleTimeout(21),		-- timed out waiting for user input
		tsExitTelnet(22),			-- user exited telnet session
		tsNoIPAddr(23),			-- no IP address, cannot assign one
		tsExitTcp(24),
		tsPassWordFail(25),		-- exit due to failed password
		tsRawTCPDisable(26),
		tsControlC(27),			-- Control C detected
		tsDestroyed(28),
		tsClosedVirtualConnect(29),	-- virtual terminal connection closed
		tsVirtualConnectDestroyed(30),
		tsExitRlogin(31),
		tsRloginBadOption(32),		-- bad rlogin command option
		tsErrorResource(33),
		mpNullMessageTimeout(35),
		pppLcpTimeout(40),		-- Link Control Protocol timeout
		pppLcpNegotiateFail(41),	-- LCP negotiation failed
		pppPAPAuthFail(42),		-- PAP authentication failed
		pppCHAPAuthFail(43),		-- CHAP authentication failed
		pppRemoteAuthFail(44),
		pppRcvTerminate(45),		-- received terminate request from far end
		pppCloseEvent(46),		-- PPP close event
		pppCloseNoNcpsOpened(47),
		pppCloseUnknownMpBundle(48),
		pppCloseMpAddChanFail(49),
		tsExitErrTooMany(50),
		tsExitErrResource(51),
		tsExitErrInvalidIP(52),
		tsExitErrHostName(53),
		tsExitErrBadPort(54),
		tsExitErrHostReset(60),
		tsExitErrConnRefused(61),
		tsExitErrTimedOut(62),
		tsExitErrClosed(63),
		tsExitErrNetUnreach(64),
		tsExitErrHostUnreach(65),
		tsExitErrNetAdminUnreach(66),
		tsExitErrHostAdminUnreach(67),
		tsExitErrPortUnreach(68),
		sessTimeOut(100),
		sessFailSecurity(101),
		sessCallback(102),
		invalidProtocol(120),
		requestByRadiusClient(150),
		localAdmin(151),
		localSnmp(152),				-- disconnected by SNMP request
		v110Timeout(160),
		pppAuthTimeout(170),
		userCallClearRequest(180),
		remoteEndHungUp(185),
		resourceQuiesced(190),
		maxCallDurationReached(195),
		lowMemory(201),
		boardDied(210)
	}
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION
	"Reason for the disconnect."
	::= { lucentPM4AMCallEventEntry 15 }

-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
-- Lucent PortMaster 4 Traps
-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++

lucentPM4BoardOfflineTrap	TRAP-TYPE
	ENTERPRISE	lucentPM4Traps
	VARIABLES	{ sysName,
			  lucentPM4FMBoardIndex,
			  lucentPM4FMUnitType,
			  lucentPM4FMUnitTrapStatus }
	DESCRIPTION
	"Board down trap. The variable bindings in the Trap packet provide 
	information about the chassis name, board number and the trap 
	status. This Trap must be cleared manually."
	::= 1

lucentPM4BoardOnlineTrap	TRAP-TYPE
	ENTERPRISE	lucentPM4Traps
	VARIABLES	{ sysName,
			  lucentPM4FMBoardIndex,
			  lucentPM4FMUnitType,
			  lucentPM4FMUnitTrapStatus }
	DESCRIPTION
	"Board up trap. The variable bindings in the Trap packet provide 
	information about the chassis name, board number and the trap 
	status. This Trap must be cleared manually."
	::= 2

lucentPM4PwrSupFailTrap	TRAP-TYPE
	ENTERPRISE	lucentPM4Traps
	VARIABLES	{ sysName,
			  lucentPM4FMUnitType,
			  lucentPM4FMUnitIndex,
			  lucentPM4FMUnitTrapStatus }
	DESCRIPTION
	"Power supply failed trap. The variable bindings in the Trap packet provide 
	information about the chassis name, power supply unit and the trap 
	status. This Trap must be cleared manually."
	::= 3

lucentPM4PwrSupWarnTrap	TRAP-TYPE
	ENTERPRISE	lucentPM4Traps
	VARIABLES	{ sysName,
			  lucentPM4FMUnitType,
			  lucentPM4FMUnitIndex,
			  lucentPM4FMUnitTrapStatus }
	DESCRIPTION
	"Power supply warning trap. The variable bindings in the Trap packet provide 
	information about the chassis name, power supply unit and the trap 
	status. This Trap is issued when the power supply fluctuates between a set
      threshold. This Trap must be cleared manually."
	::= 4

lucentPM4PwrSupRestoredTrap	TRAP-TYPE
	ENTERPRISE	lucentPM4Traps
	VARIABLES	{ sysName,
			  lucentPM4FMUnitType,
			  lucentPM4FMUnitIndex,
			  lucentPM4FMUnitTrapStatus }
	DESCRIPTION
	"Power supply restored trap. The variable bindings in the Trap packet provide 
	information about the chassis name, power supply unit and the trap 
	status. This Trap is issued when a failed power supply is restored.
	This must be cleared manually."
	::= 5

