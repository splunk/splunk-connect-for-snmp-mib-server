LIVINGSTON-PM4-MIB DEFINITIONS ::= BEGIN

IMPORTS
	IpAddress, Counter, Gauge, TimeTicks
		FROM RFC1155-SMI
	OBJECT-TYPE
		FROM RFC-1212
	TRAP-TYPE
		FROM RFC-1215
	DisplayString, PhysAddress, sysName
		FROM RFC1213-MIB
	lucentPM4
		FROM LIVINGSTON-ROOT-MIB;


-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
-- Lucent PortMaster private MIB Begin Textual Conventions
-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++

-- Lucent PortMaster Equipment Types

PMUnitType ::= INTEGER {
	mrgmodule(1),
	quadt1(2),
	trie1(3),
	modem(4),
	serialport(5),
	ether0(6),
	ether1(7),
	console(8),
	acpwrsup(9),
	fan(10),
	dcpwrsup(11),
	allunits(255)
	}

PMEquipPRIStatus ::= INTEGER {
	up(1),
	down(2),
	loopback(3),
	fault(4)
	}

PMEquipStatus ::= INTEGER {
	up(1),
	down(2),
	maintenance(3),
	fault(4),
	other(5)
	}

PMDiagCmdStatus ::= INTEGER {
	success(1),
	fail(2),
	inprogress(3),
	notsupported(4),
	aborted(5),
	other(6)
	}

PMDiagTestCntrl ::= INTEGER {
	normal(1),
	start(2),
	stop(3),
	abort(4)
	}

PMAlarmType ::= INTEGER {
	informational(1),
	warning(2),
	minor(3),
	major(4),
	critical(5)
	}

-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
-- Lucent PortMaster private MIB End of Textual Conventions
-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++


-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
--  Lucent PortMaster 4 Enterprise MIB
-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
lucentPM4Mib	OBJECT IDENTIFIER ::= { lucentPM4 1 }
lucentPM4Traps	OBJECT IDENTIFIER ::= { lucentPM4 2 }

-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
-- Lucent MIB Information Group
-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
lucentPM4MibRev	OBJECT-TYPE
	SYNTAX	DisplayString (SIZE(1..6))
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION
	"This object specifies the current MIB revision number. Example of the
	MIB revision can be PM4xxx for PM4 product and PM3xxx for PM3 products etc.
	Where xxx can be any combination of alpha-numeric characters."
	::= { lucentPM4Mib 1 }

lucentPM4SWRev	OBJECT-TYPE
	SYNTAX	DisplayString (SIZE(1..10))
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION
	"This object specifies the ComOS revision number. Example of the
	ComOS revision can be ComOS4.xx. Where xxx can be any combination
	of alpha-numeric characters."
	::= { lucentPM4Mib 2 }

-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
-- Lucent chassis Group
-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
lucentPM4Chassis	OBJECT IDENTIFIER ::= { lucentPM4Mib     3 }

lucentPM4ChasSummary	OBJECT-TYPE
	SYNTAX	DisplayString (SIZE(55))
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION
	"This object provides general information about the 
	PortMaster 4 chassis operational status. This object is 
 	read-only. The DisplayString represents a summary of all
	the devices in the chassis as follows:
	Bytes 1 - 2		'0''0'
	Byte  3		'U', 'D', 'M', 'F', 'O'
	Byte  4		space
	Bytes 5 - 6		'0''1'
	Byte  7		'U', 'D', 'M', 'F', 'O'
	Byte  8		space
	Bytes 9 - 10	'0''2'
	Byte  11		'U', 'D', 'M', 'F', 'O'
	Byte  12		space
	Bytes 13 - 14	'0''3'
	Byte  15		'U', 'D', 'M', 'F', 'O'
	Byte  16		space
	Bytes 17 - 18	'0''4'
	Byte  19		'U', 'D', 'M', 'F', 'O'
	Byte  20		space
	Bytes 21 - 22	'0''5'
	Byte  23		'U', 'D', 'M', 'F', 'O'
	Byte  24		space
	Bytes 25 - 26	'0''6'
	Byte  27		'U', 'D', 'M', 'F', 'O'
	Byte  28		space
	Bytes 29 - 30	'0''7'
	Byte  31		'U', 'D', 'M', 'F', 'O'
	Byte  32		space
	Bytes 33 - 34	'0''8'
	Byte  35		'U', 'D', 'M', 'F', 'O'
	Byte  36		space
	Bytes 37 - 38	'0''9'
	Byte  39		'U', 'D', 'M', 'F', 'O'
	Byte  40		space
	Bytes 41 - 42	'1''0'
	Byte  43		'U', 'D', 'M', 'F', 'O'
	Byte  44		space
	Bytes 45 - 46	'1''1'
	Byte  47		'U', 'D', 'M', 'F', 'O'
	Byte  48		space
	Bytes 49 - 50	'1''2'
	Byte  51		'U', 'D', 'M', 'F', 'O'
	Byte  52		space
	Bytes 53 - 54	'1''3'
	Byte  55		'U', 'D', 'M', 'F', 'O'
	Byte  56		space
	Bytes 57 - 58	'1''4'
	Byte  59		'U', 'D', 'M', 'F', 'O'
	Byte  60		space
	Bytes 61 - 62	'1''5'
	Byte  63		'U', 'D', 'M', 'F', 'O'
	Byte  64		space
	Bytes 65 - 66	'P''1'
	Byte  67		'U', 'D', 'M', 'F', 'O'
	Byte  68		space
	Bytes 69 - 70	'P''2'
	Byte  71		'U', 'D', 'M', 'F', 'O'
	Byte  72		space
	Bytes 73 - 74	'P''3'
	Byte  75		'U', 'D', 'M', 'F', 'O'
	Byte  76		space
	Bytes 77 - 78	'D''1'
	Byte  79		'U', 'D', 'M', 'F', 'O'
	Byte  80		space
	Bytes 81 - 82	'D''2'
	Byte  83		'U', 'D', 'M', 'F', 'O'
	Byte  84		space
	Bytes 85 - 86	'F''1'
	Byte  87		'U', 'D', 'M', 'F', 'O'
	Byte  88		space
	Bytes 89 - 90	'F''2'
	Byte  91		'U', 'D', 'M', 'F', 'O'
	Byte  92		space
	Bytes 93 - 94	'F''3'
	Byte  95		'U', 'D', 'M', 'F', 'O'
	Byte  96		space
	Bytes 97 - 98	'F''4'
	Byte  99		'U', 'D', 'M', 'F', 'O'
	Legend		'#''#' 	Represents the board number
				'F''#'	Represents the Fan #
				'P''#'	Represents the Power Supply #
				'D''#'	Represents the DC Power Supply #
				'U'	Up
				'D'	Down
				'M'	Maintenance
				'F'	Failed
				'O'	Other unknown state."
	::= { lucentPM4Chassis 1 }

-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
-- Lucent Chassis Command Table
-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++

lucentPM4ChasCmdTable	OBJECT-TYPE
	SYNTAX	SEQUENCE OF LucentPM4ChasCmdEntry
	ACCESS	not-accessible
	STATUS	mandatory
	DESCRIPTION
	"Table describing the commands that can be issued to the
	agent to perform specific actions to any card, port or
	device in the system. For example to erase the flash or
	a particular file from the flash. Note that only a station
	configured with the appropriate authentication string can
	issue commands to the agent."
	::= { lucentPM4Chassis 2 }

lucentPM4ChasCmdEntry	OBJECT-TYPE
	SYNTAX	LucentPM4ChasCmdEntry
	ACCESS	not-accessible
	STATUS	mandatory
	DESCRIPTION
	"Entries in the command table for the chassis commands. This
	describes one entry or row in the command table."
	INDEX	{ lucentPM4ChasCmdIndex }
	::= { lucentPM4ChasCmdTable 1 }

LucentPM4ChasCmdEntry ::=
	SEQUENCE {
		lucentPM4ChasCmdIndex
			INTEGER,
		lucentPM4ChasCmdBoardId
			INTEGER,
		lucentPM4ChasCmdUnitType
			PMUnitType,
		lucentPM4ChasCmdUnitIndex
			INTEGER,
		lucentPM4ChasCmdDevId
			INTEGER,
		lucentPM4ChasCmdId
			INTEGER,
		lucentPM4ChasCmdParams
			OCTET STRING,
		lucentPM4ChasCmdResult
			INTEGER
	}

lucentPM4ChasCmdIndex	OBJECT-TYPE
	SYNTAX	INTEGER (1..10)
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION
	"This object specifies the index in the command table.
	The values for this object is limited to the size of
	the command table on the network element. The size of
	the command table is set to 10 which can be changed if
	and when users need to schedule more than 10 commands
	at a given time."
	::= { lucentPM4ChasCmdEntry 1 }

lucentPM4ChasCmdBoardId	OBJECT-TYPE
	SYNTAX	INTEGER
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION
	"This object specifies the board for which the command is to
	be applied. The values for this object is limited to the Max
	number of boards."
	::= { lucentPM4ChasCmdEntry 2 }

lucentPM4ChasCmdUnitType	OBJECT-TYPE
	SYNTAX	PMUnitType
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION
	"This object specifies the type of the device to which
	this command must apply."
	::= { lucentPM4ChasCmdEntry 3 }

lucentPM4ChasCmdUnitIndex	OBJECT-TYPE
	SYNTAX	INTEGER
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION
	"This object specifies the interface index."
	::= { lucentPM4ChasCmdEntry 4 }

lucentPM4ChasCmdDevId	OBJECT-TYPE
	SYNTAX	INTEGER (1..96)
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION
	"This object specifies the sub-unit id for which the command
	must be applied to. This value will be used by the agent to
	index to the correct sub-device on a board. For example, this
	object can have values from 1 - 96 for the modems or 1 - 4 for
	T1 or 1 - 3 for the E1."
	::= { lucentPM4ChasCmdEntry 5 }

lucentPM4ChasCmdId		OBJECT-TYPE
	SYNTAX	INTEGER {
		eraseflashfile(1),
		eraseallflash(2),
		saveall(3),
		resetport(4),
		resetfilter(5),
		adduser(6),
		deleteuser(7),
		addlocation(8),
		diallocation(9),
		addfilter(10),
		deletefilter(11),
		addmodem(12),
		resetvirtport(13),
		addospfarea(14),
		resetospf(15),
		addprop(16),
		deleteprop(17),
		resetprop(18),
		resetether0(19),
		resetether1(20),
		resetall(21),
		resetconsole(22),
		version(23),
		traceroutes(24),
		ptrace(25),
		ifconfig(26),
		eraseconfig(27),
		erasecomos(28),
		reboot(29)
	}
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION
	"This object specifies the command. Each command takes a unique
	value. The completion status of this command is set in the result
	object of the table."
	::= { lucentPM4ChasCmdEntry 6 }

lucentPM4ChasCmdParams	OBJECT-TYPE
	SYNTAX	OCTET STRING (SIZE(0..255))
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION
	"This object specifies the command parameters. Each parameter must be
	seperated by a blank space. The last parameter is terminated with a
	';'."
	::= { lucentPM4ChasCmdEntry 7 }

lucentPM4ChasCmdResult	OBJECT-TYPE
	SYNTAX	PMDiagCmdStatus
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION
	"This object specifies the command result. The result for each
	of the previous 10 commands will be stored in a table, which
	can be retrieved by the client when needed."
	::= { lucentPM4ChasCmdEntry 8 }

-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
-- Lucent Configuration Management Group
-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
lucentPM4ConfigMgmt		OBJECT IDENTIFIER ::= { lucentPM4Mib 4 }

-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
-- The Lucent Interfaces Group
-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
lucentPM4CmInterfaces	OBJECT IDENTIFIER ::= { lucentPM4ConfigMgmt 1}

-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
-- The Lucent Serial Interfaces table
-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
lucentPM4CmSerial		OBJECT IDENTIFIER ::= { lucentPM4CmInterfaces 1 }

lucentPM4SerialTable OBJECT-TYPE
	SYNTAX  SEQUENCE OF LucentPM4SerialEntry
	ACCESS  not-accessible
	STATUS  mandatory
	DESCRIPTION
	"A list of serial interface entries."
	::= { lucentPM4CmSerial 1 }

lucentPM4SerialEntry OBJECT-TYPE
	SYNTAX  LucentPM4SerialEntry
	ACCESS  not-accessible
	STATUS  mandatory
	DESCRIPTION
	"A serial interface entry containing objects at the
	physical and session layer."
	INDEX   { lucentPM4SerialBoardIndex,
		  lucentPM4SerialIndex }
	::= { lucentPM4SerialTable 1 }

LucentPM4SerialEntry ::=
	SEQUENCE {
		lucentPM4SerialBoardIndex
			INTEGER,
		lucentPM4SerialUnitType
			PMUnitType,
		lucentPM4SerialIndex
			INTEGER,
		lucentPM4ModemId
			INTEGER,
		lucentPM4SerialPortNumber
			INTEGER,
		lucentPM4SerialPhysType
			INTEGER,
		lucentPM4SerialPortStatus
			INTEGER,
		lucentPM4SerialDS0State
			INTEGER,
		lucentPM4SerialUser
			DisplayString,
		lucentPM4SerialSessionId
			DisplayString,
		lucentPM4SerialTypeHardwired
			INTEGER,
		lucentPM4SerialTypeNwDialIn
			INTEGER,
		lucentPM4SerialTypeNwDialout
			INTEGER,
		lucentPM4SerialTypeLogin
			INTEGER,
		lucentPM4SerialTypeDevice
			INTEGER,
		lucentPM4SerialTypeDeviceName
			DisplayString,
		lucentPM4SerialDirection
			INTEGER,
		lucentPm4SerialStarted
			TimeTicks,
		lucentPM4SerialIdle
			TimeTicks,
		lucentPM4SerialInSpeed
			Gauge,
		lucentPM4SerialOutSpeed
			Gauge,
		lucentPM4SerialIpAddress
			IpAddress,
		lucentPM4SerialifDescr
			DisplayString,
		lucentPM4SerialInOctets
			Counter,
		lucentPM4SerialOutOctets
			Counter,
		lucentPm4SerialQOctets
			Counter
	}

lucentPM4SerialBoardIndex OBJECT-TYPE
	SYNTAX  INTEGER
	ACCESS  read-only
	STATUS  mandatory
	DESCRIPTION
	"A unique value for each board in the PortMaster chassis.
	The Max value of this variable is limited by the number
	of boards in the chassis. This value is limited to 255."
	::= { lucentPM4SerialEntry 1 }

lucentPM4SerialUnitType OBJECT-TYPE
	SYNTAX  PMUnitType
	ACCESS  read-only
	STATUS  mandatory
	DESCRIPTION
	"Unit type indicates the serial port. The interface table
	ifIndex is a combination of board index, unit type and
	unit index."
	::= { lucentPM4SerialEntry 2 }

lucentPM4SerialIndex OBJECT-TYPE
	SYNTAX  INTEGER
	ACCESS  read-only
	STATUS  mandatory
	DESCRIPTION
	"A unique value for each serial interface on a given board."
	::= { lucentPM4SerialEntry 3 }

lucentPM4ModemId OBJECT-TYPE
	SYNTAX  INTEGER
	ACCESS  read-only
	STATUS  mandatory
	DESCRIPTION
	"This object is the cross reference to the modem interface table index.
	The value is dynamically assigned when the call is established."
	::= { lucentPM4SerialEntry 4 }

lucentPM4SerialPortNumber OBJECT-TYPE
	SYNTAX  INTEGER
	ACCESS  read-only
	STATUS  mandatory
	DESCRIPTION
	"A serial port to which this modem is assigned for this call."
	::= { lucentPM4SerialEntry 5 }

lucentPM4SerialPhysType OBJECT-TYPE
	SYNTAX  INTEGER {
		other(1),          -- none of the following
		async(2),
		sync(3),
		isdn(4)
	}
	ACCESS  read-only
	STATUS  mandatory
	DESCRIPTION
	"The type of physical serial interface, distinguished
	according to the physical/link protocol(s) being
	currently used on the interface. When this object is
	set to asyn(2), then the service types dial-in, dial-
	out, login, and device are valid. When this object is
	set to sync(3), the serial service types dial-in, dial-
	out and hardwired are valid."
	::= { lucentPM4SerialEntry 6 }

lucentPM4SerialPortStatus OBJECT-TYPE
	SYNTAX  INTEGER {
		idle(1),
		connecting(2),
		established(3),
		disconnecting(4),
		command(5),
		noservice(6)
	}
	ACCESS  read-only
	STATUS  mandatory
	DESCRIPTION
	"The status of the serial interface."
	::= { lucentPM4SerialEntry 7 }

lucentPM4SerialDS0State	OBJECT-TYPE
	SYNTAX	INTEGER {
		notavailable(1),
		busyout(2),
		havecomport(3)
	}
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION
	"Cross reference value for each DS0 for a given T1/E1 line and a 
	given board in the PM4 chassis."
	::= { lucentPM4SerialEntry 8 }

lucentPM4SerialUser OBJECT-TYPE
	SYNTAX  DisplayString (SIZE (0..255))
	ACCESS  read-only
	STATUS  mandatory
	DESCRIPTION
	"Name of the active user.  Blank if not active."
	::= { lucentPM4SerialEntry 9 }

lucentPM4SerialSessionId OBJECT-TYPE
	SYNTAX  DisplayString (SIZE (0..255))
	ACCESS  read-only
	STATUS  mandatory
	DESCRIPTION
	"A unique Session Identifier which matches the RADIUS
	session ID.  Blank when not using RADIUS."
	::= { lucentPM4SerialEntry 10 }

lucentPM4SerialTypeHardwired OBJECT-TYPE
	SYNTAX  INTEGER {
		disable(1),
		enable(2)
	}
	ACCESS  read-write
	STATUS  mandatory
	DESCRIPTION
	"The active type of service being provided by the
	serial interface."
	::= { lucentPM4SerialEntry 11 }

lucentPM4SerialTypeNwDialIn OBJECT-TYPE
	SYNTAX  INTEGER {
		disable(1),
		enable(2)
	}
	ACCESS  read-write
	STATUS  mandatory
	DESCRIPTION
	"The active type of service being provided by the
	serial interface."
	::= { lucentPM4SerialEntry 12 }

lucentPM4SerialTypeNwDialout OBJECT-TYPE
	SYNTAX  INTEGER {
		disable(1),
		enable(2)
	}
	ACCESS  read-write
	STATUS  mandatory
	DESCRIPTION
	"The active type of service being provided by the
	serial interface."
	::= { lucentPM4SerialEntry 13 }

lucentPM4SerialTypeLogin OBJECT-TYPE
	SYNTAX  INTEGER {
		disable(1),
		enable(2)
	}
	ACCESS  read-write
	STATUS  mandatory
	DESCRIPTION
	"The active type of service being provided by the
	serial interface."
	::= { lucentPM4SerialEntry 14 }

lucentPM4SerialTypeDevice OBJECT-TYPE
	SYNTAX  INTEGER {
		disable(1),
		enable(2)
	}
	ACCESS  read-write
	STATUS  mandatory
	DESCRIPTION
	"The active type of service being provided by the
	serial interface."
	::= { lucentPM4SerialEntry 15 }

lucentPM4SerialTypeDeviceName OBJECT-TYPE
	SYNTAX  DisplayString
	ACCESS  read-write
	STATUS  mandatory
	DESCRIPTION
	"Device name if the lucentPM4SerialTypeDevice is enabled.
	This is a string of characters (e.g. /dev/tty1) indicating
	the device name."
	::= { lucentPM4SerialEntry 16 }

lucentPM4SerialDirection OBJECT-TYPE
	SYNTAX  INTEGER {
		in(1),
		out(2),
		inout(3)
	}
	ACCESS  read-only
	STATUS  mandatory
	DESCRIPTION
	"The direction the active session was initiated."
	::= { lucentPM4SerialEntry 17 }

lucentPM4SerialStarted OBJECT-TYPE
	SYNTAX  TimeTicks
	ACCESS  read-only
	STATUS  mandatory
	DESCRIPTION
	"The amount of time this session has been active."
	::= { lucentPM4SerialEntry 18 }

lucentPM4SerialIdle OBJECT-TYPE
	SYNTAX  TimeTicks
	ACCESS  read-only
	STATUS  mandatory
	DESCRIPTION
	"The amount of time this session has been idle."
	::= { lucentPM4SerialEntry 19 }

lucentPM4SerialInSpeed OBJECT-TYPE
	SYNTAX  Gauge
	ACCESS  read-only
	STATUS  mandatory
	DESCRIPTION
	"An estimate of the serial interface's current inbound
	bandwidth in bits per second."
	::= { lucentPM4SerialEntry 20 }

lucentPM4SerialOutSpeed OBJECT-TYPE
	SYNTAX  Gauge
	ACCESS  read-only
	STATUS  mandatory
	DESCRIPTION
	"An estimate of the serial interface's current outbound
	bandwidth in bits per second."
	::= { lucentPM4SerialEntry 21 }

lucentPM4SerialIpAddress OBJECT-TYPE
	SYNTAX  IpAddress
	ACCESS  read-only
	STATUS  mandatory
	DESCRIPTION
	"The IP address associated with the serial interface.
	If being used as a network type port, this is the
	remote user's IP address.  If being used as a device or
	login, this is the IP address of the host the user is
	connected to."
	::= { lucentPM4SerialEntry 22 }

lucentPM4SerialifDescr OBJECT-TYPE
	SYNTAX  DisplayString (SIZE (0..255))
	ACCESS  read-only
	STATUS  mandatory
	DESCRIPTION
	"A textual string containing information about the
	network interface bound to the serial interface."
	::= { lucentPM4SerialEntry 23 }

lucentPM4SerialInOctets OBJECT-TYPE
	SYNTAX  Counter
	ACCESS  read-only
	STATUS  mandatory
	DESCRIPTION
	"The total number of octets received on the
	serial interface."
	::= { lucentPM4SerialEntry 24 }

lucentPM4SerialOutOctets OBJECT-TYPE
	SYNTAX  Counter
	ACCESS  read-only
	STATUS  mandatory
	DESCRIPTION
	"The total number of octets transmitted on the
	serial interface."
	::= { lucentPM4SerialEntry 25 }

lucentPM4SerialQOctets OBJECT-TYPE
	SYNTAX  Counter
	ACCESS  read-only
	STATUS  mandatory
	DESCRIPTION
	"The total number of octets queued on the
	serial interface."
	::= { lucentPM4SerialEntry 26 }

-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
-- The Lucent T1/E1 Interface table
-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
lucentPM4CmT1E1	OBJECT IDENTIFIER ::= { lucentPM4CmInterfaces 2 }

lucentPM4T1E1Table OBJECT-TYPE
	SYNTAX  SEQUENCE OF LucentPM4T1E1Entry
	ACCESS  not-accessible
	STATUS  mandatory
	DESCRIPTION
	"A list of T1/E1 interface entries."
	::= { lucentPM4CmT1E1 1 }

lucentPM4T1E1Entry OBJECT-TYPE
	SYNTAX  LucentPM4T1E1Entry
	ACCESS  not-accessible
	STATUS  mandatory
	DESCRIPTION
	"A T1/E1 entry containing objects at the physical layer."
	INDEX   { lucentPM4T1E1BoardIndex,
		    lucentPM4T1E1Index }
	::= { lucentPM4T1E1Table 1 }

LucentPM4T1E1Entry ::=
	SEQUENCE {
		lucentPM4T1E1BoardIndex
			INTEGER,
		lucentPM4T1E1UnitType
			PMUnitType,
		lucentPM4T1E1Index
			INTEGER,
		lucentPM4T1E1SerialIndex
			INTEGER,
		lucentPM4T1E1SerialCount
			INTEGER,
		lucentPM4T1E1PhysType
			INTEGER,
		lucentPM4T1E1Status
			INTEGER,
		lucentPM4T1E1Function
			INTEGER,
		lucentPM4T1E1Framing
			INTEGER,
		lucentPM4T1E1Encoding
			INTEGER,
		lucentPM4T1E1PCM
			INTEGER,
		lucentPM4T1E1SuperSignal
			INTEGER,
		lucentPM4T1E1StartMode
			INTEGER,
		lucentPM4T1E1ChangeTime
			TimeTicks,
		lucentPM4T1E1RecvLevel
			Gauge,
		lucentPM4T1E1BlueAlarms
			Counter,
		lucentPM4T1E1YellowAlarms
			Counter,
		lucentPM4T1E1CarrierLoss
			Counter,
		lucentPM4T1E1SyncLoss
			Counter,
		lucentPM4T1E1BipolarErrors
			Counter,
		lucentPM4T1E1CRCErrors
			Counter,
		lucentPM4T1E1SyncErrors
			Counter
	}

lucentPM4T1E1BoardIndex OBJECT-TYPE
	SYNTAX  INTEGER
	ACCESS  read-only
	STATUS  mandatory
	DESCRIPTION
	"A unique value for each board in the PM4 chassis."
	::= { lucentPM4T1E1Entry 1 }

lucentPM4T1E1UnitType OBJECT-TYPE
	SYNTAX  PMUnitType
	ACCESS  read-only
	STATUS  mandatory
	DESCRIPTION
	"This object specifies the type of the unit as the T1/E1
	line. This value is a part of the interface table ifIndex."
	::= { lucentPM4T1E1Entry 2 }

lucentPM4T1E1Index OBJECT-TYPE
	SYNTAX  INTEGER
	ACCESS  read-only
	STATUS  mandatory
	DESCRIPTION
	"A unique value for each T1/E1 interface."
	::= { lucentPM4T1E1Entry 3 }

lucentPM4T1E1SerialIndex OBJECT-TYPE
	SYNTAX  INTEGER
	ACCESS  read-only
	STATUS  mandatory
	DESCRIPTION
	"The value of the instance for the serial port. This object
	provides a cross reference from the T1/E1 interface to the
	serial port to which it is mapped."
	::= { lucentPM4T1E1Entry 4 }

lucentPM4T1E1SerialCount OBJECT-TYPE
	SYNTAX  INTEGER
	ACCESS  read-only
	STATUS  mandatory
	DESCRIPTION
	"The number of serial ports assigned to this interface. If this is
	a Channelized T1/E1, then the count is 24/32. If this is a fractional
	T1/E1, then the count can be any number between 1 and a number less
	than 24/32."
	::= { lucentPM4T1E1Entry 5 }

lucentPM4T1E1PhysType OBJECT-TYPE
	SYNTAX  INTEGER {
		t1(1),
		e1(2)
	}
	ACCESS  read-only
	STATUS  mandatory
	DESCRIPTION
	"The type of interface (T1 or E1)."
	::= { lucentPM4T1E1Entry 6 }

lucentPM4T1E1Status OBJECT-TYPE
	SYNTAX  PMEquipPRIStatus
	ACCESS  read-only
	STATUS  mandatory
	DESCRIPTION
	"The current operational status of the interface."
	::= { lucentPM4T1E1Entry 7 }

lucentPM4T1E1Function OBJECT-TYPE
	SYNTAX  INTEGER {
		isdn(1),
		channelized(2),
		clear(3),
		fractional(4),
		isdnfrac(5)
	}
	ACCESS  read-only
	STATUS  mandatory
	DESCRIPTION
