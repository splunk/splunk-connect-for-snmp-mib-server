-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
-- The Lucent Modem Interface Configuration table
-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
lucentPM4CmModem	OBJECT IDENTIFIER ::= { lucentPM4CmInterfaces 3 }

lucentPM4ModemTable OBJECT-TYPE
	SYNTAX  SEQUENCE OF LucentPM4ModemEntry
	ACCESS  not-accessible
	STATUS  mandatory
	DESCRIPTION
	"A list of modem entries."
	::= { lucentPM4CmModem 1 }

lucentPM4ModemEntry OBJECT-TYPE
	SYNTAX  LucentPM4ModemEntry
	ACCESS  not-accessible
	STATUS  mandatory
	DESCRIPTION
	"A modem entry containing objects at the session layer."
	INDEX   { lucentPM4ModemBoardIndex,
		    lucentPM4ModemUnitType,
		    lucentPM4ModemIndex }
	::= { lucentPM4ModemTable 1 }

LucentPM4ModemEntry ::=
	SEQUENCE {
		lucentPM4ModemBoardIndex
			INTEGER,
		lucentPM4ModemUnitType
			PMUnitType,
		lucentPM4ModemIndex
			INTEGER,
		lucentPM4ModemPortName
			DisplayString,
		lucentPM4ModemStatus
			INTEGER,
		lucentPM4ModemProtocol
			INTEGER,
		lucentPM4ModemCompression
			INTEGER,
		lucentPM4ModemInSpeed
			Gauge,
		lucentPM4ModemOutSpeed
			Gauge,
		lucentPM4ModemInByteCount
			Counter,
		lucentPM4ModemOutByteCount
			Counter,
		lucentPM4ModemRetrains
			Counter,
		lucentPM4ModemRenegotiates
			Counter,
		lucentPM4ModemCalls
			Counter,
		lucentPM4ModemDetects
			Counter,
		lucentPM4ModemConnects
			Counter
	}

lucentPM4ModemBoardIndex OBJECT-TYPE
	SYNTAX  INTEGER
	ACCESS  read-only
	STATUS  mandatory
	DESCRIPTION
	"A unique value for each modem interface."
	::= { lucentPM4ModemEntry 1 }

lucentPM4ModemUnitType OBJECT-TYPE
	SYNTAX  PMUnitType
	ACCESS  read-only
	STATUS  mandatory
	DESCRIPTION
	"Unit type specifies the type of device or interface."
	::= { lucentPM4ModemEntry 2 }

lucentPM4ModemIndex OBJECT-TYPE
	SYNTAX  INTEGER
	ACCESS  read-only
	STATUS  mandatory
	DESCRIPTION
	"A unique value for each modem interface. The value of
	this object can be 1 - 96 for a Quad T1, 1 - 94 for a
	Tri E1 card."
	::= { lucentPM4ModemEntry 3 }

lucentPM4ModemPortName OBJECT-TYPE
	SYNTAX  DisplayString (SIZE (0..8))
	ACCESS  read-only
	STATUS  mandatory
	DESCRIPTION
	"A textual string containing the name of the serial
	interface (ie. S0, S1, etc)."
	::= { lucentPM4ModemEntry 4 }

lucentPM4ModemStatus OBJECT-TYPE
	SYNTAX  INTEGER {
		none(1),
		bound(2),
		connecting(3),
		active(4),
		test(5),
		down(6),
		ready(7),
		halt(8),
		admin(9)
	}
	ACCESS  read-only
	STATUS  mandatory
	DESCRIPTION
	"A current state of the modem."
	::= { lucentPM4ModemEntry 5 }

lucentPM4ModemProtocol OBJECT-TYPE
	SYNTAX  INTEGER {
		none(1),
		lapm(2),
		mnp(3),
		bufferd(4),
		direct(5),
		cellular(6)
	}
	ACCESS  read-only
	STATUS  mandatory
	DESCRIPTION
	"The error correcting protocol being used in the modem."
	::= { lucentPM4ModemEntry 6 }

lucentPM4ModemCompression OBJECT-TYPE
	SYNTAX  INTEGER {
		none(1),
		v42bis(2),
		mnp5(3),
		stac(4)
	}
	ACCESS  read-only
	STATUS  mandatory
	DESCRIPTION
	"The compression being used in the modem interface."
	::= { lucentPM4ModemEntry 7 }

lucentPM4ModemInSpeed OBJECT-TYPE
	SYNTAX  Gauge
	ACCESS  read-only
	STATUS  mandatory
	DESCRIPTION
	"An estimate of the modem interface's current inbound
	bandwidth in bits per second."
	::= { lucentPM4ModemEntry 8 }

lucentPM4ModemOutSpeed OBJECT-TYPE
	SYNTAX  Gauge
	ACCESS  read-only
	STATUS  mandatory
	DESCRIPTION
	"An estimate of the modem interface's current outbound
	bandwidth in bits per second."
	::= { lucentPM4ModemEntry 9 }

lucentPM4ModemInByteCount OBJECT-TYPE
	SYNTAX  Counter
	ACCESS  read-only
	STATUS  mandatory
	DESCRIPTION
	"The total number of bytes received on the
	serial interface."
	::= { lucentPM4ModemEntry 10 }

lucentPM4ModemOutByteCount OBJECT-TYPE
	SYNTAX  Counter
	ACCESS  read-only
	STATUS  mandatory
	DESCRIPTION
	"The total number of bytes transmitted on the
	serial interface."
	::= { lucentPM4ModemEntry 11 }

lucentPM4ModemRetrains OBJECT-TYPE
	SYNTAX  Counter
	ACCESS  read-only
	STATUS  mandatory
	DESCRIPTION
	"The number of retrains attempted by the modem."
	::= { lucentPM4ModemEntry 12 }

