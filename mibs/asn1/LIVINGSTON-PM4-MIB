-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
-- Lucent T1/E1 Performance Management Previous Intervals Table
-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
lucentPM4T1E1PMInt	OBJECT-TYPE
	SYNTAX	SEQUENCE OF LucentPM4T1E1PMIntEntry
	ACCESS	not-accessible
	STATUS	mandatory
	DESCRIPTION
	"Performance management table representing the performance
	statistics of T1/E1 interfaces in the box. This table represents
	the 24 hr statistics divided into 96 15 mins intervals. This 
	object is not accessible and present for clarity purpose. 
	This table is part of RFC 1406."
	::= { lucentPM4PerfMgmt 2 }

lucentPM4T1E1PMIntEntry	OBJECT-TYPE
	SYNTAX	LucentPM4T1E1PMIntEntry
	ACCESS	not-accessible
	STATUS	mandatory
	DESCRIPTION
	"Performance management table entries for all the T1/E1 interfaces 
	in the box. This table represents the 24 hr statistics divided into
	96 15 mins intervals. This object is not accessible and present for 
	clarity purpose."
	INDEX	{ lucentPM4T1E1PMIntBoard,
		  lucentPM4T1E1PMIntUnitType,
		  lucentPM4T1E1PMIntLineNum,
		  lucentPM4T1E1PMIntInterval }
	::= { lucentPM4T1E1PMInt 1 }

LucentPM4T1E1PMIntEntry ::=
	SEQUENCE {
		lucentPM4T1E1PMIntBoard
			INTEGER,
		lucentPM4T1E1PMIntUnitType
			PMUnitType,
		lucentPM4T1E1PMIntLineNum
			INTEGER,
		lucentPM4T1E1PMIntIfIndex
			INTEGER,
		lucentPM4T1E1PMIntInterval
			INTEGER,
		lucentPM4T1E1PMIntESs
			Gauge,
		lucentPM4T1E1PMIntSESs
			Gauge,
		lucentPM4T1E1PMIntSEFs
			Gauge,
		lucentPM4T1E1PMIntUASs
			Gauge,
		lucentPM4T1E1PMIntCSSs
			Gauge,
		lucentPM4T1E1PMIntPCVs
			Gauge,
		lucentPM4T1E1PMIntLESs
			Gauge,
		lucentPM4T1E1PMIntBESs
			Gauge,
		lucentPM4T1E1PMIntDMs
			Gauge,
		lucentPM4T1E1PMIntLCVs
			Gauge
	}

lucentPM4T1E1PMIntBoard	OBJECT-TYPE
	SYNTAX	INTEGER
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION
	"Board number of the interface. The global interface number is 
	computed by using the Most Significant nibble of the ifIndex 
	and the Least Significant nibble represents the interface index.
	Thus board 0 interface 3 is represented as 0x03 or 03 decimal 
	and board 10 interface 3 is represented as 0xa3 or 163 decimal.
	In an integer, of 4 bytes wide, the 3 MSBytes will all be zeros.
	The global interface number corresponds to the IfIndex of MIB II."
	::= { lucentPM4T1E1PMIntEntry 1 }

lucentPM4T1E1PMIntUnitType	OBJECT-TYPE
	SYNTAX	PMUnitType
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION
	"Unit type indicates the type of physical or logical device. The
	unit type for this table is either T1 or E1."
	::= { lucentPM4T1E1PMIntEntry 2 }

lucentPM4T1E1PMIntLineNum	OBJECT-TYPE
	SYNTAX	INTEGER
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION
	"Line number uniquely identifies the T1/E1 interface for this
	board."
	::= { lucentPM4T1E1PMIntEntry 3 }

lucentPM4T1E1PMIntInterval	OBJECT-TYPE
	SYNTAX	INTEGER (1..96)
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION
	"Interval number for a given board. The 24 hr period is
	divided into 96 15 min intervals, where 1 is the most recent 
	and 96 is the least recent."
	::= { lucentPM4T1E1PMIntEntry 4 }

lucentPM4T1E1PMIntIfIndex	OBJECT-TYPE
	SYNTAX	INTEGER
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION
	"Interface table ifIndex cross reference. The global interface 
	number is computed by using the Most Significant byte as the 
	board ID and the Least Significant 2 bytes represents the 
	interface index. The third byte represents the unit type which
	will be a T1 or E1. Thus board 0 interface 3 is represented as 
	0x00050003. The global interface number corresponds to the 
	IfIndex in MIB II."
	::= { lucentPM4T1E1PMIntEntry 5 }

lucentPM4T1E1PMIntESs	OBJECT-TYPE
	SYNTAX	Gauge
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION
	"The number of errored seconds, encountered by the line
	in the last 24 hrs divided into 96 15 mins intervals."
	::= { lucentPM4T1E1PMIntEntry 6 }

lucentPM4T1E1PMIntSESs	OBJECT-TYPE
	SYNTAX	Gauge
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION
	"The number of Severely Errored Seconds encountered by
	the line for one of the 96 15 mins intervals."
	::= { lucentPM4T1E1PMIntEntry 7 }

lucentPM4T1E1PMIntSEFSs	OBJECT-TYPE
	SYNTAX	Gauge
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION
	"The number of Severely Errored Framing Seconds 
	encountered by the line for one of the 96 15 mins 
	intervals."
	::= { lucentPM4T1E1PMIntEntry 8 }

lucentPM4T1E1PMIntUASs	OBJECT-TYPE
	SYNTAX	Gauge
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION
	"The number of Unavailable Seconds encountered by the line 
	for one of the 96 15 mins intervals."
	::= { lucentPM4T1E1PMIntEntry 9 }

lucentPM4T1E1PMIntCSSs	OBJECT-TYPE
	SYNTAX	Gauge
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION
	"The number of Controlled Slip Seconds encountered by the line 
	for one of the 96 15 mins intervals."
	::= { lucentPM4T1E1PMIntEntry 10 }

lucentPM4T1E1PMIntPCVs	OBJECT-TYPE
	SYNTAX	Gauge
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION
	"The number of Path Coding Violations encountered by the line 
	for one of the 96 15 mins intervals."
	::= { lucentPM4T1E1PMIntEntry 11 }

lucentPM4T1E1PMIntLESs	OBJECT-TYPE
	SYNTAX	Gauge
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION
	"The number of Line Errored Seconds encountered by the line 
	for one of the 96 15 mins intervals."
	::= { lucentPM4T1E1PMIntEntry 12 }

lucentPM4T1E1PMIntBESs	OBJECT-TYPE
	SYNTAX	Gauge
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION
	"The number of Bursty Errored Seconds encountered by the line 
	for one of the 96 15 mins intervals."
	::= { lucentPM4T1E1PMIntEntry 13 }

lucentPM4T1E1PMIntDMs	OBJECT-TYPE
	SYNTAX	Gauge
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION
	"The number of Degraded Minutes encountered by the line 
	for one of the 96 15 mins intervals."
	::= { lucentPM4T1E1PMIntEntry 14 }

lucentPM4T1E1PMIntLCVs	OBJECT-TYPE
	SYNTAX	Gauge
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION
	"The number of Line Code Violations encountered by the line 
	for one of the 96 15 mins intervals."
	::= { lucentPM4T1E1PMIntEntry 15 }

-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
-- Lucent T1/E1 Performance Management Total Cumulative Values Table
-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
lucentPM4T1E1PMTotal	OBJECT-TYPE
	SYNTAX	SEQUENCE OF LucentPM4T1E1PMTotalEntry
	ACCESS	not-accessible
	STATUS	mandatory
	DESCRIPTION
	"Performance management table representing the performance
	statistics of T1/E1 interfaces in the box. This table represents
	the 24 hr statistics divided into 96 15 mins intervals. This 
	object is not accessible and present for clarity purpose. 
	This table is part of RFC 1406."
	::= { lucentPM4PerfMgmt 3 }

lucentPM4T1E1PMTotalEntry	OBJECT-TYPE
	SYNTAX	LucentPM4T1E1PMIntEntry
	ACCESS	not-accessible
	STATUS	mandatory
	DESCRIPTION
	"Performance management table entries for all the T1/E1 interfaces 
	in the box. This table represents the 24 hr statistics divided into
	96 15 mins intervals. This object is not accessible and present for 
	clarity purpose."
	INDEX	{ lucentPM4T1E1PMTotalBoard,
		  lucentPM4T1E1PMTotalUnitType,
		  lucentPM4T1E1PMTotalLineNum,
		  lucentPM4T1E1PMTotalInterval }
	::= { lucentPM4T1E1PMTotal 1 }

LucentPM4T1E1PMTotalEntry ::=
	SEQUENCE {
		lucentPM4T1E1PMTotalBoard
			INTEGER,
		lucentPM4T1E1PMTotalUnitType
			PMUnitType,
		lucentPM4T1E1PMTotalLineNum
			INTEGER,
		lucentPM4T1E1PMTotalIfIndex
			INTEGER,
		lucentPM4T1E1PMTotalESs
			Gauge,
		lucentPM4T1E1PMTotalSESs
			Gauge,
		lucentPM4T1E1PMTotalSEFs
			Gauge,
		lucentPM4T1E1PMTotalUASs
			Gauge,
		lucentPM4T1E1PMTotalCSSs
			Gauge,
		lucentPM4T1E1PMTotalPCVs
			Gauge,
		lucentPM4T1E1PMTotalLESs
			Gauge,
		lucentPM4T1E1PMTotalBESs
			Gauge,
		lucentPM4T1E1PMTotalDMs
			Gauge,
		lucentPM4T1E1PMTotalLCVs
			Gauge
	}

lucentPM4T1E1PMTotalBoard	OBJECT-TYPE
	SYNTAX	INTEGER
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION
	"Board number of the interface. The global interface number is 
	computed by using the Most Significant nibble of the ifIndex 
	and the Least Significant nibble represents the interface index.
	Thus board 0 interface 3 is represented as 0x03 or 03 decimal 
	and board 10 interface 3 is represented as 0xa3 or 163 decimal.
	In an integer, of 4 bytes wide, the 3 MSBytes will all be zeros.
	The global interface number corresponds to the IfIndex of MIB II.
	This table stores the cumulative values for the past 24 hr period."
	::= { lucentPM4T1E1PMTotalEntry 1 }

lucentPM4T1E1PMTotalUnitType	OBJECT-TYPE
	SYNTAX	PMUnitType
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION
	"Unit type indicates the type of physical or logical device. The
	unit type for this table is either T1 or E1."
	::= { lucentPM4T1E1PMTotalEntry 2 }

lucentPM4T1E1PMTotalLineNum	OBJECT-TYPE
	SYNTAX	INTEGER
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION
	"Interface number for a given board. The global interface number is
	computed by using the Most Significant nibble of the ifIndex and 
	the Least Significant nibble represents the interface index. Thus 
	board 0 interface 3 is represented as 0x03 or 03 decimal and board 10
	interface 3 is represented as 0xa3 or 163 decimal. In an integer,
	of 4 bytes wide, the 3 MSBytes will all be zeros. The global interface
	number corresponds to the IfIndex in MIB II."
	::= { lucentPM4T1E1PMTotalEntry 3 }

lucentPM4T1E1PMTotalIfIndex	OBJECT-TYPE
	SYNTAX	INTEGER
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION
	"IfIndex cross reference value. This value is obtained from the
	Board/board number and interface number by combining them into
	the LSByte. The upper nibble represents the board and the lower
	nibble represents the line number."
	::= { lucentPM4T1E1PMTotalEntry 4 }

lucentPM4T1E1PMTotalESs	OBJECT-TYPE
	SYNTAX	Gauge
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION
	"The cumulative value of errored seconds, encountered by the line
	in the last 24 hrs."
	::= { lucentPM4T1E1PMTotalEntry 5 }

lucentPM4T1E1PMTotalSESs	OBJECT-TYPE
	SYNTAX	Gauge
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION
	"The cumulative value Severely Errored Seconds encountered by
	the line for the 24 hr period."
	::= { lucentPM4T1E1PMTotalEntry 6 }

lucentPM4T1E1PMTotalSEFSs	OBJECT-TYPE
	SYNTAX	Gauge
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION
	"The cumulative value of Severely Errored Framing Seconds 
	encountered by the line for the 24 hr period."
	::= { lucentPM4T1E1PMTotalEntry 7 }

lucentPM4T1E1PMTotalUASs	OBJECT-TYPE
	SYNTAX	Gauge
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION
	"The cumulative value of Unavailable Seconds encountered by the line 
	for the 24 hr period."
	::= { lucentPM4T1E1PMTotalEntry 8 }

lucentPM4T1E1PMTotalCSSs	OBJECT-TYPE
	SYNTAX	Gauge
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION
	"The cumulative value of Controlled Slip Seconds encountered by the line 
	for the 24 hr period."
	::= { lucentPM4T1E1PMTotalEntry 9 }

lucentPM4T1E1PMTotalPCVs	OBJECT-TYPE
	SYNTAX	Gauge
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION
	"The cumulative value of Path Coding Violations encountered by the line 
	for the 24 hr period."
	::= { lucentPM4T1E1PMTotalEntry 10 }

lucentPM4T1E1PMTotalLESs	OBJECT-TYPE
	SYNTAX	Gauge
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION
	"The cumulative value of Line Errored Seconds encountered by the line 
	for the 24 hr period."
	::= { lucentPM4T1E1PMTotalEntry 11 }

lucentPM4T1E1PMTotalBESs	OBJECT-TYPE
	SYNTAX	Gauge
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION
	"The cumulative value of Bursty Errored Seconds encountered by the line 
