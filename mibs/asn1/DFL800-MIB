    ::= { dfl800 2 }

dfl800MibModules OBJECT IDENTIFIER
    ::= { dfl800reg 1 }

dfl800MibConfs OBJECT IDENTIFIER
    ::= { dfl800reg 2 }

dfl800MibObjectGroups OBJECT IDENTIFIER
    ::= { dfl800reg 3 }


dfl800System OBJECT IDENTIFIER
    ::= { dfl800OSStats 1 }

dfl800SysCpuLoad OBJECT-TYPE
    SYNTAX      Gauge32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION 
        "The system cpu load."
    ::= { dfl800System 1 }

