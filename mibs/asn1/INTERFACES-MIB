testTable       OBJECT-TYPE
	SYNTAX  SEQUENCE OF TestTableEntry
	ACCESS  not-accessible
	STATUS  optional
	DESCRIPTION
		"Test control table."
	::= { maintenance 3 }

testTableEntry  OBJECT-TYPE
	SYNTAX  TestTableEntry
	ACCESS  not-accessible
	STATUS  mandatory
	DESCRIPTION
		"An entry in the testTable."
	INDEX   { testNearIndex, testFarIndex, testTableIndex }
	::= { testTable 1 }
	
