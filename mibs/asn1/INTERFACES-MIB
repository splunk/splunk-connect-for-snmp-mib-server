	SYNTAX  INTEGER
		{
		bertPatternSyncOther(1),
		bertPatternSyncNoTest(2),
		bertPatternSyncNoSync(3),
		bertPatternSyncInsync(4)
		}
	ACCESS  read-only
	STATUS  mandatory
	DESCRIPTION
		"Displays the current state of pattern sync during a test."
	::= { bertTableEntry 6 }

bertElapsedTime OBJECT-TYPE
	SYNTAX  INTEGER
	ACCESS  read-only
	STATUS  mandatory
