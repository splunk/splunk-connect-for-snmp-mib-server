-- Project DTS/HTS - Proxy Agent MIB
--
-- comments of the form	'BEGIN BLOCK' or are used to automate splitting
-- of this MIB into several sub-MIBs for each of the subabgents

-- BLOCK LIST system alarm error hard soft topo sql discov

-- BEGIN BLOCK common

SIEMENS-PN-MIB DEFINITIONS ::= BEGIN

IMPORTS
	MODULE-IDENTITY, OBJECT-TYPE, NOTIFICATION-TYPE,
	Counter32, Integer32
                   FROM SNMPv2-SMI
	TEXTUAL-CONVENTION, DisplayString, RowStatus, TestAndIncr
 
                   FROM SNMPv2-TC
	enterprises
                   FROM RFC1155-SMI
	OBJECT-GROUP
                   FROM SNMPv2-CONF;

hicomMib MODULE-IDENTITY
	LAST-UPDATED	"0101031300+0100"

	ORGANIZATION
		"Siemens AG, Dept. PN"
	CONTACT-INFO
		"Georg Geberthammer"
	DESCRIPTION
		"This is the MIB for Siemens 300 Hicom PBX and UNITY systems. It includes
		topology and system management, alarm management and hard/software
		management."
	::=	{ pn 1 }

sni				OBJECT IDENTIFIER ::= { enterprises 231 }
siemensUnits			OBJECT IDENTIFIER ::= { sni 7 }
pn				OBJECT IDENTIFIER ::= { siemensUnits 2 }

-- the hicom mib consists of the following subtrees/groups

hicomControl			OBJECT IDENTIFIER ::= { hicomMib 0 }
hicomSystem			OBJECT IDENTIFIER ::= { hicomMib 1 }
hicomAlarms			OBJECT IDENTIFIER ::= { hicomMib 2 }
hicomErrors			OBJECT IDENTIFIER ::= { hicomMib 3 }
hicomAlConf			OBJECT IDENTIFIER ::= { hicomMib 4 }
hicomSoft			OBJECT IDENTIFIER ::= { hicomMib 5 }
hicomHard			OBJECT IDENTIFIER ::= { hicomMib 6 }
hicomTopo			OBJECT IDENTIFIER ::= { hicomMib 7 }
hicomSQL			OBJECT IDENTIFIER ::= { hicomMib 8 }
hicomDiscov			OBJECT IDENTIFIER ::= { hicomMib 9 }
hicomMibConformance		OBJECT IDENTIFIER ::= { hicomMib 20 }

hicomTrapGroup			OBJECT IDENTIFIER ::= { hicomMib 21 }
hicomTrapVariables		OBJECT IDENTIFIER ::= { hicomTrapGroup 1 }
hicomTraps			OBJECT IDENTIFIER ::= { hicomTrapGroup 2 }

hicomSystemTrapVariables	OBJECT IDENTIFIER ::= { hicomTrapVariables 1}
hicomAlarmTrapVariables		OBJECT IDENTIFIER ::= { hicomTrapVariables 2}
hicomErrorTrapVariables		OBJECT IDENTIFIER ::= { hicomTrapVariables 3}
hicomSWTrapVariables         	OBJECT IDENTIFIER ::= { hicomTrapVariables 4}
hicomAlConfTrapVariables	OBJECT IDENTIFIER ::= { hicomTrapVariables 5}
hicomHWTrapVariables		OBJECT IDENTIFIER ::= { hicomTrapVariables 6}
hicomTopoTrapVariables		OBJECT IDENTIFIER ::= { hicomTrapVariables 7}
hicomSQLTrapVariables		OBJECT IDENTIFIER ::= { hicomTrapVariables 8}
hicomDiscTrapVariables		OBJECT IDENTIFIER ::= { hicomTrapVariables 9}

hicomSystemTraps		OBJECT IDENTIFIER ::= { hicomTraps 1 }
hicomAlarmTraps			OBJECT IDENTIFIER ::= { hicomTraps 2 }
hicomErrorTraps			OBJECT IDENTIFIER ::= { hicomTraps 3 }
hicomAlConfTraps		OBJECT IDENTIFIER ::= { hicomTraps 4 }
hicomSWTraps			OBJECT IDENTIFIER ::= { hicomTraps 5 }
hicomHWTraps			OBJECT IDENTIFIER ::= { hicomTraps 6 }
hicomTopoTraps			OBJECT IDENTIFIER ::= { hicomTraps 7 }
hicomSQLTraps			OBJECT IDENTIFIER ::= { hicomTraps 8 }
hicomDiscTraps			OBJECT IDENTIFIER ::= { hicomTraps 9 }

hicomMibGroups			OBJECT IDENTIFIER ::= { hicomMibConformance 1 }

hicomCntrlTree			OBJECT IDENTIFIER ::= { hicomMibGroups 0 }
hicomSysTree			OBJECT IDENTIFIER ::= { hicomMibGroups 1 }
hicomAlarmTree			OBJECT IDENTIFIER ::= { hicomMibGroups 2 }
hicomErrorTree			OBJECT IDENTIFIER ::= {	hicomMibGroups 3 }
hicomAlConfTree			OBJECT IDENTIFIER ::= { hicomMibGroups 4 }
hicomSoftTree			OBJECT IDENTIFIER ::= {	hicomMibGroups 5 }
hicomHardTree			OBJECT IDENTIFIER ::= { hicomMibGroups 6 }
hicomTopoTree			OBJECT IDENTIFIER ::= { hicomMibGroups 7 }
hicomSQLtree 			OBJECT IDENTIFIER ::= { hicomMibGroups 8 }
hicomDiscovTree 		OBJECT IDENTIFIER ::= { hicomMibGroups 9 }

-- common definitions 
DiscoveryStates ::= INTEGER { 
			done (1), error (2), busy (3), finok (4), finerr (5), 
			kill (6),
			masterDone(11), masterError(12), masterBusy(13), 
			masterFinok(14), masterFinerr(15), errdelDone(21),
			errdelError(22), errdelBusy(23), errdelFinok(24),
			errdelFinerr(25) } 
DiscoveryModes	  ::= INTEGER { man (1), auto (2), undef (9) }
AlarmFilterStates ::= INTEGER { filterOn (1), filterOff (2) }
AlarmPriorities   ::= INTEGER { minor (1), major (2), device (3) }

-- BEGIN BLOCK system

-- the hicomControl Object Definitions

         hicomProxyName OBJECT-TYPE
            SYNTAX        DisplayString
            MAX-ACCESS    read-only
            STATUS        current
            DESCRIPTION
                  "Name of the Hicom proxy agent."
            ::= { hicomControl 1 }

         hicomAgentVersion OBJECT-TYPE
            SYNTAX        DisplayString
            MAX-ACCESS    read-only
            STATUS        current
            DESCRIPTION
                  "Version of the Hicom proxy agent. "
            ::= { hicomControl 2 }

         hicomMIBVersion OBJECT-TYPE
            SYNTAX        DisplayString
            MAX-ACCESS    read-only
            STATUS        current
            DESCRIPTION
                  "MIB Version of the Hicom proxy agent. "
            ::= { hicomControl 3 }


hicomControlGroup OBJECT-GROUP
	OBJECTS	{ 
			hicomProxyName, hicomAgentVersion, hicomMIBVersion
		}
	STATUS current
	DESCRIPTION
		"Conformance statement of the hicomControlGroup."
	::=	{ hicomCntrlTree 1 }


-- The hicomSystem MIB group provides system information of the hicom network

-- the hicomSystem group

hicomNumHicoms		OBJECT-TYPE
	SYNTAX		Integer32
	MAX-ACCESS	read-only
	STATUS		current
	DESCRIPTION	"The number of Hicom systems managed by this agent."
	::=	{ hicomSystem 1 }

hicomSystemChanges	OBJECT-TYPE
	SYNTAX		Counter32
	MAX-ACCESS	read-only
	STATUS		current
	DESCRIPTION		"When a change (SET) is made on hicomSysTable
				 or hicomForeignSysTable, the variable is
				 incremented by one."
	::=	{ hicomSystem 2 }

-- the hicomSysTable

hicomSysTable	OBJECT-TYPE
	SYNTAX		SEQUENCE OF	HicomSysEntry
	MAX-ACCESS	not-accessible
	STATUS		current
	DESCRIPTION	"The table of Hicom systems."
	::=	{ hicomSystem 3}

	hicomSysEntry	OBJECT-TYPE
		SYNTAX		HicomSysEntry
		MAX-ACCESS	not-accessible
		STATUS		current
		DESCRIPTION	"Each entry describes a Hicom system."
		INDEX		{ hicomSysPabxId }
		::=	{ hicomSysTable 1 }

	HicomSysEntry ::=
		SEQUENCE {
			hicomSysPabxId		Integer32,
			hicomSysConNo		DisplayString,
			hicomSysEstabl		Integer32,
			hicomSysPosNo		DisplayString,
			hicomSysMnemonic	DisplayString,
			hicomSysCustName	DisplayString,
			hicomSysCbCode		DisplayString,
			hicomSysTelNo		DisplayString,
			hicomSysCutOver		DisplayString,
			hicomSysPabxNo		DisplayString,
			hicomSysClnr		DisplayString,
			hicomSysLocation	DisplayString,
			hicomSysRemark		DisplayString,
			hicomSysSwLic		DisplayString,
			hicomSysSysType		DisplayString,
            hicomSysApsPa       DisplayString,
			hicomSysTimeStamp	Integer32,
			hicomSysDescription	DisplayString,
			hicomSysVersion		DisplayString,
			hicomSysCustomerSpecific	DisplayString,
			hicomSysStreetAddress	DisplayString,
			hicomSysLineType	DisplayString,
            hicomSysProduct     DisplayString,
			hicomSysCustomerContact	DisplayString,
			hicomSysServiceDistrict	DisplayString,
			hicomSysCustomerContract	DisplayString,
			hicomSysTimeOfLastFm    Integer32,
			hicomSysHicomSwVersion	DisplayString,
			hicomSysEndWarranty	DisplayString,
            hicomSysBatteryType DisplayString,
            hicomSysBatteryCapac    DisplayString,
            hicomSysInterface   DisplayString,
			hicomSysSystemRoomTel	DisplayString,
-- new unity fields 71-
			hicomSysNoType           DisplayString,
			hicomSysCustServer       DisplayString,
			hicomSysCustNoid         DisplayString,
  			hicomSysNoName           DisplayString,
			hicomSysRemarkCd         DisplayString,
			hicomSysHSystemRelease   DisplayString,
			hicomSysHcmAmolang       DisplayString,
			hicomSysRemarkCon        DisplayString,
			hicomSysIpAddress        DisplayString,
  			hicomSysRemarkCom        DisplayString,
  			hicomSysMsvProgNum       Integer32,
  			hicomSysMsvRegNum        Integer32,
  			hicomSysHcEquipNr1       DisplayString,
  			hicomSysHcEquipNr2       DisplayString,
  			hicomSysDaUrl            DisplayString,
			hicomSysHcmCorrState     DisplayString,
-- zusatzinfo fields 100 -
			hicomSysNodeNo		Integer32,
			hicomSysLang		INTEGER,
			hicomSysMgrNetId	OCTET STRING,
			hicomSysMgrSubNetId	OCTET STRING,
			hicomSysMgrPhysNetId	OCTET STRING,
			hicomSysMgrFlags	Integer32,
			hicomSysDiscTimeOutEvnt	INTEGER,
			hicomSysDiscAgedData	INTEGER,
			hicomSysDiscComType	INTEGER,
		    hicomSysAgentFlags    Integer32	
		}

