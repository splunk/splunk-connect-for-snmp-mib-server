-- The hicomHWDiscovery group

-- The hicomHWChanges counter

-- BEGIN BLOCK discov
hicomHWChanges		OBJECT-TYPE
	SYNTAX		Counter32
	MAX-ACCESS	read-only
	STATUS		current
	DESCRIPTION	"Indicates the number of finished hardware discoveries -
				corresponds to the number of hicomHWDiscovXXX traps sent out."
		::=	{ hicomHWDiscovery 1 }

-- The hicomHWDiscovTable table

hicomHWDiscovTable	OBJECT-TYPE
	SYNTAX		SEQUENCE OF HicomHWDiscovEntry
	MAX-ACCESS	not-accessible
	STATUS		current
	DESCRIPTION	"The table describing the status concerning
				discovery of HW information."
	::=	{ hicomHWDiscovery 2 }

	hicomHWDiscovEntry	OBJECT-TYPE
		SYNTAX		HicomHWDiscovEntry
		MAX-ACCESS	not-accessible
		STATUS		current
		DESCRIPTION	"Each entry describes the status of the discovery process
					for a specific hicom."
		INDEX		{ hicomHWDiscovPabxId }
		::=	{ hicomHWDiscovTable 1 }

	HicomHWDiscovEntry ::=
		SEQUENCE {
			hicomHWDiscovPabxId	Integer32,
			hicomHWDiscovPabxMnemonic	DisplayString,
			hicomHWDiscovStatus	DiscoveryStates,
			hicomHWDiscovMode	DiscoveryModes,
			hicomHWDiscovTimDat	DisplayString,
			hicomHWDiscovErrTimDat  DisplayString
		}

	hicomHWDiscovPabxId	OBJECT-TYPE
		SYNTAX	Integer32
		MAX-ACCESS		read-only
		STATUS	current
		DESCRIPTION		"Unique identifier of the Hicom being discovered."
		::=	{ hicomHWDiscovEntry 1 }

	hicomHWDiscovPabxMnemonic	OBJECT-TYPE
		SYNTAX	DisplayString
		MAX-ACCESS		read-only
		STATUS	current
		DESCRIPTION		"Name of the Hicom system. "
		::=	{ hicomHWDiscovEntry 2 }

	hicomHWDiscovStatus	OBJECT-TYPE
		SYNTAX		DiscoveryStates
		MAX-ACCESS	read-write
		STATUS		current
		DESCRIPTION	"Status of HW discovery process.
				A Manager may initiate a discovery by setting
				this variable to value busy.
				The other values are set by the agent.	"
		::=	{ hicomHWDiscovEntry 3 }

	hicomHWDiscovMode	OBJECT-TYPE
		SYNTAX		DiscoveryModes
		MAX-ACCESS	read-write
		STATUS		current
		DESCRIPTION	"Mode of discovery process."
		::=	{ hicomHWDiscovEntry 4 }

	hicomHWDiscovTimDat	OBJECT-TYPE
		SYNTAX	DisplayString
		MAX-ACCESS		read-only
		STATUS	current
		DESCRIPTION		"Date and time (year, month, day, hour, minute, second)
				the last successful discovery action was performed for the	Hicom."
		::=	{ hicomHWDiscovEntry 5 }

	hicomHWDiscovErrTimDat OBJECT-TYPE
		SYNTAX	DisplayString
		MAX-ACCESS		read-only
		STATUS	current
		DESCRIPTION		"Date and time (year, month, day, hour, minute, second)
				the last failed discovery action was performed for the	Hicom."
		::=	{ hicomHWDiscovEntry 6 }

-- BEGIN BLOCK hard
-- The hicomHWInfo group

	hicomHardSubagentLastMsgNo	OBJECT-TYPE
		SYNTAX		Integer32
		MAX-ACCESS	read-only
		STATUS		current
		DESCRIPTION	"The last message, warning or error number 
					issued by the Hardware subagent."
		::=	{ hicomHWInfo 1 }

	hicomHardSubagentLastMsgText	OBJECT-TYPE
		SYNTAX		OCTET STRING
		MAX-ACCESS	read-only
		STATUS		current
		DESCRIPTION	"The last message, warning or error text
					issued by the Hardware subagent."
		::=	{ hicomHWInfo 2 }

	hicomHardSubagentResultData	OBJECT-TYPE
	        SYNTAX		Integer32
	        MAX-ACCESS	read-write
	        STATUS		current
	        DESCRIPTION	"Is used for agent internal communication.
			 Contains the PabxId of a Hicom for which
			 the last discovery process was conveyed.
			 Is SET by the discovery agent and
			 indicates the availability of new discovered data.
			 The new data is to be incorporated by the subagent
			 into the subagent's data base."
		::=	{ hicomHWInfo 3 }


