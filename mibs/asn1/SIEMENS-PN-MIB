hicomForeignSysTable	OBJECT-TYPE
	SYNTAX		SEQUENCE OF	HicomForeignSysEntry
	MAX-ACCESS	not-accessible
	STATUS		current
	DESCRIPTION	"The table describes foreign systems connected to 
			the Hicom network. 
			The content of the table is created and maintained 
			by the manager application."
	::=	{ hicomSystem 6}

	hicomForeignSysEntry	OBJECT-TYPE
		SYNTAX		HicomForeignSysEntry
		MAX-ACCESS	not-accessible
		STATUS		current
		DESCRIPTION	"Each entry describes a foreing system 
				connected to the Hicom network."
		INDEX		{ hicomForSysPhysNetId, hicomForSysNodeNo }
		::=	{ hicomForeignSysTable 1 }

	HicomForeignSysEntry ::=
		SEQUENCE {
			hicomForSysPhysNetId	OCTET STRING,
			hicomForSysNodeNo	Integer32,
			hicomForSysName		OCTET STRING,
			hicomForSysNetId	OCTET STRING,
			hicomForSysSubNetId	OCTET STRING,
			hicomForSysProvNetId	OCTET STRING,
			hicomForSysProvSubNetId	OCTET STRING,
			hicomForSysRowStat	RowStatus
		}

	hicomForSysPhysNetId	OBJECT-TYPE
		SYNTAX		OCTET STRING   (SIZE (1..32))
		MAX-ACCESS 	not-accessible
		STATUS		current
		DESCRIPTION		"Physical net identification of the 
					foreign system."
		::=	{ hicomForeignSysEntry 1 }

	hicomForSysNodeNo	OBJECT-TYPE
		SYNTAX		Integer32
		MAX-ACCESS 	not-accessible
		STATUS		current
		DESCRIPTION		"Node number (physical net) of the 
					foreign system."
		::=	{ hicomForeignSysEntry 2 }

	hicomForSysName		OBJECT-TYPE
		SYNTAX		OCTET STRING   (SIZE (0..32))
		MAX-ACCESS 	read-create
		STATUS		current
		DESCRIPTION		"Name of the foreign system."
		::=	{ hicomForeignSysEntry 3 }

	hicomForSysNetId	OBJECT-TYPE
		SYNTAX		OCTET STRING  (SIZE (0..32))
		MAX-ACCESS 	read-create
		STATUS		current
		DESCRIPTION		"Identifier of the network to which 
					the foreign system belongs."
		::=	{ hicomForeignSysEntry 4 }

	hicomForSysSubNetId	OBJECT-TYPE
		SYNTAX		OCTET STRING  (SIZE (0..64))
		MAX-ACCESS 	read-create
		STATUS		current
		DESCRIPTION		"Identifier of the sub network to 
					which the foreign system belongs."
		::=	{ hicomForeignSysEntry 5 }

	hicomForSysProvNetId	OBJECT-TYPE
		SYNTAX		OCTET STRING  (SIZE (0..32))
		MAX-ACCESS 	read-create
		STATUS		current
		DESCRIPTION		"Net identifier of the provider 
					network the foreign system 
					belongs to."
		::=	{ hicomForeignSysEntry 6 }

	hicomForSysProvSubNetId	OBJECT-TYPE
		SYNTAX		OCTET STRING   (SIZE (0..64))
		MAX-ACCESS 	read-create
		STATUS		current
		DESCRIPTION		"Subnet identifier of the provider 
					network the foreign system belongs to."
		::=	{ hicomForeignSysEntry 7 }

	hicomForSysRowStat      OBJECT-TYPE
		SYNTAX		RowStatus
		MAX-ACCESS	read-create
		STATUS		current
		DESCRIPTION
				    "Setting a non-existing RowStat 
				     variable to 'createAndGo', creates a new
				     row. If creation was successful, the row
				     gets the status 'active'. Variables of
				     an 'active' row can be read and modified.
				     Setting RowStat to 'destroy' effects 
				     the deletion of the row."
		::= { hicomForeignSysEntry 8 }

hicomSystemGroup OBJECT-GROUP
	OBJECTS	{ 
			hicomNumHicoms, 
