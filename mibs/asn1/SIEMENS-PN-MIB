-- hicomPeripherals group

-- The hicom peripherals table, contains information on installed peripherals

hicomPeriphTable	OBJECT-TYPE
	SYNTAX		SEQUENCE OF HicomPeriphEntry
	MAX-ACCESS	not-accessible
	STATUS		current
	DESCRIPTION	"The table of installed devices: disks, tapes etc."
	::=	{ hicomPeripherals 1 }

	hicomPeriphEntry	OBJECT-TYPE
		SYNTAX		HicomPeriphEntry
		MAX-ACCESS	not-accessible
		STATUS		current
		DESCRIPTION	"Each entry describes a device."
		INDEX		{ hicomPeriphPabxId, hicomPeriphContrID, hicomPeriphModule }
		::=	{ hicomPeriphTable 1 }

	HicomPeriphEntry ::=
		SEQUENCE {
			hicomPeriphPabxId	Integer32,
			hicomPeriphContrID	Integer32,
			hicomPeriphModule	OCTET STRING,
			hicomPeriphType		DisplayString,
			hicomPeriphSSNo		DisplayString,
			hicomPeriphSize		Integer32,
			hicomPeriphGran		Integer32
		}

	hicomPeriphPabxId	OBJECT-TYPE
		SYNTAX		Integer32
		MAX-ACCESS	read-only
		STATUS		current
		DESCRIPTION	"Unique identifier of the Hicom to which the 
					peripheral device belongs."
		::=	{ hicomPeriphEntry 1 }

	hicomPeriphContrID	OBJECT-TYPE
		SYNTAX		Integer32
		MAX-ACCESS	read-only
		STATUS		current
		DESCRIPTION	"Identifier of the controller the device is connected to."
		::=	{ hicomPeriphEntry 2 }

	hicomPeriphModule	OBJECT-TYPE
		SYNTAX		OCTET STRING
		MAX-ACCESS	read-only
		STATUS		current
		DESCRIPTION	"Identifier of the related sever module: 
					A1, C1, V1-V3 or T1-T3. "
		::=	{ hicomPeriphEntry 3 }

	hicomPeriphType		OBJECT-TYPE
		SYNTAX		DisplayString
		MAX-ACCESS	read-only
		STATUS		current
		DESCRIPTION	"Abbreviation of the device type: HD, FT, CT ..."
		::=	{ hicomPeriphEntry 4 }

	hicomPeriphSSNo		OBJECT-TYPE
		SYNTAX		DisplayString
		MAX-ACCESS	read-only
		STATUS		current
		DESCRIPTION	"Product identification."
		::=	{ hicomPeriphEntry 5 }

	hicomPeriphSize		OBJECT-TYPE
		SYNTAX		Integer32
		MAX-ACCESS	read-only
		STATUS		current
		DESCRIPTION	"Capacity (MB) of the storage medium."
		::=	{ hicomPeriphEntry 6 }

	hicomPeriphGran		OBJECT-TYPE
		SYNTAX		Integer32
		MAX-ACCESS	read-only
		STATUS		current
		DESCRIPTION	"Sector size of the medium."
		::=	{ hicomPeriphEntry 7 }

-- The hicomBoards group

-- The CDSMTable, describes hicom server module related boards which are
-- currently installed
-- The contents are derived from the Hicom AMO ABFR-CDSM

hicomCDSMTable		OBJECT-TYPE
	SYNTAX		SEQUENCE OF HicomCDSMEntry
	MAX-ACCESS	not-accessible
	STATUS		current
	DESCRIPTION	"The table of hicom CDSM boards."
	::=	{ hicomBoards 1 }

	hicomCDSMEntry		OBJECT-TYPE
		SYNTAX		HicomCDSMEntry
		MAX-ACCESS	not-accessible
		STATUS		current
		DESCRIPTION	"Each entry describes one hardware board."
		INDEX		{ hicomCDSMPabxId, hicomCDSMCabAddr, hicomCDSMMntLevel,
					hicomCDSMSlotAddr}
		::=	{ hicomCDSMTable 1 }

	HicomCDSMEntry ::=
		SEQUENCE {
			hicomCDSMPabxId		Integer32,
			hicomCDSMCabAddr	OCTET STRING,
			hicomCDSMMntLevel	Integer32,
			hicomCDSMSlotAddr	Integer32,
			hicomCDSMPartNo		DisplayString,
			hicomCDSMCode		DisplayString,
			hicomCDSMVers		Integer32,
			hicomCDSMFirmware	DisplayString
		}

	hicomCDSMPabxId		OBJECT-TYPE
		SYNTAX		Integer32
		MAX-ACCESS	read-only
		STATUS		current
		DESCRIPTION	"Identifier of the Hicom to which the Board belongs."
		::=	{ hicomCDSMEntry 1 }

	hicomCDSMCabAddr		OBJECT-TYPE
		SYNTAX		OCTET STRING
		MAX-ACCESS	read-only
		STATUS		current
		DESCRIPTION	"Identifier of the cabinet to which the board belongs."
		::=	{ hicomCDSMEntry 2 }

	hicomCDSMMntLevel	OBJECT-TYPE
		SYNTAX		Integer32
		MAX-ACCESS	read-only
		STATUS		current
		DESCRIPTION	"Mounting level of the frame containing the board. "
		::=	{ hicomCDSMEntry 3 }

	hicomCDSMSlotAddr	OBJECT-TYPE
		SYNTAX		Integer32
		MAX-ACCESS	read-only
		STATUS		current
		DESCRIPTION	"Slot address for the board within its frame."
		::=	{ hicomCDSMEntry 4 }

	hicomCDSMPartNo		OBJECT-TYPE
		SYNTAX		DisplayString
		MAX-ACCESS	read-only
		STATUS		current
		DESCRIPTION	"Siemens part number of the installed board."
		::=	{ hicomCDSMEntry 5 }

	hicomCDSMCode	OBJECT-TYPE
		SYNTAX		DisplayString
		MAX-ACCESS	read-only
		STATUS		current
		DESCRIPTION	"Short name of the board."
		::=	{ hicomCDSMEntry 6 }

	hicomCDSMVers	OBJECT-TYPE
		SYNTAX		Integer32
		MAX-ACCESS	read-only
		STATUS		current
		DESCRIPTION	"Version number of the board."
		::=	{ hicomCDSMEntry 7 }

	hicomCDSMFirmware	OBJECT-TYPE
		SYNTAX		DisplayString
		MAX-ACCESS	read-only
		STATUS		current
		DESCRIPTION	"Firmware version installed on the board."
		::=	{ hicomCDSMEntry 8 }

-- The CDSUTable, describes hicom SWU-CC related boards which are
-- currently installed
-- The contents are derived from the Hicom AMO ABFR-CDSU

hicomCDSUTable		OBJECT-TYPE
	SYNTAX		SEQUENCE OF HicomCDSUEntry
	MAX-ACCESS	not-accessible
	STATUS		current
	DESCRIPTION	"The table of hicom CDSU boards."
