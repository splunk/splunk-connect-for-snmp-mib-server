			hicomSysLocation, hicomSysRemark, hicomSysSwLic,
			hicomSysSysType, hicomSysApsPa, hicomSysTimeStamp, 
			hicomSysDescription, hicomSysVersion,
            hicomSysCustomerSpecific, hicomSysStreetAddress,
			hicomSysLineType, hicomSysProduct, hicomSysCustomerContact,
            hicomSysServiceDistrict,
			hicomSysCustomerContract, hicomSysTimeOfLastFm, 
			hicomSysHicomSwVersion, hicomSysEndWarranty,
		    hicomSysBatteryType, hicomSysBatteryCapac,
            hicomSysInterface,hicomSysSystemRoomTel,
		
-- unity start
			hicomSysNoType           ,
			hicomSysCustServer       ,
			hicomSysCustNoid         ,
  			hicomSysNoName           ,
			hicomSysRemarkCd         ,
			hicomSysHSystemRelease   ,
			hicomSysHcmAmolang       ,
			hicomSysRemarkCon        ,
			hicomSysIpAddress        ,
  			hicomSysRemarkCom        ,
  			hicomSysMsvProgNum       ,
  			hicomSysMsvRegNum        ,
  			hicomSysHcEquipNr1       ,
  			hicomSysHcEquipNr2       ,
  			hicomSysDaUrl            ,
			hicomSysHcmCorrState     ,
-- unity end
			hicomSysNodeNo, hicomSysLang, 
			hicomSysMgrNetId, hicomSysMgrSubNetId, 
			hicomSysMgrPhysNetId, hicomSysMgrFlags,
			hicomSysDiscTimeOutEvnt, hicomSysDiscAgedData,
			hicomSysDiscComType,hicomSysAgentFlags, 
			
			hicomSystemChanges, 
			hicomSysSubagentLastMsgNo, hicomSysSubagentLastMsgText,
			hicomForSysName, hicomForSysNetId, hicomForSysSubNetId,
			hicomForSysProvNetId, hicomForSysProvSubNetId,
			hicomForSysRowStat
		}
	STATUS current
	DESCRIPTION
		"Conformance statement of the hicomSystemGroup."
	::=	{ hicomSysTree 1 }

