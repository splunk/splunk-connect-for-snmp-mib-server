hicomSoftChanges		OBJECT-TYPE
	SYNTAX		Counter32
	MAX-ACCESS	read-only
	STATUS		current
	DESCRIPTION	"Indicates changes in the hicomSoft group."
	::=	{ hicomSoftDiscovery 1 }

-- the hicomSoftDiscov table

hicomSoftDiscovTable	OBJECT-TYPE
	SYNTAX	SEQUENCE OF HicomSoftDiscovEntry
	MAX-ACCESS		not-accessible
	STATUS	current
	DESCRIPTION	"This table contains information about the software
				discoveries for each managed Hicom."
	::=	{ hicomSoftDiscovery 2 }

	hicomSoftDiscovEntry	OBJECT-TYPE
		SYNTAX		HicomSoftDiscovEntry
		MAX-ACCESS	not-accessible
		STATUS		current
		DESCRIPTION	"Each entry describes the software 
					discovery for a specific hicom."
		INDEX	{ hicomSoftDiscovPabxId }
		::=	{ hicomSoftDiscovTable 1 }

	HicomSoftDiscovEntry ::=
		SEQUENCE {
			hicomSoftDiscovPabxId		Integer32,
			hicomSoftDiscovPabxMnemonic	DisplayString,
			hicomSoftDiscovStatus		DiscoveryStates,
			hicomSoftDiscovMode		DiscoveryModes,
			hicomSoftDiscovTimDat		DisplayString,
			hicomSoftDiscovErrTimDat	DisplayString
		}

	hicomSoftDiscovPabxId	OBJECT-TYPE
		SYNTAX		Integer32
		MAX-ACCESS	read-only
		STATUS		current
		DESCRIPTION	"Unique identifier of the Hicom being discovered."
		::=	{ hicomSoftDiscovEntry 1 }

	hicomSoftDiscovPabxMnemonic	OBJECT-TYPE
		SYNTAX	DisplayString
		MAX-ACCESS	read-only
		STATUS		current
		DESCRIPTION	"Name of the Hicom."
		::=	{ hicomSoftDiscovEntry 2 }

	hicomSoftDiscovStatus	OBJECT-TYPE
		SYNTAX		DiscoveryStates
		MAX-ACCESS	read-write
		STATUS		current
		DESCRIPTION	"Status of the software discovery process."
		::=	{ hicomSoftDiscovEntry 3 }

	hicomSoftDiscovMode	OBJECT-TYPE
		SYNTAX		DiscoveryModes
		MAX-ACCESS	read-write
		STATUS		current
		DESCRIPTION	"Mode of the software discovery process."
		::=	{ hicomSoftDiscovEntry 4 }

	hicomSoftDiscovTimDat	OBJECT-TYPE
		SYNTAX		DisplayString
		MAX-ACCESS	read-only
		STATUS		current
		DESCRIPTION	"Date and time of the last successful software discovery."
		::=	{ hicomSoftDiscovEntry 5 }

	hicomSoftDiscovErrTimDat	OBJECT-TYPE
		SYNTAX		DisplayString
		MAX-ACCESS	read-only
		STATUS		current
		DESCRIPTION	"Date and time of the last failed software discovery."
		::=	{ hicomSoftDiscovEntry 6 }

-- conformance information

-- units of conformance
-- BEGIN BLOCK soft
hicomSoftGroup		OBJECT-GROUP
	OBJECTS	{ hicomSoftApsPabxId, hicomSoftApsID, hicomSoftPatchNo,
			hicomSoftApsPartNo, hicomSoftPatchHWMod, hicomSoftPatchActType,
			hicomSoftPatchGroup, hicomSoftPatchPabxId }
	STATUS		current
	DESCRIPTION	"Conformance statement for hicomSoftGroup."
	::=	{ hicomSoftTree 1 }

hicomSoftInfoGroup	OBJECT-GROUP
	OBJECTS	{
		hicomSoftSubagentLastMsgNo,
		hicomSoftSubagentLastMsgText,
		hicomSoftResultData
	}
	STATUS		current
	DESCRIPTION	"Conformance statement for hicomSoftInfoGroup."
	::=	{ hicomSoftTree 2 }

-- BEGIN BLOCK discov
hicomSoftDiscoveryGroup	OBJECT-GROUP
	OBJECTS {
		hicomSoftChanges,
		hicomSoftDiscovPabxId,
		hicomSoftDiscovPabxMnemonic,
		hicomSoftDiscovStatus,
		hicomSoftDiscovMode,
		hicomSoftDiscovTimDat,
		hicomSoftDiscovErrTimDat
	}
	STATUS	current
	DESCRIPTION	"Conformance statement for hicomSoftDiscoveryGroup."
	::= { hicomSoftTree 3 }

