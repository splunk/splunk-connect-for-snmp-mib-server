	HicomAlConfDiscovEntry ::=
		SEQUENCE {
			hicomAlConfDiscovPabxId		Integer32,
			hicomAlConfDiscovPabxMnemonic	DisplayString,
			hicomAlConfDiscovStatus		DiscoveryStates,
			hicomAlConfDiscovMode		DiscoveryModes,
			hicomAlConfDiscovTimDat		DisplayString,
			hicomAlConfDiscovErrTimDat	DisplayString
		}

	hicomAlConfDiscovPabxId	OBJECT-TYPE
		SYNTAX		Integer32
		MAX-ACCESS	read-only
		STATUS		current
		DESCRIPTION	"Unique identifier of the Hicom being discovered."
		::=	{ hicomAlConfDiscovEntry 1 }

	hicomAlConfDiscovPabxMnemonic	OBJECT-TYPE
		SYNTAX	DisplayString
		MAX-ACCESS	read-only
		STATUS		current
		DESCRIPTION	"Name of the Hicom."
		::=	{ hicomAlConfDiscovEntry 2 }

	hicomAlConfDiscovStatus	OBJECT-TYPE
		SYNTAX		DiscoveryStates
		MAX-ACCESS	read-write
		STATUS		current
		DESCRIPTION	"Status of the alarm configuration discovery process."
		::=	{ hicomAlConfDiscovEntry 3 }

	hicomAlConfDiscovMode	OBJECT-TYPE
		SYNTAX		DiscoveryModes
		MAX-ACCESS	read-write
		STATUS		current
