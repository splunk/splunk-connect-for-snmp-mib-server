-- Zusatzinfo Table 	
-- ----------------

	hicomSysNodeNo		OBJECT-TYPE
		SYNTAX		Integer32
		MAX-ACCESS read-only
		STATUS		current
		DESCRIPTION		"Node number of the Hicom system."
		DEFVAL { 0 }
		::=	{ hicomSysEntry 100 }

	hicomSysLang		OBJECT-TYPE
		SYNTAX		INTEGER	{ undef	(1), german (2), english (3) }
		MAX-ACCESS	read-only
		STATUS		current
		DESCRIPTION	"Configured system language."
		DEFVAL		{ undef }
		::=	{ hicomSysEntry 101 }

	hicomSysMgrNetId	OBJECT-TYPE
		SYNTAX		OCTET STRING (SIZE (0..32))
		MAX-ACCESS 	read-write
		STATUS		current
		DESCRIPTION		"Identifier of the customer network.
					The field is set and maintained by
					the manager application."
		::=	{ hicomSysEntry 102 }

	hicomSysMgrSubNetId OBJECT-TYPE
		SYNTAX		OCTET STRING  (SIZE (0..64))
		MAX-ACCESS 	read-write
		STATUS		current
		DESCRIPTION		"Identifier of the customer's 
					sub-network.
					The field is set and maintained by
					the manager application."
		::=	{ hicomSysEntry 103 }

	hicomSysMgrPhysNetId OBJECT-TYPE
		SYNTAX		OCTET STRING  (SIZE (0..32))
		MAX-ACCESS 	read-write
		STATUS		current
		DESCRIPTION		"Identifier of a customer's 
					physical network.
					The field is set and maintained by
					the manager application."
		::=	{ hicomSysEntry 104 }

	hicomSysMgrFlags	OBJECT-TYPE
		SYNTAX		Integer32
		MAX-ACCESS 	read-write
		STATUS		current
		DESCRIPTION		"Private flags of manager application.
					The field is set and maintained by
					the manager application."
		::=	{ hicomSysEntry 105 }

	hicomSysDiscTimeOutEvnt	OBJECT-TYPE
		SYNTAX		INTEGER (24..65535)
		MAX-ACCESS 	read-write
		STATUS		current
		DESCRIPTION		"Acceptable maximum time interval 
					(unit is hours) not getting an 
					event from the hicom system.
					
					E1: (DiscTimeOutEvnt < t1 - t2)
					t1 = current Time
					t2 = Time of last Event

					If the expression E1 becomes true
					a discovery operation will started 
					for the hicom. 
					DiscTimeOutEvnt shall be set greater
					or equal to 24 hours.
					The field is set and maintained by 
					the manager application."
		DEFVAL		{ 24 }
		::=	{ hicomSysEntry 106 }

	hicomSysDiscAgedData	OBJECT-TYPE
		SYNTAX		INTEGER (168..65535)
		MAX-ACCESS 	read-write
		STATUS		current
		DESCRIPTION		"Acceptable maximum time interval 
					(unit is hours) since last discovery 
					date and current event date.

					E2: (DiscAgedData < t1 - t2)
					t1 = current Event Time
					t2 = time last discovery succeeded

					If the expression E2 becomes true 
					a discovery operation will started 
					for the hicom. 
					DiscAgedData shall be set greater or
					equal to 168 hours (7 days).
					The field is set and maintained by the 
					manager application."
		DEFVAL	{ 168 }
		::=	{ hicomSysEntry 107 }

	hicomSysDiscComType OBJECT-TYPE
		SYNTAX		INTEGER { addBatchJob (1), online (2) }
		MAX-ACCESS 	read-write
		STATUS		current
		DESCRIPTION		"Identifies the communication interface
					between DMS and Hicom."
		DEFVAL		{ addBatchJob }
		::=	{ hicomSysEntry 108 }

    hicomSysAgentFlags    OBJECT-TYPE
        SYNTAX      Integer32
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION     "Private flags of the Agent.
                    The field is set and maintained by
                    the manager/agent application."
        ::= { hicomSysEntry 109 }

hicomSysSubagentLastMsgNo	OBJECT-TYPE
