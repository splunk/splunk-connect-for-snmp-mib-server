hicomAlConfDiscovery	OBJECT IDENTIFIER ::= { hicomAlConf 3 }
-- BEGIN BLOCK error

-- the hicomErrors group

-- the hicomErrorTable

hicomErrorTable	OBJECT-TYPE
	SYNTAX		SEQUENCE	OF HicomErrorEntry
	MAX-ACCESS not-accessible
	STATUS		current
	DESCRIPTION		"The table of Hicom errors."
	::=	{ hicomErrors 1 }

	hicomErrorEntry	OBJECT-TYPE
		SYNTAX		HicomErrorEntry
		MAX-ACCESS not-accessible
		STATUS		current
		DESCRIPTION		"Each entry describes a Hicom error."
		INDEX	{ hicomErrorPabxId,
			hicomErrorAlGroup,
			hicomErrorAlSubId,
			hicomErrorSerialNo }
		::=	{ hicomErrorTable 1 }

	HicomErrorEntry	::=	SEQUENCE {
			hicomErrorPabxId		Integer32,
			hicomErrorAlGroup 		Integer32,
			hicomErrorAlSubId 		Integer32,
			hicomErrorSerialNo		Integer32,
			hicomErrorMsgId			DisplayString,
			hicomErrorPriority		DisplayString,
			hicomErrorAction		DisplayString,
			hicomErrorAbsMod		DisplayString,
			hicomErrorEvent			DisplayString,
			hicomErrorSubevent		DisplayString,
			hicomErrorCardRef		DisplayString,
			hicomErrorBoardVersion		DisplayString,
			hicomErrorFwType 		DisplayString,
			hicomErrorTimDat 		Integer32,
			hicomErrorOrigText 		DisplayString
		}

	hicomErrorPabxId	OBJECT-TYPE
		SYNTAX		Integer32
		MAX-ACCESS	read-only
		STATUS		current
		DESCRIPTION	"Internal identifier of a Hicom system."
		::=	{ hicomErrorEntry 1 }

	hicomErrorAlGroup		OBJECT-TYPE
		SYNTAX		Integer32
		MAX-ACCESS	read-only
		STATUS		current
		DESCRIPTION	"Hicom alarm group this error belongs to."
		::=	{ hicomErrorEntry 2 }

	hicomErrorAlSubId		OBJECT-TYPE
		SYNTAX		Integer32
		MAX-ACCESS	read-only
		STATUS		current
		DESCRIPTION	"Hicom alarm number within the hicomErrorAlGroup
					this error belongs to."
		::=	{ hicomErrorEntry 3 }

	hicomErrorSerialNo OBJECT-TYPE
		SYNTAX		Integer32
		MAX-ACCESS	read-only
		STATUS		current
		DESCRIPTION	"Internal serial number of a Hicom error."
		::=	{ hicomErrorEntry 4 }

	hicomErrorMsgId		OBJECT-TYPE
		SYNTAX		DisplayString
		MAX-ACCESS	read-only
		STATUS		current
		DESCRIPTION	"Identifier of the Hicom error."
		::=	{ hicomErrorEntry 5 }

	hicomErrorPriority		OBJECT-TYPE
		SYNTAX		DisplayString
		MAX-ACCESS	read-only
		STATUS		current
		DESCRIPTION	"Priority of the Hicom error."
		::=	{ hicomErrorEntry 6 }

	hicomErrorAction		OBJECT-TYPE
		SYNTAX		DisplayString
		MAX-ACCESS	read-only
		STATUS		current
		DESCRIPTION	"Required Hicom system action."
		::=	{ hicomErrorEntry 7 }

	hicomErrorAbsMod		OBJECT-TYPE
		SYNTAX		DisplayString
		MAX-ACCESS	read-only
		STATUS		current
		DESCRIPTION	"Hicom module detecting the error."
		::=	{ hicomErrorEntry 8 }

	hicomErrorEvent	OBJECT-TYPE
		SYNTAX		DisplayString
		MAX-ACCESS	read-only
		STATUS		current
		DESCRIPTION	"Description of the Hicom error."
		::=	{ hicomErrorEntry 9 }

	hicomErrorSubevent		OBJECT-TYPE
		SYNTAX		DisplayString
		MAX-ACCESS	read-only
		STATUS		current
		DESCRIPTION	"Subdescription of the Hicom error."
		::=	{ hicomErrorEntry 10 }

	hicomErrorCardRef		OBJECT-TYPE
		SYNTAX		DisplayString
		MAX-ACCESS	read-only
		STATUS		current
		DESCRIPTION	"Reference to a Hicom board."
		::=	{ hicomErrorEntry 11 }

	hicomErrorBoardVersion	OBJECT-TYPE
		SYNTAX		DisplayString
		MAX-ACCESS	read-only
		STATUS		current
		DESCRIPTION	"Version of that board. "
		::=	{ hicomErrorEntry 12 }

	hicomErrorFwType	OBJECT-TYPE
		SYNTAX		DisplayString
		MAX-ACCESS	read-only
		STATUS		current
		DESCRIPTION	"Firmware of that board."
		::=	{ hicomErrorEntry 13 }

	hicomErrorTimDat	OBJECT-TYPE
		SYNTAX		Integer32
		MAX-ACCESS	read-only
		STATUS		current
		DESCRIPTION	"Date and time of the Hicom error."
		::=	{ hicomErrorEntry 14 }

	hicomErrorOrigText	OBJECT-TYPE
		SYNTAX		DisplayString
		MAX-ACCESS	read-only
		STATUS		current
		DESCRIPTION	"Original message text of the Hicom error."
		::=	{ hicomErrorEntry 15 }


-- the hicomAlConfTable

hicomAlConfTable OBJECT-TYPE
	SYNTAX		SEQUENCE OF HicomAlConfEntry
	MAX-ACCESS	not-accessible
	STATUS		current
	DESCRIPTION	"The table of the Hicom alarm configuration."
	::=	{ hicomAlConfig 1 }

	hicomAlConfEntry		OBJECT-TYPE
		SYNTAX		HicomAlConfEntry
		MAX-ACCESS	not-accessible
		STATUS		current
		DESCRIPTION	"Each entry describes the configuration of a Hicom alarm."
		INDEX	{ hicomAlConfPabxId, hicomAlConfAlGroup, hicomAlConfAlSubId }
		::=	{ hicomAlConfTable 1 }

	HicomAlConfEntry ::=
		SEQUENCE {
			hicomAlConfPabxId		Integer32,
			hicomAlConfAlGroup		Integer32,
			hicomAlConfAlSubId		Integer32,
			hicomAlConfName			DisplayString,
			hicomAlConfThreshold1		Integer32,
			hicomAlConfThreshold2		Integer32,
			hicomAlConfTime1		Integer32,
			hicomAlConfTime2		Integer32,
			hicomAlConfTimeH		Integer32,
			hicomAlConfBase 		Integer32
		}

	hicomAlConfPabxId OBJECT-TYPE
		SYNTAX		Integer32
		MAX-ACCESS	read-only
		STATUS		current
		DESCRIPTION	"Unique identifier of the Hicom system."
		::=	{ hicomAlConfEntry 1 }

	hicomAlConfAlGroup OBJECT-TYPE
		SYNTAX		Integer32
		MAX-ACCESS	read-only
		STATUS		current
		DESCRIPTION	"Group identifier of a Hicom alarm."
		::=	{ hicomAlConfEntry 2 }

	hicomAlConfAlSubId OBJECT-TYPE
		SYNTAX		Integer32
		MAX-ACCESS	read-only
		STATUS		current
		DESCRIPTION	"Internal group identifier of a Hicom alarm."
		::=	{ hicomAlConfEntry 3 }

	hicomAlConfName	OBJECT-TYPE
		SYNTAX		DisplayString
		MAX-ACCESS	read-only
		STATUS		current
		DESCRIPTION	"Name of the Hicom alarm."
		::=	{ hicomAlConfEntry 4 }

	hicomAlConfThreshold1 OBJECT-TYPE
		SYNTAX		Integer32
		MAX-ACCESS	read-only
		STATUS		current
		DESCRIPTION	"Threshold 'minor' related Hicom errors."
	::=	{ hicomAlConfEntry 5 }

	hicomAlConfThreshold2 OBJECT-TYPE
		SYNTAX		Integer32
		MAX-ACCESS	read-only
		STATUS		current
		DESCRIPTION	"Threshold 'major' for related Hicom errors."
	::=	{ hicomAlConfEntry 6 }

	hicomAlConfTime1 OBJECT-TYPE
		SYNTAX		Integer32
		MAX-ACCESS	read-only
		STATUS		current
		DESCRIPTION	"Interval for the minor threshold."
	::=	{ hicomAlConfEntry 7 }

	hicomAlConfTime2 OBJECT-TYPE
		SYNTAX		Integer32
		MAX-ACCESS	read-only
		STATUS		current
		DESCRIPTION	"Interval for the major threshold."
	::=	{ hicomAlConfEntry 8 }

	hicomAlConfTimeH OBJECT-TYPE
		SYNTAX		Integer32
		MAX-ACCESS	read-only
		STATUS		current
		DESCRIPTION	"Threshold interval for device alarms.
				 Unit of Threshold is hours."
	::=	{ hicomAlConfEntry 9 }

	hicomAlConfBase	OBJECT-TYPE
		SYNTAX		Integer32
		MAX-ACCESS	read-only
		STATUS		current
		DESCRIPTION	"Base value for thresholds in percent."
	::=	{ hicomAlConfEntry 10 }

