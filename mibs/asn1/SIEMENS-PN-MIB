-- The hicomTopoDiscovTable table

	hicomTopoDiscovTable	OBJECT-TYPE
		SYNTAX	SEQUENCE OF HicomTopoDiscovEntry
		MAX-ACCESS		not-accessible
		STATUS	current
		DESCRIPTION		"The table describing the status
				concerning discovery of topology information."
		::=	{ hicomTopoDiscovery 2 }

	hicomTopoDiscovEntry	OBJECT-TYPE
		SYNTAX	HicomTopoDiscovEntry
		MAX-ACCESS		not-accessible
		STATUS	current
		DESCRIPTION		"An entry in the hicomTopoDiscovTable.
				Each entry describes the status of the
				discovery process for a specific hicom."
		INDEX	{ hicomTopoDiscovPabxId }
		::=	{ hicomTopoDiscovTable 1 }

	HicomTopoDiscovEntry ::=
		SEQUENCE {
			hicomTopoDiscovPabxId		Integer32,
			hicomTopoDiscovPabxMnemonic	DisplayString,
			hicomTopoDiscovStatus		DiscoveryStates,
			hicomTopoDiscovMode		DiscoveryModes,
			hicomTopoDiscovTimDat		DisplayString,
			hicomTopoDiscovErrTimDat	DisplayString
		}

	hicomTopoDiscovPabxId OBJECT-TYPE
		SYNTAX	Integer32
		MAX-ACCESS		read-only
		STATUS	current
		DESCRIPTION		"Unique identifier of the Hicom being discovered."
		::=	{ hicomTopoDiscovEntry 1 }

	hicomTopoDiscovPabxMnemonic OBJECT-TYPE
