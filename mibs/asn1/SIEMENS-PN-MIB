-- error message deletion successfully terminated

hicomErrDeleteSucc	NOTIFICATION-TYPE
	OBJECTS	{ hicomEDTrpPabxId, hicomEDTrpPabxMnemonic }
	STATUS	current
	DESCRIPTION	"A hicomErrDeleteSucc trap indicates the
				successful termination of an Error Message Delete  process.
				hicomErrDeletePabxId indicates the Pbx involved."
	::=	{ hicomDiscTraps 10 }

-- error message deletion unsuccessfully terminated

hicomErrDeleteErr	NOTIFICATION-TYPE
	OBJECTS	{ hicomEDTrpPabxId, hicomEDTrpPabxMnemonic}
	STATUS	current
	DESCRIPTION	"A hicomErrDeleteErr trap signifies the
				unsuccessful termination of an Error Message Delete process.
				hicomErrDeletePabxId indicates the Pbx involved."
	::=	{ hicomDiscTraps 11 }

-- error message deletion is busy

hicomErrDeleteBusy	NOTIFICATION-TYPE
	OBJECTS	{ hicomEDTrpPabxId, hicomEDTrpPabxMnemonic}
	STATUS	current
	DESCRIPTION	"A hicomErrDeleteBusy trap signifies
				that an Error Message Delete process is running.
				hicomErrDeletePabxId indicates the Pbx involved."
	::=	{ hicomDiscTraps 19 }


