				hicomPeriphGran
			}
		STATUS		current
		DESCRIPTION	"Conformance statement for hicomPeriperalsGroup."
		::=	{ hicomHardTree 2 }

	hicomBoardsGroup	OBJECT-GROUP
		OBJECTS	{
				hicomCDSMPabxId, hicomCDSMCabAddr, hicomCDSMMntLevel,
				hicomCDSMSlotAddr, hicomCDSMPartNo, hicomCDSMCode,
				hicomCDSMVers, hicomCDSMFirmware,
				hicomCDSUPabxId, hicomCDSUCabAddr, hicomCDSUMntLevel,
				hicomCDSUSlotAddr, hicomCDSUPartNo, hicomCDSUCode,
				hicomCDSUVers, hicomCDSUFirmware,
				hicomBCSMPabxId, hicomBCSMMod, hicomBCSMSlotAddr,
				hicomBCSMConfBoard, hicomBCSMCode, hicomBCSMInstBoard,
				hicomBCSMVers, hicomBCSMFirmware, hicomBCSMStatus,
				hicomBCSUPabxId, hicomBCSULTG, hicomBCSULTU,
				hicomBCSUSlotAddr, hicomBCSUConfBoard, hicomBCSUCode,
				hicomBCSUInstBoard, hicomBCSUVers, hicomBCSUFirmware,
				hicomBCSUStatus
			}
		STATUS		current
		DESCRIPTION	"Conformance statement for hicomBoardsGroup."
		::=	{ hicomHardTree 3 }

	hicomHWInfoGroup		OBJECT-GROUP
		OBJECTS	{
				hicomHardSubagentLastMsgNo,
				hicomHardSubagentLastMsgText,
				hicomHardSubagentResultData
			}
		STATUS		current
		DESCRIPTION	"Conformance statement for hicomHWInfoGroup."
		::=	{ hicomHardTree 4 }

