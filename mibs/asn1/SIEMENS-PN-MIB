changeConfig NOTIFICATION-TYPE
	OBJECTS		{ hicomSysPabxId, hicomSysMnemonic }
	STATUS		deprecated
	DESCRIPTION	"A changeConfig trap indicates that some change
				other than adding or removing a Hicom has been
				made to the network."
	::=	{ hicomSystemTraps 12 }


-- BEGIN BLOCK alarm

-- The hicomAlarms group provides hicom alarm information

-- the hicomAlTable

hicomAlarmsChanges	OBJECT-TYPE
	SYNTAX		Counter32
	MAX-ACCESS	read-only
	STATUS		current
	DESCRIPTION	"Is incremented when an alarm SET or RESET trap
			has been sent by the agent."
	::=	{ hicomAlarms 1 }


hicomAlTable	OBJECT-TYPE
	SYNTAX		SEQUENCE OF	HicomAlEntry
	MAX-ACCESS	not-accessible
	STATUS		current
	DESCRIPTION	"The table of all Hicom alarms."
	::=	{ hicomAlarms 2 }

	hicomAlEntry	OBJECT-TYPE
		SYNTAX		HicomAlEntry
		MAX-ACCESS	not-accessible
		STATUS		current
		DESCRIPTION	"Each entry describes a	Hicom alarm in detail."
		INDEX 		{ hicomAlFilter, hicomAlStatus, hicomAlPabxId, 
				hicomAlGroup, hicomAlSubId, hicomAlPriority, 
				hicomAlAbsMod }
		::=	{ hicomAlTable 1 }

	HicomAlEntry ::=
		SEQUENCE {
			hicomAlFilter	        AlarmFilterStates,
			hicomAlStatus           INTEGER,
			hicomAlPabxId	        Integer32,
			hicomAlGroup	        Integer32,
			hicomAlSubId	        Integer32,
			hicomAlPriority	        AlarmPriorities,
			hicomAlAbsMod	        DisplayString,
			hicomAlTimDat	        Integer32,
			hicomAlName	            DisplayString,
			hicomAlReset	        INTEGER,
			hicomAlTimOldDat	    Integer32,
			hicomAlArrivalTimDat	Integer32
		}

	hicomAlFilter		OBJECT-TYPE
		SYNTAX		AlarmFilterStates
		MAX-ACCESS	read-only
		STATUS		current
		DESCRIPTION	"Alarm filter status."
		::=	{ hicomAlEntry 1 }

	hicomAlStatus		OBJECT-TYPE
		SYNTAX		INTEGER { reset (1), set (2) }
		MAX-ACCESS	read-only
		STATUS		current
		DESCRIPTION	"Status of an alarm."
		::=	{ hicomAlEntry 2 }

	hicomAlPabxId	OBJECT-TYPE
		SYNTAX		Integer32
		MAX-ACCESS	read-only
		STATUS		current
		DESCRIPTION	"Unique identifier of a Hicom system."
		::=	{ hicomAlEntry 3 }

	hicomAlGroup	OBJECT-TYPE
		SYNTAX		Integer32
		MAX-ACCESS	read-only
		STATUS		current
		DESCRIPTION	"Group identifier of a Hicom alarm."
		::=	{ hicomAlEntry 4 }

	hicomAlSubId	OBJECT-TYPE
		SYNTAX		Integer32
		MAX-ACCESS	read-only
		STATUS		current
		DESCRIPTION	"Sub-identifier of a Hicom alarm within its
				group."
		::=	{ hicomAlEntry 5 }

	hicomAlPriority		OBJECT-TYPE
		SYNTAX 		AlarmPriorities
		MAX-ACCESS 	read-only
		STATUS 		current
		DESCRIPTION	"Priority of a Hicom alarm."
		::=	{ hicomAlEntry 6 }

	hicomAlAbsMod		OBJECT-TYPE
		SYNTAX		DisplayString
		MAX-ACCESS 	read-only
		STATUS		current
		DESCRIPTION	"Hicom module detecting the Hicom alarm."
		::=	{ hicomAlEntry 7 }

	hicomAlTimDat		OBJECT-TYPE
		SYNTAX		Integer32
		MAX-ACCESS read-only
		STATUS		current
		DESCRIPTION		"Date and time of the Hicom alarm. "
		::=	{ hicomAlEntry 8 }

	hicomAlName	OBJECT-TYPE
		SYNTAX		DisplayString
		MAX-ACCESS	read-only
		STATUS		current
		DESCRIPTION		"Name of the Hicom alarm."
		::=	{ hicomAlEntry 9 }

	hicomAlReset	OBJECT-TYPE
		SYNTAX		INTEGER { 
						init (1), error (2), busy (3), 
						finok (4), finerr (5)
					}
		MAX-ACCESS	read-write
		STATUS		current
		DESCRIPTION	"This variable can be set to reset a Hicom alarm."
		DEFVAL 		{ init }
		::= { hicomAlEntry 10 }

-- TEST new UNITY MIB

	hicomAlTimOldDat		OBJECT-TYPE
		SYNTAX		Integer32
		MAX-ACCESS read-only
		STATUS		current
		DESCRIPTION		"Date and time of the previous set of this Hicom alarm. "
		::=	{ hicomAlEntry 11 }

	hicomAlArrivalTimDat		OBJECT-TYPE
		SYNTAX		Integer32
		MAX-ACCESS read-only
		STATUS		current
		DESCRIPTION		"Unity A&S Date and time of the arrival of this Hicom alarm. "
		::=	{ hicomAlEntry 12 }

-- TEST END new UNITY MIB



hicomAlFiltConfTable	OBJECT-TYPE
	SYNTAX		SEQUENCE OF	HicomAlFiltConfEntry
	MAX-ACCESS	not-accessible
	STATUS		current
	DESCRIPTION	"The table of configured Hicom alarm filters."
	::=	{ hicomAlarms 3 }

	hicomAlFiltConfEntry	OBJECT-TYPE
		SYNTAX		HicomAlFiltConfEntry
		MAX-ACCESS	not-accessible
		STATUS		current
		DESCRIPTION	"Each entry specifies a	filter for a 
				certain type of Hicom alarm."
		INDEX 		{ hicomAlFiltConfPabxId, hicomAlFiltConfGroup, 
				hicomAlFiltConfSubId, hicomAlFiltConfPriority }
		::=	{ hicomAlFiltConfTable 1 }

	HicomAlFiltConfEntry ::=
		SEQUENCE {
			hicomAlFiltConfPabxId	Integer32,
			hicomAlFiltConfGroup	Integer32,
			hicomAlFiltConfSubId	Integer32,
			hicomAlFiltConfPriority	AlarmPriorities,
			hicomAlFiltConfSwitch	AlarmFilterStates,
			hicomAlFiltConfRowStat  RowStatus
		}

	hicomAlFiltConfPabxId	OBJECT-TYPE
		SYNTAX		Integer32
		MAX-ACCESS	not-accessible
		STATUS		current
		DESCRIPTION	"Unique identifier of a Hicom system."
		::=	{ hicomAlFiltConfEntry 1 }

	hicomAlFiltConfGroup	OBJECT-TYPE
		SYNTAX		Integer32
		MAX-ACCESS	not-accessible
		STATUS		current
		DESCRIPTION	"Group identifier of a Hicom alarm."
		::=	{ hicomAlFiltConfEntry 2 }

	hicomAlFiltConfSubId	OBJECT-TYPE
		SYNTAX		Integer32
		MAX-ACCESS	not-accessible
		STATUS		current
		DESCRIPTION	"Sub-identifier of a Hicom alarm within
				its group."
		::=	{ hicomAlFiltConfEntry 3 }

	hicomAlFiltConfPriority		OBJECT-TYPE
		SYNTAX 		AlarmPriorities
		MAX-ACCESS 	not-accessible
		STATUS 		current
		DESCRIPTION	"Priority of a Hicom alarm."
		::=	{ hicomAlFiltConfEntry 4 }

	hicomAlFiltConfSwitch	OBJECT-TYPE
		SYNTAX 		AlarmFilterStates
		MAX-ACCESS 	read-create
		STATUS 		current
		DESCRIPTION	"Specifies whether filtering for this alarm 
				is switched ON or OFF. If set to ON, no trap 
				will be generated in case the alarm is being 
				SET or RESET."
		::=	{ hicomAlFiltConfEntry 5 }

	hicomAlFiltConfRowStat  OBJECT-TYPE
		SYNTAX		RowStatus
		MAX-ACCESS	read-create
		STATUS		current
		DESCRIPTION
				    "Setting a non-existing RowStat 
				     variable to 'createAndGo', creates a new
				     row. If creation was successful, the row
				     gets the status 'active'. Variables of
				     an 'active' row can be read and modified.
				     Setting RowStat to 'destroy' effects 
				     the deletion of the row."
		::= { hicomAlFiltConfEntry 6}

hicomAlarmSubagentLastMsgNo	OBJECT-TYPE
	SYNTAX		Integer32
	MAX-ACCESS	read-only
	STATUS		current
	DESCRIPTION	"The last message, warning or error number 
				issued by the Alarm subagent."
	::=	{ hicomAlarms 4 }

hicomAlarmSubagentLastMsgText	OBJECT-TYPE
	SYNTAX		DisplayString
	MAX-ACCESS	read-only
	STATUS		current
	DESCRIPTION	"The last message, warning or error text
				issued by the Alarm subagent."
	::=	{ hicomAlarms 5 }


-- TEST NEW AlMirrorUpload table

hicomAlMirrorUploadTable    OBJECT-TYPE
    SYNTAX      SEQUENCE OF HicomAlMirrorUploadEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION "The table to start an Alarm Upload "
    ::= { hicomAlarms 6 }

    hicomAlMirrorUploadEntry    OBJECT-TYPE
        SYNTAX      HicomAlMirrorUploadEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION "Each entry describes a Hicom alarm in detail."
        INDEX  { hicomAlMirrorUploadPabxId }
        ::= { hicomAlMirrorUploadTable 1 }

    HicomAlMirrorUploadEntry ::=
        SEQUENCE {
            hicomAlMirrorUploadPabxId        Integer32,
            hicomAlMirrorUploadMnemonic      DisplayString,
            hicomAlMirrorUploadState         INTEGER,
            hicomAlMirrorUploadStartTimDat   DisplayString,
            hicomAlMirrorUploadErrorTimDat   DisplayString
        }

    hicomAlMirrorUploadPabxId   OBJECT-TYPE
        SYNTAX      Integer32
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION "Unique identifier of a Hicom system."
        ::= { hicomAlMirrorUploadEntry 1 }

    hicomAlMirrorUploadMnemonic   OBJECT-TYPE
        SYNTAX      DisplayString
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION "Mnemonic name of the Hicom system."
        ::= { hicomAlMirrorUploadEntry 2 }

    hicomAlMirrorUploadState   OBJECT-TYPE
        SYNTAX       INTEGER { done (1), error (2), busy (3) }
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION "Status of the AlarmMirror upload
                     done (1), error (2), busy (3)"
        ::= { hicomAlMirrorUploadEntry 3 }

    hicomAlMirrorUploadStartTimDat   OBJECT-TYPE
        SYNTAX      DisplayString
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION "Last successful initiation of an AlarmMirror upload."
        ::= { hicomAlMirrorUploadEntry 4 }

    hicomAlMirrorUploadErrorTimDat   OBJECT-TYPE
        SYNTAX      DisplayString
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION "Last unsuccessful initiation of an AlarmMirror upload."
        ::= { hicomAlMirrorUploadEntry 5 }

    
