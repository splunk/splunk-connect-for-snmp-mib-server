	::=	{ hicomAlarmTraps 34 }

hicomAlarmOffDevice	NOTIFICATION-TYPE
	OBJECTS	{ hicomAlTrpSysPabxId, hicomAlTrpSysMnemonic, hicomAlGroup,
		hicomAlSubId, hicomAlPriority, hicomAlAbsMod,
		hicomAlStatus, hicomAlTimDat, hicomAlName }
	STATUS	current
	DESCRIPTION	
		"A hicomAlarmOffDevice trap is sent when a device alarm is reset."
	::=	{ hicomAlarmTraps 35 }


-- BEGIN BLOCK error

-- The hicomErrors MIB subtree provides the errors of the hicom network
-- The hicomAlConfig MIB subtree provides the hicom alarm configuration
-- The MIB subtree consists of the following groups:

hicomAlConfig			OBJECT IDENTIFIER ::= { hicomAlConf 1 }
hicomErrAlConfInfo		OBJECT IDENTIFIER ::= {	hicomAlConf 2 }
