-- Traps / Notifications

-- Trap Variables

	hicomSWTrpPabxId	OBJECT-TYPE
		SYNTAX		Integer32
		MAX-ACCESS	not-accessible
		STATUS		current
		DESCRIPTION	"Unique identifier of a Hicom system."
		::=	{ hicomSWTrapVariables 1 }

	hicomSWTrpPabxMnemonic OBJECT-TYPE
		SYNTAX		DisplayString
		MAX-ACCESS 	not-accessible
		STATUS		current
		DESCRIPTION	"Mnemonic name of the Hicom system."
		::=	{ hicomSWTrapVariables 2 }

-- Message Traps
internalMessageSWSubagent NOTIFICATION-TYPE
	OBJECTS	{ hicomSoftSubagentLastMsgNo, hicomSoftSubagentLastMsgText }
	STATUS	current
	DESCRIPTION	"An internalMessage trap contains an informational
			 message generated by the subagent."
	::=	{ hicomSWTraps 0 }

-- Warning Traps

internalWarningSWSubagent NOTIFICATION-TYPE
	OBJECTS	{ hicomSoftSubagentLastMsgNo, hicomSoftSubagentLastMsgText }
	STATUS	current
	DESCRIPTION	"An internalWarning trap contains a warning message
			 generated by a subagent."
	::=	{ hicomSWTraps 1 }

-- Error Traps

internalErrorSWSubagent	NOTIFICATION-TYPE
	OBJECTS	{ hicomSoftSubagentLastMsgNo, hicomSoftSubagentLastMsgText }
	STATUS	current
	DESCRIPTION	"An internalError trap contains an error message
			 generated by a subagent. 
			 After issuing this trap, the agent terminates."
	::=	{ hicomSWTraps 2 }

-- software discovery successfully terminated

hicomSoftDiscovSucc	NOTIFICATION-TYPE
	OBJECTS	{ hicomSWTrpPabxId, hicomSWTrpPabxMnemonic }
	STATUS	current
	DESCRIPTION	"A hicomSoftDiscovSucc trap indicates the
				successful termination of a discovery process.
				hicomSoftDiscovPabxId indicates the Pbx involved."
	::=	{ hicomSWTraps 10 }

-- software discovery unsuccessfully terminated

hicomSoftDiscovErr	NOTIFICATION-TYPE
	OBJECTS	{ hicomSWTrpPabxId, hicomSWTrpPabxMnemonic}
	STATUS	current
	DESCRIPTION	"A hicomSoftDiscovErr trap signifies the
				unsuccessful termination of a discovery process.
				hicomSoftDiscovPabxId indicates the Pbx involved."
	::=	{ hicomSWTraps 11 }

-- software discovery is busy

hicomSoftDiscovBusy	NOTIFICATION-TYPE
	OBJECTS	{ hicomSWTrpPabxId, hicomSWTrpPabxMnemonic}
	STATUS	current
	DESCRIPTION	"A hicomSoftDiscovBusy trap signifies
				that a sw discovery process is running.
				hicomSoftDiscovPabxId indicates the Pbx involved."
	::=	{ hicomSWTraps 19 }


-- BEGIN BLOCK hard

-- The hicomHard MIB subtree provides an inventory of hardware components of a
-- hicom network. The MIB subtree falls into the following groups:

hicomCabinets			OBJECT IDENTIFIER ::= {	hicomHard 1 }
hicomPeripherals		OBJECT IDENTIFIER ::= {	hicomHard 2 }
hicomBoards			OBJECT IDENTIFIER ::= {	hicomHard 3 }
hicomHWInfo			OBJECT IDENTIFIER ::= {	hicomHard 4 }

-- BEGIN BLOCK discov
hicomHWDiscovery		OBJECT IDENTIFIER ::= {	hicomHard 5 }

-- BEGIN BLOCK hard

-- The hicomCabinets group provides information on installed cabinets
-- The cabinet table

hicomCabTable	OBJECT-TYPE
	SYNTAX		SEQUENCE OF HicomCabEntry
	MAX-ACCESS	not-accessible
	STATUS		current
	DESCRIPTION		"The table of installed hicom cabinets."
	::=	{ hicomCabinets 1 }

	hicomCabEntry		OBJECT-TYPE
		SYNTAX		HicomCabEntry
		MAX-ACCESS	not-accessible
		STATUS		current
		DESCRIPTION	"Each entry describes a hicom cabinet. "
		INDEX		{ hicomCabPabxId, hicomCabAddr }
		::=	{ hicomCabTable 1 }

	HicomCabEntry ::=
		SEQUENCE {
			hicomCabPabxId		Integer32,
			hicomCabAddr		OCTET STRING,
			hicomCabType		DisplayString,
			hicomCabPartNo		DisplayString,
			hicomCabNumShelves	Integer32
		}

	hicomCabPabxId		OBJECT-TYPE
		SYNTAX		Integer32
		MAX-ACCESS	read-only
		STATUS		current
		DESCRIPTION	"Unique identifier of the Hicom to which 
					the cabinet belongs."
		::=	{ hicomCabEntry 1 }

	hicomCabAddr	OBJECT-TYPE
		SYNTAX		OCTET STRING
		MAX-ACCESS	read-only
		STATUS		current
		DESCRIPTION	"Unique identifier of the cabinet within the Hicom."
		::=	{ hicomCabEntry 2 }

	hicomCabType	OBJECT-TYPE
		SYNTAX		DisplayString
		MAX-ACCESS	read-only
		STATUS		current
		DESCRIPTION	"Short name of cabinet type."
		::=	{ hicomCabEntry 3 }

	hicomCabPartNo	OBJECT-TYPE
		SYNTAX		DisplayString
		MAX-ACCESS	read-only
		STATUS		current
		DESCRIPTION	"Siemens part number of the cabinet."
		::=	{ hicomCabEntry 4 }

	hicomCabNumShelves	OBJECT-TYPE
		SYNTAX		Integer32
		MAX-ACCESS	read-only
		STATUS		current
		DESCRIPTION	"The number of shelves in a cabinet."
		::=	{ hicomCabEntry 5 }

