-- BEGIN BLOCK sql

hicomSQLsessionIdGenerator	OBJECT-TYPE
	SYNTAX		TestAndIncr
	MAX-ACCESS	read-write
	STATUS		current
	DESCRIPTION	"hicomSQLsessionIdGenerator is used to create and assign
			unique SQL session identifiers to concurrent manager 
			applications.
			hicomSQLsessionIdGenerator shall be used in accordance 
			to the semantics of TestAndIncr specified in Textual 
			Conventions (RFC1903).
			
			A GET on hicomSQLsessionIdGenerator returns a new SQL 
			session ID.  This value shall be confirmed/claimed by 
			a manager application by setting the 
			hicomSQLsessionIdGenerator variable to the value which 
			was returned by the previous GET. 
			If the SET on hicomSQLsessionIdGenerator succeeds 
			(error status 'noError' is returned), the SQL session 
			ID value is uniquely assigned to the requesting manager
			application. Further, the agent did create a 
			corresponding entry in the hicomSQLsessionTable having 
			hicomSQLsessionId set to the SQL session ID value used 
			with the SET.

			If the SET on hicomSQLsessionIdGenerator fails (error 
			status 'inconsistenValue' is returned) the same value 
			was read by another manager and this one just claimed 
			his SQL session ID by performing successfully a SET.
			In this case no entry is created in the 
			hicomSQLsessionTable.
			To get and claim a unique SQL session ID, the manager 
			application has to redo the GET and SET operation 
			cycle on hicomSQLsessionIdGenerator.
			
			NOTE: using this and other hicomSQL objects is reserved
			for Siemens internal use ONLY. No documentation on this
			interface is available for customers."
	::= { hicomSQL 1 }

hicomSQLsessionTable	OBJECT-TYPE
	SYNTAX		SEQUENCE OF HicomSQLsessionEntry
	MAX-ACCESS	not-accessible
	STATUS		current
	DESCRIPTION	"This table describes all currently active SQL sessions.
			An entry is generated after a new session ID is read 
			and claimed by a manager application (see 
			hicomSQLsessionIdGenerator).
			An entry is deleted from the table when its timeout 
			(see hicomSQLsessionTimeout) - which is restarted with 
			each access on hicomSQLresultData - expires.
			The state of a new created entry initially is idle(1). 
			The manager must then SET hicomSQLsessionStatement and
			hicomSQLsessionWhereClause, optionally may set 
			hicomSQLsessionTimeout, and then SET hicomSQLsessionState 
			to run(2). The completion of a session will be 
			indicated to the manager via a hicomSQLsessionFinished 
			trap."
	::=	{ hicomSQL 2 }

	hicomSQLsessionEntry	OBJECT-TYPE
		SYNTAX		HicomSQLsessionEntry
		MAX-ACCESS	not-accessible
		STATUS		current
		DESCRIPTION	"An entry in the hicomSQLsessionTable."
		INDEX		{ hicomSQLsessionId }
		::=	{ hicomSQLsessionTable 1 }

	HicomSQLsessionEntry ::=
		SEQUENCE {
			hicomSQLsessionId		Integer32,
			hicomSQLsessionState		INTEGER,
			hicomSQLsessionStatement	Integer32,
			hicomSQLsessionWhereClause	DisplayString,
			hicomSQLsessionTimeout		INTEGER,
			hicomSQLsessionResultSize	Integer32,
			hicomSQLsessionResultNoLines	Integer32
		}

	hicomSQLsessionId OBJECT-TYPE
		SYNTAX		Integer32
		MAX-ACCESS	read-only
		STATUS		current
		DESCRIPTION	"Unique identifier of the SQL session."
		::=	{ hicomSQLsessionEntry 1 }

	hicomSQLsessionState OBJECT-TYPE
		SYNTAX		INTEGER { idle(1), run(2), error(4), valid(5) }
		MAX-ACCESS	read-write
		STATUS		current
		DESCRIPTION	"Indicates current state of the SQL session:
				idle: entry/session has been created by the 
				     agent and has to be initialized by 
				     the manager.
				run: to be set by the manager to initiate the 
				     execution of the SQL query.
				error: execution of the SQL query resulted in 
				     an error
				valid: execution of the SQL query was 
				     successful.
				Transitions:
				idle -> run -> {valid, error}
				"
		::= { hicomSQLsessionEntry 2 }

	hicomSQLsessionStatement OBJECT-TYPE
		SYNTAX		Integer32
		MAX-ACCESS	read-write
		STATUS		current
		DESCRIPTION	"Selects the predefined SQL statement for 
				this session."
		::= { hicomSQLsessionEntry 3 }

	hicomSQLsessionWhereClause OBJECT-TYPE
		SYNTAX		DisplayString
		MAX-ACCESS	read-write
		STATUS		current
		DESCRIPTION	"The WHERE clause to be used for this 
				SQL statement."
		::= { hicomSQLsessionEntry 4 }

	hicomSQLsessionTimeout OBJECT-TYPE
		SYNTAX		INTEGER (10..86400)
		UNITS		"seconds"
		MAX-ACCESS	read-write
		STATUS		current
		DESCRIPTION	"Maximum time interval between any two 
				succeeding retrievals to the result data of 
				the session. If no access is made to the 
				to the result data of the session within
				SQLsessionTimeout, the session and the related 
				result data will be erased.
				SQLSessionTimeout is  messured in seconds."
		DEFVAL		{ 30 }
		::= { hicomSQLsessionEntry 5 }

	hicomSQLsessionResultSize OBJECT-TYPE
		SYNTAX		Integer32
		MAX-ACCESS	read-only
		STATUS		current
		DESCRIPTION	"The result of a SQL query is stored in result 
				file. hicomSQLsessionResultSize specifies the 
				size in number of octets of the result file."
		DEFVAL		{ 0 }
		::= { hicomSQLsessionEntry 6 }

	hicomSQLsessionResultNoLines OBJECT-TYPE
		SYNTAX		Integer32
		MAX-ACCESS	read-only
		STATUS		current
		DESCRIPTION	"The result of a SQL query is stored in result 
				file. hicomSQLsessionResultNoLines specifies 
				the number of lines in the result file."
		DEFVAL		{ 0 }
		::= { hicomSQLsessionEntry 7 }

hicomSQLresultTable	OBJECT-TYPE
	SYNTAX		SEQUENCE OF HicomSQLresultEntry
	MAX-ACCESS	not-accessible
	STATUS		current
	DESCRIPTION	"This table contains the results of finished 
			SQL sessions."
	::=	{ hicomSQL 3 }

	hicomSQLresultEntry	OBJECT-TYPE
		SYNTAX		HicomSQLresultEntry
		MAX-ACCESS	not-accessible
		STATUS		current
		DESCRIPTION	"An entry in the hicomSQLresultTable."
		INDEX		{ hicomSQLresultId, hicomSQLresultBlockNo}
		::=	{ hicomSQLresultTable 1 }

	HicomSQLresultEntry ::=
		SEQUENCE {
			hicomSQLresultId		Integer32,
			hicomSQLresultBlockNo		Integer32,
			hicomSQLresultData		DisplayString
		}

	hicomSQLresultId OBJECT-TYPE
		SYNTAX		Integer32
		MAX-ACCESS	read-only
		STATUS		current
		DESCRIPTION	"Session ID to which these results belong."
		::= { hicomSQLresultEntry 1 }

	hicomSQLresultBlockNo OBJECT-TYPE
		SYNTAX		Integer32
		MAX-ACCESS	read-only
		STATUS		current
		DESCRIPTION	"Identifier of the data block to be returned 
				from the result file. The result file is 
				logically segmented in blocks of 256 octets. 
				Data from the result file is read by GET in 
				block units. Each GET will return one block 
				specified by BlockNo.  A GET on BlockNo '0' 
				will return octet 0 to 255 of the result file 
				and a GET on BlockNo '1' octet 256 to 511 etc.
				If the first or last block is smaller then 256 
				octets, then a corresponding smaller 
				hicomSQLresultData block will be returned."
		::= { hicomSQLresultEntry 2 }


	hicomSQLresultData OBJECT-TYPE
		SYNTAX		DisplayString
		MAX-ACCESS	read-only
		STATUS		current
		DESCRIPTION	"Requested block of result data."
		::= { hicomSQLresultEntry 3 }

hicomSQLinfo OBJECT IDENTIFIER ::= { hicomSQL 4 }

	hicomSQLSubagentLastMsgNo	OBJECT-TYPE
		SYNTAX		Integer32
		MAX-ACCESS	read-only
		STATUS		current
		DESCRIPTION	"The last message, warning or error number 
					issued by the SQL subagent."
	::=	{ hicomSQLinfo 1 }

	hicomSQLSubagentLastMsgText	OBJECT-TYPE
		SYNTAX		OCTET STRING
		MAX-ACCESS	read-only
		STATUS		current
		DESCRIPTION	"The last message, warning or error text
					issued by the SQL subagent."
	::=	{ hicomSQLinfo 2 }


-- conformance information
-- units of conformance

hicomSQLsessionGroup	OBJECT-GROUP
	OBJECTS	{
		hicomSQLsessionIdGenerator,
		hicomSQLsessionId,
		hicomSQLsessionState,
		hicomSQLsessionResultSize,
		hicomSQLsessionResultNoLines,
		hicomSQLsessionTimeout,
		hicomSQLsessionStatement,
		hicomSQLsessionWhereClause
	}
	STATUS		current
	DESCRIPTION	"Conformance statement for hicomSQLsessionGroup."
	::=	{ hicomSQLtree 1 }

hicomSQLresultGroup	OBJECT-GROUP
	OBJECTS	{
		hicomSQLresultId,
		hicomSQLresultBlockNo,
		hicomSQLresultData
	}
	STATUS		current
	DESCRIPTION	"Conformance statement for hicomSQLresultGroup."
	::=	{ hicomSQLtree 2 }

hicomSQLinfoGroup	OBJECT-GROUP
	OBJECTS	{
		hicomSQLSubagentLastMsgNo,
		hicomSQLSubagentLastMsgText
	}
	STATUS		current
	DESCRIPTION	"Conformance statement for hicomSQLinfoGroup."
	::=	{ hicomSQLtree 3 }

-- Traps sql agent

internalMessageSQLSubagent	NOTIFICATION-TYPE
	OBJECTS		{ hicomSQLSubagentLastMsgNo, hicomSQLSubagentLastMsgText }
	STATUS		current
	DESCRIPTION	"An internalMessage trap contains an informational
			 message generated by the subagent."
	::=	{ hicomSQLTraps 0 }

internalWarningSQLSubagent	NOTIFICATION-TYPE
	OBJECTS		{ hicomSQLSubagentLastMsgNo, hicomSQLSubagentLastMsgText }
	STATUS		current
	DESCRIPTION	"An internalWarning trap contains a warning message
			 generated by a subagent."
	::=	{ hicomSQLTraps 1 }

internalErrorSQLSubagent	NOTIFICATION-TYPE
	OBJECTS		{ hicomSQLSubagentLastMsgNo, hicomSQLSubagentLastMsgText }
	STATUS		current
	DESCRIPTION	"An internalError trap contains an error message
			 generated by a subagent. 
			 After issuing this trap, the agent terminates."
	::=	{ hicomSQLTraps 2 }

hicomSQLsessionFinished		NOTIFICATION-TYPE
	OBJECTS		{ hicomSQLsessionId, 
				hicomSQLsessionState,
				hicomSQLsessionResultSize,
				hicomSQLsessionResultNoLines }
	STATUS		current
	DESCRIPTION
		"A hicomSQLsessionFinished trap indicates the completion of
		an SQL query. If hicomSQLsessionState indicates an error in
		the query, hicomSQLsessionResultLines will contain the SQL
		error code instead of the number of resulting lines. In this
		case, no entry in the hicomSQLresultTable will be created."
	::=	{ hicomSQLTraps 20 }


-- BEGIN BLOCK discov

hicomDiscovTable	OBJECT-TYPE
	SYNTAX		SEQUENCE OF HicomDiscovEntry
	MAX-ACCESS	not-accessible
	STATUS		current
	DESCRIPTION	"This table contains information about the master
			discoveries for each managed Hicom. Starting a master
			discoveries will lead to the individual discoveries 
			being started. 
			The termination of these discoveries will be
			signalled through their respective traps."
	::=	{ hicomDiscov 1 }

	hicomDiscovEntry	OBJECT-TYPE
		SYNTAX		HicomDiscovEntry
		MAX-ACCESS	not-accessible
		STATUS		current
		DESCRIPTION	"Each entry describes the master discovery for 
					a specific hicom."
		INDEX	{ hicomDiscovPabxId }
		::=	{ hicomDiscovTable 1 }

	HicomDiscovEntry ::=
		SEQUENCE {
			hicomDiscovPabxId		Integer32,
			hicomDiscovPabxMnemonic		DisplayString,
			hicomDiscovStatus		DiscoveryStates,
			hicomDiscovMode			DiscoveryModes,
			hicomDiscovTimDat		DisplayString,
			hicomDiscovErrTimDat		DisplayString
		}

	hicomDiscovPabxId	OBJECT-TYPE
		SYNTAX		Integer32
		MAX-ACCESS	read-only
		STATUS		current
		DESCRIPTION	"Unique identifier of the Hicom being discovered."
		::=	{ hicomDiscovEntry 1 }

	hicomDiscovPabxMnemonic	OBJECT-TYPE
		SYNTAX	DisplayString
		MAX-ACCESS	read-only
		STATUS		current
		DESCRIPTION	"Name of the Hicom."
		::=	{ hicomDiscovEntry 2 }

	hicomDiscovStatus	OBJECT-TYPE
		SYNTAX		DiscoveryStates
		MAX-ACCESS	read-write
		STATUS		current
		DESCRIPTION	"Status of master discovery process."
		::=	{ hicomDiscovEntry 3 }

	hicomDiscovMode		OBJECT-TYPE
		SYNTAX		DiscoveryModes
		MAX-ACCESS	read-write
		STATUS		current
		DESCRIPTION	"Mode of master discovery process."
		::=	{ hicomDiscovEntry 4 }

	hicomDiscovTimDat	OBJECT-TYPE
		SYNTAX		DisplayString
		MAX-ACCESS	read-only
		STATUS		current
		DESCRIPTION	"Date and time of the last successful discovery."
		::=	{ hicomDiscovEntry 5 }

	hicomDiscovErrTimDat	OBJECT-TYPE
		SYNTAX		DisplayString
		MAX-ACCESS	read-only
		STATUS		current
		DESCRIPTION	"Date and time of the last failed discovery."
		::=	{ hicomDiscovEntry 6 }

hicomDiscInfo OBJECT IDENTIFIER ::= { hicomDiscov 2 }

	hicomDiscSubagentLastMsgNo	OBJECT-TYPE
		SYNTAX		Integer32
		MAX-ACCESS	read-only
		STATUS		current
		DESCRIPTION	"The last message, warning or error number 
					issued by the discovery subagent."
	::=	{ hicomDiscInfo 1 }

	hicomDiscSubagentLastMsgText	OBJECT-TYPE
		SYNTAX		OCTET STRING
		MAX-ACCESS	read-only
		STATUS		current
		DESCRIPTION	"The last message, warning or error text
					issued by the discovery subagent."
	::=	{ hicomDiscInfo 2 }


hicomDiscovGroup	OBJECT-GROUP
	OBJECTS	{
		hicomDiscovPabxId,
		hicomDiscovPabxMnemonic,
		hicomDiscovStatus,
		hicomDiscovMode,
		hicomDiscovTimDat,
		hicomDiscovErrTimDat
	}
	STATUS		current
	DESCRIPTION	"Conformance statement for hicomDiscovGroup."
	::=	{ hicomDiscovTree 1 }

hicomDiscInfoGroup	OBJECT-GROUP
	OBJECTS	{
		hicomDiscSubagentLastMsgNo,
		hicomDiscSubagentLastMsgText
	}
	STATUS		current
	DESCRIPTION	"Conformance statement for hicomDiscinfoGroup."
	::=	{ hicomDiscovTree 2 }


