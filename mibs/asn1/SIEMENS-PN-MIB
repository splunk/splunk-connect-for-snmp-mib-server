-- the Aps software table

hicomSoftApsTable		OBJECT-TYPE
	SYNTAX		SEQUENCE OF HicomSoftApsEntry
	MAX-ACCESS	not-accessible
	STATUS		current
	DESCRIPTION	"The table of installed Hicom software systems."
	::=	{ hicomSoftAps 1 }

	hicomSoftApsEntry		OBJECT-TYPE
		SYNTAX		HicomSoftApsEntry
		MAX-ACCESS	not-accessible
		STATUS		current
		DESCRIPTION	"Each entry describes a Hicom software system."
		INDEX	{ hicomSoftApsPabxId, hicomSoftApsID }
		::=	{ hicomSoftApsTable 1 }

	HicomSoftApsEntry ::=
		SEQUENCE {
			hicomSoftApsPabxId		Integer32,
			hicomSoftApsID			OCTET STRING,
			hicomSoftApsPartNo		DisplayString
		}

	hicomSoftApsPabxId		OBJECT-TYPE
		SYNTAX		Integer32
		MAX-ACCESS	read-only
		STATUS		current
		DESCRIPTION	"Unique identifier of the Hicom system."
		::=	{ hicomSoftApsEntry 1 }

	hicomSoftApsID		OBJECT-TYPE
		SYNTAX		OCTET STRING
		MAX-ACCESS	read-only
		STATUS		current
		DESCRIPTION	"Identifier of the software package."
		::=	{ hicomSoftApsEntry 2 }

	hicomSoftApsPartNo	OBJECT-TYPE
		SYNTAX		DisplayString
		MAX-ACCESS	read-only
		STATUS		current
		DESCRIPTION	"Siemens part number of the software package."
		::=	{ hicomSoftApsEntry 3 }

-- the software patch table

hicomSoftPatchTable		OBJECT-TYPE
	SYNTAX		SEQUENCE OF HicomSoftPatchEntry
	MAX-ACCESS	not-accessible
	STATUS		current
	DESCRIPTION	"The table of installed Hicom software patches."
	::=	{ hicomSoftAps 2 }

	hicomSoftPatchEntry		OBJECT-TYPE
		SYNTAX		HicomSoftPatchEntry
		MAX-ACCESS	not-accessible
		STATUS		current
		DESCRIPTION	"Each entry describes a software patch."
		INDEX		{ hicomSoftPatchPabxId, hicomSoftPatchNo }
		::=	{ hicomSoftPatchTable 1 }

	HicomSoftPatchEntry	::=
		SEQUENCE {
			hicomSoftPatchPabxId	Integer32,
			hicomSoftPatchNo		OCTET STRING,
			hicomSoftPatchHWMod		DisplayString,
			hicomSoftPatchActType	DisplayString,
			hicomSoftPatchGroup	DisplayString
		}

	hicomSoftPatchPabxId	OBJECT-TYPE
		SYNTAX		Integer32
		MAX-ACCESS	read-only
		STATUS		current
		DESCRIPTION	"Unique identifier of the Hicom system."
		::=	{ hicomSoftPatchEntry 1 }

	hicomSoftPatchNo		OBJECT-TYPE
		SYNTAX		OCTET STRING
		MAX-ACCESS	read-only
		STATUS		current
		DESCRIPTION	"Identifier of the software patch."
		::=	{ hicomSoftPatchEntry 2 }

	hicomSoftPatchHWMod		OBJECT-TYPE
		SYNTAX		DisplayString
		MAX-ACCESS	read-only
		STATUS		current
		DESCRIPTION	"Identification of a hardware module the patch is for."
		::=	{ hicomSoftPatchEntry 3 }

	hicomSoftPatchActType		OBJECT-TYPE
		SYNTAX		DisplayString
		MAX-ACCESS	read-only
		STATUS		current
		DESCRIPTION	"Activation state of patch."
		::=	{ hicomSoftPatchEntry 4 }

	hicomSoftPatchGroup		OBJECT-TYPE
		SYNTAX		DisplayString
		MAX-ACCESS	read-only
		STATUS		current
		DESCRIPTION	"Group identifier of a patch."
		::=	{ hicomSoftPatchEntry 5 }

-- The hicomSWInfo group

hicomSoftSubagentLastMsgNo OBJECT-TYPE
	SYNTAX		Integer32
	MAX-ACCESS	read-only
	STATUS		current
	DESCRIPTION	"The last message, warning or error number 
				issued by the Software subagent."
	::=	{ hicomSoftInfo 1 }

hicomSoftSubagentLastMsgText	OBJECT-TYPE
	SYNTAX		OCTET STRING
	MAX-ACCESS	read-only
	STATUS		current
	DESCRIPTION	"The last message, warning or error text
				issued by the Software subagent."
	::=	{ hicomSoftInfo 2 }

hicomSoftResultData	OBJECT-TYPE
	SYNTAX		Integer32
	MAX-ACCESS	read-write
	STATUS		current
	DESCRIPTION	"Is used for agent internal communication.
			 Contains the PabxId of a Hicom for which
			 the last discovery process was conveyed.
			 Is SET by the discovery agent and
			 indicates the availability of new discovered data.
			 The new data is to be incorporated by the subagent
			 into the subagent's data base."
	::=	{ hicomSoftInfo 3 }

