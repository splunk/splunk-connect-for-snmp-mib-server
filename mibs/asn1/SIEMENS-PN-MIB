	HicomCDSUEntry ::=
		SEQUENCE {
			hicomCDSUPabxId		Integer32,
			hicomCDSUCabAddr	OCTET STRING,
			hicomCDSUMntLevel	Integer32,
			hicomCDSUSlotAddr	Integer32,
			hicomCDSUPartNo		DisplayString,
			hicomCDSUCode		DisplayString,
			hicomCDSUVers		Integer32,
			hicomCDSUFirmware	DisplayString
		}

	hicomCDSUPabxId		OBJECT-TYPE
		SYNTAX		Integer32
		MAX-ACCESS	read-only
		STATUS		current
		DESCRIPTION	"The identifier of the Hicom to which the board belongs."
		::=	{ hicomCDSUEntry 1 }

	hicomCDSUCabAddr	OBJECT-TYPE
		SYNTAX		OCTET STRING
		MAX-ACCESS	read-only
		STATUS		current
		DESCRIPTION	"Identifier of the cabinet to which the board belongs."
		::=	{ hicomCDSUEntry 2 }

	hicomCDSUMntLevel	OBJECT-TYPE
		SYNTAX		Integer32
		MAX-ACCESS	read-only
		STATUS		current
		DESCRIPTION	"Mounting level of the frame containing the board. "
		::=	{ hicomCDSUEntry 3 }

	hicomCDSUSlotAddr	OBJECT-TYPE
		SYNTAX		Integer32
		MAX-ACCESS	read-only
		STATUS		current
		DESCRIPTION	"Slot address for the board within its frame."
		::=	{ hicomCDSUEntry 4 }

	hicomCDSUPartNo		OBJECT-TYPE
		SYNTAX		DisplayString
		MAX-ACCESS	read-only
		STATUS		current
		DESCRIPTION	"Siemens part number of the installed board."
		::=	{ hicomCDSUEntry 5 }

	hicomCDSUCode	OBJECT-TYPE
		SYNTAX		DisplayString
		MAX-ACCESS	read-only
		STATUS		current
		DESCRIPTION	"Short name of the board."
		::=	{ hicomCDSUEntry 6 }

	hicomCDSUVers		OBJECT-TYPE
		SYNTAX		Integer32
		MAX-ACCESS	read-only
		STATUS		current
		DESCRIPTION	"Version number of the board."
		::=	{ hicomCDSUEntry 7 }

	hicomCDSUFirmware	OBJECT-TYPE
		SYNTAX		DisplayString
		MAX-ACCESS	read-only
		STATUS		current
		DESCRIPTION	"Firmware version installed on the board."
		::=	{ hicomCDSUEntry 8 }

-- The BCSMTable, describes service module related boards for line control/	line bus control
-- Shows configured board type and actual installed board type
-- The contents are derived from the Hicom AMO ABFR-BCSM

hicomBCSMTable	OBJECT-TYPE
	SYNTAX		SEQUENCE OF HicomBCSMEntry
	MAX-ACCESS	not-accessible
	STATUS		current
	DESCRIPTION	"The table of hicom BCSM boards."
	::=	{ hicomBoards 3 }

	hicomBCSMEntry		OBJECT-TYPE
		SYNTAX		HicomBCSMEntry
		MAX-ACCESS	not-accessible
		STATUS		current
		DESCRIPTION	"Each entry describes one HW board."
		INDEX		{ hicomBCSMPabxId, hicomBCSMMod, hicomBCSMSlotAddr}
		::=	{ hicomBCSMTable 1 }

	HicomBCSMEntry ::=
		SEQUENCE {
			hicomBCSMPabxId		Integer32,
			hicomBCSMMod		OCTET STRING,
			hicomBCSMSlotAddr	Integer32,
			hicomBCSMConfBoard	DisplayString,
			hicomBCSMCode		DisplayString,
			hicomBCSMInstBoard	DisplayString,
			hicomBCSMVers		Integer32,
			hicomBCSMFirmware	DisplayString,
			hicomBCSMStatus		DisplayString
		}

	hicomBCSMPabxId		OBJECT-TYPE
		SYNTAX		Integer32
		MAX-ACCESS	read-only
		STATUS		current
		DESCRIPTION	"The identifier of the Hicom to which the board belongs."
		::=	{ hicomBCSMEntry 1 }

	hicomBCSMMod	OBJECT-TYPE
		SYNTAX		OCTET STRING
		MAX-ACCESS	read-only
		STATUS		current
		DESCRIPTION	"Identifier of the module to which the board belongs."
		::=	{ hicomBCSMEntry 2 }

	hicomBCSMSlotAddr	OBJECT-TYPE
		SYNTAX		Integer32
		MAX-ACCESS	read-only
		STATUS		current
		DESCRIPTION	"Address of slot containing the board. "
		::=	{ hicomBCSMEntry 3 }

	hicomBCSMConfBoard	OBJECT-TYPE
		SYNTAX		DisplayString
		MAX-ACCESS	read-only
		STATUS		current
		DESCRIPTION	"Siemens part number of the configured board."
		::=	{ hicomBCSMEntry 4 }

	hicomBCSMCode		OBJECT-TYPE
		SYNTAX		DisplayString
		MAX-ACCESS	read-only
		STATUS		current
		DESCRIPTION	"Short name of the board."
		::=	{ hicomBCSMEntry 5 }

	hicomBCSMInstBoard OBJECT-TYPE
		SYNTAX		DisplayString
		MAX-ACCESS	read-only
		STATUS		current
		DESCRIPTION	"Siemens part number of the installed board.
					This information may in fact differ from that in
					hicomBCSMConfBoard."
		::=	{ hicomBCSMEntry 6 }

	hicomBCSMVers		OBJECT-TYPE
		SYNTAX		Integer32
		MAX-ACCESS	read-only
