-- Message Traps

internalMessageHWSubagent NOTIFICATION-TYPE
	OBJECTS	{ hicomHardSubagentLastMsgNo,
		hicomHardSubagentLastMsgText }
	STATUS	current
	DESCRIPTION	"An internalMessage trap contains an informational
			 message generated by the subagent."
	::=	{ hicomHWTraps 0 }

-- Warning Traps

internalWarningHWSubagent NOTIFICATION-TYPE
	OBJECTS	{ hicomHardSubagentLastMsgNo,
		hicomHardSubagentLastMsgText }
	STATUS	current
	DESCRIPTION	"An internalWarning trap contains a warning message
			 generated by a subagent."
	::=	{ hicomHWTraps 1 }

-- Error Traps

internalErrorHWSubagent	NOTIFICATION-TYPE
	OBJECTS	{ hicomHardSubagentLastMsgNo,
		hicomHardSubagentLastMsgText }
	STATUS	current
	DESCRIPTION	"An internalError trap contains an error message
			 generated by a subagent. 
			 After issuing this trap, the agent terminates."
	::=	{ hicomHWTraps 2 }

-- HW discovery successfully terminated
hicomHWDiscovSucc	NOTIFICATION-TYPE
	OBJECTS	{ hicomHWTrpPabxId, hicomHWTrpPabxMnemonic }
	STATUS	current
	DESCRIPTION		"A hicomHWDiscovSucc trap signifies the
			successful termination of a discovery request / process.
			hicomHWDiscovPabxId: indicates the	Pbx involved."
	::=	{ hicomHWTraps 10 }

-- HW discovery unsuccessfully terminated

hicomHWDiscovErr	NOTIFICATION-TYPE
	OBJECTS	{ hicomHWTrpPabxId, hicomHWTrpPabxMnemonic }
	STATUS	current
	DESCRIPTION		"A hicomHWDiscovErr trap signifies the
			unsuccessful termination of a discovery request / process.
			hicomHWDiscovPabxId: indicates the	Pbx involved."
	::=	{ hicomHWTraps 11 }


-- HW discovery process is busy

hicomHWDiscovBusy	NOTIFICATION-TYPE
	OBJECTS	{ hicomHWTrpPabxId, hicomHWTrpPabxMnemonic }
	STATUS	current
	DESCRIPTION		"A hicomHWDiscovBusy trap signifies 
			that a hw discovery process is running.
			hicomHWDiscovPabxId: indicates the	Pbx involved."
	::=	{ hicomHWTraps 19 }



-- BEGIN BLOCK topo

-- The hicomTopo MIB subtree components
-- The MIB subtree falls into the following groups:

hicomTrunkGrp		OBJECT IDENTIFIER ::= {	hicomTopo 1 }
hicomTrunk			OBJECT IDENTIFIER ::= { hicomTopo 2 }
hicomTopoInfo		OBJECT IDENTIFIER ::= {	hicomTopo 3 }

-- BEGIN BLOCK discov
hicomTopoDiscovery	OBJECT IDENTIFIER ::= {	hicomTopo 4 }

-- BEGIN BLOCK topo
hicomKntop			OBJECT IDENTIFIER ::= { hicomTopo 5 }

TrunkTypes ::= INTEGER { undef(1), analog(2), digital(3), special(4) }

-- The hicomTrunkGrp group

-- The TrunkGrp table

	hicomTrunkGrpTable	OBJECT-TYPE
		SYNTAX	SEQUENCE OF HicomTrunkGrpEntry
		MAX-ACCESS	not-accessible
		STATUS	current
		DESCRIPTION		"The table of trunk groups."
		::=	{ hicomTrunkGrp 1 }

	hicomTrunkGrpEntry	OBJECT-TYPE
		SYNTAX	HicomTrunkGrpEntry
		MAX-ACCESS		not-accessible
		STATUS	current
		DESCRIPTION		"Each entry describes a Hicom trunk group. "
		INDEX	{ hicomTrunkGrpPabxId, hicomTrunkGrpNo }
		::=	{ hicomTrunkGrpTable 1 }

	HicomTrunkGrpEntry ::=
		SEQUENCE {
			hicomTrunkGrpPabxId		Integer32,
			hicomTrunkGrpNo			Integer32,
			hicomTrunkGrpNNodeNo		Integer32,
			hicomTrunkGrpVNodeNo		Integer32,
			hicomTrunkGrpTrunkType		TrunkTypes,
			hicomTrunkGrpMaxNo		Integer32,
			hicomTrunkGrpName		DisplayString,
			hicomTrunkGrpDevice		DisplayString,
			hicomTrunkGrpAlarms		OCTET STRING
		}

	hicomTrunkGrpPabxId		OBJECT-TYPE
		SYNTAX	Integer32
		MAX-ACCESS		read-only
		STATUS	current
		DESCRIPTION		"Unique identifier of the Hicom system."
		::=	{ hicomTrunkGrpEntry 1 }

	hicomTrunkGrpNo		OBJECT-TYPE
		SYNTAX		Integer32
		MAX-ACCESS	read-only
		STATUS		current
		DESCRIPTION		"Identifier Trunk Group."
		::=	{ hicomTrunkGrpEntry 2 }

	hicomTrunkGrpNNodeNo	OBJECT-TYPE
		SYNTAX		Integer32
		MAX-ACCESS	read-write
		STATUS		current
		DESCRIPTION		"Number of neighbour node.	Value to be delivered by manager."
		DEFVAL		{ 0 }
		::=	{ hicomTrunkGrpEntry 3 }

	hicomTrunkGrpVNodeNo	OBJECT-TYPE
		SYNTAX		Integer32
		MAX-ACCESS	read-only
		STATUS		current
		DESCRIPTION		"Derived from source SNodeNo of trunk table."
			DEFVAL		{ 0 }
		::=	{ hicomTrunkGrpEntry 4 }

	hicomTrunkGrpTrunkType	OBJECT-TYPE
		SYNTAX		TrunkTypes
		MAX-ACCESS	read-only
		STATUS		current
		DESCRIPTION	"Type of the trunk group."
		::=	{ hicomTrunkGrpEntry 5 }

	hicomTrunkGrpMaxNo OBJECT-TYPE
		SYNTAX		Integer32
		MAX-ACCESS	read-only
		STATUS		current
		DESCRIPTION		"The maximum number of trunks a trunk groupp may contain."
		::=	{ hicomTrunkGrpEntry 6 }

	hicomTrunkGrpName	OBJECT-TYPE
		SYNTAX		DisplayString
		MAX-ACCESS	read-only
		STATUS		current
		DESCRIPTION		"Name of the trunk group. "
		::=	{ hicomTrunkGrpEntry 7 }

	hicomTrunkGrpDevice	OBJECT-TYPE
		SYNTAX	DisplayString
		MAX-ACCESS		read-only
		STATUS	current
		DESCRIPTION		"Type of device."
		::= { hicomTrunkGrpEntry 8 }

	hicomTrunkGrpAlarms	OBJECT-TYPE
		SYNTAX		OCTET STRING
		MAX-ACCESS	read-only
		STATUS		current
		DESCRIPTION		"Octet string contains configured AlSubIds (0 up to 519)
				of related Pabx und trunkgroup.
				The hicomTrunkGrpAlarms string is 65 octets long.
				AlSubId '0' is set, if (hicomTrunkGrpAlarms[0] AND 0x01)
				evaluates to true.
				AlSubId '7' is set, if (hicomTrunkGrpAlarms[0] AND 0x80)
				evaluates to true.
				AlSubId '8' is set, if (hicomTrunkGrpAlarms[1] AND 0x01)
				evaluates to true etc."
		::=	{ hicomTrunkGrpEntry 9 }

-- The hicomTrunk group

-- The Trunk Table

	hicomTrunkTable		OBJECT-TYPE
		SYNTAX	SEQUENCE OF HicomTrunkEntry
		MAX-ACCESS		not-accessible
		STATUS	current
		DESCRIPTION		"The table of trunks."
		::=	{ hicomTrunk 1 }

	hicomTrunkEntry		OBJECT-TYPE
		SYNTAX	HicomTrunkEntry
		MAX-ACCESS		not-accessible
		STATUS	current
		DESCRIPTION		"Each entry describes a Hicom trunk."
		INDEX	{ hicomTrunkPabxId, hicomTrunkType, hicomTrunkTrunkGrpNo,
		hicomTrunkLtg, hicomTrunkLtu, hicomTrunkSlot, hicomTrunkNo,
		hicomTrunkChannelGrp}
		::=	{ hicomTrunkTable 1 }

	HicomTrunkEntry	::=
		SEQUENCE {
			hicomTrunkPabxId	Integer32,
			hicomTrunkType		TrunkTypes,
			hicomTrunkTrunkGrpNo	Integer32,
			hicomTrunkLtg		Integer32,
			hicomTrunkLtu		Integer32,
			hicomTrunkSlot		Integer32,
			hicomTrunkNo		Integer32,
			hicomTrunkChannelGrp	Integer32,
			hicomTrunkSNodeNo	Integer32,
			hicomTrunkStatOp	INTEGER,
			hicomTrunkAlSubId	Integer32,
			hicomTrunkChannels	OCTET STRING,
			hicomTrunkDevice	DisplayString,
			hicomTrunkName		DisplayString
		}

	hicomTrunkPabxId	OBJECT-TYPE
