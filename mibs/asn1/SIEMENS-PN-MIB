hicomErrorGroup OBJECT-GROUP
	OBJECTS {
		hicomErrorPabxId, hicomErrorSerialNo,
		hicomErrorMsgId, hicomErrorPriority,
		hicomErrorAction, hicomErrorAbsMod,
		hicomErrorEvent, hicomErrorSubevent,
		hicomErrorCardRef, hicomErrorBoardVersion,
		hicomErrorFwType, hicomErrorTimDat,
		hicomErrorAlGroup, hicomErrorAlSubId,
		hicomErrorOrigText
	}
	STATUS	current
	DESCRIPTION "Conformance statement for hicomErrorGroup."
	::= { hicomErrorTree 1 }

hicomAlConfigGroup	OBJECT-GROUP
	OBJECTS { hicomAlConfPabxId, hicomAlConfAlGroup, hicomAlConfAlSubId,
			hicomAlConfName, hicomAlConfThreshold1, hicomAlConfThreshold2,
			hicomAlConfTime1, hicomAlConfTime2, hicomAlConfTimeH,
			hicomAlConfBase, hicomAlConfTargetPabxId,
			hicomAlConfTargetAlGroup, hicomAlConfTargetAlSubId,
			hicomAlConfTargetLtg, hicomAlConfTargetLtu,
			hicomAlConfTargetSlot, hicomAlConfTargetTrunkNo,
			hicomAlConfPersonPabxId, hicomAlConfPersonAlGroup,
			hicomAlConfPersonAlSubId, hicomAlConfPersonExtNo
		}
	STATUS	current
	DESCRIPTION "Conformance statement for hicomAlConfigGroup."
	::= { hicomAlConfTree 1 }

hicomErrAlConfInfoGroup OBJECT-GROUP
	OBJECTS { hicomErrAlConfSubagentLastMsgNo,
		hicomErrAlConfSubagentLastMsgText,
		hicomErrAlConfResultData
		}
	STATUS		current
	DESCRIPTION "Conformance statement for hicomErrAlConfInfoGroup."
	::= { hicomAlConfTree 2 }

