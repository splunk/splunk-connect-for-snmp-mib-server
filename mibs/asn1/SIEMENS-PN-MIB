	hicomEDTrpPabxId	OBJECT-TYPE
		SYNTAX		Integer32
		MAX-ACCESS	not-accessible
		STATUS		current
		DESCRIPTION	"Unique identifier of a Hicom system."
		::=	{ hicomDiscTrapVariables 1 }

	hicomEDTrpPabxMnemonic OBJECT-TYPE
		SYNTAX		DisplayString
		MAX-ACCESS 	not-accessible
		STATUS		current
		DESCRIPTION	"Mnemonic name of the Hicom system."
		::=	{ hicomDiscTrapVariables 2 }

