IMPORTS
    enterprises, IpAddress,
    TimeTicks, Counter, Gauge FROM RFC1155-SMI
    OBJECT-TYPE FROM RFC-1212
    TRAP-TYPE FROM RFC-1215
    DisplayString, ifIndex FROM RFC1213-MIB
     pdnmsdsl
      FROM PDN-HEADER-MIB;
