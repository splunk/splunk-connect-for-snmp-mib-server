hdsl2ShdslMaxBw     OBJECT-TYPE
      SYNTAX  INTEGER {
      notApplicable (1),
      r192          (2), 
      r256          (3), 
      r320          (4),
	  r384          (5),
	  r448          (6),
      r512          (7), 
      r576          (8),
	  r640          (9),
	  r704          (10),
      r768          (11), 
      r832          (12),
	  r896          (13),
	  r960          (14),
      r1024         (15), 
      r1088         (16),
	  r1152         (17),
	  r1216         (18),
	  r1280         (19),
	  r1344         (20),
	  r1408         (21),
	  r1472         (22),
	  r1536         (23),
	  r1600         (24),
	  r1664         (25),
	  r1728         (26),
	  r1792         (27),
	  r1856         (28),
