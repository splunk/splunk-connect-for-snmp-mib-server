     wfX25PadCurrentCfgLCNHigh OBJECT-TYPE
        SYNTAX  INTEGER(1..4095)
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION
                "Upper range for logical channel numbers "
        ::= { wfX25PadCurrentCfgEntry 9 }

	
    wfX25PadStatTable OBJECT-TYPE
        SYNTAX  SEQUENCE OF WfX25PadStatEntry
        ACCESS  not-accessible
        STATUS  mandatory
        DESCRIPTION
                "Table of X25 PAD Stat Objects"
        ::= { wfX25PadGroup 3 }

    wfX25PadStatEntry OBJECT-TYPE
        SYNTAX  WfX25PadStatEntry
        ACCESS  not-accessible
        STATUS  mandatory
        DESCRIPTION
                "An entry in the X25 Pad Config Table"
        INDEX   { wfX25PadStatSlot,
                  wfX25PadStatConnector }
        ::= { wfX25PadStatTable 1 }

    WfX25PadStatEntry ::= SEQUENCE {
            wfX25PadStatState
                INTEGER,
            wfX25PadStatSlot
                INTEGER,
            wfX25PadStatConnector
                INTEGER,
	    wfX25PadStatTotalConnCount
                Counter,
	    wfX25PadStatRxRR
		Counter,
	    wfX25PadStatTxRR
		Counter,
	    wfX25PadStatRxRNR
		Counter,
	    wfX25PadStatTxRNR
		Counter,
	    wfX25PadStatRxRestart
		Counter,
	    wfX25PadStatTxRestart
		Counter
    }
    wfX25PadStatState OBJECT-TYPE
        SYNTAX  INTEGER {
                    up(1),
                    down(2),
                    init(3)
                }
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION
                "Current state of X.25 PAD sofware on the I-SDB"
        DEFVAL  { down }
        ::= { wfX25PadStatEntry 1 }


    wfX25PadStatSlot OBJECT-TYPE
        SYNTAX  INTEGER
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION
                "Instance ID Slot, filled in by ISDB mib gate"
        ::= { wfX25PadStatEntry 2 }

    wfX25PadStatConnector OBJECT-TYPE
        SYNTAX  INTEGER
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION
                "Instance ID Connector, filled in by ISDB mib gate. For the ASN
                this attribute is an encoded value and is equal to
                'module'*10 + 'connector'. Where 'module' is defined in
                attribute 86 and 'connector' is defined in attribute 78
                of this table. For non-ASN platforms this attribute is
