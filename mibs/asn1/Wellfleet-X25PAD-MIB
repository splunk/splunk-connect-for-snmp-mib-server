                INTEGER,
            wfX25PadPortDefaultCfgX3echo
                INTEGER,
            wfX25PadPortDefaultCfgX3forward
                INTEGER,
            wfX25PadPortDefaultCfgX3idle
                INTEGER,
            wfX25PadPortDefaultCfgX3device
                INTEGER,
            wfX25PadPortDefaultCfgX3signals
                INTEGER,
            wfX25PadPortDefaultCfgX3break
                INTEGER,
            wfX25PadPortDefaultCfgX3discard
                INTEGER,
