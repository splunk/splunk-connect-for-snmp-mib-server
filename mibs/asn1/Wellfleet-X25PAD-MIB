Wellfleet-X25PAD-MIB DEFINITIONS ::= BEGIN

    IMPORTS
      Counter
        FROM RFC1155-SMI
      OBJECT-TYPE
        FROM RFC-1212
      DisplayString
        FROM RFC1213-MIB
      wfX25PadGroup
        FROM Wellfleet-COMMON-MIB;

    wfX25PadDefaultCfgTable OBJECT-TYPE
        SYNTAX  SEQUENCE OF WfX25PadDefaultCfgEntry
        ACCESS  not-accessible
        STATUS  mandatory
        DESCRIPTION
                "Table of X25 PAD Config Objects"
        ::= { wfX25PadGroup 1 }

    wfX25PadDefaultCfgEntry OBJECT-TYPE
        SYNTAX  WfX25PadDefaultCfgEntry
        ACCESS  not-accessible
        STATUS  mandatory
        DESCRIPTION
                "An entry in the X25 Pad Config Table"
        INDEX   { wfX25PadDefaultCfgSlot,
                  wfX25PadDefaultCfgConnector }
        ::= { wfX25PadDefaultCfgTable 1 }

    WfX25PadDefaultCfgEntry ::= SEQUENCE {
            wfX25PadDefaultCfgDelete
                INTEGER,
            wfX25PadDefaultCfgDisable
                INTEGER,
            wfX25PadDefaultCfgState
                INTEGER,
            wfX25PadDefaultCfgSlot
                INTEGER,
            wfX25PadDefaultCfgConnector
                INTEGER,
	    wfX25PadDefaultCfgCircuit
	        INTEGER,
	    wfX25PadDefaultCfgX121Addr
		DisplayString,
	    wfX25PadDefaultCfgDNIC
		DisplayString,
	    wfX25PadDefaultCfgLenSubAddress
		INTEGER,
	    wfX25PadDefaultCfgLCNLow
		INTEGER,
	    wfX25PadDefaultCfgLCNHigh
		INTEGER
    }

    wfX25PadDefaultCfgDelete OBJECT-TYPE
        SYNTAX  INTEGER {
                    created(1),
                    deleted(2)
                }
        ACCESS  read-write
        STATUS  mandatory
        DESCRIPTION
                "Create/Delete parameter"
        DEFVAL  { created }
        ::= { wfX25PadDefaultCfgEntry 1 }

    wfX25PadDefaultCfgDisable OBJECT-TYPE
        SYNTAX  INTEGER {
                    enabled(1),
                    disabled(2)
                }
        ACCESS  read-write
        STATUS  mandatory
        DESCRIPTION
                "Enable/Disable parameter"
        DEFVAL  { enabled }
        ::= { wfX25PadDefaultCfgEntry 2 }

    wfX25PadDefaultCfgState OBJECT-TYPE
        SYNTAX  INTEGER {
                    up(1),
                    down(2),
                    init(3)
                }
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION
                "Current state of X.25 PAD sofware on the I-SDB"
        DEFVAL  { down }
        ::= { wfX25PadDefaultCfgEntry 3 }


    wfX25PadDefaultCfgSlot OBJECT-TYPE
        SYNTAX  INTEGER
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION
                "Instance ID Slot, filled in by ISDB mib gate"
        ::= { wfX25PadDefaultCfgEntry 4 }

    wfX25PadDefaultCfgConnector OBJECT-TYPE
        SYNTAX  INTEGER
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION
                "Instance ID Connector, filled in by ISDB mib gate. For the ASN
                this attribute is an encoded value and is equal to
                'module'*10 + 'connector'. Where 'module' is defined in
                attribute 86 and 'connector' is defined in attribute 78
                of this table. For non-ASN platforms this attribute is
                the physical connector number on the slot."
        ::= { wfX25PadDefaultCfgEntry 5 }

    wfX25PadDefaultCfgCircuit OBJECT-TYPE
        SYNTAX  INTEGER
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION
                "Circuit number, filled in by ISDB mib gate"
        ::= { wfX25PadDefaultCfgEntry 6 }

    wfX25PadDefaultCfgX121Addr OBJECT-TYPE
        SYNTAX  DisplayString (SIZE (0..15))
        ACCESS  read-write
        STATUS  mandatory
        DESCRIPTION
                "X.121 address of the X.25 Pad"
        ::= { wfX25PadDefaultCfgEntry 7 }

    wfX25PadDefaultCfgDNIC OBJECT-TYPE
        SYNTAX  DisplayString (SIZE (0..4))
        ACCESS  read-write
        STATUS  mandatory
        DESCRIPTION
                "Data Network Identification Code"
        ::= { wfX25PadDefaultCfgEntry 8 }

    wfX25PadDefaultCfgLenSubAddress OBJECT-TYPE
        SYNTAX  INTEGER(0..8)
        ACCESS  read-write
        STATUS  mandatory
        DESCRIPTION
                "Length of X.121 subaddress "
	DEFVAL  { 2 }
        ::= { wfX25PadDefaultCfgEntry 9 }

    wfX25PadDefaultCfgLCNLow OBJECT-TYPE
        SYNTAX  INTEGER(1..4095)
        ACCESS  read-write
        STATUS  mandatory
        DESCRIPTION
                "Lower range for logical channel numbers"
        ::= { wfX25PadDefaultCfgEntry 10 }

     wfX25PadDefaultCfgLCNHigh OBJECT-TYPE
        SYNTAX  INTEGER(1..4095)
        ACCESS  read-write
        STATUS  mandatory
        DESCRIPTION
                "Upper range for logical channel numbers "
        ::= { wfX25PadDefaultCfgEntry 11 }


    wfX25PadCurrentCfgTable OBJECT-TYPE
        SYNTAX  SEQUENCE OF WfX25PadCurrentCfgEntry
