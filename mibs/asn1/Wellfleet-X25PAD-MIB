        ::= { wfX25PadPortDefaultCfgEntry 34 }

    wfX25PadPortDefaultCfgReverseCharge OBJECT-TYPE
        SYNTAX  OCTET STRING (SIZE (0..2)) 
        ACCESS  read-write
        STATUS  mandatory
        DESCRIPTION
                "If subscribed to, authorizes the DCE to transmit to
		the DTE incoming calls which request the reverse charging
		facility.  In absence of this facility the DCE will not
		transmit to the DTE incoming calls which request the
		reverse charging facility. "
        ::= { wfX25PadPortDefaultCfgEntry 35 }

    wfX25PadPortDefaultCfgThruClassNegotiation OBJECT-TYPE
        SYNTAX  OCTET STRING (SIZE (0..2)) 
        ACCESS  read-write
        STATUS  mandatory
        DESCRIPTION
                "This facility, if subscribed to, allows negotiation on a per
		call basis of the throughput classes.  The throughput classes 
		are considered independently for each direction of data
		transmission. "
        ::= { wfX25PadPortDefaultCfgEntry 36 }

    wfX25PadPortDefaultCfgPacketSizeNegotiation OBJECT-TYPE
        SYNTAX  OCTET STRING (SIZE (0..3)) 
        ACCESS  read-write
        STATUS  mandatory
        DESCRIPTION
                "This facility, if subscribed to, will cause the DTE to
		request packet sizes for both directions of data transmission "
        ::= { wfX25PadPortDefaultCfgEntry 37 }

    wfX25PadPortDefaultCfgWindowSizeNegotiation OBJECT-TYPE
        SYNTAX  OCTET STRING (SIZE (0..3)) 
        ACCESS  read-write
        STATUS  mandatory
        DESCRIPTION
                "This facility, if subscribed to, will cause the DTE to
		request window sizes for both directions of data transmission "
        ::= { wfX25PadPortDefaultCfgEntry 38 }

    wfX25PadPortDefaultCfgIntervalTimer OBJECT-TYPE
        SYNTAX  INTEGER (0..999)
        ACCESS  read-write
        STATUS  mandatory
        DESCRIPTION
                "Time (where 1 equals 10 milliseconds) that the X.25 PAD will
		 wait before forwarding packets with characters in them "
        DEFVAL  { 0 }
        ::= { wfX25PadPortDefaultCfgEntry 39 }

    wfX25PadPortDefaultCfgFullAddressing OBJECT-TYPE
        SYNTAX  INTEGER {
                    enabled(1),
                    disabled(2)
                }
        ACCESS  read-write
        STATUS  mandatory
        DESCRIPTION
                "Determines whether calling addresses should be included or not"
        DEFVAL  { enabled }
        ::= { wfX25PadPortDefaultCfgEntry 40 }

