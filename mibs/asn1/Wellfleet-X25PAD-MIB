        ::= { wfX25PadStatEntry 3 }

    wfX25PadStatTotalConnCount OBJECT-TYPE
        SYNTAX  Counter
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION
                "Total number of connections made to the X.25 Pad "
        ::= { wfX25PadStatEntry 4 }

    wfX25PadStatRxRR OBJECT-TYPE
        SYNTAX  Counter
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION
                "Number of receiver ready packets received"
        ::= { wfX25PadStatEntry 5 }

    wfX25PadStatTxRR OBJECT-TYPE
        SYNTAX  Counter
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION
                "Number of receiver ready packets transmitted"
        ::= { wfX25PadStatEntry 6 }

    wfX25PadStatRxRNR OBJECT-TYPE
        SYNTAX  Counter
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION
                "Number of receiver not ready packets received" 
        ::= { wfX25PadStatEntry 7 }

    wfX25PadStatTxRNR OBJECT-TYPE
        SYNTAX  Counter
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION
                "Number of receiver not ready packets transmitted"
        ::= { wfX25PadStatEntry 8 }

    wfX25PadStatRxRestart OBJECT-TYPE
        SYNTAX  Counter
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION
                "Number of restart packets received"
        ::= { wfX25PadStatEntry 9 }

    wfX25PadStatTxRestart OBJECT-TYPE
        SYNTAX  Counter
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION
                "Number of restart packets transmitted"
        ::= { wfX25PadStatEntry 10 }

    wfX25PadPortDefaultCfgTable OBJECT-TYPE
