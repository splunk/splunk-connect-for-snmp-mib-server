-- ----------------------------------------------------------------------------
-- Port information

dfPortinf OBJECT-TYPE
                  SYNTAX    SEQUENCE OF DFPortinfEntry
                  ACCESS    not-accessible
                  STATUS    mandatory
                  DESCRIPTION
                             "Port information."
                  ::= { dfPort 1 }

dfPortinfEntry OBJECT-TYPE
                  SYNTAX    DFPortinfEntry
                  ACCESS    not-accessible
                  STATUS    mandatory
                  DESCRIPTION
                             "Entry of port information."
                  INDEX {
                      dfPortSerialNumber,
                      dfPortID
                  }
                  ::= { dfPortinf 1 }

DFPortinfEntry ::=
    SEQUENCE {
        dfPortSerialNumber  INTEGER,
        dfPortID            INTEGER,
        dfPortKind          DisplayString (SIZE (0..8)),
        dfPortHostMode      OCTET STRING (SIZE(0..1)),
        dfPortFibreAddress  INTEGER,
        dfPortFibreTopology INTEGER,
        dfPortControlStatus INTEGER,
        dfPortDisplayName   DisplayString (SIZE (0..8)),
        dfPortWWN           OCTET STRING (SIZE(0..8))
    }

dfPortSerialNumber OBJECT-TYPE
                  SYNTAX    INTEGER
                  ACCESS    read-only
                  STATUS    mandatory
                  DESCRIPTION
                             "Serial Number of the DF."
                  ::= { dfPortinfEntry 1 }

dfPortID OBJECT-TYPE
                  SYNTAX    INTEGER
                  ACCESS    read-only
                  STATUS    mandatory
                  DESCRIPTION
                             "Port ID."
                  ::= { dfPortinfEntry 2 }

dfPortKind OBJECT-TYPE
                  SYNTAX    DisplayString (SIZE(0..8))
                  ACCESS    read-only
                  STATUS    mandatory
                  DESCRIPTION
                             "Kind of port.

                                Value   Mean
                                SCSI    SCSI port.
                                Fibre   Fibre port.
                             "
                  ::= { dfPortinfEntry 3 }

dfPortHostMode OBJECT-TYPE
                  SYNTAX    OCTET STRING (SIZE(0..1))
                  ACCESS    read-only
                  STATUS    mandatory
                  DESCRIPTION
         "Host mode of the port.

            Upper 4 bits. - Each bit has the meaning.
            Bit Mean
            0   Not used. (For reservation. Always 0.)
            1   Not used. (For reservation. Always 0.)
            2   Not used. (For reservation. Always 0.)
            3   Ultra-SCSI mode.

            Lower 4 bits. - Numerical value shown by four bits.
            Value   Mean
            0   Normal setting.
            1   IBM-7135 mode.
            2   NCR mode.
            3   Not usesd. (Reserved)
            4   Sequent mode.
            5   DEC OPEN VMS mode.
            6   UNISYS-NX mode
            7   UNISYS-IX mode.
            8   HP mode.
            9   VxVM-DMP mode.
            10  Not used. (Reserved)
            11  MPE mode.
            other   Not used. (Reserved)

            Note: The following value has a special meaning.
            Value   Mean
            0xFF    Out of the host mode setting object.
         "
                  ::= { dfPortinfEntry 4 }

dfPortFibreAddress OBJECT-TYPE
                  SYNTAX    INTEGER
                  ACCESS    read-only
                  STATUS    mandatory
                  DESCRIPTION
    "Fibre address.

            Value & Mean - describes by the format of Value(Address).

        1:(EF)  17:(CD)  33:(B2)  49:(98)  65:(72)  81:(55)  97:(3A) 113:(25)
        2:(E8)  18:(CC)  34:(B1)  50:(97)  66:(71)  82:(54)  98:(39) 114:(23)
        3:(E4)  19:(CB)  35:(AE)  51:(90)  67:(6E)  83:(53)  99:(36) 115:(1F)
        4:(E2)  20:(CA)  36:(AD)  52:(8F)  68:(6D)  84:(52) 100:(35) 116:(1E)
        5:(E1)  21:(C9)  37:(AC)  53:(88)  69:(6C)  85:(51) 101:(34) 117:(1D)
        6:(E0)  22:(C7)  38:(AB)  54:(84)  70:(6B)  86:(4E) 102:(33) 118:(1B)
        7:(DC)  23:(C6)  39:(AA)  55:(82)  71:(6A)  87:(4D) 103:(32) 119:(18)
        8:(DA)  24:(C5)  40:(A9)  56:(81)  72:(69)  88:(4C) 104:(31) 120:(17)
        9:(D9)  25:(C3)  41:(A7)  57:(80)  73:(67)  89:(4B) 105:(2E) 121:(10)
       10:(D6)  26:(BC)  42:(A6)  58:(7C)  74:(66)  90:(4A) 106:(2D) 122:(0F)
       11:(D5)  27:(BA)  43:(A5)  59:(7A)  75:(65)  91:(49) 107:(2C) 123:(08)
       12:(D4)  28:(B9)  44:(A3)  60:(79)  76:(63)  92:(47) 108:(2B) 124:(04)
       13:(D3)  29:(B6)  45:(9F)  61:(76)  77:(5C)  93:(46) 109:(2A) 125:(02)
       14:(D2)  30:(B5)  46:(9E)  62:(75)  78:(5A)  94:(45) 110:(29) 126:(01)
       15:(D1)  31:(B4)  47:(9D)  63:(74)  79:(59)  95:(43) 111:(27) 
       16:(CE)  32:(B3)  48:(9B)  64:(73)  80:(56)  96:(3C) 112:(26) 

            Note: The following value has a special meaning.
            Value   Mean
            0   Out of the Fibre address setting object.
    "
                  ::= { dfPortinfEntry 5 }

dfPortFibreTopology OBJECT-TYPE
                  SYNTAX    INTEGER
                  ACCESS    read-only
                  STATUS    mandatory
                  DESCRIPTION
         "Toplogy information.

            Value   Mean
            1   Fabric(on)&FCAL
            2   Fabric(off)&FCAL
            3   Fabric(on)&PointToPoint
            4   Fabric(off)&PointToPoint
            5   Not Fibre
         "
                  ::= { dfPortinfEntry 6 }

dfPortControlStatus OBJECT-TYPE
                  SYNTAX    INTEGER
                  ACCESS    read-only
                  STATUS    mandatory
                  DESCRIPTION
         "Control flag.

            Value   Mean
            1   Return value.
         "
                  ::= { dfPortinfEntry 7 }

dfPortDisplayName OBJECT-TYPE
                  SYNTAX    DisplayString (SIZE(0..8))
                  ACCESS    read-only
                  STATUS    mandatory
                  DESCRIPTION
                             "Display name of the port."
                  ::= { dfPortinfEntry 8 }

dfPortWWN OBJECT-TYPE
                  SYNTAX    OCTET STRING (SIZE(0..8))
                  ACCESS    read-only
                  STATUS    mandatory
                  DESCRIPTION
                             "WWN of the port."
                  ::= { dfPortinfEntry 9 }

-- ----------------------------------------------------------------------------

dfCommandInternalTable OBJECT-TYPE
                  SYNTAX    SEQUENCE OF DfCommandInternalEntry
                  ACCESS    not-accessible
                  STATUS    mandatory
                  DESCRIPTION
                      "The internal command execution conditon table."
                  ::= { dfCommandExecutionInternalCondition 1 }

dfCommandInternalEntry OBJECT-TYPE
                  SYNTAX    DfCommandInternalEntry
                  ACCESS    not-accessible
                  STATUS    mandatory
                  DESCRIPTION
                      "The internal command execution conditon table entry."
                  INDEX     { dfInternalLun }
                  ::= { dfCommandInternalTable 1 }

DfCommandInternalEntry ::=
    SEQUENCE {
        dfInternalLun
            INTEGER,
        dfInternalReadCommandNumber
            INTEGER,
        dfInternalReadHitNumber
            INTEGER,
        dfInternalReadHitRate
            INTEGER,
        dfInternalWriteCommandNumber
            INTEGER,
        dfInternalWriteHitNumber
            INTEGER,
        dfInternalWriteHitRate
            INTEGER
    }

dfInternalLun OBJECT-TYPE
                  SYNTAX    INTEGER
                  ACCESS    read-only
                  STATUS    mandatory
                  DESCRIPTION
                  "The internal logical unit number."
                  ::= { dfCommandInternalEntry 1 }

dfInternalReadCommandNumber OBJECT-TYPE
                  SYNTAX    INTEGER
                  ACCESS    read-only
                  STATUS    mandatory
                  DESCRIPTION
                  "The numbers of received read commands to the internal LUN."
                  ::= { dfCommandInternalEntry 2 }

dfInternalReadHitNumber OBJECT-TYPE
                  SYNTAX    INTEGER
                  ACCESS    read-only
                  STATUS    mandatory
                  DESCRIPTION
                  "The numbers of read hit commands to the internal LUN."
                  ::= { dfCommandInternalEntry 3 }

dfInternalReadHitRate OBJECT-TYPE
                  SYNTAX    INTEGER
                  ACCESS    read-only
                  STATUS    mandatory
                  DESCRIPTION
                  "The read hit rate to the internal LUN."
                  ::= { dfCommandInternalEntry 4 }

dfInternalWriteCommandNumber OBJECT-TYPE
                  SYNTAX    INTEGER
                  ACCESS    read-only
                  STATUS    mandatory
                  DESCRIPTION
                  "The numbers of received write commands to the internal LUN."
                  ::= { dfCommandInternalEntry 5 }

dfInternalWriteHitNumber OBJECT-TYPE
                  SYNTAX    INTEGER
                  ACCESS    read-only
                  STATUS    mandatory
                  DESCRIPTION
                  "The numbers of write hit commands to the internal LUN."
                  ::= { dfCommandInternalEntry 6 }

dfInternalWriteHitRate OBJECT-TYPE
                  SYNTAX    INTEGER
                  ACCESS    read-only
                  STATUS    mandatory
                  DESCRIPTION
                  "The write hit rate to the internal LUN."
                  ::= { dfCommandInternalEntry 7 }


-- Traps 

systemDown   TRAP-TYPE
              ENTERPRISE  dfraidLan
              DESCRIPTION
                      "System down occurred."
              ::= 1

driveFailure   TRAP-TYPE
              ENTERPRISE  dfraidLan
              DESCRIPTION
                      "Drive blocking occurred."
              ::= 2

fanFailure   TRAP-TYPE
              ENTERPRISE  dfraidLan
              DESCRIPTION
                      "Fan failure occurred."
              ::= 3 

powerSupplyFailure   TRAP-TYPE
              ENTERPRISE  dfraidLan
              DESCRIPTION
                      "Power supply failure occurred."
              ::= 4 

batteryFailure   TRAP-TYPE
              ENTERPRISE  dfraidLan
              DESCRIPTION
                      "Battery failure occurred."
              ::= 5 

cacheFailure   TRAP-TYPE
              ENTERPRISE  dfraidLan
              DESCRIPTION
                      "Cache memory failure occurred."
              ::= 6 

upsFailure    TRAP-TYPE
              ENTERPRISE  dfraidLan
              DESCRIPTION
                      "UPS failure occurred."
              ::= 7 

inboxFailure  TRAP-TYPE
              ENTERPRISE  dfraidLan
              DESCRIPTION
                      "AC line or inbox failure occurred."
              ::= 8 

backupCircuitFailure   TRAP-TYPE
              ENTERPRISE  dfraidLan
              DESCRIPTION
                      "Cache backup circuit failure occurred."
              ::= 9 

otherControllerFailure TRAP-TYPE
              ENTERPRISE  dfraidLan
              DESCRIPTION
                      "Other controller failure occurred."
              ::= 10 

warning                TRAP-TYPE
              ENTERPRISE  dfraidLan
              DESCRIPTION
                      "Warning occurred."
              ::= 11 

spareDriveFailure      TRAP-TYPE
              ENTERPRISE  dfraidLan
              DESCRIPTION
                      "Spare drive failure occurred."
              ::= 12 

microprogramReplacementExecuted      TRAP-TYPE
              ENTERPRISE  dfraidLan
              DESCRIPTION
                      "Microprogram Replacement Executed."
              ::= 13 

encFailure      TRAP-TYPE
              ENTERPRISE  dfraidLan
              DESCRIPTION
                      "Enclosure controller failure occurred."
              ::= 14 

loopFailure      TRAP-TYPE
              ENTERPRISE  dfraidLan
              DESCRIPTION
                      "Loop failure occurred."
              ::= 15 

pathFailure      TRAP-TYPE
              ENTERPRISE  dfraidLan
              DESCRIPTION
                      "Path failure occurred."
              ::= 16 

-- Trap Code #17-20 is reserved.

drivePreMainte   TRAP-TYPE
              ENTERPRISE  dfraidLan
              DESCRIPTION
                      "The count has exceeded the drive preventive
                       maintenance threshold."
              ::= 21

nasServerFailure      TRAP-TYPE
              ENTERPRISE  dfraidLan
              DESCRIPTION
                      "NAS Server failure occurred."
              ::= 200

nasPathFailure      TRAP-TYPE
              ENTERPRISE  dfraidLan
              DESCRIPTION
                      "NAS Path failure occurred."
              ::= 201

nasUpsFailure      TRAP-TYPE
              ENTERPRISE  dfraidLan
              DESCRIPTION
                      "NAS UPS failure occurred."
              ::= 202

