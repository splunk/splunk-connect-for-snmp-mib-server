dfLUNSSwitch OBJECT-TYPE
                  SYNTAX    SEQUENCE OF DFLUNSSwitchEntry
                  ACCESS    not-accessible
                  STATUS    mandatory
                  DESCRIPTION
                             "LUN security switch."
                  ::= { dfLUNS 1 }

dfLUNSSwitchEntry OBJECT-TYPE
                  SYNTAX    DFLUNSSwitchEntry
                  ACCESS    not-accessible
                  STATUS    mandatory
                  DESCRIPTION
                             "Entry of LUN security switch."
                  INDEX {
                      dfSwitchSerialNumber,
                      dfSwitchPortID
                  }
                  ::= { dfLUNSSwitch 1 }

DFLUNSSwitchEntry ::= 
    SEQUENCE {
        dfSwitchSerialNumber  INTEGER,
        dfSwitchPortID        INTEGER,
        dfSwitchOnOff         INTEGER,
        dfSwitchControlStatus INTEGER
    }

dfSwitchSerialNumber OBJECT-TYPE
                  SYNTAX    INTEGER
                  ACCESS    read-only
                  STATUS    mandatory
                  DESCRIPTION
                             "Serial Number of the DF."
                  ::= { dfLUNSSwitchEntry 1 }

dfSwitchPortID OBJECT-TYPE
                  SYNTAX    INTEGER
                  ACCESS    read-only
                  STATUS    mandatory
                  DESCRIPTION
                             "Port ID of the DF."
                  ::= { dfLUNSSwitchEntry 2 }

dfSwitchOnOff OBJECT-TYPE
                  SYNTAX    INTEGER
                  ACCESS    read-only
                  STATUS    mandatory
                  DESCRIPTION
          "Functional switch.

            Value   Mean
            0   off (LUN Security unused)
            1   on (Use LUN Security)
          "
                  ::= { dfLUNSSwitchEntry 3 }

dfSwitchControlStatus OBJECT-TYPE
                  SYNTAX    INTEGER
                  ACCESS    read-only
                  STATUS    mandatory
                  DESCRIPTION
          "Control flag.

            Value   Mean
            1   Return value.
          "
                  ::= { dfLUNSSwitchEntry 4 }

