Hitachi-DF-RAID-LAN-MIB DEFINITIONS ::= BEGIN

IMPORTS
     enterprises, IpAddress, Counter, Gauge, TimeTicks
          FROM RFC1155-SMI
     DisplayString
          FROM RFC1213-MIB
     OBJECT-TYPE
          FROM RFC-1212 
     TRAP-TYPE
          FROM RFC-1215;


-- Note: Below the 'hitachi' subtree follows Hitachi internal standards.
--       enterprises = iso(1).org(3).dod(6).internet(1).private(4).1

hitachi                OBJECT IDENTIFIER ::= { enterprises 116 }
system                 OBJECT IDENTIFIER ::= { hitachi 3 }
storage                OBJECT IDENTIFIER ::= { system 11 } 
dfraid                 OBJECT IDENTIFIER ::= { storage 1 }
dfraidLan              OBJECT IDENTIFIER ::= { dfraid 2 }
systemExMib            OBJECT IDENTIFIER ::= { hitachi 5 }
storageExMib           OBJECT IDENTIFIER ::= { systemExMib 11 } 
dfraidExMib            OBJECT IDENTIFIER ::= { storageExMib 1 }
dfraidLanExMib           OBJECT IDENTIFIER ::= { dfraidExMib 2 }


-- Note: Below the 'dfraidLanExMib' subtree is controlled by this MIB document.      

dfSystemParameter    OBJECT IDENTIFIER ::= {dfraidLanExMib 1 }
dfWarningCondition    OBJECT IDENTIFIER ::= {dfraidLanExMib 2 }
dfCommandExecutionCondition    OBJECT IDENTIFIER ::= {dfraidLanExMib 3 }
dfCacheLoadCondition    OBJECT IDENTIFIER ::= {dfraidLanExMib 4 }
-- LUN Security information
dfLUNS               OBJECT IDENTIFIER ::= { dfraidLanExMib 5 }
-- Port information
dfPort               OBJECT IDENTIFIER ::= { dfraidLanExMib 6 }
dfCommandExecutionInternalCondition    OBJECT IDENTIFIER ::= {dfraidLanExMib 7 }





dfSystemProductName OBJECT-TYPE
                  SYNTAX    DisplayString (SIZE (0..24))
                  ACCESS    read-only
                  STATUS    mandatory
                  DESCRIPTION
                             "The description of the product name.
                              It is mandatory that this only contain
                              printable ASCII characters."
                  ::= { dfSystemParameter 1 }

dfSystemMicroRevision OBJECT-TYPE
                  SYNTAX    DisplayString (SIZE (0..6))
                  ACCESS    read-only
                  STATUS    mandatory
                  DESCRIPTION
                             "The software revision.
                              It is mandatory that this only contain
                              printable ASCII characters."
                  ::= { dfSystemParameter 2 }

dfSystemSerialNumber OBJECT-TYPE
                  SYNTAX    INTEGER
                  ACCESS    read-only
                  STATUS    mandatory
                  DESCRIPTION
                             "The serial number."
                  ::= { dfSystemParameter 3 }

dfRegressionStatus OBJECT-TYPE
                  SYNTAX    INTEGER
                  ACCESS    read-only
                  STATUS    mandatory
                  DESCRIPTION
                             "A value of regression status.

                              Bit          Comment
                               0           drive status.
                               1           spare drive status.
                               2           data drive status.
                               3           ENC status.
                               4-5         not used,always 0.
                               6           warning status.
                               7           Other controller status.
                               8           UPS status.
                               9           loop status.
                               10          path status.
                               11          NAS Server status.
                               12          NAS Path status.
                               13          NAS UPS status.
                               14-15       not used,always 0.
                               16          battery status.
                               17          power supply status.
                               18          AC status.
                               19          BK status.
                               20          fan status.
                               21-23       not used,always 0.
                               24          cache memory status.
                               25-31       not used,always 0.

                               When the status is normal,each bit value
                               is 0. When the status is abnormal,each bit 
                               value is 1.
                               "
                  ::= { dfWarningCondition 1 }

dfPreventiveMaintenanceInformation OBJECT-TYPE
                  SYNTAX    INTEGER
                  ACCESS    read-only
                  STATUS    mandatory
                  DESCRIPTION
                             "The Preventive Maintenance Information ."
                  ::= { dfWarningCondition 2 }

dfWarningReserve1 OBJECT-TYPE
                  SYNTAX    INTEGER
                  ACCESS    read-only
                  STATUS    mandatory
                  DESCRIPTION
                             "This is not used."
                  ::= { dfWarningCondition 3 }

dfWarningReserve2 OBJECT-TYPE
                  SYNTAX    INTEGER
                  ACCESS    read-only
                  STATUS    mandatory
                  DESCRIPTION
                             "This is not used."
                  ::= { dfWarningCondition 4 }

dfCommandTable OBJECT-TYPE
                  SYNTAX    SEQUENCE OF DfCommandEntry
                  ACCESS    not-accessible
                  STATUS    mandatory
                  DESCRIPTION
                             "The command execution conditon table."
                  ::= { dfCommandExecutionCondition 1 }

dfCommandEntry OBJECT-TYPE
                  SYNTAX    DfCommandEntry
                  ACCESS    not-accessible
                  STATUS    mandatory
                  DESCRIPTION
                             "The command execution conditon table entry."
                  INDEX     { dfLun }
                  ::= { dfCommandTable 1 }

DfCommandEntry ::=
    SEQUENCE {
        dfLun
            INTEGER,
        dfReadCommandNumber
            INTEGER,
        dfReadHitNumber
            INTEGER,
        dfReadHitRate
            INTEGER,
        dfWriteCommandNumber
            INTEGER,
        dfWriteHitNumber
            INTEGER,
        dfWriteHitRate
            INTEGER
    }

dfLun OBJECT-TYPE
                  SYNTAX    INTEGER
                  ACCESS    read-only
                  STATUS    mandatory
                  DESCRIPTION
                             "The logical unit number."
                  ::= { dfCommandEntry 1 }

dfReadCommandNumber OBJECT-TYPE
                  SYNTAX    INTEGER
                  ACCESS    read-only
                  STATUS    mandatory
                  DESCRIPTION
                             "The numbers of received read commands."
                  ::= { dfCommandEntry 2 }

dfReadHitNumber OBJECT-TYPE
                  SYNTAX    INTEGER
                  ACCESS    read-only
                  STATUS    mandatory
                  DESCRIPTION
                             "The numbers of read hit commands."
                  ::= { dfCommandEntry 3 }

dfReadHitRate OBJECT-TYPE
                  SYNTAX    INTEGER
                  ACCESS    read-only
                  STATUS    mandatory
                  DESCRIPTION
                             "The read hit rate."
                  ::= { dfCommandEntry 4 }

dfWriteCommandNumber OBJECT-TYPE
                  SYNTAX    INTEGER
                  ACCESS    read-only
                  STATUS    mandatory
                  DESCRIPTION
                             "The numbers of received write commands."
                  ::= { dfCommandEntry 5 }

dfWriteHitNumber OBJECT-TYPE
                  SYNTAX    INTEGER
                  ACCESS    read-only
                  STATUS    mandatory
                  DESCRIPTION
                             "The numbers of write hit commands."
                  ::= { dfCommandEntry 6 }

dfWriteHitRate OBJECT-TYPE
                  SYNTAX    INTEGER
                  ACCESS    read-only
                  STATUS    mandatory
                  DESCRIPTION
                             "The write hit rate."
                  ::= { dfCommandEntry 7 }

dfWriteDataRate OBJECT-TYPE
                  SYNTAX    INTEGER
                  ACCESS    read-only
                  STATUS    mandatory
                  DESCRIPTION
                             "The write date rate in the cache memory."
                  ::= { dfCacheLoadCondition 1 }

