                  SYNTAX    INTEGER
                  ACCESS    read-only
                  STATUS    mandatory
                  DESCRIPTION
                             "LUN of the Port."
                  ::= { dfLUNSLUNEntry 3 }

dfLUNWWNSecurity OBJECT-TYPE
                  SYNTAX    OCTET STRING (SIZE(0..16))
                  ACCESS    read-only
                  STATUS    mandatory
                  DESCRIPTION
          "WWN security setting.

            The bit at the position corresponding to WWNID shows the 
            state of the access permittee.
            
            Value   Mean
            0   Does not accessible.
            1   Accessible.
          "
                  ::= { dfLUNSLUNEntry 4 }

dfLUNWWNGroupSecurity OBJECT-TYPE
                  SYNTAX    OCTET STRING (SIZE(0..1))
                  ACCESS    read-only
                  STATUS    mandatory
                  DESCRIPTION
          "WWN group Security setting.

            The bit at the position corresponding to WWNID group shows 
            the state of the access permittee.
            
            Value   Mean
            0   Does not accessible.
          "
                  ::= { dfLUNSLUNEntry 5 }

dfLUNControlStatus OBJECT-TYPE
                  SYNTAX    INTEGER
                  ACCESS    read-only
                  STATUS    mandatory
                  DESCRIPTION
          "Control flag.

            Value   Mean
            1   Return value.
          "
                  ::= { dfLUNSLUNEntry 6 }

