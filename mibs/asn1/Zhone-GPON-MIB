			UNITS "seconds"
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"Frequency in seconds for detection 
				background task."
			DEFVAL { 10 }
			::= { zhoneGponOltConfigEntry 44 }

		
		-- 1.3.6.1.4.1.5504.5.14.1.1.1.45
