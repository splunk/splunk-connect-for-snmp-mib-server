    SYNTAX          SEQUENCE OF SonusDs1ProfileEntry
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION     
            "Table for T1/E1 profile"
    ::= { sonusDs1Profile 2 }

sonusDs1ProfileEntry OBJECT-TYPE
    SYNTAX          SonusDs1ProfileEntry
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION     
            "Entry in Table consisting of T1/E1 profile. The T1/E1 profiles
             have to be created by users and then configured. These profiles
             can then be applied onto any T1/E1 span."
    INDEX           { sonusDs1ProfileIndex }
    ::= { sonusDs1ProfileTable 1 }

SonusDs1ProfileEntry ::=
    SEQUENCE {
        sonusDs1ProfileRowStatus        RowStatus,
        sonusDs1ProfileName             SonusName,
        sonusDs1ProfileIndex            Integer32,
        sonusDs1ProfileState            SonusAdminState,
        sonusDs1ProfileDs1LineBO        INTEGER,
        sonusDs1ProfileDs1LineType      INTEGER,
        sonusDs1ProfileDs1LineCoding    INTEGER,
        sonusDs1ProfileDs1SignalMode    INTEGER,
        sonusDs1ProfileDs1Fdl           INTEGER,
        sonusDs1ProfileDs0IdleCode      Integer32,
        sonusDs1ProfileAvailChannels    BITS,
        sonusDs1ProfileDs1LineBOState   SonusAdminState,
        sonusDs1ProfileDs1LineTypeState SonusAdminState,
        sonusDs1ProfileDs1LineCodingState SonusAdminState,
        sonusDs1ProfileDs1SignalModeState SonusAdminState,
        sonusDs1ProfileDs1FdlState      SonusAdminState,
        sonusDs1ProfileDs0IdleCodeState SonusAdminState,
        sonusDs1ProfileAvailChannelsState SonusAdminState,
        sonusDs1ProfileType             INTEGER,
        sonusDs1ProfileESThreshold      Integer32,
        sonusDs1ProfileSESThreshold     Integer32,
        sonusDs1ProfileCSSThreshold     Integer32,
        sonusDs1ProfileDayESThreshold   Integer32,
        sonusDs1ProfileDaySESThreshold  Integer32,
        sonusDs1ProfileDayCSSThreshold  Integer32,
        sonusDs1ProfileFarESThreshold   Integer32,
        sonusDs1ProfileFarSESThreshold  Integer32,
        sonusDs1ProfileFarDayESThreshold Integer32,
        sonusDs1ProfileFarDaySESThreshold Integer32,
        sonusDs1ProfilePCVThreshold     Integer32,
        sonusDs1ProfileLCVThreshold     Integer32,
        sonusDs1ProfileUASThreshold     Integer32,
        sonusDs1ProfileFarPCVThreshold  Integer32,
        sonusDs1ProfileFarLCVThreshold  Integer32,
        sonusDs1ProfileFarUASThreshold  Integer32,
        sonusDs1ProfileDayPCVThreshold  Integer32,
        sonusDs1ProfileDayLCVThreshold  Integer32,
        sonusDs1ProfileDayUASThreshold  Integer32,
        sonusDs1ProfileFarDayPCVThreshold Integer32,
        sonusDs1ProfileFarDayLCVThreshold Integer32,
        sonusDs1ProfileFarDayUASThreshold Integer32,
        sonusDs1ProfileESThresholdState SonusAdminState,
        sonusDs1ProfileSESThresholdState SonusAdminState,
        sonusDs1ProfileCSSThresholdState SonusAdminState,
        sonusDs1ProfileDayESThresholdState  SonusAdminState,
        sonusDs1ProfileDaySESThresholdState   SonusAdminState,
        sonusDs1ProfileDayCSSThresholdState   SonusAdminState,
        sonusDs1ProfileFarESThresholdState  SonusAdminState,
        sonusDs1ProfileFarSESThresholdState   SonusAdminState,
        sonusDs1ProfileFarDayESThresholdState     SonusAdminState,
        sonusDs1ProfileFarDaySESThresholdState      SonusAdminState,
        sonusDs1ProfilePCVThresholdState SonusAdminState,
        sonusDs1ProfileLCVThresholdState SonusAdminState,
        sonusDs1ProfileUASThresholdState SonusAdminState,
        sonusDs1ProfileFarPCVThresholdState   SonusAdminState,
        sonusDs1ProfileFarLCVThresholdState   SonusAdminState,
        sonusDs1ProfileFarUASThresholdState   SonusAdminState,
        sonusDs1ProfileDayPCVThresholdState   SonusAdminState,
        sonusDs1ProfileDayLCVThresholdState   SonusAdminState,
        sonusDs1ProfileDayUASThresholdState   SonusAdminState,
        sonusDs1ProfileFarDayPCVThresholdState      SonusAdminState,
        sonusDs1ProfileFarDayLCVThresholdState      SonusAdminState,
        sonusDs1ProfileFarDayUASThresholdState      SonusAdminState
    }

