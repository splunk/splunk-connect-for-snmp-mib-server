sonusDsx1MIBNotifications OBJECT IDENTIFIER
    ::= { sonusDsx1MIB 2 }

sonusDsx1MIBNotificationsPrefix OBJECT IDENTIFIER
    ::= { sonusDsx1MIBNotifications 0 }

sonusDsx1MIBNotificationsObjects OBJECT IDENTIFIER
    ::= { sonusDsx1MIBNotifications 1 }

sonusDs1OutOfServiceReason OBJECT-TYPE
    SYNTAX          INTEGER { maintenance(1) }
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "The reason for T1/E1 span going out of service"
    ::= { sonusDsx1MIBNotificationsObjects 1 }

sonusDs1OutOfServiceType OBJECT-TYPE
    SYNTAX          INTEGER { admin(1), hwFailure(2) }
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "The type of operation which caused T1/E1 span to go out of service"
    ::= { sonusDsx1MIBNotificationsObjects 2 }

sonusDs1ThresholdType OBJECT-TYPE
    SYNTAX          INTEGER { currentLineCodeViolation(1), 
                        currentPathCodeViolation(2), 
                        currentPathCodeViolationFarEnd(3), 
                        currentControlledSlips(4), 
                        currentErroredSeconds(5), 
                        currentSeverelyErrSeconds(6), 
                        currentUnavailableSeconds(7), 
                        totalLineCodeViolation(8), 
                        totalPathCodeViolation(9), 
                        totalPathCodeViolationFarEnd(10), 
                        totalControlledSlips(11), 
                        totalErroredSeconds(12), 
                        totalSeverelyErrSeconds(13), 
                        totalUnavailableSeconds(14), 
                        currentFarEndErroredSeconds(15), 
                        currentFarEndSeverelyErrSeconds(16), 
                        totalFarEndErroredSeconds(17), 
                        totalFarEndCurrentSeverelyErrSeconds(18), 
                        currentFarEndUnavailableSeconds(19), 
                        totalFarEndUnavailableSeconds(20) }
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "Type of threshold that was crossed which caused a threshold crossing trap"
    ::= { sonusDsx1MIBNotificationsObjects 3 }

sonusDs1LineState OBJECT-TYPE
    SYNTAX          INTEGER { green(1), red(2), yellow(3), ais(4), 
                        hwfailure(5) }
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "The type of Alarm or failure which caused T1/E1 span to go out of service"
    ::= { sonusDsx1MIBNotificationsObjects 4 }

sonusDs1AdmnState OBJECT-TYPE
    SYNTAX          INTEGER { disabled(1), enabled(2), deleted(3), 
                        inservice(4), dryingUp(5) }
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "The administrative state of the T1/E1 span."
    ::= { sonusDsx1MIBNotificationsObjects 5 }

sonusDsx1AdminChangeNotification NOTIFICATION-TYPE
    OBJECTS         { sonusShelfIndex, sonusSlotIndex, sonusPortIndex, 
                      sonusDs3Index, sonusDs1Index, sonusDs1AdmnState, 
                      sonusEventDescription, sonusEventClass, 
                      sonusEventLevel }
    STATUS          current
    DESCRIPTION     
            "A sonusDsx1AdminChangeNotification trap indicates the DS1 admin changes. 
             It is used to notify out-of-service, in-service, enabled and
             	 disabled"
    ::= { sonusDsx1MIBNotificationsPrefix 1 }

sonusDsx1OutOfServiceNotification NOTIFICATION-TYPE
    OBJECTS         { sonusShelfIndex, sonusSlotIndex, sonusPortIndex, 
                      sonusDs3Index, sonusDs1Index, 
                      sonusDs1OutOfServiceReason, 
                      sonusDs1OutOfServiceType, sonusEventDescription, 
                      sonusEventClass, sonusEventLevel }
    STATUS          current
    DESCRIPTION     
            "A sonusDsx1OutOfServiceNotification trap indicates that a T1/E1 span
             went out of service. The reason indicates the reason why and
             type indicates the type of failure or alarm."
    ::= { sonusDsx1MIBNotificationsPrefix 2 }

sonusDsx1ThresholdCrossingNotification NOTIFICATION-TYPE
    OBJECTS         { sonusShelfIndex, sonusSlotIndex, sonusPortIndex, 
                      sonusDs3Index, sonusDs1Index, 
                      sonusDs1ThresholdType, sonusEventDescription, 
                      sonusEventClass, sonusEventLevel }
    STATUS          current
    DESCRIPTION     
            "A sonusDsx1ThresholdCrossingNotification trap indicates that a T1/E1 span
             threshold has been crossed. The type indicates the type of threshold."
    ::= { sonusDsx1MIBNotificationsPrefix 3 }

sonusDsx1LineStatusChangeNotification NOTIFICATION-TYPE
    OBJECTS         { sonusShelfIndex, sonusSlotIndex, sonusPortIndex, 
                      sonusDs3Index, sonusDs1Index, sonusDs1LineState, 
                      sonusEventDescription, sonusEventClass, 
                      sonusEventLevel }
    STATUS          current
    DESCRIPTION     
            "A sonusDsx1LineStatusChangeNotification trap indicates the T1/E1 span state changes. 
             It is used to notify T1/E1 span Alarms states"
    ::= { sonusDsx1MIBNotificationsPrefix 4 }

END -- end of module SONUS-DS1-MIB.
