ciscoVismConnStatGroup3 OBJECT-GROUP
          OBJECTS {
                vismCntChanNum,
                vismCntClrButton,
                vismChanAal2HecErrors,
                vismChanAal2CrcErrors,
                vismChanAal2OamLpbLostCells,
