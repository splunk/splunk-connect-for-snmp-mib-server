    OBJECTS  {
        vmstoreHostname,
        trapSeverity,
        trapMessageId,
        trapMessage }
    STATUS current
    DESCRIPTION
          "Disabling background file reclamation due to an internal error. Filesystem space utilization will increase. Please contact support."
    ::= { vmStoreTraps 251 }


fsBackgroundSnapshotReclaimStopped1 NOTIFICATION-TYPE
    OBJECTS  {
        vmstoreHostname,
        trapSeverity,
        trapMessageId,
        trapMessage }
    STATUS current
    DESCRIPTION
          "Disabling background snapshot reclamation due to an internal error. Filesystem space utilization will increase. Please contact support."
    ::= { vmStoreTraps 252 }


fsBackgroundSnapshotReclaimStopped2 NOTIFICATION-TYPE
    OBJECTS  {
        vmstoreHostname,
        trapSeverity,
        trapMessageId,
        trapMessage }
    STATUS current
    DESCRIPTION
          "An error is detected in snapshot deletion of a file. Disabling background snapshot reclamation for all snapshots of this file. Filesystem space utilization will increase. Please contact support."
    ::= { vmStoreTraps 253 }


rbacAdsAccountChangeKeyFailed NOTIFICATION-TYPE
    OBJECTS  {
        vmstoreHostname,
        trapSeverity,
        trapMessageId,
        trapMessage }
    STATUS current
    DESCRIPTION
          "Failed to change ADS machine trust account key. Please contact support if data and management access relying on active directory is interrupted."
    ::= { vmStoreTraps 254 }


remoteMgmtAdminPasswordResetRequired2 NOTIFICATION-TYPE
    OBJECTS  {
        vmstoreHostname,
        trapSeverity,
        trapMessageId,
        trapMessage }
    STATUS current
    DESCRIPTION
