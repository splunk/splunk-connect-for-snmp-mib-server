vmStore OBJECT-IDENTITY
STATUS  current
DESCRIPTION
    "Parent of all managed objects relating to VMStore identification."
::= { tintri 1 }

vmStoreTraps OBJECT-IDENTITY
    STATUS  current
    DESCRIPTION
    "..."
    ::= { vmStore 1 }

vmStorePerfStats OBJECT-IDENTITY
    STATUS  current
    DESCRIPTION
    "..."
    ::= { vmStore 2 }

vmStoreMIBConformance OBJECT IDENTIFIER ::= { vmStore 100 }
    

-- **********************************************************
-- Notification parameters
-- **********************************************************

vmstoreHostname OBJECT-TYPE
    SYNTAX     DisplayString
    MAX-ACCESS accessible-for-notify
    STATUS     current
    DESCRIPTION
    "This is the fully qualified name of the VMStore that generates the trap."
    ::= { vmStoreTraps 1 }

trapSeverity OBJECT-TYPE
    SYNTAX   Integer32
    MAX-ACCESS accessible-for-notify
    STATUS     current
    DESCRIPTION
    "This is the trap severity level."
    ::= { vmStoreTraps 2 }

trapMessageId OBJECT-TYPE
    SYNTAX     DisplayString
    MAX-ACCESS accessible-for-notify
    STATUS     current
    DESCRIPTION
        "This is the alert message ID."
    ::= { vmStoreTraps 3 }  

trapMessage OBJECT-TYPE
    SYNTAX     DisplayString
    MAX-ACCESS accessible-for-notify
    STATUS     current
    DESCRIPTION
        "This field provides the notification information."
    ::= { vmStoreTraps 4 }


-- **********************************************************
-- Notifications
-- **********************************************************


diskRAIDFailed NOTIFICATION-TYPE
    OBJECTS  {
        vmstoreHostname,
        trapSeverity,
        trapMessageId,
        trapMessage }
    STATUS current
    DESCRIPTION
          "Failed to form ExtentStore RAID. Contact support."
    ::= { vmStoreTraps 101 }

