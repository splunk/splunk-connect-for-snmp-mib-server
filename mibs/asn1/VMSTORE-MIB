-- **********************************************************
-- The vmstore performance statistics summary table
-- **********************************************************

vmStorePerfStatsSummaryTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF VMStorePerfStatsSummaryEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "This table provides the VMstore performance statistics summary for various resources."
    ::= { vmStorePerfStats 1 }

vmStorePerfStatsSummaryEntry OBJECT-TYPE
    SYNTAX      VMStorePerfStatsSummaryEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
            "A conceptual row in the vmStorePerfStatsSummaryTable."
    INDEX { vmStorePerfStatsSummaryIndex }
    ::= { vmStorePerfStatsSummaryTable 1 }

VMStorePerfStatsSummaryEntry ::= SEQUENCE {
    vmStorePerfStatsSummaryIndex    Integer32,
    perfStatsCollectionDate         DateAndTime,
    uptime                          DisplayString,
    model                           DisplayString,
    swVersion                       DisplayString,
    serialNumber                    DisplayString,
    vmStoreStatus                   INTEGER,
    numUnreadAlerts                 Unsigned32,
    manualAssignedPerfResource      Unsigned32,
    autoAssignedPerfResource        Unsigned32,
    orphanedSpace                   Unsigned32,
    tintriSnapshotSpace             Unsigned32,
    vmwSnapshotSpace                Unsigned32,
    liveDataSpace                   Unsigned32,
    remainingSpace                  Unsigned32,
    provisionedPercent              Unsigned32,
    daysTillFull                    Unsigned32,
    throughput                      Unsigned32,
    lowThroughput                   Unsigned32,
    highThroughput                  Unsigned32,
    iops                            Unsigned32,
    lowIops                         Unsigned32,
    highIops                        Unsigned32,
    latency                         Unsigned32,
    hostLatency                     Unsigned32,
    networkLatency                  Unsigned32,
    fsLatency                       Unsigned32,
    diskLatency                     Unsigned32,
    cacheHitRatio                   Unsigned32,
    replLinkCount                   Unsigned32,
    replOutgoingCount               Unsigned32,
    replIncomingCount               Unsigned32,
    replBytesRemaining              Unsigned32,
    replSrcLogicalThroughput        Unsigned32,
    replSrcPhysicalThroughput       Unsigned32,
    replDstLogicalThroughput        Unsigned32,
    replDstPhysicalThroughput       Unsigned32,
    replSrcLogicalReplicatedPerDay  Unsigned32,
    replDstLogicalReplicatedPerDay  Unsigned32,
    replSrcPhysicalReplicatedPerDay Unsigned32,
    replDstPhysicalReplicatedPerDay Unsigned32,
    mirrorLatency                   Unsigned32,
    mirrorReadLatency               Unsigned32,
    mirrorWriteLatency              Unsigned32,
    mirrorWriteNetworkLatency       Unsigned32
}


vmStorePerfStatsSummaryIndex OBJECT-TYPE
    SYNTAX      Integer32 (1..65535)
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
            "An arbitrary integer value which uniquely identifies a
            row in this table.  "
    ::= { vmStorePerfStatsSummaryEntry 1 }

