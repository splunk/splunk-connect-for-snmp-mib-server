			STATUS current
			DESCRIPTION 
				"Description."
			::= { sheAlarmingGroups 2 }

		
		sheAlarmHistoryGroup OBJECT-GROUP
			OBJECTS { sheAlarmHistoryEnabled, sheAlarmHistoryEntryNumber, sheAlarmHistorySeverityLevel, sheAlarmHistoryControlRequest, sheAlarmHistoryControlStatus, 
				sheAlarmHistoryControlTimer, sheAlarmHistoryControlTimeStamp, sheAlarmHistoryControlReqResult, sheAlarmHistoryControlEnabled, sheAlarmHistoryControlEntryNumber, 
				sheAlarmHistoryControlSeverityLevel, sheAlarmHistoryIndex, sheAlarmHistoryArisalTimeAndDate, sheAlarmHistoryClearingTimeAndDate, sheAlarmHistoryAlarmID, 
				sheAlarmHistoryReportingSource, sheAlarmHistoryReportingSourceType, sheAlarmHistorySeverity, sheAlarmHistoryLatestOverwriteTimeAndDate }
			STATUS current
			DESCRIPTION 
				"Description."
			::= { sheAlarmingGroups 3 }

		
		sheAlarmHistoryNotificationGroup NOTIFICATION-GROUP
			NOTIFICATIONS { sheAlarmHistoryEnabledChanged, sheAlarmHistoryBaseChanged, sheAlarmHistoryCleared }
			STATUS current
			DESCRIPTION 
				"Description."
			::= { sheAlarmingGroups 4 }

		
	
	END

