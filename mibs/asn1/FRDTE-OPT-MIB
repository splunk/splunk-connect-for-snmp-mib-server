          -- Version 2.15.1.1, modified 2001-05-31 17:27:56-04 

          FRDTE-OPT-MIB DEFINITIONS ::= BEGIN

          IMPORTS
                  mgmt, NetworkAddress, IpAddress, Counter, Gauge,
                          TimeTicks, enterprises
                      FROM RFC1155-SMI
                  OBJECT-TYPE
                      FROM RFC-1212;

               -- Codex MIB Group definitions

                         codex OBJECT IDENTIFIER ::= { enterprises 449 }
            cdxProductSpecific OBJECT IDENTIFIER ::= { codex 2 }
                       cdx6500 OBJECT IDENTIFIER ::= { cdxProductSpecific 1 }

         cdx6500Configuration  OBJECT IDENTIFIER ::= { cdx6500 2 }
       cdx6500CfgProtocolGroup OBJECT IDENTIFIER ::= { cdx6500Configuration 1 }
   cdx6500PCTPortProtocolGroup OBJECT IDENTIFIER ::= { cdx6500CfgProtocolGroup 1 }
cdx6500PCTStationProtocolGroup OBJECT IDENTIFIER ::= { cdx6500CfgProtocolGroup 3 }

             cdx6500Statistics OBJECT IDENTIFIER ::= { cdx6500 3 }
      cdx6500StatProtocolGroup OBJECT IDENTIFIER ::= { cdx6500Statistics 1 }
   cdx6500PSTPortProtocolGroup OBJECT IDENTIFIER ::= { cdx6500StatProtocolGroup 1 }
cdx6500PSTStationProtocolGroup OBJECT IDENTIFIER ::= { cdx6500StatProtocolGroup 3 }

               cdx6500Controls OBJECT IDENTIFIER ::= { cdx6500 4 }

          -- Textual conventions
          Counter16 ::= INTEGER (0..65535)

          DisplayString ::= OCTET STRING
          -- This data type is used to model textual information taken
          -- from the NVT ASCII character set.  By convention, objects
          -- with this syntax are declared as having SIZE (0..255)

          -- The Frame Relay DTE Port Configuration Table
          -- This table contains Configuration paramaters 
          -- for the Frame Relay DTE Port table.

        cdx6500PCTFRDTEPortTable OBJECT-TYPE
            SYNTAX SEQUENCE OF Cdx6500PCTFRDTEPortEntry
            ACCESS not-accessible
            STATUS mandatory
            DESCRIPTION
                "All of the configuration parameters relevant
                 to the Frame Relay DTE Port table."
        ::= { cdx6500PCTPortProtocolGroup 5 }

        cdx6500PCTFRDTEPortEntry OBJECT-TYPE
            SYNTAX Cdx6500PCTFRDTEPortEntry
            ACCESS not-accessible
            STATUS mandatory
            DESCRIPTION
                "A row of FRI Port configuration parameters."
            INDEX { cdx6500frdtepCfgPortNum }
        ::= { cdx6500PCTFRDTEPortTable 1 }

        Cdx6500PCTFRDTEPortEntry ::=
            SEQUENCE {
              cdx6500frdtepCfgPortNum
                  INTEGER,
              cdx6500frdtepConnectionType
                  INTEGER,
              cdx6500frdtepClockSource
                  INTEGER,
              cdx6500frdtepClockSpeed
                  INTEGER,
              cdx6500frdtepMaxStations
                  INTEGER,
              cdx6500frdtepFrameSeqCounting
                  INTEGER,
              cdx6500frdtepPktSeqCounting
                  INTEGER,
              cdx6500frdtepCtrlProtocol
                  INTEGER,
              cdx6500frdtepT391
                  INTEGER,
              cdx6500frdtepT392
                  INTEGER,
              cdx6500frdtepN391
                  INTEGER,
              cdx6500frdtepN392
                  INTEGER,
              cdx6500frdtepN393
                  INTEGER,
              cdx6500frdtepNT1
                  INTEGER,
              cdx6500frdtepNT2
                  INTEGER,
              cdx6500frdtepNN1
                  INTEGER,
              cdx6500frdtepNN2
                  INTEGER,
              cdx6500frdtepNN3
                  INTEGER,
              cdx6500frdtepHighPriorityStn
                    INTEGER,
              cdx6500frdtepMaxVoiceBWBitsPerSec
                    INTEGER,
              cdx6500frdtepSegSizeVoicePresent
                    INTEGER,
              cdx6500frdtepSegSizeVoiceNotPresent
                    INTEGER,
              cdx6500frdtepInvertTXClock
                  INTEGER,
              cdx6500frdtepControlProtocolOptions
                  DisplayString,
              cdx6500frdtepDiscardControlOptions
                  INTEGER,
              cdx6500frdtepElectricalInterfaceType
                  INTEGER,
              cdx6500frdtepV24ElectricalInterfaceOption
                  INTEGER,
              cdx6500frdtepHighSpeedElectricalInterfaceOption
                  INTEGER
              }

        cdx6500frdtepCfgPortNum OBJECT-TYPE
            SYNTAX INTEGER (1..255)
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
                "Specifies the Port number for the Frame Relay 
                 Interface port you are selecting."
        ::= { cdx6500PCTFRDTEPortEntry 1 }

        cdx6500frdtepConnectionType OBJECT-TYPE
            SYNTAX INTEGER {
                    simp  (1),
                    dtr   (2),
                    simpb (21),
                    nc    (100)
                    }
            ACCESS read-write
            STATUS mandatory
            DESCRIPTION
            "Specify the control signal handshake and clocking required
             for a connection to be made to this port:
             simp  - simple, no control signals required
             dtr   - dedicated, require the data terminal ready signal (DTR)
             simpb - simple, no control signals required for data
                     transmission and reception. By default output
                     leads will be low.
             nc    - Skipped during configuration."
        ::= { cdx6500PCTFRDTEPortEntry 2 }

        cdx6500frdtepClockSource OBJECT-TYPE
            SYNTAX INTEGER {
                    int    (1),
                    ext    (2),
                    extint (3),
                    extlp  (4),
                    nc     (100)
                    }
            ACCESS read-write
            STATUS mandatory
            DESCRIPTION
                "int     - internal clock source
                 ext     - external clock source
                 extint  - Internal receive and external transmit
                           clock source (DCE only).
                 extlp   - external receive and loopback transmit
                           clock source (DTE only).
                           EXTLP must be configured in conjunction
                           with EXTINT.
                 nc      - Skipped during configuration."
        ::= { cdx6500PCTFRDTEPortEntry 3 }

        cdx6500frdtepClockSpeed OBJECT-TYPE
            SYNTAX INTEGER (1200..2048000)
            ACCESS read-write
            STATUS mandatory
            DESCRIPTION
                "This is the speed of the port in bits per second, 
                 when using internal clocking.  The highest available 
                 speed is dependent on the card type,and on the interface type."
        ::= { cdx6500PCTFRDTEPortEntry 4 }

        cdx6500frdtepMaxStations OBJECT-TYPE
            SYNTAX INTEGER
            ACCESS read-only
            STATUS deprecated
            DESCRIPTION
                "This is the highest station number to be configured on this 
                 FRI port.
                 It is used to limit the configuration of stations."
        ::= { cdx6500PCTFRDTEPortEntry 5 }

        cdx6500frdtepFrameSeqCounting OBJECT-TYPE
            SYNTAX INTEGER {
                    normal   (1),
                    extended (2),
                    nc       (100)
                    }
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
                "Port uses normal or extended frame sequence numbering:

                 normal - normal sequencing  (modulo 8)
                 ext    - extended sequencing (modulo 128)
                 nc     - Skipped during configuration.
                 All stations on this port will be set to this value."
        ::= { cdx6500PCTFRDTEPortEntry 6 }

        cdx6500frdtepPktSeqCounting OBJECT-TYPE
            SYNTAX INTEGER {
                    normal   (1),
                    extended (2),
                    nc       (100)
                    }
            ACCESS read-write
            STATUS mandatory
            DESCRIPTION
             "Port uses normal or extended packet level sequence numbers
             normal - normal sequencing (Modulo 8)
             ext    - extended sequencing (Modulo 128)
             nc     - Skipped during configuration.
             This parameter must be set to the same value 
             at both this FRI port and the remote FRI port.
             All stations on this port will be set to this value."
        ::= { cdx6500PCTFRDTEPortEntry 7 }

        cdx6500frdtepCtrlProtocol OBJECT-TYPE
            SYNTAX INTEGER {
                    annexD (1),
                    none   (2),
                    lmi    (3),
                    annexA (4),
                    auto   (5),
                    nc     (100)
                    }
            ACCESS read-write
            STATUS mandatory
            DESCRIPTION
                "Whether ANSI T1.617 Annex D or LMI Support is enabled
                 none   - None is enabled.
                 annexD - Annex D is enabled
                 lmi    - LMI is enabled.
                 annexA - Annex A is enabled.
                 auto   - Auto-learn the Network control protocol type
                 nc     - Skipped during configuration."
        ::= { cdx6500PCTFRDTEPortEntry 8 }

        cdx6500frdtepT391 OBJECT-TYPE
            SYNTAX INTEGER (5..30)
            ACCESS read-write
            STATUS mandatory
            DESCRIPTION
                "This parameter is the Link Integrity Verification
                 Timer polling period. The FRI port sends status enquiry
                 messages to network every T391/nT1 seconds."
--            DEFVAL { 10 }
        ::= { cdx6500PCTFRDTEPortEntry 9 }

        cdx6500frdtepT392 OBJECT-TYPE
            SYNTAX INTEGER (5..30)
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
                "This parameter is the polling Cycle Verification
                 Timer period. The FRI port expects status enquiry messages
                 every T392/nT2 seconds. This parameter must be set to a
                 value greater than N391/nT1, and applies only in
                 bi-directional mode."
--            DEFVAL { 15 }
        ::= { cdx6500PCTFRDTEPortEntry 10 }

        cdx6500frdtepN391 OBJECT-TYPE
            SYNTAX INTEGER
            ACCESS read-write
            STATUS mandatory
            DESCRIPTION
                "Full Status polling cycle.  FRI port sends Full status
                 enquiry messages to network every nN1 polls."
--            DEFVAL { 5 }
        ::= { cdx6500PCTFRDTEPortEntry 12 }

        cdx6500frdtepN392 OBJECT-TYPE
            SYNTAX INTEGER
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
                "Error inactivity threshold.  Number of errors during NN3
                 events that cause the station to be declared inactive.
                 Should be less than or equal to nN3."
--            DEFVAL { 5 }
        ::= { cdx6500PCTFRDTEPortEntry 13 }

        cdx6500frdtepN393 OBJECT-TYPE
            SYNTAX INTEGER
            ACCESS read-write
            STATUS mandatory
            DESCRIPTION
                "Monitored events count for measuring nN3. nN2 errors during
                 nN3 events will cause the station to be declared inactive.
                 Should be greater than nN3."
--            DEFVAL { 4 }
        ::= { cdx6500PCTFRDTEPortEntry 14 }

        cdx6500frdtepNT1 OBJECT-TYPE
            SYNTAX INTEGER (5..30)
            ACCESS read-write
            STATUS mandatory
            DESCRIPTION
                "This parameter is the Link Integrity Verification
                 Timer polling period. The FRI port sends status enquiry
                 messages to network every T391/nT1 seconds."
--            DEFVAL { 10 }
        ::= { cdx6500PCTFRDTEPortEntry 15 }

        cdx6500frdtepNT2 OBJECT-TYPE
            SYNTAX INTEGER (5..30)
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
                "This parameter is the polling Cycle Verification
                 Timer period. The FRI port expects status enquiry messages
                 every T392/nT2 seconds. This parameter must be set to a
                 value greater than N391/nT1, and applies only in
                 bi-directional mode."
--            DEFVAL { 15 }
        ::= { cdx6500PCTFRDTEPortEntry 16 }

        cdx6500frdtepNN1 OBJECT-TYPE
            SYNTAX INTEGER
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
                "Full Status polling cycle.  FRI port sends Full status
                 enquiry messages to network every nN1 polls."
--            DEFVAL { 5 }
        ::= { cdx6500PCTFRDTEPortEntry 17 }

        cdx6500frdtepNN2 OBJECT-TYPE
            SYNTAX INTEGER
            ACCESS read-write
            STATUS mandatory
            DESCRIPTION
                "Error inactivity threshold.  Number of errors during NN3
                 events that cause the station to be declared inactive.
                 Should be less than or equal to nN3."
--            DEFVAL { 3 }
        ::= { cdx6500PCTFRDTEPortEntry 18 }

        cdx6500frdtepNN3 OBJECT-TYPE
            SYNTAX INTEGER
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
                "Monitored events count for measuring nN3. nN2 errors during
                 nN3 events will cause the station to be declared inactive.
                 Should be greater than nN3."
--            DEFVAL { 4 }
        ::= { cdx6500PCTFRDTEPortEntry 19 }


        cdx6500frdtepHighPriorityStn OBJECT-TYPE
            SYNTAX INTEGER (1..254)
            ACCESS read-write
            STATUS mandatory
            DESCRIPTION
                "This parameter specifies a station who's PVC status has
                 priority over all other stations when FRI same port back
                 up is enabled. When the network reports this DLCI inactive,
                 an alternate connection over a backup link is attempted."
        ::= { cdx6500PCTFRDTEPortEntry 20 }

        cdx6500frdtepMaxVoiceBWBitsPerSec OBJECT-TYPE
            SYNTAX INTEGER (0..2048000)
            ACCESS read-write
            STATUS mandatory
            DESCRIPTION
                "This parameter limits the bandwidth, in bits per sec
                 (including overhead), for voice traffic passing through this
                 frame relay port. A zero value disables any voice passing
                 through this port."
--            DEFVAL { 2048000 }
        ::= { cdx6500PCTFRDTEPortEntry 21 }

        cdx6500frdtepSegSizeVoicePresent OBJECT-TYPE
            SYNTAX INTEGER {
                   segSize32   (33),
                   segSize64   (65),
                   segSize128  (129),
                   segSize256  (257),
                   segSize512  (513),
                   segSize1024 (1025),
                   nc          (100)
                   }
            ACCESS read-write
            STATUS mandatory
            DESCRIPTION
                "Data packets to be segmented into this maximum frame size when
                 voice is active on this port.
                 segSize32   - 32   byte segments
                 segSize64   - 64   byte segments
                 segSize128  - 128  byte segments
                 segSize256  - 256  byte segments
                 segSize512  - 512  byte segments
                 segSize1024 - 1024 byte segments
                 nc          - Skipped during configuration."
--            DEFVAL { segSize64 }
        ::= { cdx6500PCTFRDTEPortEntry 22 }

        cdx6500frdtepSegSizeVoiceNotPresent  OBJECT-TYPE
            SYNTAX INTEGER {
                   segSize32   (33),
                   segSize64   (65),
                   segSize128  (129),
                   segSize256  (257),
                   segSize512  (513),
                   segSize1024 (1025),
                   segSize2048 (2049),
                   segSize4096 (4097),
                   disable     (32000),
                   nc          (100)
                   }
            ACCESS read-write
            STATUS mandatory
            DESCRIPTION
                "Data packets to be segmented into this maximum frame size when
                 voice is not active on this port.
                 segSize32   - 32   byte segments
                 segSize64   - 64   byte segments
                 segSize128  - 128  byte segments
                 segSize256  - 256  byte segments
                 segSize512  - 512  byte segments
                 segSize1024 - 1024 byte segments
                 segSize2048 - 2048 byte segments
                 segSize4096 - 4096 byte segments
                 disable     - segment size is 32768 bytes
                 nc          - Skipped during configuration."
--            DEFVAL { disable }
        ::= { cdx6500PCTFRDTEPortEntry 23 }

        cdx6500frdtepInvertTXClock OBJECT-TYPE
            SYNTAX INTEGER {
                   no  (1),
                   yes (2),
                   nc (100)
                   }
            ACCESS read-write
            STATUS mandatory
            DESCRIPTION
                "This parameter specifies whether the phase of the transmit
                 clock should be inverted. Note: This parameter is primarily
                 for X.21 electrical interfaces.
                   no - Don't invert.
                   yes - Invert."
--            DEFVAL { no }
        ::= { cdx6500PCTFRDTEPortEntry 24 }

        cdx6500frdtepControlProtocolOptions OBJECT-TYPE
            SYNTAX DisplayString (SIZE(0..16))
            ACCESS read-write
            STATUS mandatory
            DESCRIPTION
                "These options control the PVC management protocol
                   none  - No option selected
                   async - The port will send/receive asynchronous
                           A-bit if it is performing Network side
                           protocol functionality
                   nbit  - The port will send and accept N-bit messages
                   dbit  - The port will send and accept D-bit messages
                           This forces ASYNC to be specified (dbit+async)"
        ::= { cdx6500PCTFRDTEPortEntry 25 }

        cdx6500frdtepDiscardControlOptions OBJECT-TYPE
            SYNTAX INTEGER {
                   none  (1),
                   debit (2)
                   }
            ACCESS read-write
            STATUS mandatory
            DESCRIPTION
                "This parameter controls the discard frame options
                   none - No option selected
                   debit - DE marked frames are discarded before
                           non-DE frames"
--            DEFVAL { none }
        ::= { cdx6500PCTFRDTEPortEntry 26 }

        cdx6500frdtepElectricalInterfaceType OBJECT-TYPE
            SYNTAX INTEGER {
                    v24       (1),
                    v35       (2),
                    v36       (3),
                    x21       (4),
                    none      (5)
                    }
            ACCESS read-write
            STATUS mandatory
            DESCRIPTION
                 "Specify the Electrical Interface Type:
                   V.24  - V.24 Electrical Interface Type
                   V.35  - V.35 Electrical Interface Type
                   V.36  - V.36 Electrical Interface Type
                   X.21  - X.21 Electrical Interface Type
                   NONE  - Electrically disabled"
        ::= { cdx6500PCTFRDTEPortEntry 27 }

        cdx6500frdtepV24ElectricalInterfaceOption OBJECT-TYPE
            SYNTAX INTEGER {
                    ri        (1),
                    tm        (2)
                    }
            ACCESS read-write
            STATUS mandatory
            DESCRIPTION
                 "Specify the Pin 22 option:
                  RI    - V.24 uses Pin 22 for Ring Indicator output signal
                  TM    - V.24 uses Pin 22 for Test Mode input signal"
        ::= { cdx6500PCTFRDTEPortEntry 28 }

        cdx6500frdtepHighSpeedElectricalInterfaceOption OBJECT-TYPE
            SYNTAX INTEGER {
                    none      (1),
                    xover     (2)
                    }
            ACCESS read-write
            STATUS mandatory
            DESCRIPTION
                 "Specify the cable type:
                  NONE  - V.35/V.36/X.21 DCE with straight through cable
                  XOVER - V.35/V.36/X.21 DCE with crossover adapter cable"
        ::= { cdx6500PCTFRDTEPortEntry 29 }

        -- The Frame Relay DTE Port Statistics Table
        -- This table contains Statistics paramaters 
        -- for the Frame Relay DTE Port table.

        cdx6500PPSTFRDTEPortTable OBJECT-TYPE
            SYNTAX SEQUENCE OF Cdx6500PPSTFRDTEPortEntry
            ACCESS not-accessible
            STATUS mandatory
            DESCRIPTION
                "All of the statistics parameters relevant
                 to the Frame Relay DTE Port table."
        ::= { cdx6500PSTPortProtocolGroup 5 }

        cdx6500PPSTFRDTEPortEntry OBJECT-TYPE
            SYNTAX Cdx6500PPSTFRDTEPortEntry
            ACCESS not-accessible
            STATUS mandatory
            DESCRIPTION
                "A row of Frame Relay DTE Port statistics parameters."
            INDEX { cdx6500frdtepStatsPortNum }
        ::= { cdx6500PPSTFRDTEPortTable 1 }

        Cdx6500PPSTFRDTEPortEntry ::=
            SEQUENCE {
              cdx6500frdtepStatsPortNum
                  INTEGER,
              cdx6500frdtepPortStatus
                  INTEGER,
              cdx6500frdtepPortSpeed
                  INTEGER,
              cdx6500frdtepUtilizationIn
                  INTEGER,
              cdx6500frdtepUtilizationOut
                  INTEGER,
              cdx6500frdtepCharInTotal
                  Counter,
              cdx6500frdtepCharOutTotal
                  Counter,
              cdx6500frdtepCharsInPerSec
                  INTEGER,
              cdx6500frdtepCharsOutPerSec
                  INTEGER,
              cdx6500frdtepFrameInTotal
                  Counter,
              cdx6500frdtepFrameOutTotal
                  Counter,
              cdx6500frdtepFramesInPerSec
                  INTEGER,
              cdx6500frdtepFramesOutPerSec
                  INTEGER,
              cdx6500frdtepOverrunErrors
                  Counter16,
              cdx6500frdtepUnderrunErrors
                  Counter16,
              cdx6500frdtepCRCErrors
                  Counter16
              }

        cdx6500frdtepStatsPortNum OBJECT-TYPE
            SYNTAX INTEGER (1..255)
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
                "Specifies a specific port in the network diagram."
        ::= { cdx6500PPSTFRDTEPortEntry 1 }

        cdx6500frdtepPortStatus OBJECT-TYPE
            SYNTAX INTEGER {
                    disabled (1),
                    enabled  (2),
                    busyOut  (3),
                    up       (4),
                    down     (5),
                    na       (100)
                    }
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
                "Specifies the current port status.
                 disabled - Port has been disabled.
                 enabled  - Port is operational.
                 busyOut  - Port has been busied out.
                 up       - Port is operational.
                 down     - Port is down.
                 na       - Value not Available."
        ::= { cdx6500PPSTFRDTEPortEntry 2 }

        cdx6500frdtepPortSpeed OBJECT-TYPE
            SYNTAX INTEGER
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
                "Message transmition speed in bps."
        ::= { cdx6500PPSTFRDTEPortEntry 3 }

        cdx6500frdtepUtilizationIn OBJECT-TYPE
            SYNTAX INTEGER (0..100)
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
                "Factor determined by comparison of volume (characters per 
                 second) of data and clock rate for a specific port."
        ::= { cdx6500PPSTFRDTEPortEntry 4 }

        cdx6500frdtepUtilizationOut OBJECT-TYPE
            SYNTAX INTEGER (0..100)
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
                "Factor determined by comparison of volume (characters per 
                 second) of data and clock rate for a specific port."
        ::= { cdx6500PPSTFRDTEPortEntry 5 }

        cdx6500frdtepCharInTotal OBJECT-TYPE
            SYNTAX Counter
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
                "Number of characters received since last boot or statistics 
                 reset."
        ::= { cdx6500PPSTFRDTEPortEntry 6 }

        cdx6500frdtepCharOutTotal OBJECT-TYPE
            SYNTAX Counter
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
                "Number of characters transmitted since last boot or statistics
                 reset."
        ::= { cdx6500PPSTFRDTEPortEntry 7 }

        cdx6500frdtepCharsInPerSec OBJECT-TYPE
            SYNTAX INTEGER
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
                "Average number of characters received per second."
        ::= { cdx6500PPSTFRDTEPortEntry 8 }

        cdx6500frdtepCharsOutPerSec OBJECT-TYPE
            SYNTAX INTEGER
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
                "Average number of characters transmitted per second."
        ::= { cdx6500PPSTFRDTEPortEntry 9 }

        cdx6500frdtepFrameInTotal OBJECT-TYPE
            SYNTAX Counter
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
                "Number of frames received since last boot or statistics reset."
        ::= { cdx6500PPSTFRDTEPortEntry 10 }

        cdx6500frdtepFrameOutTotal OBJECT-TYPE
            SYNTAX Counter
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
                "Number of frames transmitted since last boot or statistics 
                 reset."
        ::= { cdx6500PPSTFRDTEPortEntry 11 }

        cdx6500frdtepFramesInPerSec OBJECT-TYPE
            SYNTAX INTEGER
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
                "Average number of frames received per second."
        ::= { cdx6500PPSTFRDTEPortEntry 12 }

        cdx6500frdtepFramesOutPerSec OBJECT-TYPE
            SYNTAX INTEGER
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
                "Average number of frames transmitted per second."
        ::= { cdx6500PPSTFRDTEPortEntry 13 }

        cdx6500frdtepOverrunErrors OBJECT-TYPE
            SYNTAX Counter16
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
                "An input buffer overflowed and characters were discarded."
        ::= { cdx6500PPSTFRDTEPortEntry 14 }

        cdx6500frdtepUnderrunErrors OBJECT-TYPE
            SYNTAX Counter16
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
                "Number of errors detected since last node boot or reset of 
                 statistics."
        ::= { cdx6500PPSTFRDTEPortEntry 15 }

        cdx6500frdtepCRCErrors OBJECT-TYPE
            SYNTAX Counter16
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
                "Number of errors detected by Cyclic Redundancy Check since 
                 last node boot reset of statistics."
        ::= { cdx6500PPSTFRDTEPortEntry 16 }

        -- The Frame Relay DTE Station Configuration Table
        -- This table contains Configuration paramaters 
        -- for the Frame Relay DTE Station table.

        cdx6500SPCTFRDTEStationTable OBJECT-TYPE
            SYNTAX SEQUENCE OF Cdx6500SPCTFRDTEStationEntry
            ACCESS not-accessible
            STATUS mandatory
            DESCRIPTION
                "All of the configuration parameters relevant to the Frame 
                 Relay DTE Station table."
        ::= { cdx6500PCTStationProtocolGroup 2 }

        cdx6500SPCTFRDTEStationEntry OBJECT-TYPE
            SYNTAX Cdx6500SPCTFRDTEStationEntry
            ACCESS not-accessible
            STATUS mandatory
            DESCRIPTION
                "A row of Frame Realy DTE station configuration parameters."
            INDEX { cdx6500frdtesCfgPortNum, cdx6500frdtesCfgStationNum }
        ::= { cdx6500SPCTFRDTEStationTable 1 }

        Cdx6500SPCTFRDTEStationEntry ::=
            SEQUENCE {
                cdx6500frdtesCfgPortNum
                    INTEGER,
                cdx6500frdtesCfgDLCI
                    INTEGER,
                cdx6500frdtesStationType
                    INTEGER,
                cdx6500frdtesCommInfoRate
                    INTEGER,
                cdx6500frdtesCommBurstSize
                    INTEGER,
                cdx6500frdtesTransDelay
                    INTEGER,
                cdx6500frdtesControlledMode
                    INTEGER,
                cdx6500frdtesLinkAddress
                    INTEGER,
                cdx6500frdtesPVCChannels
                    INTEGER,
                cdx6500frdtesStartingPVC
                    INTEGER,
                cdx6500frdtesSVCChannels
                    INTEGER,
                cdx6500frdtesStartingSVC
                    INTEGER,
                cdx6500frdtesInitialFrame
                    INTEGER,
                cdx6500frdtesRetryTimer
                    INTEGER,
                cdx6500frdtesPollTimer
                    INTEGER,
                cdx6500frdtesTries
                    INTEGER,
                cdx6500frdtesFrameWinSize
                    INTEGER,
                cdx6500frdtesPacketWinSize
                    INTEGER,
                cdx6500frdtesMaxPacketSize
                    INTEGER,
                cdx6500frdtesUpperQueue
                    INTEGER,
                cdx6500frdtesLowerQueue
                    INTEGER,
                cdx6500frdtesRestartTimer
                    INTEGER,
                cdx6500frdtesResetTimer
                    INTEGER,
                cdx6500frdtesCallTimer
                    INTEGER,
                cdx6500frdtesClearTimer
                    INTEGER,
                cdx6500frdtesX25Options
                    INTEGER,
                cdx6500frdtesRCDestination
                    DisplayString,
                cdx6500frdtesCUG
                    DisplayString,
                cdx6500frdtesBillingRecords
                    INTEGER,
                cdx6500frdtesCfgStationNum
                    INTEGER,
                cdx6500frdtesStnX25Options
                    DisplayString,
                cdx6500frdtesStnFrameSegmenter
                    INTEGER,
                cdx6500frdtesStnVoiceSVCChannels
                    INTEGER,
                cdx6500frdtesStnVoiceCongCtrlMode
                    INTEGER,
                cdx6500frdtesStnPeakUtilization
                    INTEGER,
                cdx6500frdtesStnMaxInboundQueue
                    INTEGER,
                cdx6500frdtesStnAnnexGRateReduction
                    INTEGER
                }

        cdx6500frdtesCfgPortNum OBJECT-TYPE
            SYNTAX INTEGER (1..255)
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
                "FR DTE Port number."
        ::= { cdx6500SPCTFRDTEStationEntry 1 }

        cdx6500frdtesCfgDLCI OBJECT-TYPE
            SYNTAX INTEGER (16..1007)
            ACCESS read-write
            STATUS mandatory
            DESCRIPTION
                "The Data Link Connection Identifier (dlci) is the unique 
                 identifier for the station on the FRI Port. Its valid range 
                 is 16 - 1007. It must match the DLCI configured on the FR 
                 Networking Node."
        ::= { cdx6500SPCTFRDTEStationEntry 2 }

        cdx6500frdtesStationType OBJECT-TYPE
            SYNTAX INTEGER {
                    annexG     (1),
                    bypass     (2),
                    voiceRelay (3)
                    }
            ACCESS read-write
            STATUS mandatory
            DESCRIPTION
                "Identifies the type of station being configured.
                 annexG      - This station uses X.25 signaling for layer 3
                 bypass      - This station type bypasses layer 3. There is
                               no signaling for SVC's on this station.
                 voiceRelay  - Not used in the current version."
        ::= { cdx6500SPCTFRDTEStationEntry 3 }

        cdx6500frdtesCommInfoRate OBJECT-TYPE
            SYNTAX INTEGER
            ACCESS read-write
            STATUS mandatory
            DESCRIPTION
                "This parameter (CIR) specifies the rate (in bits-per-second) 
                 that the Frame Relay network agrees to transfer information 
                 under congested conditions for the FRI Station.
                 This value is used to throttle data on the FRI station for 
                 congestion control purposes. The CIR cannot be greater than 
                 the link speed."
        ::= { cdx6500SPCTFRDTEStationEntry 4 }

        cdx6500frdtesCommBurstSize OBJECT-TYPE
            SYNTAX INTEGER
            ACCESS read-write
            STATUS mandatory
            DESCRIPTION
                "This parameter (BC) specifies the maximum amount of data (in 
                 bits) that the Frame Relay network agrees to transfer over a 
                 time interval T = Committed Burst Size / Committed Information
                 Rate. Must be > 1/20th of the Committed Information Rate.
                 This is used for congestion control purposes."
        ::= { cdx6500SPCTFRDTEStationEntry 5 }

        cdx6500frdtesTransDelay OBJECT-TYPE
            SYNTAX INTEGER
            ACCESS read-write
            STATUS mandatory
            DESCRIPTION
                "This parameter is the expected one-way delay through the 
                 network. This is specified in milliseconds."
        ::= { cdx6500SPCTFRDTEStationEntry 6 }

        cdx6500frdtesControlledMode OBJECT-TYPE
            SYNTAX INTEGER {
                    normal    (1),
                    disable   (2),
                    congested (3),
                    limit     (4),
                    nc        (100)
                    }
            ACCESS read-write
            STATUS mandatory
            DESCRIPTION
                "This parameter controls the method of congestion control:
                 normal     - congestion control is normal
                 disable    - never enter Congested State (CTRL)
                 congested  - never enter a Non-Congested State (UCTRL)
                 limit      - congestion control is normal but limits the
                              data rate to no lower than CIR.
                 nc         - Skipped during configuration"
        ::= { cdx6500SPCTFRDTEStationEntry 7 }

        cdx6500frdtesLinkAddress OBJECT-TYPE
            SYNTAX  INTEGER {
                     dte (1),
                     dce (2),
                     nc  (100)
                     }
            ACCESS read-write
            STATUS mandatory
            DESCRIPTION
                "The logical link address must be set to complementary values 
                 in this FRI station and its corresponding remote FRI station:
                 dte - station to have logical DTE address (A)
                 dce - station to have logical DCE address (B)
                 nc  - Skipped during configuration."
        ::= { cdx6500SPCTFRDTEStationEntry 8 }

        cdx6500frdtesPVCChannels OBJECT-TYPE
            SYNTAX INTEGER
            ACCESS read-write
            STATUS mandatory
            DESCRIPTION
                "Number of logical channels used for Permanent Virtual Circuits.
                 The total number of PVC and SVC channels on a station (logical
                 link) should be kept as small as possible and consistent with 
                 needs. PVC connections must be configured in the PVC Table."
        ::= { cdx6500SPCTFRDTEStationEntry 9 }

        cdx6500frdtesStartingPVC OBJECT-TYPE
            SYNTAX INTEGER
            ACCESS read-write
            STATUS mandatory
            DESCRIPTION
                "This is the starting logical channel number for the Permanent 
                 Virtual Circuits on this FRI station. Not used if the number 
                 of PVCs = 0."
        ::= { cdx6500SPCTFRDTEStationEntry 10 }

        cdx6500frdtesSVCChannels OBJECT-TYPE
            SYNTAX INTEGER
            ACCESS read-write
            STATUS mandatory
            DESCRIPTION
                "Number of logical channels used in Switched Virtual Circuits.
                 The total number of PVC and SVC channels on a station (logical
                 link) should be kept as small as possible and consistent with 
                 needs."
        ::= { cdx6500SPCTFRDTEStationEntry 11 }

        cdx6500frdtesStartingSVC OBJECT-TYPE
            SYNTAX INTEGER
            ACCESS read-write
            STATUS mandatory
            DESCRIPTION
                "This is the starting logical channel number for the Switched 
                 Virtual Circuits on this station (logical link). Not used if 
                 the number of SVCs = 0."
        ::= { cdx6500SPCTFRDTEStationEntry 12 }

        cdx6500frdtesInitialFrame OBJECT-TYPE
            SYNTAX INTEGER {
                    sabm (1),
                    disc (2),
                    none (3),
                    nc   (100)
                    }
            ACCESS read-write
            STATUS mandatory
            DESCRIPTION
                "Specify the initial frame required by other end for (logical)
                 link startup:
                 none - do nothing (other end initiates link-up)
                 sabm - send SABM
                 disc - send DISC, then SABM
                 nc   - Skipped during configuration"
        ::= { cdx6500SPCTFRDTEStationEntry 13 }

        cdx6500frdtesRetryTimer OBJECT-TYPE
            SYNTAX INTEGER
            ACCESS read-write
            STATUS mandatory
            DESCRIPTION
                "T1 link retry timer value in tenths of a second (e.g. 30 = 3.0
                 seconds)."
        ::= { cdx6500SPCTFRDTEStationEntry 14 }

        cdx6500frdtesPollTimer OBJECT-TYPE
            SYNTAX INTEGER
            ACCESS read-write
            STATUS mandatory
            DESCRIPTION
                "This sets the time an idle link is probed for assurance of 
                 connection to the remote FRI station.
                 0 - disable the timer
                 X - X is in tenths of a second (e.g. 40 = 4.0 seconds) and
                     must be set greater than T1."
        ::= { cdx6500SPCTFRDTEStationEntry 15 }

        cdx6500frdtesTries OBJECT-TYPE
            SYNTAX INTEGER
            ACCESS read-write
            STATUS mandatory
            DESCRIPTION
                "The maximum number of attempts allowed to complete a 
                 transmission."
        ::= { cdx6500SPCTFRDTEStationEntry 16 }

        cdx6500frdtesFrameWinSize OBJECT-TYPE
            SYNTAX INTEGER
            ACCESS read-write
            STATUS mandatory
            DESCRIPTION
                "Frame level window size must be set to the same value
                 at both this FRI station and the remote FRI station."
        ::= { cdx6500SPCTFRDTEStationEntry 17 }

        cdx6500frdtesPacketWinSize OBJECT-TYPE
            SYNTAX INTEGER
            ACCESS read-write
            STATUS mandatory
            DESCRIPTION
                "Packet level window size must be set to the same value 
                 at both this FRI station and the remote FRI station."
        ::= { cdx6500SPCTFRDTEStationEntry 18 }

        cdx6500frdtesMaxPacketSize OBJECT-TYPE
            SYNTAX INTEGER {
                    psize32   (6),
                    psize64   (7),
                    psize128  (8),
                    psize256  (9),
                    psize512  (10),
                    psize1024 (11),
                    nc        (100)
                    }
            ACCESS read-write
            STATUS mandatory
            DESCRIPTION
                "This is the maximum X.25 packet size used on the station 
                 (logical link).
                 For proper operation, this must be set to the same value 
                 at both this FRI station and the remote FRI station.
                 nc - Skipped during configuration. "
        ::= { cdx6500SPCTFRDTEStationEntry 19 }

        cdx6500frdtesUpperQueue OBJECT-TYPE
            SYNTAX INTEGER
            ACCESS read-write
            STATUS mandatory
            DESCRIPTION
                "This is the maximum number of data packets a channel on this 
                 station (logical link) will queue for transmission before 
                 invoking flow control to the attached channel"
        ::= { cdx6500SPCTFRDTEStationEntry 20 }

        cdx6500frdtesLowerQueue OBJECT-TYPE
            SYNTAX INTEGER
            ACCESS read-write
            STATUS mandatory
            DESCRIPTION
                "This is the number of data packets a channel on this FRI
                 station will have queued for transmission when it releases
                 flow control to the attached channel"
        ::= { cdx6500SPCTFRDTEStationEntry 21 }

        cdx6500frdtesRestartTimer OBJECT-TYPE
            SYNTAX INTEGER
            ACCESS read-write
            STATUS mandatory
            DESCRIPTION
                "Restart-Request timeout timer in seconds. If timer expires on 
                 this FRI station, the Restart request is sent again."
        ::= { cdx6500SPCTFRDTEStationEntry 22 }

        cdx6500frdtesResetTimer OBJECT-TYPE
            SYNTAX INTEGER
            ACCESS read-write
            STATUS mandatory
            DESCRIPTION
                "Reset-Request timeout timer in seconds. If timer expires on 
                 this FRI station, the Reset request is sent again."
        ::= { cdx6500SPCTFRDTEStationEntry 23 }

        cdx6500frdtesCallTimer OBJECT-TYPE
            SYNTAX INTEGER
            ACCESS read-write
            STATUS mandatory
            DESCRIPTION
                "Call-Request timeout timer in seconds. If timer expires on 
                 this FRI station, the call is cleared."
        ::= { cdx6500SPCTFRDTEStationEntry 24 }

        cdx6500frdtesClearTimer OBJECT-TYPE
            SYNTAX INTEGER
            ACCESS read-write
            STATUS mandatory
            DESCRIPTION
                "Clear-Request timeout timer in seconds. If timer expires on 
                 this FRI station, the Clear request is sent again."
        ::= { cdx6500SPCTFRDTEStationEntry 25 }

        cdx6500frdtesX25Options OBJECT-TYPE
            SYNTAX INTEGER
            ACCESS read-only
            STATUS deprecated
            DESCRIPTION
                "Select X.25 options on this station (logical link) as follows:
                 NONE  - no option specified
                 CUG   - check closed user group; otherwise, CUG passes 
                         transparently
                 CAUSE - pass cause, diagnostic, code in outbound packets
                 HOLD  - hold calls over link restart
                 INL   - inter-node link. Link goes to another 6500 device
                 DELAY - Enable Delay and Path Trace on this link (link must be
                         connected to Rel. 4.xx node)
                 Any combination of above specified by summing 
                 (e.g. CUG+CAUSE+. . .).
                 NOTE: Please use cdx6500frdtesStnX25Options, which has been 
                 added to replace this object."
        ::= { cdx6500SPCTFRDTEStationEntry 26 }

        cdx6500frdtesRCDestination OBJECT-TYPE
            SYNTAX DisplayString (SIZE(0..255))
            ACCESS read-write
            STATUS mandatory
            DESCRIPTION
                "All calls entering this station will be routed to the 
                 destination specified in this parameter, irrespective of 
                 route selection table entries. For example, to route all 
                 calls to X.25 port 3, use X25-3.
                 To route calls to FRI port 2, station 4, use FRI-2S4. Blank 
                 this field to disable this function."
        ::= { cdx6500SPCTFRDTEStationEntry 27 }

        cdx6500frdtesCUG OBJECT-TYPE
            SYNTAX DisplayString (SIZE(0..23))
            ACCESS read-write
            STATUS mandatory
            DESCRIPTION
                "The station may be a member of up to 8 different Closed User 
                 Groups (CUGs). Each CUG membership must be a two digit number
                 and separated by a comma. 
                 (e.g. 12,34,56,09,02,03 ).
                 Two hyphens following each oher indicates no CUG Membership
                 00-99 - CUG Membership"
        ::= { cdx6500SPCTFRDTEStationEntry 28 }

        cdx6500frdtesBillingRecords OBJECT-TYPE
            SYNTAX INTEGER {
                    off (1),
                    on  (2),
                    nc  (100)
                    }
            ACCESS read-write
            STATUS mandatory
            DESCRIPTION
                "This controls whether billing (accounting) records will be 
                 created for calls on this FRI station.
                 off - Billing records will not be created.
                 on  - Billing records will be created.
                 nc  - Skipped during configuration."
        ::= { cdx6500SPCTFRDTEStationEntry 29 }

        cdx6500frdtesCfgStationNum OBJECT-TYPE
            SYNTAX INTEGER
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
                "The number of the logical link on the line.
                 This is the value of Sxx for routing purposes."
        ::= { cdx6500SPCTFRDTEStationEntry 30 }

        cdx6500frdtesStnX25Options OBJECT-TYPE
            SYNTAX DisplayString (SIZE(0..24))
            ACCESS read-write
            STATUS mandatory
            DESCRIPTION
                "Select X.25 options on this station (logical link) as follows:
                 NONE  - no option specified
                 CUG   - check closed user group; otherwise, CUG passes 
                         transparently
                 CAUSE - pass cause, diagnostic, code in outbound packets
                 HOLD  - hold calls over link restart
                 INL   - inter-node link. Link goes to another 6500 device
                 DELAY - Enable Delay and Path Trace on this link (link must be
                         connected to Rel. 4.xx node)
                 Any combination of above specified by summing 
                 (e.g. CUG+CAUSE+. . .).
                 NOTE: This object has been added to replace 
                 cdx6500frdtesX25Options."
        ::= { cdx6500SPCTFRDTEStationEntry 31 }

        cdx6500frdtesStnFrameSegmenter OBJECT-TYPE
            SYNTAX INTEGER {
                   disabled (1),
                   enabled  (2),
                   nc       (100)
                   }
            ACCESS read-write
            STATUS mandatory
            DESCRIPTION
                "Determine if this station will support the Frame Segmenter
                 Header.The Frame Segmenter should be enabled if the frame
                 relay port is configured to pass voice traffic.
                 disabled - Frame Segmenter disabled.
                 enabled  - Frame Segmenter enabled.
                 nc       - Skipped during configuration."
--            DEFVAL { disabled }
        ::= { cdx6500SPCTFRDTEStationEntry 32 }


        cdx6500frdtesStnVoiceSVCChannels OBJECT-TYPE
            SYNTAX INTEGER (0..15)
            ACCESS read-write
            STATUS mandatory
            DESCRIPTION
                "Number of logical voice channels used in Switched Virtual
                 Circuits.The total number of voice and data channels on a
                 station (logical link) should be kept as small as possible
                 and consistent with needs."
--            DEFVAL { 0 }
        ::= { cdx6500SPCTFRDTEStationEntry 33 }

        cdx6500frdtesStnVoiceCongCtrlMode OBJECT-TYPE
            SYNTAX INTEGER {
                   disabled (1),
                   enabled  (2),
                   nc       (100)
                   }
            ACCESS read-write
            STATUS mandatory
            DESCRIPTION
                "This parameter controls the method of voice congestion 
                 control:
                 enabled  - Station is forced into congestion when voice 
                            calls are detected.
                 disabled - Station operates normally
                 Use this parameter in voice applications, when the frame 
                 relay network can have more than 140ms of data buffered. 
                 One typical application is asymmetrical speeds. When this 
                 parameter is enabled, and voice is detected, delta T is 
                 forced to 50 ms and the BC is forced to 1/20 CIR."
--            DEFVAL { disabled }
        ::= { cdx6500SPCTFRDTEStationEntry 34 }

        cdx6500frdtesStnPeakUtilization OBJECT-TYPE
            SYNTAX INTEGER (0..240)
            ACCESS read-write
            STATUS mandatory
            DESCRIPTION
                 "The monitoring interval size specifies the time period
                  to use when monitoring peak data link/CIR (Committed
                  Information Rate) utilization.

                  0 - Disables this monitoring feature.
                  1 to 240 - The number of 64 second increments to use for the
                  monitoring interval time period.
                  For example: a value of 30 specifies a time duration
                  equal to 30 * 64 seconds = 1920 seconds or 32 minutes."
--            DEFVAL { 0 }
        ::= { cdx6500SPCTFRDTEStationEntry 35 }

        cdx6500frdtesStnMaxInboundQueue OBJECT-TYPE
            SYNTAX INTEGER (100..2500)
            ACCESS read-write
            STATUS mandatory
            DESCRIPTION
                "This specifies the maximum number of frames from the
                 network which may be queued in a BYPASS station. A
                 smaller value may ve choosen for applications which
                 experience objectable delays, if due to excessive
                 growth of the BYPASS inbound queue."
--            DEFVAL { 2500 }
        ::= { cdx6500SPCTFRDTEStationEntry 36 }

        cdx6500frdtesStnAnnexGRateReduction OBJECT-TYPE
            SYNTAX INTEGER {
                   disabled (1),
                   enabled  (2),
                   nc (100)
                }
            ACCESS read-write
            STATUS mandatory
            DESCRIPTION
                "This parameter determines whether Annex G stations will
                 invoke implicit congestion control when a link error is
                 detected, reducing their transmitted information rate
                 to 1/4 CIR. The rate will be gradually increased in the
                 normal way as non-BECN frames are received."
--            DEFVAL { enabled }
        ::= { cdx6500SPCTFRDTEStationEntry 37 }

        -- The Frame Relay DTE Station Statistics Table
        -- This table contains Statistics paramaters
        -- for the Frame Relay DTE Station table.

        cdx6500SPSTFRDTEStationTable OBJECT-TYPE
            SYNTAX SEQUENCE OF Cdx6500SPSTFRDTEStationEntry
            ACCESS not-accessible
            STATUS mandatory
            DESCRIPTION
                "All of the statistics parameters relevant
                 to the Frame Relay DTE Station (DLCI) table."
        ::= { cdx6500PSTStationProtocolGroup 2 }

        cdx6500SPSTFRDTEStationEntry OBJECT-TYPE
            SYNTAX Cdx6500SPSTFRDTEStationEntry
            ACCESS not-accessible
            STATUS mandatory
            DESCRIPTION
                "A row of Frame Relay DTE station (DLCI)
                 Statistics parameters."
            INDEX { cdx6500frdtesStatsPortNum, cdx6500frdtesStatsStationNumber }
        ::= { cdx6500SPSTFRDTEStationTable 1 }

        Cdx6500SPSTFRDTEStationEntry ::=
            SEQUENCE {
                cdx6500frdtesStatsPortNum
                    INTEGER,
                cdx6500frdtesStatsDLCI
                    INTEGER,
                cdx6500frdtesUtilizationIn
                    INTEGER,
                cdx6500frdtesUtilizationOut
                    INTEGER,
                cdx6500frdtesMaxSVCCount
                    INTEGER,
                cdx6500frdtesCurrentSVCCount
                    INTEGER,
                cdx6500frdtesCharInTotal
                    Counter,
                cdx6500frdtesCharOutTotal
                    Counter,
                cdx6500frdtesCharsInPerSec
                    INTEGER,
                cdx6500frdtesCharsOutPerSec
                    INTEGER,
                cdx6500frdtesPktInTotal
                    Counter,
                cdx6500frdtesPktOutTotal
                    Counter,
                cdx6500frdtesPktsInPerSec
                    INTEGER,
                cdx6500frdtesPktsOutPerSec
                    INTEGER,
                cdx6500frdtesPacketsQueued
                    INTEGER,
                cdx6500frdtesFrameInTotal
                    Counter,
                cdx6500frdtesFrameOutTotal
                    Counter,
                cdx6500frdtesFramesInPerSec
                    INTEGER,
                cdx6500frdtesFramesOutPerSec
                    INTEGER,
                cdx6500frdtesInfoFramesIn
                    Counter,
                cdx6500frdtesInfoFramesOut
                    Counter,
                cdx6500frdtesRNRFramesIn
                    Counter,
                cdx6500frdtesRNRFramesOut
                    Counter,
                cdx6500frdtesRRFramesIn
                    Counter,
                cdx6500frdtesRRFramesOut
                    Counter,
                cdx6500frdtesREJFramesIn
                    Counter,
                cdx6500frdtesREJFramesOut
                    Counter,
                cdx6500frdtesDataPktsIn
                    Counter,
                cdx6500frdtesDataPktsOut
                    Counter,
                cdx6500frdtesResetStats
                    INTEGER,
                cdx6500frdtesBoot
                    INTEGER,
                cdx6500frdtesDisable
                    INTEGER,
                cdx6500frdtesEnable
                    INTEGER,
                cdx6500frdtesStatsStationNumber
                    INTEGER
                }

