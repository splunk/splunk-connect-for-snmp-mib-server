                "Used to clear the RNR request."
        ::= { cdx6500SPSTFRDTEStationEntry 24 }

        cdx6500frdtesRRFramesOut OBJECT-TYPE
            SYNTAX Counter
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
                "Used to clear the RNR request."
        ::= { cdx6500SPSTFRDTEStationEntry 25 }

        cdx6500frdtesREJFramesIn OBJECT-TYPE
            SYNTAX Counter
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
                "A REJ transmitted by a receiver requests retransmission of a 
                 frame, due to detection of a transmission or frame sequence 
                 error."
        ::= { cdx6500SPSTFRDTEStationEntry 26 }

        cdx6500frdtesREJFramesOut OBJECT-TYPE
            SYNTAX Counter
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
                "A REJ transmitted by a receiver requests retransmission of a 
                 frame, due to detection of a transmission or frame sequence 
                 error."
        ::= { cdx6500SPSTFRDTEStationEntry 27 }

        cdx6500frdtesDataPktsIn OBJECT-TYPE
            SYNTAX Counter
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
                "A packet containing user information."
        ::= { cdx6500SPSTFRDTEStationEntry 28 }

        cdx6500frdtesDataPktsOut OBJECT-TYPE
            SYNTAX Counter
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
                "A packet containing user information."
        ::= { cdx6500SPSTFRDTEStationEntry 29 }

        -- The Frame Relay DTE Station Control objects
        -- These objects provide Control parameters
        -- for the Frame Relay DTE Stations.

        cdx6500frdtesResetStats OBJECT-TYPE
            SYNTAX INTEGER {
                    reset   (1),
                    noReset (2)
                    }
            ACCESS write-only
            STATUS mandatory
            DESCRIPTION
                "Resets the specified Frame Relay station statistics.
                 reset   - Reset station statistics.
                 noReset - Do not reset station statistics."
        ::= { cdx6500SPSTFRDTEStationEntry 30 }

        cdx6500frdtesBoot OBJECT-TYPE
            SYNTAX INTEGER {
                     boot   (1),
                     noBoot (2)
                    }
            ACCESS write-only
            STATUS mandatory
            DESCRIPTION
                "Boots the specified Frame Relay station.
                 boot   - Boot station.
                 noBoot - Do not boot station."
        ::= { cdx6500SPSTFRDTEStationEntry 31 }

        cdx6500frdtesDisable OBJECT-TYPE
            SYNTAX INTEGER {
                    disable   (1),
                    noDisable (2)
                    }
            ACCESS write-only
            STATUS mandatory
            DESCRIPTION
                "Disables the specified Frame Relay station.
                 disable   - Disable station.
                 noDisable - Do not disable station."
        ::= { cdx6500SPSTFRDTEStationEntry 32 }

        cdx6500frdtesEnable OBJECT-TYPE
            SYNTAX INTEGER {
                     enable   (1),
                     noEnable (2)
                     }
            ACCESS write-only
            STATUS mandatory
            DESCRIPTION
                "Enables the specified Frame Relay station.
                 enable   - Enable station.
                 noEnable - Do not enable station."
        ::= { cdx6500SPSTFRDTEStationEntry 33 }

        cdx6500frdtesStatsStationNumber OBJECT-TYPE
            SYNTAX INTEGER
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
                "The number of the FRI Station."
           ::= { cdx6500SPSTFRDTEStationEntry 34 }

END

