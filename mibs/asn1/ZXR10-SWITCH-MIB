            zessSecondaryPort
                DisplayString,
            zessPreupDelayTime
                Integer32,
            zessDomainMode
                INTEGER,
            zessDomainState
                INTEGER,
            zessPriIfState
                INTEGER,
            zessSecIfState
                INTEGER,
            zessChangeTimes
                INTEGER,
            zessClearChangeTimes
                INTEGER,    
            zessDomainRowStatus
                RowStatus                    
    }   

