   mscPppNcpBmcEntryRowStatus
      RowStatus,
   mscPppNcpBmcEntryComponentName
      DisplayString,
   mscPppNcpBmcEntryStorageType
      StorageType,
   mscPppNcpBmcEntryMacTypeIndex
      INTEGER }

