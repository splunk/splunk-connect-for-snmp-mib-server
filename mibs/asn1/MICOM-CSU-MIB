--
-- ***** Passport Frame Relay Access Device MIB File - CSU Interfaces  *****
--
-- Based on CSU Requirements documents (T1, E1, 56K).
-- MIB module applicable to Software versions beginning with Release 3.0.
-- 
-- MIB Revision : 1.0 dated 6/1/98
-- Change Desc. : Initial submittal - R. Carmona, A. Llanes, S. Gaur. 
-- 
-- MIB Revision : 1.1 dated 6/11/98
-- Change Desc. : Revised CSU MIB as per Chien Lee's request:
--                Moved mcmT1E1CsuCfgDS0Connection to the end of 
--                mcmT1E1CsuCfgTable.  Changed the 
--                mcmE1CsuGenStatusLnCdViolations object name to
--				  mcmE1CsuGenStatusBpvOrLnCdViolations. - A. Llanes
-- 
-- MIB Revision : 1.2 dated 7/02/98
-- Change Desc. : Revised CSU MIB as per Nina Tranle's request:
-- 				  Due to the CLI unable to support imported indices,
--				  imported ifindex has been removed and an ifindex
--				  object has been added to each table. 
--				  Note:  This revision has caused OID's to change. 
--						 This revision has caused the traps' ifindex
--						 variable OID to change. - A. Llanes
-- MIB Revision : 1.3 dated 11/17/98
-- Change Desc. : Removed all "56k" from mcm56kCsuCfgTable and mcm56kCsuStatusTable.
--                This change fixed the issue of duplicated names in mcm56kcsu.mib
--                and mcmcsu.mib.

MICOM-CSU-MIB DEFINITIONS ::= BEGIN

IMPORTS

	OBJECT-TYPE
		FROM RFC-1212
	mcmSysAsciiTimeOfDay
		FROM MICOM-SYS-MIB
	TRAP-TYPE
		FROM RFC-1215
	DisplayString 
		FROM SNMPv2-TC
	micom-oscar 
		FROM MICOM-OSCAR-MIB;


-- group OBJECT IDENTIFIERs

	micom-csu			OBJECT IDENTIFIER ::= {micom-oscar 29}
	csu-configuration	OBJECT IDENTIFIER ::= {micom-csu 1}
	csu-status			OBJECT IDENTIFIER ::= {micom-csu 2}


--  the T1/E1 CSU Configuration Table
--  This table provides the T1/E1 CSU configuration parameters.

mcmT1E1CsuCfgTable OBJECT-TYPE
    SYNTAX  SEQUENCE OF McmT1E1CsuCfgEntry
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
      "NAME = ;
           DESC = The T1/E1 CSU Configuration table.;
       HELP = ;
           CAPABILITIES = NET_CFG, VPN_CFG;"
    ::= { csu-configuration 1 }

mcmT1E1CsuCfgEntry OBJECT-TYPE
    SYNTAX  McmT1E1CsuCfgEntry
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
      "NAME = ;
           DESC = An entry in the T1/E1 CSU Configuration \
                  table.;
           HELP = ;
           CAPABILITIES = ;"
    INDEX   { mcmT1E1CsuCfgifIndex }
    ::= { mcmT1E1CsuCfgTable 1 }

McmT1E1CsuCfgEntry ::= SEQUENCE {
	mcmT1E1CsuCfgifIndex			INTEGER,
    mcmT1E1CsuCfgLineType			INTEGER,
    mcmT1E1CsuCfgLineSpeed		INTEGER,
    mcmT1E1CsuCfgLineBuildOut		INTEGER,
    mcmT1E1CsuCfgTxIdleCode		INTEGER,
    mcmT1E1CsuCfgTxRxClkSource  	INTEGER,
    mcmT1E1CsuCfgDS0BasicRate		INTEGER,
    mcmT1E1CsuCfgLocalLoopback		INTEGER,
    mcmT1E1CsuCfgRemoteLoopback		INTEGER,
    mcmT1E1CsuCfgFramerLoopback  	INTEGER,
    mcmT1E1CsuCfgTrapEnable		INTEGER,
    mcmT1E1CsuCfgFrameFmt		INTEGER,
    mcmT1E1CsuCfgTxRx0CodeSuppress	INTEGER,
    mcmT1E1CsuCfgTxB7ZeroSuppress	INTEGER,
    mcmT1E1CsuCfgPayloadLoopback	INTEGER,
    mcmT1E1CsuCfgTransmitLoopUp		INTEGER,
    mcmT1E1CsuCfgTransmitLoopDown	INTEGER,
    mcmT1E1CsuCfgTxRxHDB3		INTEGER,
    mcmT1E1CsuCfgTxRxCRC4		INTEGER,
    mcmT1E1CsuCfgDS0Connection		DisplayString
    }

mcmT1E1CsuCfgifIndex OBJECT-TYPE
	SYNTAX	INTEGER (1..255)
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = The T1E1 Csu Cfg interface index \
				value of the specifc entry.;
	   HELP = ;
	   CAPABILITIES = ;"
	::= { mcmT1E1CsuCfgEntry 1 }

mcmT1E1CsuCfgLineType OBJECT-TYPE
	SYNTAX	INTEGER {
		t1Csu (1),
		e1Csu (2)
		}
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = The value of this object determines  \
		  whether the line is a T1 CSU or E1 CSU.;
	   HELP = ;
	   CAPABILITIES = ;"
	::= { mcmT1E1CsuCfgEntry 2 }

mcmT1E1CsuCfgLineSpeed OBJECT-TYPE
	SYNTAX	INTEGER {
		ls-1ds0Times64K-64K (1),
		ls-2ds0Times64K-128K (2),
		ls-3ds0Times64K-192K (3),
		ls-4ds0Times64K-256K (4),
		ls-5ds0Times64K-320K (5),
		ls-6ds0Times64K-384K (6),
		ls-7ds0Times64K-448K (7),
		ls-8ds0Times64K-512K (8),
		ls-9ds0Times64K-576K (9),
		ls-10ds0Times64K-640K (10),
		ls-11ds0Times64K-704K (11),
		ls-12ds0Times64K-768K (12),
		ls-13ds0Times64K-832K (13),
		ls-14ds0Times64K-896K (14),
		ls-15ds0Times64K-960K (15),
		ls-16ds0Times64K-1024K (16),
		ls-17ds0Times64K-1088K (17),
		ls-18ds0Times64K-1152K (18),
		ls-19ds0Times64K-1216K (19),
		ls-20ds0Times64K-1280K (20),
		ls-21ds0Times64K-1344K (21),
		ls-22ds0Times64K-1408K (22),
		ls-23ds0Times64K-1472K (23),
		ls-24ds0Times64K-1536K (24),
		ls-25ds0Times64K-1600K (25),
		ls-26ds0Times64K-1664K (26),
		ls-27ds0Times64K-1728K (27),
		ls-28ds0Times64K-1792K (28),
		ls-29ds0Times64K-1856K (29),
		ls-30ds0Times64K-1920K (30),
		ls-1ds0Times56K-56K (31),
		ls-2ds0Times56K-112K (32),
		ls-3ds0Times56K-168K (33),
		ls-4ds0Times56K-224K (34),
		ls-5ds0Times56K-280K (35),
		ls-6ds0Times56K-336K (36),
		ls-7ds0Times56K-392K (37),
		ls-8ds0Times56K-448K (38),
		ls-9ds0Times56K-504K (39),
		ls-10ds0Times56K-560K (40),
		ls-11ds0Times56K-616K (41),
		ls-12ds0Times56K-672K (42),
		ls-13ds0Times56K-728K (43),
		ls-14ds0Times56K-784K (44),
		ls-15ds0Times56K-840K (45),
		ls-16ds0Times56K-896K (46),
		ls-17ds0Times56K-952K (47),
		ls-18ds0Times56K-1008K (48),
		ls-19ds0Times56K-1064K (49),
		ls-20ds0Times56K-1120K (50),
		ls-21ds0Times56K-1176K (51),
		ls-22ds0Times56K-1232K (52),
		ls-23ds0Times56K-1288K (53),
		ls-24ds0Times56K-1344K (54),
		ls-25ds0Times56K-1400K (55),
		ls-26ds0Times56K-1456K (56),
		ls-27ds0Times56K-1512K (57),
		ls-28ds0Times56K-1568K (58),
		ls-29ds0Times56K-1624K (59),
		ls-30ds0Times56K-1680K (60),
		idle (61)
		}
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = The value of this object indicates the T1/E1 CSU \
		  total configured DS0 line speed.;
	   HELP = ;
	   CAPABILITIES = ;"
	::= { mcmT1E1CsuCfgEntry 3 }

mcmT1E1CsuCfgLineBuildOut OBJECT-TYPE
	SYNTAX	INTEGER {
		dist-0-133-feet (1),
		dist-133-266-feet (2),
		dist-266-399-feet (3),
		dist-399-533-feet (4),
		dist-533-655-feet (5),
		neg-7pt5-db (6),
		neg-15-db (7),
		neg-22pt5-db (8),
		ohm-75-normal (9),
		ohm-120-normal (10),
		ohm-75-protection-resistors (11),
		ohm-120-protection-resistors (12),
		ohm-75-high-return-loss-1-1pt15-step-up (13),
		ohm-75-high-return-loss-1-1pt36-step-up (14),
		ohm-120-high-return-loss-1-1pt36-step-up (15)
		}
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = The value of this object selects the T1/E1 CSU \
		  line build out.  For a T1 CSU, the default value \
		  is neg-7pt5-db (6).  For an E1 CSU, the default \
		  value is either ohm-75-normal (9) (an E1 CSU interface \
		  with 2 BNC connectors) or ohm-120-normal (10) \
		  (an E1 CSU interface with an RJ48 connector). \
		  Note that the ranges of enumeration values 1-8 and \
		  9-15 apply uniquely to T1 and E1 interfaces; respectively.;
	   HELP = ;
	   CAPABILITIES = ;"
	::= { mcmT1E1CsuCfgEntry 4 }

mcmT1E1CsuCfgTxIdleCode OBJECT-TYPE
	SYNTAX	INTEGER (0..'ff'h)
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = The value of this object selects the T1/E1 CSU \
		  transmit idle code. For a T1 CSU, the default value is \
		  hex 0x7F. For an E1 CSU, the default value is hex 0x55.;
	   HELP = ;
	   CAPABILITIES = ;"
	::= { mcmT1E1CsuCfgEntry 5 }

mcmT1E1CsuCfgTxRxClkSource OBJECT-TYPE
	SYNTAX	INTEGER {
		internalClockingSource (1),
		externalClockingSource (2)
		}
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = The value of this object selects the T1/E1 CSU \
		  line clocking source.;
	   HELP = ;
	   CAPABILITIES = ;"
	DEFVAL { externalClockingSource }
	::= { mcmT1E1CsuCfgEntry 6 }

mcmT1E1CsuCfgDS0BasicRate OBJECT-TYPE
	SYNTAX	INTEGER {
		br64K-basicRate (1),
		br56K-basicRate (2)
		}
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = The value of this object selects the T1/E1 CSU \
		  line speed to be a multiple of the specified \
		  basic rate.  E.g., Line Speed 256K = 4 x 64K.;
	   HELP = ;
	   CAPABILITIES = ;"
	DEFVAL { br64K-basicRate }
	::= { mcmT1E1CsuCfgEntry 7 }

mcmT1E1CsuCfgLocalLoopback OBJECT-TYPE
	SYNTAX	INTEGER {
		disable (1),
		enable (2)
		}
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = The value of this object selects the T1/E1 CSU \
		  local loopback.;
	   HELP = ;
	   CAPABILITIES = ;"
	DEFVAL { disable }
	::= { mcmT1E1CsuCfgEntry 8 }

mcmT1E1CsuCfgRemoteLoopback OBJECT-TYPE
	SYNTAX	INTEGER {
		disable (1),
		enable (2)
		}
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = The value of this object selects the T1/E1 CSU \
		  remote loopback.;
	   HELP = ;
	   CAPABILITIES = ;"
	DEFVAL { disable }
	::= { mcmT1E1CsuCfgEntry 9 }

mcmT1E1CsuCfgFramerLoopback OBJECT-TYPE
	SYNTAX	INTEGER {
		disable (1),
		enable (2)
		}
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = The value of this object selects the T1/E1 CSU \
		  framer loopback.;
	   HELP = ;
	   CAPABILITIES = ;"
	DEFVAL { disable }
	::= { mcmT1E1CsuCfgEntry 10 }

mcmT1E1CsuCfgTrapEnable OBJECT-TYPE
	SYNTAX	INTEGER {
		disable (1),
		enable (2)
		}
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = The value of this object selects whether the T1/E1 \
		  CSU traps should be generated for this interface.;
	   HELP = ;
	   CAPABILITIES = ;"
	DEFVAL { enable }
	::= { mcmT1E1CsuCfgEntry 11 }

mcmT1E1CsuCfgFrameFmt OBJECT-TYPE
	SYNTAX	INTEGER {
		d4FramingMode (1),
		esfFramingMode (2)
		}
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = The value of this object selects the T1 CSU \
		  frame format. \
		  Note that for an E1 CSU interface, this object is not used.;
	   HELP = ;
	   CAPABILITIES = ;"
	DEFVAL { esfFramingMode }
	::= { mcmT1E1CsuCfgEntry 12 }

mcmT1E1CsuCfgTxRx0CodeSuppress OBJECT-TYPE
	SYNTAX	INTEGER {
		disable-b8zs (1),
		enable-b8zs (2)
		}
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = The value of this object selects zero \
		  code suppression for a T1 CSU interface. \
		  Note that for an E1 CSU interface, this object is not used.;
	   HELP = ;
	   CAPABILITIES = ;"
	DEFVAL { enable-b8zs }
	::= { mcmT1E1CsuCfgEntry 13 }

mcmT1E1CsuCfgTxB7ZeroSuppress OBJECT-TYPE
	SYNTAX	INTEGER {
		disable-b7zs (1),
		enable-b7zs (2)
		}
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = The value of this object selects bit 7 zero\
		  suppression for a T1 CSU interface.  Selects stuffing \
		  bit 7 to 1 if 8 consecutive zeros occurred. \
		  Note that for an E1 CSU interface, this object is not used.;
	   HELP = b7zsDisable (1): no stuffing occurred. \
		  b7zsEnable (2): bit 7 forced to a one in \
		  channels with all zeros.;
	   CAPABILITIES = ;"
	DEFVAL { disable-b7zs }
	::= { mcmT1E1CsuCfgEntry 14 }

mcmT1E1CsuCfgPayloadLoopback OBJECT-TYPE
	SYNTAX	INTEGER {
		disable (1),
		enable (2)
		}
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = The value of this object selects the T1 CSU \
		  payload loopback. \
		  Note that for an E1 CSU interface, this object is not used.;
	   HELP = ;
	   CAPABILITIES = ;"
	DEFVAL { disable }
	::= { mcmT1E1CsuCfgEntry 15 }

mcmT1E1CsuCfgTransmitLoopUp OBJECT-TYPE
	SYNTAX	INTEGER {
		disable (1),
		enable (2)
		}
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = The value of this object selects the T1 CSU \
		  transmit loop up. \
		  Note that for an E1 CSU interface, this object is not used.;
	   HELP = ;
	   CAPABILITIES = ;"
	DEFVAL { disable }
	::= { mcmT1E1CsuCfgEntry 16 }

mcmT1E1CsuCfgTransmitLoopDown OBJECT-TYPE
	SYNTAX	INTEGER {
		disable (1),
		enable (2)
		}
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = The value of this object selects the T1 CSU \
		  transmit loop down. \
		  Note that for an E1 CSU interface, this object is not used.;
	   HELP = ;
	   CAPABILITIES = ;"
	DEFVAL { disable }
	::= { mcmT1E1CsuCfgEntry 17 }

mcmT1E1CsuCfgTxRxHDB3	 OBJECT-TYPE
	SYNTAX	INTEGER {
		disable (1),
		enable  (2)
		}
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = The value of this object selects the E1 CSU HDB3. \
		  Note that for a T1 CSU interface, this object is not used.;
	   HELP = ;
	   CAPABILITIES = ;"
	DEFVAL { enable }
	::= { mcmT1E1CsuCfgEntry 18 }

mcmT1E1CsuCfgTxRxCRC4	 OBJECT-TYPE
	SYNTAX	INTEGER {
		disable (1),
		enable  (2)
		}
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = The value of this object selects the E1 CSU CRC4. \
		  Note that for a T1 CSU interface, this object is not used.;
	   HELP = ;
	   CAPABILITIES = ;"
	DEFVAL { disable }
	::= { mcmT1E1CsuCfgEntry 19 }

mcmT1E1CsuCfgDS0Connection OBJECT-TYPE
	SYNTAX	DisplayString (SIZE (0..83))
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = The value of this object selects the DS0 \
		  channels for the T1/E1 CSU line connection. \
		  Valid characters are decimal digits, comma \
		  (','), and hyphen ('-').;
	   HELP = Comma (',') is used as a delimiter between \
		  individual channel selections. \
		  e.g., 13,16 means DS0 channels 13 and 16. \
		  Hyphen ('-') is used only in Set operations \
		  to indicate a range of DS0 channels. \
		  e.g., 2,5-8 means DS0 channels 2,5,6,7, and 8.;
	   CAPABILITIES = ;"
	DEFVAL { "" }
	::= { mcmT1E1CsuCfgEntry 20 }



--  the 56K CSU Configuration Table 
--  This table provides the 56K CSU configuration parameters.

mcmCsuCfgTable  OBJECT-TYPE
    SYNTAX  SEQUENCE OF McmCsuCfgEntry
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
      "NAME = ;
           DESC = The 56K CSU Configuration table.;
       HELP = ;
           CAPABILITIES = NET_CFG, VPN_CFG;"
    ::= { csu-configuration 2 }

mcmCsuCfgEntry OBJECT-TYPE
    SYNTAX  McmCsuCfgEntry
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
      "NAME = ;
           DESC = An entry in the 56K CSU Configuration \
                  table.);
           HELP = ;
           CAPABILITIES = ;"
    INDEX   { mcmCsuCfgifIndex }
    ::= { mcmCsuCfgTable 1 }

McmCsuCfgEntry ::= SEQUENCE {
	mcmCsuCfgifIndex				INTEGER,
    mcmCsuCfgOperatingMode		INTEGER,
    mcmCsuCfgClockingSource		INTEGER,
    mcmCsuCfgTxOutOfFrame		INTEGER,
    mcmCsuCfgTxOutOfService		INTEGER,
    mcmCsuCfgTxControlModeIdle  	INTEGER,
    mcmCsuCfgZeroSuppressDisable 	INTEGER,
    mcmCsuCfgTxIdle			INTEGER,
    mcmCsuCfgCSULoopback		INTEGER,
    mcmCsuCfgFilterForceEnable  	INTEGER,
    mcmCsuCfgFilterForcingCntl   	INTEGER
    }

mcmCsuCfgifIndex OBJECT-TYPE
	SYNTAX	INTEGER (1..255)
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = The 56k Csu Cfg interface index \
				value of the specifc entry.;
	   HELP = ;
	   CAPABILITIES = ;"
	::= { mcmCsuCfgEntry 1 }

mcmCsuCfgOperatingMode OBJECT-TYPE
	SYNTAX	INTEGER {
		dds-pri-4Wire-56k (1),
		dds-sc-WithSecondaryChannel-72k (2),
		cc-64k-ClearChannel-64k (3),
		dds-pri-2Wire-56k (4)
		}
	ACCESS	read-write
	STATUS	mandatory	
	DESCRIPTION
	  "NAME = ;
	   DESC = The value of this object selects the 56K CSU \
		  operating mode.;
	   HELP = ;
	   CAPABILITIES = ;"
	DEFVAL { dds-pri-4Wire-56k }
	::= { mcmCsuCfgEntry 2 }

mcmCsuCfgClockingSource OBJECT-TYPE
	SYNTAX	INTEGER {
		externalClock (1),
		internalClock (2)
		}
	ACCESS	read-write
	STATUS	mandatory	
	DESCRIPTION
	  "NAME = ;
	   DESC = The value of this object selects the 56K CSU \
		  clocking source.;
	   HELP = ;
	   CAPABILITIES = ;"
	DEFVAL { externalClock }
	::= { mcmCsuCfgEntry 3 }

mcmCsuCfgTxOutOfFrame OBJECT-TYPE
	SYNTAX	INTEGER {
		normalTransmitCondition (1),
		transmitOutOfFrame (2)
		}
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = This object selects transmission of the 56K CSU \
		  Out Of Frame sequence for DDS-PRI mode, or \
		  transmission of Multiplexer Out Of Sync \
		  sequence for DDS-SC mode.;
	   HELP = ;
	   CAPABILITIES = ;"
	DEFVAL { normalTransmitCondition }
	::= { mcmCsuCfgEntry 4 }

mcmCsuCfgTxOutOfService OBJECT-TYPE
	SYNTAX	INTEGER {
		normalTransmitCondition (1),
		transmitOutOfService (2)
		}
	ACCESS	read-write
	STATUS	mandatory	
	DESCRIPTION
	  "NAME = ;
	   DESC = This object selects transmission of the 56K CSU \
		  Out Of Service sequence for DDS-PRI mode, or \
		  transmission of Abnormal Station Code \
		  sequence for DDS-SC mode.;
	   HELP = ;
	   CAPABILITIES = ;"
	DEFVAL { normalTransmitCondition }
	::= { mcmCsuCfgEntry 5 }

mcmCsuCfgTxControlModeIdle OBJECT-TYPE
	SYNTAX	INTEGER {
		normalTransmitCondition (1),
		transmitControlModeIdle (2)
		}
	ACCESS	read-write
	STATUS	mandatory	
	DESCRIPTION
	  "NAME = ;
	   DESC = This object selects transmission of the 56K CSU \
		  Control Mode Idle sequence.;
	   HELP = ;
	   CAPABILITIES = ;"
	DEFVAL { normalTransmitCondition }
	::= { mcmCsuCfgEntry 6 }

mcmCsuCfgZeroSuppressDisable OBJECT-TYPE
	SYNTAX	INTEGER {
		normalTransmitCondition (1),
		zeroSuppressionDisable (2)
		}
	ACCESS	read-write
	STATUS	mandatory	
	DESCRIPTION
	  "NAME = ;
	   DESC = This object selects the 56K CSU disable \
		  transmit Zero Suppression for DDS-PRI mode, \
		  or transmission of all zeros for DDS-SC mode.;
	   HELP = ;
	   CAPABILITIES = ;"
	DEFVAL { normalTransmitCondition }
	::= { mcmCsuCfgEntry 7 }

mcmCsuCfgTxIdle OBJECT-TYPE
	SYNTAX	INTEGER {
		normalTransmitCondition (1),
		transmitIdle (2)
		}
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = This object selects transmission of the 56K CSU \
		  All Marks (or 1s) of the Data Mode Idle sequence \
		  for all the modes.;
	   HELP = ;
	   CAPABILITIES = ;"
	DEFVAL { normalTransmitCondition }
	::= { mcmCsuCfgEntry 8 }

mcmCsuCfgCSULoopback OBJECT-TYPE
	SYNTAX	INTEGER {
		normalReceiveCondition (1),
		forceCSUtoLoopback (2)
		}
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION
	  "NAME = ;
