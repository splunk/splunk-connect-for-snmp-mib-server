--  PRI Japanese Security Operation Table


        ncmJapPRISecurOperTable OBJECT-TYPE
        SYNTAX  SEQUENCE OF NcmJapPRISecurOperEntry
        ACCESS  not-accessible
        STATUS  mandatory
	DESCRIPTION
        "The PRI Security Operation Table  ."

        --DEFAULT test-function    japisdn-test
        --DEFAULT next-function    japisdn-next

         ::= { ncm-japisdn 8005 }


        ncmJapPRISecurOperEntry OBJECT-TYPE
        SYNTAX  NcmJapPRISecurOperEntry
        ACCESS  not-accessible
        STATUS  mandatory
        DESCRIPTION
        "An entry in the PRI Security Operation Table  ."
         INDEX   { ncmJapPRISecOpNIDIndex, ncmJapPRISecOpLineIndex }
         ::= { ncmJapPRISecurOperTable 1 }


         NcmJapPRISecurOperEntry ::=
         SEQUENCE {
	
	   ncmJapPRISecOpNIDIndex
		 INTEGER,
           ncmJapPRISecOpLineIndex
                 INTEGER,
	   ncmJapPRISecOpFirstNum
		 INTEGER,
	   ncmJapPRISecOpListype
		 INTEGER,
	   ncmJapPRISecOpCountNum
		 INTEGER,
	   ncmJapPRISecOpClearElement
		 INTEGER,
	   ncmJapPRISecOpStatus
		 INTEGER,
           ncmJapPRISecOpAction
		 INTEGER

	}


	 ncmJapPRISecOpNIDIndex OBJECT-TYPE
          SYNTAX  INTEGER (1..'7fffffff'h)
          ACCESS  read-only
          STATUS  mandatory
          DESCRIPTION
         "The object specifies the node id of the node."
	 --DEFAULT get-function    get-ncmJapPRINIDIndex 
           
          ::= { ncmJapPRISecurOperEntry 1 }


	 ncmJapPRISecOpLineIndex OBJECT-TYPE
          SYNTAX  INTEGER (1..'7fffffff'h)
          ACCESS  read-only
          STATUS  mandatory
          DESCRIPTION
         "The index value which uniquely identifies  the
          interface to which this entry is applicable."
	 --DEFAULT get-function    get-ncmJapPRILineIndex  
           
          ::= { ncmJapPRISecurOperEntry 2 }
 
         
         ncmJapPRISecOpFirstNum OBJECT-TYPE
          SYNTAX INTEGER {
		       zeroth(1),	
                       twelfth(13),
                       twenty-Fourth(25),
		       thirty-Sixth(37),
		       fourty-Eighth(49)

                   }
             ACCESS  read-write
             STATUS  mandatory
             DESCRIPTION
                "The first item number of the present group of security number."
	     --DEFAULT get-function    get-ncmPRISecOpFirstNum
             --DEFAULT set-function    set-ncmPRISecOpFirstNum

           ::= { ncmJapPRISecurOperEntry 3 }
 

         ncmJapPRISecOpListype OBJECT-TYPE
          SYNTAX INTEGER {

                       own-number(1),
                       remote-number(2)

                   }
           ACCESS read-write
           STATUS mandatory
           DESCRIPTION
           "The variable specifies the type of list: own or remote numbers."
	    --DEFAULT get-function    get-ncmPRISecOpListype
	    --DEFAULT set-function    set-ncmPRISecOpListype

           ::= { ncmJapPRISecurOperEntry 4 }


         ncmJapPRISecOpCountNum OBJECT-TYPE
           SYNTAX INTEGER 
           ACCESS read-only
           STATUS mandatory
           DESCRIPTION
           "The variable specifies the number of numbers/elements in this group."
	    --DEFAULT get-function    get-ncmPRISecOpCountNum

           ::= { ncmJapPRISecurOperEntry 5 }


         ncmJapPRISecOpClearElement OBJECT-TYPE
           SYNTAX INTEGER 
           ACCESS read-write
           STATUS mandatory
           DESCRIPTION
           "The variable specifies the element number of the number to be cleared."
	    --DEFAULT get-function    get-ncmPRISecOpClearElement
	    --DEFAULT set-function    set-ncmPRISecOpClearElement

           ::= { ncmJapPRISecurOperEntry 6 }


         ncmJapPRISecOpStatus OBJECT-TYPE
           SYNTAX INTEGER {

                       configuration-OK(1),
                       configuration-Error(2)

                   }
           ACCESS read-only
           STATUS mandatory
           DESCRIPTION
           "The variable specifies the result of writing security number."
            --DEFAULT get-function    get-ncmPRISecOpStatus

           ::= { ncmJapPRISecurOperEntry 7 }


         ncmJapPRISecOpAction OBJECT-TYPE
          SYNTAX INTEGER {

                       clear-Security-List(1),
                       set-Security-List(2)

                   }
             ACCESS  read-write
             STATUS  mandatory
             DESCRIPTION
                "The operation that involves sending the corresponding ACP command
                 to execute the specified action with all necessary parameters setup
                 properly." 

		 --DEFAULT get-function    get-ncmPRISecOpAction
		 --DEFAULT set-function    set-ncmPRISecOpAction

           ::= { ncmJapPRISecurOperEntry 8 }
 



