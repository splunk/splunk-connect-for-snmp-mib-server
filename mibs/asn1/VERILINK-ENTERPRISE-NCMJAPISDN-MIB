         NcmJapPRIIntervalEntry ::=
         SEQUENCE {
	
	   ncmJapPRIntvlNIDIndex
		 INTEGER,
           ncmJapPRIntvlLineIndex
                 INTEGER,
           ncmJapPRIntvlEndType 
                 INTEGER,
	   ncmJapPRIntvlIndex
		 INTEGER,
	   ncmJapPRIntvlTimestamp
	         INTEGER,
	   ncmJapPRIntvlSecsInCurrIntvl
		 INTEGER,
	   ncmJapPRIntvlInfoRx
	         INTEGER,
	   ncmJapPRIntvlInfoTx
	         INTEGER,
	   ncmJapPRIntvlCRCErrRx
	         INTEGER,
	   ncmJapPRIntvlInvalidFrameRx
	         INTEGER,
	   ncmJapPRIntvlFrameAbortRx
	         INTEGER,
	   ncmJapPRIntvlDISCSRx
	         INTEGER,
	   ncmJapPRIntvlDISCSTx
	         INTEGER,
	   ncmJapPRIntvlFramerRx
	         INTEGER,
	   ncmJapPRIntvlFramerTx
	         INTEGER,
	   ncmJapPRIntvlLyr3ProtErr
	         INTEGER,
	   ncmJapPRIntvlCallSetupSent
	         INTEGER,
	   ncmJapPRIntvlCallSetupSentnFail
	         INTEGER,
	   ncmJapPRIntvlCallSetupRx
	         INTEGER,
	   ncmJapPRIntvlCallSetupRxnFail
	         INTEGER,
	   ncmJapPRIntvlUnSupportMsgRx
