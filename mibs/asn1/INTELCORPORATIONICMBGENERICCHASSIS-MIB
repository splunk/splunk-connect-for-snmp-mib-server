-- Trap definitions

trap1ForPhysicalContainer TRAP-TYPE
    ENTERPRISE eEventGenerationForPhysicalContainer
    VARIABLES  {
	a116EventType,
	a116EventSeverity,
	a116IsEventState-based,
	a116EventStateKey,
	a116AssociatedGroup,
	a116EventSystem,
	a116EventSubsystem
       }

    DESCRIPTION "Container Security Breached"

    ::= 6

trap2ForPhysicalContainer TRAP-TYPE
    ENTERPRISE eEventGenerationForPhysicalContainer
    VARIABLES  {
	a116EventType,
	a116EventSeverity,
	a116IsEventState-based,
	a116EventStateKey,
	a116AssociatedGroup,
	a116EventSystem,
	a116EventSubsystem
       }

    DESCRIPTION "Container Secured"

    ::= 256


trap1ForIcmbState TRAP-TYPE
    ENTERPRISE eEventGenerationForIcmbState
    VARIABLES  {
	a1007EventType,
	a1007EventSeverity,
	a1007IsEventState-based,
	a1007EventStateKey,
	a1007AssociatedGroup,
	a1007EventSystem,
	a1007EventSubsystem
       }

    DESCRIPTION "Chassis availability change"

    ::= 1

trap1ForEifControl TRAP-TYPE
    ENTERPRISE eEventGenerationForEifControl
    VARIABLES  {
	a2002EventType,
	a2002EventSeverity,
	a2002IsEventState-based,
	a2002EventStateKey,
	a2002AssociatedGroup,
	a2002EventSystem,
	a2002EventSubsystem
       }

    DESCRIPTION "No DIM Context created"

    ::= 1

END
