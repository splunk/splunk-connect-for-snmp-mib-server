           basCmtsPktDecodeControlFilterUcd          TruthValue,
           basCmtsPktDecodeControlFilterMap          TruthValue,
           basCmtsPktDecodeControlFilterRngReq       TruthValue,
           basCmtsPktDecodeControlFilterRngRsp       TruthValue,
           basCmtsPktDecodeControlFilterRegReq       TruthValue,
           basCmtsPktDecodeControlFilterRegRsp       TruthValue,
           basCmtsPktDecodeControlFilterUccReq       TruthValue,
           basCmtsPktDecodeControlFilterUccRsp       TruthValue,
           basCmtsPktDecodeControlFilterBpkmReq      TruthValue,
           basCmtsPktDecodeControlFilterBpkmRsp      TruthValue,
           basCmtsPktDecodeControlFilterPdu          TruthValue,
	     basCmtsPktDecodeControlFilterBwReq        TruthValue
       }

