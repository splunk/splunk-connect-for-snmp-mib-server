    SYNTAX  IpAddress
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The IP address of the destination prefix. "
    ::= { adnRdssForwardingEntry  1 }

adnRdssFatMask OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           " This field represent the valid number of leading bits in the 
          prefix of this address. It is the network IP mask. "
    ::= { adnRdssForwardingEntry  2 }

