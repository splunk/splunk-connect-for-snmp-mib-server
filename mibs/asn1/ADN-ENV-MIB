    INTEGER {
        add(1),
        delete(2),
        update(3),
        internal(255)
    }

AdnRdssStatus ::=
    INTEGER {
        up(1),
        down(2)
    }

adnRdssRegistrationTable OBJECT-TYPE
    SYNTAX  SEQUENCE OF AdnRdssRegistrationEntry
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
           "This table contains an array of records to those subsystems 
         that have established IACS session with Routes Distribution."
    ::= { adnRdssMIB  1 }

