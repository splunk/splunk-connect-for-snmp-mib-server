            Counter,

        adnAtmBoardErrQRP0DMACounter
            Counter,

        adnAtmBoardErrQRP0FIFOCounter
            Counter,

        adnAtmBoardErrQRP1RAMPCounter
            Counter,

        adnAtmBoardErrQRP1DMACounter
            Counter,

        adnAtmBoardErrQRP1FIFOCounter
            Counter,

