    SYNTAX  INTEGER (-2147483648..2147483647)
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "Indicates the number of active TDP Virtual
	 Circuits in the current node."
    DEFVAL  { 0 }
    ::= { adnCvsiHssEntry  7 }

adnRdssMIB OBJECT IDENTIFIER ::= { adnEnvHss  100 }

