    SYNTAX  AdnCvsiHssEntry
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
           "An entry in the CVSI HSS Table."
    INDEX   { adnCvsiHssIndex }
    ::= { adnCvsiHssTable  1 }

AdnCvsiHssEntry ::=
    SEQUENCE {
        adnCvsiHssIndex
            INTEGER (-2147483648..2147483647),

        adnCvsiHssTraceCntl
            INTEGER,

        adnCvsiHssTraceArea
            OCTET STRING,

        adnCvsiHssCounters
            OCTET STRING,

        adnCvsiHssGlobalVars
            OCTET STRING,

        adnCvsiHssPrintCntl
            INTEGER,

        adnCvsiHssVcActive
            INTEGER (-2147483648..2147483647)
    }

