-- Rogue AP locationing
		-- 1.3.6.1.4.1.388.14.3.2.1.16.11
		wsCcRfRapLocationing OBJECT IDENTIFIER ::= { wsCcRfRap 11 }

		
-- This table gives information about the rouge APs to be used in AP locationing
		-- 1.3.6.1.4.1.388.14.3.2.1.16.11.1
		wsCcRfRapLocationingTable OBJECT-TYPE
			SYNTAX SEQUENCE OF WsCcRfRapLocationingEntry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"This table gives locationing information about the rogue APs"
			::= { wsCcRfRapLocationing 1 }

		
		-- 1.3.6.1.4.1.388.14.3.2.1.16.11.1.1
		wsCcRfRapLocationingEntry OBJECT-TYPE
			SYNTAX WsCcRfRapLocationingEntry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"An entry in the rogue AP locationing table"
			INDEX { wsCcRfRapLocationingIndex }
			::= { wsCcRfRapLocationingTable 1 }

		
		WsCcRfRapLocationingEntry ::=
			SEQUENCE { 
				wsCcRfRapLocationingIndex
					INTEGER,
				wsCcRfRapLocationingAPMac
					PhysAddress,
				wsCcRfRapLocationingRadioMac
					PhysAddress,
				wsCcRfRapLocationingSignalStrength
					Integer32,
				wsCcRfRapLocationingHeardChannel
					Integer32,
				wsCcRfRapLocationingHeardTime
					TimeTicks
			 }

-- Index of the row
		-- 1.3.6.1.4.1.388.14.3.2.1.16.11.1.1.1
		wsCcRfRapLocationingIndex OBJECT-TYPE
			SYNTAX INTEGER (1..5000)
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"Index of the row"
			::= { wsCcRfRapLocationingEntry 1 }

		
-- MAC address of the rogue AP
		-- 1.3.6.1.4.1.388.14.3.2.1.16.11.1.1.2
