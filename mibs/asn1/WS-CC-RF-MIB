		wsCcRfRadioChannelPower11nTable OBJECT-TYPE
			SYNTAX SEQUENCE OF WsCcRfRadioChannelPower11nEntry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"This table lists the channel/power/placement
				properties of 11n radio.
				
				A row in this table is an extension of a row in
				wsCcRfRadioChannelPowerTable with the relationship
				being sparse."
			::= { wsCcRfRadio 28 }

		
		-- 1.3.6.1.4.1.388.14.3.2.1.11.28.1
		wsCcRfRadioChannelPower11nEntry OBJECT-TYPE
			SYNTAX WsCcRfRadioChannelPower11nEntry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"An entry in the table."
			INDEX { wsCcRfRadioIndex }
			::= { wsCcRfRadioChannelPower11nTable 1 }

		
		WsCcRfRadioChannelPower11nEntry ::=
			SEQUENCE { 
				wsCcRfRadioCp11nChannelWidth
					INTEGER,
				wsCcRfRadioCp11nSecondaryChannel
					INTEGER
			 }

		-- 1.3.6.1.4.1.388.14.3.2.1.11.28.1.1
		wsCcRfRadioCp11nChannelWidth OBJECT-TYPE
			SYNTAX INTEGER
				{
				twentyMHz(1),
				fortyMHz(2)
				}
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"The channel width. This object is for 11n radio
				when wsCcRfRadioCpChannelMode is 'random' or 'acs'.
				
				Values of channel width are:
				20 MHz - The Legacy mode, single channel is used
				40 MHz - Primary and secondary channel chosen
				
				The default channel width is 40 MHz."
			::= { wsCcRfRadioChannelPower11nEntry 1 }

		
		-- 1.3.6.1.4.1.388.14.3.2.1.11.28.1.2
		wsCcRfRadioCp11nSecondaryChannel OBJECT-TYPE
			SYNTAX INTEGER
				{
				none(1),
				upper(2),
				lower(3)
				}
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"The choice for secondary channel. This object is for
				11n radio when wsCcRfRadioCpChannelMode is 'fixed'.
				
				Values of secondary channel are:
				none - 20 MHz channel width is used.
				upper - The upper adjacent channel is used thereby
				        doubling the channel width
				lower - The lower adjacent channel is used thereby
				        doubling the channel width.
				
				The default value is 'none'."
			::= { wsCcRfRadioChannelPower11nEntry 2 }

		
		-- 1.3.6.1.4.1.388.14.3.2.1.11.29
		wsCcRfRadioDefaultChannelPower11nTable OBJECT-TYPE
			SYNTAX SEQUENCE OF WsCcRfRadioDefaultChannelPower11nEntry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"This table lists the default channel/power/placement
				properties of 11n radio.
				
				A row in this table is an extension of a row in
				wsCcRfRadioDefaultChannelPowerTable with the
				relationship being sparse."
			::= { wsCcRfRadio 29 }

		
		-- 1.3.6.1.4.1.388.14.3.2.1.11.29.1
		wsCcRfRadioDefaultChannelPower11nEntry OBJECT-TYPE
			SYNTAX WsCcRfRadioDefaultChannelPower11nEntry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"An entry in the table."
			INDEX { wsCcRfRadioDefaultIndex }
			::= { wsCcRfRadioDefaultChannelPower11nTable 1 }

		
		WsCcRfRadioDefaultChannelPower11nEntry ::=
			SEQUENCE { 
				wsCcRfRadioDefaultCp11nChannelWidth
					INTEGER
			 }

		-- 1.3.6.1.4.1.388.14.3.2.1.11.29.1.1
