-- Smart-RF Management.
		-- 1.3.6.1.4.1.388.14.3.2.1.28
		wsCcRfSmart OBJECT IDENTIFIER ::= { wsCcRfMIB 28 }

		
		-- 1.3.6.1.4.1.388.14.3.2.1.28.1
		wsCcRfSmartEnable OBJECT IDENTIFIER ::= { wsCcRfSmart 1 }

		
		-- 1.3.6.1.4.1.388.14.3.2.1.28.1.1
		wsCcRfSmartEnableSmartRF OBJECT-TYPE
			SYNTAX TruthValue
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"Enable or disable Smart-RF functionality."
			::= { wsCcRfSmartEnable 1 }

		
		-- 1.3.6.1.4.1.388.14.3.2.1.28.1.2
		wsCcRfSmartEnableDetectorAssign OBJECT-TYPE
			SYNTAX TruthValue
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"Enable or disable automatic assignment of radio detectors."
			::= { wsCcRfSmartEnable 2 }

		
		-- 1.3.6.1.4.1.388.14.3.2.1.28.1.3
		wsCcRfSmartEnableChannelAssign OBJECT-TYPE
			SYNTAX TruthValue
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"Enable or disable automatic assignment of channels to
				working radios to avoid channel overlap and
				avoid interference from external RF sources."
			::= { wsCcRfSmartEnable 3 }

		
		-- 1.3.6.1.4.1.388.14.3.2.1.28.1.4
		wsCcRfSmartEnablePowerAssign OBJECT-TYPE
			SYNTAX TruthValue
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"Enable or disable automatic assignment of transmit power."
			::= { wsCcRfSmartEnable 4 }

		
		-- 1.3.6.1.4.1.388.14.3.2.1.28.1.5
		wsCcRfSmartEnableRescuerAssign OBJECT-TYPE
			SYNTAX TruthValue
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"Enable or disable automatic assignment of rescuers
				along with rescuing power."
			::= { wsCcRfSmartEnable 5 }

		
		-- 1.3.6.1.4.1.388.14.3.2.1.28.1.6
		wsCcRfSmartEnableFaultyRadioRecover OBJECT-TYPE
			SYNTAX TruthValue
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"Enable or disable monitoring for defective radio and
				self-healing by its rescuer."
			::= { wsCcRfSmartEnable 6 }

		
		-- 1.3.6.1.4.1.388.14.3.2.1.28.1.7
		wsCcRfSmartEnableInterferenceRecover OBJECT-TYPE
			SYNTAX TruthValue
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"Enable or disable monitoring for interference and
				self-healing it by rescuer."
			::= { wsCcRfSmartEnable 7 }

		
		-- 1.3.6.1.4.1.388.14.3.2.1.28.1.8
		wsCcRfSmartEnableCoverageHoleRecover OBJECT-TYPE
			SYNTAX TruthValue
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"Enable or disable monitoring and recovering for
				coverage hole."
			::= { wsCcRfSmartEnable 8 }

		
		-- 1.3.6.1.4.1.388.14.3.2.1.28.1.9
		wsCcRfSmartEnableScheduledCalibration OBJECT-TYPE
			SYNTAX TruthValue
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"Enable or disable scheduled Smart-RF calibration."
			::= { wsCcRfSmartEnable 9 }

		
		-- 1.3.6.1.4.1.388.14.3.2.1.28.1.10
		wsCcRfSmartEnableVerboseMode OBJECT-TYPE
			SYNTAX TruthValue
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"Enables or disables RF smart verbose mode."
			::= { wsCcRfSmartEnable 10 }

		
		-- 1.3.6.1.4.1.388.14.3.2.1.28.1.11
		wsCcRfSmartEnableExtensiveScanMode OBJECT-TYPE
			SYNTAX TruthValue
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"Enables or disables RF smart extensive scan mode."
			::= { wsCcRfSmartEnable 11 }

		
		-- 1.3.6.1.4.1.388.14.3.2.1.28.2
		wsCcRfSmartRadioTable OBJECT-TYPE
			SYNTAX SEQUENCE OF WsCcRfSmartRadioEntry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"Table of radio specific configuration for
				Smart-RF Management."
			::= { wsCcRfSmart 2 }

		
		-- 1.3.6.1.4.1.388.14.3.2.1.28.2.1
		wsCcRfSmartRadioEntry OBJECT-TYPE
			SYNTAX WsCcRfSmartRadioEntry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"An entry in the table."
			INDEX { wsCcRfSmartRadioMac }
			::= { wsCcRfSmartRadioTable 1 }

		
		WsCcRfSmartRadioEntry ::=
			SEQUENCE { 
				wsCcRfSmartRadioMac
					PhysAddress,
				wsCcRfSmartRadioCfgIndex
					Integer32,
				wsCcRfSmartRadioEnableDetectorLock
					TruthValue,
				wsCcRfSmartRadioEnableChannelLock
					TruthValue,
				wsCcRfSmartRadioEnablePowerLock
					TruthValue,
				wsCcRfSmartRadioEnableRescuerLock
					TruthValue,
				wsCcRfSmartRadioAntGain
					RadioAntennaGain,
				wsCcRfSmartRadioCovRate
					TransmitRate,
				wsCcRfSmartRadioSwichIpAddr
					IpAddress,
				wsCcRfSmartRadioCurrChannel
					Unsigned32,
				wsCcRfSmartRadioCurrPower
					INTEGER
			 }

		-- 1.3.6.1.4.1.388.14.3.2.1.28.2.1.1
