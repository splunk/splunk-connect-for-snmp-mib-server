		-- 1.3.6.1.4.1.388.14.3.2.1.14.9
		wsCcRfWlanVlanTable OBJECT-TYPE
			SYNTAX SEQUENCE OF WsCcRfWlanVlanEntry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"Table of Vlans assigned to parent Wlan"
			::= { wsCcRfWlan 9 }

		
		-- 1.3.6.1.4.1.388.14.3.2.1.14.9.1
		wsCcRfWlanVlanEntry OBJECT-TYPE
			SYNTAX WsCcRfWlanVlanEntry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"An entry in wsCcRfWlanVlanTable"
			INDEX { wsCcRfWlanIndex, wsCcRfWlanVlanIndex }
			::= { wsCcRfWlanVlanTable 1 }

		
		WsCcRfWlanVlanEntry ::=
			SEQUENCE { 
				wsCcRfWlanVlanIndex
					Integer32,
				wsCcRfWlanVlanUserLimit
					INTEGER,
				wsCcRfWlanVlanRowStatus
					AbbrevRowStatus
			 }

		-- 1.3.6.1.4.1.388.14.3.2.1.14.9.1.1
		wsCcRfWlanVlanIndex OBJECT-TYPE
			SYNTAX Integer32 (1..4094)
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"The numeric vlan ID for this vlan entry, 
				valid range is [ 1, 4094 ]."
			::= { wsCcRfWlanVlanEntry 1 }

		
		-- 1.3.6.1.4.1.388.14.3.2.1.14.9.1.2
		wsCcRfWlanVlanUserLimit OBJECT-TYPE
			SYNTAX INTEGER (0..8192)
			MAX-ACCESS read-create
			STATUS current
			DESCRIPTION
				"The maximum number of users that can be associated 
				on this VLAN of the WLAN.
				
				The value 0 has a special meaning. 
				It means no limit on this VLAN of the WLAN.
				
				Other limits, such as maximum number of users per 
				radio, etc, would still apply.
				
				Also, the maximum number of users is limited by the maximum
				number of MUs supported by the wireless switch 
				e.g. 4096 for WS5100, 8192 for RFS7000.
				The range given here is the upper limit for any WiOS platform."
			::= { wsCcRfWlanVlanEntry 2 }

		
		-- 1.3.6.1.4.1.388.14.3.2.1.14.9.1.3
		wsCcRfWlanVlanRowStatus OBJECT-TYPE
			SYNTAX AbbrevRowStatus
			MAX-ACCESS read-create
			STATUS current
			DESCRIPTION
				"At the time of row-creation, rowStatus is set to createAndGo(4).
				  
				If user does not wish to have this vlanIndex mapped to a WLAN 
				any more, user deletes the row by setting rowstatus to destroy(6).
				
				When a walk is done on this table, it will show one row for each 
				vlan-wlan mapping and the rowStatus of all those rows will 
				be active(1)."
			::= { wsCcRfWlanVlanEntry 3 }

		
		-- 1.3.6.1.4.1.388.14.3.2.1.14.10
		wsCcRfWlanMaxVlans OBJECT-TYPE
			SYNTAX Integer32
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"Maximum number of vlans per wlan."
			::= { wsCcRfWlan 10 }

		
