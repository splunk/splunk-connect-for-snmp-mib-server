		-- 1.3.6.1.4.1.388.14.3.2.1.28.2.1.10
		wsCcRfSmartRadioCurrChannel OBJECT-TYPE
			SYNTAX Unsigned32
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"The current channel (may be different from configured
				channel due to DFS etc).
				
				A value of 0 for this object implies that the radio
				is not adopted."
			::= { wsCcRfSmartRadioEntry 10 }

		
		-- 1.3.6.1.4.1.388.14.3.2.1.28.2.1.11
		wsCcRfSmartRadioCurrPower OBJECT-TYPE
			SYNTAX INTEGER (0..255)
			UNITS "dBm"
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"The output power level for the radio in dBm.
				
				A value of 0 for this object implies that the radio
				is not adopted."
			::= { wsCcRfSmartRadioEntry 11 }

		
		-- 1.3.6.1.4.1.388.14.3.2.1.28.3
		wsCcRfSmartRadioRetryThrshld OBJECT-TYPE
			SYNTAX INTEGER (0..150)
			UNITS "average retries/packet * 10"
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"Retry threshold is the average number of retries 
				per packet to cause a radio to re-run channel selection.
				
				The range in the CLI is 0.0 to 15.0. Since SNMP does not 
				support decimal numbers, the range for this object is 
				0 to 150 and the set value would be divided by 10 by the
				smart-rf module."
			::= { wsCcRfSmart 3 }

		
		-- 1.3.6.1.4.1.388.14.3.2.1.28.4
		wsCcRfSmartRescuerTable OBJECT-TYPE
			SYNTAX SEQUENCE OF WsCcRfSmartRescuerEntry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"Table to assign rescuers for radios.
				Up to 5 rescuers per radio can be assigned."
			::= { wsCcRfSmart 4 }

		
		-- 1.3.6.1.4.1.388.14.3.2.1.28.4.1
		wsCcRfSmartRescuerEntry OBJECT-TYPE
			SYNTAX WsCcRfSmartRescuerEntry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"An entry in the table."
			INDEX { wsCcRfSmartRadioMac, wsCcRfSmartRescuerMac }
			::= { wsCcRfSmartRescuerTable 1 }

		
		WsCcRfSmartRescuerEntry ::=
			SEQUENCE { 
				wsCcRfSmartRescuerMac
					PhysAddress,
				wsCcRfSmartRescuerPowerBoost
					Unsigned32,
				wsCcRfSmartRescuerAttenuation
					Unsigned32,
				wsCcRfSmartRescuerRowStatus
					AbbrevRowStatus
			 }

		-- 1.3.6.1.4.1.388.14.3.2.1.28.4.1.1
