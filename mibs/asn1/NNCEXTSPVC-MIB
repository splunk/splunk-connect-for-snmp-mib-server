       nncCeSpvcPriority,
       nncCeSpvcMaxAdminWeight,
       nncCeSpvcOperation,
       nncCeSpvcCallStatus,
       nncCeSpvcLocRerouteConfig,
       nncCeSpvcFwdTmBucketOneRate,
       nncCeSpvcFwdTmBucketOneCdvt,
       nncCeSpvcBwdTmBucketOneRate,
       nncCeSpvcBwdTmBucketOneCdvt,
       nncCeSpvcCreator,
       nncCeSpvcRowStatus
     }
   STATUS      current
   DESCRIPTION
         "Common MIB objects for configuring a Circuit Emulation-based SPVC source 
          end-point across all Alcatel CID ATM platforms."
   ::= { nncExtSpvcGroups 7 }

   nncCeSpvcDstCfgGroup OBJECT-GROUP
     OBJECTS {
       nncCeSpvcDstCfgCdvt,
       nncCeSpvcDstCfgLocRerouteConfig,
       nncCeSpvcDstCfgRowStatus
    }
   STATUS  current
   DESCRIPTION
         "Common MIB objects for configuring a Circuit Emulation SPVC 
          destination end-point across all Alcatel CID ATM platforms."
 
   ::= { nncExtSpvcGroups 8 }


   ------------------------------------------------------------------
   -- Compliance Statements
   ------------------------------------------------------------------
   nncSpvcCompliance     MODULE-COMPLIANCE
     STATUS     current
     DESCRIPTION 
           "The compliance statement for Alcatel CID SPVC MIB implementation."
     MODULE
       MANDATORY-GROUPS {
         nncCrSpvpcGroup,
         nncCrSpvpcDstCfgGroup,
         nncCrSpvccGroup,
         nncCrSpvccDstCfgGroup,
         nncFrSpvcGroup,
         nncFrSpvcDstCfgGroup,
         nncCeSpvcGroup,
         nncCeSpvcDstCfgGroup
       }
   ::= { nncExtSpvcCompliances 1 }

END -- DEFINITION OF NNCEXTSPVC-MIB

