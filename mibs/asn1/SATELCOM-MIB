--      The Controller configuration section

        ctrlParamTable OBJECT-TYPE
            SYNTAX      SEQUENCE OF CtrlParamEntry
            ACCESS      not-accessible
            STATUS      mandatory
            DESCRIPTION
                "The controller configuration table."
            ::= { controller 4 }

        ctrlParamEntry OBJECT-TYPE
            SYNTAX      CtrlParamEntry
            ACCESS      not-accessible
            STATUS      mandatory
            DESCRIPTION
                "An individual controller configuration."
            INDEX       { ctrlParamIndex }
            ::= { ctrlParamTable 1 }

        CtrlParamEntry ::= SEQUENCE {
            ctrlParamIndex
                INTEGER,
            ctrlParamName
                DisplayString,
            ctrlParamType
                INTEGER,
            ctrlParamSubType
                INTEGER,
            ctrlParamNoChannels
                INTEGER,
            ctrlParamSpeed
                INTEGER,
            ctrlParamInterfaceType
                INTEGER,
            ctrlParamPacketSize
                INTEGER,
            ctrlParamV54Modem
                INTEGER,
            ctrlParamAddress
                INTEGER,
            ctrlParamOptions
                INTEGER,
            ctrlParamInitFrame
                INTEGER,
            ctrlParamT1
                INTEGER,
            ctrlParamTries
                INTEGER,
            ctrlParamKLevel2
                INTEGER,
            ctrlParamKLevel3
                INTEGER,
            ctrlParamLineGroup
                INTEGER,
            ctrlParamLCGN
                INTEGER,
            ctrlParamLCNOffset
                INTEGER,
            ctrlParamAddressGroup
                INTEGER,
            ctrlParamFrameSequence
                INTEGER,
            ctrlParamCallTimeOut
                INTEGER,
            ctrlParamErrorThreshold
                INTEGER,
            ctrlParamLoopBarPriority
                INTEGER,
            ctrlParamExtendedCallMgmt
                INTEGER,
            ctrlParamOptionalTimers
                INTEGER,
            ctrlParamOptionalFlags
                INTEGER,
            ctrlParamTransportLevelType
                INTEGER,
            ctrlParamTransportClass
                INTEGER,
            ctrlParamDefaultWindowSize
                INTEGER,
            ctrlParamDisableFlowControl
                INTEGER,
            ctrlParamTSAPFormat
                INTEGER,
            ctrlParamMaximumTPDUSize
                INTEGER,
            ctrlParamTTRTime
                INTEGER,
            ctrlParamTWRTime
                INTEGER,
            ctrlParamDestinationAddress
                DisplayString,
            ctrlParamDSAP
                INTEGER,
            ctrlParamSSAP
                INTEGER,
            ctrlParamSDLCOptions
                INTEGER,
            ctrlParamPrimaryStation
                INTEGER,
            ctrlParamIdleDetectTime
                INTEGER,
            ctrlParamPollInterval
                INTEGER,
            ctrlParamDuplex
                INTEGER,
            ctrlParamTransmitDelay
                INTEGER,
            ctrlParamSlowPollTimer
                INTEGER,
            ctrlParamMaxCallInterval
                INTEGER,
            ctrlParamMaxCallCycles
                INTEGER,
            ctrlParamGroupPoll
                INTEGER,
            ctrlParamDelayToRTSLow
                INTEGER,
            ctrlParamDelayToCTSHigh
                INTEGER,
            ctrlParamInputSyncDeletion
            INTEGER,
            ctrlParamOutputSyncInsertion
            INTEGER,
            ctrlParamDCDFilter
            INTEGER,
            ctrlParamInhibitTimeFill
            INTEGER,
            ctrlParamDirectedBroadcasts
            INTEGER,
            ctrlParamDefaultLocalPrinter
            INTEGER,
            ctrlParamNewLANInterface
            INTEGER,
            ctrlParamNewSerialInterface
            INTEGER,
            ctrlParamDlci
            INTEGER,
            ctrlParamMode
            INTEGER,
            ctrlParamLmi
            INTEGER,
            ctrlParamKeepAliveTimer
            INTEGER,
            ctrlParamStatusEnquiryInterval
                INTEGER,
            ctrlParamBandwidth
            INTEGER,
            ctrlParamBurstCommit
            INTEGER,
            ctrlParamBurstEligibility
            INTEGER

        }

        ctrlParamIndex OBJECT-TYPE
            SYNTAX      INTEGER
            ACCESS      read-only
            STATUS      mandatory
            DESCRIPTION
                "Index
                The position of this controller in relation to 'BASE'.
                 i.e.   BASE = 0
                        NMSC = 1
                        PS01 = 2
                        ..."
            REFERENCE
                "ALL"
            ::= { ctrlParamEntry 1 }

        ctrlParamName OBJECT-TYPE
            SYNTAX      DisplayString (SIZE(4))
            ACCESS      read-write
            STATUS      mandatory
            DESCRIPTION
                "Name.
                The unique 4 character name by which this controller is
                known."
            REFERENCE
                "ALL"
            ::= { ctrlParamEntry 2 }

        ctrlParamType OBJECT-TYPE
            SYNTAX      INTEGER {
                                x25(1),
                                llc2(2),
                                null(3),
                                epci(4),
                                base(5),
                                hdlc(8),
                                qllc(9),
                                xlan(10),
                                ilan(11),
                                term(12),
                                lan(13),
                                clus(14),
                                mult(15),
                                sdlc(16),
                                ip(17),
                                ace(18),
                                v25b(19),
                                cons(20),
                                nms(21),
                                dlc(22),
                                map(23),
                                tlan(24),
                                snmp(25),
                                nfrm(26),
                                xfrm(27),
                                tip(28),
                                rout(29),
                                ppp(30),
                                ipx(31),
                                tipx(32),
                                esis(34),
                                tsis(35),
                                sip(36),
                                smdx(37),
                                dlcx(38),
