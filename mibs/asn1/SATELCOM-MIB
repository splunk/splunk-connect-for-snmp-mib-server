        megaPac-E-CPU-D-Dual-Fan-48V  OBJECT IDENTIFIER ::= { products 26 }

        megaPac-E-CPU-E-Dual-Fan-48V  OBJECT IDENTIFIER ::= { products 27 }

        megaPAC-E-cpu-E               OBJECT IDENTIFIER ::= { products 28 }

        megaPac-V-Dual-PSU            OBJECT IDENTIFIER ::= { products 29 }

