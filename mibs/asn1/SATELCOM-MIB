            SYNTAX      DisplayString (SIZE (150)) 
            ACCESS      read-only
            STATUS      mandatory
            DESCRIPTION
                "The current BILLING record data."
            ::= { base 40 }

       baseCurrentConfig OBJECT-TYPE
            SYNTAX      INTEGER 
            ACCESS      read-only
            STATUS      mandatory
            DESCRIPTION
                "A decimal number from 0000 to 9999 representing the current configuration."
            ::= { base 41 }

       baseCommandBuffer OBJECT-TYPE
            SYNTAX      DisplayString (SIZE (50)) 
            ACCESS      read-write
            STATUS      mandatory
