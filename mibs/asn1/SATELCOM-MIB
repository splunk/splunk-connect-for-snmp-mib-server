            SYNTAX      ChanParamEntry
            ACCESS      not-accessible
            STATUS      mandatory
            DESCRIPTION
                "An individual Channel."
            INDEX       { chanParamIndex }
            ::= { chanParamTable 1 }

        ChanParamEntry ::= SEQUENCE {
                chanParamIndex
