                                isdb(46),
                                oem1(47),
                                mman(48)
                                }
            ACCESS      read-write
            STATUS      mandatory
            DESCRIPTION
                "Type.
                The type of controller"
            REFERENCE
                "ALL"
            ::= { ctrlParamEntry 3 }

        ctrlParamSubType OBJECT-TYPE
            SYNTAX      INTEGER {
                                none(1),
                                sio(2),
                                xio(3),
                                auto(4),
                                gio(5),
                                pri(9),
                                scc(17),
                                vio(33),
                                rio(65)

                                }
            ACCESS      read-write
            STATUS      mandatory
            DESCRIPTION
                "Sub-Type.
                The link between the controller and its associated hardware
                type."
            REFERENCE
                "NFRM VFRM PPP QLLC SDLC X25 NULL XFRM"
            ::= { ctrlParamEntry 4 }

        ctrlParamNoChannels OBJECT-TYPE
            SYNTAX      INTEGER (0..4096)
            ACCESS      read-write
            STATUS      mandatory
            DESCRIPTION
                "Number of channels.
                The number of logical channels configured on the controller."
            REFERENCE
                "ALL"
            ::= { ctrlParamEntry 5 }

            ctrlParamSpeed OBJECT-TYPE
            SYNTAX      INTEGER {
                                bps-2000000(19),
                                bps-1000000(18),
                                bps-512000(17),
                                bps-256000(16),
                                bps-128000(15),
                                bps-64000(14),
                                bps-1200(13),
                                bps-19200(12),
                                bps-9600(11),
                                bps-7200(10),
                                bps-4800(9),
                                bps-3600(8),
                                bps-2400(7),
                                bps-2000(6),
                                bps-1800(5),
                                bps-1200(4),
                                bps-600(3),
                                bps-300(2),
                                ext-clock(1)
                                }
            ACCESS      read-write
            STATUS      mandatory
            DESCRIPTION
                "Speed.
                The clock speed of the interface."
            REFERENCE
                "NFRM VFRM PPP QLLC SDLC X25 NULL XFRM"
            ::= { ctrlParamEntry 6 }

            ctrlParamInterfaceType OBJECT-TYPE
            SYNTAX      INTEGER {
                                x21(2),
                                v35(3),
                                v24(1),
                                epci(68)
                                }
            ACCESS      read-write
            STATUS      mandatory
            DESCRIPTION
                "The serial interface type.  Note, Only applicable to MegaCard."
            REFERENCE
                "NFRM VFRM PPP QLLC SDLC X25 NULL XFRM"
            ::= { ctrlParamEntry 7 }

            ctrlParamPacketSize OBJECT-TYPE
            SYNTAX      INTEGER {
                                bytes-32(2),
                                bytes-64(3),
                                bytes-128(5),
                                bytes-256(9),
                                bytes-512(17),
                                bytes-1024(33),
                                tran(1)
                                }
            ACCESS      read-write
            STATUS      mandatory
            DESCRIPTION
                "Packet Size.
                The maximum packet size in bytes of outgoing packets,
                this parameter must be less than or equal to the 'BASE'
                statistic 'maxPacketSize'."
            REFERENCE
                "CONS DLC TERM X25 NULL HDLC"
            ::= { ctrlParamEntry 8 }

            ctrlParamV54Modem OBJECT-TYPE
            SYNTAX      INTEGER {
                                yes(61),
                                no(1)
                                }
            ACCESS      read-write
            STATUS      mandatory
            DESCRIPTION
                "V54 Modem.
                Enable V54 loop back testing. Note this does not initiate
                loop back testing, the loop back is initiated by typing
                in the 'TEST' command."
            REFERENCE
                "X25 NULL HDLC"
            ::= { ctrlParamEntry 9 }

            ctrlParamAddress OBJECT-TYPE
            SYNTAX      INTEGER {
                                dte-a(1),
                                dce-b(2)
                                }
            ACCESS      read-write
            STATUS      mandatory
            DESCRIPTION
                "Address.
                The logical address designation of the controller."
            REFERENCE
                "X25 NULL HDLC"
            ::= { ctrlParamEntry 10 }

            ctrlParamOptions OBJECT-TYPE
            SYNTAX      INTEGER {
                                none(1),
                                iso1984(16385),
                                poll(32769),
                                poll-net(33025),
                                ext(8193),
                                hold(4097),
                                stat(2049),
                                net(257),
                                dmod(1025),
                                dcd(513),
                                ui(17),
                                xidc(5),
                                xidr(9),
                                test(3),
                                dtr(2),
                                tcp(65),
                                idle(33),
                                poll-ext(40961),
                                poll-dcd(33281),
                                poll-ext-dcd(41473)
                                }
            ACCESS      read-write
            STATUS      mandatory
            DESCRIPTION
                "Options.
                A range of controller options, options may be specified
                individually or added together."
            REFERENCE
                "CONS DLC ILAN QLLC SDLC X25 NULL XLAN"
            ::= { ctrlParamEntry 11 }

            ctrlParamInitFrame OBJECT-TYPE
            SYNTAX      INTEGER {
                                sabm(48),
                                sabm-pbit(64),
                                disc(68),
                                disc-pbit(84),
--                              test(), ILAN + XLAN Only
                                none(1)
                                }
            ACCESS      read-write
            STATUS      mandatory
            DESCRIPTION
                "Initial Frame."
            REFERENCE
                "CONS DLC ILAN X25 NULL XLAN"
            ::= { ctrlParamEntry 12 }

            ctrlParamT1 OBJECT-TYPE
            SYNTAX      INTEGER (2..80)
            ACCESS      read-write
            STATUS      mandatory
            DESCRIPTION
                "T1.
                Timer for retransmission of unacknowleged frames. Specified
                in 0.1 second units."
            REFERENCE
                "CONS DLC ILAN X25 NULL XLAN HDLC"
            ::= { ctrlParamEntry 13 }

            ctrlParamTries OBJECT-TYPE
            SYNTAX      INTEGER (2..50)
            ACCESS      read-write
            STATUS      mandatory
            DESCRIPTION
                "Tries.
                The number of times a frame will be retransmitted before
                a link restart is initiated."
            REFERENCE
                "CONS DLC ILAN QLLC SDLC X25 NULL XLAN HDLC"
            ::= { ctrlParamEntry 14 }

