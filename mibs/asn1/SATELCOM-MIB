-- The Controller Group (Statistics Section)

-- The Controller Group described below relates to MegaPAC protocol
-- interfaces that support a physical or virtual interface
-- connection.  The controller Group defines all parameters available
-- in "flat table format", and as such, certain object types may or
-- may not be presented depending on the controller type being used.

        ctrlNumberConfigured OBJECT-TYPE
            SYNTAX      INTEGER
            ACCESS      read-only
            STATUS      mandatory
            DESCRIPTION
                "Total Number of Controllers Configured.
                The number of controllers in the MegaPAC which have been
                configured."
            ::= { controller 1 }

        ctrlTotalNumber OBJECT-TYPE
            SYNTAX      INTEGER
            ACCESS      read-only
            STATUS      mandatory
            DESCRIPTION
                "Total Number of Controllers.
                The TOTAL number of controllers in the MegaPAC including
                unconfigured (NULL) controllers."
            ::= { controller 2 }

        ctrlStatTable OBJECT-TYPE
            SYNTAX      SEQUENCE OF CtrlStatEntry
            ACCESS      not-accessible
            STATUS      mandatory
--          DESCRIPTION
--              ""
            ::= { controller 3 }

        ctrlStatEntry OBJECT-TYPE
            SYNTAX      CtrlStatEntry
            ACCESS      not-accessible
            STATUS      mandatory
            DESCRIPTION
                ""
            INDEX       { ctrlStatIndex }
            ::= { ctrlStatTable 1 }

        CtrlStatEntry ::= SEQUENCE {
            ctrlStatIndex
                INTEGER,
            ctrlStatName
                DisplayString,
            ctrlStatType
                INTEGER,
            ctrlStatSubType
                INTEGER,
            ctrlStatNumberChannels
                INTEGER,
            ctrlStatFirstChannelIndex
                INTEGER,
            ctrlStatCRCErrors
                Counter,
            ctrlStatTotalCallSetUps
                Counter,
            ctrlStatCurrentCalls
                INTEGER,
            ctrlStatDataIn
                Counter,
            ctrlStatDataOut
                Counter,
            ctrlStatDataFramesIn
                Counter,
            ctrlStatDataFramesOut
                Counter,
            ctrlStatDataFrameRateIn
                INTEGER,
            ctrlStatDataFrameRateOut
                INTEGER,
            ctrlStatCallsReceived
                Counter,
            ctrlStatCallsSent
                Counter,
            ctrlStatClearsReceived
                Counter,
            ctrlStatRetransmissions
                Counter,
            ctrlStatRejects
                Counter,
            ctrlStatState
                INTEGER,
            ctrlStatSubState
                INTEGER,
            ctrlStatNoTries
                Counter,
            ctrlStatOptions
                INTEGER,
            ctrlStatLCGN
                INTEGER,
            ctrlStatResetsReceived
                Counter,
            ctrlStatInterruptsReceived
                Counter,
            ctrlStatOuputQueueLength
                INTEGER,
            ctrlStatStationAddress
                DisplayString,
            ctrlStatDestinationAddress
                DisplayString,
            ctrlStatEIA
                DisplayString,
            ctrlStatSdlcOptions
                INTEGER,
            ctrlStatArpRequestRec
                INTEGER,
            ctrlStatArpRequestSent
                INTEGER,
            ctrlStatArpResponsesRec
                INTEGER,
            ctrlStatTestType
                INTEGER,
            ctrlStatTestSet
                INTEGER,
            ctrlStatTest
               INTEGER,
            ctrlStatLstate
               INTEGER,
            ctrlStatRstate
               INTEGER 
        }

        ctrlStatIndex OBJECT-TYPE
            SYNTAX      INTEGER
            ACCESS      read-only
            STATUS      mandatory
            DESCRIPTION
                "Controller index.
                The position of this controller in relation to 'BASE'.
                 i.e.   BASE = 0
                        NMSC = 1
                        PS01 = 2
                        ..."
            REFERENCE
                "ALL"
            ::= { ctrlStatEntry 1 }

        ctrlStatName OBJECT-TYPE
            SYNTAX      DisplayString (SIZE(4))
            ACCESS      read-only
            STATUS      mandatory
            DESCRIPTION
                "Controller name.
                The unique 4 character name by which this controller is
                known."
            REFERENCE
                "ALL"
            ::= { ctrlStatEntry 2 }

        ctrlStatType OBJECT-TYPE
            SYNTAX      INTEGER {
                                x25(1),
                                llc2(2),
                                null(3),
                                epci(4),
                                base (5),
                                hdlc(8),
                                qllc(9),
                                xlan(10),
                                ilan(11),
                                term(12),
                                lan(13),
                                clus(14),
                                mult(15),
                                sdlc(16),
                                ip(17),
                                ace(18),
                                v25b(19),
                                cons(20),
                                nms(21),
                                dlc(22),
                                map(23),
                                tlan(24),
                                snmp(25),
                                nfrm(26),
                                xfrm(27),
                                tip(28),
                                rout(29),
                                ppp(30),
                                ipx(31),
                                tipx(32),
                                esis(34),
                                tsis(35),
                                sip(36),
                                smdx(37),
                                dlcx(38),
                                edlc(39),
                                comp(40),
                                bond(41),
                                eses(42),
                                rlay(43),
                                lap(44),
                                isdp(45),
                                isdb(46),
                                oem1(47),
                                mman(48)


                                }
            ACCESS      read-only
            STATUS      mandatory
            DESCRIPTION
                "Controller Type.
                The type of software controller."
            REFERENCE
                "PPP X25 XFRM NFRM VFRM CONS IP TIP IPX TIPX ESIS
                DLC LAN TLAN ILAN XLAN SDLC TERM QLLC HDLC"
            ::= { ctrlStatEntry 3 }

        ctrlStatSubType OBJECT-TYPE
            SYNTAX      INTEGER {
                                none(1),
                                sio(2),
                                xio(3),
                                auto(4),
                                gio(5),
                                pri(9),
                                scc(17),
                                vio(33),
                                rio(65)
                                }
            ACCESS      read-only
            STATUS      mandatory
            DESCRIPTION
                "Sub Type.
                The serial I/O board installed."
            REFERENCE
                "PPP X25 XFRM NFRM VFRM SDLC QLLC HDLC"
            ::= { ctrlStatEntry 4 }

        ctrlStatNumberChannels OBJECT-TYPE
            SYNTAX      INTEGER
            ACCESS      read-only
            STATUS      mandatory
            DESCRIPTION
                "Channel Count.
                The number of channels assigned to a controller."
            REFERENCE
                "PPP X25 XFRM NFRM VFRM CONS IP TIP IPX TIPX ESIS
                DLC LAN TLAN ILAN XLAN SDLC TERM QLLC HDLC"
            ::= { ctrlStatEntry 5 }

        ctrlStatFirstChannelIndex OBJECT-TYPE
            SYNTAX      INTEGER
            ACCESS      read-only
            STATUS      mandatory
            DESCRIPTION
                "First Channnel.
                The 1st channel index number used by the controller
                : PPP, X.25, XFRM, NFRM VFRM, CONS, IP, TIP, IPX, TIPX,
                ESIS, DLC, LAN, TLAN, ILAN, XLAN SDLC, TERM, QLLC & HDLC"
            REFERENCE
                "PPP X25 XFRM NFRM VFRM CONS IP TIP IPX TIPX ESIS
