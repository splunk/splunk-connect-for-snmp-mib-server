            INTEGER,
                chanParamFold
            INTEGER,
                chanParamDRO
            INTEGER,
                chanParamLFI
            INTEGER,
                chanParamLFPad
            INTEGER,
                chanParamEdit
            INTEGER,
                chanParamCharDel
            INTEGER,
                chanParamBufferDel
            INTEGER,
                chanParamDisp
