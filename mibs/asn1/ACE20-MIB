atmCESConfExtnsTable OBJECT-TYPE
      SYNTAX  SEQUENCE OF AtmCESConfExtnsEntry
      MAX-ACCESS  not-accessible
      STATUS current
      DESCRIPTION
       "This table defines virtual connections between ATM port and
        DS1-CBR port."
      ::= { atmSuAtmCbr 2 }
atmCESConfExtnsEntry OBJECT-TYPE
      SYNTAX  AtmCESConfExtnsEntry
      MAX-ACCESS  not-accessible
      STATUS current
      DESCRIPTION
       "An entry defines virtual connection between ATM port and
        DS1-CBR port."
--      AUGMENTS    { atmfCESConfEntry }
      INDEX { atmfCESCbrIndex }
      ::= { atmCESConfExtnsTable 1 }
AtmCESConfExtnsEntry ::= SEQUENCE {
      atmCESLastChange
            TimeStamp,
      atmCESRxBytesPerCell
            Integer32,
      atmCESTxBytesPerCell
            Integer32,
      atmCESAal1Type
            INTEGER,
      atmCESParity
            INTEGER,
      atmCESSigType
            INTEGER,
      atmCESSigABBits
            OCTET STRING
      }
atmCESLastChange   OBJECT-TYPE
      SYNTAX TimeStamp
      MAX-ACCESS  read-only
      STATUS current
      DESCRIPTION
       "The value of sysUpTime at the time the ATM to CBR
        connection entered its mandatory operational state.
        If the mandatory state was entered prior to the last
        re-initialization of the local network management
        subsystem, then this object contains a zero value."
      ::= { atmCESConfExtnsEntry 1 }
atmCESRxBytesPerCell   OBJECT-TYPE
      SYNTAX  Integer32  (1..47) 
      MAX-ACCESS read-write
      STATUS current 
      DESCRIPTION
       "Number of bytes to be received per cell, in order to decrease the delay. "
      DEFVAL { 47 }
      ::= { atmCESConfExtnsEntry 2 }
atmCESTxBytesPerCell   OBJECT-TYPE
      SYNTAX  Integer32  (1..47) 
      MAX-ACCESS read-write
      STATUS current 
      DESCRIPTION
       "Number of bytes to be transmitted per cell, in order to decrease the delay. "
      DEFVAL { 47 }
      ::= { atmCESConfExtnsEntry 3 }
atmCESAal1Type   OBJECT-TYPE
      SYNTAX INTEGER {
          ces(1),
          dbces(2),
		  transparent(3)
      }
      MAX-ACCESS read-write
      STATUS current
      DESCRIPTION
       "Type of CBR connection: CES , DBCES or transparent ."
      ::= { atmCESConfExtnsEntry 4 }
atmCESParity   OBJECT-TYPE
      SYNTAX INTEGER 
      {
            off (1),
            on  (2)
      }
      MAX-ACCESS read-write
      STATUS current
      DESCRIPTION
      " Indicate if we'll add even parity to the pointer of the structure of CES."
      ::= { atmCESConfExtnsEntry 5 }
atmCESSigType   OBJECT-TYPE
      SYNTAX INTEGER
      {
            eAndmOrPlar    (1),
            fxoLoopStart   (2),
            fxsLoopStart   (3),
            fxoGroundStart (4),
            fxsGroundStart (5),
            r2             (6),
            other          (7)
      }
      MAX-ACCESS read-write
      STATUS current
      DESCRIPTION
       "type of signalling for a DBCES connection.
        Must be 'other' for CES connection."
      ::= { atmCESConfExtnsEntry 6 }
atmCESSigABBits   OBJECT-TYPE
      SYNTAX OCTET STRING (SIZE(1))
      MAX-ACCESS read-write
      STATUS current
      DESCRIPTION
       "This attribute is applicable only when value of atmCESSigType is other.
        Represents the AB bits for Rx/Tx and their masks in this way:
        bits 7..6: Tx AB bits value,
        bits 5..4: Tx AB bits mask,
        bits 3..2: Rx AB bits value,
        bits 1..0: Rx AB bits mask.
        All bits must be 0 for CES connection."
      ::= { atmCESConfExtnsEntry 7 }
