atmSuPortAtmVclStatsTable OBJECT-TYPE
      SYNTAX SEQUENCE OF AtmSuPortAtmVclStatsEntry
      MAX-ACCESS  not-accessible
      STATUS current
      DESCRIPTION
       "The table contains additional information about
        the ATM Vcl end points."
      ::= { atmSuPortAtm 3 }
atmSuPortAtmVclStatsEntry  OBJECT-TYPE
      SYNTAX  AtmSuPortAtmVclStatsEntry
      MAX-ACCESS   not-accessible
      STATUS  current
      DESCRIPTION
       "An entry in the ATM Vcl statistics table."
      INDEX  {ifIndex, atmVclVpi, atmVclVci }
      ::= { atmSuPortAtmVclStatsTable 1 }
AtmSuPortAtmVclStatsEntry ::= SEQUENCE {
      atmSuPortAtmVclStatsRxCells
                Counter32,
      atmSuPortAtmVclStatsRxClp0Cells
                Counter32,
      atmSuPortAtmVclStatsRxClp1Cells
                Counter32,
      atmSuPortAtmVclStatsRxDiscardsCells
                Counter32,
      atmSuPortAtmVclStatsTxCells
                Counter32,
      atmSuPortAtmVclStatsTxClp0Cells
                Counter32,
      atmSuPortAtmVclStatsTxClp1Cells
                Counter32,
      atmSuPortAtmVclStatsRxTotalAISCells 
                Counter32,
      atmSuPortAtmVclStatsRxTotalCCCells  
                Counter32,
      atmSuPortAtmVclStatsTotalSuccLoopbacks 
                Counter32,
      atmSuPortAtmVclStatsTotalFailLoopbacks 
                Counter32,
      atmSuPortAtmVclStatsRxTotalRDICells  
                Counter32,
      atmSuPortAtmVclStatsTotalLOCSeconds
                Counter32,
      atmSuPortAtmVclStatsRxTotalLoopbackRequests 
                Counter32,
      atmSuPortAtmVclStatsTxTotalAISCells 
                Counter32,
      atmSuPortAtmVclStatsTxTotalCCCells 
                Counter32,
      atmSuPortAtmVclStatsTxTotalRDICells 
                Counter32,
      atmSuPortAtmVclStatsTxTotalLoopbackResponses 
                Counter32,
      atmSuPortAtmVclStatsTxTotalLoopbackRequests 
                Counter32,
      atmSuPortAtmVclStatsRxTotalLoopbackResponses 
                Counter32
      }
atmSuPortAtmVclStatsRxCells   OBJECT-TYPE
      SYNTAX Counter32
