ACE20-MIB DEFINITIONS ::= BEGIN

   IMPORTS TimeSlots, radAtm, agnLed, agnIndication, alarmSeverity, alarmState FROM RAD-MIB
	ifAlias FROM IF-MIB
	OBJECT-TYPE, IpAddress FROM RFC1155-SMI
	NOTIFICATION-TYPE, OBJECT-IDENTITY, Counter32, Integer32 FROM SNMPv2-SMI
   TEXTUAL-CONVENTION, DisplayString, RowStatus, TimeStamp, MacAddress  FROM SNMPv2-TC
	AtmTrafficDescrParamIndex FROM ATM-MIB
	;

	--Title:     RAD's ACE20 PRODUCT MIB
--Version:   3.0
--Date:      01/06/99
--By:        Eyal Sadovsky & Isac Winter
--File:      v:\doc\rdv\mib\ver506\atm\Ace20.txt
--Last Update:    13-June-04 by Shay Harari
-- IMPORTS
--        OBJECT-TYPE , OBJECT-IDENTITY ,
--        Integer32 , Counter32              FROM SNMPv2-SMI
--        TimeStamp, 
--        TEXTUAL-CONVENTION, DisplayString  FROM SNMPv2-TC
--        ifIndex                            FROM SNMPv2-TC
--        atmVclVpi, atmVclVci,
--        AtmTrafficDescrParamIndex		   FROM ATM-MIB
--        frPVCEndptDLCIIndex                FROM FRNETSERV-MIB;
   --   TRAP-TYPE                          FROM RFC-1215
   --     radAtm, agnLed, agnIndication      FROM RAD-GEN-MIB;
   --RowStatus ::= TEXTUAL-CONVENTION
   --    STATUS       current
   --    DESCRIPTION
   --    "."
  --     SYNTAX  INTEGER {
  --        active(1),
--				  notInService(2),
--				  notReady(3),
--				  createAndGo(4),
--				  createAndWait(5),
--				  destroy(6)
--                       }
atmSu           OBJECT IDENTIFIER ::= { radAtm 4 } -- RAD ATM Service Unit
atmSuSystem   OBJECT IDENTIFIER ::= { atmSu 1 }   -- agent system level configuration
-- ***********************************************************************
--   RAD System Software Group
--
--   This table contains software verions parameters.
-- ***********************************************************************
atmSuSystemSwOperStatus OBJECT-TYPE
      SYNTAX INTEGER {
            disable (1) ,
            enable  (2)
      }
      MAX-ACCESS  read-write
      STATUS current
      DESCRIPTION
      "This value indicates if tftp-write operation (for downloading
       software from an external host) is allowed by the agent. "
      DEFVAL { enable }
      ::= { atmSuSystem 1 }
atmSuSystemSwSwitchVersionsCmd OBJECT-TYPE
      SYNTAX INTEGER {
           notActive (1) ,
           active    (2)
      }
      MAX-ACCESS   read-write
      STATUS  current
      DESCRIPTION
       "This command is used to switch beetween software versions stored
        in the flash. setting this value to active will reset the agent
        and re-activate it with the new version ( only if new version is valid).
        on get-request this value will be always notActive. "
      ::= { atmSuSystem 2 }
-- *******************************************************
-- agent Alarms definitions - two alarms table defined :
-- 1. History Table which hold a buffer contains history events.
-- 2. Mandatory Table hold information of all mandatory alarms
--
-- the following alarms ifIndex defined :
-- 0. system alarms
-- 1. physical DS1 alarms - T1/E1 layer
-- 2. ATM alarms
-- 5. ATM-DXI / Frame-Relay alarms
-- *******************************
atmSuSystemHistoryAlrTable OBJECT-TYPE
      SYNTAX SEQUENCE OF AtmSuSystemHistoryAlrEntry
      MAX-ACCESS  not-accessible
      STATUS current
      DESCRIPTION
      "buffer contain history buffer of agent alarms."
      ::= { atmSuSystem 3 }
atmSuSystemHistoryAlrEntry OBJECT-TYPE
      SYNTAX AtmSuSystemHistoryAlrEntry
      MAX-ACCESS  not-accessible
      STATUS current
      DESCRIPTION
       " Agent alarm history list entry."
      INDEX  { atmSuSystemHistoryAlrIndex }
      ::= { atmSuSystemHistoryAlrTable 1}
AtmSuSystemHistoryAlrEntry ::=
      SEQUENCE {
      atmSuSystemHistoryAlrIndex
	     Integer32,
      atmSuSystemHistoryAlrIfIndex
	     Integer32,
      atmSuSystemHistoryAlrDescription
         DisplayString,
      atmSuSystemHistoryAlrStatus
	     INTEGER,
      atmSuSystemHistoryAlrOccurrenceTime
         DisplayString,
	  atmSuSystemHistoryAlrVpi
	  	 Integer32,
	  atmSuSystemHistoryAlrVci
	  	 Integer32
      }
atmSuSystemHistoryAlrIndex OBJECT-TYPE
      SYNTAX  Integer32
      MAX-ACCESS   read-only
      STATUS  current
      DESCRIPTION
      "A unique number to identify an entry in the history alarm table."
      ::= { atmSuSystemHistoryAlrEntry 1 }
atmSuSystemHistoryAlrIfIndex OBJECT-TYPE
      SYNTAX  Integer32
      MAX-ACCESS   read-only
      STATUS  current
      DESCRIPTION
      "This value indicates the port on which the alarms occured.
       configuration(system) alarms have port index equal to 0. "
      ::= { atmSuSystemHistoryAlrEntry 2 }
atmSuSystemHistoryAlrDescription OBJECT-TYPE
      SYNTAX  DisplayString (SIZE (0..255))
      MAX-ACCESS   read-only
      STATUS  current
      DESCRIPTION
      "This attribute is a text description of the alarm.
       The alarm descipion include alarm code and alarm text."
     ::= { atmSuSystemHistoryAlrEntry 3 }
atmSuSystemHistoryAlrStatus OBJECT-TYPE
      SYNTAX  INTEGER {
            on      (1),
            off     (2),
			event   (3)
      }
      MAX-ACCESS   read-only
      STATUS  current
      DESCRIPTION
      "This attribute indicates the status of the alarm."
      ::= { atmSuSystemHistoryAlrEntry 4 }
atmSuSystemHistoryAlrOccurrenceTime OBJECT-TYPE
      SYNTAX   DisplayString
      MAX-ACCESS    read-only
      STATUS   current
      DESCRIPTION
      "This value indicates the  date and time of the occurrence 
       of the event that caused this alarm. 
       Date and time format: YYYY-MM-DD, HH:MM:SS "
      ::= { atmSuSystemHistoryAlrEntry 5 }
atmSuSystemHistoryAlrVpi  OBJECT-TYPE
      SYNTAX      Integer32
      MAX-ACCESS  read-only
      STATUS      current
      DESCRIPTION
      "This value indicates the  ATM VPI of the connection
       associated with the event that caused this alarm. 
       When not applicable, value is 0"
      ::= { atmSuSystemHistoryAlrEntry 6 }
atmSuSystemHistoryAlrVci OBJECT-TYPE
      SYNTAX        Integer32
      MAX-ACCESS    read-only
      STATUS        current
      DESCRIPTION
      "This value indicates the ATM VCI of the connection 
       associated with with the event that caused this alarm. 
       When not applicable, value is 0 "
      ::= { atmSuSystemHistoryAlrEntry 7 }
atmSuSystemClearHistoryAlrListCmd  OBJECT-TYPE
      SYNTAX  INTEGER
	      {
    		off(1),
	    	on (2)
	      }
      MAX-ACCESS   read-write
      STATUS  current
      DESCRIPTION
      "Setting this attribute to ON, enforce a  clear operation on
       the agent history event buffer .
       On reading, attribute will always get status 'off' ."
      ::= { atmSuSystem 4 }
atmSuSystemCurrentAlrTable OBJECT-TYPE
      SYNTAX SEQUENCE OF AtmSuSystemCurrentAlrEntry
      MAX-ACCESS  not-accessible
      STATUS current
      DESCRIPTION
      "table which contains all alarms Currently active ( status ON )
       in the agent."
      ::= { atmSuSystem 5 }
atmSuSystemCurrentAlrEntry OBJECT-TYPE
      SYNTAX AtmSuSystemCurrentAlrEntry
      MAX-ACCESS  not-accessible
      STATUS current
      DESCRIPTION
       " Agent alarm list entry."
      INDEX  { atmSuSystemCurrentAlrIndex }
      ::= { atmSuSystemCurrentAlrTable 1}
AtmSuSystemCurrentAlrEntry ::=
      SEQUENCE {
     atmSuSystemCurrentAlrIndex
	    Integer32,
     atmSuSystemCurrentAlrIfIndex
	    Integer32,
     atmSuSystemCurrentAlrDescription
        DisplayString
      }
atmSuSystemCurrentAlrIndex OBJECT-TYPE
      SYNTAX  Integer32
      MAX-ACCESS   read-only
      STATUS  current
      DESCRIPTION
      "A unique number to identify an entry in the Current alarms table."
      ::= { atmSuSystemCurrentAlrEntry 1 }
atmSuSystemCurrentAlrIfIndex OBJECT-TYPE
      SYNTAX  Integer32
      MAX-ACCESS   read-only
      STATUS  current
      DESCRIPTION
      "This value indicates the port on which the alarms occured.
       configuration alarms have port index equal to 0. "
      ::= { atmSuSystemCurrentAlrEntry 2 }
atmSuSystemCurrentAlrDescription OBJECT-TYPE
      SYNTAX  DisplayString (SIZE (0..255))
      MAX-ACCESS   read-only
      STATUS  current
      DESCRIPTION
      "This attribute is a text description of the alarm.
       The alarm descipion include alarm code and alarm text."
     ::= { atmSuSystemCurrentAlrEntry 3 }
-- *******************************************************
--       Agent  Config  Managers  Group
-- *******************************************************
atmSuSystemILMICommunity OBJECT-TYPE
      SYNTAX   DisplayString
      MAX-ACCESS  read-write
      STATUS   current
      DESCRIPTION
       "Community string for ILMI requests. 10 chars."
      ::= { atmSuSystem 6 }
atmSuSystemTrapMaxManagers OBJECT-TYPE
      SYNTAX Integer32
      MAX-ACCESS  read-only
      STATUS current
      DESCRIPTION
       "The maximum number of managers that can be registered
        for receiving traps and sending requests."
      DEFVAL { 10 }
      ::= { atmSuSystem 7 }
-- *******************************************************
--       Agent AAL1 / AAL2 selection
-- *******************************************************
 atmSuSystemAalSelection OBJECT-TYPE
	  SYNTAX  INTEGER
	  {
	   notApplicable (1),
	   aal2  (2),
       aal1  (3)
       }
      MAX-ACCESS  read-write
      STATUS  current
      DESCRIPTION
      "Select ATM adaptation layers AAL1 or AAL2
       Applicable for LA-140"
	  ::= { atmSuSystem 8 }
-- *******************************************************
--       Agent u-Law selection
-- *******************************************************
atmSuSystemCodingLaw OBJECT-TYPE
	  SYNTAX  INTEGER
	  {
	   notApplicable (1),
	   aLaw  (2),
       uLaw  (3)
       }
      MAX-ACCESS  read-write
      STATUS  current
      DESCRIPTION
      "This attribute indicates the Method/Law for decoding/encoding the voice
       information.
       aLaw  (2) is the standard Law for E1 links.
       uLaw  (3) is the standard Law for T1 links.
       Coding law is applicable for all the ports in the system"
	  ::= { atmSuSystem 9 }
-- *******************************************************
--       Agent ISDN selection
-- *******************************************************
atmSuSystemISDNDataLinkActivation OBJECT-TYPE
	  SYNTAX  INTEGER
	  {
	   notApplicable (1),
	   nonPermanent  (2),
       permanent     (3)
       }
      MAX-ACCESS  read-write
      STATUS  current
      DESCRIPTION
      "This attribute indicates data link aactivation for all 
       ISDN ports in the system.
       nonPermanent (2) =  non permanent data link activation
       permanent    (3) =  permanent data link activation"
	  ::= { atmSuSystem 10 }
-- *******************************************************
--       Agent ATM Jitter Buffer
-- *******************************************************
atmSuSystemJitterBuffer OBJECT-TYPE
	  SYNTAX  Integer32
      MAX-ACCESS  read-write
      STATUS  current
      DESCRIPTION
        "The field is set in ms and sets the jitter buffer size. 
         The jitter buffer adapts the size of the LA140 traffic 
         to the cdv (cell  delay variation) of the ATM network."
	  ::= { atmSuSystem 11 }
-- *******************************************************
--       Agent ATM mode of operation
-- *******************************************************
atmSuSystemOperMode OBJECT-TYPE
	  SYNTAX  INTEGER
	  {
	   notApplicable (1),
	   ima           (2),
       miniDslam     (3)
       }
      MAX-ACCESS  read-only
      STATUS  current
      DESCRIPTION
        "The field indicates device mode of operation:
         ima (2) = Inverse Multiplex ATM - ATM traffic is 
		           distributed between links in IMA group.   
         miniDSLAM (3) => each link serves as separate  
                          ATM interface. "
	  ::= { atmSuSystem 12 }
atmSuSystemDSPGain OBJECT-TYPE
 	  SYNTAX  Integer32
      MAX-ACCESS  read-write
      STATUS  current
      DESCRIPTION
      "This object indicates DSP Gain.
       Range of possible values is -12....12.
       For LA-110 suset of this range is supported. "
	  ::= { atmSuSystem 13 }
-- ****************************************************************
-- ****************************************************************
--                    agent ports definitions
-- ****************************************************************
-- ****************************************************************
atmSuPort      OBJECT IDENTIFIER ::= { atmSu 2 } -- agent ports definitions
-- ********************************
--  global data port definitions
-- ********************************
--
-- the following table uses parameters which define the physical data port
-- and they are the same for FR and ATM-DXI interfaces
--
atmSuPortDataCnfgTable OBJECT-TYPE
      SYNTAX  SEQUENCE OF AtmSuPortDataCnfgEntry
      MAX-ACCESS   not-accessible
      STATUS  current
      DESCRIPTION
       " Data Port global configuration table.
         this table include parametes which are common to the physical port
         and not depend on whether the port interface is Frame-Relay or
         ATM-DXI."
      ::= { atmSuPort 1 }
atmSuPortDataCnfgEntry OBJECT-TYPE
      SYNTAX  AtmSuPortDataCnfgEntry
      MAX-ACCESS   not-accessible
      STATUS  current
      DESCRIPTION
      "the entries at this table describes the configuration
	   of the Data-Port."
      INDEX { ifIndex }
      ::= { atmSuPortDataCnfgTable 1}
AtmSuPortDataCnfgEntry ::=
      SEQUENCE {
      atmSuPortDataApplication
	          INTEGER,
      atmSuPortDataRate
	          INTEGER,
      atmSuPortDataDCD
	          INTEGER,
      atmSuPortDataMode
	          INTEGER,
      atmSuPortDataCRC
              INTEGER,
      atmSuPortDataClock
	          INTEGER,
      atmSuPortDataMaxNoOfConns
            Integer32,
      atmSuPortDataConfNoOfConns
            Integer32,
      atmSuPortDataIdleCode
            Integer32,
	  atmSuPortDataInterfaceType
	        INTEGER,
	  atmSuPortDataClkPolarity
			INTEGER,
	  atmSuPortDataCtsStatus
			INTEGER,
	  atmSuPortDataRtsStatus
			INTEGER
        }
atmSuPortDataApplication OBJECT-TYPE
      SYNTAX  INTEGER
	      {
        frameRelay		(1),
        atmDxi			(2),
		bridgedEthernet (3),
        smdsDxi         (4),  -- for future use
        aal1Stream		(5),
		routedEthernet	(6),
		notApplicable (255)
	      }
      MAX-ACCESS   read-write
      STATUS  current
      DESCRIPTION
      "This attribute indicates the protocol application which runs
       above the physical port."
      ::= { atmSuPortDataCnfgEntry 1 }
atmSuPortDataRate OBJECT-TYPE
      SYNTAX  INTEGER
	      {
        r64KBps      (1),
        r128KBps     (2),
        r192KBps     (3),
        r256KBps     (4),
        r320KBps     (5),
        r384KBps     (6),
        r448KBps     (7),
        r512KBps     (8),
        r576KBps     (9),
        r640KBps     (10),
        r704KBps     (11),
        r768KBps     (12),
        r832KBps     (13),
        r896KBps     (14),
        r960KBps     (15),
        r1024KBps    (16),
        r1088KBps    (17),
        r1152KBps    (18),
        r1216KBps    (19),
        r1280KBps    (20),
        r1344KBps    (21),
        r1408KBps    (22),
        r1472KBps    (23),
        r1536KBps    (24),
        r1544network (25), -- take the clock from the T1 network clock for ACE-20T/ACE-2T1
        r1600KBps    (26),
        r1664KBps    (27),
        r1728KBps    (28),
        r1792KBps    (29),
        r1856KBps    (30),
        r1920KBps    (31),
        r1984KBps    (32),
        r2048KBps    (33),
        r2048network (34),  -- take the clock from the E1 network clock for ACE-20E/ACE-2E1
		r8192KBps    (35),  -- for IMXi4
		notApplicable (255)
	      }
      MAX-ACCESS   read-write
      STATUS  current
      DESCRIPTION
      "This attribute indicates the operation rate of the port.
       AmcdT1 / Ace-20T allowes rates up to 1536KBps ( options 1-12 ),
       while AmcdE1 / Ace-20E allowes all options. "
      ::= { atmSuPortDataCnfgEntry 2 }
atmSuPortDataDCD OBJECT-TYPE
      SYNTAX  INTEGER
	         {
             off (1),            -- RTS = Ignore ,DCD= Obey Remote Signal        
             on  (2),            -- RTS = Obey   ,DCD= Obey Remote Signall
	         obeyRtsDcdOn (3),   -- RTS = Obey   ,DCD= On
	         obeyRtsDcdOff (4),  -- RTS = Obey   ,DCD= Off 
	         ignoreRtsDcdOn (5), -- RTS = Ignore ,DCD= On
	         ignoreRtsDcdOff (6), -- RTS = Ignore ,DCD= Off
	         notApplicable (255)
	         }
      MAX-ACCESS   read-write
      STATUS  current
      DESCRIPTION
      "This attribute define behavior of control signals DCD and RTS 
       These signals may obey or ignore  signal from remote device. 
       The terms 'I signal' and 'C signal' are used 
       when physical data interface is  X.21 ; 
	   RTS  = C signal, DCD = I signal" 
      ::= { atmSuPortDataCnfgEntry 3 }
atmSuPortDataMode OBJECT-TYPE
      SYNTAX  INTEGER
	      {
            nrzi  (1),
            nrz   (2),
			notApplicable (255)
	      }
      MAX-ACCESS   read-only
      STATUS  deprecated
      DESCRIPTION
      "This attribute indicates the port mode of sampling data physical. "
      ::= { atmSuPortDataCnfgEntry 4 }
atmSuPortDataCRC OBJECT-TYPE
      SYNTAX  INTEGER
	      {
            cRC16  (1),
            cRC32  (2),
            noCRC  (3),
			notApplicable (255)
	      }
      MAX-ACCESS   read-write
      STATUS  current
      DESCRIPTION
      "This attribute indicates the type of CRC checking 
      on receiving/transmiting frames. "
      ::= { atmSuPortDataCnfgEntry 5 }
atmSuPortDataClock OBJECT-TYPE
      SYNTAX  INTEGER
	      {
            dce  (1),
            edce (2), -- for future use
            dte  (3),
			notApplicable (255)
	      }
      MAX-ACCESS   read-write
      STATUS  current
      DESCRIPTION
      "This attribute indicates the clock using for synchronization 
      for the data port. "
      ::= { atmSuPortDataCnfgEntry 6 }
atmSuPortDataMaxNoOfConns  OBJECT-TYPE
      SYNTAX  Integer32
      MAX-ACCESS   read-only
      STATUS  current
      DESCRIPTION
       " this object indicates the maximum number of cross-connect
         connections allowed beetween the ATM port and the Data Port
         identified by IfIndex. "
      ::= { atmSuPortDataCnfgEntry 7 }
atmSuPortDataConfNoOfConns  OBJECT-TYPE
      SYNTAX  Integer32
      MAX-ACCESS   read-only
      STATUS  current
      DESCRIPTION
       " this object indicates the configured number of cross-connect
         connections exist beetween the ATM port and the Data Port
         identified by IfIndex. "
      ::= { atmSuPortDataCnfgEntry 8 }
atmSuPortDataIdleCode  OBJECT-TYPE
      SYNTAX  Integer32 (0..255)
      MAX-ACCESS   read-write
      STATUS  current
      DESCRIPTION
       " This attribute is applicable only for application aal1Stream.
         It indicates the value to be transmitted whenever the port is idle. "
      ::= { atmSuPortDataCnfgEntry 9 }
atmSuPortDataInterfaceType OBJECT-TYPE
    SYNTAX INTEGER 
       {
       notApplicable(1),
       x21 (2),
       v35 (3),
	   v36 (4),
	   rs530 (5)
	   }
	MAX-ACCESS read-write
	STATUS current
	DESCRIPTION
	"Configureable physical layer interface type"
	::= {atmSuPortDataCnfgEntry 10 }
atmSuPortDataClkPolarity   OBJECT-TYPE     
	 SYNTAX  INTEGER
       {
       notApplicable   (1),
       normal          (2),
       inverted        (3)
       }
      MAX-ACCESS  read-write
      STATUS  current
      DESCRIPTION
      "This object indicates the clock polarity.
       For LA-110/140  this object will be applicable only for 
       X.21/V.35/data port "
      ::= {atmSuPortDataCnfgEntry 11}
atmSuPortDataCtsStatus  OBJECT-TYPE
     SYNTAX INTEGER 
       {
       notApplicable (1),
       off           (2), --down
       on            (3)
       }
     MAX-ACCESS  read-only
     STATUS  current
     DESCRIPTION
    	"CTS status - the signal state received from ATM network  
    	 (for the local DTE usage). 
    	 LA110 receiving RTS down from remote DTE device
    	  via ATM network, will change CTS signal state 
    	  to down (off) toward local DTE device"	 
     ::= { atmSuPortDataCnfgEntry 12 }
atmSuPortDataRtsStatus  OBJECT-TYPE
     SYNTAX INTEGER 
       {
       notApplicable (1),
       off           (2), --down
       on            (3)
       }
     MAX-ACCESS  read-only
     STATUS  current
     DESCRIPTION
    	"RTS status - the signal state received from local DTE
    	 (to be forwarded to ATM network).
	     LA110 receiving RTS down from the local DTE device
	     will transmit all '1' to ATM (in order to carry the 
	     signaling to remote LA110), and forward it (RTS-down) 
	     to far end through ATM cell (pointer field)."
     ::= { atmSuPortDataCnfgEntry 13 }
-- *************************************************************
--    agent physical network definitions ( T1/E1 interface )
-- *************************************************************
atmSuPortDs1   OBJECT IDENTIFIER ::= { atmSuPort 2 } -- DS1 definitions
--
-- this table include expansion to the config table in rfc1406
--
atmSuPortDs1CnfgTable OBJECT-TYPE
      SYNTAX  SEQUENCE OF AtmSuPortDs1CnfgEntry
      MAX-ACCESS   not-accessible
      STATUS  current
      DESCRIPTION
       " This table include expansion configuration to the DS1 physical port."
      ::= { atmSuPortDs1 1 }
atmSuPortDs1CnfgEntry OBJECT-TYPE
      SYNTAX  AtmSuPortDs1CnfgEntry
      MAX-ACCESS   not-accessible
      STATUS  current
      DESCRIPTION
      "the entries at this table describes the configuration
	   of the agent physical."
      INDEX  { ifIndex }
      ::= { atmSuPortDs1CnfgTable 1}
AtmSuPortDs1CnfgEntry ::=
      SEQUENCE {
      atmSuPortDs1CnfgRxSensitivity
	        INTEGER,
      atmSuPortDs1CnfgLBO
	        INTEGER,
      atmSuPortDs1CnfgRestoreTime
            INTEGER,
      atmSuPortDs1CnfgInbandMng
 	        INTEGER,
      atmSuPortDs1CnfgDedicatedTs 
            Integer32
      }
atmSuPortDs1CnfgRxSensitivity OBJECT-TYPE
      SYNTAX  INTEGER
	  {
		s12dB      (1),	 -- for E1 Interface
        s30dB      (2),	 -- for T1/E1 Interface
        s36dB      (3),	 -- for T1 Interface
		s15dB      (4),	 
		s43dB      (5)
	  }
      MAX-ACCESS   read-write
      STATUS  current
      DESCRIPTION
      "This attribute is the rx line sensetivity parameter used only for
       T1/CSU and E1/LTU interfaces."
      ::= { atmSuPortDs1CnfgEntry 1 }
atmSuPortDs1CnfgLBO OBJECT-TYPE
      SYNTAX  INTEGER
      {
        txGain0db(1),
        txGain7dot5db  (2),	 --
        txGain15db     (3),  -- for T1/CSU Interface
        txGain22dot5db (4),  --
        len0p133ft     (5),  --
        len133p266ft   (6),  --
        len266p399ft   (7),  --	for T1/DSU Interface
        len399p533ft   (8),  --
        len533p655ft   (9),  --
        unbalanceE1    (10),	 -- for E1 Interface
		balanceE1      (11)  --
      }
      MAX-ACCESS   read-write
      STATUS  current
      DESCRIPTION
      "This attribute is the line-Build-Out indicates sensitivity of the DS1 interface.
       The user can define if the agent will be CSU or DSU (for T1 only) by choosing the
       right option.
       On E1 interface this is done by hardware, so this parameter cannot be written. "
      ::= { atmSuPortDs1CnfgEntry 2 }
atmSuPortDs1CnfgRestoreTime OBJECT-TYPE
      SYNTAX  INTEGER
      {
        ccittE1               (1),
        restoreT1secFast      (2),
        restoreT10sec62411    (3)
      }
      MAX-ACCESS   read-write
      STATUS  current
      DESCRIPTION
      "Determines the time passing from the disappearance of the loss of
       synchronization condition (red alarm) to the restoration of normal
       service. The options 1secFast(2) and 10sec62411(3) for E1 interfaces
       are proprietary implementation of the T1 standard to the E1 interface."
      ::= { atmSuPortDs1CnfgEntry 3 }
atmSuPortDs1CnfgInbandMng OBJECT-TYPE
      SYNTAX  INTEGER
      {
       off         (2),
       dedicatedTs (4),	-- RAD proprietary
       --dedicatedPpp (5),
       dedicatedFr (6)
      }
      MAX-ACCESS   read-write
      STATUS  current
      DESCRIPTION
      "A unique number to identify type of 
       inband management over the link.
       For LA-104 and IMXi4, only one port may be used
       for inband management "
      ::= { atmSuPortDs1CnfgEntry 4 }
atmSuPortDs1CnfgDedicatedTs OBJECT-TYPE
      SYNTAX  Integer32
      MAX-ACCESS  read-write
      STATUS  current
      DESCRIPTION
      "A unique number to identify Dedicated Time Slot  for inband management
       of the T1/E1 port.
       It is applicable only when atmSuPortDs1CnfgInbandMng 
       selected is not off.
       1..24 - Dedicated TS of T1
       1..31 - Dedicated TS of E1
       255   - notApplicable (for agents that do not support this 
               feature)
       Other values are invalid."
	   ::= {atmSuPortDs1CnfgEntry 5}
-- *********************************************************
--          Protocol  DS1-CBR Port   Definitions
-- *********************************************************
-- this table include parameters in T1/E1 level which
-- are relevant to cbr port	only.
atmSuPortDs1CbrCnfgTable OBJECT-TYPE
      SYNTAX  SEQUENCE OF AtmSuPortDs1CbrCnfgEntry
      MAX-ACCESS   not-accessible
      STATUS  current
      DESCRIPTION
       " This table include expansion configuration to the DS1 physical port."
      ::= { atmSuPortDs1 2 }
atmSuPortDs1CbrCnfgEntry OBJECT-TYPE
      SYNTAX  AtmSuPortDs1CbrCnfgEntry
      MAX-ACCESS   not-accessible
      STATUS  current
      DESCRIPTION
      "the entries at this table describes the configuration
	   of the agent physical."
      INDEX  { ifIndex }
      ::= { atmSuPortDs1CbrCnfgTable 1}
AtmSuPortDs1CbrCnfgEntry ::=
      SEQUENCE {
      atmSuPortDs1CbrIdleCode
            Integer32,
      atmSuPortDs1CbrRxTimeSlots
	 TimeSlots,
      atmSuPortDs1CbrTxTimeSlots
	 TimeSlots,
      atmSuPortDs1CbrMaxNoOfConns
            Integer32,
      atmSuPortDs1CbrConfNoOfConns
            Integer32,
      atmSuPortDs1CbrSignallingSampleTime
            INTEGER
      }
atmSuPortDs1CbrIdleCode OBJECT-TYPE
      SYNTAX  Integer32
      MAX-ACCESS   read-write
      STATUS  current
      DESCRIPTION
      "An Idle code which sent on unused time-slots at CBR port ."
      ::= { atmSuPortDs1CbrCnfgEntry 1 }
atmSuPortDs1CbrRxTimeSlots   OBJECT-TYPE
      SYNTAX TimeSlots 
      MAX-ACCESS  read-only
      STATUS current
      DESCRIPTION
"This value indicates the time-slots used by the port for receiving.
        It uses 32 bits ( in 4 bytes octet string ) for E1 and low 24 bits for T1.
        each bit represents a time slot for receiving :
        time-slot 0  = bit 0 (LSB - most right bit in last byte),
           ...
           ...
        time-slot 31 = bit 31 (MSB - most left bit in first byte ).
        value '0' indicate unused time-slot and value '1' indicates used time-slot.
        note : time-slots '0' and '16' are used for signalling in E1 interface. "
      ::= { atmSuPortDs1CbrCnfgEntry 2 }
atmSuPortDs1CbrTxTimeSlots   OBJECT-TYPE
      SYNTAX TimeSlots 
      MAX-ACCESS  read-only
      STATUS current
      DESCRIPTION
      "This value indicates the time-slots used by the port for transmiting.
        It uses 32 bits for E1 and low 24 bits for T1.
        each bit represents a time slot for transmiting :
        time-slot 0  = bit 0 (LSB - most right bit in last byte),
           ...
           ...
        time-slot 31 = bit 31 (MSB - most left bit in first byte ).
        value '0' indicate unused time-slot and value '1' indicates used time-slot.
        note : time-slots '0' and '16' are used for signalling in E1 interface. "
      ::= { atmSuPortDs1CbrCnfgEntry 3 }
atmSuPortDs1CbrMaxNoOfConns  OBJECT-TYPE
      SYNTAX  Integer32
      MAX-ACCESS   read-only
      STATUS  current
      DESCRIPTION
       " this object indicates the maximum number of cross-connect
         connections allowed beetween the ATM port and the CBR Port
         identified by IfIndex. "
      ::= { atmSuPortDs1CbrCnfgEntry 4 }
atmSuPortDs1CbrConfNoOfConns  OBJECT-TYPE
      SYNTAX  Integer32
      MAX-ACCESS   read-only
      STATUS  current
      DESCRIPTION
       " this object indicates the configured number of cross-connect
         connections exist beetween the ATM port and the CBR Port
         identified by IfIndex. "
      ::= { atmSuPortDs1CbrCnfgEntry 5 }
atmSuPortDs1CbrSignallingSampleTime  OBJECT-TYPE
      SYNTAX  INTEGER {
                msec100  (1),
                msec200  (2),
                msec500  (3),
                msec1000 (4),
                msec1500 (5),
                msec2000 (6)
	  }
      MAX-ACCESS   read-write
      STATUS  current
      DESCRIPTION
       " determines how often the signalling of the DBCES connections are sampled
		 and thier activity determined. "
      DEFVAL { msec500 }
      ::= { atmSuPortDs1CbrCnfgEntry 6 }
-- *********************************************************
--             Protocol   ATM    Definitions
-- *********************************************************
atmSuPortAtm     OBJECT IDENTIFIER ::= { atmSuPort 3 } -- ATM expansions to RFC1695
atmSuPortAtmCnfgTable OBJECT-TYPE
      SYNTAX  SEQUENCE OF AtmSuPortAtmCnfgEntry
      MAX-ACCESS   not-accessible
      STATUS  current
      DESCRIPTION
       "This table contains global configuration to the ATM port."
      ::= { atmSuPortAtm 1 }
atmSuPortAtmCnfgEntry OBJECT-TYPE
      SYNTAX  AtmSuPortAtmCnfgEntry
      MAX-ACCESS   not-accessible
      STATUS  current
      DESCRIPTION
       "This object contains information about agent ATM configuration."
      INDEX   { ifIndex }
      ::= { atmSuPortAtmCnfgTable 1 }
AtmSuPortAtmCnfgEntry ::= SEQUENCE {
      atmSuPortAtmCnfgIdleCellCLP
             INTEGER,
      atmSuPortAtmCnfgScramble
             INTEGER,
      atmSuPortAtmSNMPVpi
             Integer32,
      atmSuPortAtmSNMPVci
             Integer32,
      atmSuPortAtmSNMPStatus
             INTEGER,
      atmSuPortAtmILMIStatus
             INTEGER,
      atmSuPortAtmSNMPIp
             IpAddress, 
      atmSuPortAtmCnfgCDVOptimization
             INTEGER,
	  atmSuPortAtmCnfgShapingMode
	         INTEGER,
	  atmSuPortAtmSNMPTrafficDescrIndex
		     AtmTrafficDescrParamIndex
    }
atmSuPortAtmCnfgIdleCellCLP OBJECT-TYPE
      SYNTAX  INTEGER  {
          clp0 (1) , --> ATM Forum
          clp1 (2)   --> ITU
      }
      MAX-ACCESS   read-write
      STATUS  current
      DESCRIPTION
     "Determines the clp bit value for transmition
      in empty time slots."
      ::= { atmSuPortAtmCnfgEntry 1 }
atmSuPortAtmCnfgScramble OBJECT-TYPE
      SYNTAX  INTEGER  {
          enable  (1) ,
          disable (2)
      }
      MAX-ACCESS   read-write
      STATUS  current
      DESCRIPTION
     "Determined by the standards if the cell will be scrambled or not."
      ::= { atmSuPortAtmCnfgEntry 2 }
atmSuPortAtmSNMPVpi  OBJECT-TYPE
      SYNTAX Integer32
      MAX-ACCESS   read-write
      STATUS  current
      DESCRIPTION
       "The Vcc Vpi for SNMP requests."
      ::= { atmSuPortAtmCnfgEntry 3 }
atmSuPortAtmSNMPVci  OBJECT-TYPE
      SYNTAX Integer32
      MAX-ACCESS   read-write
      STATUS  current
      DESCRIPTION
       "The Vcc Vci for SNMP requests."
      ::= { atmSuPortAtmCnfgEntry 4 }
atmSuPortAtmSNMPStatus  OBJECT-TYPE
      SYNTAX INTEGER   {
            active    (1) ,
            notActive (2)
      }
      MAX-ACCESS   read-write
      STATUS  current
      DESCRIPTION
       "value indicates if the SNMP Vcc is available for management."
      ::= { atmSuPortAtmCnfgEntry 5 }
atmSuPortAtmILMIStatus  OBJECT-TYPE
      SYNTAX  INTEGER   {
            active    (1) ,
            notActive (2)
      }
      MAX-ACCESS   read-write
      STATUS  current
      DESCRIPTION
       "Value indicates if agent should respond to ILMI requests from the
        Atm switch or forward it.
        note: when using Atm to Frame-Relay configuration this value should be
              active to support ILMI.
              when using Atm-AtmDxi configuration the ILMI requests should be
              moved forward to the DTE or may be answered by the DCE. "
      DEFVAL { active }
      ::= { atmSuPortAtmCnfgEntry 6 }
atmSuPortAtmSNMPIp OBJECT-TYPE
      SYNTAX  IpAddress 
      MAX-ACCESS  read-write
      STATUS  current
      DESCRIPTION
       "The interface IP address for SNMP requests."
      ::= { atmSuPortAtmCnfgEntry 7 }
atmSuPortAtmCnfgCDVOptimization  OBJECT-TYPE
      SYNTAX INTEGER   {
          notApplicable  (1),
          disable        (2),
          enable         (3)
      }
      MAX-ACCESS   read-write
      STATUS  current
      DESCRIPTION
       "value indicates if optimization of CDV (Cell Delay Varation)
         is enbabled."
      ::= { atmSuPortAtmCnfgEntry 8 }
atmSuPortAtmCnfgShapingMode	 OBJECT-TYPE
      SYNTAX INTEGER   {
          notApplicable  (1),
          sw       (2),
          hwNcits1 (3), -- HW Shaping, APC with NCITS = 1
		  hwNcits2 (4)  -- HW Shaping, APC with NCITS = 2 
		  }
      MAX-ACCESS   read-write
      STATUS  current
      DESCRIPTION
       "ATM Shaping mode.
        Three possible values:
         SW Shaping.                 
         HW Shaping - APC with NCITS = 1.
         HW Shaping - APC with NCITS = 2
         The difference between the two hardware modes
         is in the connection cell granularity 
         and maximum bandwidth (BW):
         * For NCITS=1, the granularity is of one cell
           and the max. BW is the uplink BW, 
         * For NCITS=2, the granularity is of every two cells   
           and the max. BW is half of  the uplink BW."
      ::= { atmSuPortAtmCnfgEntry 9 } 
atmSuPortAtmSNMPTrafficDescrIndex  OBJECT-TYPE
      SYNTAX       AtmTrafficDescrParamIndex
      MAX-ACCESS   read-write
      STATUS       current
      DESCRIPTION
       "The value of this object identifies the row
        in the atmTrafficDescrParamTable which
        applies to the transmit direction of the VCL."
       ::= { atmSuPortAtmCnfgEntry 10 }
-- *************************************************************
--  RAD private ATM Interface statistics table. The table adds
--  more columns to the RFC1695 atmInterfaceConfTable
-- *************************************************************
atmSuPortAtmIfStatsTable OBJECT-TYPE
      SYNTAX SEQUENCE OF AtmSuPortAtmIfStatsEntry
      MAX-ACCESS  not-accessible
      STATUS current
      DESCRIPTION
       "The table contains additional statistics information about
        the ATM interface."
      ::= { atmSuPortAtm 2 }
atmSuPortAtmIfStatsEntry OBJECT-TYPE
      SYNTAX AtmSuPortAtmIfStatsEntry
      MAX-ACCESS  not-accessible
      STATUS current
      DESCRIPTION
       "An entry in the ATM Interface statistics table."
      INDEX { ifIndex }
      ::= { atmSuPortAtmIfStatsTable 1 }
AtmSuPortAtmIfStatsEntry ::= SEQUENCE {
      atmSuPortAtmRxClp0Cells
           Counter32,
      atmSuPortAtmRxClp1Cells
           Counter32,
     atmSuPortAtmRxEfciSetCells
           Counter32,
      atmSuPortAtmRxOamCells
           Counter32,
      atmSuPortAtmTxClp0Cells
           Counter32,
      atmSuPortAtmTxClp1Cells
           Counter32,
      atmSuPortAtmTxEfciSetCells
           Counter32,
      atmSuPortAtmTxOamCells
           Counter32
      }
atmSuPortAtmRxClp0Cells   OBJECT-TYPE
      SYNTAX Counter32
      MAX-ACCESS  read-only
      STATUS current
      DESCRIPTION
       "The number of cells received with correct
        HECs and CLP=0 and not misrouted"
      ::= { atmSuPortAtmIfStatsEntry 1 }
atmSuPortAtmRxClp1Cells   OBJECT-TYPE
      SYNTAX Counter32
      MAX-ACCESS  read-only
      STATUS current
      DESCRIPTION
       "The number of cells received with correct
        HEC's and CLP=1 and not misrouted."
      ::= { atmSuPortAtmIfStatsEntry 2 }
atmSuPortAtmRxEfciSetCells   OBJECT-TYPE
      SYNTAX Counter32
      MAX-ACCESS read-only
      STATUS current
      DESCRIPTION
       "The number of cells received with EFCI bit set by
        the Agent"
      ::= { atmSuPortAtmIfStatsEntry 3 }
atmSuPortAtmRxOamCells   OBJECT-TYPE
      SYNTAX Counter32
      MAX-ACCESS  read-only
      STATUS current
      DESCRIPTION
       "The number of OAM cells received with correct HECs."
::= { atmSuPortAtmIfStatsEntry 4 }
atmSuPortAtmTxClp0Cells   OBJECT-TYPE
      SYNTAX Counter32
      MAX-ACCESS  read-only
      STATUS current
      DESCRIPTION
       "The number of cells transmitted with CLP=0."
      ::= { atmSuPortAtmIfStatsEntry 5 }
atmSuPortAtmTxClp1Cells   OBJECT-TYPE
      SYNTAX Counter32
      MAX-ACCESS  read-only
      STATUS current
      DESCRIPTION
       "The number of cells transmitted with CLP=1."
      ::= { atmSuPortAtmIfStatsEntry 6 }
atmSuPortAtmTxEfciSetCells   OBJECT-TYPE
      SYNTAX Counter32
      MAX-ACCESS read-only
      STATUS current
      DESCRIPTION
       "The number of cells transmitted with EFCI bit set by
        the Agent"
      ::= { atmSuPortAtmIfStatsEntry 7 }
atmSuPortAtmTxOamCells   OBJECT-TYPE
      SYNTAX Counter32
      MAX-ACCESS  read-only
      STATUS current
      DESCRIPTION
       "The number of transmitted OAM cells."
      ::= { atmSuPortAtmIfStatsEntry 8 }
-- **************************************************************
