-- **********************************************************
-- **********************************************************
--              Protocol   FR     Definitions
-- **********************************************************
-- **********************************************************
atmSuPortFr     OBJECT IDENTIFIER ::= { atmSuPort 4 }
-- **********************************************************
--  the Frame Relay Logical Port Group
-- **********************************************************
atmSuPortFrLportTable OBJECT-TYPE
      SYNTAX  SEQUENCE OF  AtmSuPortFrLportEntry
      MAX-ACCESS   not-accessible
      STATUS  current
      DESCRIPTION
       "The table Contains configuration parameters of the Frame-Relay
        port and has expansion to  frLportTable in RFC1604 MIB."
      ::= { atmSuPortFr 1 }
atmSuPortFrLportEntry OBJECT-TYPE
      SYNTAX  AtmSuPortFrLportEntry
      MAX-ACCESS   not-accessible
      STATUS  current
      DESCRIPTION
       "An entry in the Frame Relay Logical Port
        Information table."
      INDEX   { ifIndex }
      ::= { atmSuPortFrLportTable 1 }
AtmSuPortFrLportEntry ::=
      SEQUENCE {
         atmSuPortFrLportSNMPDlci
              Integer32,
         atmSuPortFrLportSNMPStatus
              INTEGER,
         atmSuPortFrLportVCSigProtocol
              INTEGER,
         atmSuPortFrLportVCSigProcedure
              INTEGER,
         atmSuPortFrLportCLLM
              INTEGER,
         atmSuPortFrLportSNMPIp
              IpAddress
      }
atmSuPortFrLportSNMPDlci  OBJECT-TYPE
      SYNTAX Integer32
      MAX-ACCESS   read-write
      STATUS  current
      DESCRIPTION
       "The SNMP Dlci of this entry."
      ::= { atmSuPortFrLportEntry 1 }
atmSuPortFrLportSNMPStatus  OBJECT-TYPE
      SYNTAX INTEGER   {
            active    (1) ,
            notActive (2)
      }
      MAX-ACCESS   read-write
      STATUS  current
      DESCRIPTION
       "value indicates if SNMP over FR is available for management."
      ::= { atmSuPortFrLportEntry 2 }
atmSuPortFrLportVCSigProtocol OBJECT-TYPE
      SYNTAX  INTEGER {
                none(1),
                ccittQ933A(2),
                ansiT1617D(3),
                lmi(4)
      }
      MAX-ACCESS   read-write
      STATUS  current
      DESCRIPTION
       "The value of this object identifies the Local
        In-Channel Signaling Protocol that is used for
        this frame relay UNI/NNI logical port."
      DEFVAL { none }
      ::= { atmSuPortFrLportEntry 3 }
 atmSuPortFrLportVCSigProcedure  OBJECT-TYPE
     SYNTAX  INTEGER {
               notApplicable (1),
               u2nnet(2),
               u2nuser(3),
               bidirect(4)
             }
      MAX-ACCESS   read-write
      STATUS  current
      DESCRIPTION
       "The value of this object identifies the Local
        In-Channel Signaling Procedure that is used for
        this UNI/NNI logical port.  The UNI/NNI logical
        port can be performing user-to-network
        network-side and user-to-network user-side procedures
        or bidirectional procedures.
        this parameter gets value 'notApplicable when the VCSigProtocol is none"
      ::= { atmSuPortFrLportEntry 4 }
atmSuPortFrLportCLLM OBJECT-TYPE
      SYNTAX  INTEGER {
                active   (1),
                notActive(2)
      }
      MAX-ACCESS   read-write
      STATUS  current
      DESCRIPTION
       " indicate if the configuration is supporting CLLM ( Consolidated Link
         Layer Management ) ."
      ::= { atmSuPortFrLportEntry 5 }
