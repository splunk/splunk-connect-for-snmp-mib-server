		for mxa: 0 - p0; 1 - p1; 2 - p2; 3 - cpu; 4 - dsl0; 5 - sfp;  6 - dsl1;
		for asl: 0 - lan;                3 - cpu; 4 - dsl0; 5 - dsl1; 6 - s0; 7 - s1"
	::= { mc240AlarmTraps 20 }
	
