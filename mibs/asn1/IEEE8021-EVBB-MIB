    STATUS      current
    DESCRIPTION
        "The compliance statement for devices supporting EVBB
        as defined in IEEE 802.1Qbg."
    MODULE
        MANDATORY-GROUPS {
            ieee8021BridgeEvbConfigGroup,
            ieee8021BridgeEvbPhyPortGroup,
            ieee8021BridgeEvbPortGroup,
            ieee8021BridgeEvbVSIDBGroup,
            ieee8021BridgeEvbUAPGroup,
            ieee8021BridgeEvbCAPConfigGroup,
            ieee8021BridgeEvbbCAPConfigGroup
            
        }
        
    ::= { ieee8021BridgeEvbCompliances 1 }
    
    
