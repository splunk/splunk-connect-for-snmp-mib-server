	--DEFAULT apjam-chart-def cntHitsChart, pie, apCntInst, "Hit Information for Content Rule"
	--DEFAULT apjam-chart-item cntHitsChart, get, green
	::= {apCntEntry 18}

apCntRedirects OBJECT-TYPE
	SYNTAX          Counter32
	MAX-ACCESS      read-only
