	ENTERPRISE 		aiTrap_Greater
	DESCRIPTION 		"The ai channel-01 Greater Trap."
	::= 2

aiTrapG2 TRAP-TYPE
	ENTERPRISE 		aiTrap_Greater
	DESCRIPTION 		"The ai channel-02 Greater Trap."
	::= 3

aiTrapG3 TRAP-TYPE       
	ENTERPRISE 		aiTrap_Greater
	DESCRIPTION 		"The ai channel-03 Greater Trap."
	::= 4


----------------------------------------------------------------
