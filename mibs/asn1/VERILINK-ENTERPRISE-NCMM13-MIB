	--  ncm m13 24 Hour SnapShot
 
	-- The m13 24 Hour SnapShot table contains various statistics being
	-- collected for the last 24 Hours.
 
 
         ncmm13PerformanceSnapShotTable OBJECT-TYPE
             SYNTAX  SEQUENCE OF Ncmm13PerformanceSnapShotEntry
             ACCESS  not-accessible
             STATUS  mandatory
                --DEFAULT test-function   m13-test
              --DEFAULT set-function    null-set-proc
                --DEFAULT next-function   m13-next
            ::= { ncm-m13 14005 }
 
 
         ncmm13PerformanceSnapShotEntry OBJECT-TYPE
             SYNTAX  Ncmm13PerformanceSnapShotEntry
             ACCESS  not-accessible
             STATUS  mandatory
             DESCRIPTION
             "An entry in the  m13 Performance Snap Shot Entry table."
             INDEX   { ncmm13SnapShotNIDIndex, ncmm13SnapShotIndex }
            ::= { ncmm13PerformanceSnapShotTable 1 }
 
 
        Ncmm13PerformanceSnapShotEntry ::=
         SEQUENCE {
           ncmm13SnapShotNIDIndex
                INTEGER,
           ncmm13SnapShotIndex
                 INTEGER,
           ncmm13SnapShotCRCStatus
                INTEGER,
           ncmm13SnapShot
                INTEGER,
           ncmm13TimeStampSec
                INTEGER,
           ncmm13TimeStampMilliSec
                 INTEGER,
           ncmm13SnapShotSecs
                 INTEGER,
           ncmm13ResetPerfReg
                 INTEGER
                }
          
          
       ncmm13SnapShotNIDIndex OBJECT-TYPE
             SYNTAX  INTEGER (1..'7fffffff'h)
             ACCESS  read-only
             STATUS  mandatory
             DESCRIPTION
                "The object specifies the node id index of the node."
            ::= { ncmm13PerformanceSnapShotEntry 1 }
 
	
        ncmm13SnapShotIndex OBJECT-TYPE
             SYNTAX  INTEGER (1..'7fffffff'h)
             ACCESS  read-only
             STATUS  mandatory
             DESCRIPTION
                "The index value which uniquely identifies  the
                m13  interface  to which this entry is applica-
                ble.  The interface identified by a  particular
                value  of  this  index is the same interface as
                identified by the same value as a LineIndex
                object instance."
            ::= { ncmm13PerformanceSnapShotEntry 2 }
 
 
          ncmm13SnapShotCRCStatus OBJECT-TYPE
               SYNTAX INTEGER {
                       disabled (1)
               } 
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      "CRC4 Status."
              ::= { ncmm13PerformanceSnapShotEntry 3 }
 
        ncmm13SnapShot OBJECT-TYPE
            SYNTAX INTEGER {
                                no(1),
                                yes(2)
                       } 
           ACCESS read-write
           STATUS mandatory
           DESCRIPTION
           "The variable specifies the option to initiate snap shot
               command."
 
            ::= { ncmm13PerformanceSnapShotEntry 4 }
 
        ncmm13TimeStampSec OBJECT-TYPE
           SYNTAX  INTEGER (1..'7fffffff'h)
           ACCESS read-only
           STATUS mandatory
           DESCRIPTION
           "The variable specifies the Time Stamp in Seconds ."
 
           ::= { ncmm13PerformanceSnapShotEntry 5 }
 
 
        ncmm13TimeStampMilliSec OBJECT-TYPE
            SYNTAX  INTEGER (1..65535)
           ACCESS read-only
           STATUS mandatory
           DESCRIPTION
           "The variable specifies the Time Stamp in milli  Seconds ."
 
           ::= { ncmm13PerformanceSnapShotEntry  6 }


	
         ncmm13SnapShotSecs OBJECT-TYPE
            SYNTAX  INTEGER (1..65535)
           ACCESS read-only
            STATUS mandatory
           DESCRIPTION
           "The variable specifies number of seconds in the current interval."
            ::= { ncmm13PerformanceSnapShotEntry 7 }
 
         ncmm13ResetPerfReg OBJECT-TYPE
             SYNTAX  INTEGER {
                         no(1),
                         yes(2)
                     }
             ACCESS  read-write
             STATUS  mandatory
             DESCRIPTION
                "Reset Performance Monitoring registers."
            ::= { ncmm13PerformanceSnapShotEntry 8 }
 
