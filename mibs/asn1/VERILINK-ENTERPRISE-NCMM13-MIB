	VERILINK-ENTERPRISE-NCMM13-MIB

        -- set up MIB-wide defaults
        -- all functions can use %n for object name and
        -- %t for object type, %p for parent name,
        -- %d for name of default-bearing node and %% for %

          DEFINITIONS ::= BEGIN

     IMPORTS
                Gauge
                        FROM RFC1155-SMI
            	DisplayString
                     FROM RFC1213-MIB
        	OBJECT-TYPE
                     FROM RFC-1212
        	ncm-m13
             FROM VERILINK-ENTERPRISE-NCMALARM-MIB;


     --  This MIB module uses the extended OBJECT-TYPE macro as
     --  defined in RFC 1212.
     --  This MIB module uses the TRAP-TYPE macro as
     --  defined in RFC 1215.


     --   verilink                    OBJECT IDENTIFIER ::= { enterprises 321 }
     --   as2000                      OBJECT IDENTIFIER ::= { verilink 1 }
     --   ncm-m13                     OBJECT IDENTIFIER ::= { as2000 3037 }



	--
     --ncm m13 DS3 table
     --  
     ncmm13DS3PortTable OBJECT-TYPE
          SYNTAX SEQUENCE OF Ncmm13DS3PortEntry
          ACCESS not-accessible
          STATUS mandatory
          DESCRIPTION
               "The m13 DS3 Port table."
                --DEFAULT test-function   m13-test
              --DEFAULT set-function    null-set-proc
                --DEFAULT next-function   m13-next
          ::= { ncm-m13 14000 }

     ncmm13DS3PortEntry OBJECT-TYPE
          SYNTAX Ncmm13DS3PortEntry
          ACCESS not-accessible
          STATUS mandatory
          DESCRIPTION
               "An entry in the m13 information table."
          INDEX   { ncmm13DS3PortNIDIndex, ncmm13DS3PortIndex }
          ::= { ncmm13DS3PortTable 1 }

     Ncmm13DS3PortEntry ::= SEQUENCE {
             ncmm13DS3PortNIDIndex
                INTEGER,
             ncmm13DS3PortIndex
                INTEGER,
             ncmm13DS3ModeControl
                INTEGER,
             ncmm13DS3LineType
                INTEGER,
	     ncmm13DS3PerfControl
		INTEGER,
	     ncmm13DS3AlarmControl
		INTEGER,
	     ncmm13DS3FarEndControl
                INTEGER,
	     ncmm13DS3BusType
                INTEGER,
             ncmm13DS3InbandControl
                INTEGER,
	     ncmm13DS3CableLength
		INTEGER,
             ncmm13DS3EquipCode
                 DisplayString,
             ncmm13DS3LocationIDCode
                 DisplayString,
             ncmm13DS3FrameIDCode
                 DisplayString,
             ncmm13DS3UnitCode
                 DisplayString,
             ncmm13DS3FacilityIDCode
                 DisplayString,
             ncmm13DS3PortIdCode
                 DisplayString,
             ncmm13DS3GenIDCode
                 DisplayString
          }


	ncmm13DS3PortNIDIndex OBJECT-TYPE
          SYNTAX  INTEGER (1..'7fffffff'h)
          ACCESS  read-only
          STATUS  mandatory
          DESCRIPTION
                "This variable indicates the node id value of the node."
            ::= { ncmm13DS3PortEntry 1 }
 
     	ncmm13DS3PortIndex OBJECT-TYPE
          SYNTAX  INTEGER (1..65535)
          ACCESS  read-only
          STATUS  mandatory
          DESCRIPTION
               "The index value which uniquely identifies  the
                DS3  interface  to which this entry is applica-
                ble.  The interface identified by a  particular
                value  of  this  index is the same interface as
                identified by the same value as a dsx1LineIndex
                object instance."
            ::= { ncmm13DS3PortEntry 2 }
 
     	ncmm13DS3ModeControl OBJECT-TYPE
          SYNTAX  INTEGER {
                c-bit(1),
                m23-bit(2)
          }
          ACCESS  read-write
          STATUS  mandatory
          DESCRIPTION
                "The Mode Control Bit for M13."
            ::= { ncmm13DS3PortEntry 3 }
 
	 ncmm13DS3LineType  OBJECT-TYPE
          SYNTAX  INTEGER{
                bi-polar(1),
                uni-polar(2)
          }
          ACCESS  read-write
          STATUS  mandatory
          DESCRIPTION
               "The Line Type for M13."
            ::= { ncmm13DS3PortEntry 4 }
 
	 ncmm13DS3PerfControl OBJECT-TYPE
          SYNTAX  INTEGER { 
		disabled (1),
                enabled (2)
          }
          ACCESS  read-write
          STATUS  mandatory
          DESCRIPTION
                "Performance Control Enabled/Disabled for M13."
           ::= { ncmm13DS3PortEntry 5 }


	ncmm13DS3AlarmControl OBJECT-TYPE
	SYNTAX INTEGER { 
		disabled (1),
                enabled (2)
          }
          ACCESS  read-write
          STATUS  mandatory
          DESCRIPTION
                "Alarm Control Enabled/Disabled for M13."
           ::= { ncmm13DS3PortEntry 6 }

	ncmm13DS3FarEndControl OBJECT-TYPE
	SYNTAX INTEGER { 
		disabled (1),
                enabled (2)
          }
          ACCESS  read-write
          STATUS  mandatory
          DESCRIPTION
                "Far End Control Enabled/Disabled for M13."
           ::= { ncmm13DS3PortEntry 7 }

	ncmm13DS3BusType	OBJECT-TYPE
	SYNTAX INTEGER { 
		c-bus (1),
                a-bus(2)
          }
          ACCESS  read-write
          STATUS  mandatory
          DESCRIPTION
                "Inband Control Enabled/Disabled for M13."
           ::= { ncmm13DS3PortEntry 8 }



	
	ncmm13DS3InbandControl	OBJECT-TYPE
	SYNTAX INTEGER { 
		disabled (1),
                enabled (2)
          }
          ACCESS  read-write
          STATUS  mandatory
          DESCRIPTION
                "Inband Control Enabled/Disabled for M13."
           ::= { ncmm13DS3PortEntry 9 }


	
	ncmm13DS3CableLength	OBJECT-TYPE
	SYNTAX INTEGER { 
		short (1),
                long (2)
          }
          ACCESS  read-write
          STATUS  mandatory
          DESCRIPTION
                "Cable length for M13."
           ::= { ncmm13DS3PortEntry 10 }

	 
     ncmm13DS3EquipCode OBJECT-TYPE
          SYNTAX  DisplayString (SIZE (0..10))
          ACCESS  read-write
          STATUS  mandatory
          DESCRIPTION
               "This is the Far End Equipment Identification code
                that describes the specific piece of equipment.
                It is sent within the Path
                Identification Message."
          ::= { ncmm13DS3PortEntry 11 }
 
     ncmm13DS3LocationIDCode OBJECT-TYPE
          SYNTAX  DisplayString (SIZE (0..11))
          ACCESS  read-write
          STATUS  mandatory
          DESCRIPTION
               "This is the Far End Location Identification code
                that describes the specific location of the
                equipment.  It is sent within the Path
                Identification Message."
          ::= { ncmm13DS3PortEntry 12 }
 
     ncmm13DS3FrameIDCode OBJECT-TYPE
          SYNTAX  DisplayString (SIZE (0..10))
          ACCESS  read-write
          STATUS  mandatory
          DESCRIPTION
               "This is the Far End Frame Identification code
                that identifies where the equipment is located
                within a building at a given location.
                It is sent within the Path Identification Message."
          ::= { ncmm13DS3PortEntry 13 }


	ncmm13DS3UnitCode OBJECT-TYPE
          SYNTAX  DisplayString (SIZE (0..6))
          ACCESS  read-write
          STATUS  mandatory
          DESCRIPTION
               "This is the Far End code
                that identifies the equipment location within a bay.
                It is sent within the Path
                Identification Message."

          ::= { ncmm13DS3PortEntry 14 }
 
     ncmm13DS3FacilityIDCode OBJECT-TYPE
          SYNTAX  DisplayString (SIZE (0..38))
          ACCESS  read-write
          STATUS  mandatory
          DESCRIPTION
               "This code identifies a specific Far End DS3 path.
                It is sent within the Path Identification Message."
          ::= { ncmm13DS3PortEntry 15 }
 
     ncmm13DS3PortIdCode OBJECT-TYPE
          SYNTAX  DisplayString (SIZE (0..38))
          ACCESS read-write
          STATUS mandatory
          DESCRIPTION
               "Port ID."
          ::= { ncmm13DS3PortEntry 16 }
 
 
     ncmm13DS3GenIDCode OBJECT-TYPE
          SYNTAX  DisplayString (SIZE (0..38))
          ACCESS read-write
          STATUS mandatory
          DESCRIPTION
               "Test Signal Generator ID."
          ::= { ncmm13DS3PortEntry 17 }


	-- ncm m13 Status Table

 

	ncmm13StatTable OBJECT-TYPE
          SYNTAX SEQUENCE OF Ncmm13StatEntry
          ACCESS not-accessible
          STATUS mandatory
          DESCRIPTION   
               "The m13 Stats."
                --DEFAULT test-function   m13-test
              --DEFAULT set-function    null-set-proc
                --DEFAULT next-function   m13-next
          ::= { ncm-m13 14001 }

     ncmm13StatEntry OBJECT-TYPE
          SYNTAX Ncmm13StatEntry
          ACCESS not-accessible
          STATUS mandatory
          DESCRIPTION   
               "An entry in the m13 stats table."
          INDEX   { ncmm13StatNIDIndex, ncmm13StatIndex }
          ::= { ncmm13StatTable 1 }

     Ncmm13StatEntry ::= SEQUENCE {
             ncmm13StatNIDIndex
                 INTEGER,
             ncmm13StatIndex
                 INTEGER,
             ncmm13StatAIS
                 INTEGER,
             ncmm13StatIdle
                 INTEGER,
             ncmm13StatYellowAlm
                 INTEGER,
             ncmm13StatOOF
                 INTEGER,
             ncmm13StatLOS
                 INTEGER,
             ncmm13DS3NearEndLocalLoopback
                 INTEGER,
             ncmm13DS3NearEndLineLoopback
                 INTEGER,
	     ncmm13DS3FarEndLineLoopback
                 INTEGER,
	     ncmm13T1NearEndLineLoopback
		DisplayString,
	     ncmm13T1FarEndLineLoopback
		DisplayString,
	     ncmm13FarEndAllT1LineLoopback
		INTEGER,
	     ncmm13T1LocalLoopback
	        DisplayString	

                  }


	ncmm13StatNIDIndex OBJECT-TYPE
          SYNTAX  INTEGER (1..'7fffffff'h)
          ACCESS  read-only
          STATUS  mandatory
          DESCRIPTION
                "This variable indicates the node id value of the node."
            ::= { ncmm13StatEntry 1 }
               
     ncmm13StatIndex OBJECT-TYPE
          SYNTAX  INTEGER (1..65535)
          ACCESS  read-only
          STATUS  mandatory
          DESCRIPTION
               "The index value which uniquely identifies  the
                DS1  interface  to which this entry is applica-
                ble.  The interface identified by a  particular
                value  of  this  index is the same interface as
                identified by the same value as a dsx1LineIndex
                object instance."
            ::= { ncmm13StatEntry 2 }
 
     ncmm13StatAIS OBJECT-TYPE
          SYNTAX INTEGER {
                        oFF (1),
                        oN (2)
               }
          ACCESS read-only
          STATUS mandatory
          DESCRIPTION
               "Port Status m13  AIS off/on."
          ::= { ncmm13StatEntry 3 }
 
     ncmm13StatIdle OBJECT-TYPE
          SYNTAX INTEGER {
                        oFF (1),
                        oN (2)
               }
          ACCESS read-only
          STATUS mandatory
          DESCRIPTION
               "port Status m13  Idle off/on."
          ::= { ncmm13StatEntry 4 }
 
     ncmm13StatYellowAlm OBJECT-TYPE
          SYNTAX INTEGER {
                        oFF (1),
                        oN (2)
               }
          ACCESS read-only
          STATUS mandatory
          DESCRIPTION
               "Port Status m13 Yellow Alarm off/on."
          ::= { ncmm13StatEntry 5 }
 
     ncmm13StatOOF OBJECT-TYPE
          SYNTAX INTEGER {
                        oFF (1),
                        oN (2)
               }
          ACCESS read-only
          STATUS mandatory
          DESCRIPTION
               "port status m13 off/on."
          ::= { ncmm13StatEntry 6 }


	ncmm13StatLOS OBJECT-TYPE
          SYNTAX INTEGER {
                        oFF (1),
                        oN (2)
               }
          ACCESS read-only
          STATUS mandatory
          DESCRIPTION
               "Port Status m13 LOS off/on."
          ::= {  ncmm13StatEntry 7 }

	ncmm13DS3NearEndLocalLoopback OBJECT-TYPE 
          SYNTAX INTEGER {
                        oFF (1),
                        oN (2)
               }
          ACCESS read-only
          STATUS mandatory
          DESCRIPTION
               "Port Status m13 Ds3 Local Loopback off/on."
          ::= { ncmm13StatEntry 8 }
 
          ncmm13DS3NearEndLineLoopback OBJECT-TYPE
          SYNTAX INTEGER {
                        oFF (1),
                        oN (2)
               }
          ACCESS read-only
          STATUS mandatory
          DESCRIPTION
               "Port Status m13 ds3 near end Line loopback  off/on."
          ::= { ncmm13StatEntry 9 }
 
           ncmm13DS3FarEndLineLoopback OBJECT-TYPE
          SYNTAX INTEGER {
                        oFF (1),
                        oN (2)
               }
          ACCESS read-only
          STATUS mandatory
          DESCRIPTION
               "Port Status m13 ds3 far end line loopback  off/on."
          ::= { ncmm13StatEntry 10 }
 
           ncmm13T1NearEndLineLoopback OBJECT-TYPE
          SYNTAX DisplayString 
          ACCESS read-only
          STATUS mandatory
          DESCRIPTION
               "Port Status m13 T1 near end line loopback."
          ::= { ncmm13StatEntry 11 }


	ncmm13T1FarEndLineLoopback OBJECT-TYPE
          SYNTAX DisplayString 
          ACCESS read-only
          STATUS mandatory
          DESCRIPTION    
               "Port Status m13 T1 Far end line loopback."
          ::= { ncmm13StatEntry 12 }

 
          ncmm13FarEndAllT1LineLoopback OBJECT-TYPE
          SYNTAX INTEGER {
                        disabled (1),
                        enabled (2)
               }
          ACCESS read-only
          STATUS mandatory
          DESCRIPTION
               "Port Status m13 T1 Far End All T1 Line Loopback."
          ::= { ncmm13StatEntry 13 }


	ncmm13T1LocalLoopback OBJECT-TYPE
          SYNTAX DisplayString
          ACCESS read-only
          STATUS mandatory
          DESCRIPTION  
               "Port Status m13 T1 Local loopback."
          ::= { ncmm13StatEntry 14 }




    	           -- the ncm m13 Current
 
-- The ncm m13 current table contains various statistics being
-- collected for the current 15 minute interval.
 
          ncmm13CurrentTable OBJECT-TYPE
              SYNTAX  SEQUENCE OF Ncmm13CurrentEntry
              ACCESS  not-accessible
              STATUS  mandatory
              DESCRIPTION
                      "The m13 Current table."
                --DEFAULT test-function   m13-test
              --DEFAULT set-function    null-set-proc
                --DEFAULT next-function   m13-next
              ::= { ncm-m13 14002 }
 
          ncmm13CurrentEntry OBJECT-TYPE
              SYNTAX  Ncmm13CurrentEntry
              ACCESS  not-accessible
              STATUS  mandatory
              DESCRIPTION
                      "An entry in the m13 Current table."
              INDEX   { ncmm13CurrentNIDIndex, ncmm13CurrentIndex }
              ::= { ncmm13CurrentTable 1 }
 
          Ncmm13CurrentEntry ::=
              SEQUENCE {
                  ncmm13CurrentNIDIndex
                      INTEGER,
                  ncmm13CurrentIndex
                      INTEGER,
                  ncmm13CurrentCRCStatus
                      INTEGER,
                  ncmm13CurrentTimeStampSecs
                      INTEGER,
                  ncmm13CurrentTimeStampMSecs
                      INTEGER,
                  ncmm13CurrentIntervalSecsElaps
                      INTEGER,
                  ncmm13CurrentFarEndCCV
                      Gauge,
                  ncmm13CurrentFarEndCES
                      Gauge,
                  ncmm13CurrentFarEndCSES
                      Gauge,
                  ncmm13CurrentFarEndLUAS
                      Gauge,
                  ncmm13CurrentLCV
                      Gauge,
                  ncmm13CurrentLES
                      Gauge,
                  ncmm13CurrentLSES
                      Gauge,
	          ncmm13CurrentPCV
		      Gauge,	
                  ncmm13CurrentPES
                      Gauge,
                  ncmm13CurrentPSES
                      Gauge,
                  ncmm13CurrentCCV
                      Gauge,
                  ncmm13CurrentCES
                      Gauge,
                  ncmm13CurrentCSES
                      Gauge,
		  ncmm13CurrentSEFS
                      Gauge,
                  ncmm13CurrentAISS
                      Gauge,
		  ncmm13CurrentLUAS
		      Gauge,
                  ncmm13CurrentLOOS
                      Gauge,
                  ncmm13CurrentLOFS
                      Gauge
              }
 
        ncmm13CurrentNIDIndex OBJECT-TYPE
          SYNTAX  INTEGER (1..'7fffffff'h)
          ACCESS  read-only
          STATUS  mandatory
          DESCRIPTION
                "This variable indicates the node id value of the node."
            ::= { ncmm13CurrentEntry 1 }
               
          ncmm13CurrentIndex OBJECT-TYPE
              SYNTAX  INTEGER (1..65535)
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      "The index value which uniquely identifies the
                      m13 interface to which this entry is
                      applicable.  The interface identified by a
                      particular value of this index is the same
                      interface as identified by the same value an
                      ncmhdmLineIndex object instance."
              ::= { ncmm13CurrentEntry 2 }
 
          ncmm13CurrentCRCStatus OBJECT-TYPE
                SYNTAX INTEGER {
                       disabled (1)
               } 
          ACCESS read-only
          STATUS mandatory
          DESCRIPTION
               "Current CRC4 Status."
          ::= { ncmm13CurrentEntry  3  }
 
 
          ncmm13CurrentTimeStampSecs OBJECT-TYPE
              SYNTAX  INTEGER (1..65535)
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      "Current Time in seconds."
              ::= { ncmm13CurrentEntry 4 }
 
          ncmm13CurrentTimeStampMSecs OBJECT-TYPE
              SYNTAX  INTEGER (1..65535)
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      "Current Time in milliseconds."
              ::= { ncmm13CurrentEntry 5 }


	 ncmm13CurrentIntervalSecsElaps OBJECT-TYPE
              SYNTAX  INTEGER (1..65535)
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      "Seconds elapsed in the current interval."
              ::= { ncmm13CurrentEntry 6 }
 
          ncmm13CurrentFarEndCCV OBJECT-TYPE
              SYNTAX  Gauge
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      "The counter associated with the number of
                        Far End C-bit Code Violations,
                       in the current 15 minute interval."
              ::= { ncmm13CurrentEntry 7 }
               
          ncmm13CurrentFarEndCES OBJECT-TYPE
              SYNTAX  Gauge
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      "The counter associated with the number of
                        Far End C-bit Errored Seconds,
                       in the current 15 minute interval."
              ::= { ncmm13CurrentEntry 8 }
               
          ncmm13CurrentFarEndCSES OBJECT-TYPE
              SYNTAX  Gauge
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      "The counter associated with the number of
                        Far End C-bit Severely Errored Seconds,
                       in the current 15 minute interval."
 
              ::= { ncmm13CurrentEntry 9 }
 
          ncmm13CurrentFarEndLUAS OBJECT-TYPE
              SYNTAX  Gauge
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      "Far End Line Unavailable Seconds. The counter associated
                      with the number of Seconds the Far End m13 interface
                      was Out Of Service in the current 15 minute interval."
              ::= { ncmm13CurrentEntry 10 }
               
          ncmm13CurrentLCV OBJECT-TYPE
              SYNTAX  Gauge
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      "The counter associated with the number of Line
                      Code Violations, a bipolar or excesive zero violation,
                       in the current 15 minute interval."
              ::= { ncmm13CurrentEntry 11 }


	 ncmm13CurrentLES OBJECT-TYPE
              SYNTAX  Gauge
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      "Line Errored Second Total.  The counter associated
                      with the number of Seconds of in which one or more
                      LCV occurred in the current 15 minute interval."
              ::= { ncmm13CurrentEntry 12 }
               
          ncmm13CurrentLSES OBJECT-TYPE
              SYNTAX  Gauge
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      "The counter associated with the number of
                       Line Severely Errored Seconds (LSES) in
                       the current 15 minute interval."
              ::= { ncmm13CurrentEntry 13 }
               
          ncmm13CurrentPCV OBJECT-TYPE
              SYNTAX  Gauge
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      "The counter associated with the number
                      of Parity Code Violations (locally calculated P-bits
                      disagree with those received),
                       in the current 15 minute interval."
              ::= { ncmm13CurrentEntry 14 }
               
          ncmm13CurrentPES OBJECT-TYPE
              SYNTAX  Gauge
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      "The counter associated with the number of Seconds
                      having at least one Parity Code Violation,
                       in the current 15 minute interval."
              ::= { ncmm13CurrentEntry 15 }
               
          ncmm13CurrentPSES OBJECT-TYPE
              SYNTAX  Gauge
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      "The counter associated with the number of Seconds
                      having 44 or more Parity Code Violations,
                       in the current 15 minute interval."
              ::= { ncmm13CurrentEntry 16 }
               
          ncmm13CurrentCCV OBJECT-TYPE
              SYNTAX  Gauge
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      "The counter associated with the number of
                        C-bit Code Violations,
                       in the current 15 minute interval."
              ::= { ncmm13CurrentEntry 17 }


	ncmm13CurrentCES OBJECT-TYPE
              SYNTAX  Gauge
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      "The counter associated with the number of
                        C-bit Errored Seconds,
                       in the current 15 minute interval."
              ::= { ncmm13CurrentEntry 18 }
               
          ncmm13CurrentCSES OBJECT-TYPE
              SYNTAX  Gauge
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      "The counter associated with the number of
                        C-bit Severely Errored Seconds,
                       in the current 15 minute interval."
              ::= { ncmm13CurrentEntry 19 }
               
          ncmm13CurrentSEFS OBJECT-TYPE
              SYNTAX  Gauge
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      "The counter associated with the number of seconds
                      in which one or more Out of Frame occurred
                       in the current 15 minute interval."
              ::= { ncmm13CurrentEntry 20 }
               
          ncmm13CurrentAISS OBJECT-TYPE
              SYNTAX  Gauge
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      "The counter associated with the number of Seconds
                      in which one or more incoming AISS was detected
                       in the current 15 minute interval."
              ::= { ncmm13CurrentEntry 21 }

	ncmm13CurrentLUAS OBJECT-TYPE
              SYNTAX  Gauge
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      "The counter associated with the number of Seconds
                      in which one or more incoming LUAS was detected
                       in the current 15 minute interval."
              ::= { ncmm13CurrentEntry 22 }

               
          ncmm13CurrentLOOS OBJECT-TYPE
              SYNTAX  Gauge
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      "The counter associated with the number of Seconds
                      the m13 interface was Out Of Service
                      in the current 15 minute interval."
              ::= { ncmm13CurrentEntry 23 }
               
          ncmm13CurrentLOFS OBJECT-TYPE
              SYNTAX  Gauge
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      "The counter associated with the number of Seconds
                      Of system Loss Of Signal (LOS) in the current
                      15 minute interval."
              ::= { ncmm13CurrentEntry 24 }


	 
     -- ncm m13 Specific Interval table
     -- The m13 Interval table contains various statistics
     -- collected by each m13 Interface over the previous 24 hours of
     -- operation.  The past 24 hours are broken into 96 completed
     -- 15 minute intervals.
 
 
          ncmm13IntervalTable OBJECT-TYPE
              SYNTAX  SEQUENCE OF Ncmm13IntervalEntry
              ACCESS  not-accessible
              STATUS  mandatory
              DESCRIPTION
                      "The DS3/E3 Interval table."
                --DEFAULT test-function   m13-test
              --DEFAULT set-function    null-set-proc
                --DEFAULT next-function   m13interval-next
              ::= { ncm-m13 14003 }
 
          ncmm13IntervalEntry OBJECT-TYPE
              SYNTAX  Ncmm13IntervalEntry
              ACCESS  not-accessible
              STATUS  mandatory
              DESCRIPTION
                      "An entry in the m13 Interval table."
              INDEX   { ncmm13IntervalNIDIndex, ncmm13IntervalIndex, ncmm13IntervalNumber }
              ::= { ncmm13IntervalTable 1 }
 
          Ncmm13IntervalEntry ::=
              SEQUENCE {
                  ncmm13IntervalNIDIndex
                      INTEGER,
                  ncmm13IntervalIndex
                      INTEGER,
                  ncmm13IntervalNumber
                      INTEGER,
                  ncmm13IntervalFarEndCCV
                      Gauge,
                  ncmm13IntervalFarEndCES
                      Gauge,
                  ncmm13IntervalFarEndCSES
                      Gauge,
                  ncmm13IntervalFarEndLUAS
                      Gauge,
                  ncmm13IntervalLCV
                      Gauge,
                  ncmm13IntervalLES
                      Gauge,
                  ncmm13IntervalLSES
                      Gauge,
                  ncmm13IntervalPCV
                      Gauge,
                  ncmm13IntervalPES
                      Gauge,
		  ncmm13IntervalPSES
                      Gauge,
                  ncmm13IntervalCCV
                      Gauge,
                  ncmm13IntervalCES
                      Gauge,
                  ncmm13IntervalCSES
                      Gauge,
                  ncmm13IntervalSEFS
                      Gauge,
                  ncmm13IntervalAISS
                      Gauge,
		  ncmm13IntervalLUAS
                      Gauge,
		  ncmm13IntervalLOSS
		      Gauge,
		  ncmm13IntervalLOFS
		      Gauge

               }
 
        ncmm13IntervalNIDIndex OBJECT-TYPE
          SYNTAX  INTEGER (1..'7fffffff'h)
          ACCESS  read-only
          STATUS  mandatory
          DESCRIPTION
                "This variable indicates the node id value of the node."
            ::= { ncmm13IntervalEntry 1 }
               
 
          ncmm13IntervalIndex OBJECT-TYPE
              SYNTAX  INTEGER (1..65535)
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      "The index value which uniquely identifies the
                      m13 interface to which this entry is
                      applicable.  The interface identified by a
                      particular value of this index is the same
                      interface as identified by the same value an
                      ncmhdmLineIndex object instance."
              ::= { ncmm13IntervalEntry 2 }
 
         ncmm13IntervalNumber OBJECT-TYPE
             SYNTAX  INTEGER (1..96)
             ACCESS  read-only
             STATUS  mandatory
             DESCRIPTION
                "A number between 1 and 96, where 1 is the most
                recently completed 15 minute interval and 96 is
                the least recently completed 15 minutes  inter-
                val   (assuming   that  all  96  intervals  are
                valid)."
            ::= { ncmm13IntervalEntry 3 }
 
 
          ncmm13IntervalFarEndCCV OBJECT-TYPE
              SYNTAX  Gauge
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      "The counter associated with the number of
                        Far End C-bit Code Violations,
                       in the previous 15 minute interval."
              ::= { ncmm13IntervalEntry 4 }


	ncmm13IntervalFarEndCES OBJECT-TYPE
              SYNTAX  Gauge
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      "The counter associated with the number of
                        Far End C-bit Errored Seconds,
                       in the current 15 minute interval."
              ::= { ncmm13IntervalEntry 5 }
               
