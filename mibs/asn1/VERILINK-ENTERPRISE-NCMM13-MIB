                      "The 24 hour m13 Alert Threshold Table."
                --DEFAULT test-function   m13-test
              --DEFAULT set-function    null-set-proc
                --DEFAULT next-function   m13-next
              ::= { ncm-m13 14007 }
 
          ncmm13TCADayEntry OBJECT-TYPE
              SYNTAX  Ncmm13TCADayEntry
              ACCESS  not-accessible
              STATUS  mandatory
              DESCRIPTION
                      "An entry in the 24 hour m13 Alert Threshold Table."
              INDEX   { ncmm13TCADayNIDIndex,  ncmm13TCADayIndex }
              ::= { ncmm13TCADayTable 1 }
 
          Ncmm13TCADayEntry ::=
              SEQUENCE {
                  ncmm13TCADayNIDIndex
                      INTEGER,
                  ncmm13TCADayIndex
                      INTEGER,
                  ncmm13TCADayFarEndCCV
                      INTEGER,
                  ncmm13TCADayFarEndCES
                      INTEGER,
                  ncmm13TCADayFarEndCSES
                      INTEGER,
                  ncmm13TCADayFarEndLUAS
                      INTEGER,
                  ncmm13TCADayLCV
                      INTEGER,
                  ncmm13TCADayLES
                      INTEGER,
                  ncmm13TCADayLSES
                      INTEGER,
                  ncmm13TCADayPCV
                      INTEGER,
                  ncmm13TCADayPES
                      INTEGER,
                  ncmm13TCADayPSES
                      INTEGER,
                  ncmm13TCADayCCV
                      INTEGER,
                  ncmm13TCADayCES
                      INTEGER,
                  ncmm13TCADayCSES
                      INTEGER,
                  ncmm13TCADaySEFS
                      INTEGER,
                  ncmm13TCADayAISS
                      INTEGER,
                  ncmm13TCADayLUAS
                      INTEGER,
                  ncmm13TCADayLOSS
                      INTEGER,
                  ncmm13TCADayLOFS
                      INTEGER
              }


	ncmm13TCADayNIDIndex OBJECT-TYPE
          SYNTAX  INTEGER (1..'7fffffff'h)
          ACCESS  read-only
          STATUS  mandatory
          DESCRIPTION
                "This variable indicates the node id value of the node."
            ::= { ncmm13TCADayEntry 1 }
               
