              ACCESS  not-accessible
              STATUS  mandatory
              DESCRIPTION
                      "An entry in the m13 Loopback table."
              INDEX   { ncmm13DS3LPBKNIDIndex, ncmm13DS3LPBKIndex }
              ::= { ncmm13DS3LPBKTable 1 }

 	Ncmm13DS3LPBKEntry ::=
              SEQUENCE {
                  ncmm13DS3LPBKNIDIndex
                      INTEGER,
                  ncmm13DS3LPBKIndex
                      INTEGER,
                  ncmm13DS3LPBKAction
                      INTEGER
              }  
 
        ncmm13DS3LPBKNIDIndex OBJECT-TYPE
          SYNTAX  INTEGER (1..'7fffffff'h)
          ACCESS  read-only
          STATUS  mandatory
          DESCRIPTION
                "This variable indicates the node id value of the node."
            ::= { ncmm13DS3LPBKEntry 1 }
         
          ncmm13DS3LPBKIndex OBJECT-TYPE
              SYNTAX  INTEGER (1..65535)
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      "The index value which uniquely identifies the
                      m13 interface to which this entry is
                      applicable.  The interface identified by a
                      particular value of this index is the same
                      interface as identified by the same value an
                      ncmm13LineIndex object instance."
              ::= { ncmm13DS3LPBKEntry 2 }

	ncmm13DS3LPBKAction OBJECT-TYPE
