	-- the  ncm m13 Total Table
 
          ncmm13TotalTable OBJECT-TYPE
              SYNTAX  SEQUENCE OF Ncmm13TotalEntry
              ACCESS  not-accessible
              STATUS  mandatory
              DESCRIPTION
                      "The m13 Total table."
                --DEFAULT test-function   m13-test
              --DEFAULT set-function    null-set-proc
                --DEFAULT next-function   m13-next
              ::= { ncm-m13 14004 }
 
          ncmm13TotalEntry OBJECT-TYPE
              SYNTAX  Ncmm13TotalEntry
              ACCESS  not-accessible
              STATUS  mandatory
              DESCRIPTION
                      "An entry in the m13 Total table."
              INDEX   { ncmm13TotalNIDIndex, ncmm13TotalIndex }
              ::= { ncmm13TotalTable 1 }
 
 
          Ncmm13TotalEntry ::=
              SEQUENCE {
                  ncmm13TotalNIDIndex
                      INTEGER,
                  ncmm13TotalIndex
                      INTEGER,
                  ncmm13TotalCRCStatus
                      INTEGER,
                  ncmm13TotalValidInterv
                      INTEGER,
                  ncmm13TotalFarEndCCV
                      Gauge,
                  ncmm13TotalFarEndCES
                      Gauge,
                  ncmm13TotalFarEndCSES
                      Gauge,
                  ncmm13TotalFarEndLUAS
                      Gauge,
                  ncmm13TotalLCV
                      Gauge,
                  ncmm13TotalLES
                      Gauge,
                  ncmm13TotalLSES
                      Gauge,
                  ncmm13TotalPCV
                      Gauge,
                  ncmm13TotalPES
                      Gauge,
                  ncmm13TotalPSES
                      Gauge,
                  ncmm13TotalCCV
                      Gauge,
                  ncmm13TotalCES
                      Gauge,
                  ncmm13TotalCSES
                      Gauge,
                  ncmm13TotalSEFS
                      Gauge,
                  ncmm13TotalAISS
                      Gauge,
		  ncmm13TotalLUAS
		      Gauge,
		  ncmm13TotalLOSS
		      Gauge,
		  ncmm13TotalLOFS
		      Gauge
                 }
 
        ncmm13TotalNIDIndex OBJECT-TYPE
          SYNTAX  INTEGER (1..'7fffffff'h)
          ACCESS  read-only
          STATUS  mandatory
          DESCRIPTION
                "This variable indicates the node id value of the node."
            ::= { ncmm13TotalEntry 1 }
               
          ncmm13TotalIndex OBJECT-TYPE
              SYNTAX  INTEGER (1..65535)
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      "The index value which uniquely identifies the
                      m13 interface to which this entry is
                      applicable.  The interface identified by a
                      particular value of this index is the same
                      interface as identified by the same value an
                      ncmhdmLineIndex object instance."
              ::= { ncmm13TotalEntry 2 }
 
          ncmm13TotalCRCStatus OBJECT-TYPE
               SYNTAX INTEGER {
                       disabled (1)
               } 
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      "CRC4 Status."
              ::= { ncmm13TotalEntry 3 }
 
