	SYNTAX  SEQUENCE OF EnclPowerSupplyEntry
	ACCESS  not-accessible
	STATUS  optional
	DESCRIPTION
		"A List of SAF-TE Enclosure Power Supply Entries"
	::= { megaRaidMib 17 }

	enclPowerSupplyEntry OBJECT-TYPE
		SYNTAX  EnclPowerSupplyEntry 
		ACCESS  not-accessible
		STATUS  optional
		DESCRIPTION
			"An Entry in the Enclosure Power Supply Table."
		INDEX   { enclNumberPower, enclPowerSupplyIndex }
		::= { enclPowerSupplyTable 1 }

	EnclPowerSupplyEntry ::=
		SEQUENCE {
			enclNumberPower
				INTEGER,
			enclPowerSupplyIndex
				INTEGER,
			enclPowerSupplyStatus
				INTEGER
		}

