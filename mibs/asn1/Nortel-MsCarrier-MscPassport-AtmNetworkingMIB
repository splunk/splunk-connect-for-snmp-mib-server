mscAtmIfVccSrcRemoteVpiVci OBJECT-TYPE
   SYNTAX     IntegerSequence (SIZE (3..9))
   ACCESS     read-write
   STATUS     obsolete
   DESCRIPTION
     "This attribute represents the remote VPI and VCI of the soft PVC."
::= { mscAtmIfVccSrcProvEntry 2 }

mscAtmIfVccSrcCallingAddress OBJECT-TYPE
   SYNTAX     HexString (SIZE (0..20))
   ACCESS     read-write
   STATUS     mandatory
   DESCRIPTION
     "This attribute represents the calling address of the soft PVC. If it is 
      a null string, then the calling address is the address of the current 
      interface (that is, where the soft PVC originates)."
   DEFVAL     { ''H }  -- ""
::= { mscAtmIfVccSrcProvEntry 3 }

mscAtmIfVccSrcCalledAddress OBJECT-TYPE
   SYNTAX     HexString (SIZE (20))
   ACCESS     read-write
   STATUS     mandatory
   DESCRIPTION
     "This attribute represents the called (remote) address of the soft 
      PVC."
::= { mscAtmIfVccSrcProvEntry 4 }

mscAtmIfVccSrcCalledVpiVci OBJECT-TYPE
   SYNTAX     IntegerSequence (SIZE (3..10))
   ACCESS     read-write
   STATUS     mandatory
   DESCRIPTION
     "This attribute represents the remote VPI and VCI of the soft PVC."
::= { mscAtmIfVccSrcProvEntry 5 }

mscAtmIfVccSrcOperTable OBJECT-TYPE
   SYNTAX     SEQUENCE OF MscAtmIfVccSrcOperEntry
   ACCESS     not-accessible
   STATUS     mandatory
   DESCRIPTION
     "This attribute group contains the operational attributes associated 
      with the SrcPvp or SrcPvc component."
::= { mscAtmIfVccSrc 11 }

mscAtmIfVccSrcOperEntry OBJECT-TYPE
   SYNTAX     MscAtmIfVccSrcOperEntry
   ACCESS     not-accessible
   STATUS     mandatory
   DESCRIPTION
     "An entry in the mscAtmIfVccSrcOperTable."
   INDEX {
      mscAtmIfIndex,
      mscAtmIfVccIndex,
      mscAtmIfVccSrcIndex }
::= { mscAtmIfVccSrcOperTable 1 }

MscAtmIfVccSrcOperEntry ::= SEQUENCE {
   mscAtmIfVccSrcState
      INTEGER,
   mscAtmIfVccSrcRetryCount
      Counter32,
   mscAtmIfVccSrcLastFailureCauseCode
      Unsigned32,
   mscAtmIfVccSrcLastFailureDiagCode
      AsciiString }

mscAtmIfVccSrcState OBJECT-TYPE
   SYNTAX     INTEGER  {
                 active(0),
                 inactive(1) }
   ACCESS     read-only
   STATUS     mandatory
   DESCRIPTION
     "This attribute indicates the state of the soft PVP or soft PVC."
::= { mscAtmIfVccSrcOperEntry 1 }

mscAtmIfVccSrcRetryCount OBJECT-TYPE
   SYNTAX     Counter32 
   ACCESS     read-only
   STATUS     mandatory
   DESCRIPTION
     "This attribute indicates the number of failed attempts to set up the 
      soft PVP or soft PVC since the last time the connection failed."
::= { mscAtmIfVccSrcOperEntry 2 }

mscAtmIfVccSrcLastFailureCauseCode OBJECT-TYPE
   SYNTAX     Unsigned32 (0..255)
   ACCESS     read-only
   STATUS     mandatory
   DESCRIPTION
     "This attribute contains the cause code in the last transmitted 
      signalling message that contains the CAUSE information element. 
      The cause code is used to describe the reason for generating certain 
      signalling messages. 
      The default value for this attribute is set to 0."
::= { mscAtmIfVccSrcOperEntry 3 }

mscAtmIfVccSrcLastFailureDiagCode OBJECT-TYPE
   SYNTAX     AsciiString (SIZE (0..8))
   ACCESS     read-only
   STATUS     mandatory
   DESCRIPTION
     "This attribute contains the diagnostic code in the last transmitted 
      signalling message. The diagnostic code is contained in the CAUSE 
      information element and identifies an information element type or 
      timer type. The diagnostic code is present only if a procedural error 
      is detected by the signalling protocol. A diagnostic code is always 
      accompanied by the cause code. If there is no failure, this attribute 
      is set to NULL."
::= { mscAtmIfVccSrcOperEntry 4 }


