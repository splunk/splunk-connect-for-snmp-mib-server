       ::= {hneGroups 5}

-- HNE Statistic Objects  hneMIBStats

-- HNE SONET Statistic Objects (SONET-MIB Extensions)



  -- HNE SONET/SDH Path Current Table

  -- HNE SONET/SDH Path Current table contains 
  -- various statistics being collected for the 
  -- current 15 minute interval.
  
  hneSonetPathCurrentTable OBJECT-TYPE
       SYNTAX     SEQUENCE OF    HneSonetPathCurrentEntry
       MAX-ACCESS not-accessible
       STATUS     current
       DESCRIPTION
            "The HNE SONET/SDH Path Current table."
       ::= { hneMIBStats 5 } 
    
  hneSonetPathCurrentEntry OBJECT-TYPE
       SYNTAX  HneSonetPathCurrentEntry
       MAX-ACCESS  not-accessible
       STATUS  current
       DESCRIPTION
         "An entry in the HNE sonet path current table.  An entry 
          contains HNE specific sonet path information not found 
          in the SONET / SDH MIB."
       AUGMENTS { sonetPathCurrentEntry }   
       ::= { hneSonetPathCurrentTable 1}
          
       HneSonetPathCurrentEntry ::= SEQUENCE {
           hneSonetPathCurrentRxPosPointerJustCount   Gauge32,
           hneSonetPathCurrentRxNegPointerJustCount   Gauge32,
           hneSonetPathCurrentTxPosPointerJustCount   Gauge32,
           hneSonetPathCurrentTxNegPointerJustCount   Gauge32
       }
       
  hneSonetPathCurrentRxPosPointerJustCount OBJECT-TYPE
	    SYNTAX	   Gauge32
	    MAX-ACCESS	read-only
	    STATUS	   current
	    DESCRIPTION
		      "The counter associated with the number of 
		       positive pointer justification events that occurred
		       on the receive side of a SONET/SDH path 
		       in the current 15 minute interval."
       ::= { hneSonetPathCurrentEntry 1 }        
       
  hneSonetPathCurrentRxNegPointerJustCount OBJECT-TYPE
	    SYNTAX	   Gauge32
	    MAX-ACCESS	read-only
	    STATUS	   current
	    DESCRIPTION
		      "The counter associated with the number of 
		       negative pointer justification events that occurred
		       on the receive side of a SONET/SDH path 
		       in the current 15 minute interval."
       ::= { hneSonetPathCurrentEntry 2 }    

  hneSonetPathCurrentTxPosPointerJustCount OBJECT-TYPE
	    SYNTAX	   Gauge32
	    MAX-ACCESS	read-only
	    STATUS	   current
	    DESCRIPTION
		      "The counter associated with the number of 
		       positive pointer justification events that occurred
		       on the transmit side of a SONET/SDH path 
		       in the current 15 minute interval."
       ::= { hneSonetPathCurrentEntry 3 }        
       
  hneSonetPathCurrentTxNegPointerJustCount OBJECT-TYPE
	    SYNTAX	   Gauge32
	    MAX-ACCESS	read-only
	    STATUS	   current
	    DESCRIPTION
		      "The counter associated with the number of 
		       negative pointer justification events that occurred
		       on the transmit side of a SONET/SDH path 
		       in the current 15 minute interval."
       ::= { hneSonetPathCurrentEntry 4 }                   


  -- the HNE SONET/SDH Path Interval Table
   
  -- The HNE SONET/SDH Path Interval Table
  -- contains various statistics
  -- collected by each system over a maximum
  -- of the previous 24 hours of
  -- operation.  The past 24 hours may be broken into 96
  -- completed 15 minute intervals.
  -- A system is required to store at
  -- least 4 completed 15 minute interval.
  -- The default value is 32 intervals.      
  
  
  hneSonetPathIntervalTable OBJECT-TYPE
       SYNTAX     SEQUENCE OF    HneSonetPathIntervalEntry
       MAX-ACCESS not-accessible
       STATUS     current
       DESCRIPTION
            "The HNE SONET/SDH Path Interval table."
       ::= { hneMIBStats 6 } 
    
  hneSonetPathIntervalEntry OBJECT-TYPE
       SYNTAX  HneSonetPathIntervalEntry
       MAX-ACCESS  not-accessible
       STATUS  current
       DESCRIPTION
         "An entry in the HNE sonet path interval table.  An entry 
          contains HNE specific sonet path information not found 
          in the SONET / SDH MIB."
       AUGMENTS { sonetPathIntervalEntry }   
       ::= { hneSonetPathIntervalTable 1}
          
       HneSonetPathIntervalEntry ::= SEQUENCE {
           hneSonetPathIntervalRxPosPointerJustCount   Gauge32,
           hneSonetPathIntervalRxNegPointerJustCount   Gauge32,
           hneSonetPathIntervalTxPosPointerJustCount   Gauge32,
           hneSonetPathIntervalTxNegPointerJustCount   Gauge32
       }
       
  hneSonetPathIntervalRxPosPointerJustCount OBJECT-TYPE
	    SYNTAX	   Gauge32
	    MAX-ACCESS	read-only
	    STATUS	   current
	    DESCRIPTION
		      "The counter associated with the number of 
		       positive pointer justification events that occurred
		       on the receive side of a SONET/SDH path 
		       in a particular 15-minute interval 
		       in the past 24 hours."
       ::= { hneSonetPathIntervalEntry 1 }        
       
  hneSonetPathIntervalRxNegPointerJustCount OBJECT-TYPE
	    SYNTAX	   Gauge32
	    MAX-ACCESS	read-only
	    STATUS	   current
	    DESCRIPTION
		      "The counter associated with the number of 
		       negative pointer justification events that occurred
		       on the receive side of a SONET/SDH path 
		       in a particular 15-minute interval 
		       in the past 24 hours."
       ::= { hneSonetPathIntervalEntry 2 }    

  hneSonetPathIntervalTxPosPointerJustCount OBJECT-TYPE
	    SYNTAX	   Gauge32
	    MAX-ACCESS	read-only
	    STATUS	   current
	    DESCRIPTION
		      "The counter associated with the number of 
		       positive pointer justification events that occurred
		       on the transmit side of a SONET/SDH path 
		       in a particular 15-minute interval 
		       in the past 24 hours."
       ::= { hneSonetPathIntervalEntry 3 }        
       
  hneSonetPathIntervalTxNegPointerJustCount OBJECT-TYPE
	    SYNTAX	   Gauge32
	    MAX-ACCESS	read-only
	    STATUS	   current
	    DESCRIPTION
		      "The counter associated with the number of 
		       negative pointer justification events that occurred
		       on the transmit side of a SONET/SDH path 
             in a particular 15-minute interval 
		       in the past 24 hours."
       ::= { hneSonetPathIntervalEntry 4 }         

  -- HNE ATM LIU Current Table

  -- HNE ATM Line Interface Unit Current Table contains 
  -- various ATM traffic statistics on a line interface
  -- unit being collected for the 
  -- current 15 minute interval.
  
  hneAtmLiuCurrentTable OBJECT-TYPE
       SYNTAX     SEQUENCE OF    HneAtmLiuCurrentEntry
       MAX-ACCESS not-accessible
       STATUS     current
       DESCRIPTION
            "The HNE ATM Current table."
       ::= { hneMIBStats 7 } 
    
  hneAtmLiuCurrentEntry OBJECT-TYPE
       SYNTAX     HneAtmLiuCurrentEntry
       MAX-ACCESS not-accessible
       STATUS     current
       DESCRIPTION
         "An entry in the HNE line interface unit current entry table.  An entry 
          contains atm statistics for an entire line interface unit"
       INDEX   { ifIndex }    
       ::= { hneAtmLiuCurrentTable 1}
          
       HneAtmLiuCurrentEntry ::= SEQUENCE {           
           hneAtmLiuCurrentPhysLayerCellCount         Gauge32,
           hneAtmLiuCurrentRxCellCount                Counter64,
           hneAtmLiuCurrentTxCellCount                Gauge32,
           hneAtmLiuCurrentCongestionDroppedCount     Gauge32,
           hneAtmLiuCurrentInvalidCellCount           Gauge32,
           hneAtmLiuCurrentErroredOAMCellCount        Gauge32,
           hneAtmLiuCurrentValidOAMCellCount          Gauge32,
           hneAtmLiuCurrentTxOverrunCount             Gauge32,         
           hneAtmLiuCurrentMissingHeaderCellCount     Gauge32,
           hneAtmLiuCurrentNonRoutableCellCount       Gauge32,
           hneAtmLiuCurrentVcSearchFailedCount        Gauge32,
           hneAtmLiuCurrentInternalParityErrorCount   Gauge32  
       }   

  hneAtmLiuCurrentPhysLayerCellCount     OBJECT-TYPE
	    SYNTAX	   Gauge32
	    MAX-ACCESS	read-only
	    STATUS	   current
	    DESCRIPTION
		      "The counter associated with the number of  
		       physical layer cells encountered on all atm interfaces 
		       on a line interface unit in the current 15 minute interval."
       ::= { hneAtmLiuCurrentEntry 1 }         

  hneAtmLiuCurrentRxCellCount     OBJECT-TYPE
	    SYNTAX	   Counter64
	    MAX-ACCESS	read-only
	    STATUS	   current
	    DESCRIPTION
            "The counter associated with the number of cells  
		       received on all atm interfaces on a line interface unit 
		       in the current 15 minute interval."		    
       ::= { hneAtmLiuCurrentEntry 2 }    
       
  hneAtmLiuCurrentTxCellCount    OBJECT-TYPE
	    SYNTAX	   Gauge32
	    MAX-ACCESS	read-only
	    STATUS	   current
	    DESCRIPTION
		      "The counter associated with the number of cells 
		       transmitted on all atm interfaces on a line interface unit 
		       in the current 15 minute interval."
       ::= { hneAtmLiuCurrentEntry 3 }                

  hneAtmLiuCurrentCongestionDroppedCount     OBJECT-TYPE
	    SYNTAX	   Gauge32
	    MAX-ACCESS	read-only
	    STATUS	   current
	    DESCRIPTION
		      "The counter associated with the number of 
		       cells dropped due to congestion on all atm interfaces 
		       on a line interface unit in the current 15 minute interval."
       ::= { hneAtmLiuCurrentEntry 4 }                 

  hneAtmLiuCurrentInvalidCellCount    OBJECT-TYPE
	    SYNTAX	   Gauge32
	    MAX-ACCESS	read-only
	    STATUS	   current
	    DESCRIPTION
		      "The counter associated with the number of 
		       invalid cells encountered on all atm interfaces on a line 
		       interface unit in the current 15 minute interval."
       ::= { hneAtmLiuCurrentEntry 5 }                 

  hneAtmLiuCurrentErroredOAMCellCount    OBJECT-TYPE
	    SYNTAX	   Gauge32
	    MAX-ACCESS	read-only
	    STATUS	   current
	    DESCRIPTION
		      "The counter associated with the number of 
		       errored OAM cells encountered on all atm interfaces 
		       on a line interface unit 
		       in the current 15 minute interval."
       ::= { hneAtmLiuCurrentEntry 6 }          
       
 hneAtmLiuCurrentValidOAMCellCount             OBJECT-TYPE
	    SYNTAX	   Gauge32
	    MAX-ACCESS	read-only
	    STATUS	   current
	    DESCRIPTION
		      "The counter associated with the number of 
		       valid OAM cells encountered on all atm interfaces 
		       on a line interface unit 
		       in the current 15 minute interval."
       ::= { hneAtmLiuCurrentEntry 7 }         
      
  hneAtmLiuCurrentTxOverrunCount  OBJECT-TYPE
     SYNTAX	      Gauge32
     MAX-ACCESS	read-only
     STATUS	      current
     DESCRIPTION
	         "The counter associated with the number of times the output 
	         queue of the atm interfaces on a line interface unit 
	         was overrun in the current 15 minute interval."
     ::= { hneAtmLiuCurrentEntry 8 }   
 
  hneAtmLiuCurrentMissingHeaderCellCount  OBJECT-TYPE
	    SYNTAX	   Gauge32
	    MAX-ACCESS	read-only
	    STATUS	   current
	    DESCRIPTION
		      "The counter associated with the number of times a   
		       physical cell is found missing an internal header in the
		       current 15 minute interval."
       ::= { hneAtmLiuCurrentEntry 9 }    
       
  hneAtmLiuCurrentNonRoutableCellCount  OBJECT-TYPE
	    SYNTAX	   Gauge32
	    MAX-ACCESS	read-only
	    STATUS	   current
	    DESCRIPTION
		      "The counter associated with the number of 
		       non routable cells encountered on all of the 
		       atm interfaces on a line interface unit 
		       in the current 15 minute interval."
       ::= { hneAtmLiuCurrentEntry 10}    

  hneAtmLiuCurrentVcSearchFailedCount  OBJECT-TYPE
	    SYNTAX	   Gauge32
	    MAX-ACCESS	read-only
	    STATUS	   current
	    DESCRIPTION
		      "The counter associated with the number of 
		       virtual ciruit search failures encountered 
		       on all of the atm interfaces on a line interface unit 
		       in the current 15 minute interval."
       ::= { hneAtmLiuCurrentEntry 11 }    

  hneAtmLiuCurrentInternalParityErrorCount OBJECT-TYPE
	    SYNTAX	   Gauge32
	    MAX-ACCESS	read-only
	    STATUS	   current
	    DESCRIPTION
		      "The counter associated with the number of 
		       internal parity errors encountered 
		       in the current 15 minute interval."
       ::= { hneAtmLiuCurrentEntry 12 }    

