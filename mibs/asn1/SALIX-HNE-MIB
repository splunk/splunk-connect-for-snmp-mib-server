		       in the current 15 minute interval."
       ::= { hneVclCurrentEntry 13 }   

  -- the HNE Virtual Channel Link History Table
   
  -- The HNE Virtual Channel Link History Table
  -- contains various statistics
  -- collected by each system over a maximum
  -- of the previous 24 hours of
  -- operation.  The past 24 hours may be broken into 96
  -- completed 15 minute intervals.
  -- A system is required to store at
  -- least 4 completed 15 minute interval.
  -- The default value is 32 intervals.      
  

  hneVclHistoryTable OBJECT-TYPE
       SYNTAX     SEQUENCE OF    HneVclHistoryEntry
       MAX-ACCESS not-accessible
       STATUS     current
       DESCRIPTION
            "The HNE ATM Virtual Channel Link History table."
       ::= { hneMIBStats 10 } 
    
  hneVclHistoryEntry OBJECT-TYPE
       SYNTAX     HneVclHistoryEntry
       MAX-ACCESS not-accessible
       STATUS     current
       DESCRIPTION
         "An entry in the HNE vitual channel link history entry table.  
          An entry contains virtual channel link statistics."
       INDEX   { ifIndex,
                 atmVclVpi,
                 atmVclVci,
                 atmfM4CellProtoHistIndex}    
       ::= { hneVclHistoryTable 1}
          
       HneVclHistoryEntry ::= SEQUENCE {       
            hneVclHistoryBIP16Count                       Gauge32,
            hneVclHistoryRxSECBCount                      Gauge32,   
            hneVclHistoryRxLostCellCount                  Gauge32,
            hneVclHistoryRxMisinsertedCellCount           Gauge32,
            hneVclHistoryRxBIPVCount                      Gauge32,
            hneVclHistoryTxSECBCount                      Gauge32,
            hneVclHistoryTxLostCellCount                  Gauge32,
            hneVclHistoryTxMisinsertedCellCount           Gauge32,
            hneVclHistoryTxBIPVCount                      Gauge32
      }   

  
  hneVclHistoryBIP16Count                  OBJECT-TYPE
	    SYNTAX	   Gauge32
	    MAX-ACCESS	read-only
	    STATUS	   current
	    DESCRIPTION
		       "The counter associated with the number of virtual 
		       channel cells with a Bit Interleave Parity (BIP) error 
		       in a particular 15-minute interval 
		       in the past 24 hours."
       ::= { hneVclHistoryEntry 5 }   
  
  hneVclHistoryRxSECBCount           OBJECT-TYPE
	    SYNTAX	   Gauge32
	    MAX-ACCESS	read-only
	    STATUS	   current
	    DESCRIPTION
		      "The counter associated with the number of 
		       Severely Errored Cell Blocks (SECB) received within 
		       virtual channel cells in a particular 15-minute interval 
		       in the past 24 hours."
       ::= { hneVclHistoryEntry 6 }   
  
  hneVclHistoryRxLostCellCount       OBJECT-TYPE
	    SYNTAX	   Gauge32
	    MAX-ACCESS	read-only
	    STATUS	   current
	    DESCRIPTION
		      "The counter associated with the number of 
		       received cells lost within an ingress virtual channel 
		       in a particular 15-minute interval 
		       in the past 24 hours."
       ::= { hneVclHistoryEntry 7 }   
  
  hneVclHistoryRxMisinsertedCellCount     OBJECT-TYPE
	    SYNTAX	   Gauge32
	    MAX-ACCESS	read-only
	    STATUS	   current
	    DESCRIPTION
		      "The counter associated with the number of 
		       received cells misinserted within an igress virtual channel 
		       in a particular 15-minute interval 
		       in the past 24 hours."
       ::= { hneVclHistoryEntry 8 }   
  
  hneVclHistoryRxBIPVCount           OBJECT-TYPE
	    SYNTAX	   Gauge32
	    MAX-ACCESS	read-only
	    STATUS	   current
	    DESCRIPTION
		       "The counter associated with the number of 
		       Bit Interleave Parity Violations (BIPV) received
		       in virtual channel in a particular 15-minute interval 
		       in the past 24 hours."
       ::= { hneVclHistoryEntry 9 }   
  
  hneVclHistoryTxSECBCount            OBJECT-TYPE
	    SYNTAX	   Gauge32
	    MAX-ACCESS	read-only
	    STATUS	   current
	    DESCRIPTION
		      "The counter associated with the number of 
		       Severely Errored Cell Blocks (SECB) virtual channel 
		       cells transmitted in a particular 15-minute interval 
		       in the past 24 hours."
       ::= { hneVclHistoryEntry 10 }   
  
  hneVclHistoryTxLostCellCount        OBJECT-TYPE
	    SYNTAX	   Gauge32
	    MAX-ACCESS	read-only
	    STATUS	   current
	    DESCRIPTION
		      "The counter associated with the number of 
