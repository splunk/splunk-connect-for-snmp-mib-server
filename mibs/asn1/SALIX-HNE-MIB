		      "The counter associated with the number of 
		       virtual ciruit search failures encountered 
		       on all of the ATM interfaces on a line interface unit 
		       in a particular 15-minute 
		       interval in the past 24 hours."
       ::= { hneAtmLiuHistoryEntry 11 }    

  hneAtmLiuHistoryInternalParityErrorCount OBJECT-TYPE
	    SYNTAX	   Gauge32
	    MAX-ACCESS	read-only
	    STATUS	   current
	    DESCRIPTION
		      "The counter associated with the number of 
		       internal parity errors encountered 
		       in a particular 15-minute interval 
		       in the past 24 hours."
       ::= { hneAtmLiuHistoryEntry 12 }    

  -- HNE ATM Virtual Channel Link Current Table

  -- HNE ATM Virtual Channel Link Current table contains 
  -- various statistics being collected for the 
  -- current 15 minute interval.
  
  hneVclCurrentTable OBJECT-TYPE
       SYNTAX     SEQUENCE OF    HneVclCurrentEntry
       MAX-ACCESS not-accessible
       STATUS     current
       DESCRIPTION
            "The HNE ATM Virtual Channel Link Current table."
       ::= { hneMIBStats 9 } 
    
  hneVclCurrentEntry OBJECT-TYPE
       SYNTAX     HneVclCurrentEntry
       MAX-ACCESS not-accessible
       STATUS     current
       DESCRIPTION
         "An entry in the HNE vitual channel link current entry table.  
          An entry contains virtual channel statistics."
       INDEX   { ifIndex,
                 atmVclVpi,
                 atmVclVci }    
       ::= { hneVclCurrentTable 1}
          
       HneVclCurrentEntry ::= SEQUENCE {           
            hneVclCurrentBIP16Count                 Gauge32,
            hneVclCurrentRxSECBCount                Gauge32,   
            hneVclCurrentRxLostCellCount            Gauge32,
            hneVclCurrentRxMisinsertedCellCount     Gauge32,
            hneVclCurrentRxBIPVCount                Gauge32,
            hneVclCurrentTxSECBCount                Gauge32,
            hneVclCurrentTxLostCellCount            Gauge32,
            hneVclCurrentTxMisinsertedCellCount     Gauge32,
            hneVclCurrentTxBIPVCount                Gauge32
      }   

  
  hneVclCurrentBIP16Count                  OBJECT-TYPE
	    SYNTAX	   Gauge32
	    MAX-ACCESS	read-only
	    STATUS	   current
	    DESCRIPTION
		       "The counter associated with the number of virtual 
		       channel cells with a Bit Interleave Parity (BIP) error 
		       in the current 15 minute interval."
       ::= { hneVclCurrentEntry 5 }   
  
  hneVclCurrentRxSECBCount           OBJECT-TYPE
	    SYNTAX	   Gauge32
	    MAX-ACCESS	read-only
	    STATUS	   current
	    DESCRIPTION
		      "The counter associated with the number of 
		       Severely Errored Cell Blocks (SECB) received within 
		       virtual channel cells in the current 
		       15 minute interval."
       ::= { hneVclCurrentEntry 6 }   
  
  hneVclCurrentRxLostCellCount       OBJECT-TYPE
	    SYNTAX	   Gauge32
	    MAX-ACCESS	read-only
	    STATUS	   current
	    DESCRIPTION
		      "The counter associated with the number of 
		       received cells lost within an ingress virtual channel 
		       in the current 15 minute interval."
       ::= { hneVclCurrentEntry 7 }   
  
  hneVclCurrentRxMisinsertedCellCount     OBJECT-TYPE
	    SYNTAX	   Gauge32
	    MAX-ACCESS	read-only
	    STATUS	   current
	    DESCRIPTION
		      "The counter associated with the number of 
		       received cells misinserted in an igress virtual channel 
		       in the current 15 minute interval."
       ::= { hneVclCurrentEntry 8 }   
  
  hneVclCurrentRxBIPVCount           OBJECT-TYPE
	    SYNTAX	   Gauge32
	    MAX-ACCESS	read-only
	    STATUS	   current
	    DESCRIPTION
		       "The counter associated with the number of 
		       Bit Interleave Parity Violations (BIPV) received
		       in virtual channel in the current 15 minute interval."
       ::= { hneVclCurrentEntry 9 }   
  
  hneVclCurrentTxSECBCount            OBJECT-TYPE
	    SYNTAX	   Gauge32
	    MAX-ACCESS	read-only
	    STATUS	   current
	    DESCRIPTION
		      "The counter associated with the number of 
		       Severely Errored Cell Blocks (SECB) virtual channel 
		       cells transmitted in the current 15 minute interval."
       ::= { hneVclCurrentEntry 10 }   
  
  hneVclCurrentTxLostCellCount        OBJECT-TYPE
	    SYNTAX	   Gauge32
	    MAX-ACCESS	read-only
	    STATUS	   current
	    DESCRIPTION
		      "The counter associated with the number of 
		       transmitted cells lost within an eggress virtual channel 
		       in the current 15 minute interval."
       ::= { hneVclCurrentEntry 11 }   
  
  hneVclCurrentTxMisinsertedCellCount OBJECT-TYPE
	    SYNTAX	   Gauge32
	    MAX-ACCESS	read-only
	    STATUS	   current
	    DESCRIPTION
		      "The counter associated with the number of 
		       transmitted cells misinserted within a eggress virtual channel 
		       in the current 15 minute interval."
       ::= { hneVclCurrentEntry 12 }   
  
  hneVclCurrentTxBIPVCount            OBJECT-TYPE
	    SYNTAX	   Gauge32
	    MAX-ACCESS	read-only
	    STATUS	   current
	    DESCRIPTION
		      "The counter associated with the number of 
		       Bit Interleave Parity Violations (BIPV)  
