-- Compliance Statements

hneCompliance MODULE-COMPLIANCE
     STATUS       current
     DESCRIPTION
                  "The basic implementation requirements for the HNE MIB."
     MODULE       -- This module
     MANDATORY-GROUPS { hneSystemGroup }
     ::= { hneCompliances 1 }

-- Units of conformance

  hneSystemGroup     OBJECT-GROUP
       OBJECTS {
                    hneSysSyncTimingMode,
                    hneSysSyncPrimaryRefLine,
                    hneSysSyncSecondaryRefLine,
                    hneSysSyncRevertiveSwitch,
                    hneSysSyncClockMode,
                    hneSysSyncConfiguredActiveRef,
                    hneSysSyncAutoRefSwitch,
                    hneSysSyncFreeRunAlarm,
                    hneSysSyncWorking,
                    hneSysSyncProtect,
                    hneSysSyncPrimaryRefLineState,
                    hneSysSyncSecondaryRefLineState,
                    hneSysSyncCurrentActiveRef,
                    hneSysSyncForcedState,
                    hneSysHsfWorking,
                    hneSysHsfProtect
               }
       STATUS       current
       DESCRIPTION  "Additional HNE specific objects to the mib-2 system
                     group."
       ::= {hneGroups 1}

  hneSonetGroup      OBJECT-GROUP
       OBJECTS {
                     hneSonetLineTimingSource,
                     hneSonetLineDccSelection,
                     hneSonetLineOverheadLoopThroughModeEnabled,
                     hneSonetProtectionInterfaceWorking,
