-- Remote Port Object Definitions
--
-- This section defines the objects under the Remote Port node.
--
-- Instance Identifiers for Remote Port Objects
--
--      There is only one instance of the remote port on the NB30 bridge
--      thus the Instance Identifier is 0.
--
-- Port Objects

nb30RemPortTable        OBJECT-TYPE
    --OID       1.3.6.1.4.1.52.1.6.12.5.1
    SYNTAX      SEQUENCE OF Nb30RemPortEntry
    ACCESS      not-accessible
    STATUS      mandatory
    DESCRIPTION
                "Bridge Remote Port information table"
::= { nb30RemPort 1 }

nb30RemPortEntry        OBJECT-TYPE
    --OID       1.3.6.1.4.1.52.1.6.12.5.1.1
    SYNTAX      Nb30RemPortEntry
    ACCESS      not-accessible
    STATUS      mandatory
    DESCRIPTION
                ""
    INDEX       { nb30RemPortId }
::= { nb30RemPortTable 1 }

Nb30RemPortEntry ::=
    SEQUENCE {
                nb30RemPortName     OCTET STRING,
                nb30RemPortType     OCTET STRING,
                nb30RemPortStatus   OCTET STRING,
                nb30RemPortNetName  OCTET STRING,
                nb30RemPortFrRxs    Counter,
                nb30RemPortFwdOutbs Counter,
                nb30RemPortDisLOBs  Counter,
                nb30RemPortDisTDEs  Counter,
                nb30RemPortCRCErrs  Counter,
                nb30RemPortFrAlErrs Counter,
		nb30RemPortId	INTEGER
             }

nb30RemPortName     OBJECT-TYPE
    --OID       1.3.6.1.4.1.52.1.6.12.5.1.1.1
    SYNTAX      OCTET STRING
    ACCESS      read-write
    STATUS      mandatory
    DESCRIPTION
                "The user-defined name assigned to the remote port. The
                default Remote port name is REMOTE PORT."
::= { nb30RemPortEntry 1 }

nb30RemPortType     OBJECT-TYPE
    --OID       1.3.6.1.4.1.52.1.6.12.5.1.1.2
    SYNTAX      OCTET STRING
    ACCESS      read-only
    STATUS      mandatory
    DESCRIPTION
                "The Electrical interface specification the port meets, e.g.
                DSX-1, V.35."
::= { nb30RemPortEntry 2 }

nb30RemPortStatus       OBJECT-TYPE
    --OID       1.3.6.1.4.1.52.1.6.12.5.1.1.3
    SYNTAX      OCTET STRING
    ACCESS      read-only
    STATUS      mandatory
    DESCRIPTION
                "The status in which the remote port is operating. The status
                messages for the Remote Port are:

                NO EXT CLOCK
                DCE NOT READY / NO DM
                DCE NOT READY / NO CTS
                LINK OK / DCE READY
                NET LOOP / RED ALARM
                REM LOOP / RED ALARM
                NORMAL / RED ALARM
                NET LOOP / YELLOW ALARM
                REM LOOP / YELLOW ALARM
                NORMAL / YELLOW ALARM
                NET LOOP / LINK OK
                REM LOOP / LINK OK
                NORMAL / LINK OK
                NET LOOP / LINK LOST
                REM LOOP / LINK LOST
                NORMAL / LINK LOST"
::= { nb30RemPortEntry 3 }

nb30RemPortNetName  OBJECT-TYPE
    --OID       1.3.6.1.4.1.52.1.6.12.5.1.1.4
    SYNTAX      OCTET STRING
    ACCESS      read-write
    STATUS      mandatory
    DESCRIPTION
                "The user-defined name assigned to a wide area segment
                connected to the remote port. The default network name
                for Remote Port is REMOTE LINK."
::= { nb30RemPortEntry 4 }

nb30RemPortFrRxs    OBJECT-TYPE
    --OID       1.3.6.1.4.1.52.1.6.12.5.1.1.5
    SYNTAX      Counter
    ACCESS      read-only
    STATUS      mandatory
    DESCRIPTION
                "The number of frames received at the remote port."
::= { nb30RemPortEntry 5}

nb30RemPortFwdOutbs OBJECT-TYPE
    --OID       1.3.6.1.4.1.52.1.6.12.5.1.1.6
    SYNTAX      Counter
    ACCESS      read-only
    STATUS      mandatory
    DESCRIPTION
                "The number of frames forwarded successfully to the
                remote port."
::= { nb30RemPortEntry 6 }

nb30RemPortDisLOBs      OBJECT-TYPE
    --OID       1.3.6.1.4.1.52.1.6.12.5.1.1.7
    SYNTAX      Counter
    ACCESS      read-only
    STATUS      mandatory
    DESCRIPTION
                "The number of packets that were discarded by the bridge
                because of a lack of buffer space to maintain the data."
::= { nb30RemPortEntry 7 }

nb30RemPortDisTDEs      OBJECT-TYPE
    --OID       1.3.6.1.4.1.52.1.6.12.5.1.1.8
    SYNTAX      Counter
    ACCESS      read-only
    STATUS      mandatory
    DESCRIPTION
                "The number of frames that were intended for forwarding
                but were discarded when the maximum time period set for
                transmission was exceeded before forwarding was possible."
::= { nb30RemPortEntry 8 }

nb30RemPortCRCErrs      OBJECT-TYPE
    --OID       1.3.6.1.4.1.52.1.6.12.5.1.1.9
    SYNTAX      Counter
    ACCESS      read-only
    STATUS      mandatory
    DESCRIPTION
                "The number of packets detected with Cyclical Redundancy
                Check failures."
::= { nb30RemPortEntry 9 }

nb30RemPortFrAlErrs     OBJECT-TYPE
    --OID       1.3.6.1.4.1.52.1.6.12.5.1.1.10
    SYNTAX      Counter
    ACCESS      read-only
    STATUS      mandatory
    DESCRIPTION
                "The number of errors due to misaligned packets."
::= { nb30RemPortEntry 10 }

nb30RemPortId	OBJECT-TYPE
    SYNTAX	INTEGER
    ACCESS	read-only
    STATUS	mandatory
    DESCRIPTION
