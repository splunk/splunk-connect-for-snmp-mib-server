IMPORTS
                OBJECT-TYPE
                  FROM RFC-1212
                Counter 
                  FROM RFC1155-SMI
	        nb30Rev1
		  FROM IRM-OIDS;

nb30Device      OBJECT IDENTIFIER ::= { nb30Rev1 1 }

nb30Dsx         OBJECT IDENTIFIER ::= { nb30Rev1 2 }

nb30DsxChannel  OBJECT IDENTIFIER ::= { nb30Rev1 3 }

nb30Port        OBJECT IDENTIFIER ::= { nb30Rev1 4 }

nb30RemPort     OBJECT IDENTIFIER ::= { nb30Rev1 5 }

nb30FilterDB    OBJECT IDENTIFIER ::= { nb30Rev1 6 }

nb30AcqDB       OBJECT IDENTIFIER ::= { nb30FilterDB 1 }

nb30PermDB      OBJECT IDENTIFIER ::= { nb30FilterDB 2 }

nb30SpecDB      OBJECT IDENTIFIER ::= { nb30FilterDB 3 }

-- Device Object Definitions
--
--      This section defines the objects under the Device node.
--
-- Instance Identifiers for Device Objects
--
--      There is only one instance of a Device object, thus the Instance
--      Identifier is 0.
--
-- Device Objects


nb30DeviceDisableBdg    OBJECT-TYPE
    --OID       1.3.6.1.4.1.52.1.6.12.1.1
    SYNTAX      INTEGER
                {
                    disableBridge(2),
                    enableBridge(1)
                }
    ACCESS      read-write
    STATUS      mandatory
    DESCRIPTION
                "Indicates if the bridge will be disabled."
::= { nb30Device 1 }

nb30DeviceRestoreSettings   OBJECT-TYPE
    --OID       1.3.6.1.4.1.52.1.6.12.1.2
    SYNTAX      INTEGER
                {
                    restoreSettings(1)
                }
    ACCESS      read-write
    STATUS      mandatory
    DESCRIPTION
                "Restore the following settings to their default values:
                Bridge Name to CABLETRON REMOTE BRIDGE,
                Location to LOCAL,
                Enet Port 1 Name to ETHERNET PORT 1,
                Enet Port 2 Name to ETHERNET PORT 2,
                Remote Port Name to REMOTE PORT,
                Enet Port 1 Network Name to LAN_1,
                Enet Port 2 Network Name to LAN_2,
                Defualt Enet Port to Port 1,
                Enable Redundancy to NO,
                Port 1 and Port 2 Poll Address to 00-00-00-00-00-00,
                Port 1 and Port 2 Poll Interval to 5 Seconds,
                Port 1 and Port 2 Max Retries to 6,
                Remote Port Network Name to REMOTE LINK,
                Ageing Time for Acquired Database to 300 seconds,
                Place seventeen default multicast addresses into
                the permanent and acquired databases,
                Type of Spanning Tree to 802.1,
                Type of Filtering to IEEE 802.1,
                Bridge Max Age to 20 seconds,
                Bridge Forward Delay to 15 seconds,
                Bridge Hello Time to 2 seconds,
                Bridge Priority to 8000,
                Port 1 Priority to 80,
                Port 2 Priority to 80,
                Port 1 Path Cost to 1000,
                Port 2 Path Cost to 1000,
                Number of Restarts to 0."
::= { nb30Device 2 }

nb30DeviceBdgName   OBJECT-TYPE
    --OID       1.3.6.1.4.1.52.1.6.12.1.3
    SYNTAX      OCTET STRING
    ACCESS      read-write
    STATUS      mandatory
    DESCRIPTION
                "The user-defined name (up to 32 characters long) of the
                bridge. The default bridge name is CABLETRON REMOTE BRIDGE."
::= { nb30Device 3 }

nb30DeviceNumPorts      OBJECT-TYPE
    --OID       1.3.6.1.4.1.52.1.6.12.1.4
    SYNTAX      INTEGER
    ACCESS      read-only
    STATUS      mandatory
    DESCRIPTION
                "The number of ports present on the bridge."
::= { nb30Device 4 }

nb30DeviceType      OBJECT-TYPE
    --OID       1.3.6.1.4.1.52.1.6.12.1.5
    SYNTAX      OCTET STRING
    ACCESS      read-only
    STATUS      mandatory
    DESCRIPTION
                "The type of bridge, NB30."
::= { nb30Device 5 }

nb30DeviceVersion       OBJECT-TYPE
    --OID       1.3.6.1.4.1.52.1.6.12.1.6
    SYNTAX      OCTET STRING
    ACCESS      read-only
    STATUS      mandatory
    DESCRIPTION
                "The release version of the firmware installed in the bridge."
::= { nb30Device 6 }

nb30DeviceLocation      OBJECT-TYPE
    --OID       1.3.6.1.4.1.52.1.6.12.1.7
    SYNTAX      OCTET STRING
    ACCESS      read-write
    STATUS      mandatory
    DESCRIPTION
                "The user-defined name (up to 14 characters long) to
                indicate the location of the bridge on the network.
                The default location name is LOCAL."
::= { nb30Device 7 }

nb30DeviceStatus        OBJECT-TYPE
    --OID       1.3.6.1.4.1.52.1.6.12.1.8
    SYNTAX      OCTET STRING
    ACCESS      read-only
    STATUS      mandatory
    DESCRIPTION
                "The status of the bridge, ON-LINE, STAND BY or DISABLED."
::= { nb30Device 8 }

nb30DeviceRestartBdg    OBJECT-TYPE
    --OID       1.3.6.1.4.1.52.1.6.12.1.9
    SYNTAX      INTEGER
                {
                    restartBridge(1)
                }
    ACCESS      read-write
    STATUS      mandatory
    DESCRIPTION
                "Force the bridge to undergo a software reset."
::= { nb30Device 9 }

nb30DeviceFrFwds    OBJECT-TYPE
    --OID       1.3.6.1.4.1.52.1.6.12.1.10
    SYNTAX      Counter
    ACCESS      read-only
    STATUS      mandatory
    DESCRIPTION
                "The number of frames forwarded by the bridge."
::= { nb30Device 10 }

nb30DeviceFrRxs     OBJECT-TYPE
    --OID       1.3.6.1.4.1.52.1.6.12.1.11
    SYNTAX      Counter
    ACCESS      read-only
    STATUS      mandatory
    DESCRIPTION
                "The number of frames received by the bridge."
::= { nb30Device 11 }

nb30DeviceFrFlts    OBJECT-TYPE
    --OID       1.3.6.1.4.1.52.1.6.12.1.12
    SYNTAX      Counter
    ACCESS      read-only
    STATUS      mandatory
    DESCRIPTION
                "The number of frames filtered by the bridge."
::= { nb30Device 12 }

nb30DeviceErrs      OBJECT-TYPE
    --OID       1.3.6.1.4.1.52.1.6.12.1.13
    SYNTAX      Counter
    ACCESS      read-only
    STATUS      mandatory
    DESCRIPTION
                "The total number of network errors that have occurred."
::= { nb30Device 13 }

nb30DeviceNumRestarts   OBJECT-TYPE
    --OID       1.3.6.1.4.1.52.1.6.12.1.14
    SYNTAX      Counter
    ACCESS      read-only
    STATUS      mandatory
    DESCRIPTION
                "The number of times the bridge has been powered up or
                restarted."
::= { nb30Device 14 }

nb30DeviceTypeFiltering OBJECT-TYPE
    --OID       1.3.6.1.4.1.52.1.6.12.1.15
    SYNTAX      INTEGER
                {
                    ieee8021(1),
                    specialDB(2),
                    both(3)
                }
    ACCESS      read-write
    STATUS      mandatory
    DESCRIPTION
                "The type of filtering to be performed by the bridge. The
                default is IEEE 802.1."
::= { nb30Device 15 }

nb30DeviceSTAProtocol   OBJECT-TYPE
    --OID       1.3.6.1.4.1.52.1.6.12.1.16
    SYNTAX      INTEGER
                {
                    ieee8021(1),
                    dec(2),
                    none(3)
                }
    ACCESS      read-write
    STATUS      mandatory
    DESCRIPTION
                "The spanning tree algorithm under which the bridge is
                operating. The selections are as follows:
                - IEEE 802.1 compliant spanning tree algorithm
                  environment (802.1)
                - DEC LAN Bridge 100 environment (DEC)
                - Without the spanning tree algorithm enabled (NONE)
                The default is 802.1."
::= { nb30Device 16 }

nb30DeviceBridgeID      OBJECT-TYPE
    --OID       1.3.6.1.4.1.52.1.6.12.1.17
    SYNTAX      OCTET STRING
    ACCESS      read-only
    STATUS      mandatory
    DESCRIPTION
                "The unique identifier of the bridge. The first two bytes of
                the identifier are the bridge priority and the last six bytes
                are the Ethernet address."
::= { nb30Device 17 }

nb30DeviceTopChgCnts OBJECT-TYPE
    --OID       1.3.6.1.4.1.52.1.6.12.1.18
    SYNTAX      Counter
    ACCESS      read-only
    STATUS      mandatory
    DESCRIPTION
                "The number of times the bridge's Topology Change Flag
                has been changed since the bridge was powered up or
                initialized."
::= { nb30Device 18 }

nb30DeviceTimeTopChg    OBJECT-TYPE
    --OID       1.3.6.1.4.1.52.1.6.12.1.19
    SYNTAX      INTEGER
    ACCESS      read-only
    STATUS      mandatory
    DESCRIPTION
                "The time in seconds that has elapsed since the bridge's
                Topology Change Flag last recorded the value of a
                topology change."
::= { nb30Device 19 }

nb30DeviceTopChg        OBJECT-TYPE
    --OID       1.3.6.1.4.1.52.1.6.12.1.20
    SYNTAX      INTEGER
                {
                    noTopologyChangeInProgress(1),
                    topologyChangeInProgress(2)
                }
    ACCESS      read-only
    STATUS      mandatory
    DESCRIPTION
                "Indicates if a bridge topology change is in progress."
::= { nb30Device 20 }

nb30DeviceRootCost      OBJECT-TYPE
    --OID       1.3.6.1.4.1.52.1.6.12.1.21
    SYNTAX      INTEGER
    ACCESS      read-only
    STATUS      mandatory
    DESCRIPTION
                "The cost of the path to the root from this bridge."
::= { nb30Device 21 }

nb30DeviceRootPort      OBJECT-TYPE
    --OID       1.3.6.1.4.1.52.1.6.12.1.22
    SYNTAX      INTEGER
    ACCESS      read-only
    STATUS      mandatory
    DESCRIPTION
                "The port identifier for the port which offers the lowest cost
                path to the root, i.e. that port for which the sum of the
                values of the designated cost and path cost parameters
                held for the port is lowest."
::= { nb30Device 22 }

nb30DeviceDesigRoot     OBJECT-TYPE
    --OID       1.3.6.1.4.1.52.1.6.12.1.23
    SYNTAX      OCTET STRING
    ACCESS      read-only
    STATUS      mandatory
    DESCRIPTION
                "The unique identifier of the bridge recorded as the root."
::= { nb30Device 23 }

nb30DeviceMaxAge        OBJECT-TYPE
    --OID       1.3.6.1.4.1.52.1.6.12.1.24
    SYNTAX      INTEGER
    ACCESS      read-only
    STATUS      mandatory
    DESCRIPTION
                "The maximum age of received protocol information before
                it is discarded."
::= { nb30Device 24 }

nb30DeviceHoldTime      OBJECT-TYPE
    --OID       1.3.6.1.4.1.52.1.6.12.1.25
    SYNTAX      INTEGER
    ACCESS      read-only
    STATUS      mandatory
    DESCRIPTION
                "The minimum time period elapsing between the transmission
                of configuration BPDU's through a given bridge port."
::= { nb30Device 25 }

nb30DeviceFwdDly        OBJECT-TYPE
    --OID       1.3.6.1.4.1.52.1.6.12.1.26
    SYNTAX      INTEGER
    ACCESS      read-only
    STATUS      mandatory
    DESCRIPTION
                "The time spent in the listening state while moving from the
                blocking state to the learning state, or the time spent in
                the learning state while moving from the listening state to
                the forwarding state."
::= { nb30Device 26 }

nb30DeviceHelloTime     OBJECT-TYPE
    --OID       1.3.6.1.4.1.52.1.6.12.1.27
    SYNTAX      INTEGER
    ACCESS      read-only
    STATUS      mandatory
    DESCRIPTION
                "The time interval between the transmission of Configuration
                BPDU's by a bridge which is attempting to become the root or
                is the root."
::= { nb30Device 27 }

nb30DeviceBdgFwdDly OBJECT-TYPE
    --OID       1.3.6.1.4.1.52.1.6.12.1.28
    SYNTAX      INTEGER
    ACCESS      read-write
    STATUS      mandatory
    DESCRIPTION
                "The value of the forward delay parameter when the bridge
                is the root or is attempting to become the root. A time
                of 4 to 30 seconds is allowed. The default is 15 seconds."
::= { nb30Device 28 }

nb30DeviceBdgMaxAge OBJECT-TYPE
    --OID       1.3.6.1.4.1.52.1.6.12.1.29
    SYNTAX      INTEGER
    ACCESS      read-write
    STATUS      mandatory
    DESCRIPTION
                "The value of the Max Age parameter when the bridge is
                the root or is attempting to become the root. A time of
                6 to 40 seconds is allowed. The default is 20 seconds."
::= { nb30Device 29 }

nb30DeviceBdgHello      OBJECT-TYPE
    --OID       1.3.6.1.4.1.52.1.6.12.1.30
    SYNTAX      INTEGER
    ACCESS      read-write
    STATUS      mandatory
    DESCRIPTION
                "The value of the Hello Time parameter when the bridge is
                the root or is attempting to become the root. A time of 1
                to 10 seconds is allowed. The default is 2 seconds."
::= { nb30Device 30 }

nb30DeviceBdgPriority   OBJECT-TYPE
    --OID       1.3.6.1.4.1.52.1.6.12.1.31
    SYNTAX      INTEGER
    ACCESS      read-write
    STATUS      mandatory
    DESCRIPTION

                "Used in the spanning tree for priority comparisons. Allowed
                range is 0 through FFFF. The default is 8000."
::= { nb30Device 31 }

nb30DeviceResetCounts   OBJECT-TYPE
    --OID       1.3.6.1.4.1.52.1.6.12.1.32
    SYNTAX      INTEGER
                {
                    resetCounts(1)
                }
    ACCESS      read-write
    STATUS      mandatory
    DESCRIPTION
                "Reset all counters to zero."
::= { nb30Device 32 }

nb30DeviceUptime        OBJECT-TYPE
    --OID       1.3.6.1.4.1.52.1.6.12.1.33
    SYNTAX      INTEGER
    ACCESS      read-only
    STATUS      mandatory
    DESCRIPTION
                "The time, in seconds, that has elapsed since the bridge
                was last reset or initialized."
::= { nb30Device 33 }

nb30DeviceRootSwitch OBJECT-TYPE
    --OID       1.3.6.1.4.1.52.1.6.12.1.34
    SYNTAX      INTEGER
                {
                    noRoot(1),
                    root(2)
                }
    ACCESS      read-only
    STATUS      mandatory
    DESCRIPTION
                "The status of the Root switch on the front panel of the
                NB30."
::= { nb30Device 34 }

nb30DeviceFwdBroadcast  OBJECT-TYPE
    --OID       1.3.6.1.4.1.52.1.6.12.1.35
    SYNTAX      INTEGER
                {
                    forwardBroadcast(1),
                    filterBroadcast(2)
                }
    ACCESS      read-only
    STATUS      mandatory
    DESCRIPTION
                "The status of the Forward Broadcast switch on the front
                panel of the NB30."
::= { nb30Device 35 }

