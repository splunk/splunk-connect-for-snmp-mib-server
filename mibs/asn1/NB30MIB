                    disableRedundancy(1),
                    enableRedundancy(2)
                }
    ACCESS      read-write
    STATUS      mandatory
    DESCRIPTION
                "The state of the Redundancy Enable Flag."
::= { nb30Device 42 }

-- This section defines the objects specific to the DSX-1 Remote interface.
--
-- DSX-1 Remote Interface Objects

nb30DsxLoopUp       OBJECT-TYPE
    --OID       1.3.6.1.4.1.52.1.6.12.2.1
    SYNTAX      INTEGER
                {
                    loopUp(1)
                }
    ACCESS      read-write
    STATUS      mandatory
    DESCRIPTION
                "The loop up command for the DSX-1 Remote Interface."
::= { nb30Dsx 1 }

nb30DsxLoopDn       OBJECT-TYPE
    --OID       1.3.6.1.4.1.52.1.6.12.2.2
    SYNTAX      INTEGER
                {
                    loopDown(1)
                }
    ACCESS      read-write
    STATUS      mandatory
    DESCRIPTION
                "The loop down command for the DSX-1 Remote Interface."
::= { nb30Dsx 2 }

nb30DsxLoopSt       OBJECT-TYPE
    --OID       1.3.6.1.4.1.52.1.6.12.2.3
    SYNTAX      INTEGER
                {
                    notInLoopback(1),
                    loopbackInProgress(2),
                    networkLoopback(3),
                    loopbackPassed(4),
                    loopbackFailed(5)
                }
    ACCESS      read-only
    STATUS      mandatory
    DESCRIPTION
                "The loop status for the DSX-1 Remote Interface."
::= { nb30Dsx 3 }

nb30DsxLoopPattern      OBJECT-TYPE
    --OID       1.3.6.1.4.1.52.1.6.12.2.4
    SYNTAX      INTEGER
                {
                    alternate1sAnd0s(1),
                    all1s(2),
                    all0s(3),
                    incrementingData(4)
                }
    ACCESS      read-write
    STATUS      mandatory
    DESCRIPTION
                "The loop test pattern for the DSX-1 Remote Interface."
::= { nb30Dsx 4 }

-- DSX-1 Channel Object Definitions
--
-- This section defines the objects under the DSX-1 Channel node.
--
-- Instance Identifiers for DSX-1 Channel Objects
--
--      The Instance Identifier for nb30DsxChannel objects is the number of
--      channels on the DSX-1 interface. Valid instance identifiers for the
--      remote channels are 1 - 24.
--
-- DSX-1 Channel Objects

nb30DsxChannelTable OBJECT-TYPE
    --OID       1.3.6.1.4.1.52.1.6.12.3.1
    SYNTAX      SEQUENCE OF Nb30DsxEntry
    ACCESS      not-accessible
    STATUS      mandatory
    DESCRIPTION
                "NB30 DSX-1 Remote Port Channelization Table"
::= { nb30DsxChannel 1 }

nb30DsxEntry            OBJECT-TYPE
    --OID       1.3.6.1.4.1.52.1.6.12.3.1.1
    SYNTAX      Nb30DsxEntry
    ACCESS      not-accessible
    STATUS      mandatory
    DESCRIPTION
                ""
    INDEX       { nb30DsxChannelId }

::= { nb30DsxChannelTable 1 }

Nb30DsxEntry ::= SEQUENCE
             {
                nb30DsxChannelControl   INTEGER,
		nb30DsxChannelId	INTEGER
             }

nb30DsxChannelControl   OBJECT-TYPE
    --OID       1.3.6.1.4.1.52.1.6.12.3.1.1.1
    SYNTAX      INTEGER
                {
                    disableChannel(1),
                    enableChannel(2)
                }
    ACCESS      read-write
    STATUS      mandatory
    DESCRIPTION
                "Disable the DSX-1 Channel."
::= { nb30DsxEntry 1 }

nb30DsxChannelId	OBJECT-TYPE
    SYNTAX	INTEGER
    ACCESS	read-write
    STATUS	mandatory
    DESCRIPTION
	"The channel number for this entry."
    ::= { nb30DsxEntry 2 }


-- Port Object Definitions
--
-- This section defines the objects under the Port node.
--
-- Instance Identifiers for Port Objects
--
--      The Instance Identifier for nb30Port objects is the number of the
--      port on the bridge. Valid instance identifiers for the ports on
--      the NB30 are 1 and 2.
--
-- Port Objects

nb30PortTable           OBJECT-TYPE
    --OID       1.3.6.1.4.1.52.1.6.12.4.1
    SYNTAX      SEQUENCE OF Nb30PortEntry
    ACCESS      not-accessible
    STATUS      mandatory
    DESCRIPTION
                "Bridge Port information table"
::= { nb30Port 1 }

nb30PortEntry           OBJECT-TYPE
    --OID       1.3.6.1.4.1.52.1.6.12.4.1.1
    SYNTAX      Nb30PortEntry
    ACCESS      not-accessible
    STATUS      mandatory
    DESCRIPTION
                ""
    INDEX       { nb30PortId }

::= { nb30PortTable 1 }

Nb30PortEntry ::=
    SEQUENCE {
                nb30PortAddress         OCTET STRING,
                nb30PortName            OCTET STRING,
                nb30PortType            OCTET STRING,
                nb30PortStatus          OCTET STRING,
                nb30PortNetName         OCTET STRING,
                nb30PortFrRxs           Counter,
                nb30PortDisInbounds     Counter,
                nb30PortFwdOutbs        Counter,
                nb30PortDisLOBs         Counter,
                nb30PortDisTDEs         Counter,
                nb30PortCollisions      Counter,
                nb30PortTxAborts        Counter,
                nb30PortOowCollisions   Counter,
                nb30PortCRCErrs         Counter,
                nb30PortFrAlignErrs     Counter,
                nb30PortPriority        INTEGER,
                nb30PortState           OCTET STRING,
                nb30PortPathCost        INTEGER,
                nb30PortDesigCost       INTEGER,
                nb30PortDesigBrdg       OCTET STRING,
                nb30PortDesigPort       INTEGER,
                nb30PortTopChgAck       INTEGER ,
                nb30PortDesigRoot       OCTET STRING,
                nb30PortOversizePkts    Counter,
                nb30PortFrFiltereds     Counter,
                nb30PortPollAddress     OCTET STRING,
                nb30PortPollInterval    INTEGER,
                nb30PortMaxRetry        INTEGER,
		nb30PortId		INTEGER
             }

nb30PortAddress     OBJECT-TYPE
    --OID       1.3.6.1.4.1.52.1.6.12.4.1.1.1
    SYNTAX      OCTET STRING
    ACCESS      read-only
    STATUS      mandatory
    DESCRIPTION
                "The Ethernet address of the port. Instance of 1 represents
                the local Ethernet address, instance of 2 represents the
                remote Ethernet address."
::= { nb30PortEntry 1 }

nb30PortName        OBJECT-TYPE
    --OID       1.3.6.1.4.1.52.1.6.12.4.1.1.2
    SYNTAX      OCTET STRING
    ACCESS      read-write
    STATUS      mandatory
    DESCRIPTION
                "The user-defined name (up to 32 characters long)
                assigned to the port. The default Enet port 1 name is
                ETHERNET PORT 1 and the default Enet Port 2 name is
                ETHERNET PORT 2."
::= { nb30PortEntry 2 }

nb30PortType            OBJECT-TYPE
    --OID       1.3.6.1.4.1.52.1.6.12.4.1.1.3
    SYNTAX      OCTET STRING
    ACCESS      read-only
    STATUS      mandatory
    DESCRIPTION
                "The IEEE specification the port meets, e.g. 802.3."
::= { nb30PortEntry 3 }

nb30PortStatus      OBJECT-TYPE
     --OID       1.3.6.1.4.1.52.1.6.12.4.1.1.4
    SYNTAX      OCTET STRING
    ACCESS      read-only
    STATUS      mandatory
    DESCRIPTION
                "The status in which the port is operating. The status
                messages for Ports 1 and 2 are:

        OFF - The port is off, due to a failed network interface
        chip associated with that port.

        OK/SQE ON - The port is communicating with the network
        and the transceiver making the connection to the network
        has SQE enabled.

        OK/SQE OFF - The port is communicating with the network
        and the transceiver making the connection to the network
        has SQE disabled.

        CARRIER LOST - Communication with the network has not
        been established or has been lost."
::= { nb30PortEntry 4 }

nb30PortNetName     OBJECT-TYPE
    --OID       1.3.6.1.4.1.52.1.6.12.4.1.1.5
    SYNTAX      OCTET STRING
    ACCESS      read-write
    STATUS      mandatory
    DESCRIPTION
                "The user-defined name (up to 32 characters long)
                assigned to a network segment connected to the port.
                The default network name for Port 1 is LAN_1 and the
                default network name for Port 2 is LAN_2."
::= { nb30PortEntry 5 }

nb30PortFrRxs           OBJECT-TYPE
    --OID       1.3.6.1.4.1.52.1.6.12.4.1.1.6
    SYNTAX      Counter
    ACCESS      read-only
    STATUS      mandatory
    DESCRIPTION
                "The number of frames received at the specified port."
::= { nb30PortEntry 6 }

nb30PortDisInbounds     OBJECT-TYPE
    --OID       1.3.6.1.4.1.52.1.6.12.4.1.1.7
    SYNTAX      Counter
    ACCESS      read-only
    STATUS      mandatory
    DESCRIPTION
                "The total number of valid frames that were received at
                the port but then discarded by the bridge in the
                forwarding process."
::= { nb30PortEntry 7 }

nb30PortFwdOutbs    OBJECT-TYPE
    --OID       1.3.6.1.4.1.52.1.6.12.4.1.1.8
    SYNTAX      Counter
    ACCESS      read-only
    STATUS      mandatory
    DESCRIPTION
                "The number of frames forwarded successfully to the
                appropriate port."
::= { nb30PortEntry 8 }

nb30PortDisLOBs     OBJECT-TYPE
    --OID       1.3.6.1.4.1.52.1.6.12.4.1.1.9
