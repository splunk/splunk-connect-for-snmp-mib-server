                    switchOff(1),
                    switchOn(2)
                }
    ACCESS      read-only
    STATUS      mandatory
    DESCRIPTION
                "The status of Config switch 4 on the back panel of the
                NB30."
::= { nb30Device 39 }

nb30DevicePortActive    OBJECT-TYPE
    --OID       1.3.6.1.4.1.52.1.6.12.1.40
    SYNTAX      INTEGER
    ACCESS      read-only
    STATUS      mandatory
    DESCRIPTION
                "The Port number which is acting as the online Ethernet
                Port on the NB30."
::= { nb30Device 40 }

nb30DeviceDefPort       OBJECT-TYPE
    --OID       1.3.6.1.4.1.52.1.6.12.1.41
    SYNTAX      INTEGER
                {
                    ethernetPort1(1),
                    ethernetPort2(2)
                }
    ACCESS      read-write
    STATUS      mandatory
    DESCRIPTION
                "The Port number which is Default Ethernet Port on the
                NB30 at power up."
::= { nb30Device 41 }

