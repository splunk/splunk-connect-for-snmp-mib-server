	::= {control 10}

lanCardResetEntry OBJECT-TYPE
	SYNTAX	LanCardResetEntry
	ACCESS	not-accessible
	STATUS	mandatory
	DESCRIPTION	"Entry within the table to specify a particular card/module to
			be reset." 
	INDEX		{lanCardResetCardNumber}
	::= {lanCardResetTable 1}

LanCardResetEntry ::= SEQUENCE {
	lanCardResetCardNumber		INTEGER,
	lanCardResetType		INTEGER
	}

lanCardResetCardNumber	 OBJECT-TYPE
	SYNTAX	INTEGER (2..5)
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION	"Identifies the module number to be reset." 
	 ::= { lanCardResetEntry 1}

lanCardResetType OBJECT-TYPE
	SYNTAX	INTEGER {
		resetWithCurrentValues (1),
		resetWithDefaultValues (2),
		wanToLanDownload (3),
		lanToLanDownload (4)
		}
	ACCESS	write-only
	STATUS	mandatory
	DESCRIPTION	"Commands a specific type of reset for the integral Lan module." 
	 ::= {lanCardResetEntry 2}

-- N_CMOS.O_CHCNST ========================================================

chControlTable OBJECT-TYPE
	SYNTAX	SEQUENCE OF ChControlEntry
	ACCESS	not-accessible
	STATUS	mandatory
	DESCRIPTION	"Table controls the operating condition (status) of the channels
			in the system." 
	::= {control 11}

chControlEntry OBJECT-TYPE
	SYNTAX	ChControlEntry
	ACCESS	not-accessible
	STATUS	mandatory
	DESCRIPTION	"Entry within the table specific to a particular channel on 
			specific module/card." 
	INDEX		{chControlCardNumber, chControlChannelNumber}
	::= {chControlTable 1}

ChControlEntry ::= SEQUENCE {
	chControlCardNumber		INTEGER,
	chControlChannelNumber		INTEGER,
	chControlCommand		INTEGER,
	chControlForceConnToNodeName	DisplayString,
	chControlForceConnToCard	INTEGER,
	chControlForceConnToChannel	INTEGER
	}

chControlCardNumber OBJECT-TYPE
	SYNTAX	INTEGER (1..5)
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION	"Identifies the card/module number of channel to be configured." 
	::= {chControlEntry 1}

chControlChannelNumber OBJECT-TYPE
	SYNTAX	INTEGER
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION	"Identifies the channel number to be configured." 
	::= {chControlEntry 2}

chControlCommand OBJECT-TYPE
	SYNTAX	INTEGER {
		enable (1),
		disable (2),
		forcedConnect (3),
		forcedDisconnect (4)
		}
	ACCESS	write-only
	STATUS	mandatory
	DESCRIPTION	"Object triggers the specified status action valid on the 
			channels. Option 1 returns a out-of-service async or voice/fax
			port to the idle state. The option 2 permits to take an async or
			voice/fax channel from idle state to out-of-service. Option 3
			causes two selected (idle or out-of-service) ports to be connected.
			The ports must be of like kind.  An attempt to SET this Option
			without first SET'ing the other required information
			(chControlForceConnToNodeName, chControlForceConnToCard,
			and chControlForceConnToChannel) will return an error." 
	::= {chControlEntry 3}

chControlForceConnToNodeName OBJECT-TYPE
	SYNTAX	DisplayString (SIZE(1..8))
	ACCESS	write-only
	STATUS	mandatory
	DESCRIPTION	"The node number of the other end of the channel to be 
			connected." 
	::= {chControlEntry 4}

chControlForceConnToCard OBJECT-TYPE
	SYNTAX	INTEGER (1..5)
	ACCESS	write-only
	STATUS	mandatory
	DESCRIPTION	"The module/card number of the other end of the channel 
			to be connected." 
	::= {chControlEntry 5}

chControlForceConnToChannel OBJECT-TYPE
	SYNTAX	INTEGER
	ACCESS	write-only
	STATUS	mandatory
	DESCRIPTION	"The channel number of the other end of the channel to be 
			connected." 
	::= {chControlEntry 6}


