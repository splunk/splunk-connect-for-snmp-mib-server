-- N_STATUS.O_VOICE =======================================================

voiceChStatusTable OBJECT-TYPE
	SYNTAX	SEQUENCE OF VoiceChStatusEntry
	ACCESS	not-accessible
	STATUS	mandatory
	DESCRIPTION	"Table of voice channel status indicators." 
	::= {status 4}

voiceChStatusEntry OBJECT-TYPE
	SYNTAX	VoiceChStatusEntry
	ACCESS	not-accessible
	STATUS	mandatory
	DESCRIPTION	"Entry within the table specific to a particular voice channel
			on a module/card for status indicators." 
	INDEX		{voiceChStatusCardNumber, voiceChStatusChannelNumber}
	::= {voiceChStatusTable 1}

VoiceChStatusEntry ::= SEQUENCE {
	voiceChStatusCardNumber		INTEGER,
	voiceChStatusChannelNumber		INTEGER,
	voiceChStatusInputLevel			INTEGER,
	voiceChStatusStatus			INTEGER, 
	voiceChStatusPromID			DisplayString, 
	voiceChStatusSoftwareRevision		DisplayString, 
	voiceChStatusTestMode			INTEGER, 
	voiceChStatusTestDSPResult		INTEGER,
	voiceChStatusTestDataRAMResult		INTEGER,
	voiceChStatusTestCodeRAMResult	INTEGER,
	voiceChStatusTestAnalogHwResult	INTEGER,
	voiceChStatusTestDeviceCfgResult		INTEGER,
	voiceChStatusFlashState			INTEGER,
	voiceChStatusEPROMStatus		INTEGER
	}

voiceChStatusCardNumber  OBJECT-TYPE
	SYNTAX	INTEGER (1..5)
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION	"Identifies the module/card number of the voice channel
			for status indications." 
	  ::= {voiceChStatusEntry 1}

voiceChStatusChannelNumber OBJECT-TYPE
	SYNTAX	INTEGER
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION	"Identifies the voice channel number for status indications." 
	  ::= {voiceChStatusEntry 2}

voiceChStatusInputLevel  OBJECT-TYPE
	SYNTAX	INTEGER {
		levelLessThanNeg25dBm (1),
		levelNeg24dBm (2),
		levelNeg21dBm (3),
		levelNeg18dBm (4),
		levelNeg15dBm (5),
		levelNeg12dBm (6),
		levelNeg10dBm (7),
		levelNeg8dBm (8),
		levelNeg6dBm (9),
		levelNeg5dBm (10),
		levelNeg4dBm (11),
		levelNeg3dBm (12),
		levelNeg2dBm (13),
		levelNeg1dBm (14),
		level0dBm (15),
		levelgreaterThan0dBm (16),
		levelNotAvailable (17)
		}
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION	"Indicates the level of the input signal monitored, in dBm." 
	::= {voiceChStatusEntry 3}

voiceChStatusStatus  OBJECT-TYPE
	SYNTAX	INTEGER {
		onHook (1),
		offHook (2),
		busy (3)
		}
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION	"Indicates the mode of the current voice channel operation." 
	::= {voiceChStatusEntry 4}

voiceChStatusPromID OBJECT-TYPE
	SYNTAX	DisplayString  (SIZE (0..4))
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION	"Indicates the Prom ID of the voice channel." 
	::= {voiceChStatusEntry 5}

voiceChStatusSoftwareRevision OBJECT-TYPE
	SYNTAX	DisplayString  (SIZE (0..3))
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION	"Indicates the software revision of the voice channel." 
	::= {voiceChStatusEntry 6}

voiceChStatusTestMode  OBJECT-TYPE
	SYNTAX	INTEGER {
		none (1),
		levelDisplay(2),
		localSelfTest (3),
		localLoopback (4),
		remoteSelfTest (5),
		remoteLoopback (6)
		}
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION	"Indicates the type of test that is currently running, if testing." 
	::= {voiceChStatusEntry 7}

voiceChStatusTestDSPResult  OBJECT-TYPE
	SYNTAX	INTEGER {
		ok (1),
		failed (2)
		}
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION	"Indicates the DSP test results of the voice channel following 
			initiation of a voice channel self test." 
	::= {voiceChStatusEntry 8}

voiceChStatusTestDataRAMResult  OBJECT-TYPE
	SYNTAX	INTEGER {
		ok (1),
		failed (2)
		}
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION	"Indicates the data RAM test results of the voice channel following 
			initiation of a voice channel self test." 
	::= {voiceChStatusEntry 9}

voiceChStatusTestCodeRAMResult  OBJECT-TYPE
	SYNTAX	INTEGER {
		ok (1),
		failed (2)
		}
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION	"Indicates the code RAM test results of the voice channel following 
			initiation of a voice channel self test." 
	::= {voiceChStatusEntry 10}

voiceChStatusTestAnalogHwResult  OBJECT-TYPE
	SYNTAX	INTEGER {
		ok (1),
		failed (2)
		}
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION	"Indicates the Analog Hardware test results of the voice channel 
			following initiation of a voice channel self test." 
	::= {voiceChStatusEntry 11}

voiceChStatusTestDeviceCfgResult  OBJECT-TYPE
	SYNTAX	INTEGER {
		ok (1),
		failed (2)
		}
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION	"Indicates the device configuration test results of the voice channel 
			following initiation of a voice channel self test." 
	::= {voiceChStatusEntry 12}

voiceChStatusFlashState OBJECT-TYPE
	SYNTAX	INTEGER {
		notInstalled (1),
		valid (2),
		erased (3),
		invalidChecksum (4),
		noAnalogDriver (5),
		securityViolation (6)
		}
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION	"Indicates the state of the flash memory residing on the voice 
			channel." 
	::= {voiceChStatusEntry 13}

voiceChStatusEPROMStatus OBJECT-TYPE
	SYNTAX	INTEGER {
		notInstalled (1),
		currentlyOperating (2),
		presentButNotOperating (3)
		}
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION	"Indicates the status of the EPROM memory residing on the voice 
			channel." 
	::= {voiceChStatusEntry 14}


-- N_STATUS.O_LNKCH =====================================================

linkStatusTable OBJECT-TYPE
	SYNTAX	SEQUENCE OF LinkStatusEntry
	ACCESS	not-accessible
	STATUS	mandatory
	DESCRIPTION	"Table of link channel status of the Integration unit." 
	::= {status 5}

linkStatusEntry OBJECT-TYPE
	SYNTAX	LinkStatusEntry
	ACCESS	not-accessible
	STATUS	mandatory
	DESCRIPTION	"Entry within the table specific to a particular channel
			number on a module/card for status of the links." 
	INDEX		{linkStatusCardNumber,  linkStatusChannelNumber}
	::= {linkStatusTable 1} 

LinkStatusEntry ::= SEQUENCE {
	linkStatusCardNumber			INTEGER,
	linkStatusChannelNumber			INTEGER,
	linkStatusLinkType			INTEGER,
	linkStatusIntegralDevType		INTEGER,
	linkStatusIntegralDevSlotNumber		INTEGER,
	linkStatusIntegralDeviceNumber		INTEGER,
	linkStatusStatus				INTEGER,
	linkStatusNodeNumber			INTEGER,
	linkStatusRemoteChannelNumber		INTEGER,
	linkStatusRemoteCardNumber		INTEGER,
	linkStatusEiaOutputsRTSStatus		INTEGER,
	linkStatusEiaOutputsDTRStatus		INTEGER,
	linkStatusEiaOutputsBUSYStatus		INTEGER,
	linkStatusEiaOutputsUAStatus		INTEGER,
	linkStatusEiaInputsRLSDStatus		INTEGER,
	linkStatusEiaInputsDSRStatus		INTEGER,
	linkStatusEiaInputsRIStatus		INTEGER,
	linkStatusEiaInputsCTSStatus		INTEGER
	}

