	SYNTAX	DisplayString  (SIZE (0..19))
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION	"Indicates the Prom ID file identifier of the Integration unit
			image file." 
	 ::= {flashMemoryStatusEntry 12}

-- N_CMOS.O_FCONNS  =====================================================

chForceConnStatusTable OBJECT-TYPE
	SYNTAX	SEQUENCE OF ChForceConnStatusEntry
	ACCESS	not-accessible
	STATUS	mandatory
	DESCRIPTION	"Table of channel force connects status.  Note that the
		table will be incomplete for Digital Voice modules (DVM) if the
		modules are not contiguous on the T1, E1, or Digital Voice Expansion
		modules. (Ex. - B1, B2, B5; table will only show B1 and B2, not B5.)" 
	::= {status 13}

chForceConnStatusEntry OBJECT-TYPE
	SYNTAX	ChForceConnStatusEntry
	ACCESS	not-accessible
	STATUS	mandatory
	DESCRIPTION	"Entry within the table specific to a particular channel number
			and module/card number for channel force connection status." 
	INDEX		{chForceConnStatusCardNumber, chForceConnStatusChannelNumber}
	::= {chForceConnStatusTable 1}

ChForceConnStatusEntry ::= SEQUENCE {
	chForceConnStatusCardNumber		INTEGER,
	chForceConnStatusChannelNumber	INTEGER,
	chForceConnStatusNodeNumber		INTEGER,
	chForceConnStatusToNodeNumber	INTEGER,
	chForceConnStatusToCardNumber		INTEGER,
	chForceConnStatusToChannelNumber	INTEGER
	}

chForceConnStatusCardNumber OBJECT-TYPE
	SYNTAX	INTEGER (1..5)
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION	"Identifies the module/card number of the source channel for
			channel force connection status." 
	::= {chForceConnStatusEntry 1}

chForceConnStatusChannelNumber OBJECT-TYPE
	SYNTAX	INTEGER (1..32)
	ACCESS	read-only
	STATUS	mandatory
