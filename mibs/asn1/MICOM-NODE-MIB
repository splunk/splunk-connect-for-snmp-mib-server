-- SWITCH STATISTICS

swAccumStatisticsNumOfCurrentConnects OBJECT-TYPE
	SYNTAX	Counter
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION	"Number of current connects for the accumulated
			statistics report." 
	::= { statistics-accumulated 6}

swAccumStatisticsTable OBJECT-TYPE
	SYNTAX	SEQUENCE OF SwAccumStatisticsEntry
	ACCESS	not-accessible
	STATUS	mandatory
	DESCRIPTION	"Table of switch statistics for accumulated 
			statistics reports." 
	::= { statistics-accumulated 7}

swAccumStatisticsEntry OBJECT-TYPE
	SYNTAX	SwAccumStatisticsEntry
	ACCESS	not-accessible
	STATUS	mandatory
	DESCRIPTION	"Entry within the table to specify a particular
			class number." 
	INDEX		{swAccumStatisticsClassNumber}
	::= {swAccumStatisticsTable 1}

SwAccumStatisticsEntry ::= SEQUENCE {
	swAccumStatisticsClassNumber			INTEGER,
	swAccumStatisticsMonth				INTEGER,
	swAccumStatisticsDay				INTEGER,
	swAccumStatisticsHours				INTEGER,
	swAccumStatisticsMinutes			INTEGER,
	swAccumStatisticsSeconds			INTEGER,
	swAccumStatisticsNumOfCurrentConnects 		Counter,
	swAccumStatisticsNumOfSuccessfulConnects	Counter,
	swAccumStatisticsNumOfUnsuccessfulAttempts	Counter,
	swAccumStatisticsMaxNumInQueue			INTEGER
	}

swAccumStatisticsClassNumber OBJECT-TYPE
	SYNTAX	INTEGER
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION	"Identifies the class number for accumulated statistics 
			report data." 
	::= {swAccumStatisticsEntry 1}

swAccumStatisticsMonth OBJECT-TYPE
	SYNTAX	INTEGER (1..12)
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION	"Month portion of the time stamp for the switch
			statistics accumulated statistics report." 
	::= {swAccumStatisticsEntry 2}

swAccumStatisticsDay OBJECT-TYPE
	SYNTAX	INTEGER (1..31)
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION	"Day portion of the time stamp for the switch
			statistics accumulated statistics report." 
	::= {swAccumStatisticsEntry 3}

swAccumStatisticsHours OBJECT-TYPE
	SYNTAX	INTEGER (0..23)
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION	"Hours portion of the time stamp for the switch
			statistics accumulated statistics report." 
	::= {swAccumStatisticsEntry 4}

swAccumStatisticsMinutes OBJECT-TYPE
	SYNTAX	INTEGER (0..59)
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION	"Minutes portion of the time stamp for the switch
			statistics accumulated statistics report." 
	::= {swAccumStatisticsEntry 5}

swAccumStatisticsSeconds OBJECT-TYPE
	SYNTAX	INTEGER (0..59)
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION	"Seconds portion of the time stamp for the switch
			statistics accumulated statistics report." 
	::= {swAccumStatisticsEntry 6}

swAccumStatisticsNumOfSuccessfulConnects OBJECT-TYPE
	SYNTAX	Counter
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION	"Number of successful connects for the switch statistics
			accumulated statistics report." 
	::= {swAccumStatisticsEntry 7}

swAccumStatisticsNumOfUnsuccessfulAttempts OBJECT-TYPE
	SYNTAX	Counter
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION	"Number of unsuccessful connects (attempts) for the 
			switch statistics accumulated statistics report." 
	::= {swAccumStatisticsEntry 8}

swAccumStatisticsMaxNumInQueue OBJECT-TYPE
	SYNTAX	INTEGER
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION	"Maximum number in the queue during report period
			for the switch statistics accumulated statistics report." 
	::= {swAccumStatisticsEntry 9}

