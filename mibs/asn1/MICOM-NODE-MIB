			data rate. Note: When the value of this parameter is changed,
			the link must be reset." 
	::= {esiLinkCfgEntry 3}

-- Pre-Configuration for Esi and ESI-Sec Link =========================================

esiOrSecLinkPreConfigTable OBJECT-TYPE
	SYNTAX	SEQUENCE OF EsiOrSecLinkPreConfigEntry
	ACCESS	not-accessible
	STATUS	mandatory
	DESCRIPTION	"Table to pre-configure the data rate of an ESI (aka Interconnect)
			Link (or secondary link) prior to configuring the link port.  By 
			using this table to 'preset' the data rate, only 1 node reset 
			is needed following the port link assignment to complete the 
			configuration." 
	::= {esi-link 2}

esiOrSecLinkPreConfigEntry OBJECT-TYPE
	SYNTAX	EsiOrSecLinkPreConfigEntry
	ACCESS	not-accessible
	STATUS	mandatory
	DESCRIPTION	"Entry within the table to specify a particular port for ESI or
			secondary link pre-configuration on a specific module/card." 
	INDEX		{esiOrSecLinkPreConfigCardNumber, esiOrSecLinkPreConfigChannelNumber}
	::= {esiOrSecLinkPreConfigTable 1} 

EsiOrSecLinkPreConfigEntry ::= SEQUENCE {
	esiOrSecLinkPreConfigCardNumber	INTEGER,
	esiOrSecLinkPreConfigChannelNumber	INTEGER,
	esiOrSecLinkPreConfigDataRate		INTEGER
	}

esiOrSecLinkPreConfigCardNumber OBJECT-TYPE
	SYNTAX	INTEGER (1..5)
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION	"Identifies the module/card for ESI or secondary link 
			pre-configuration." 
	::= {esiOrSecLinkPreConfigEntry 1}

esiOrSecLinkPreConfigChannelNumber OBJECT-TYPE
	SYNTAX	INTEGER (1..6)
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION	"Identifies the port number for ESI or secondary link 
			pre-configuration." 
	::= {esiOrSecLinkPreConfigEntry 2}

esiOrSecLinkPreConfigDataRate OBJECT-TYPE
	SYNTAX	INTEGER {
		external (1),
		rate2400Sync (2),
		rate4800Sync (3),
		rate9600Sync (4),
		rate12000Sync (5),
		rate14400Sync (6),
		rate16800Sync (7),
		rate19200Sync (8),
		rate38400Sync (9),
		rate56000Sync (10),
		rate64000Sync (11)
		}
	ACCESS	write-only
	STATUS	mandatory
	DESCRIPTION	"Sets the data rate in bits per second for ESI and secondary 
			link pre-configurations.  Note: The only valid data rate 
			value for Secondary links is 'external (1)'.  (Secondary links 
			exclusively use external clocking.)" 
	::= {esiOrSecLinkPreConfigEntry 3}

-- N_CMOS.O_LKACTV ========================================================

esiOrSecLinkAssignCfgTable OBJECT-TYPE
	SYNTAX	SEQUENCE OF EsiOrSecLinkAssignCfgEntry
	ACCESS	not-accessible
	STATUS	mandatory
	DESCRIPTION	"Table for configuring the activation and deactivation
			of ESI (aka Interconnect) and secondary links." 
	::= {esi-link 3}

esiOrSecLinkAssignCfgEntry OBJECT-TYPE
	SYNTAX	EsiOrSecLinkAssignCfgEntry
	ACCESS	not-accessible
	STATUS	mandatory
	DESCRIPTION	"Entry into the table pertaining to a specific ESI or secondary
			link for activation/deactivation configuration on a specific 
			module/card." 
	INDEX		{esiOrSecLinkAssignCfgCardNumber,
			 esiOrSecLinkAssignCfgChannelNumber}
	::= {esiOrSecLinkAssignCfgTable 1} 

EsiOrSecLinkAssignCfgEntry ::= SEQUENCE {
	esiOrSecLinkAssignCfgCardNumber				INTEGER,
	esiOrSecLinkAssignCfgChannelNumber				INTEGER,
	esiOrSecLinkAssignCfgLinkForcedOn				INTEGER,
	esiOrSecLinkAssignCfgSecLinkBackUpOnly			INTEGER,
	esiOrSecLinkAssignCfgUtilThresholdToActivateSec			INTEGER,
	esiOrSecLinkAssignCfgUtilThresholdToDeactivateSec		INTEGER,
	esiOrSecLinkAssignCfgTimeThresholdExceedToActivateSec		INTEGER,
	esiOrSecLinkAssignCfgTimeThresholdBelowToDeactivateSec	INTEGER,
	esiOrSecLinkAssignCfgTerminateCallonDeactivation		INTEGER
	}

esiOrSecLinkAssignCfgCardNumber OBJECT-TYPE
	SYNTAX	INTEGER
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION	"Identifies the module/card number specific to the ESI or 
			secondary link for activation/deactivation configuration." 
	 ::= {esiOrSecLinkAssignCfgEntry 1}

esiOrSecLinkAssignCfgChannelNumber OBJECT-TYPE
	SYNTAX	INTEGER
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION	"Identifies the ESI or secondary link port specific to the 
			activation/deactivation configuration." 
	 ::= {esiOrSecLinkAssignCfgEntry 2}

esiOrSecLinkAssignCfgLinkForcedOn OBJECT-TYPE
	SYNTAX	INTEGER {
		yes (1),
		no (2)
		}
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"Both the primary and secondary links can be configured 
			as Forced On.  This overrides the current time of day and 
			utilization restrictions for testing, or to provide bandwidth 
			during a normally off period without changing other 
			configuration.  The default is YES for primary links and 
			NO for secondary links." 
	 ::= {esiOrSecLinkAssignCfgEntry 3}

esiOrSecLinkAssignCfgSecLinkBackUpOnly OBJECT-TYPE
	SYNTAX	INTEGER {
		yes (1),
		no (2)
		}
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"If the primary or secondary link is configured to be off, 
			according to the time of day configuration, then the secondary 
			link will not be activated upon primary link. The default value, 
			Yes, indicates the secondary link is for backup only." 
	 ::= {esiOrSecLinkAssignCfgEntry 4}

esiOrSecLinkAssignCfgUtilThresholdToActivateSec OBJECT-TYPE
	SYNTAX	INTEGER (1..99)
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"This option allows utilization thresholds to be set for 
			activation of the secondary link. Utilization thresholds 
			are specified as a percentage of available bandwidth on the 
			primary link.  Secondary channels may be configured to become 
			active when the bandwidth utilization on the primary link 
			exceeds a predetermined threshold. The secondary link will 
			also be activated if primary link failure is detected. However, 
			the secondary link will not be activated if either the secondary 
			or primary link is configured to be off according to the time 
			of day configuration.  The default value is 75." 
	 ::= {esiOrSecLinkAssignCfgEntry 5}

esiOrSecLinkAssignCfgUtilThresholdToDeactivateSec OBJECT-TYPE
	SYNTAX	INTEGER (1..99)
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"This option allow utilization thresholds to be set for 
			deactivation of the secondary link.  The secondary link will 
			be de-activated when the total utilization drops below a configured 
			threshold.  The de-activation threshold value must be less than 
			the activation threshold value. The default value is 50." 
	 ::= {esiOrSecLinkAssignCfgEntry 6}

esiOrSecLinkAssignCfgTimeThresholdExceedToActivateSec OBJECT-TYPE
	SYNTAX	INTEGER (1..40)
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"Time (in minutes) that utilization threshold (to activate) must 
			be exceeded to activate a secondary link. The default value is 5." 
	 ::= {esiOrSecLinkAssignCfgEntry 7}

esiOrSecLinkAssignCfgTimeThresholdBelowToDeactivateSec OBJECT-TYPE
	SYNTAX	INTEGER (1..40)
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"Time (in minutes) that utilization threshold (to deactivate) must 
			be below to deactivate a secondary link. The default value is 5." 
	 ::= {esiOrSecLinkAssignCfgEntry 8}

esiOrSecLinkAssignCfgTerminateCallonDeactivation OBJECT-TYPE
	SYNTAX	INTEGER {
		yes (1),
		no (2)
		}
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"This parameter allows the option of leaving a link up if a 
			switched voice call is in place even if the time of day 
			configuration indicates it is time to deactivate the link. 
			Setting this parameter to NO will bring down the link only 
			when all switched voice calls have hung up. The default is NO." 
	 ::= {esiOrSecLinkAssignCfgEntry 9}

-- N_CMOS.O_TODSCH ========================================================

esiOrSecLinkTODActivationTable OBJECT-TYPE
	SYNTAX	SEQUENCE OF EsiOrSecLinkTODActivationEntry
	ACCESS	not-accessible
	STATUS	mandatory
	DESCRIPTION	"Table for configuring the Time Of Day (TOD) activation 
			and deactivation schedule of ESI (aka Interconnect) and 
			secondary links." 
	::= {esi-link 4}

esiOrSecLinkTODActivationEntry OBJECT-TYPE
	SYNTAX	EsiOrSecLinkTODActivationEntry
	ACCESS	not-accessible
	STATUS	mandatory
	DESCRIPTION	"Entry into the table pertaining to a specific module/card,
			port location, day schedule, and time schedule for setting
			ESI or secondary link activation/deactivation configurations." 
	INDEX		{esiOrSecLinkTODActivationCardNumber,
			 esiOrSecLinkTODActivationChannelNumber,
			esiOrSecLinkTODActivationSetSchedule,
			esiOrSecLinkTODActivationTenMinInterval}
	::= {esiOrSecLinkTODActivationTable 1} 

EsiOrSecLinkTODActivationEntry ::= SEQUENCE {
	esiOrSecLinkTODActivationCardNumber		INTEGER,
	esiOrSecLinkTODActivationChannelNumber	INTEGER,
	esiOrSecLinkTODActivationSetSchedule		INTEGER,
	esiOrSecLinkTODActivationTenMinInterval	INTEGER,
	esiOrSecLinkTODActivationLinkState		INTEGER
	}

esiOrSecLinkTODActivationCardNumber OBJECT-TYPE
	SYNTAX	INTEGER
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION	"Identifies the module/card number specific to an ESI or secondary
			link for the TOD activation/deactivation schedule." 
	 ::= {esiOrSecLinkTODActivationEntry 1}

esiOrSecLinkTODActivationChannelNumber OBJECT-TYPE
	SYNTAX	INTEGER
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION	"Identifies the port location specific to an ESI or secondary
			link for the TOD activation/deactivation schedule." 
	 ::= {esiOrSecLinkTODActivationEntry 2}

esiOrSecLinkTODActivationSetSchedule OBJECT-TYPE
	SYNTAX	INTEGER {
		sunday (1),
		weekdays (2),
		saturday (3)
		}
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION	"Identifies the particular day(s) schedule specific to an ESI 
			or secondary link for the TOD activation/deactivation schedule." 
	 ::= {esiOrSecLinkTODActivationEntry 3}

esiOrSecLinkTODActivationTenMinInterval OBJECT-TYPE
	SYNTAX	INTEGER (1..144)
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION	"Identifies the particular time of day (in 10 minute intervals)
			specific to an ESI or secondary link for the TOD 
			activation/deactivation schedule." 
	 ::= {esiOrSecLinkTODActivationEntry 4}

esiOrSecLinkTODActivationLinkState OBJECT-TYPE
	SYNTAX	INTEGER {
		off (1),
		on (2)
		}
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"Sets the TOD link state for an ESI or secondary link.  This 
			parameter is used to configure a link to active (on) or not active 
			(off) for a particular 10 minute interval as specified by the 
			previous 4 indexing parameters contained in this table." 
	 ::= {esiOrSecLinkTODActivationEntry 5}


x21LinkCfgTable OBJECT-TYPE
	SYNTAX	SEQUENCE OF X21LinkCfgEntry
	ACCESS	not-accessible
	STATUS	mandatory
	DESCRIPTION	"Table to configure the X.21 links. It defines the X.21 parameters
			for the Integration unit and for the MICOM mux side." 
	::= {x-dot-21-link 1}

x21LinkCfgEntry OBJECT-TYPE
	SYNTAX	X21LinkCfgEntry
	ACCESS	not-accessible
	STATUS	mandatory
	DESCRIPTION	"Entry in the table for a particular X.21 link.  Note that a link 
			reset is required if any value in this table is changed." 
	INDEX		{x21LinkCfgCardNumber, x21LinkCfgChannelNumber}
	::= {x21LinkCfgTable 1} 

X21LinkCfgEntry ::= SEQUENCE {
	x21LinkCfgCardNumber			INTEGER,
	x21LinkCfgChannelNumber		INTEGER,
	x21LinkCfgLocalConnectMode		INTEGER,
	x21LinkCfgRemoteConnectMode		INTEGER,
	x21LinkCfgLocalConnectTimeout		INTEGER,
	x21LinkCfgRemoteConnectTimeout	INTEGER,
	x21LinkCfgLocalConnectRetries		INTEGER,
	x21LinkCfgRemoteConnectRetries		INTEGER
	}

x21LinkCfgCardNumber OBJECT-TYPE
	SYNTAX	INTEGER
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION	"The card/module number of the X.21 link channel the entry pertains to." 
	::= {x21LinkCfgEntry 1}

x21LinkCfgChannelNumber OBJECT-TYPE
	SYNTAX	INTEGER
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION	"The particular X.21 link channel on specific module to be configured." 
	::= {x21LinkCfgEntry 2}

x21LinkCfgLocalConnectMode OBJECT-TYPE
	SYNTAX	INTEGER {
		dataActivityConnect (1),
		dtrConnect (2)
		}
	ACCESS	read-write
	STATUS	mandatory
--	DEFVAL	1
	DESCRIPTION	"Configures the Integration unit side of the X.21 link.  
			In Integration units, the only connect mode for X.21 
			links is data activity connect. In this mode, the Integration
			unit will place a call over the X.21 network whenever data 
			activity is detected on any force connected channel, matrix
			connection, or port connected to a class defined with ports on 
			a MICOM mux attached to an X.21 link." 
	::= {x21LinkCfgEntry 3}

x21LinkCfgRemoteConnectMode OBJECT-TYPE
	SYNTAX	INTEGER {
		dataActivityConnect(1),
		dtrConnect (2)
		}
	ACCESS	read-write
	STATUS	mandatory
--	DEFVAL	1
	DESCRIPTION	"Configures the MICOM mux side of the X.21 link. There 
			are two options: DTR and Data Activity Connect.  DTR 
			(DTR connect) establishes the X.21 connection whenever 
			DTR is raised on any terminal connected to the MICOM mux.
			Data Activity Connect establishes the X.21 connection 
			whenever data activity is detected on any terminal connected 
			to the MICOM mux." 
	::= {x21LinkCfgEntry 4}

x21LinkCfgLocalConnectTimeout OBJECT-TYPE
	SYNTAX	INTEGER (0..20)
	ACCESS	read-write
	STATUS	mandatory
--	DEFVAL	5
	DESCRIPTION	"The value in this field specifies the amount of time that if no 
			data activity is detected, the X.21 connection will be dropped. 
			The default is 5 seconds." 
	::= {x21LinkCfgEntry 5}

x21LinkCfgRemoteConnectTimeout OBJECT-TYPE
	SYNTAX	INTEGER (0..20)
	ACCESS	read-write
	STATUS	mandatory
--	DEFVAL	5
	DESCRIPTION	"If data activity connect mode is selected for the remote MICOM 
			mux, this field specifies the amount of time that if no data 
			activity is detected on any of the MICOM mux's channels, the X.21 
			connection will be dropped. The default is 5 seconds." 
	::= {x21LinkCfgEntry 6}

x21LinkCfgLocalConnectRetries OBJECT-TYPE
	SYNTAX	INTEGER (1..64)
	ACCESS	read-write
	STATUS	mandatory
--	DEFVAL	20
	DESCRIPTION	"Configures the number of attempts the Integration unit will make to
			place a call over the X.21 network." 
	::= {x21LinkCfgEntry 7}

x21LinkCfgRemoteConnectRetries OBJECT-TYPE
	SYNTAX	INTEGER (1..64)
	ACCESS	read-write
	STATUS	mandatory
--	DEFVAL	20
	DESCRIPTION	"Configures the number of attempts the MICOM mux will 
			make to place a call over the X.21 network." 
	::= {x21LinkCfgEntry 8}

-- PreConfig x.21 ==============================================================

x21LinkPreConfigTable OBJECT-TYPE
	SYNTAX	SEQUENCE OF X21LinkPreConfigEntry
	ACCESS	not-accessible
	STATUS	mandatory
	DESCRIPTION	"Table to configure an X.21 link prior to configuring
			a port as an X.21 link. By using this table to 'preset' the X.21
			link parameters, only 1 node reset is needed following the X.21
			assignment to complete the configuration."
	::= {x-dot-21-link 2}

x21LinkPreConfigEntry OBJECT-TYPE
	SYNTAX	X21LinkPreConfigEntry
	ACCESS	not-accessible
	STATUS	mandatory
	DESCRIPTION	"Entry within the table to specify a particular port number for
			pre-configuration of an X.21 link on a specific module/card." 
	INDEX		{x21LinkPreConfigCardNumber, x21LinkPreConfigChannelNumber}
	::= {x21LinkPreConfigTable 1} 

X21LinkPreConfigEntry ::= SEQUENCE {
	x21LinkPreConfigCardNumber			INTEGER,
	x21LinkPreConfigChannelNumber			INTEGER,
	x21LinkPreConfigLocalConnectMode		INTEGER,
	x21LinkPreConfigRemoteConnectMode		INTEGER,
	x21LinkPreConfigLocalConnectTimeout		INTEGER,
	x21LinkPreConfigRemoteConnectTimeout	INTEGER,
	x21LinkPreConfigLocalConnectRetries		INTEGER,
	x21LinkPreConfigRemoteConnectRetries		INTEGER
	}

x21LinkPreConfigCardNumber OBJECT-TYPE
	SYNTAX	INTEGER (1..5)
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION	"The card/module number of the X.21 link channel to be pre-configured." 
	::= {x21LinkPreConfigEntry 1}

x21LinkPreConfigChannelNumber OBJECT-TYPE
	SYNTAX	INTEGER (1..6)
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION	"The particular X.21 link channel on specific module to be 
			pre-configured." 
	::= {x21LinkPreConfigEntry 2}

x21LinkPreConfigLocalConnectMode OBJECT-TYPE
	SYNTAX	INTEGER {
		dataActivityConnect (1),
		dtrConnect (2)
		}
	ACCESS	write-only 
	STATUS	mandatory
	DESCRIPTION	"Pre-configures the Integration unit side of the X.21 link.  
			In Integration units, the only connect mode for X.21 
			links is data activity connect. In this mode, the Integration
			unit will place a call over the X.21 network whenever data 
			activity is detected on any force connected channel, matrix
			connection, or port connected to a class defined with ports on 
			a MICOM mux attached to an X.21 link." 
	::= {x21LinkPreConfigEntry 3}

x21LinkPreConfigRemoteConnectMode OBJECT-TYPE
	SYNTAX	INTEGER {
		dataActivityConnect(1),
		dtrConnect (2)
		}
	ACCESS	write-only 
	STATUS	mandatory
	DESCRIPTION	"Pre-configures the MICOM mux side of the X.21 link. There 
			are two options: DTR and Data Activity Connect.  DTR 
			(DTR connect) establishes the X.21 connection whenever 
			DTR is raised on any terminal connected to the MICOM mux.
			Data Activity Connect establishes the X.21 connection 
			whenever data activity is detected on any terminal connected 
			to the MICOM mux." 
	::= {x21LinkPreConfigEntry 4}

x21LinkPreConfigLocalConnectTimeout OBJECT-TYPE
	SYNTAX	INTEGER (0..20)
	ACCESS	write-only 
	STATUS	mandatory
	DESCRIPTION	"Pre-configuration of this field specifies the amount of time that  
			if no data activity is detected, the X.21 connection will be dropped." 
--	DEFVAL	5
	::= {x21LinkPreConfigEntry 5}

x21LinkPreConfigRemoteConnectTimeout OBJECT-TYPE
	SYNTAX	INTEGER (0..20)
	ACCESS	write-only 
	STATUS	mandatory
--	DEFVAL	5
	DESCRIPTION	"If data activity connect mode is selected for the remote MICOM 
			mux, pre-configuration of this field specifies the amount of time 
			that if no data activity is detected on any of the MICOM mux's 
			channels, the X.21 connection will be dropped." 
	::= {x21LinkPreConfigEntry 6}

x21LinkPreConfigLocalConnectRetries OBJECT-TYPE
	SYNTAX	INTEGER (1..64)
	ACCESS	write-only 
	STATUS	mandatory
--	DEFVAL	20
	DESCRIPTION	"Pre-configures the number of attempts the Integration unit will 
			make to place a call over the X.21 network." 
	::= {x21LinkPreConfigEntry 7}

x21LinkPreConfigRemoteConnectRetries OBJECT-TYPE
	SYNTAX	INTEGER (1..64)
	ACCESS	write-only 
	STATUS	mandatory
--	DEFVAL	20
	DESCRIPTION	"Pre-configures the number of attempts the MICOM mux will 
			make to place a call over the X.21 network." 
	::= {x21LinkPreConfigEntry 8}


localESILinkCfgTable OBJECT-TYPE
	SYNTAX	SEQUENCE OF LocalESILinkCfgEntry
	ACCESS	not-accessible
	STATUS	mandatory
	DESCRIPTION	"Table to configure the data rate for a local ESI link
			(aka Local Interconnect Link or LIL)." 
	::= {local-esi-link 1}

localESILinkCfgEntry OBJECT-TYPE
	SYNTAX	LocalESILinkCfgEntry
	ACCESS	not-accessible
	STATUS	mandatory
	DESCRIPTION	"Entry within the table to specify a particular port number
			on a specific module/card for an LIL configuration." 
	INDEX		{localESILinkCfgCardNumber, localESILinkCfgChannelNumber}
	::= {localESILinkCfgTable 1} 

LocalESILinkCfgEntry ::= SEQUENCE {
	localESILinkCfgCardNumber	INTEGER,
	localESILinkCfgChannelNumber	INTEGER,
	localESILinkCfgDataRate		INTEGER
	}

