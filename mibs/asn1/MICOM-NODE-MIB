	SYNTAX  INTEGER {
		yes (1),
		no (2)
		}
	ACCESS  read-only
	STATUS  mandatory
	DESCRIPTION	"Indicates the status of the DLCI channel; active or not." 
	::= {frameRelayLinkDLCIStatusEntry 7}

