	::= {linkAccumStatisticsEntry 24}

-- FRAME RELAY LINK STATISTICS

linkFRAccumStatisticsTable OBJECT-TYPE
	SYNTAX	SEQUENCE OF LinkFRAccumStatisticsEntry
	ACCESS	not-accessible
	STATUS	mandatory
	DESCRIPTION	"Table of Frame Relay link statistics for accumulated 
			statistics reports. (Note: More accumulated FR link stats
			are contained in table linkFRAccLnkAccumStatisticsTable.)" 
	::= { statistics-accumulated 4}

linkFRAccumStatisticsEntry OBJECT-TYPE
	SYNTAX	LinkFRAccumStatisticsEntry
	ACCESS	not-accessible
	STATUS	mandatory
	DESCRIPTION	"Entry within the table to specify a particular FR link
			channel on specific module/card." 
	INDEX		{linkFRAccumStatisticsCardNumber, linkFRAccumStatisticsChannelNumber }
	::= {linkFRAccumStatisticsTable 1}

LinkFRAccumStatisticsEntry ::= SEQUENCE {
	linkFRAccumStatisticsCardNumber		INTEGER,
	linkFRAccumStatisticsChannelNumber		INTEGER,
	linkFRAccumStatisticsMonth			INTEGER,
	linkFRAccumStatisticsDay			INTEGER,
	linkFRAccumStatisticsHours			INTEGER,
	linkFRAccumStatisticsMinutes			INTEGER,
	linkFRAccumStatisticsSeconds			INTEGER,
	linkFRAccumStatisticsCRCErrs			Counter,
	linkFRAccumStatisticsLostStatusMsgCounts	Counter,
	linkFRAccumStatisticsStatusMsgProtocolErrs	Counter,
	linkFRAccumStatisticsNetworkResets		Counter,
	linkFRAccumStatisticsLinkResets			Counter,
	linkFRAccumStatisticsNumOfDLCIs		INTEGER
	}

linkFRAccumStatisticsCardNumber OBJECT-TYPE
	SYNTAX	INTEGER (1..5)
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION	"Identifies the module/card number for the FR
			link channel specific to the accumulated statistics 
			report data." 
	::= {linkFRAccumStatisticsEntry 1}

linkFRAccumStatisticsChannelNumber	OBJECT-TYPE
	SYNTAX	INTEGER
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION	"Identifies the FR link channel number specific to
			 the accumulated statistics report data." 
	::= {linkFRAccumStatisticsEntry 2}

linkFRAccumStatisticsMonth OBJECT-TYPE
	SYNTAX	INTEGER
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION	"Month portion of the time stamp for the FR link 
			channel accumulated statistics report." 
	::= {linkFRAccumStatisticsEntry 3}
	
linkFRAccumStatisticsDay OBJECT-TYPE
	SYNTAX	INTEGER
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION	"Day portion of the time stamp for the FR link 
			channel accumulated statistics report." 
	::= {linkFRAccumStatisticsEntry 4}

linkFRAccumStatisticsHours OBJECT-TYPE
	SYNTAX	INTEGER
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION	"Hours portion of the time stamp for the FR link 
			channel accumulated statistics report." 
	::= {linkFRAccumStatisticsEntry 5}

linkFRAccumStatisticsMinutes OBJECT-TYPE
	SYNTAX	INTEGER
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION	"Minutes portion of the time stamp for the FR link 
			channel accumulated statistics report." 
	::= {linkFRAccumStatisticsEntry 6}

linkFRAccumStatisticsSeconds OBJECT-TYPE
	SYNTAX	INTEGER
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION	"Seconds portion of the time stamp for the FR link 
			channel accumulated statistics report." 
	::= {linkFRAccumStatisticsEntry 7}

linkFRAccumStatisticsCRCErrs OBJECT-TYPE
	SYNTAX	Counter
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION	"CRC errors on the specified FR link access
			channel in the accumulated statistics report." 
	::= {linkFRAccumStatisticsEntry 8}

linkFRAccumStatisticsLostStatusMsgCounts OBJECT-TYPE
	SYNTAX	Counter
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION	"Lost status messsage count of the specified FR 
			link channel in the accumulated statistics report." 
	::= {linkFRAccumStatisticsEntry 9}

linkFRAccumStatisticsStatusMsgProtocolErrs OBJECT-TYPE
	SYNTAX	Counter
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION	"Status message protocol errors of the specified FR 
			link channel in the accumulated statistics report." 
	::= {linkFRAccumStatisticsEntry 10}

linkFRAccumStatisticsNetworkResets OBJECT-TYPE
	SYNTAX	Counter
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION	"Resets initiated by the network of the specified FR 
			link channel in the accumulated statistics report." 
	::= {linkFRAccumStatisticsEntry 11}

linkFRAccumStatisticsLinkResets OBJECT-TYPE
	SYNTAX	Counter
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION	"Resets initiated by the Marathon of the specified FR 
			link channel in the accumulated statistics report." 
	::= {linkFRAccumStatisticsEntry 12}

linkFRAccumStatisticsNumOfDLCIs OBJECT-TYPE
	SYNTAX	INTEGER
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION	"Number of configured DLCIs for the specified FR 
			link channel in the accumulated statistics report." 
	::= {linkFRAccumStatisticsEntry 13}

