	SYNTAX	SEQUENCE OF T1DS1InterfaceEntry
	ACCESS	not-accessible
	STATUS	mandatory
	DESCRIPTION	"Table for Network T1 Interface for long-haul connection to the
			telephone network." 
	::= {t1-interface 1}

t1DS1InterfaceEntry OBJECT-TYPE
	SYNTAX	T1DS1InterfaceEntry
	ACCESS	not-accessible
	STATUS	mandatory
	DESCRIPTION	"Entry within the table for specified (Network Interface) DSO 
			(1..24) channels." 
	INDEX		{ t1DS1InterfaceDS0Index}
	::= {t1DS1InterfaceTable 1}

