	SYNTAX	INTEGER (1..5)
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION	"Identifies the specific module/card number for an LIL
			configuration." 
	::= {localESILinkCfgEntry 1}

localESILinkCfgChannelNumber OBJECT-TYPE
	SYNTAX	INTEGER (1..6)
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION	"Identifies the specific port number for an LIL configuration." 
	::= {localESILinkCfgEntry 2}

localESILinkCfgDataRate OBJECT-TYPE
	SYNTAX	INTEGER {
		external (1),
		rate64000 (2),
		rate126000 (3),
		rate192000 (4),
		rate252000 (5)
		}
	ACCESS	read-write
	STATUS	mandatory
--	DEFVAL	1
	DESCRIPTION	"Sets the link clocking method to external, or if internal, the
			data rate for an LIL. Note: When the value of this parameter is 
			changed, the link must be reset." 
	::= {localESILinkCfgEntry 3}


