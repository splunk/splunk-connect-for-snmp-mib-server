		rate9600Sync (4),
		rate12000Sync (5),
		rate14400Sync (6),
		rate16800Sync (7),
		rate19200Sync (8),
		rate38400Sync (9),
		rate56000Sync (10),
		rate64000Sync (11)
		}
	ACCESS	read-write
	STATUS	mandatory
