	SYNTAX	INTEGER (1..255)
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION	"Indicates the Node ID # of the source channel for
			channel force connection status." 
	::= {chForceConnStatusEntry 3}

chForceConnStatusToNodeNumber OBJECT-TYPE
	SYNTAX	INTEGER (1..255)
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION	"Indicates the Node ID # of the destination channel for
			channel force connection status." 
	::= {chForceConnStatusEntry 4}

chForceConnStatusToCardNumber OBJECT-TYPE
	SYNTAX	INTEGER (1..5)
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION	"Identifies the module/card number of the destination channel
			for channel force connection status." 
	::= {chForceConnStatusEntry 5}

chForceConnStatusToChannelNumber OBJECT-TYPE
	SYNTAX	INTEGER (1..32)
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION	"Identifies the destination channel number for channel force 
			connection status." 
	::= {chForceConnStatusEntry 6}


-- Statistics Group

-- N_CMOS.O_CMDRPT ==================================================

sysPeriodicStatisticsReportGroup    OBJECT IDENTIFIER ::= {statistics-configuration 1}

sysPeriodicStatisticsReportInterval	 OBJECT-TYPE
	SYNTAX	INTEGER {
		zero (1),
		ten (2),
		twenty (3),
		thirty (4),
		sixty (5)
		}
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"Time interval (in minutes) to collect
			data for a periodic statistics report." 
	::= { sysPeriodicStatisticsReportGroup 1}

sysReportDestination OBJECT-TYPE
	SYNTAX	INTEGER {
		localCommandPort (1),
		localLogPort (2),
		remoteNode (3)
		}
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"Destination for statistics reports to be routed." 
	::= { sysPeriodicStatisticsReportGroup 2}


