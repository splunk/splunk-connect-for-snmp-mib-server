mcmAlarmE1L2RemoteYAlarm TRAP-TYPE
	ENTERPRISE	micom
	VARIABLES	{trapNodeName}
	DESCRIPTION "A Remote 'Y' Signaling Alarm condition has occurred on
				the Line 2 (L2) interface.  This is the result of the
				EAM setting the 'Y' bit in time slot 16 of frame 0.  The
				condition indicates the loss of signaling at the remote
				site."
	::= 169

mcmAlarmE1L2RemoteYAlarmClear TRAP-TYPE
	ENTERPRISE	micom
	VARIABLES	{trapNodeName}
	DESCRIPTION "The E1 Remote 'Y' Signaling Alarm condition on the L2
				interface has cleared."
	::= 170

mcmAlarmE1L1RemoteYAlarm TRAP-TYPE
	ENTERPRISE	micom
	VARIABLES	{trapNodeName}
	DESCRIPTION "A Remote 'Y' Signaling Alarm condition has occurred on
				the Line 1 (L1) interface.  This is the result of the
				EAM setting the 'Y' bit in time slot 16 of frame 0.  The
				condition indicates the loss of signaling at the remote
				site."
	::= 171

mcmAlarmE1L1RemoteYAlarmClear TRAP-TYPE
	ENTERPRISE	micom
	VARIABLES	{trapNodeName}
	DESCRIPTION "The E1 Remote 'Y' Signaling Alarm condition on the L1
				interface has cleared."
	::= 172

mcmAlarmE1L2RemoteAAlarm TRAP-TYPE
	ENTERPRISE	micom
	VARIABLES	{trapNodeName}
	DESCRIPTION "A Remote 'A' Signaling Alarm condition has occurred on
				the Line 2 (L2) interface.  This Frame Alignment Signaling
				condition occurs when the 'A' bit, received in Bit 3 of
				Time Slot 0 of the Non Frame Alignment Signal (NFAS), is
				detected.  The alarm indicates that the receiver at the
				remote is not detecting valid information; which could
				be the result of a loss of signal, loss of frame
				synchronization, or high-bit errors."
	::= 173

mcmAlarmE1L2RemoteAAlarmClear TRAP-TYPE
	ENTERPRISE	micom
	VARIABLES	{trapNodeName}
	DESCRIPTION "The E1 Remote 'A' Signaling Alarm condition on the L2
				interface has cleared."
	::= 174

mcmAlarmE1L1RemoteAAlarm TRAP-TYPE
	ENTERPRISE	micom
	VARIABLES	{trapNodeName}
	DESCRIPTION "A Remote 'A' Signaling Alarm condition has occurred on
				the Line 1 (L1) interface.  This Frame Alignment Signaling
				condition occurs when the 'A' bit, received in Bit 3 of
				Time Slot 0 of the Non Frame Alignment Signal (NFAS), is
				detected.  The alarm indicates that the receiver at the
				remote is not detecting valid information; which could
				be the result of a loss of signal, loss of frame
				synchronization, or high-bit errors."
	::= 175

mcmAlarmE1L1RemoteAAlarmClear TRAP-TYPE
	ENTERPRISE	micom
	VARIABLES	{trapNodeName}
	DESCRIPTION "The E1 Remote 'A' Signaling Alarm condition on the L1
				interface has cleared."
	::= 176

mcmProxyAlarmNodeUnreachable TRAP-TYPE
	ENTERPRISE	micom
	VARIABLES	{trapNodeName}
	DESCRIPTION "The proxy agent's event polling application could not reach 
				the intended node."
	::= 500


-- Proxy Object Group

pxyVersionNumber OBJECT-TYPE
	SYNTAX	DisplayString
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION	"Identifies the version number of the proxy agent installed." 
	 ::= { proxy 1}

pxyCfgProxySwitch OBJECT-TYPE
	SYNTAX	INTEGER {
		enable (1),
		disable (2)
		}
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"To enable or disable the proxy agent operation on the
			router module." 
	 ::= {proxy 2}

pxyAddressMapTableNumber OBJECT-TYPE
	SYNTAX	INTEGER
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION	"The number of entries in the address map table. This will give 
			 the highest numbered entry allocated."
	::= {proxy 3}

pxyAddressMapTable OBJECT-TYPE
	SYNTAX	SEQUENCE OF PxyAddressMapEntry
	ACCESS	not-accessible
	STATUS	mandatory
	DESCRIPTION	"A table of all the MICOM Nodes this proxy is responsible for."
	::= {proxy 4}

pxyAddressMapEntry OBJECT-TYPE
	SYNTAX	PxyAddressMapEntry
	ACCESS	not-accessible
	STATUS	mandatory
	DESCRIPTION	"An entry that describes the network node which this proxy
			agent manages."
	INDEX		{ pxyAddressMapNodeNumber }
	::= {pxyAddressMapTable 1}

PxyAddressMapEntry ::= SEQUENCE {
	pxyAddressMapNodeNumber		INTEGER,
	pxyAddressMapEntryStatus		INTEGER,
	pxyAddressMapNodeNameAndCommunity	DisplayString,
	pxyAddressMapChangeNodeNumberTo	INTEGER
	}

pxyAddressMapNodeNumber OBJECT-TYPE
	SYNTAX	INTEGER 
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION	"The node number used to communicate with the device associated 
			 with this entry."
	::= {pxyAddressMapEntry 1}

pxyAddressMapEntryStatus OBJECT-TYPE
	SYNTAX	INTEGER {
		active (1),
		notInService (2),
		notReady (3),
		createAndGo (4),
		createAndWait (5),
		destroy (6)
		}
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"A code representing the current state of  the table entry. This is 
			provided to define relationship within the table entry and to delete 
			and/or add devics to the Address Table. Default is notReady "
	::= {pxyAddressMapEntry 2}


pxyAddressMapNodeNameAndCommunity OBJECT-TYPE
	SYNTAX	DisplayString  (SIZE (1..8))
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"The node name used to communicate with the device associated with
			 this entry. This unique name is also used to de-multiplex the proxy 
			managed nodes."
	::= {pxyAddressMapEntry 3}

pxyAddressMapChangeNodeNumberTo OBJECT-TYPE
	SYNTAX	INTEGER 
	ACCESS	write-only
	STATUS	mandatory
	DESCRIPTION	"In order to change a node number of an existing node you must
			use this entry with the NodeNumber entry being the existing node
			number."
	::= {pxyAddressMapEntry 4}




pxySupportedVersionTable OBJECT-TYPE
	SYNTAX	SEQUENCE OF PxySupportedVersionEntry
	ACCESS	not-accessible
	STATUS	mandatory
	DESCRIPTION	"A table of sotware revision numbers this proxy agent support."
	::= { proxy 5}

pxySupportedVersionEntry OBJECT-TYPE
	SYNTAX	PxySupportedVersionEntry
	ACCESS	not-accessible
	STATUS	mandatory
	DESCRIPTION	"An entry that describes the version number of the software this 
			proxy agent supports."
	INDEX		{pxySupportedIndex}
	::= {pxySupportedVersionTable 1}

PxySupportedVersionEntry ::= SEQUENCE {
	pxySupportedIndex			INTEGER,
	pxySupportedVersionNumber 		INTEGER 
	}

pxySupportedIndex OBJECT-TYPE
	SYNTAX	INTEGER
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION	"Identifies the instance and hence the row of the entry in this table."
	 ::= { pxySupportedVersionEntry 1}

pxySupportedVersionNumber OBJECT-TYPE
	SYNTAX	INTEGER {
		phase5dot0 (1),
		phase5dot0plus (2)
		}
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION	"Identifies the Node Software Version number this proxy agent
			understands and supports."
	 ::= { pxySupportedVersionEntry 2}



-- Proxy Statistics Object Group
-- This group maintains statistics of the different operational and exceptional conditions occurred 
-- while communicating to the managed nodes. All the statistics are maintained since the last reboot 
-- of the system

pxyStatisticsLastNodeServed OBJECT-TYPE
	SYNTAX	INTEGER
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION	"Represent the last successul transaction between this proxy
			agent and the foreign node."
	::= { proxy 6}

pxyStatisticsLastErrorNode OBJECT-TYPE
	SYNTAX	INTEGER
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION	"Represent the last un-successful transaction of this proxy agent
			and the foreign node."
	::= { proxy 7}

pxyStatisticsLastSNMPError OBJECT-TYPE
	SYNTAX	INTEGER
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION	"The SNMP error from the last transaction of this proxy agent with
			the node referenced by pxyStatisticsLastErrorNode."
	::= {proxy 8}

pxyStatisticsLastErrorReturnCode OBJECT-TYPE
	SYNTAX	INTEGER
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION	"The specific error code from the last transaction of this proxy 
			agent with the node referenced by pxyStatisticsLastErrorNode."
	::= {proxy 9}

pxyStatisticsTotalRequests OBJECT-TYPE
	SYNTAX	Counter
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION	"A count of requests received since the last reboot of the system."
	::= { proxy 10}

pxyStatisticsTotalInternalTraps OBJECT-TYPE
	SYNTAX	Counter
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION	"A count of internal Traps transmitted since the last reboot of 
			the system."
	::= { proxy 11}


-- Proxy PPP Object Group
-- This group maintains the statistics for PPP protocol operations.

pxyPPPTotalCounts OBJECT-TYPE
	SYNTAX	Counter
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION	"A count of translated PPP requests transmitted to the managed nodes 
			since last reboot of the system."
	::= {proxy 12}

pxyPPPRejectCounts OBJECT-TYPE
	SYNTAX	Counter
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION	"A count of rejected PPP requests transmitted to the managed nodes 
			since last reboot of the system."
	::= {proxy 13}

pxyCacheUpdate OBJECT-TYPE
	SYNTAX	INTEGER {
		cdChanTable (1)
		}
	ACCESS	write-only
	STATUS	mandatory
	DESCRIPTION	"Configures to force update the cached information maintained
			in proxy agent." 
	 ::= { proxy 14}


pxyNetworkAccessNodeName OBJECT-TYPE
	SYNTAX	DisplayString  (SIZE (1..8))
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"This is the name of the Integration unit that the
			proxy is physically attached to.  This node must already be
			in the proxy's Address Map in order to set it here.  This
			NodeName is stored in the FileControlFile.Data file on the
			host that the proxy is installed on."
	::= { proxy 15 }


-- Control Group

-- N_HOUSE.O_RESET ========================================================

nodeResetGroup OBJECT IDENTIFIER ::= {control 1}

nodeResetCode OBJECT-TYPE
	SYNTAX	INTEGER {
		coldRestart (1),
		warmRestart (2)
		}
	ACCESS	write-only
	STATUS	mandatory
	DESCRIPTION	"Resets the local node and configures to restart with the factory
			default values or retain the current configuration; all channels 
			connected locally are also reset." 
	 ::= {nodeResetGroup 1}

nodeResetType OBJECT-TYPE
	SYNTAX	INTEGER {
		restartCurrentSoftware (1),
		reloadSoftwareFromFlash (2)
		}
	ACCESS	write-only
	STATUS	mandatory
	DESCRIPTION	"Configures to retstart with the current operational code or restart
			and load the image from the flash." 
	 ::= {nodeResetGroup 2}


-- N_HOUSE.O_ASYNC ========================================================

asyncResetTable OBJECT-TYPE
	SYNTAX	SEQUENCE OF AsyncResetEntry
	ACCESS	not-accessible
	STATUS	mandatory
	DESCRIPTION	"Table to reset an async channel." 
	::= { control 2}

asyncResetEntry OBJECT-TYPE
	SYNTAX	AsyncResetEntry
	ACCESS	not-accessible
	STATUS	mandatory
	DESCRIPTION	"Entry within the table specifying particular channel on 
			specific module/card." 
	INDEX		{asyncResetCardNumber, asyncResetChannelNumber}
	::= {asyncResetTable 1}

AsyncResetEntry ::= SEQUENCE {
	asyncResetCardNumber		INTEGER,
	asyncResetChannelNumber	INTEGER,
	asyncResetCommand		INTEGER
	}

asyncResetCardNumber OBJECT-TYPE
	SYNTAX	INTEGER (1..5)
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION	"Indicates the card/module number of the channel to be reset." 
	 ::= {asyncResetEntry 1}

asyncResetChannelNumber OBJECT-TYPE
	SYNTAX	INTEGER (1..6)
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION	"Identifies the channel number to be reset." 
	 ::= {asyncResetEntry 2}

asyncResetCommand OBJECT-TYPE
	SYNTAX	INTEGER {
		resetChannel (1)
		}
	ACCESS	write-only
	STATUS	mandatory
	DESCRIPTION	"Triggers the reset operation." 
	 ::= {asyncResetEntry 3}

