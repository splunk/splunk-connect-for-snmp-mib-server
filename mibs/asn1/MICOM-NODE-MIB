-- N_STATUS.O_QSTAT ======================================================

classChannelStatusTable OBJECT-TYPE
	SYNTAX	SEQUENCE OF ClassChannelStatusEntry
	ACCESS	not-accessible
	STATUS	mandatory
	DESCRIPTION	"Table to indicate current queue status of the configured classes.
			Should all the class channels be busy when the channel request
			a class, that channel is put in waiting queue on first-come,
			first-serve basis." 
	::= {status 11}

classChannelStatusEntry OBJECT-TYPE
	SYNTAX	ClassChannelStatusEntry
	ACCESS	not-accessible
	STATUS	mandatory
	DESCRIPTION	"An entry for a specified class number that indicates the current
			location in the queue." 
	INDEX		{classQueueStatusClassNum, classQueueStatusChannelIndex}
	 ::= {classChannelStatusTable 1}

ClassChannelStatusEntry ::= SEQUENCE {
	classChannelStatusClassNum	INTEGER,
	classQueueStatusChannelIndex	INTEGER,
	classChannelStatusNode		INTEGER,
	classChannelStatusChannel	INTEGER,
	classChannelStatusCard		INTEGER
	}

classChannelStatusClassNum OBJECT-TYPE
	SYNTAX	INTEGER (1..64)
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION	"Identifies the class number for which the channels are in queue." 
	 ::= {classChannelStatusEntry 1}

classQueueStatusChannelIndex OBJECT-TYPE
	SYNTAX	INTEGER
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION	"Identifies all the channels in the queue for a particular class." 
	 ::= {classChannelStatusEntry 2}

classChannelStatusNode OBJECT-TYPE
	SYNTAX	INTEGER
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION	"The node number of the channel waiting in the queue." 
	 ::= {classChannelStatusEntry 3}

classChannelStatusChannel OBJECT-TYPE
	SYNTAX	INTEGER
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION	"The port number of the channel waiting in the queue." 
	 ::= {classChannelStatusEntry 4}

classChannelStatusCard OBJECT-TYPE
	SYNTAX	INTEGER
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION	"The module/card number of the channel waiting in the queue." 
	 ::= {classChannelStatusEntry 5}

