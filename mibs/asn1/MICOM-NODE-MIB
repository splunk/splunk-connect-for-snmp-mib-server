-- N_HOUSE.O_PPPLNK =======================================================

pppLinkOperationGroup OBJECT IDENTIFIER ::= {control 7}

-- Note that the PPP Link Operation Group objects are used when debugging $PPP link operations.
--    These parameters should not normally be modified by the end user. 

pppLinkOperationAction OBJECT-TYPE
	SYNTAX	INTEGER {
		resumeNormalOperation (1)
		}
	ACCESS	write-only
	STATUS	mandatory
	DESCRIPTION	"Command to resume normal operation of the $PPP link.
			(Used for $PPP link debugging purposes.)" 
	 ::= { pppLinkOperationGroup 1}

pppLinkTimeOutAction OBJECT-TYPE
	SYNTAX	INTEGER {
		disableLinkTimeout (1)
		}
	ACCESS	write-only
	STATUS	mandatory
	DESCRIPTION	"Command to disable the link timeout feature of the $PPP link.
			(Used for $PPP link debugging purposes.)" 
	 ::= { pppLinkOperationGroup 2}

pppLinkDumpAction OBJECT-TYPE
	SYNTAX	INTEGER {
		dumpRejectedNmsCmdToLogport (1)
		}
	ACCESS	write-only
	STATUS	mandatory
	DESCRIPTION	"Command to route rejected NMS commands to the log port for output.
			(Used for $PPP link debugging purposes.)" 
	 ::= { pppLinkOperationGroup 3}

