t1CfgSystemClockSource OBJECT-TYPE
	SYNTAX	INTEGER {
		ds1ClockDerived (1),
		dsx1Clockderived (2),
		internalClocking (3)
		}
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"Configures the source of the clock used in the T1 Access 
			Module. Options include:
			- DSX-1. The clock source is from the DSX-1 interface.
			- DS-1. The clock source is from the DS-1 interface.
			- Internal. The clock is derived from a clock source internal 
				to the T1 Access Module."
--	DEFVAL	1
	::= {t1CfgGroup 24}

t1CfgDS1Config OBJECT-TYPE
	SYNTAX INTEGER {
		init (1),
		commit (2)
		}
	ACCESS write-only
	STATUS mandatory
	DESCRIPTION 	"This object is a write only object that will signal the proxy to
		start caching changes to the DS1 ds0 connection tables.  This is done to
		solve an anomaly in the marathon/t1 design. The manager must set
		this object to init before updating any of the t1DS1InterfaceTable objects.
		When all the changes have been set then this object will have to be set to
		commit (2).  The init will cause the proxy to upload the table from the marathon
		and the commit will cause the proxy to send all the changes to the marathon."
	::= {t1CfgGroup 25}

t1CfgDSX1Config OBJECT-TYPE
	SYNTAX INTEGER {
		init (1),
		commit (2)
		}
	ACCESS write-only
	STATUS mandatory
	DESCRIPTION 	"This object is a write only object that will signal the proxy to
		start caching changes to the DSX1 ds0 connection tables.  This is done to
		solve an anomaly in the marathon/t1 design. The manager must set
		this object to init before updating any of the t1DSX1InterfaceTable objects.
		When all the changes have been set then this object will have to be set to
		commit (2).  The init will cause the proxy to upload the table from the marathon
		and the commit will cause the proxy to send all the changes to the marathon."
	::= {t1CfgGroup 26}

-- N_HOUSE.O_T1FLSH ===================================================

t1FlashControlTable OBJECT-TYPE
	SYNTAX	SEQUENCE OF T1FlashControlEntry
	ACCESS	not-accessible
	STATUS	mandatory
	DESCRIPTION	"Table to control (erase) T1 flash memory in preparation for a 
			code download." 
	::= { t1-interface 4}

t1FlashControlEntry OBJECT-TYPE
	SYNTAX	T1FlashControlEntry 
	ACCESS	not-accessible
	STATUS	mandatory
	DESCRIPTION	"Entry within the table specific to a particular T1 module/card
			number for flash memory control." 
	INDEX	 	{ t1FlashControlCardNumber }
	::= { t1FlashControlTable 1}

T1FlashControlEntry ::= SEQUENCE {
	t1FlashControlCardNumber		INTEGER,
	t1FlashControlCommand		INTEGER,
	t1FlashControlFlashStatus		INTEGER
	}

t1FlashControlCardNumber OBJECT-TYPE
	SYNTAX	INTEGER (2..5)
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION	"Identifies the T1 module/card number for flash memory control." 
	::= { t1FlashControlEntry 1}

t1FlashControlCommand OBJECT-TYPE
	SYNTAX	INTEGER {
		eraseCardFlash (1)
		}
	ACCESS	write-only
	STATUS	mandatory
	DESCRIPTION	"Controls the T1 module flash memory in preparation of a code
			download.  Perform a SET with the value eraseCardFlash to erase
			the flash memory prior to downloading new T1 flash software." 
	::= { t1FlashControlEntry 2}

t1FlashControlFlashStatus OBJECT-TYPE
	SYNTAX	INTEGER {
		bootImageCorrupt (1),
		t1FlashImageValid (2),
		t1FlashErased (3),
		t1FlashImageInvalid (4),
		securityViolation (5),
		eraseAborted (6)
		}
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION	"Identifies the status of the T1 module/card flash memory." 
	::= { t1FlashControlEntry 3}

-- N_CMOS.O_PBXID ======================================================

-- E1 Interface Group

e1L1InterfaceTable OBJECT-TYPE
	SYNTAX	SEQUENCE OF E1L1InterfaceEntry
	ACCESS	not-accessible
	STATUS	mandatory
	DESCRIPTION	"Table to configure the DS0 connections for the L1 interface
			of an E1 module/card." 
	::= {e1-interface 1}

e1L1InterfaceEntry OBJECT-TYPE
	SYNTAX	E1L1InterfaceEntry
	ACCESS	not-accessible
	STATUS	mandatory
	DESCRIPTION	"Entry within the table specific to a particular DS0 channel
			for the L1 interface of an E1 module/card." 
	INDEX		{ e1L1InterfaceDS0Index}
	::= {e1L1InterfaceTable 1}

E1L1InterfaceEntry ::= SEQUENCE {
	e1L1InterfaceDS0Index			INTEGER,
	e1L1InterfaceConnectionState		INTEGER
	}

e1L1InterfaceDS0Index OBJECT-TYPE
	SYNTAX	INTEGER (1..30)
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION	"Identifies the DS0 channel of the E1 module for configuration
			of the L1 interface." 
	::= { e1L1InterfaceEntry 1}

e1L1InterfaceConnectionState OBJECT-TYPE
	SYNTAX	INTEGER {
		idle (1),
		bypass (2),
		toCCMLinkA1 (3),
		toCCMLinkA2 (4),
		toCCMLinkA3 (5),
		toCCMLinkA4 (6),
		toCCMLinkA5 (7),
		toCCMLinkA6 (8),
		toDataPort1 (9),
		toDataPort2 (10),
		toVChOnE1DVM1 (11),
		toVChOnE1DVM2 (12),
		toVChOnE1DVM3 (13),
		toVChOnE1DVM4 (14),
		toVChOnE1DVM5 (15),
		toVChOnE1DVM6 (16),
		toVChOnSlotCDVM1 (17),		
		toVChOnSlotCDVM2 (18),
		toVChOnSlotCDVM3 (19),
		toVChOnSlotCDVM4 (20),
		toVChOnSlotCDVM5 (21),
		toVChOnSlotCDVM6 (22),
		toVChOnSlotCDVM7 (23),
		toVChOnSlotCDVM8 (24),
		toVChOnSlotCDVM9 (25),
		toVChOnSlotCDVM10 (26),
		toVChOnSlotCDVM11 (27),
		toVChOnSlotCDVM12 (28),
		toVChOnSlotDDVM1 (29),		
		toVChOnSlotDDVM2 (30),
		toVChOnSlotDDVM3 (31),
		toVChOnSlotDDVM4 (32),
		toVChOnSlotDDVM5 (33),
		toVChOnSlotDDVM6 (34),
		toVChOnSlotDDVM7 (35),
		toVChOnSlotDDVM8 (36),
		toVChOnSlotDDVM9 (37),
		toVChOnSlotDDVM10 (38),
		toVChOnSlotDDVM11 (39),
		toVChOnSlotDDVM12 (40)
		}
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"Select the DS0 connection for DS0 configuration of the L1 
			interface.  Available DS0 connections include Idle, Bypass, 
			CCM A1-A6, Data Port 1-2, Voice B1-B6 (on e1 card), Voice C1-C12, 
			and Voice D1-D12." 
	::= { e1L1InterfaceEntry 2}


-- N_CMOS.O_NETID ======================================================

e1L2InterfaceTable OBJECT-TYPE
	SYNTAX	SEQUENCE OF E1L2InterfaceEntry
	ACCESS	not-accessible
	STATUS	mandatory
	DESCRIPTION	"Table to configure the DS0 connections for the L2 interface
			of an E1 module/card." 
	::= {e1-interface 2}

e1L2InterfaceEntry OBJECT-TYPE
	SYNTAX	E1L2InterfaceEntry
	ACCESS	not-accessible
	STATUS	mandatory
	DESCRIPTION	"Entry within the table specific to a particular DS0 channel
			for the L2 interface of an E1 module/card." 
	INDEX		{ e1L2InterfaceDS0Index}
	::= {e1L2InterfaceTable 1}

E1L2InterfaceEntry ::= SEQUENCE {
	e1L2InterfaceDS0Index		INTEGER,
	e1L2InterfaceConnectionState	INTEGER
	}

e1L2InterfaceDS0Index OBJECT-TYPE
	SYNTAX	INTEGER (1..30)
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION	"Identifies the DS0 channel of the E1 module for configuration
			of the L2 interface."
	::= { e1L2InterfaceEntry 1}

e1L2InterfaceConnectionState OBJECT-TYPE
	SYNTAX	INTEGER {
		idle (1),
		bypass (2),
		toCCMLinkA1 (3),
		toCCMLinkA2 (4),
		toCCMLinkA3 (5),
		toCCMLinkA4 (6),
		toCCMLinkA5 (7),
		toCCMLinkA6 (8),
		toDataPort1 (9),
		toDataPort2 (10),
		toVChOnE1DVM1 (11),
		toVChOnE1DVM2 (12),
		toVChOnE1DVM3 (13),
		toVChOnE1DVM4 (14),
		toVChOnE1DVM5 (15),
		toVChOnE1DVM6 (16),
		toVChOnSlotCDVM1 (17),		
		toVChOnSlotCDVM2 (18),
		toVChOnSlotCDVM3 (19),
		toVChOnSlotCDVM4 (20),
		toVChOnSlotCDVM5 (21),
		toVChOnSlotCDVM6 (22),
		toVChOnSlotCDVM7 (23),
		toVChOnSlotCDVM8 (24),
		toVChOnSlotCDVM9 (25),
		toVChOnSlotCDVM10 (26),
		toVChOnSlotCDVM11 (27),
		toVChOnSlotCDVM12 (28),
		toVChOnSlotDDVM1 (29),		
		toVChOnSlotDDVM2 (30),
		toVChOnSlotDDVM3 (31),
		toVChOnSlotDDVM4 (32),
		toVChOnSlotDDVM5 (33),
		toVChOnSlotDDVM6 (34),
		toVChOnSlotDDVM7 (35),
		toVChOnSlotDDVM8 (36),
		toVChOnSlotDDVM9 (37),
		toVChOnSlotDDVM10 (38),
		toVChOnSlotDDVM11 (39),
		toVChOnSlotDDVM12 (40)
		}
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"Select the DS0 connection for DS0 configuration of the L2 
			interface.  Available DS0 connections include Idle, Bypass, 
			CCM A1-A6, Data Port 1-2, Voice B1-B6 (on e1 card), Voice C1-C12, 
			and Voice D1-D12." 
	::= { e1L2InterfaceEntry 2}

-- N_CMOS.O_SYSID ======================================================

e1CfgGroup    OBJECT IDENTIFIER ::= {e1-interface 3}

e1CfgL1LineCode OBJECT-TYPE
	SYNTAX	INTEGER {
		ami (1),
		hdb3 (2)
		}
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"Configures the L1 interface line coding method of the E1  
			module/card either to Alternate Mark Inversion (AMI) coding or 
			High Density Bipolar 3 (HDB3).  AMI indicates that HDB3 zeros 
			suppression is not enabled.  To obtain zeros suppression (bit 
			stuffing method) with AMI, you must also set the base rate of 
			the CCM and Data ports to 56Kbps. Otherwise, zeros suppression 
			will be the responsibility of the Data Terminal Equipment.  
			HDB3 does not allow more than three consecutive zeros. HDB3 
			format can be enabled only when supported by the connecting 
			equipment or the E1 service provider." 
--	DEFVAL	2
	::= {e1CfgGroup 1}

e1CfgL1IdleCode OBJECT-TYPE
	SYNTAX	OCTET STRING (SIZE(1))
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"Configures the idle code character of the L1 interface for
			E1 module/card." 
--	DEFVAL	055
	::= {e1CfgGroup 2}

e1CfgL1SignallingMode OBJECT-TYPE
	SYNTAX	INTEGER {
		cas (1),
		transparentIdle (2),
		transparentTS16ToA2 (3),
		transparentTS16ToA3 (4),
		transparentTS16ToA4 (5),
		transparentTS16ToA5 (6),
		transparentTS16ToA6 (7),
		transparentTS16ToDP1 (8),
		transparentTS16ToDP2 (9),
		casTS16Bypass (10),
		ccsTS16Bypass (11)
		}
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"Configures the signalling mode of the L1 interface for
			E1 module/card." 
--	DEFVAL	1
	::= {e1CfgGroup 3}

e1CfgL2LineCode OBJECT-TYPE
	SYNTAX	INTEGER {
		ami (1),
		hdb3 (2)
		}
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"Configures the L2 interface line coding method of the E1  
			module/card either to Alternate Mark Inversion (AMI) coding or 
			High Density Bipolar 3 (HDB3).  AMI indicates that HDB3 zeros 
			suppression is not enabled.  To obtain zeros suppression (bit 
			stuffing method) with AMI, you must also set the base rate of 
			the CCM and Data ports to 56Kbps. Otherwise, zeros suppression 
			will be the responsibility of the Data Terminal Equipment.  
			HDB3 does not allow more than three consecutive zeros. HDB3 
			format can be enabled only when supported by the connecting 
			equipment or the E1 service provider." 
--	DEFVAL	2
	::= {e1CfgGroup 4}

e1CfgL2IdleCode OBJECT-TYPE
	SYNTAX	OCTET STRING (SIZE(1))
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"Configures the idle code character of the L2 interface for
			E1 module/card." 
--	DEFVAL	055
	::= {e1CfgGroup 5}

e1CfgL2SignallingMode OBJECT-TYPE
	SYNTAX	INTEGER {
		cas (1),
		transparentIdle (2),
		transparentTS16ToA2 (3),
		transparentTS16ToA3 (4),
		transparentTS16ToA4 (5),
		transparentTS16ToA5 (6),
		transparentTS16ToA6 (7),
		transparentTS16ToDP1 (8),
		transparentTS16ToDP2 (9),
		casTS16Bypass (10),
		ccsTS16Bypass (11)
		}
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"Configures the signalling mode of the L2 interface for
			E1 module/card." 
	::= {e1CfgGroup 6}

e1CfgDataPort1BaseRate OBJECT-TYPE
	SYNTAX	INTEGER {
		multipleOf64kbps (1),
		multipleOf56kbps (2)
		}
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"Sets the base rate of Data Port channel 1 on the E1 module.
			The actual rate will be determined by the following formula:
			rate of the data port = (base rate)*(number of DS0 channels)." 
--	DEFVAL	1
	::= {e1CfgGroup 7}

e1CfgDataPort1Timing OBJECT-TYPE
	SYNTAX	INTEGER {
		normal (1),
		tailCircuit (2)
		}
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"Sets how the clocking will be supplied for Data Port 1 on the
			E1 module." 
--	DEFVAL	1
	::= {e1CfgGroup 8}

e1CfgDataPort1DTRControl OBJECT-TYPE
	SYNTAX	INTEGER {
		dtrNormal (1),
		dtrForced (2)
		}
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"Sets the DTR control for Data Port 1 on the E1 module.  Option (1),
			dtrNormal, will configure DTR control from the DTE equipment.  
			Option (2), dtrForced, will force DTR to a '1'." 
--	DEFVAL	1
	::= {e1CfgGroup 9}

e1CfgDataPort1RTSControl OBJECT-TYPE
	SYNTAX	INTEGER {
		rtsNormal (1),
		rtsForced (2)
		}
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"Sets the RTS control for Data Port 1 on the E1 module.  Option (1),
			rtsNormal, will configure RTS control from the DTE equipment.  
			Option (2), rtsForced, will force RTS to a '1'." 
--	DEFVAL	1
	::= {e1CfgGroup 10}

e1CfgDataPort2BaseRate OBJECT-TYPE
	SYNTAX	INTEGER {
		multipleOf64kbps (1),
		multipleOf56kbps (2)
		}
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"Sets the base rate of Data Port channel 2 on the E1 module.
			The actual rate will be determined by the following formula:
			rate of the data port = (base rate)*(number of DS0 channels)." 
--	DEFVAL	1
	::= {e1CfgGroup 11}

e1CfgDataPort2Timing OBJECT-TYPE
	SYNTAX	INTEGER {
		normal (1),
		tailCircuit (2)
		}
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"Sets how the clocking will be supplied for Data Port 2 on the
			E1 module." 
--	DEFVAL	1
	::= {e1CfgGroup 12}

