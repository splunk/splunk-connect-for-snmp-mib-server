	SYNTAX	INTEGER (2..5)
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION	"Identifies the card number of the voice module
			for erasing the flash memory." 
	 ::= { voiceFlashEPROMEntry 1}

voiceFlashEPROMChannelNumber OBJECT-TYPE
	SYNTAX	INTEGER (1..6)
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION	"Identifies the channel number of the voice module
			for erasing the flash memory." 
	 ::= { voiceFlashEPROMEntry 2}

voiceFlashEPROMCommand OBJECT-TYPE
	SYNTAX	INTEGER {
		eraseFlash (1)
		}
	ACCESS	write-only
	STATUS	mandatory
	DESCRIPTION	"Command to erase the flash memory of a particular voice
			channel module." 
	 ::= { voiceFlashEPROMEntry 3}


