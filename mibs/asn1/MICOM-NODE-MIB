	::= {card 2}

-- N_CMOS.O_LANTYP ========================================================

eliCardFunctionTable OBJECT-TYPE
	SYNTAX	SEQUENCE OF EliCardFunctionEntry
	ACCESS	not-accessible
	STATUS	mandatory
	DESCRIPTION	"Table containing the functionality on resident eli cards (if any)." 
	::= {card 3}

eliCardFunctionEntry OBJECT-TYPE
	SYNTAX	EliCardFunctionEntry
	ACCESS	not-accessible
	STATUS	mandatory
	DESCRIPTION	"Entry within the table to specify a particular slot where an eli
			card is located." 
	INDEX		{eliCardFunctionSlotNumber}
	::= {eliCardFunctionTable 1}

EliCardFunctionEntry ::= SEQUENCE {
	eliCardFunctionSlotNumber		INTEGER,
	eliCardFunctionPromID			DisplayString,
	eliCardFunctionTypeRTSFunction		INTEGER,
	eliCardFunctionTypeBridgeFunction	INTEGER,
	eliCardFunctionTypeRouterFunction	INTEGER,
	eliCardFunctionFlashMemory		INTEGER
	}

eliCardFunctionSlotNumber OBJECT-TYPE
	SYNTAX	INTEGER (2..5)
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION	"The slot number of the eli card." 
	  ::= {eliCardFunctionEntry 1}

eliCardFunctionPromID OBJECT-TYPE
	SYNTAX	DisplayString  (SIZE(0..8))
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION	"The prom part number of the software that is installed and
				running on the eli card." 
	  ::= {eliCardFunctionEntry 2}

eliCardFunctionTypeRTSFunction OBJECT-TYPE
	SYNTAX	INTEGER {
		on (1),
		off (2)
		}
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION	"Identifies whether this eli card performs the function of
				an RTS or not." 
	  ::= {eliCardFunctionEntry 3}

eliCardFunctionTypeBridgeFunction OBJECT-TYPE
	SYNTAX	INTEGER {
		on (1),
		off (2)
		}
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION	"Identifies whether this eli card performs the function of
				a bridge or not." 
	  ::= {eliCardFunctionEntry 4}

eliCardFunctionTypeRouterFunction OBJECT-TYPE
	SYNTAX	INTEGER {
		on (1),
		off (2)
		}
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION	"Identifies whether this eli card performs the function of
				a router or not." 
	  ::= {eliCardFunctionEntry 5}

eliCardFunctionFlashMemory OBJECT-TYPE
	SYNTAX	INTEGER {
		on (1),
		off (2)
		}
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION	"Identifies whether this eli card contains flash memory or not." 
	  ::= {eliCardFunctionEntry 6}

-- N_CMOS.O_T1TYPE ======================================================

t1e1CardSoftwareInfoTable OBJECT-TYPE
	SYNTAX	SEQUENCE OF T1e1CardSoftwareInfoEntry
	ACCESS	not-accessible
	STATUS	mandatory
	DESCRIPTION	"Table describing the software on resident (if any) T1 or E1 card." 
	::= {card 4}

t1e1CardSoftwareInfoEntry OBJECT-TYPE
	SYNTAX	T1e1CardSoftwareInfoEntry 
	ACCESS	not-accessible
	STATUS	mandatory
	DESCRIPTION	"Entry within the table to specify a particular T1 or E1 card." 
	INDEX		{t1e1CardSoftwareInfoCardNumber}
	 ::= {t1e1CardSoftwareInfoTable 1}

T1e1CardSoftwareInfoEntry ::= SEQUENCE {
	t1e1CardSoftwareInfoCardNumber		INTEGER,
	t1e1CardSoftwareInfoCardType		INTEGER,
	t1e1CardSoftwareInfoReleaseLevel		INTEGER,
	t1e1CardSoftwareInfoSoftwareID		DisplayString,
	t1e1CardSoftwareInfoBootCode		DisplayString
	}

t1e1CardSoftwareInfoCardNumber OBJECT-TYPE
	SYNTAX	INTEGER (2..5)
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION	"Displays the Card number of the T1 or E1 module." 
	::= { t1e1CardSoftwareInfoEntry 1}

t1e1CardSoftwareInfoCardType OBJECT-TYPE
	SYNTAX	INTEGER {
		t1 (1),
		e1 (2)
		}
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION	"Displays the access module type; T1 or E1." 
	::= { t1e1CardSoftwareInfoEntry 2}

t1e1CardSoftwareInfoReleaseLevel OBJECT-TYPE
	SYNTAX	INTEGER {
		releaseZero (1),
		releaseOne (2),
		releaseTwo (3)
		}
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION	"Displays which Software release is installed on the T1/E1 card." 
	::= { t1e1CardSoftwareInfoEntry 3}

t1e1CardSoftwareInfoSoftwareID OBJECT-TYPE
	SYNTAX	DisplayString
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION	"Displays which Software part number is installed on the T1/E1 card." 
	::= { t1e1CardSoftwareInfoEntry 4}

t1e1CardSoftwareInfoBootCode OBJECT-TYPE
	SYNTAX	DisplayString 
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION	"Displays which boot prom part number is installed on the T1/E1 card." 
	::= { t1e1CardSoftwareInfoEntry 5}

-- PORT GROUP

portCfgTable OBJECT-TYPE
	SYNTAX	SEQUENCE OF PortCfgEntry
	ACCESS	not-accessible
	STATUS	mandatory
	DESCRIPTION	"Table to establish the type of port connection to be assigned to
			the selected port." 
	::= {port 1}

portCfgEntry OBJECT-TYPE
	SYNTAX	PortCfgEntry
	ACCESS	not-accessible
	STATUS	mandatory
	DESCRIPTION	"Entry into the table for the selected port." 
	INDEX		{portCfgCardNumber, portCfgChannelNumber}
	::= {portCfgTable 1}

PortCfgEntry ::= SEQUENCE {
	portCfgCardNumber		INTEGER,
	portCfgChannelNumber		INTEGER,
	portCfgChannelType		INTEGER
	}

portCfgCardNumber OBJECT-TYPE
	SYNTAX	INTEGER (1..5)
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION	"Indicates the card/module of the channel to be configured." 
	::= {portCfgEntry 1}

portCfgChannelNumber OBJECT-TYPE
	SYNTAX	INTEGER (1..6)
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION	"Indicates the channel/port number to be configured." 
	::= {portCfgEntry 2}

portCfgChannelType OBJECT-TYPE
	SYNTAX	INTEGER {
		mux (1),
		async (2),
		sync (3),
		esi (4),
		x21 (5),
		localEsi (6),
		esiSecondary (7),
		frameRelay (8)
		}
	ACCESS	write-only
	STATUS	mandatory
	DESCRIPTION	"Assigns the type of connection to the selected port.
			Note:  A node reset is required after setting the port
			configuration.  Also note that if a channel is currently
			connected, forced or switch connected, the channel must first
			be disconnected.  The procedure to do this would be to place
			the channel out of service (chControlCommand disable(2)), then
			disconnect (chControlCommand forceDIsconnect(4)) the channel." 
	::= {portCfgEntry 3}


syncPortCfgTable OBJECT-TYPE
	SYNTAX	SEQUENCE OF SyncPortCfgEntry
	ACCESS	not-accessible
	STATUS	mandatory
	DESCRIPTION	"Table to configure the protocol for the specified sync channel." 
	::= {port 2}

syncPortCfgEntry OBJECT-TYPE
	SYNTAX	SyncPortCfgEntry
	ACCESS	not-accessible
	STATUS	mandatory
	DESCRIPTION	"Entry into the table specifying a particular channel on a particular
			module/card." 
	INDEX		{syncPortCfgCardNumber, syncPortCfgChannelNumber}
	::= {syncPortCfgTable 1} 

SyncPortCfgEntry ::= SEQUENCE {
	syncPortCfgCardNumber			INTEGER,
	syncPortCfgChannelNumber		INTEGER,
	syncPortCfgProtocol			INTEGER
	}

syncPortCfgCardNumber OBJECT-TYPE
	SYNTAX	INTEGER (1..5)
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION	"Identifies the card/module of the sync channel to be configured." 
	::= {syncPortCfgEntry 1}

syncPortCfgChannelNumber OBJECT-TYPE
	SYNTAX	INTEGER (1..6)
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION	"Indicates the sync channel number to be configured." 
	::= {syncPortCfgEntry 2}

syncPortCfgProtocol OBJECT-TYPE
	SYNTAX	INTEGER {
		dlcChannel (1),
		asciiBisync (2),
		ebcdicBisync (3),
		hpSync (4),
		rts (5),
		syncPad (6),
		micomDlc (7),
		micomVoice (8),
		fastPacket (9),
		tdm (10)
		}
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"Assigns the sync protocol to the selected port. Note: A node reset is
			required after the protocol is configured." 
	::= {syncPortCfgEntry 3}


-- N_CMOS.O_FCMDPT ========================================================

fCmdPortPhyParaTable OBJECT-TYPE
	SYNTAX	SEQUENCE OF FCmdPortPhyParaEntry
	ACCESS	not-accessible
	STATUS	mandatory
	DESCRIPTION	"Table of configurations for the physical parameters of the 
			fixed command port on an NMS module." 
	::= {port 3}

fCmdPortPhyParaEntry OBJECT-TYPE
	SYNTAX	FCmdPortPhyParaEntry
	ACCESS	not-accessible
	STATUS	mandatory
	DESCRIPTION	"Entry within the table to specify the fixed command port
			channel number (2) on an NMS module/card (6)." 
	INDEX	{fCmdPortPhyParaCardNumber, fCmdPortPhyParaChannelNumber}
	::= {fCmdPortPhyParaTable 1}

FCmdPortPhyParaEntry ::= SEQUENCE {
	fCmdPortPhyParaCardNumber		INTEGER,
	fCmdPortPhyParaChannelNumber		INTEGER,
	fCmdPortPhyParaStopBits			INTEGER,
	fCmdPortPhyParaCodeLevel		INTEGER,
	fCmdPortPhyParaDataRate		INTEGER,
	fCmdPortPhyParaEcho			INTEGER,
	fCmdPortPhyParaCRDelay		INTEGER,
	fCmdPortPhyParaLFDelay		INTEGER,
	fCmdPortPhyParaFFDelay			INTEGER,
	fCmdPortPhyParaParity			INTEGER
	}

fCmdPortPhyParaCardNumber  OBJECT-TYPE
	SYNTAX	INTEGER (6)
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION	"The card number (=6) of the NMS module containing the fixed cmd port." 
	::= {fCmdPortPhyParaEntry 1}

fCmdPortPhyParaChannelNumber  OBJECT-TYPE
	SYNTAX	INTEGER (2)
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION	"The channel number (=2) of the fixed cmd port on the NMS module." 
	::= {fCmdPortPhyParaEntry 2}

fCmdPortPhyParaStopBits	 OBJECT-TYPE
	SYNTAX	INTEGER {
		onebit (1),
		onePointFiveBits (2),
		twoBits (3)
		}
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"Sets the number of stop-bits inserted at the end of a character." 
	::= {fCmdPortPhyParaEntry 3}

fCmdPortPhyParaCodeLevel OBJECT-TYPE
	SYNTAX	INTEGER {
		fiveBits (1),
		sixBits (2),
		sevenBits (3),
		eightBits (4),
		nineBits (5)
		}
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"Sets the number of data bits per character. This includes the
			parity bit." 
	::= {fCmdPortPhyParaEntry 4}

fCmdPortPhyParaDataRate OBJECT-TYPE
	SYNTAX	INTEGER {
		abr (1),
		rate50 (2),
		rate75 (3),
		rate110 (4),
		rate134dot5 (5),
		rate150 (6),
		rate200 (7),
		rate225 (8),
		rate300 (9),
		rate600 (10),
		rate1200 (11),
		rate1800 (12),
		rate2400 (13),
		rate3600 (14),
		rate4800 (15),
		rate7200 (16),
		rate9600 (17),
		rate14400 (18),
		rate19200 (19),
		rate28800 (20),
		rate38400 (21)
		}
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"Sets the data rate in bits per seconds of the fixed command port." 
	::= {fCmdPortPhyParaEntry 5}

fCmdPortPhyParaEcho OBJECT-TYPE
	SYNTAX	INTEGER {
		on (1),
		off (2)
	}
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"Determines whether the Integration unit should echo input from the terminal
				or not." 
	::= {fCmdPortPhyParaEntry 6}

fCmdPortPhyParaCRDelay OBJECT-TYPE
	SYNTAX	INTEGER (0..99)
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"Configures the number of character delays that should be introduced
				after each CR (carriage return)." 
	::= {fCmdPortPhyParaEntry 7}

fCmdPortPhyParaLFDelay OBJECT-TYPE
	SYNTAX	INTEGER (0..99)
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"Configures the number of character delays that should be introduced
				after each LF (line feed)." 
	::= {fCmdPortPhyParaEntry 8}

fCmdPortPhyParaFFDelay OBJECT-TYPE
	SYNTAX	INTEGER (0..99)
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"Configures the number of chanacter delays that should be introduced
				afer each FF (form feed)." 
	::= {fCmdPortPhyParaEntry 9}

fCmdPortPhyParaParity OBJECT-TYPE
	SYNTAX	INTEGER {
		space (1),
		odd (2),
		even (3),
		mark (4),
		none (5),
		auto (6)
		}
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"Sets the message and data parity."
	::= {fCmdPortPhyParaEntry 10}


-- N_CMOS.O_LOGPRT ========================================================

logPortPhyParaTable OBJECT-TYPE
	SYNTAX	SEQUENCE OF LogPortPhyParaEntry
	ACCESS	not-accessible
	STATUS	mandatory
	DESCRIPTION	"Table to configure the log port (present on NMS module) 
			parameters." 
	::= {port 4}

logPortPhyParaEntry OBJECT-TYPE
	SYNTAX	LogPortPhyParaEntry
	ACCESS	not-accessible
	STATUS	mandatory
	DESCRIPTION	"Entry within the table to specify the log port
			channel number (1) on an NMS module/card (6)." 
	INDEX		{logPortPhyParaCardNumber, logPortPhyParaChannelNumber}
	::= {logPortPhyParaTable 1}

LogPortPhyParaEntry ::= SEQUENCE {
	logPortPhyParaCardNumber		INTEGER,
	logPortPhyParaChannelNumber		INTEGER,
	logPortPhyParaStopBits			INTEGER,
	logPortPhyParaCodeLevel			INTEGER,
	logPortPhyParaDataRate			INTEGER,
	logPortPhyParaCRDelay			INTEGER,
	logPortPhyParaLFDelay			INTEGER,
	logPortPhyParaFFDelay			INTEGER,
	logPortPhyParaXONChar			INTEGER,
	logPortPhyParaXOFFChar			INTEGER,
	logPortPhyParaFlowControl		INTEGER,
	logPortPhyParaBufferControl		INTEGER,
	logPortPhyParaParity			INTEGER
	}

logPortPhyParaCardNumber  OBJECT-TYPE
	SYNTAX	INTEGER (6)
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION	"The card number (=6) of the NMS Module that contains the log port." 
	::= {logPortPhyParaEntry 1}

logPortPhyParaChannelNumber  OBJECT-TYPE
	SYNTAX	INTEGER (1)
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION	"The channel number of the log port (=1)." 
	::= {logPortPhyParaEntry 2}

logPortPhyParaStopBits	 OBJECT-TYPE
	SYNTAX	INTEGER {
		oneBit (1),
		onePointFiveBits (2),
		twoBits (3)
		}
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"Configures the number of stop bits that the Integration unit should
				generate and check for on this channel." 
	::= {logPortPhyParaEntry 3}

logPortPhyParaCodeLevel OBJECT-TYPE
	SYNTAX	INTEGER {
		fiveBits (1),
		sixBits (2),
		sevenBits (3),
		eightBits (4),
		nineBits (5)
		}
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"Sets the number of data bits per character. This includes the
			parity bit." 
	::= {logPortPhyParaEntry 4}

logPortPhyParaDataRate	 OBJECT-TYPE
	SYNTAX	INTEGER {
		rate50 (2),
		rate75 (3),
		rate110 (4),
		rate134dot5 (5),
		rate150 (6),
		rate200 (7),
		rate300 (9),
		rate600 (10),
		rate1200 (11),
		rate1800 (12),
		rate2400 (13),
		rate3600 (14),
		rate4800 (15),
		rate7200 (16),
		rate9600 (17),
		rate14400 (18),
		rate19200 (19),
		rate28800 (20),
		rate38400 (21)
		}
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"Sets the data rate in bits per seconds of the log port." 
	::= {logPortPhyParaEntry 5}

logPortPhyParaCRDelay OBJECT-TYPE
	SYNTAX	INTEGER (0..99)
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"Configures the number of Character Delays required to be
				inserted after each CR (carriage return)." 
	::= {logPortPhyParaEntry 6}

logPortPhyParaLFDelay OBJECT-TYPE
	SYNTAX	INTEGER (0..99)
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"Configures the number of Character Delays required to be
				inserted after each LF (Line Feed)." 
	::= {logPortPhyParaEntry 7}

logPortPhyParaFFDelay OBJECT-TYPE
	SYNTAX	INTEGER (0..99)
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"Configures the number of Character Delays required to be
				inserted after each FF (form feed)."
	::= {logPortPhyParaEntry 8}

logPortPhyParaXONChar	 OBJECT-TYPE
	SYNTAX	INTEGER {
		dc1 (1),
		dc2 (2),
		dc3 (3),
		dc4 (4)
		}
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"Configures which character the Integration unit should check for
				and respond to for stopping data flow on a channel." 
	::= {logPortPhyParaEntry 9}

logPortPhyParaXOFFChar OBJECT-TYPE
	SYNTAX	INTEGER {
		dc1 (1),
		dc2 (2),
		dc3 (3),
		dc4 (4)
		}
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"Configures which character the Marathon should check for
				and respond to for re-starting data flow on a channel." 
	::= {logPortPhyParaEntry 10}

logPortPhyParaFlowControl OBJECT-TYPE
	SYNTAX	INTEGER {
		none (1),
		xonXof (2),
		dtr (3),
		xonXofDtr (4),
		wangx (5),
		rts (6),
		dataGeneral (7)
		}
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"Configures which type of flow control should be used
				for the log port." 
	::= {logPortPhyParaEntry 11}

logPortPhyParaBufferControl OBJECT-TYPE
	SYNTAX	INTEGER {
		none (1),
		xonXof (2),
		cts (3),
		wangx (4)
		}
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"Configures what type of buffer control to be used
				in the communications with the log port." 
	::= {logPortPhyParaEntry 12}

logPortPhyParaParity OBJECT-TYPE
	SYNTAX	INTEGER {
		space (1),
		odd (2),
		even (3),
		mark (4),
		none (5)
		}
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"Configures what type of parity to use with the log port." 
	::= {logPortPhyParaEntry 13}


-- N_CMOS.O_EXTMDM ======================================================

commandPortExtModemNumber  OBJECT-TYPE
	SYNTAX	DisplayString  (SIZE (0..32))
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"Modem phone number to call to send ASCII events out." 
	::= {port 5}


-- N_CMOS.O_CMDPRT ======================================================

commandPortDataActivityTimeout  OBJECT-TYPE
	SYNTAX	INTEGER (0..60)
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"Configures the command facility port inactivity timeout (minutes)." 
	::= {port 6}

-- Pre-Configuration SYNC Port ============================================

syncPortPreConfigTable OBJECT-TYPE
	SYNTAX	SEQUENCE OF SyncPortPreConfigEntry
	ACCESS	not-accessible
	STATUS	mandatory
	DESCRIPTION	"Table to configure a sync channel protocol prior to configuring
			a port as a sync channel. By using this table to 'preset' the sync
			protocol, only 1 node reset is needed following the sync channel
			assignment to complete the configuration." 
	::= {port 7}

syncPortPreConfigEntry OBJECT-TYPE
	SYNTAX	SyncPortPreConfigEntry
	ACCESS	not-accessible
	STATUS	mandatory
	DESCRIPTION	"Entry within the table to specify a particular sync channel for
			pre-configuration on a specific module/card." 
	INDEX		{syncPortPreConfigCardNumber, syncPortPreConfigChannelNumber}
	::= {syncPortPreConfigTable 1} 

SyncPortPreConfigEntry ::= SEQUENCE {
	syncPortPreConfigCardNumber		INTEGER,
	syncPortPreConfigChannelNumber		INTEGER,
	syncPortPreConfigProtocol		INTEGER
	}

syncPortPreConfigCardNumber OBJECT-TYPE
	SYNTAX	INTEGER (1..5)
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION	"Identifies the module/card for sync channel pre-configuration." 
	::= {syncPortPreConfigEntry 1}

syncPortPreConfigChannelNumber OBJECT-TYPE
	SYNTAX	INTEGER (1..6)
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION	"Identifies the port number for sync channel pre-configuration." 
	::= {syncPortPreConfigEntry 2}

syncPortPreConfigProtocol OBJECT-TYPE
	SYNTAX	INTEGER {
		dlcChannel (1),
		asciiBisync (2),
		ebcdicBisync (3),
		hpSync (4),
		rts (5),
		syncPad (6),
		micomDlc (7),
		micomVoice (8),
		fastPacket (9),
		tdm (10)
		}
	ACCESS	write-only
	STATUS	mandatory
	DESCRIPTION	"The protocol to be used for pre-configuring a sync channel." 
	::= {syncPortPreConfigEntry 3}


-- CHANNEL GROUP

-- N_CMOS.O_SYNCH ========================================================

syncChPhyParaTable OBJECT-TYPE
	SYNTAX	SEQUENCE OF SyncChPhyParaEntry
	ACCESS	not-accessible
	STATUS	mandatory
	DESCRIPTION	"For the configuration of synchronous channels, this table controls
			and modifies the operating parameters for the port." 
	::= {sync-channel 1}

syncChPhyParaEntry OBJECT-TYPE
	SYNTAX	SyncChPhyParaEntry
	ACCESS	not-accessible
	STATUS	mandatory
	DESCRIPTION	"Entry into the table pertaining to a specific sync channel on a
			specific module." 
	INDEX		{syncChPhyParaCardNumber, syncChPhyParaChannelNumber}
	::= {syncChPhyParaTable 1}

SyncChPhyParaEntry ::= SEQUENCE {
	syncChPhyParaCardNumber		INTEGER,
	syncChPhyParaChannelNumber		INTEGER,
	syncChPhyParaDataRate			INTEGER,
	syncChPhyParaCarrierMode		INTEGER,
	syncChPhyParaInterfaceType		INTEGER,
	syncChPhyParaIdleFill			INTEGER,
	syncChPhyParaHighSyncChar		OCTET STRING,
	syncChPhyParaLowSyncChar		OCTET STRING,
	syncChPhyParaNumLeadSyncCount	INTEGER,
	syncChPhyParaMaxTxBlockSize		INTEGER,
	syncChPhyParaPadChar			OCTET STRING,
	syncChPhyParaNumLeadingPads		INTEGER,
	syncChPhyParaNumTrailingPads		INTEGER,
	syncChPhyParaClockFlowControl		INTEGER,
	syncChPhyParaEncoding			INTEGER,
	syncChPhyParaChTxClocking		INTEGER,
	syncChPhyParaChRxClocking		INTEGER,
	syncChPhyParaPriority			INTEGER,
	syncChPhyParaDSRControl		INTEGER,
	syncChPhyParaBufferControl		INTEGER,
	syncChPhyParaMaxRxBlockSize		INTEGER,
	syncChPhyParaProtocol			INTEGER,
	syncChPhyParaClaimedBandwidth		INTEGER,
	syncChPhyParaDLCTransitRate		INTEGER
	}

syncChPhyParaCardNumber OBJECT-TYPE
	SYNTAX	INTEGER (1..5)
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION	"The card/module number of the channel this entry pertains to." 
	::= {syncChPhyParaEntry 1}

syncChPhyParaChannelNumber OBJECT-TYPE
	SYNTAX	INTEGER (1..6)
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION	"The channel number on a specified module this configuration
			entry pertains to." 
	::= {syncChPhyParaEntry 2}

syncChPhyParaDataRate OBJECT-TYPE
	SYNTAX	INTEGER {
		rate1200 (1),
		rate2400 (2),
		rate3600 (3),
		rate4800 (4),
		rate7200 (5),
		rate9600 (6),
		rate12000 (7),
		rate14400 (8),
		rate16800 (9),
		rate19200 (10),
		rate28800 (11),
		rate38400 (12),
		rate48000 (13),
		rate56000 (14),
		rate64000 (15)
		}
	ACCESS	read-write
	STATUS	mandatory
--	DEFVAL	2
	DESCRIPTION	"Sets data rate in bits per second for the user sync channel. Default 
			is 2400." 
	::= {syncChPhyParaEntry 3}

syncChPhyParaCarrierMode OBJECT-TYPE
	SYNTAX	INTEGER {
		variable (1),
		constant (2)
		}
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"Defines when carrier is held high, either constant or variable.
			This parameter is configurable for all Sync protocols EXCEPT tdm(10)
			and fastPacket(9).  For Sync protocol, rtsCts(5), the parameter
			default is Option 'variable(1)'.  For Sync protocols - dlcChannel(1),
			asciiBisync(2), ebcdicBisync(3), hpSync(4), syncPad(6), micomDlc(7),
			and micomVoice(8); the parameter default is Option 'constant(2)'." 
	::= {syncChPhyParaEntry 4}

syncChPhyParaInterfaceType OBJECT-TYPE
	SYNTAX	INTEGER {
		toDTE (1),
		toDCE (2)
		}
	ACCESS	read-write
	STATUS	mandatory
--	DEFVAL	1
	DESCRIPTION	"Sets the user channel for toDTE to interface with data terminal
			equipment; and toDCE to interface with data communication equipment
			such as a tail-circuit modem.  This parameter is configurable for all
			Sync protocols EXCEPT fastPacket(9)."
	::= {syncChPhyParaEntry 5}

syncChPhyParaIdleFill OBJECT-TYPE
	SYNTAX	INTEGER {
		flagFill (1),
		auto (2),
		autoMod128 (3),
		markFill (4)
		}
	ACCESS	read-write
	STATUS	mandatory
--	DEFVAL	1
	DESCRIPTION	"Selects what the Integration unit will output when the line
			is idle.  This parameter is configurable ONLY for Sync protocol
			dlcChannel(1)."
	::= {syncChPhyParaEntry 6}

syncChPhyParaHighSyncChar OBJECT-TYPE
	SYNTAX	OCTET STRING (SIZE(1))
	ACCESS	read-write
	STATUS	mandatory
--	DEFVAL	16
	DESCRIPTION	"Sets the value (high byte) of the leading sync character for
			the start of the block.  This parameter is configurable ONLY for
			Sync protocol syncPad(6)."
	::= {syncChPhyParaEntry 7}

syncChPhyParaLowSyncChar OBJECT-TYPE
	SYNTAX	OCTET STRING (SIZE(1))
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	" Sets the value (low byte) of the leading sync character for
			the start of the block.  This parameter is configurable for all
			Sync protocols EXCEPT dlcChannel(1), micomDlc(7), and micomVoice(8)."
	::= {syncChPhyParaEntry 8}

syncChPhyParaNumLeadSyncCount OBJECT-TYPE
	SYNTAX	INTEGER (0..9)
	ACCESS	read-write
	STATUS	mandatory
--	DEFVAL	3
	DESCRIPTION	"Sets the number of leading sync characters that the Integration
			unit will generate on the output (0-9). This will allow the device
			on the selected channel to synchronize. Enter zero to specify 
			all leading syncs as transparent. They will not be stripped out.
			This parameter is ONLY configurable for Sync protocols asciiBisync(2),
			ebcdicBisync(3), hpSync(4), rtsCts(5), and syncPad(6),"
	::= {syncChPhyParaEntry 9}

syncChPhyParaMaxTxBlockSize OBJECT-TYPE
	SYNTAX	INTEGER (1..9999)
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"Sets the block size in bytes of the data to the Integration
			unit from the attached device.  This parameter is configurable
			for all Sync protocols EXCEPT micomDlc(7), fastPacket(9), and
			tdm(10).

			For Sync protocol hpSync(4), the parameter default is 9876.  For
			Sync protocol micomVoice(8), the parameter default is 1024.  For
			Sync protocols dlcChannel(1), asciiBisync(2), ebcdicBisync(3),
			rtsCts(5), and syncPad(6); the parameter default is 512."
	::= {syncChPhyParaEntry 10}

syncChPhyParaPadChar OBJECT-TYPE
	SYNTAX	OCTET STRING (SIZE(1))
	ACCESS	read-write
	STATUS	mandatory
--	DEFVAL	FF
	DESCRIPTION	"Sets the value of pad character (alpha numeric). It precedes the
			sync character and follows the end of the block. It is used to pad
			the block.  This parameter is ONLY configurable for the Sync protocols
			asciiBisync(2), ebcdicBisync(3), hpSync(4), rtsCts(5), and syncPad(6)."
	::= {syncChPhyParaEntry 11}

syncChPhyParaNumLeadingPads OBJECT-TYPE
	SYNTAX	INTEGER (1..9)
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"Sets the number of leading pad characters to be transmitted at
			the start of the block for a time delay (1-9).  This parameter is
			ONLY configurable for the Sync protocols asciiBisync(2), ebcdicBisync(3),
			hpSync(4), rtsCts(5), and syncPad(6).

			For Sync protocols asciiBisync(2), ebcdicBisync(3), and hpSync(4) the
			parameter default is 1.  For Sync protocol rtsCts(5) the default is
			3.  For Sync protocol syncPad(6) the default is 2."
	::= {syncChPhyParaEntry 12}

syncChPhyParaNumTrailingPads OBJECT-TYPE
	SYNTAX	INTEGER (0..20)
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	" Sets the number of trailing pad characters to be transmitted at
			the end of the block.  This parameter is ONLY configurable for the
			Sync protocols asciiBisync(2), ebcdicBisync(3), hpSync(4), rtsCts(5),
			and syncPad(6).

			For Sync protocols asciiBisync(2), ebcdicBisync(3), hpSync(4), and
			syncPad(6) the parameter default is 2.  For Sync protocol rtsCts(5)
			the default is 3."
	::= {syncChPhyParaEntry 13}

syncChPhyParaClockFlowControl OBJECT-TYPE
	SYNTAX	INTEGER {
		enable (1),
		disable (2)
		}
	ACCESS	read-write
	STATUS	mandatory
--	DEFVAL	1
	DESCRIPTION	"Selects whether or not the local sync channel will be flow
			controlled.  This parameter is configurable for all Sync protocols
			EXCEPT fastPacket(9), and tdm(10)."
	::= {syncChPhyParaEntry 14}

syncChPhyParaEncoding OBJECT-TYPE 
	SYNTAX	INTEGER {
		nrz (1),
		nrzi (2)
		}
	ACCESS	read-write
	STATUS	mandatory
--	DEFVAL	1
	DESCRIPTION	"Selects one of the two types of encoding,  NRZ or NRZI, 
			depending on the attached device.  This parameter is ONLY
			configurable for Sync protocol dlcChannel(1)."
	::= {syncChPhyParaEntry 15}

syncChPhyParaChTxClocking OBJECT-TYPE
	SYNTAX	INTEGER {
		internal (1),
		external (2)
		}
	ACCESS	read-write
	STATUS	mandatory
--	DEFVAL 	1
	DESCRIPTION	"Sets the channel's source for transmit clock.  This
			parameter, combined with the syncChPhyParaChRxClocking
			parameter, corresponds to the Channel Clocking parameter
			which is displayed in the Sync channel configuration menu
			of the unit's $CMD facility.  This parameter is configurable
			for all Sync protocols EXCEPT fastPacket(9)."
	::= {syncChPhyParaEntry 16}

syncChPhyParaChRxClocking OBJECT-TYPE
	SYNTAX	INTEGER {
		internal (1),
		external (2)
		}
	ACCESS	read-write
	STATUS	mandatory
--	DEFVAL 	1
	DESCRIPTION	"Sets the channel's source for receive clock.  This
			parameter, combined with the syncChPhyParaChTxClocking
			parameter, corresponds to the Channel Clocking parameter
			which is displayed in the Sync channel configuration menu
			of the unit's $CMD facility.  This parameter is configurable
			for all Sync protocols EXCEPT fastPacket(9)."
	::= {syncChPhyParaEntry 17}

syncChPhyParaPriority OBJECT-TYPE
	SYNTAX	INTEGER {
		low (1),
		high (2)
		}
	ACCESS 	read-write
	STATUS	mandatory
--	DEFVAL	1
	DESCRIPTION	"Selects the priority of the port, high or low.  This parameter
			is configurable for all Sync protocols EXCEPT tdm(10)." 
	::= {syncChPhyParaEntry 18}

syncChPhyParaDSRControl OBJECT-TYPE
	SYNTAX	INTEGER {
		normal (1),
		fornedOn (2),
		localDTR (3)
		}
	ACCESS	read-write
	STATUS	mandatory
--	DEFVAL	1
	DESCRIPTION	"Selects whether the remote DTR changes appear on the 
			local DSR or are ignored." 
	::= {syncChPhyParaEntry 19}

syncChPhyParaBufferControl OBJECT-TYPE
	SYNTAX	INTEGER {
		none (1),
		cts (2)
		}
	ACCESS	read-write
	STATUS	mandatory
--	DEFVAL	1
	DESCRIPTION	"Used to regulate data transmission from a device attached 
			to user channel.  This parameter is configurable for all Sync
			protocols EXCEPT fastPacket(9) and tdm(10)."
	::= {syncChPhyParaEntry 20}

syncChPhyParaMaxRxBlockSize OBJECT-TYPE
	SYNTAX	INTEGER (1..9999)
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"Sets the block size in bytes of the data to attached device 
			from the Integration unit.  This parameter is configurable for
			all Sync protocols EXCEPT micomDlc(7), fastPacket(9), and tdm(10).

			For Sync protocol hpSync(4), the parameter default is 9876.  For
			Sync protocol micomVoice(8), the parameter default is 1024.  For
			Sync protocols dlcChannel(1), asciiBisync(2), ebcdicBisync(3),
			rtsCts(5), and syncPad(6); the parameter default is 512."
	::= {syncChPhyParaEntry 21}

syncChPhyParaProtocol OBJECT-TYPE
	SYNTAX	INTEGER {
		dlc (1),
		asciiBisync (2),
		ebcdicBisync (3),
		hpSync (4),
		rtsCts (5),
		syncPad (6),
		micomDlc (7),
		micomVoice (8),
		fastPacket (9),
		tdm (10)
		}
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION	"Sets the sync channel protocol." 
	::= {syncChPhyParaEntry 22}

syncChPhyParaClaimedBandwidth OBJECT-TYPE
	SYNTAX	INTEGER
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"Sets the claimed bandwidth of the sync channel.  This
			is the amount of bandwidth that should be reserved on a link
			for this channel / 10.  -ie-  If you want to reserve 9600 bps
			for this channel you should set this to 960.  This parameter
			is configurable for all Sync protocols EXCEPT tdm(10)." 
	::= {syncChPhyParaEntry 23}

syncChPhyParaDLCTransitRate OBJECT-TYPE
	SYNTAX	INTEGER {
		channelRate (1),
		rate1200 (2),
		rate2400 (3),
		rate3600 (4),
		rate4800 (5),
		rate7200 (6),
		rate9600 (7),
		rate12000 (8),
		rate14400 (9),
		rate16800 (10),

		rate19200 (11),
		rate28800 (12),
		rate38400 (13),
		rate48000 (14),
		rate56000 (15),
		rate64000 (16)
		}
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"Sets the transit rate of the sync channel.  This parameter
			is ONLY configurable for the Sync protocol dlcChannel(1)." 
--	DEFVAL	1
	::= {syncChPhyParaEntry 24}

-- N_CMOS.O_ASYNC ========================================================

asyncChPhyParaTable OBJECT-TYPE
	SYNTAX	SEQUENCE OF AsyncChPhyParaEntry
	ACCESS	not-accessible
	STATUS	mandatory
	DESCRIPTION	"For the configuration of asynchronous channels, this table controls
			and modifies the operating parameters for the port." 
	::= {async-channel 1}

asyncChPhyParaEntry OBJECT-TYPE
	SYNTAX	AsyncChPhyParaEntry
	ACCESS	not-accessible
	STATUS	mandatory
	DESCRIPTION	"Entry into the table pertaining to a specific channel on a specific module." 
	INDEX		{asyncChPhyParaCardNumber, asyncChPhyParaChannelNumber}
	::= {asyncChPhyParaTable 1}

AsyncChPhyParaEntry ::= SEQUENCE {
	asyncChPhyParaCardNumber		INTEGER,
	asyncChPhyParaChannelNumber		INTEGER,
	asyncChPhyParaStopBits			INTEGER,
	asyncChPhyParaCodeLevel		INTEGER,
	asyncChPhyParaDataRate			INTEGER,
	asyncChPhyParaEcho			INTEGER,
	asyncChPhyParaHPXMode		INTEGER,
	asyncChPhyParaConnectTo		INTEGER,
	asyncChPhyParaRemCTSControl		INTEGER,
	asyncChPhyParaCRDelay			INTEGER,
	asyncChPhyParaLFDelay			INTEGER,
	asyncChPhyParaFFDelay			INTEGER,
	asyncChPhyParaXONChar		INTEGER,
	asyncChPhyParaXOFFChar		INTEGER,
	asyncChPhyParaCmdFacilityAccess	INTEGER,
	asyncChPhyParaCmdModeEntry		INTEGER,
	asyncChPhyParaLocChannelConfig	INTEGER,
	asyncChPhyParaCmdModeAccess		INTEGER,
	asyncChPhyParaTandemSupport		INTEGER,
	asyncChPhyParaSmoothScroll		INTEGER,
	asyncChPhyParaFlowControl		INTEGER,
	asyncChPhyParaBufferControl		INTEGER,
	asyncChPhyParaFlowControlStrip		INTEGER,
	asyncChPhyParaSyncLossDisconnect	INTEGER,
	asyncChPhyParaEIAControls		INTEGER,
	asyncChPhyParaPriority			INTEGER,
	asyncChPhyParaParity			INTEGER,
	asyncChPhyParaCompression		INTEGER
	}

asyncChPhyParaCardNumber  OBJECT-TYPE
	SYNTAX	INTEGER (1..5)
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION	"The module/card number containing the async channel to which
			the entry pertains to." 
	::= {asyncChPhyParaEntry 1}

asyncChPhyParaChannelNumber  OBJECT-TYPE
	SYNTAX	INTEGER (1..32)
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION	"The async channel number to which the entry pertains to." 
	::= {asyncChPhyParaEntry 2}

asyncChPhyParaStopBits	 OBJECT-TYPE
	SYNTAX	INTEGER {
		oneBit (1),
		onePointFiveBits (2),
		twoBits (3)
		}
	ACCESS	read-write
	STATUS	mandatory
--	DEFVAL	1
	DESCRIPTION	"Sets the number of stop-bits inserted at the end of a character." 
	::= {asyncChPhyParaEntry 3}

asyncChPhyParaCodeLevel OBJECT-TYPE
	SYNTAX	INTEGER {
		fiveBits (1),
		sixBits (2),
		sevenBits (3),
		eightBits (4),
		nineBits (5)
		}
	ACCESS	read-write
	STATUS	mandatory
--	DEFVAL	4
	DESCRIPTION	"Sets the number of data bits per character. This includes the
			parity bit." 
	::= {asyncChPhyParaEntry 4}

asyncChPhyParaDataRate	 OBJECT-TYPE
	SYNTAX	INTEGER {
		abr (1),
		rate50 (2),
		rate75 (3),
		rate110 (4),
		rate134dot5 (5),
		rate150 (6),
		rate200 (7),
		rate225 (8),
		rate300 (9),
		rate600 (10),
		rate1200 (11),
		rate1800 (12),
		rate2400 (13),
		rate3600 (14),
		rate4800 (15),
		rate7200 (16),
		rate9600 (17),
		rate14400 (18),
		rate19200 (19),
		rate28800 (20),
		rate38400 (21)
		}
	ACCESS	read-write
	STATUS	mandatory
--	DEFVAL	17
	DESCRIPTION	"Sets the data rate in bits per seconds of the async user channel." 
	::= {asyncChPhyParaEntry 5}

asyncChPhyParaEcho OBJECT-TYPE
	SYNTAX	INTEGER {
		on (1),
		off (2)
	}
	ACCESS	read-write
	STATUS	mandatory
--	DEFVAL	2
	DESCRIPTION	"Turns the echo function on or off when the terminal(2) option
			is selected for the asyncChPhyParaConnectTo object." 
	::= {asyncChPhyParaEntry 6}

asyncChPhyParaHPXMode OBJECT-TYPE
	SYNTAX	INTEGER {
		on (1),
		off (2)
	}
	ACCESS	read-write
	STATUS	mandatory
--	DEFVAL	2
	DESCRIPTION	"Sould be set to ON only when Hewlett Packerd HP-3000 view
			systems using type 26XX display stations (operating under
			term type 10) are connected to the port being configured." 
	::= {asyncChPhyParaEntry 7}

asyncChPhyParaConnectTo OBJECT-TYPE
	SYNTAX	INTEGER {
		host (1),
		terminal (2)
	}
	ACCESS	read-write
	STATUS	mandatory
--	DEFVAL	2
	DESCRIPTION	" Sets the type of equipment which is connected to the
			port, a host or terminal (also printer)." 
	::= {asyncChPhyParaEntry 8}

asyncChPhyParaRemCTSControl OBJECT-TYPE
	SYNTAX	INTEGER {
		forcedOn (1),
		normal (2)
	}
	ACCESS	read-write
	STATUS	mandatory
--	DEFVAL	1
	DESCRIPTION	"Determines whether remote CTS signal is strapped or transparent." 
	::= {asyncChPhyParaEntry 9}

asyncChPhyParaCRDelay OBJECT-TYPE
	SYNTAX	INTEGER (0..99)
	ACCESS	read-write
	STATUS	mandatory
--	DEFVAL	0
	DESCRIPTION	"Sets the delay in character times to allow for mechanical
			action to take place when a carriage return control character
			is transmitted to a channel." 
	::= {asyncChPhyParaEntry 10}

asyncChPhyParaLFDelay OBJECT-TYPE
	SYNTAX	INTEGER (0..99)
	ACCESS	read-write
	STATUS	mandatory
--	DEFVAL	0
	DESCRIPTION	"Sets the delay in character times to allow for mechanical
			action to take place when a line feed control character is
			transmitted to a channel." 
	::= {asyncChPhyParaEntry 11}

asyncChPhyParaFFDelay OBJECT-TYPE
	SYNTAX	INTEGER (0..99)
	ACCESS	read-write
	STATUS	mandatory
--	DEFVAL	0
	DESCRIPTION	"Sets the delay in character times to allow for mechanical
			action to take place when a form feed control character is
			transmitted to a channel." 
	::= {asyncChPhyParaEntry 12}

asyncChPhyParaXONChar OBJECT-TYPE
	SYNTAX	INTEGER {
		dc1 (1),
		dc2 (2),
		dc3 (3),
		dc4 (4)
		}
	ACCESS	read-write
	STATUS	mandatory
--	DEFVAL	1
	DESCRIPTION	"Defines the different XON characters." 
	::= {asyncChPhyParaEntry 13}

asyncChPhyParaXOFFChar OBJECT-TYPE
	SYNTAX	INTEGER {
		dc1 (1),
		dc2 (2),
		dc3 (3),
		dc4 (4)
		}
	ACCESS	read-write
--	DEFVAL	3
	STATUS	mandatory
	DESCRIPTION	"Defines the different XOFF characters." 
	::= {asyncChPhyParaEntry 14}

asyncChPhyParaCmdFacilityAccess OBJECT-TYPE
	SYNTAX	INTEGER {
		enable (1),
		disable (2)
		}
	ACCESS	read-write
	STATUS	mandatory
--	DEFVAL	1
	DESCRIPTION	"Permits or denies (the port being configured) access to the
			Command Facility Main Menu to view or change the system
			operating parameters." 
	::= {asyncChPhyParaEntry 15}

asyncChPhyParaCmdModeEntry OBJECT-TYPE
	SYNTAX	INTEGER {
		ctlXctlY (1),
		ctlXbreak (2)
		}
	ACCESS	read-write
	STATUS	mandatory
--	DEFVAL	2
	DESCRIPTION	"Selects the control character sequence to access to the 
			Command Mode." 
	::= {asyncChPhyParaEntry 16}

