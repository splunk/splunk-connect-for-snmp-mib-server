			configuration." 
	::= {esiSecLinkCfgEntry 2}

esiSecLinkCfgDataRate OBJECT-TYPE
	SYNTAX	INTEGER {
		external (1),
		rate2400Sync (2),
		rate4800Sync (3),
		rate9600Sync (4),
		rate12000Sync (5),
		rate14400Sync (6),
		rate16800Sync (7),
		rate19200Sync (8),
		rate38400Sync (9),
		rate56000Sync (10),
		rate64000Sync (11)
		}
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"Sets the link clocking method to external, or if internal, the
			data rate for a secondary ESI link. Note: When the value of this
			parameter is changed, the link must be reset." 
	::= {esiSecLinkCfgEntry 3}

-- N_CMOS.O_ESI2L ========================================================

esiSecLinkAssignmentTable OBJECT-TYPE
	SYNTAX	SEQUENCE OF EsiSecLinkAssignmentEntry
	ACCESS	not-accessible
	STATUS	mandatory
	DESCRIPTION	"Table to assign a secondary link channel to a primary 
			ESI link." 
	::= {esi-secondary-link 2}

esiSecLinkAssignmentEntry OBJECT-TYPE
	SYNTAX	EsiSecLinkAssignmentEntry
	ACCESS	not-accessible
	STATUS	mandatory
	DESCRIPTION	"Entry within the table to specify a particular link/port
			on a specific module/card for a secondary assignment to a
			primary link." 
	INDEX		{esiSecLinkAssignmentSecCardNumber,
			 esiSecLinkAssignmentSecChannelNumber}
	::= {esiSecLinkAssignmentTable 1} 

EsiSecLinkAssignmentEntry ::= SEQUENCE {
	esiSecLinkAssignmentSecCardNumber	INTEGER,
	esiSecLinkAssignmentSecChannelNumber	INTEGER,
	esiSecLinkAssignmentEntryAction		INTEGER,
	esiSecLinkAssignmentPriCardNumber	INTEGER,
	esiSecLinkAssignmentPriChannelNumber	INTEGER
	}

esiSecLinkAssignmentSecCardNumber OBJECT-TYPE
	SYNTAX	INTEGER
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION	"Identifies the specific module/card number of the link
			to be assigned as the secondary link." 
	 ::= {esiSecLinkAssignmentEntry 1}

esiSecLinkAssignmentSecChannelNumber OBJECT-TYPE
	SYNTAX	INTEGER
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION	"Identifies the specific link/port number of the link
			to be assigned as the secondary link." 
	 ::= {esiSecLinkAssignmentEntry 2}

esiSecLinkAssignmentEntryAction OBJECT-TYPE
	SYNTAX	INTEGER {
		active (1)
		}
	ACCESS	write-only
	STATUS	mandatory
	DESCRIPTION	"This is the command to perform the secondary link assignment. 
			Selecting 'active' following the successful setting of 
			esiSecLinkAssignmentPriCardNumber & esiSecLinkAssignmentPriChannelNumber
			will complete the assignment." 
	::= {esiSecLinkAssignmentEntry 3}

esiSecLinkAssignmentPriCardNumber OBJECT-TYPE
	SYNTAX	INTEGER
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"Indicates the card number where the primary link channel is residing. 
			A GET value of FFh indicates that no secondary link assignment 
			currently exists.  A SET with a value of zero (0)
			with zero (0) as the channel number de-assigns the link." 
	 ::= {esiSecLinkAssignmentEntry 4}

esiSecLinkAssignmentPriChannelNumber OBJECT-TYPE
	SYNTAX	INTEGER
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"Indicates the channel number where the primary channel is residing. 
			A read value of FFh indicates that no secondary link assignment 
			currently exists.  A SET with a value of zero (0)
			with zero (0) as the card number de-assigns the link. " 
	 ::= {esiSecLinkAssignmentEntry 5}



-- FRAME RELAY

frameRelayLinkCfgTable OBJECT-TYPE
	SYNTAX	SEQUENCE OF FrameRelayLinkEntry
	ACCESS	not-accessible
	STATUS	mandatory
	DESCRIPTION	"Table to configure a Frame Relay link." 
	::= {frame-relay-link 1}

frameRelayLinkCfgEntry OBJECT-TYPE
	SYNTAX	FrameRelayLinkEntry
	ACCESS	not-accessible
	STATUS	mandatory
	DESCRIPTION	"Entry within the table to specify a particular port number
			on a specific module/card for a Frame Relay link configuration." 
	INDEX		{frameRelayLinkCfgCardNumber, frameRelayLinkCfgChannelNumber}
	::= {frameRelayLinkCfgTable 1} 

FrameRelayLinkEntry ::= SEQUENCE {
	frameRelayLinkCfgCardNumber			INTEGER,
	frameRelayLinkCfgChannelNumber		INTEGER,
	frameRelayLinkCfgDataRate			INTEGER,
	frameRelayLinkCfgLocalMgmtProtocol		INTEGER,
	frameRelayLinkCfgFullStatusPollingCounter	INTEGER,
	frameRelayLinkCfgErrorThreshold			INTEGER,
	frameRelayLinkCfgEventCount			INTEGER,
	frameRelayLinkCfgLinkIntegrityVerificationTimer	INTEGER,
	frameRelayLinkCfgPollingVerificationTimer	INTEGER,
	frameRelayLinkCfgMaxFrameSize			INTEGER,
	frameRelayLinkCfgRxClockingSource		INTEGER,
	frameRelayLinkCfgTxClockingSource		INTEGER,
	frameRelayLinkCfgNetworkAddress		OCTET STRING,
	frameRelayLinkCfgAccessLinkMode 		INTEGER
	}

frameRelayLinkCfgCardNumber OBJECT-TYPE
	SYNTAX	INTEGER
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION	"Identifies the specific module/card number for a Frame Relay
			link configuration." 
	 ::= {frameRelayLinkCfgEntry 1}

frameRelayLinkCfgChannelNumber OBJECT-TYPE
	SYNTAX	INTEGER
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION	"Identifies the specific port number for a Frame Relay link
			configuration." 
	 ::= {frameRelayLinkCfgEntry 2}

frameRelayLinkCfgDataRate OBJECT-TYPE
	SYNTAX	INTEGER {
		rate9600 (1),
		rate19200 (2),
		rate64000 (3),
		rate126000 (4),
		rate126400 (5),
		rate128000 (6),
		rate189500 (7),
		rate192000 (8),
		rate246400 (9),
		rate252000 (10),
		rate256000 (11),
		rate308000 (12),
		rate310200 (13),
		rate336000 (14),
		rate366500 (15),
		rate379100 (16),
		rate384000 (17),
		rate448000 (18),
		rate492800 (19),
		rate504000 (20),
		rate537600 (21)
		}
	ACCESS	read-write
	STATUS	mandatory
--	DEFVAL	3
	DESCRIPTION	"Sets the data rate for a Frame Relay link.  Note: If the 
			frame relay access link is externally clocked, no configuration
			of this paramter is necessary." 
	::= { frameRelayLinkCfgEntry 3}

frameRelayLinkCfgLocalMgmtProtocol OBJECT-TYPE
	SYNTAX	INTEGER {
		none (1),
		annexD (2),
		lmi (3)
		}
	ACCESS	read-write
	STATUS	mandatory
--	DEFVAL	2
	DESCRIPTION	"Sets the frame relay access link local management protocol." 
	::= { frameRelayLinkCfgEntry 4}

frameRelayLinkCfgFullStatusPollingCounter OBJECT-TYPE
	SYNTAX	INTEGER (1..255)
	ACCESS	read-write
	STATUS	mandatory
--	DEFVAL	6
	DESCRIPTION	"Sets the frame relay access link full status poll counter." 
	::= { frameRelayLinkCfgEntry 5}

frameRelayLinkCfgErrorThreshold OBJECT-TYPE
	SYNTAX	INTEGER (1..10)
	ACCESS	read-write
	STATUS	mandatory
--	DEFVAL	4
	DESCRIPTION	"Sets the frame relay access link error threshold." 
	::= { frameRelayLinkCfgEntry 6}

frameRelayLinkCfgEventCount OBJECT-TYPE
	SYNTAX	INTEGER (1..10)
	ACCESS	read-write
	STATUS	mandatory
--	DEFVAL	4
	DESCRIPTION	"Sets the frame relay access link monitored events count." 
	::= { frameRelayLinkCfgEntry 7}

frameRelayLinkCfgLinkIntegrityVerificationTimer OBJECT-TYPE
	SYNTAX	INTEGER (5..30)
	ACCESS	read-write
	STATUS	mandatory
--	DEFVAL	10
	DESCRIPTION	"Sets the frame relay access link integrity verification
			timer." 
	::= { frameRelayLinkCfgEntry 8}

frameRelayLinkCfgPollingVerificationTimer OBJECT-TYPE
	SYNTAX	INTEGER (5..30)
	ACCESS	read-write
	STATUS	mandatory
--	DEFVAL	15
	DESCRIPTION	"Sets the frame relay access link polling verification
			timer." 
	::= { frameRelayLinkCfgEntry 9}

frameRelayLinkCfgMaxFrameSize OBJECT-TYPE
	SYNTAX	INTEGER (5..8189)
	ACCESS	read-write
	STATUS	mandatory
--	DEFVAL	4096
	DESCRIPTION	"Sets the maximum frame relay access link frame size. 
			The numbers are in octets." 
	::= { frameRelayLinkCfgEntry 10}

frameRelayLinkCfgRxClockingSource OBJECT-TYPE
	SYNTAX	INTEGER {
		external (1),
		internal (2)
		}
	ACCESS	read-write
	STATUS	mandatory
--	DEFVAL	1
	DESCRIPTION	"Sets the port clocking configuration for receiving 
			data over the frame relay access link." 
	::= { frameRelayLinkCfgEntry 11}

frameRelayLinkCfgTxClockingSource OBJECT-TYPE
	SYNTAX	INTEGER {
		external (1),
		internal (2)
		}
	ACCESS	read-write
	STATUS	mandatory
--	DEFVAL	1
	DESCRIPTION	"Sets the port clocking configuration for transmitting
			data over the frame relay access link." 
	::= { frameRelayLinkCfgEntry 12}

frameRelayLinkCfgNetworkAddress OBJECT-TYPE
	SYNTAX	OCTET STRING (SIZE (0..16))
	ACCESS	read-write
	STATUS	mandatory
--	DEFVAL	1
	DESCRIPTION	"Sets the network address for the frame relay access link. 
			This address is assigned by the carrier provider based on 
			a standardized numbering scheme." 
	::= { frameRelayLinkCfgEntry 13}

frameRelayLinkCfgAccessLinkMode OBJECT-TYPE
	SYNTAX	INTEGER {
		userEquipmentMode (1),
		networkMode (2)
		}
	ACCESS	read-write
	STATUS	mandatory
--	DEFVAL	1
	DESCRIPTION	"Sets the access mode for the frame relay access link." 
	::= { frameRelayLinkCfgEntry 14}

-- N_CMOS.O_DLCI ======================================================

frameRelayLinkCfgDLCITable OBJECT-TYPE
	SYNTAX	SEQUENCE OF FrameRelayLinkCfgDLCIEntry
	ACCESS	not-accessible
	STATUS	mandatory
	DESCRIPTION	"Table to configure the DLCI bandwidth charactistics for a 
			frame relay link." 
	::= {frame-relay-link 2}

frameRelayLinkCfgDLCIEntry OBJECT-TYPE
	SYNTAX	FrameRelayLinkCfgDLCIEntry
	ACCESS	not-accessible
	STATUS	mandatory
	DESCRIPTION	"Entry within the table to specify a DLCI number, the FR link/port
			number, and the module/card number for setting DLCI bandwidth
			characteristics." 
	INDEX		{ frameRelayLinkCfgDLCICardNumber, frameRelayLinkCfgDLCIChannelNumber,
			frameRelayLinkCfgDLCINumber }
	::= {frameRelayLinkCfgDLCITable 1}

FrameRelayLinkCfgDLCIEntry ::= SEQUENCE {
	frameRelayLinkCfgDLCICardNumber			INTEGER,
	frameRelayLinkCfgDLCIChannelNumber			INTEGER,
	frameRelayLinkCfgDLCINumber				INTEGER,
	frameRelayLinkCfgDLCIType				INTEGER,	
	frameRelayLinkCfgDLCICommittedInfoRateForward 	INTEGER,
	frameRelayLinkCfgDLCICommittedInfoRateBackward 	INTEGER,
	frameRelayLinkCfgDLCICommittedBurstSizeForward	INTEGER,
	frameRelayLinkCfgDLCICommittedBurstSizeBackward 	INTEGER,
	frameRelayLinkCfgDLCIExcessBurstSizeForward 		INTEGER,
	frameRelayLinkCfgDLCIExcessBurstSizeBackward 		INTEGER
	}

frameRelayLinkCfgDLCICardNumber OBJECT-TYPE
	SYNTAX	INTEGER
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION	"Identifies the module/card number for a particular DLCI
			of a specific FR link for setting DLCI bandwidth characteristics." 
	::= {frameRelayLinkCfgDLCIEntry 1}

frameRelayLinkCfgDLCIChannelNumber OBJECT-TYPE
	SYNTAX	INTEGER
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION	"Identifies the FR link/port number of a particular DLCI channel
			for setting the bandwidth characteristics." 
	::= {frameRelayLinkCfgDLCIEntry 2}

frameRelayLinkCfgDLCINumber OBJECT-TYPE
	SYNTAX	INTEGER
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION	"Identifies a particular DLCI number for setting the bandwidth 
			characteristics." 
	::= {frameRelayLinkCfgDLCIEntry 3}

frameRelayLinkCfgDLCIType OBJECT-TYPE
	SYNTAX	INTEGER {
		undefined (1),
		annexDType (2),
		virtualLink (3),
		lmi (4),
		transparent (5),
		fragmentation (6)
		}
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION	"Identifies the DLCI type configuration.  Note that only 
			Options (3), (5), and (6) are expected." 
	::= {frameRelayLinkCfgDLCIEntry 4}

frameRelayLinkCfgDLCICommittedInfoRateForward OBJECT-TYPE
	SYNTAX	INTEGER (0..9999999)
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"Sets the value for committed information rate forward. 
			This value is available from your frame relay service provider." 
	::= {frameRelayLinkCfgDLCIEntry 5}

frameRelayLinkCfgDLCICommittedInfoRateBackward OBJECT-TYPE
	SYNTAX	INTEGER (0..9999999)
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"Sets the value for committed information rate backward. 
			This value is available from your frame relay service provider." 
	::= {frameRelayLinkCfgDLCIEntry 6}

frameRelayLinkCfgDLCICommittedBurstSizeForward OBJECT-TYPE
	SYNTAX	INTEGER (0..9999999)
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"Sets the value for committed burst size backward. 
			This value is available from your frame relay service provider." 
	::= {frameRelayLinkCfgDLCIEntry 7}

frameRelayLinkCfgDLCICommittedBurstSizeBackward OBJECT-TYPE
	SYNTAX	INTEGER (0..9999999)
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"Sets the value for committed burst size forward. 
			This value is available from your frame relay service provider." 
	::= {frameRelayLinkCfgDLCIEntry 8}

frameRelayLinkCfgDLCIExcessBurstSizeForward OBJECT-TYPE
	SYNTAX	INTEGER (0..9999999)
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"Sets the value for excess burst size backward. 
			This value is available from your frame relay service provider.
			Note: should be equal to the FR access data rate minus the 
			CIR backward." 
	::= {frameRelayLinkCfgDLCIEntry 9}

frameRelayLinkCfgDLCIExcessBurstSizeBackward OBJECT-TYPE
	SYNTAX	INTEGER (0..9999999)
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"Sets the value for excess burst size forward. 
			This value is available from your frame relay service provider.
			Note: should be equal to the FR access data rate minus the 
			CIR forward." 
	::= {frameRelayLinkCfgDLCIEntry 10}


-- N_CMOS.O_FRCNTL  =====================================================

frameRelayLinkControlTable OBJECT-TYPE
	SYNTAX	SEQUENCE OF FrameRelayLinkControlEntry
	ACCESS	not-accessible
	STATUS	mandatory
	DESCRIPTION	"Table to add or delete DLCI channels for a FR link." 
	::= {frame-relay-link 3}

frameRelayLinkControlEntry OBJECT-TYPE
	SYNTAX	FrameRelayLinkControlEntry
	ACCESS	not-accessible
	STATUS	mandatory
	DESCRIPTION	"Entry within the table to specify a particular DLCI number
			for addition/deletion to a particular FR access link on a
			specific module/card." 
	INDEX		{frameRelayLinkControlLinkCardNumber, 
			frameRelayLinkControlLinkChannelNumber,
			frameRelayLinkControlDLCINumber}
	::= {frameRelayLinkControlTable 1}

FrameRelayLinkControlEntry ::= SEQUENCE {
	frameRelayLinkControlLinkCardNumber		INTEGER,
	frameRelayLinkControlLinkChannelNumber	INTEGER,
	frameRelayLinkControlDLCINumber		INTEGER,
	frameRelayLinkControlCommand			INTEGER,
	frameRelayLinkControlDLCIType 			INTEGER
	}

frameRelayLinkControlLinkCardNumber OBJECT-TYPE
	SYNTAX	INTEGER
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION	"Identifies the module/card number of a FR access link for
			adding/deleting a DLCI channel." 
	 ::= {frameRelayLinkControlEntry 1}

frameRelayLinkControlLinkChannelNumber OBJECT-TYPE
	SYNTAX	INTEGER
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION	"Identifies a FR access link/port number for adding/deleting
			a DLCI channel." 
	 ::= {frameRelayLinkControlEntry 2}

frameRelayLinkControlDLCINumber OBJECT-TYPE
	SYNTAX	INTEGER (16..991)
	ACCESS	write-only
	STATUS	mandatory
	DESCRIPTION	"Sets the DLCI channel number to be added or deleted." 
	 ::= {frameRelayLinkControlEntry 3}

frameRelayLinkControlCommand OBJECT-TYPE
	SYNTAX	INTEGER {
		addDLCI (1),
		deleteDLCI (2)
		}
	ACCESS	write-only
	STATUS	mandatory
	DESCRIPTION	"Command to select whether the DLCI number specified in
			frameRelayLinkControlDLCINumber is to be added or deleted.
			This command sould be sent ater the DLCI type has been sent." 
	 ::= {frameRelayLinkControlEntry 4}

frameRelayLinkControlDLCIType OBJECT-TYPE
	SYNTAX	INTEGER {
		virtualLink (1),
		transparent (2),
		fragmentation (3)
		}
	ACCESS	write-only
	STATUS	mandatory
	DESCRIPTION	"Sets the type of DLCI channel that is being added.
			This object should be set before the previous command
			object is set." 
	 ::= {frameRelayLinkControlEntry 5}



-- N_CMOS.O_DLCNTL  =====================================================

frameRelayDLCIControlTable OBJECT-TYPE
	SYNTAX	SEQUENCE OF FrameRelayDLCIControlEntry
	ACCESS	not-accessible
	STATUS	mandatory
	DESCRIPTION	"Table to configure force connections from one DLCI channel
			on a FR access link to another FR access link DLCI channel.
			Note: This will connect 2 FR access links together on a single
			Integration unit which acts like a FR switch." 
	::= {frame-relay-link 4}

frameRelayDLCIControlEntry OBJECT-TYPE
	SYNTAX	FrameRelayDLCIControlEntry
	ACCESS	not-accessible
	STATUS	mandatory
	DESCRIPTION	"Entry within the table specifying a particular DLCI channel,
			a FR access link/port number, and module/card number for a 
			force connection configuration." 
	INDEX		{frameRelayDLCIControlLinkCardNumber, 
			frameRelayDLCIControlLinkChannelNumber,
			frameRelayDLCIControlDLCINumber}
	::= {frameRelayDLCIControlTable 1}

FrameRelayDLCIControlEntry ::= SEQUENCE {
	frameRelayDLCIControlLinkCardNumber		INTEGER,
	frameRelayDLCIControlLinkChannelNumber	INTEGER,
	frameRelayDLCIControlDLCINumber		INTEGER,
	frameRelayDLCIControlCommand			INTEGER,
	frameRelayDLCIControlFconnNodeName 		DisplayString,
	frameRelayDLCIControlFconnCardNumber		INTEGER,
	frameRelayDLCIControlFconnChannelNumber	INTEGER,
	frameRelayDLCIControlFconnDLCINumber	INTEGER
	}

frameRelayDLCIControlLinkCardNumber OBJECT-TYPE
	SYNTAX	INTEGER
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION	"Identifies the module/card number of the source FR access link
			for force connection configuration." 
	 ::= {frameRelayDLCIControlEntry 1}

frameRelayDLCIControlLinkChannelNumber OBJECT-TYPE
	SYNTAX	INTEGER
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION	"Identifies the port/channel number of the source FR access link
			for force connection configuration." 
	 ::= {frameRelayDLCIControlEntry 2}

frameRelayDLCIControlDLCINumber OBJECT-TYPE
	SYNTAX	INTEGER (16..991)
	ACCESS	write-only
	STATUS	mandatory
	DESCRIPTION	"Identifies the DLCI channel number of the source FR access link
			for force connection configuration." 
	 ::= {frameRelayDLCIControlEntry 3}

frameRelayDLCIControlCommand OBJECT-TYPE
	SYNTAX	INTEGER {
		connectDLCI (1),
		disconnectDLCI (2)
		}
	ACCESS	write-only
	STATUS	mandatory
	DESCRIPTION	"Selects the force connection configuration between 2 FR access
			links on a single Integration unit." 
	 ::= {frameRelayDLCIControlEntry 4}

frameRelayDLCIControlFconnNodeName OBJECT-TYPE
	SYNTAX	DisplayString (SIZE(1..8))
	ACCESS	write-only
	STATUS	mandatory
	DESCRIPTION	"Selects the name of the Integration unit for the FR link force 
			connection configuration." 
	 ::= {frameRelayDLCIControlEntry 5}

frameRelayDLCIControlFconnCardNumber OBJECT-TYPE
	SYNTAX	INTEGER
	ACCESS	write-only
	STATUS	mandatory
	DESCRIPTION	"Selects the module/card number of the destination FR access link
			for force connection configuration." 
	 ::= {frameRelayDLCIControlEntry 6}

frameRelayDLCIControlFconnChannelNumber OBJECT-TYPE
	SYNTAX	INTEGER
	ACCESS	write-only
	STATUS	mandatory
	DESCRIPTION	"Selects the port/channel number of the destination FR access link
			for force connection configuration." 
	 ::= {frameRelayDLCIControlEntry 7}

frameRelayDLCIControlFconnDLCINumber OBJECT-TYPE
	SYNTAX	INTEGER
	ACCESS	write-only
	STATUS	mandatory
	DESCRIPTION	"Selects the DLCI channel number of the destination FR access link
			for force connection configuration." 
	 ::= {frameRelayDLCIControlEntry 8}

-- N_CMOS.O_FRCONS  =====================================================

frameRelayDLCIConnectsTable OBJECT-TYPE
	SYNTAX	SEQUENCE OF FrameRelayDLCIConnectsEntry
	ACCESS	not-accessible
	STATUS	mandatory
	DESCRIPTION	"Table to view DLCI connections between FR access links." 
	::= {frame-relay-link 5}

frameRelayDLCIConnectsEntry OBJECT-TYPE
	SYNTAX	FrameRelayDLCIConnectsEntry
	ACCESS	not-accessible
	STATUS	mandatory
	DESCRIPTION	"Entry within the table specific to the FR access link 
			module/card number, the port/channel number, and DLCI
			channel number for viewing DLCI connections." 
	INDEX		{frameRelayDLCIConnectsLinkCardNumber, 
			frameRelayDLCIConnectsLinkChannelNumber,
			frameRelayDLCIConnectsDLCIConnected}
	::= {frameRelayDLCIConnectsTable 1}

FrameRelayDLCIConnectsEntry ::= SEQUENCE {
	frameRelayDLCIConnectsLinkCardNumber		INTEGER,
	frameRelayDLCIConnectsLinkChannelNumber	INTEGER,
	frameRelayDLCIConnectsDLCIConnected 		INTEGER,
	frameRelayDLCIConnectsDestCardNumber		INTEGER,
	frameRelayDLCIConnectsDestChannelNumber	INTEGER,
	frameRelayDLCIConnectsDestDLCINumber	INTEGER
	}

frameRelayDLCIConnectsLinkCardNumber OBJECT-TYPE
	SYNTAX	INTEGER
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION	"Identifies the module/card number of the source FR access link
			to view connection configurations." 
	 ::= {frameRelayDLCIConnectsEntry 1}

frameRelayDLCIConnectsLinkChannelNumber OBJECT-TYPE
	SYNTAX	INTEGER
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION	"Identifies the port/channel number of the source FR access link
			to view connection configurations." 
	 ::= {frameRelayDLCIConnectsEntry 2}

frameRelayDLCIConnectsDLCIConnected OBJECT-TYPE
	SYNTAX	INTEGER
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION	"Identifies the DLCI channel number of the source FR access link
			to view connection configurations." 
	 ::= {frameRelayDLCIConnectsEntry 3}

frameRelayDLCIConnectsDestCardNumber OBJECT-TYPE
	SYNTAX	INTEGER
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION	"Identifies the module/card number of the destination FR access link
			to view connection configurations." 
	 ::= {frameRelayDLCIConnectsEntry 4}

frameRelayDLCIConnectsDestChannelNumber OBJECT-TYPE
	SYNTAX	INTEGER
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION	"Identifies the port/channel number of the destination FR access link
			to view connection configurations." 
	 ::= {frameRelayDLCIConnectsEntry 5}

