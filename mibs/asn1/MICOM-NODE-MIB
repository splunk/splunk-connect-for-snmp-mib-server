	SYNTAX	INTEGER {
		resetAllDLCI (1)
		}
	ACCESS	write-only
	STATUS	mandatory
	DESCRIPTION	"Resets all the configured DLCI on a specific link/channel." 
	 ::= {frameRelayAllDLCIResetEntry 3}


