			The actual rate will be determined by the following formula:
			rate of the CCM port = (base rate)*(number of DS0 channels." 
--	DEFVAL	1
	::= {e1CfgGroup 20}

e1CfgSystemClockSource OBJECT-TYPE
	SYNTAX	INTEGER {
		clockDerivedL1 (1),
		clockDerivedL2 (2),
		internalClocking (3)
		}
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"Configures the source of the clock used in the E1 Access 
			Module. Options include:
			- clockDerivedL1.  The clock source is from the L1 interface.
			- clockDerivedL2.  The clock source is from the L2 interface.
			- internalClocking.  The clock is derived from a clock source
				internal to the E1 Access Module." 
--	DEFVAL	1
	::= {e1CfgGroup 21}

e1CfgL1Crc4 OBJECT-TYPE
	SYNTAX	INTEGER {
		enabled (1),
		disabled (2)
		}
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"Enables/Disables the CRC-4 mode for the L1 interface on the
			E1 module." 
--	DEFVAL	055
	::= {e1CfgGroup 22}

e1CfgL2Crc4 OBJECT-TYPE
	SYNTAX	INTEGER {
		enabled (1),
		disabled (2)
		}
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"Enables/Disables the CRC-4 mode for the L2 interface on the
			E1 module." 
--	DEFVAL	055
	::= {e1CfgGroup 23}

e1CfgL1Config OBJECT-TYPE
	SYNTAX INTEGER {
		init (1),
		commit (2)
		}
	ACCESS write-only
	STATUS mandatory
	DESCRIPTION 	"This object is a write only object that will signal the proxy to
		start caching changes to the L1 ds0 connection tables.  This is done to
		solve an anomaly in the marathon/e1 design. The manager must set
		this object to init before updating any of the e1L1InterfaceTable objects.
		When all the changes have been set then this object will have to be set to
		commit (2).  The init will cause the proxy to upload the table from the marathon
		and the commit will cause the proxy to send all the changes to the marathon."
	::= {e1CfgGroup 24}

e1CfgL2Config OBJECT-TYPE
	SYNTAX INTEGER {
		init (1),
		commit (2)
		}
	ACCESS write-only
	STATUS mandatory
	DESCRIPTION 	"This object is a write only object that will signal the proxy to
		start caching changes to the L2 ds0 connection tables.  This is done to
		solve an anomaly in the marathon/e1 design. The manager must set
		this object to init before updating any of the e1L2InterfaceTable objects.
		When all the changes have been set then this object will have to be set to
		commit (2).  The init will cause the proxy to upload the table from the marathon
		and the commit will cause the proxy to send all the changes to the marathon."
	::= {e1CfgGroup 25}


-- N_HOUSE.O_T1FLSH ===================================================


e1FlashControlTable OBJECT-TYPE
	SYNTAX	SEQUENCE OF E1FlashControlEntry
	ACCESS	not-accessible
	STATUS	mandatory
	DESCRIPTION	"Table to control (erase) E1 flash memory in preparation for a 
			code download." 
	::= { e1-interface 4}

e1FlashControlEntry OBJECT-TYPE
	SYNTAX	E1FlashControlEntry 
	ACCESS	not-accessible
	STATUS	mandatory
	DESCRIPTION	"Entry within the table specific to a particular E1 module/card
			number for flash memory control." 
	INDEX	 	{ e1FlashControlCardNumber }
	::= { e1FlashControlTable 1}

E1FlashControlEntry ::= SEQUENCE {
	e1FlashControlCardNumber		INTEGER,
	e1FlashControlCommand		INTEGER,
	e1FlashControlFlashStatus		INTEGER
	}

e1FlashControlCardNumber OBJECT-TYPE
	SYNTAX	INTEGER (2..5)
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION	"Identifies the E1 module/card number for flash memory control." 
	::= { e1FlashControlEntry 1}

e1FlashControlCommand OBJECT-TYPE
	SYNTAX	INTEGER {
		eraseCardFlash (1)
		}
	ACCESS	write-only
	STATUS	mandatory
	DESCRIPTION	"Controls the E1 module flash memory in preparation of a code
			download.  Perform a SET with the value eraseCardFlash to erase
			the flash memory prior to downloading new E1 flash software." 
	::= { e1FlashControlEntry 2}

e1FlashControlFlashStatus OBJECT-TYPE
	SYNTAX	INTEGER {
		bootImageCorrupt (1),
		t1FlashImageValid (2),
		t1FlashErased (3),
		t1FlashImageInvalid (4),
		securityViolation (5),
		eraseAborted (6)
		}
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION	"Identifies the status of the E1 module/card flash memory." 
	::= { e1FlashControlEntry 3}

-- Trap Configuration group

-- N_CMOS.O_CMDRPT ======================================================

trapAlarmEventReportingGroup    OBJECT IDENTIFIER ::= {trap 1}

trapAlarmEventReportingAlarms OBJECT-TYPE
	SYNTAX	INTEGER {
		enable (1),
		disable (2)
		}
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"When enabled, displays the received alarms on command facility." 
	::= {trapAlarmEventReportingGroup 1}

trapAlarmEventReportingEvents OBJECT-TYPE
	SYNTAX	INTEGER {
		enable (1),
		disable (2)
		}
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"When enabled, displays the received events on command facility." 
	::= {trapAlarmEventReportingGroup 2}

-- ====================================================================

trapEventsCollectionInterval OBJECT-TYPE
	SYNTAX	INTEGER (1..15)
	ACCESS	read-write 
	STATUS	mandatory
--	DEFVAL	5
	DESCRIPTION	"Periodic interval time between event collection poll cycles 
			(in minutes)."
	::= {trap 2}

-- Trap Subscription Group

trapSubsTable OBJECT-TYPE
	SYNTAX	SEQUENCE OF TrapSubsEntry
	ACCESS	not-accessible
	STATUS	mandatory
	DESCRIPTION	"Table to configure the recipients of the traps." 
	::= {trap 3}

trapSubsEntry OBJECT-TYPE
	SYNTAX	TrapSubsEntry
	ACCESS	not-accessible
	STATUS	mandatory
	DESCRIPTION	"Entry within the table specifying each recipient." 
	INDEX		{ trapSubsIPAddress}
	::= {trapSubsTable 1}

TrapSubsEntry ::= SEQUENCE {
	trapSubsIPAddress		IpAddress,
	trapSubsCommunityString	DisplayString,
	trapSubsAction		INTEGER
	}

trapSubsIPAddress OBJECT-TYPE
	SYNTAX	IpAddress
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"Configures the IP address of the recipient station (manager)."
	::= {trapSubsEntry 1}

trapSubsCommunityString OBJECT-TYPE
	SYNTAX	DisplayString
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"Configures the expected community string for the trap PDU to
			be forwarded to the configured recipient." 
	::= {trapSubsEntry 2}

trapSubsAction OBJECT-TYPE
	SYNTAX	INTEGER {
		subscribe (1),
		cancelSubscription (2)
	}
	ACCESS	write-only
	STATUS	mandatory
	DESCRIPTION	"Adds a new recipient or deletes an already configured recipient
			from the subscription list." 
	::= {trapSubsEntry 3}


-- N_CMOS.O_CMDRPT =======================================================

trapCfgAlarmEventsPortGroup    OBJECT IDENTIFIER ::= {trap 4}

trapCfgTrapPort OBJECT-TYPE
	SYNTAX	INTEGER {
		localCommandPort (1),
		localLogPort (2),
		remoteNode (3),
		externalModem (4)
	}
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"Configures the destination for the output of alarms and events." 
	::= { trapCfgAlarmEventsPortGroup 1}

trapCfgExtModemPriority OBJECT-TYPE
	SYNTAX	INTEGER {
		high (1),
		low (2)
	}
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"Configures the priority for alarms and events sent to an external
			modem." 
	::= { trapCfgAlarmEventsPortGroup 2}


