-- LINK STATISTICS

linkAccumStatisticsTable OBJECT-TYPE
	SYNTAX	SEQUENCE OF LinkAccumStatisticsEntry
	ACCESS	not-accessible
	STATUS	mandatory
	DESCRIPTION	"Table of link statistics for accumulated statistics 
			reports. (all link types except Frame Relay)." 
	::= { statistics-accumulated 3}

linkAccumStatisticsEntry OBJECT-TYPE
	SYNTAX	LinkAccumStatisticsEntry
	ACCESS	not-accessible
	STATUS	mandatory
	DESCRIPTION	"Entry within the table to specify a particular link
			channel on specific module/card." 
	INDEX		{linkAccumStatisticsCardNumber, linkAccumStatisticsChannelNumber }
	::= {linkAccumStatisticsTable 1}

LinkAccumStatisticsEntry ::= SEQUENCE {
	linkAccumStatisticsCardNumber				INTEGER,
	linkAccumStatisticsChannelNumber			INTEGER,
	linkAccumStatisticsMonth				INTEGER,
	linkAccumStatisticsDay					INTEGER,
	linkAccumStatisticsHours				INTEGER,
	linkAccumStatisticsMinutes				INTEGER,
	linkAccumStatisticsSeconds				INTEGER,
	linkAccumStatisticsLinkType				INTEGER,
	linkAccumStatisticsTxFrames				Counter,
	linkAccumStatisticsRxFrames				Counter,
	linkAccumStatisticsLocCompositeUtil			Gauge,
	linkAccumStatisticsRemCompositeUtil			Gauge,
	linkAccumStatisticsLocBufferUtil			Gauge,
	linkAccumStatisticsRemBufferUtil			Gauge,
	linkAccumStatisticsLocRetransmits			Counter,
	linkAccumStatisticsRemRetransmits			Counter,
	linkAccumStatisticsLocLineAlarms			INTEGER,
	linkAccumStatisticsRemLineAlarms			INTEGER,
	linkAccumStatisticsLocTimeInSysFlowCtrl			INTEGER,
	linkAccumStatisticsRemTimeInSysFlowCtrl			INTEGER,
	linkAccumStatisticsLocTimeInSyncLossOrX21Connect	INTEGER,
	linkAccumStatisticsRemTimeInSyncLoss			INTEGER,
	linkAccumStatisticsLocTimeInCarrierLoss			INTEGER,
	linkAccumStatisticsRemTimeInCarrierLoss			INTEGER
	}

linkAccumStatisticsCardNumber OBJECT-TYPE
	SYNTAX	INTEGER (1..5)
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION	"Identifies the module/card number for the 
			link channel specific to the accumulated statistics 
			report data." 
	::= {linkAccumStatisticsEntry 1}

linkAccumStatisticsChannelNumber OBJECT-TYPE
	SYNTAX	INTEGER
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION	"Identifies the link channel number specific to
			 the accumulated statistics report data." 
	::= {linkAccumStatisticsEntry 2}

linkAccumStatisticsMonth OBJECT-TYPE
	SYNTAX	INTEGER (1..12)
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION	"Month portion of the time stamp for the link 
			channel accumulated statistics report." 
	::= {linkAccumStatisticsEntry 3}

linkAccumStatisticsDay OBJECT-TYPE
	SYNTAX	INTEGER (1..31)
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION	"Day portion of the time stamp for the link 
			channel accumulated statistics report.." 
	::= {linkAccumStatisticsEntry 4}

linkAccumStatisticsHours OBJECT-TYPE
	SYNTAX	INTEGER (0..23)
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION	"Hours portion of the time stamp for the link 
			channel accumulated statistics report.." 
	::= {linkAccumStatisticsEntry 5}

linkAccumStatisticsMinutes OBJECT-TYPE
	SYNTAX	INTEGER (0..59)
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION	"Minutes portion of the time stamp for the link 
			channel accumulated statistics report.." 
	::= {linkAccumStatisticsEntry 6}

linkAccumStatisticsSeconds OBJECT-TYPE
	SYNTAX	INTEGER (0..59)
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION	"Seconds portion of the time stamp for the link 
			channel accumulated statistics report.." 
	::= {linkAccumStatisticsEntry 7}

linkAccumStatisticsLinkType OBJECT-TYPE
	SYNTAX	INTEGER {
		esm (1),
		esi (2),
		xDot21 (3),
		localEsi (4),
		esiSecondary (5),
		frameRelay (6)
		}
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION	"Link type of the specified link channel in 
			the accumulated statistics report." 
	::= {linkAccumStatisticsEntry 8}

linkAccumStatisticsTxFrames OBJECT-TYPE
	SYNTAX	Counter
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION	"Transmitted frames (x100) of the specified link
			channel in the accumulated statistics report." 
	::= {linkAccumStatisticsEntry 9}

