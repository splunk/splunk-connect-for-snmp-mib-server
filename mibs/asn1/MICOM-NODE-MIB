	SYNTAX	DisplayString  (SIZE(1..20))
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"Displays the banner string on the LCD keypad." 
	 ::= { messages 14}



-- CARD GROUP

-- N_CMOS.O_MCARDS =======================================================

cardTable OBJECT-TYPE
	SYNTAX	SEQUENCE OF CardEntry
	ACCESS	not-accessible
	STATUS	mandatory
	DESCRIPTION	"Table to view the hardware configuration of the Integration unit." 
	::= {card 1}

cardEntry OBJECT-TYPE
	SYNTAX	CardEntry
	ACCESS	not-accessible
	STATUS	mandatory
	DESCRIPTION	"Entry pertaining to the present module(s)." 
	INDEX		{cardSlotNumber}
	::= {cardTable 1}

CardEntry ::= SEQUENCE {
	cardSlotNumber		INTEGER,
	cardModuleID		INTEGER,
	cardPcbRevision		INTEGER
	}

cardSlotNumber OBJECT-TYPE
	SYNTAX	INTEGER {
		slotA (1),
		slotB (2),
		slotC (3),
		slotD (4),
		slotE (5),
		slotNMS (6)
		}
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION	"Identifies the location of the module within the unit." 
	::= {cardEntry 1}

