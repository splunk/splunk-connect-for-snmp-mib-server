		toVChOnSlotDDVM9 (38),
		toVChOnSlotDDVM10 (39),
		toVChOnSlotDDVM11 (40),
		toVChOnSlotDDVM12 (41)
		}
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"For local t1 interfaces, this parameter defines the connection 
			state of the specified DSO channel. This defines the ability for 
			data passed directly between the Local and Network Interfaces, 
			or for being dropped and inserted to/from the Integration unit 
			resources (CCM links, Data Ports, Voice Channels)."
	::= { t1DSX1InterfaceEntry 2}

-- N_CMOS.O_SYSID ======================================================

t1CfgGroup    OBJECT IDENTIFIER ::= {t1-interface 3}

t1CfgDSX1FrameFormat OBJECT-TYPE
	SYNTAX	INTEGER {
		superFrame (1),
		extendedSuperFrame (2)
		}
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"Configures the local T1 interface frame formats to either (SF)
			super frame or (ESF) extended super frame." 
--	DEFVAL	1
	::= {t1CfgGroup 1}

t1CfgDSX1LineCode OBJECT-TYPE
	SYNTAX	INTEGER {
		ami (1),
		b8zs (2)
		}
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"Configures the line coding method (at the Local interface) either
			to Alternate Mark Inversion (AMI) coding or Bipolar Eight Zero 
			Suppression (B8ZS). In AMI 0's are indicated by the absence of a 
			pulse, and 1's are indicated by either a positive or negative polarity 
			pulse. In B8ZS an all 0 byte is replaced by a special code of 00011011 
			which indicate two fixed bipolar code violations. On remote end these 
			violations are ignored and the data is retrieved." 
--	DEFVAL	1
	::= {t1CfgGroup 2}

t1CfgDSX1IdleCode OBJECT-TYPE
	SYNTAX	OCTET STRING (SIZE(1))
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"Configures the idle code character at the local T1 interface." 
--	DEFVAL	07F
	::= {t1CfgGroup 3}

t1CfgDSX1lineBuildout OBJECT-TYPE
	SYNTAX	INTEGER {
		lessThan110Feet (1),
		lessThan220Feet (2),
		lessThan330Feet (3),
		lessThan440Feet (4),
		externalLBO (5),
		greaterThan665Feet (6),
		lessThan550Feet (7),
		lessThan660Feet (8)
		}
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"Configures a transmitter attenuation parameter based on various 
			cable lengths, reflecting the distance from the T1 Access Module 
			to the T1 service provider." 
-- 	DEFVAL	1
	::= {t1CfgGroup 4}

t1CfgDSX1NetworkInvokedLoopback OBJECT-TYPE
	SYNTAX	INTEGER {
		enable (1),
		disable (2)
		}
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"Enables/Disables the DSX1 Network-invoked loopback on the T1 
			module." 
--	DEFVAL	1
	::= {t1CfgGroup 5}

t1CfgDS1FrameFormat OBJECT-TYPE
	SYNTAX	INTEGER {
		superFrame (1),
		extendedSuperFrame (2)
		}
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"Configures the network T1 interface frame formats to either (SF)
			super frame or (ESF) extended super frame." 
--	DEFVAL	1
	::= {t1CfgGroup 6}

t1CfgDS1LineCode OBJECT-TYPE
	SYNTAX	INTEGER {
		ami (1),
		b8zs (2)
		}
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"Configures the line coding method (at the Network interface) either
			to Alternate Mark Inversion (AMI) coding or Bipolar Eight Zero 
			Suppression (B8ZS). In AMI 0's are indicated by the absence of a 
			pulse, and 1's are indicated by either a positive or negative polarity 
			pulse. In B8ZS an all 0 byte is replaced by a special code of 00011011 
			which indicate two fixed bipolar code violations. On remote end these 
			violations are ignored and the data is retrieved."  
--	DEFVAL	1
	::= {t1CfgGroup 7}

t1CfgDS1IdleCode OBJECT-TYPE
	SYNTAX	OCTET STRING (SIZE(1))
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"Configures the idle code character at the network T1 interface." 
--	DEFVAL	07F
	::= {t1CfgGroup 8}

t1CfgDS1NetworkInvokedLoopback OBJECT-TYPE
	SYNTAX	INTEGER {
		enable (1),
		disable (2)
		}
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"Enables/Disables the DS1 Network-invoked loopback on the T1 
			module." 
	::= {t1CfgGroup 9}

t1CfgDataPort1Rate OBJECT-TYPE
	SYNTAX	INTEGER {
		multipleOf64kbps (1),
		multipleOf56kbps (2)
		}
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"Sets the base rate of Data Port channel 1 on the T1 module.  
			The actual rate will be determined by the following formula:
			rate of the data port = (base rate)*(number of DS0 channels)." 
--	DEFVAL	1
	::= {t1CfgGroup 10}

t1CfgDataPort1Timing OBJECT-TYPE
	SYNTAX	INTEGER {
		internallySuppliedNormal (1),
		tailCircuit (2)
		}
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"Sets how the clocking will be supplied for Data Port 1 on the
			T1 module." 
--	DEFVAL	1
	::= {t1CfgGroup 11}

t1CfgDataPort1DTRControl OBJECT-TYPE
	SYNTAX	INTEGER {
		dtrNormal (1),
		dtrForced (2)
		}
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"Sets the DTR control for Data Port 1 on the T1 module.  Option (1),
			dtrNormal, will configure DTR control from the DTE equipment.  
			Option (2), dtrForced, will force DTR to a '1'." 
--	DEFVAL	1
	::= {t1CfgGroup 12}

t1CfgDataPort1RTSControl OBJECT-TYPE
	SYNTAX	INTEGER {
		rtsNormal (1),
		rtsForced (2)
		}
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"Sets the RTS control for Data Port 1 on the T1 module.  Option (1),
			rtsNormal, will configure RTS control from the DTE equipment.  
			Option (2), rtsForced, will force RTS to a '1'." 
--	DEFVAL	1
	::= {t1CfgGroup 13}

t1CfgDataPort2Rate OBJECT-TYPE
	SYNTAX	INTEGER {
		multipleOf64kbps (1),
		multipleOf56kbps (2)
		}
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"Sets the base rate of Data Port channel 2 on the T1 module.
			The actual rate will be determined by the following formula:
			rate of the data port = (base rate)*(number of DS0 channels)." 
--	DEFVAL	1
	::= {t1CfgGroup 14}

t1CfgDataPort2Timing OBJECT-TYPE
	SYNTAX	INTEGER {
		internallySupplied (1),
