-- N_STATUS.O_CHCNST =======================================================

chConnectionStatusTable OBJECT-TYPE
	SYNTAX	SEQUENCE OF ChConnectionStatusEntry
	ACCESS	not-accessible
	STATUS	mandatory
	DESCRIPTION	"Table of channel connection status indications." 
	 ::= {status 7}

chConnectionStatusEntry OBJECT-TYPE
	SYNTAX	ChConnectionStatusEntry
	ACCESS	not-accessible
	STATUS	mandatory
	DESCRIPTION	"Entry within the table specific to a particular channel number
			on a module/card for connection status indications." 
	INDEX		{chConnectionStatusCardNumber, chConnectionStatusChannelNumber}
	 ::= {chConnectionStatusTable 1}

ChConnectionStatusEntry ::= SEQUENCE {
	chConnectionStatusCardNumber			INTEGER,
	chConnectionStatusChannelNumber		INTEGER,
	chConnectionStatusChannelStatus			INTEGER,
	chConnectionStatusConnectedToNode		INTEGER,
	chConnectionStatusConnectedToChannel		INTEGER,
	chConnectionStatusConnectedToCard		INTEGER,
	chConnectionStatusConnectedToInternalFacility	INTEGER
	}

chConnectionStatusCardNumber	 OBJECT-TYPE
	SYNTAX	INTEGER (1..5)
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION	"Identifies the module/card number of the channel for connection
			status indication." 
	  ::= {chConnectionStatusEntry 1}

chConnectionStatusChannelNumber OBJECT-TYPE
	SYNTAX	INTEGER
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION	"Identifies the channel number for connection status indication." 
	  ::= {chConnectionStatusEntry 2}

chConnectionStatusChannelStatus OBJECT-TYPE
	SYNTAX	INTEGER {
		idle (1),
		connectInProgress (2),
		fconnInProgress (3),
		queued (4),
		connected (5),
 		forceConnected (6),
		disconnectInProgress (7),
		outOfService (8)
		}
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION	"Indicates the connection status of the channel." 
	  ::= {chConnectionStatusEntry 3}

chConnectionStatusConnectedToNode OBJECT-TYPE
	SYNTAX	INTEGER 
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION	"Indicates the node ID # of the destination channel connection, if any." 
	  ::= {chConnectionStatusEntry 4}

chConnectionStatusConnectedToChannel OBJECT-TYPE
	SYNTAX	INTEGER
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION	"Indicates the destination channel number of the connection, if any." 
	  ::= {chConnectionStatusEntry 5}

chConnectionStatusConnectedToCard OBJECT-TYPE
	SYNTAX	INTEGER
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION	"Indicates the destination module/card number of the connection, if any." 
	  ::= {chConnectionStatusEntry 6}

chConnectionStatusConnectedToInternalFacility OBJECT-TYPE
	SYNTAX	INTEGER {
		notApplicable (1),
		dollarCMDFacility (2),
		dollarPPPFacility (3)
		}
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION	"Indicates a channel connection to an internal facility ($CMD or $PPP),
			if any." 
	  ::= {chConnectionStatusEntry 7}

