	SYNTAX	INTEGER {
		enable (1),
		disable (2)
	}
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"Controls the external alarm relay on an NMS module." 
	::= {trap 5}


-- Trap identification group

trapNodeName OBJECT-TYPE
	SYNTAX	DisplayString
	ACCESS	not-accessible
	STATUS	mandatory
	DESCRIPTION	"Identifies the name of the node that generated the trap." 
	::= {trap 6}

trapCardNumber OBJECT-TYPE
	SYNTAX	INTEGER
	ACCESS	not-accessible
	STATUS	mandatory
	DESCRIPTION	"Identifies the module/card number responsible for generating
			the trap." 
	::= {trap 7}

trapChannelNumber OBJECT-TYPE
	SYNTAX	INTEGER
	ACCESS	not-accessible
	STATUS	mandatory
	DESCRIPTION	"Identifies the port/channel number responsible for generating
			the trap." 
	::= {trap 8}

trapFirstNodeName OBJECT-TYPE
	SYNTAX	DisplayString
	ACCESS	not-accessible
	STATUS	mandatory
	DESCRIPTION	"Identifies the source node name of the trap." 
	::= {trap 9}

trapFirstCardNumberOrInternalFacilityIndication OBJECT-TYPE
	SYNTAX	INTEGER
	ACCESS	not-accessible
	STATUS	mandatory
	DESCRIPTION	"This identifies the card number that has generated the trap.
			A value of  0FFh siginifies a special condition which identifies that the
			trapChannelNumberOrInternalFacility object in the varBind contains
			the type of internal facility." 
	::= {trap 10}

trapFirstChannelNumberOrInternalFacility OBJECT-TYPE
	SYNTAX	INTEGER
	ACCESS	not-accessible
	STATUS	mandatory
	DESCRIPTION	"This identifies the channel number that has generated the trap. If the
			trapCardNumberOrInternalFacilityIndication object has a value of
			0FFh then this objects identifies the internal facilities as $CMD if value
			is 0, $PPP if the value is 1, and $DLD if the value is 2."
	::= {trap 11}

trapSecondNodeName OBJECT-TYPE
	SYNTAX	DisplayString
	ACCESS	not-accessible
	STATUS	mandatory
	DESCRIPTION	"Identifies the destination node name of the trap." 
	::= {trap 12}

trapSecondCardNumberOrInternalFacilityIndication OBJECT-TYPE
	SYNTAX	INTEGER (1..5)
	ACCESS	not-accessible
	STATUS	mandatory
	DESCRIPTION	"This identifies the second card number that has generated the trap.
			A value of  0FFh siginifies a special condition which identifies that the
			trapChannelNumberOrInternalFacility object in the varBind contains
			the type of internal facility."  
	::= {trap 13}

trapSecondChannelNumberOrInternalFacility OBJECT-TYPE
	SYNTAX	INTEGER
	ACCESS	not-accessible
	STATUS	mandatory
	DESCRIPTION	"This identifies the second channel number that has generated the trap.
			If the trapCardNumberOrInternalFacilityIndication object has a value of
			0ffh then this objects identifies the internal facilities as $CMD if
			value is 0, $PPP if the value is 1, and $DLD if the value is 2."
 
	::= {trap 14}

trapClassRequestedOrMatrixRequest OBJECT-TYPE
	SYNTAX	INTEGER 
	ACCESS	not-accessible
	STATUS	mandatory
	DESCRIPTION	"This identifies either the requested class number if the values are 
			between the allowable class number range (i.e. 1 to 64) or the matrix 
			request if the object value is 0." 
	::= {trap 15}

trapLinkType OBJECT-TYPE
	SYNTAX	INTEGER {
		physicalLink (1),
		frameRelayDLCI (2)
		}
	ACCESS	not-accessible
	STATUS	mandatory
	DESCRIPTION	"Identifies the Link type associated with the trap." 
	::= {trap 16}

trapDLCINumber OBJECT-TYPE
	SYNTAX	INTEGER
	ACCESS	not-accessible
	STATUS	mandatory
	DESCRIPTION	"Identifies the DLCI number associated with the trap corresponding
			to a Frame Relay link." 
	::= {trap 17}

trapDLCIFirstLinkCardNumber OBJECT-TYPE
	SYNTAX	INTEGER
	ACCESS	not-accessible
	STATUS	mandatory
	DESCRIPTION	"Identifies the module/card number associated with the trap
			corresponding to a DLCI channel of the source Frame Relay link." 
	::= {trap 18}

trapDLCIFirstLinkChannelNumber OBJECT-TYPE
	SYNTAX	INTEGER
	ACCESS	not-accessible
	STATUS	mandatory
	DESCRIPTION	"Identifies the port/channel number associated with the trap
			corresponding to a DLCI channel of the source Frame Relay link." 
	::= {trap 19}

trapDLCIFirstLinkDLCINumber OBJECT-TYPE
	SYNTAX	INTEGER
	ACCESS	not-accessible
	STATUS	mandatory
	DESCRIPTION	"Identifies the DLCI channel number associated with the trap
			corresponding to the source Frame Relay link." 
	::= {trap 20}

trapDLCISecondLinkCardNumber OBJECT-TYPE
	SYNTAX	INTEGER
	ACCESS	not-accessible
	STATUS	mandatory
	DESCRIPTION	"Identifies the port/channel number associated with the trap
			corresponding to a DLCI channel of the destination Frame Relay link." 
	::= {trap 21}

trapDLCISecondLinkChannelNumber OBJECT-TYPE
	SYNTAX	INTEGER
	ACCESS	not-accessible
	STATUS	mandatory
	DESCRIPTION	"Identifies the port/channel number associated with the trap
			corresponding to a DLCI channel of the destination Frame Relay link." 
	::= {trap 22}

trapDLCISecondLinkDLCINumber OBJECT-TYPE
	SYNTAX	INTEGER
	ACCESS	not-accessible
	STATUS	mandatory
	DESCRIPTION	"Identifies the DLCI channel number associated with the trap
			corresponding to the destination Frame Relay link." 
	::= {trap 23}

-- Marathon Trap Messages

mcmEventConnect TRAP-TYPE
	ENTERPRISE	micom
	VARIABLES	{trapFirstNodeName, trapFirstCardNumberOrInternalFacilityIndication, 
			trapFirstChannelNumberOrInternalFacility, trapSecondNodeName,
			trapSecondCardNumberOrInternalFacilityIndication, 
			trapSecondChannelNumberOrInternalFacility}
	DESCRIPTION	"A connection has been established between the specified
				pair of unit ports."
	::= 1

mcmEventDisconnect TRAP-TYPE
	ENTERPRISE	micom
	VARIABLES	{trapFirstNodeName, trapFirstCardNumberOrInternalFacilityIndication, 
			trapFirstChannelNumberOrInternalFacility, trapSecondNodeName,
			trapSecondCardNumberOrInternalFacilityIndication, 
			trapSecondChannelNumberOrInternalFacility}
	DESCRIPTION	"The connection between the specified pair of unit ports
				has been broken."
	::= 2

mcmEventQueueEntry TRAP-TYPE
	ENTERPRISE	micom
	VARIABLES	{trapNodeName, trapChannelNumber, trapCardNumber,
			 trapClassRequestedOrMatrixRequest}
	DESCRIPTION	"A port has been placed in a first-come first-served queue
				for the next available port in the requested class."
	::= 3

mcmEventCallFail TRAP-TYPE
	ENTERPRISE	micom
	VARIABLES	{trapNodeName, trapCardNumber, trapChannelNumber, 
			 trapClassRequestedOrMatrixRequest }
	DESCRIPTION	"A matrix connection made by the local port did not complete
				successfully (for example, the password was incorrect)."
	::= 4

mcmEventDDSInService TRAP-TYPE
	ENTERPRISE	micom
	VARIABLES	{trapNodeName, trapCardNumber, trapChannelNumber}
	DESCRIPTION	"The unit's ISU module is back in service, and the associated
				link is up."
	::= 5

mcmEventDDSOutService TRAP-TYPE
	ENTERPRISE	micom
	VARIABLES	{trapNodeName, trapCardNumber, trapChannelNumber}
	DESCRIPTION	"The unit's ISU module is out of service, and the associated
				link is down."
	::= 6

mcmEventDDSLoopTest TRAP-TYPE
	ENTERPRISE	micom
	VARIABLES	{trapNodeName, trapCardNumber, trapChannelNumber}
	DESCRIPTION	"The telephone company has placed the unit's ISU module
				into a loop test mode."
	::= 7

mcmEventADBConnectMade TRAP-TYPE
	ENTERPRISE	micom
	VARIABLES	{trapNodeName, trapCardNumber, trapChannelNumber}
	DESCRIPTION	"The unit's modem module has completed a dial backup
				connection, and the associated link is up."
	::= 8

mcmEventADBConnectDrop TRAP-TYPE
	ENTERPRISE	micom
	VARIABLES	{trapNodeName, trapCardNumber, trapChannelNumber}
	DESCRIPTION	"The unit's modem module has dropped the dial line
				connection due to bad lines, Data Terminal Ready
				dropped, or Carrier Detect lost.  As a result, the
				associated link is down."
	::= 9

mcmEventForcedDialMode TRAP-TYPE
	ENTERPRISE	micom
	VARIABLES	{trapNodeName, trapCardNumber, trapChannelNumber}
	DESCRIPTION	"The unit's modem module has been placed into the forced
				dial mode.  The associated link will always be established
				over the dial lines."
	::= 10

mcmEventForcedLeaseMode TRAP-TYPE
	ENTERPRISE	micom
	VARIABLES	{trapNodeName, trapCardNumber, trapChannelNumber}
	DESCRIPTION	"The unit's modem module has been placed into the forced
				lease mode.  The associated link will always be established
				over the lease lines; no dial backup can occur."
	::= 11

mcmEventLeasedConnect TRAP-TYPE
	ENTERPRISE	micom
	VARIABLES	{trapNodeName, trapCardNumber, trapChannelNumber}
	DESCRIPTION	"The unit's modem module has established communications
				over the lease lines, and the associated link is up."
	::= 12

mcmEventLeaseLineRetry TRAP-TYPE
	ENTERPRISE	micom
	VARIABLES	{trapNodeName, trapCardNumber, trapChannelNumber}
	DESCRIPTION	"The unit's modem module has started operating on the
				lease lines while the dial line connection is still
				established."
	::= 13

mcmEventLinkRestart TRAP-TYPE
	ENTERPRISE	micom
	VARIABLES	{trapNodeName, trapCardNumber, trapChannelNumber, 
			trapLinkType, trapDLCINumber}
	DESCRIPTION	"Communications on the local side of the associated link
				have been restarted."
	::= 14

mcmEventRemoteLinkRestart TRAP-TYPE
	ENTERPRISE	micom
	VARIABLES	{trapNodeName, trapCardNumber, trapChannelNumber, 
			trapLinkType, trapDLCINumber}
	DESCRIPTION	"Communications on the remote end of the specified link
				have been restarted."
	::= 15

mcmEventNoADBNumber TRAP-TYPE
	ENTERPRISE	micom
	VARIABLES	{trapNodeName, trapCardNumber, trapChannelNumber}
	DESCRIPTION	"No number has been configured for auto dial backup
				for the specified link."
	::= 16

