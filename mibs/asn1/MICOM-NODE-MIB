	SYNTAX	VoiceFlashEPROMEntry
	ACCESS	not-accessible
	STATUS	mandatory
	DESCRIPTION	"Entry within the table specific to a particular voice channel
			on a particular module/card for erasing flash memory." 
	INDEX		{voiceFlashEPROMCardNumber, voiceFlashEPROMChannelNumber}
	::= {voiceFlashEPROMTable 1}

VoiceFlashEPROMEntry ::= SEQUENCE {
	voiceFlashEPROMCardNumber		INTEGER,
	voiceFlashEPROMChannelNumber	INTEGER,
	voiceFlashEPROMCommand		INTEGER
	}

