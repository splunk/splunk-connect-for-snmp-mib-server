    --
    -- Event Group
    --
    -- This group contains objects that are specifically for the
    -- purpose of defining one or more traps.
    --
    -- Implementation of this group is optional.
    --
    dptScsiEventInfo OBJECT-TYPE
       SYNTAX           DisplayString (SIZE (0..256))
       ACCESS           read-only
       STATUS           mandatory
       DESCRIPTION
         "Represents information in a human readable form
          (English string) for generic DPT SCSI system traps."
       ::= { dptScsiEvent 1 }

    dptScsiEventAuxVoltage OBJECT-TYPE
       SYNTAX           LowHighStatus
       ACCESS           not-accessible
       STATUS           mandatory
       DESCRIPTION
          "Auxiliary voltage status reported by the controller."
       ::= { dptScsiEvent 2 }




    --
    -- DPT enterprise trap definitions
    -- The trap notation uses the Novell description standard
    --
    -- Traps are grouped into the following severity levels:
    -- Severity         trap range
    -- Unknown          0..99
    -- Informational    100..199
    -- Minor            200..299
    -- Major            300..399
    -- Critical         400..499
    --

    dptHbaTemperatureVeryHighTrap TRAP-TYPE
       ENTERPRISE       dpt
       VARIABLES {
                 dptScsiHbaTemperature,
                 dptScsiHbaNumber
                 }
       DESCRIPTION
          "Very high temperature detected on controller."
    --Internal events: 0x18
    --#TYPE "Very high temperature on controller"
    --#SUMMARY "Very high temperature on controller %d"
    --#ARGUMENTS {1}
    --#SEVERITY CRITICAL
    --#TIMEINDEX 99
    --#STATE OPERATIONAL
       ::= 400

    dptArrayStatusChangeFailedTrap TRAP-TYPE
       ENTERPRISE       dpt
       VARIABLES {
                 dptScsiDevStatus,
                 dptScsiHbaNumber,
                 dptScsiBusNumber,
                 dptScsiDevId,
                 dptScsiDevLun,
                 dptScsiDevLevel
                 }
       DESCRIPTION
          "Array Failed due to multiple drive failures."
    --Internal events: 0x0B w/ sub-codes in event.
    --#TYPE "Array failed"
    --#SUMMARY "Array failed on Controller %d Bus %d ID %d LUN %d."
    --#ARGUMENTS {1,2,3,4}
    --#SEVERITY CRITICAL
    --#TIMEINDEX 99
    --#STATE NONOPERATIONAL
       ::= 401

    dptHbaVoltageChangeTrap TRAP-TYPE
       ENTERPRISE       dpt
       VARIABLES {
                 dptScsiHbaVoltage,
                 dptScsiHbaNumber
                 }
       DESCRIPTION
          "Controller detected 5V voltage is outside of the normal range."
    --Internal events: 0x13, 0x15
    --#TYPE "5V Voltage outside range on controller"
    --#SUMMARY "5V Voltage status is now %d on controller %d"
    --#ARGUMENTS {0,1}
    --#SEVERITY MAJOR
    --#TIMEINDEX 99
    --#STATE OPERATIONAL
       ::= 300

    dptHbaTemperatureHighTrap TRAP-TYPE
       ENTERPRISE       dpt
       VARIABLES {
                 dptScsiHbaTemperature,
                 dptScsiHbaNumber
                 }
       DESCRIPTION
          "High temperature detected on controller."
    --Internal events: 0x17
    --#TYPE "High temperature on controller"
    --#SUMMARY "High temperature on controller %d"
    --#ARGUMENTS {1}
    --#SEVERITY MINOR
    --#TIMEINDEX 99
    --#STATE OPERATIONAL
       ::= 301

    dptHbaEccRAMErrorUncorrectableTrap TRAP-TYPE
       ENTERPRISE       dpt
       VARIABLES {
                 dptScsiHbaBadMemoryAddress,
                 dptScsiHbaNumber
                 }
       DESCRIPTION
          "The controller detected and uncorrectable ECC RAM error.
           dptScsiHbaBadMemoryAddress contains the RAM address."
    --Internal events: 0x3007
    --#TYPE "Controller detected an ECC RAM error that could not be corrected"
    --#SUMMARY "Uncorrectable ECC error found on controller %d"
    --#ARGUMENTS {1}
    --#SEVERITY MAJOR
    --#TIMEINDEX 99
    --#STATE NONOPERATIONAL
       ::= 302

    dptArrayStatusChangeDegradedTrap TRAP-TYPE
       ENTERPRISE       dpt
       VARIABLES {
                 dptScsiDevStatus,
                 dptScsiHbaNumber,
                 dptScsiBusNumber,
                 dptScsiDevId,
                 dptScsiDevLun,
                 dptScsiDevLevel
                 }
       DESCRIPTION
          "Array Degraded due to drive failure."
    --Internal events: 0x0B w/ sub-codes in event.
    --#TYPE "Array degraded"
    --#SUMMARY "Array on Controller %d Bus %d ID %d LUN %d running degraded."
    --#ARGUMENTS {1,2,3,4}
    --#SEVERITY MAJOR
    --#TIMEINDEX 99
    --#STATE OPERATIONAL
       ::= 303

    dptDevReqSenseSeriousErrorTrap TRAP-TYPE
       ENTERPRISE       dpt
       VARIABLES {
                 dptScsiDevLastReqSenseInfo,
                 dptScsiHbaNumber,
                 dptScsiBusNumber,
                 dptScsiDevId,
                 dptScsiDevLun,
                 dptScsiDevLevel
                 }
       DESCRIPTION
          "Serious: request sense information from controller."
    --Internal events: 0x3004
    --#TYPE "Serious: Request sense data received."
    --#SUMMARY "Serious: Request sense data for controller %d Bus %d ID %d LUN %d."
    --#ARGUMENTS {1,2,3,4}
    --#SEVERITY MAJOR
    --#TIMEINDEX 99
    --#STATE NONOPERATIONAL
       ::= 304

    dptDevArrayDataInconsistencyTrap TRAP-TYPE
       ENTERPRISE       dpt
       VARIABLES {
                 dptScsiDevBadBlockNumber,
                 dptScsiDevBadBlockCount,
                 dptScsiHbaNumber,
                 dptScsiBusNumber,
                 dptScsiDevId,
                 dptScsiDevLun,
                 dptScsiDevLevel
                 }
       DESCRIPTION
          "Array Data consistency error detected on controller."
    --Internal events: 0x300C
    --#TYPE "Array data inconsistency found."
    --#SUMMARY "Array Data Inconsistency: Controller %d Bus %d ID %d LUN %d."
    --#ARGUMENTS {2,3,4,5}
    --#SEVERITY MAJOR
    --#TIMEINDEX 99
    --#STATE DEGRADED
       ::= 305

    dptHbaErrorTrap TRAP-TYPE
       ENTERPRISE       dpt
       VARIABLES {
                 dptScsiHbaNumber
                 }
       DESCRIPTION
          "The controller has detected a non-SCSI related error, as defined in the I2O spec."
    --Internal events: 0x3005
    --#TYPE "controller error found."
    --#SUMMARY "controller %d reported a non-SCSI related error."
    --#ARGUMENTS {0}
    --#SEVERITY MAJOR
    --#TIMEINDEX 99
    --#STATE DEGRADED
       ::= 306

