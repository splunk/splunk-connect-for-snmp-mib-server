    dptOtherTrap TRAP-TYPE
       ENTERPRISE       dpt
       VARIABLES {
                 dptScsiEventInfo
                 }
       DESCRIPTION
          "Controller event generated with a value dptScsiEventInfo."
    --Internal events: all remaining codes
    --#TYPE "Controller generated an event"
    --#SUMMARY "Controller generated event: %s"
    --#ARGUMENTS {0}
    --#SEVERITY INFORMATIONAL
    --#TIMEINDEX 99
    --#STATE OPERATIONAL
       ::= 14


