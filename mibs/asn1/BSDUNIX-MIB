-- the group/user table

grUserTable OBJECT-TYPE
        SYNTAX     SEQUENCE OF GrUserEntry
        MAX-ACCESS not-accessible
        STATUS     obsolete
        DESCRIPTION
                "(former) group/user table."
        ::= { users 3 }

grUserEntry OBJECT-TYPE
        SYNTAX     GrUserEntry
        MAX-ACCESS not-accessible
        STATUS     obsolete
        DESCRIPTION
                "An entry in the group/user table."
        INDEX      { groupName, grUserZero, IMPLIED userName }
        ::= { grUserTable 1 }

GrUserEntry ::=
    SEQUENCE {
        grUserStatus
            INTEGER
    }

grUserStatus OBJECT-TYPE
        SYNTAX     INTEGER {
                    other(1),
                    invalid(2)
                }
        MAX-ACCESS not-accessible
        STATUS     obsolete
        DESCRIPTION
                "The type of relationship between a user and a group."
        ::= { grUserEntry 1 }

grUserZero OBJECT-TYPE
        SYNTAX     INTEGER (0..0)
        MAX-ACCESS not-accessible
        STATUS     obsolete
        DESCRIPTION
                "(former) moby hack."
        ::= { users 4 }

grNewUserTable OBJECT-TYPE
        SYNTAX     SEQUENCE OF GrNewUserEntry
        MAX-ACCESS not-accessible
        STATUS     current
        DESCRIPTION
                "The group/user table."
        ::= { users 5 }

grNewUserEntry OBJECT-TYPE
        SYNTAX     GrNewUserEntry
        MAX-ACCESS not-accessible
        STATUS     current
        DESCRIPTION
                "An entry in the group/user table."
        INDEX      { groupName, IMPLIED userName }
        ::= { grNewUserTable 1 }

GrNewUserEntry ::=
    SEQUENCE {
        grNewUserStatus
            INTEGER
    }

grNewUserStatus OBJECT-TYPE
        SYNTAX     INTEGER {
                    other(1),
                    invalid(2)
                }
        MAX-ACCESS read-write
        STATUS     current
        DESCRIPTION
                "The type of relationship between a user and a group.

                 Setting this object to the value invalid(2) has the effect
                 of invaliding the corresponding entry in the grUserTable.  It
                 is an implementation-specific matter as to whether the agent
                 removes an invalidated entry from the table.  Accordingly,
                 management stations must be prepared to receive tabular
                 information from agents that correspond to entries not
                 currently in use.  Proper interpretation of such entries
                 requires examination of the relative grUserStatus object."
        ::= { grNewUserEntry 1 }


-- unix file-system group

unixFs OBJECT IDENTIFIER ::= { unix 8 }


-- the file-system table

fsTable OBJECT-TYPE
        SYNTAX     SEQUENCE OF FsTableEntry
        MAX-ACCESS not-accessible
        STATUS     current
        DESCRIPTION
                "The file system table"
        ::= { unixFs 1 }

fsTableEntry OBJECT-TYPE
        SYNTAX     FsTableEntry
        MAX-ACCESS not-accessible
        STATUS     current
        DESCRIPTION
                "A row in the file system table"
        INDEX      { fsIdentifier }
        ::= { fsTable 1 }

FsTableEntry ::=
    SEQUENCE { 
        fsIdentifier
            Integer32,
        fsName
            DisplayString,
        fsMountPoint
            DisplayString,
        fsMountType
            DisplayString,
        fsMountOptions
            DisplayString,
        fsBlockSize
            INTEGER (0..2147483647),
        fsBlockCount
            INTEGER (0..2147483647),
        fsBlocksFree
            INTEGER (0..2147483647),
        fsBlocksAvailable
            INTEGER (0..2147483647),
        fsInodeCount
            INTEGER (0..2147483647),
        fsInodesAvailable
            INTEGER (0..2147483647)
    }

fsIdentifier OBJECT-TYPE
        SYNTAX     Integer32
        MAX-ACCESS read-only
        STATUS     current
        DESCRIPTION
                "A system-dependent unique identifier."
        ::= { fsTableEntry 1 }

fsName OBJECT-TYPE
        SYNTAX     DisplayString
        MAX-ACCESS read-only
        STATUS     current
        DESCRIPTION
                "The name of the file system (device)."
        ::= { fsTableEntry 2 }

fsMountPoint OBJECT-TYPE
        SYNTAX     DisplayString
        MAX-ACCESS read-only
        STATUS     current
        DESCRIPTION
                "Mount point for the file system."
        ::= { fsTableEntry 3 }

fsMountType OBJECT-TYPE
        SYNTAX     DisplayString
        MAX-ACCESS read-only
        STATUS     current
        DESCRIPTION
                "The type of the mounted file system."
        ::= { fsTableEntry 4 }

fsMountOptions OBJECT-TYPE
        SYNTAX     DisplayString
        MAX-ACCESS read-only
        STATUS     current
        DESCRIPTION
                "File system specific options."
        ::= { fsTableEntry 5 }

fsBlockSize OBJECT-TYPE
        SYNTAX     INTEGER (0..2147483647)
        MAX-ACCESS read-only
        STATUS     current
        DESCRIPTION
                "Block size for this file system."
        ::= { fsTableEntry 6 }

fsBlockCount OBJECT-TYPE
        SYNTAX     INTEGER (0..2147483647)
        MAX-ACCESS read-only
        STATUS     current
        DESCRIPTION
                "Total number on blocks in file system."
        ::= { fsTableEntry 7 }

fsBlocksFree OBJECT-TYPE
        SYNTAX     INTEGER (0..2147483647)
        MAX-ACCESS read-only
        STATUS     current
        DESCRIPTION
                "Total number of blocks free in file system."
        ::= { fsTableEntry 8 }

fsBlocksAvailable OBJECT-TYPE
        SYNTAX     INTEGER (0..2147483647)
        MAX-ACCESS read-only
        STATUS     current
        DESCRIPTION
                "Number of blocks available to non-privileged users."
        ::= { fsTableEntry 9 }

fsInodeCount OBJECT-TYPE
        SYNTAX     INTEGER (0..2147483647)
        MAX-ACCESS read-only
        STATUS     current
        DESCRIPTION
                "Total number of i-nodes on this file system."
        ::= { fsTableEntry 10 }

fsInodesAvailable OBJECT-TYPE
        SYNTAX     INTEGER (0..2147483647)
        MAX-ACCESS read-only
        STATUS     current
        DESCRIPTION
                "Number of i-nodes available on file system."
        ::= { fsTableEntry 11 }


