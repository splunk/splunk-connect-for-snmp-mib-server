-- the Trace group


bcamTraceSavingState OBJECT-TYPE
  SYNTAX INTEGER {
-- ASSMULTI:
--                 noSaving(0),   ASSVAL: 0 0
		 initiated(1), -- ASSVAL: YTCSAVIN 1
		 active(2),    -- ASSVAL: YTCSAVAC 2
		 shutting(4),  -- ASSVAL: YTCTSHUT 4
		 passive(8),   -- ASSVAL: YTCSAVPA 40
		 waiting(16),  -- ASSVAL: YTCSAVWT 80
		 noSaving(32)  -- ASSVAL: 0 0
		 }
  ACCESS read-only
  STATUS mandatory
  DESCRIPTION
    " Describes if any of the running BCAM traces are saved to a file.
		 noSaving (32) : no trace will be written
		 initiated (1) : the trace saving task has been created
		 active    (2) : the trace saving task is active
		 shutting  (4) : the trace saving task is terminating
		 passive   (8) : the trace file has been closed
		 waiting  (16) : the trace saving task is waiting for trace data to write
      Several bits may be set simultaneous."
  ::= { bcamTrace 1 }
-- SNMPNAME: TSST
-- ASSNAME: YTCTFLAG
-- implemented

bcamTraceFilename OBJECT-TYPE
  SYNTAX DisplayString (SIZE(54)) -- ASSLEN: 55 VAR
  ACCESS read-only
  STATUS mandatory
  DESCRIPTION
    " The name of the file currently used for saving BCAM traces."
  ::= { bcamTrace 2 }
-- SNMPNAME: TSFN
-- ASSNAME: YTCTFPAT
-- implemented

bcamTraceMaxFilesize OBJECT-TYPE
  SYNTAX INTEGER
  ACCESS read-only
  STATUS mandatory
  DESCRIPTION
    " The maximum size of one file used for saving BCAM traces in bytes. "
  ::= { bcamTrace 3 }
-- SNMPNAME: TSFS
-- ASSNAME: YTCTFSIZ
-- implemented

bcamTraceNumberFiles OBJECT-TYPE
  SYNTAX INTEGER
  ACCESS read-only
  STATUS mandatory
  DESCRIPTION
    " The number of files used for saving BCAM traces. "
  ::= { bcamTrace 4 }
-- SNMPNAME: TSNF
-- ASSNAME: YTCTF#
-- implemented

bcamTraceAdmState OBJECT-TYPE
  SYNTAX INTEGER {
-- ASSMULTI:
--                 stopped(0),  ASSVAL: 0 0
		 running(1), -- ASSVAL: YTETALLO 1
		 save(2),    -- ASSVAL: YTESAVRQ 2
		 hold(4),    -- ASSVAL: YTEHLDRQ 4
		 stopped(8)  -- ASSVAL: 0 0
		 }
  ACCESS read-only
  STATUS mandatory
  DESCRIPTION
    " The current state of the trace.
      stopped (8) : the trace is not running.
      running (1) : the trace is running.
      save    (2) : the trace is running and saved to a file.
      hold    (4) : the trace data will be kept in case of an error.
      Several bits may be set simultaneous."
  ::= { bcamTrace 5 }
-- SNMPNAME: TADS
-- ASSNAME: YTETFLAG
-- implemented

bcamTraceAdmNumberBuffer OBJECT-TYPE
  SYNTAX INTEGER (1..16) -- ASSLEN: 2
  ACCESS read-only
  STATUS mandatory
  DESCRIPTION
    " The number of buffers used for the trace"
  ::= { bcamTrace 6 }
-- SNMPNAME: TADB
-- ASSNAME: YTEACBFN
-- implemented

