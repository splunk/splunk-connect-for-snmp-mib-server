bcamRouteTabCommandState OBJECT-TYPE
  SYNTAX INTEGER {
-- ASSMULTI:
-- ASSVAL: 0 means excluded
		 included(1), -- ASSVAL: YRROSINC 16
		 active(2)    -- ASSVAL: YRROSACT 128
		 }
  ACCESS read-only
  STATUS mandatory
  DESCRIPTION
    " The current state of this route caused by route specific commands.
      included   (1)  : a BCIN command was given successfully for this route.
      active     (2)  : a BCACT command was given successfully for this route.
      Several bits may be set simultaneous."
  ::= { bcamRouteTabEntry 33 }
-- SNMPNAME: RCST
-- ASSNAME: YRROSTAT
-- implemented

bcamRouteTabChangeState OBJECT-TYPE
  SYNTAX INTEGER {
-- ASSMULTI:
--                 generated(0),         ASSVAL: YRROCHGE 0
		 dynamic(1),          -- ASSVAL: YRROCHDY 1
		 defByProtocol(2),    -- ASSVAL: YRROCHRP 2
		 switchByProtocol(4), -- ASSVAL: YRROCHSW 4
		 changed(8),          -- ASSVAL: YRROCHCH 128
		 generated(16)        -- ASSVAL: YRROCHGE 0
		 }
  ACCESS read-only
  STATUS mandatory
  DESCRIPTION
    " Describes how the route was defined or modified.
      generated        (16) : the route has been defined by KOGS and not been modified.
      dynamic           (1) : the route was created dynamically by a BCIN command.
      defByProtocol     (2) : the route was defined by routing protocol.
      switchByProtocol  (4) : the route has been switched by routing protocol.
      changed           (8) : the route has been modified.

      Several bits may be set simultaneous."
  ::= { bcamRouteTabEntry 34 }
-- SNMPNAME: RCHS
-- ASSNAME: YRROCHST
-- implemented

bcamRouteTabIso9542 OBJECT-TYPE
  SYNTAX INTEGER {
-- ASSMULTI:
		 eshReceived(1),          -- ASSVAL: YRRORESH 1
		 ishReceived(2),          -- ASSVAL: YRRORISH 2
		 iso8473QueryReceived(4), -- ASSVAL: YRROQUCF 8
		 refreshRequired(8)       -- ASSVAL: YRROREFR 128
		 }
  ACCESS read-only
  STATUS mandatory
  DESCRIPTION
    " Describes the ISO9542 processing, if this route is an INTF route
      eshReceived          (1) : endsystem hello received
      ishReceived          (2) : intermediate system hello received.
      iso8473QueryReceived (4) : ISO 8473 query configuration request received.
      refreshRequired      (8) : refreshing of routing information is required.
      Several bits may be set simultaneous."
  ::= { bcamRouteTabEntry 35 }
-- SNMPNAME: RI95
-- ASSNAME: YRRO9542
-- implemented

bcamRouteTabMaxNetLength OBJECT-TYPE
  SYNTAX INTEGER {
-- ASSSINGLE:
		 ethernet(1), -- ASSVAL: YWRYMXNE 1
		 fddi(2)     -- ASSVAL: YWRYMXNF 2
		 }
  ACCESS read-only
  STATUS mandatory
  DESCRIPTION
    " The maximum length of a packet used on layer 3 (only valid for FDDI routes).
      ethernet (1) : the length will be limited to the maximum length for ethernet.
      fddi     (2) : the length will be limited to the maximum length for FDDI.
    "
  ::= { bcamRouteTabEntry 36 }
-- SNMPNAME: RMNL
-- ASSNAME: YRROMXN
-- implemented

bcamRouteTabState2 OBJECT-TYPE
  SYNTAX INTEGER {
-- ASSMULTI:
--                 silent(0),                  ASSVAL: YRROSSIL 0
		 testing(1),                -- ASSVAL: YRROSTST 16
		 working(2),                -- ASSVAL: YRROSWOR 32
		 waitAddrResolution(4),     -- ASSVAL: YRROSARP 64
		 lifeSupervisionFailed(8),  -- ASSVAL: YRROSFAI 128
		 silent(16)                 -- ASSVAL: YRROSSIL 0
		 }
  ACCESS read-only
  STATUS mandatory
  DESCRIPTION
    " Describes the state of the route viewed by routing protocols.
      silent                  (16) : route not yet or no more used.
      testing                  (1) : delivery problems on route detected.
      working                  (2) : route working.
      waitAddressResolution    (4) : waiting for address resolution.
      lifetimeSupervsionFailed (8) : lifetime supervision failed.
      Several bits may be set simultaneous."
  ::= { bcamRouteTabEntry 37 }
-- SNMPNAME: RST2
-- ASSNAME: YRROSTA2
-- implemented

bcamRouteTabDeactReason OBJECT-TYPE
  SYNTAX INTEGER {
-- ASSMULTI:
		 flush(1),        -- ASSVAL: YRRODFLR 1
		 supposedDown(2), -- ASSVAL: YRRODRSD 16
		 noArpReply(4)    -- ASSVAL: YRRODNAR 32
		 }
  ACCESS read-only
  STATUS mandatory
  DESCRIPTION
    " The reason for the deactivation of the route.
      flush        (1) : an ISO9542 protocol element that requests the invalidation of
			 the routing information has been received.
      supposedDown (2) : the route is supposed to be down (derived from state of TCP
			 protocol machine or ICMP message received).
      noArpReply   (4) : no arp reply has been received.

      Several bits may be set simultaneous."
  ::= { bcamRouteTabEntry 38 }
-- SNMPNAME: RDAR
-- ASSNAME: YRRODREA
-- implemented

bcamRouteTabSwitchType OBJECT-TYPE
  SYNTAX INTEGER {
-- ASSMULTI:
		 localRemote(1),     -- ASSVAL: YRROSWLR 1
		 remoteLocal(2),     -- ASSVAL: YRROSWRL 2
		 remoteRemote(4),    -- ASSVAL: YRROSWRR 4
		 lsapChanged(8),     -- ASSVAL: YRROSWLS 8
		 lanAddrChanged(16)  -- ASSVAL: YRROSWLL 16
		 }
  ACCESS read-only
  STATUS mandatory
  DESCRIPTION
    " The type of the last performed route switching.
      localRemote     (1) : the route has been changed from local to remote route.
      remoteLocal     (2) : the route has been changed from remote to local route.
      remoteRemote    (4) : the route has been changed from remote to remote route.
      lsapChanged     (8) : the route has been switched to another LSAPCB.
      lanAddrChanged (16) : the lan address of the route has been changed.
      Several bits may be set simultaneous."
  ::= { bcamRouteTabEntry 39 }
-- SNMPNAME: RSWT
-- ASSNAME: YRROSWTY
-- implemented

bcamRouteTabReasonCreation OBJECT-TYPE
  SYNTAX INTEGER {
-- ASSMULTI:
-- ASSVAL: 0 means route has been defined by generation
		 incomingData(1),    -- ASSVAL: YBTAECID 16
		 outgoingData(2),    -- ASSVAL: YBTAECOD 32
		 routingProtocol(4), -- ASSVAL: YBTAECRP 64
		 routingData(8)      -- ASSVAL: YBTAECRD 128
		 }
  ACCESS read-only
  STATUS mandatory
  DESCRIPTION
    " Describes the reason for the automatic creation of the route.
      incomingData    (1) : the route has been created because of incoming data.
      outgoingData    (2) : the route has been created because of outgoing data.
      routingProtocol (4) : the route has been created because of incoming routing protocol.
      routingData     (8) : the route has been created because of data to be routed.
      Several bits may be set simultaneous."
  ::= { bcamRouteTabEntry 40 }
-- SNMPNAME: RRAC
-- ASSNAME: YRROREAF
-- implemented

