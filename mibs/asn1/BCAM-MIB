  SYNTAX INTEGER {
-- ASSMULTI:
--                 stopped(0),  ASSVAL: 0 0
		 running(1), -- ASSVAL: YTETALLO 1
		 save(2),    -- ASSVAL: YTESAVRQ 2
		 hold(4),    -- ASSVAL: YTEHLDRQ 4
		 stopped(8)  -- ASSVAL: 0 0
		 }
  ACCESS read-only
  STATUS mandatory
  DESCRIPTION
    " The current state of the trace.
      stopped (8) : the trace is not running.
      running (1) : the trace is running.
      save    (2) : the trace is running and saved to a file.
      hold    (4) : the trace data will be kept in case of an error.
      Several bits may be set simultaneous."
  ::= { bcamIfTabEntry 89 }
-- SNMPNAME: LTST
-- ASSNAME: YTETFLAG

bcamIfTabTraceNumberBuffer OBJECT-TYPE
  SYNTAX INTEGER (1..16) -- ASSLEN: 2
  ACCESS read-only
  STATUS mandatory
  DESCRIPTION
    " The number of buffers used for the trace"
  ::= { bcamIfTabEntry 90 }
-- SNMPNAME: LTNB
-- ASSNAME: YTEACBFN

bcamIfTabTraceBufferLen OBJECT-TYPE
  SYNTAX INTEGER (512..16384) -- ASSLEN: 2
  ACCESS read-only
  STATUS mandatory
  DESCRIPTION
    " The length of one trace buffer in bytes (rounded up to multiples of 256). "
  ::= { bcamIfTabEntry 91 }
-- SNMPNAME: LTBL
-- ASSNAME: YTEACBFL











-- the Router Group

bcamRouterNumTable OBJECT-TYPE
  SYNTAX INTEGER
  ACCESS read-only
  STATUS mandatory
  DESCRIPTION
    " The number of table entries in the router table "
  ::= { bcamRouter 1 }
-- SNMPNAME: UNTB
-- ASSNAME:

bcamRouterTab OBJECT-TYPE
  SYNTAX SEQUENCE OF RouterEntry
  ACCESS not-accessible
  STATUS mandatory
  DESCRIPTION
    "Table of all routers"
  ::= { bcamRouter 2 }
-- SNMPNAME:
-- ASSNAME:

bcamRouterTabEntry OBJECT-TYPE
  SYNTAX RouterEntry
  ACCESS not-accessible
  STATUS mandatory
  DESCRIPTION
    "Information about a router"
  INDEX {
	bcamRouterTabIpLow,
	bcamRouterTabIpHigh,
	bcamRouterTabIpLocal
	}
  ::= { bcamRouterTab 1 }
-- SNMPNAME:
-- ASSNAME:

  RouterEntry ::=
    SEQUENCE {
	     bcamRouterTabIpLow IpAddress,
	     bcamRouterTabIpHigh IpAddress,
	     bcamRouterTabIpLocal IpAddress,
	     bcamRouterTabIpRouter IpAddress,
	     bcamRouterTabNumRouter INTEGER
	     }


bcamRouterTabIpLow OBJECT-TYPE
  SYNTAX IpAddress
  ACCESS read-only
  STATUS mandatory
  DESCRIPTION
    " The lower IP address limit of the IP address range reachable via this router. "
  ::= { bcamRouterTabEntry 1 }
-- SNMPNAME: UTIL
-- ASSNAME: YRGTIPA

bcamRouterTabIpHigh OBJECT-TYPE
  SYNTAX IpAddress
  ACCESS read-only
  STATUS mandatory
  DESCRIPTION
    " The upper IP address limit of the IP address range reachable via this router. "
  ::= { bcamRouterTabEntry 2 }
-- SNMPNAME: UTIH
-- ASSNAME: YRGTUPA

bcamRouterTabIpLocal OBJECT-TYPE
  SYNTAX IpAddress
  ACCESS read-only
  STATUS mandatory
  DESCRIPTION
    " The local IP address used for this endsystem. "
  ::= { bcamRouterTabEntry 3 }
-- SNMPNAME: UTIC
-- ASSNAME:

bcamRouterTabIpRouter OBJECT-TYPE
  SYNTAX IpAddress
  ACCESS read-only
  STATUS mandatory
  DESCRIPTION
    " The IP address of the router "
  ::= { bcamRouterTabEntry 4 }
-- SNMPNAME: UTIR
-- ASSNAME: YRROIPA

bcamRouterTabNumRouter OBJECT-TYPE
  SYNTAX INTEGER
  ACCESS read-only
  STATUS mandatory
  DESCRIPTION
    " The route number of the router. "
  ::= { bcamRouterTabEntry 5 }
-- SNMPNAME: UTNR
-- ASSNAME: YRROZKEY


-- the Host Group

bcamHostNumTable OBJECT-TYPE
  SYNTAX INTEGER
  ACCESS read-only
  STATUS mandatory
  DESCRIPTION
    " The number of table entries in the host table "
  ::= { bcamHost 1 }
-- SNMPNAME: HNTB
-- ASSNAME: YBCHOSTF

bcamHostTab OBJECT-TYPE
  SYNTAX SEQUENCE OF HostEntry
  ACCESS not-accessible
  STATUS mandatory
  DESCRIPTION
    "Table of all Hosts"
  ::= { bcamHost 2 }
-- SNMPNAME:
-- ASSNAME:

bcamHostTabEntry OBJECT-TYPE
  SYNTAX HostEntry
  ACCESS not-accessible
  STATUS mandatory
  DESCRIPTION
    "Information about a Host"
  INDEX {
	bcamHostTabNumber
	}
  ::= { bcamHostTab 1 }
-- SNMPNAME:
-- ASSNAME:

  HostEntry ::=
    SEQUENCE {
	     bcamHostTabNumber INTEGER ,
	     bcamHostTabName DisplayString (SIZE(8)) ,
	     bcamHostTabSocketName DisplayString (SIZE(0..32)) ,
	     bcamHostTabTyp INTEGER ,
	     bcamHostTabState INTEGER
	     }


bcamHostTabNumber OBJECT-TYPE
  SYNTAX INTEGER
  ACCESS read-only
  STATUS mandatory
  DESCRIPTION
    " The definite identifier for this host. "
  ::= { bcamHostTabEntry 1 }
-- SNMPNAME: HNUM
-- ASSNAME: YRHONUMB

bcamHostTabName OBJECT-TYPE
  SYNTAX DisplayString (SIZE(8)) -- ASSLEN: 9 VAR
  ACCESS read-only
  STATUS mandatory
  DESCRIPTION
    " The NEA name of the host. "
  ::= { bcamHostTabEntry 2 }
-- SNMPNAME: HNAM
-- ASSNAME: YRHOHNAM

bcamHostTabSocketName OBJECT-TYPE
  SYNTAX DisplayString (SIZE(0..32)) -- ASSLEN: 33 VAR
  ACCESS read-only
  STATUS mandatory
  DESCRIPTION
    " The socket name of the host. "
  ::= { bcamHostTabEntry 3 }
-- SNMPNAME: HSON
-- ASSNAME: YRHOSHON

bcamHostTabTyp OBJECT-TYPE
  SYNTAX INTEGER {
-- ASSSINGLE:
		 normal(1),  -- ASSVAL: YRHOTYNO 213
		 virtual(2)  -- ASSVAL: YRHOTYVI 229
		 }
  ACCESS read-only
  STATUS mandatory
  DESCRIPTION
    " The type of the host.
      normal  (1) : the host is the BCAM standard host.
      virtual (2) : the host is a virtual host. "
  ::= { bcamHostTabEntry 4 }
-- SNMPNAME: HTYP
-- ASSNAME: YRHOTYPE

bcamHostTabState OBJECT-TYPE
  SYNTAX INTEGER {
-- ASSMULTI:
		 active(1),       -- ASSVAL: YRHOFACT 128
		 included(2)      -- ASSVAL: YRHOFINC 64
		 }
  ACCESS read-only
  STATUS mandatory
  DESCRIPTION
    " The current state of the host:
      active   (1) : the host is working.
      included (2) : the host is known by BCAM.
      Several bits may be set simultaneous."
  ::= { bcamHostTabEntry 5 }
-- SNMPNAME: HSTA
-- ASSNAME: YRHOFLAG


END

