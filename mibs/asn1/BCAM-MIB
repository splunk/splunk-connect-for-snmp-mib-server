-- the Connection group


bcamCepCurrent OBJECT-TYPE
  SYNTAX INTEGER
  ACCESS read-only
  STATUS mandatory
  DESCRIPTION
    "The number of connections in the state open. "
  ::= { bcamCep 1 }
-- SNMPNAME: CCCU
-- ASSNAME: YBC#CONA
-- implemented

