  SYNTAX INTEGER (0..255) -- ASSLEN: 1
  ACCESS read-only
  STATUS mandatory
  DESCRIPTION
    " The number of times the limits of the outbound processing time buckets have
      been changed."
  ::= { bcamGlobal 35 }
-- SNMPNAME: GOCT
-- ASSNAME: YBTMOCNT
-- implemented

bcamGlobalOutProcSet OBJECT-TYPE
  SYNTAX INTEGER {
-- ASSMULTI:
--                 off(0),
		 on(1),  -- ASSVAL: YBTMOUB 8
		 off(2)  -- ASSVAL: 0 0
		 }
