  bcamRouteTab OBJECT-TYPE
    SYNTAX SEQUENCE OF RoutEntry
    ACCESS not-accessible
    STATUS mandatory
    DESCRIPTION
      "Table of all Routes"
  ::= { bcamRoute 10 }
-- SNMPNAME:
-- ASSNAME:

  bcamRouteTabEntry OBJECT-TYPE
    SYNTAX RoutEntry
    ACCESS not-accessible
    STATUS mandatory
    DESCRIPTION
      "Information about a route"
    INDEX {
      bcamRouteTabNumbers
      }
  ::= { bcamRouteTab 1 }
-- SNMPNAME:
-- ASSNAME:

  RoutEntry ::=
    SEQUENCE {
    bcamRouteTabNumbers Counter ,
    bcamRouteTabName DisplayString (SIZE(8)),
    bcamRouteTabNumNeaConn Gauge ,
    bcamRouteTabNumIsoConn Gauge ,
    bcamRouteTabMaxNeaConn Gauge ,
    bcamRouteTabMaxIsoConn Gauge ,
    bcamRouteTabBadNeaElems Counter ,
    bcamRouteTabBadIsoElems Counter ,
    bcamRouteTabMaxL4Conn Gauge ,
    bcamRouteTabMaxUnackTpdu Gauge ,
    bcamRouteTabRouteNetTyp INTEGER ,
    bcamRouteTabNumberIsoIpRouter INTEGER ,
    bcamRouteTabNumberNeaRouter INTEGER ,
    bcamRouteTabNumberGateway INTEGER ,
    bcamRouteTabFunction INTEGER ,
    bcamRouteTabProcNetTyp INTEGER ,
    bcamRouteTabMaxTsduLen Gauge ,
    bcamRouteTabNameEndsystem DisplayString (SIZE(8)),
    bcamRouteTabIso4WindowTimer Gauge ,
    bcamRouteTabIso4RetransTimer Gauge ,
    bcamRouteTabAckTimer Gauge ,
    bcamRouteTabErrorRecovTimer Gauge ,
    bcamRouteTabRejectTimer Gauge ,
    bcamRouteTabExpedRetransTimer Gauge ,
    bcamRouteTabNameGateway DisplayString (SIZE(8)),
    bcamRouteTabNameX25Station DisplayString (SIZE(8)),
    bcamRouteTabL3InputProfil INTEGER ,
    bcamRouteTabTransState INTEGER ,
    bcamRouteTabOption INTEGER ,
    bcamRouteTabTyp INTEGER ,
    bcamRouteTabUsage INTEGER ,
    bcamRouteTabL3Subprofile INTEGER ,
    bcamRouteTabCommandState INTEGER ,
    bcamRouteTabChangeState INTEGER ,
    bcamRouteTabIso9542 INTEGER ,
    bcamRouteTabMaxNetLength INTEGER ,
    bcamRouteTabState2 INTEGER ,
    bcamRouteTabDeactReason INTEGER ,
    bcamRouteTabSwitchType INTEGER ,
    bcamRouteTabReasonCreation INTEGER ,
    bcamRouteTabOrigLanAddress PhysAddress ,
    bcamRouteTabLanAddress PhysAddress ,
    bcamRouteTabTypAddress INTEGER ,
    bcamRouteTabLocalAddr OCTET STRING (SIZE(0..20)),
    bcamRouteTabRemoteAddr OCTET STRING (SIZE(0..20)),
    bcamRouteTabOutPacketsDatas Counter ,
    bcamRouteTabOutPacketsFlowControls Counter ,
    bcamRouteTabInPacketsDatas Counter ,
    bcamRouteTabInPacketsFlowControls Counter ,
    bcamRouteTabOutRetransPackets Counter ,
    bcamRouteTabInDetectedGaps Counter ,
    bcamRouteTabInDuplicatedPackets Counter ,
    bcamRouteTabInIncorrectPackets Counter ,
    bcamRouteTabRoundTripTimeClosed INTEGER (0..65535),
    bcamRouteTabRoundTripTimeCurrent INTEGER (0..65535),
    bcamRouteTabArpReqSend INTEGER ,
    bcamRouteTabArpRepSend INTEGER ,
    bcamRouteTabArpReqRec INTEGER ,
    bcamRouteTabArpRepRec INTEGER ,
    bcamRouteTabIcmpReq INTEGER ,
    bcamRouteTabIcmpReply INTEGER ,
    bcamRouteTabIcmpRedirect INTEGER ,
    bcamRouteTabSwitched INTEGER ,
    bcamRouteTabDown INTEGER ,
    bcamRouteTabOspfHello INTEGER ,
    bcamRouteTabPacketNoConn INTEGER (0..65535),
    bcamRouteTabPacketInternDiscon INTEGER (0..65535),
    bcamRouteTabPacketBadProtocol INTEGER (0..65535),
    bcamRouteTabConnReqOut INTEGER (0..65535),
    bcamRouteTabConnReqOutAck INTEGER (0..65535),
    bcamRouteTabConnReqOutRej INTEGER (0..65535),
    bcamRouteTabConnReqIn INTEGER (0..65535),
    bcamRouteTabConnReqInAck INTEGER (0..65535),
    bcamRouteTabConnReqInRej INTEGER (0..65535),
    bcamRouteTabDisconnOut INTEGER (0..65535),
    bcamRouteTabDisconnOutAck INTEGER (0..65535),
    bcamRouteTabDisconnIn INTEGER (0..65535),
    bcamRouteTabDisconnInAck INTEGER (0..65535),
    bcamRouteTabNumberLink INTEGER,
    bcamRouteTabArpFlag INTEGER,
    bcamRouteTabNsduLen INTEGER,
    bcamRouteTabMinNsduLen INTEGER,
    bcamRouteTabMaxNsduLen INTEGER

      }

