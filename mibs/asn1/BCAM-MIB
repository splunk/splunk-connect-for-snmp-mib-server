bcamRouteTabL3InputProfil OBJECT-TYPE
  SYNTAX INTEGER {
-- ASSSINGLE:
		 nea(1),     -- ASSVAL: YRRO3PNE 1
		 int0(2),    -- ASSVAL: YRRO3PI0 2
		 intf(3),    -- ASSVAL: YRRO3PIF 3
		 gateway(4), -- ASSVAL: YRRO3PGA 4
		 ip(5)       -- ASSVAL: YRRO3PIP 5
		 }
  ACCESS read-only
  STATUS mandatory
  DESCRIPTION
    " The input profile used on the network layer.
      nea       (1) : proprietary NEAN protocol.
      int0      (2) : protocol according ISO 8473 (inactive network layer).
      intf      (3) : protocol according ISO 8473.
      gateway   (4) : no network layer protocol used.
      ip        (5) : proctocol according RFC 791."

  ::= { bcamRouteTabEntry 27 }
-- SNMPNAME: RL3I
-- ASSNAME: YRRO3PRF
-- implemented

bcamRouteTabTransState OBJECT-TYPE
  SYNTAX INTEGER {
-- ASSSINGLE:
--                 notReady(0),                ASSVAL: YBAPSNOT 0
		 waitForTransferInit(1),    -- ASSVAL: YBAPSWIN 1
		 waitForReadyToTransfer(2), -- ASSVAL: YBAPSWAR 2
		 ready(3),                  -- ASSVAL: YBAPSRDY 3
		 notReady(4)                -- ASSVAL: YBAPSNOT 0
		 }
  ACCESS read-only
  STATUS mandatory
  DESCRIPTION
    " The current transfer state of this route as viewed by transport layer.
      notReady               (4) : no packets can be sent.
      waitForTransferInit    (1) : route may be initialised.
      waitForReadyToTransfer (2) : address resolution running
      ready                  (3) : packets may be sent "
  ::= { bcamRouteTabEntry 28 }
-- SNMPNAME: RTST
-- ASSNAME: YRROTSTA
-- implemented


bcamRouteTabOption OBJECT-TYPE
  SYNTAX INTEGER {
-- ASSMULTI:
		 delayedAckAllowed(1),   -- ASSVAL: YRROODAN 128
		 optimalSegmentSizeOn(2) -- ASSVAL: YRROOSSN 64
		 }
  ACCESS read-only
  STATUS mandatory
  DESCRIPTION
    " The currently valid options for this route (only valid for TCP).
      delayedAckAllowed    (1) : do not try do minimize the number of outgoing acknowledgements
      optimalSegmentSizeOn (2) : do not try do minimize the number of incoming acknowledgements
      Several bits may be set simultaneous."
  ::= { bcamRouteTabEntry 29 }
-- SNMPNAME: ROPT
-- ASSNAME: YRROOFLG
-- implemented

bcamRouteTabTyp OBJECT-TYPE
  SYNTAX INTEGER {
-- ASSSINGLE:
		 direct(1), -- ASSVAL: YRROTDIR 196
		 remote(2),  -- ASSVAL: YRROTREM 217
		 node(3)    -- ASSVAL: YRROTNOD 213
		 }
  ACCESS read-only
  STATUS mandatory
  DESCRIPTION
    " Describes how the remote system described by this route is reached.
      direct (1) : the remote system is connected directly to the local system (e.g via DAST).
      remote (2) : the remote system is reached via at least one router.
      node   (3) : the remote system is connected directly to ethernet or fddi node. "
  ::= { bcamRouteTabEntry 30 }
-- SNMPNAME: RRTY
-- ASSNAME: YRROTYPE
-- implemented

bcamRouteTabUsage OBJECT-TYPE
  SYNTAX INTEGER {
-- ASSMULTI:
--                 noIntermediate(0),  ASSVAL: NONE 0
		 gatewayRoute(1),   -- ASSVAL: YRROUFUG 2
		 intfIpRouter(2),   -- ASSVAL: YRROUFUR 4
		 neaRouter(4),      -- ASSVAL: YRROUFUN 8
		 int0Router(8),     -- ASSVAL: YRROUFU0 16
		 noIntermediate(16) -- ASSVAL: 0 0
		 }
  ACCESS read-only
  STATUS mandatory
  DESCRIPTION
    " Describes whether this route describes an intermediate system.
      noIntermediate (16) : no intermediate route
      gatewayRouter   (1) : route describes an intermediate system used as gateway
      intfIpRouter    (2) : route describes an INTF or an IP router
      neaRouter       (4) : route describes a NEA router
      int0Router      (8) : route describes an INT0 router

      Several bits may be set simultaneous."
  ::= { bcamRouteTabEntry 31 }
-- SNMPNAME: RRUS
-- ASSNAME: YRROUFLG
-- implemented

bcamRouteTabL3Subprofile OBJECT-TYPE
  SYNTAX INTEGER {
-- ASSMULTI:
		 rfc1042(1),     -- ASSVAL: YRRO3SIS 1
		 netConnLess(2), -- ASSVAL: YRRO3SCL 4
		 neaNetConn(4),  -- ASSVAL: YRRO3SNC 128
		 neattNetConn(8) -- ASSVAL: YRRO3STT 64
		 }
  ACCESS read-only
  STATUS mandatory
  DESCRIPTION
    " rfc1042      (1) : a layer 3 subprotocol according RFC1042 is used on this route.
      netConnLess  (2) : a connectionless layer 3 protocol is used on this route.
      neaNetConn   (4) : a NEA net connection is used on this route.
      neattNetConn (8) : a NEATT net connection is used on this route.
      Several bits may be set simultaneous."
  ::= { bcamRouteTabEntry 32 }
-- SNMPNAME: RL3S
-- ASSNAME: YRRO3SPF
