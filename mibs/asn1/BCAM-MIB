-- the Route group


bcamRouteNumTable OBJECT-TYPE
    SYNTAX INTEGER
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
      "The number of all routes known by BCAM"
  ::= { bcamRoute 1 }
-- SNMPNAME: RNTB
-- ASSNAME:

  bcamRouteActive OBJECT-TYPE
    SYNTAX INTEGER
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
      "The number of active routes"
  ::= { bcamRoute 2 }
-- SNMPNAME: RRAT
-- ASSNAME:

bcamRouteArpDefault OBJECT-TYPE
  SYNTAX INTEGER {
-- ASSMULTI:
		 off(1),  -- ASSVAL: YBTARPNO 128
		 on(2),   -- ASSVAL: YBTARPTI 32
		 quiet(4) -- ASSVAL: YBTARPQU 64
		 }
  ACCESS read-only
  STATUS mandatory
  DESCRIPTION
    " The ARP state to use as default when a route of type node (bcamRouteTabTyp) is activated.
      Several bits may be set simultaneous.
      off   (1) : No usage of ARP.
      on    (2) :  periodic usage of ARP.
      quiet (4) : ARP will only be used if no connection oriented data were transferred during the
		  last ARP period."
  ::= { bcamRoute 3 }
-- SNMPNAME: RADS
-- ASSNAME: YBTARPFG

