-- the Interface group

  bcamIfNumTable OBJECT-TYPE
    SYNTAX INTEGER
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
      "  "
  ::= { bcamIf 1 }
-- SNMPNAME: LNTB
-- ASSNAME:


  bcamIfTab OBJECT-TYPE
    SYNTAX SEQUENCE OF LinkEntry
    ACCESS not-accessible
    STATUS mandatory
    DESCRIPTION
      "Table of all interfaces"
  ::= { bcamIf 2 }
-- SNMPNAME:
-- ASSNAME:

  bcamIfTabEntry OBJECT-TYPE
    SYNTAX LinkEntry
    ACCESS not-accessible
    STATUS mandatory
    DESCRIPTION
      "Information about an interface"
    INDEX {
      bcamIfTabNumbers
      }
  ::= { bcamIfTab 1 }
-- SNMPNAME:
-- ASSNAME:

  LinkEntry ::=
    SEQUENCE {
    bcamIfTabNumbers Counter ,
    bcamIfTabName DisplayString (SIZE(8)),
    bcamIfTabProfile INTEGER ,
    bcamIfTabMnemonicWrite DisplayString (SIZE(4)),
    bcamIfTabMnemonicRead DisplayString (SIZE(4)),
    bcamIfTabLanAddress PhysAddress ,
    bcamIfTabConfigUpdate INTEGER ,
    bcamIfTabMaxLpdu INTEGER ,
    bcamIfTabL2Monitoring INTEGER ,
    bcamIfTabDevice INTEGER ,
    bcamIfTabAdminState INTEGER ,
    bcamIfTabCurrentState INTEGER ,
    bcamIfTabMode INTEGER ,
    bcamIfTabPortName DisplayString (SIZE(8)),
    bcamIfTabLenTraceOut INTEGER (0..4096),
    bcamIfTabLenTraceIn INTEGER (0..4096),
    bcamIfTabNumRouteSwitchings INTEGER ,
    bcamIfTabTimeLastChange INTEGER ,
    bcamIfTabMnemonicDiag DisplayString (SIZE(4)),
    bcamIfTabNumMulticastAddr INTEGER ,
    bcamIfTabMulticastAddr1 PhysAddress ,
    bcamIfTabMulticastAddr2 PhysAddress ,
    bcamIfTabMulticastAddr3 PhysAddress ,
    bcamIfTabMulticastAddr4 PhysAddress ,
    bcamIfTabMulticastAddr5 PhysAddress ,
    bcamIfTabMulticastAddr6 PhysAddress ,
    bcamIfTabMulticastAddr7 PhysAddress ,
    bcamIfTabMulticastAddr8 PhysAddress ,
    bcamIfTabMulticastAddr9 PhysAddress ,
    bcamIfTabMulticastAddr10 PhysAddress ,
    bcamIfTabNumNeaAddress INTEGER ,
    bcamIfTabNeaAddress1 OCTET STRING (SIZE(2)),
    bcamIfTabNeaAddress2 OCTET STRING (SIZE(2)),
    bcamIfTabNeaAddress3 OCTET STRING (SIZE(2)),
    bcamIfTabNeaAddress4 OCTET STRING (SIZE(2)),
    bcamIfTabNeaAddress5 OCTET STRING (SIZE(2)),
    bcamIfTabNeaAddress6 OCTET STRING (SIZE(2)),
    bcamIfTabNumIpAddress INTEGER ,
    bcamIfTabIpAddress1 IpAddress ,
    bcamIfTabIpAddress2 IpAddress ,
    bcamIfTabIpAddress3 IpAddress ,
    bcamIfTabIpAddress4 IpAddress ,
    bcamIfTabIpAddress5 IpAddress ,
    bcamIfTabIpAddress6 IpAddress ,
    bcamIfTabNumInt0Address INTEGER ,
    bcamIfTabInt0Address1 PhysAddress ,
    bcamIfTabInt0Address2 PhysAddress ,
    bcamIfTabInt0Address3 PhysAddress ,
    bcamIfTabInt0Address4 PhysAddress ,
    bcamIfTabInt0Address5 PhysAddress ,
    bcamIfTabInt0Address6 PhysAddress ,
    bcamIfTabNumIntfAddress INTEGER ,
    bcamIfTabIntfAddress1 OCTET STRING (SIZE(0..20)),
    bcamIfTabIntfAddress2 OCTET STRING (SIZE(0..20)),
    bcamIfTabIntfAddress3 OCTET STRING (SIZE(0..20)),
    bcamIfTabIntfAddress4 OCTET STRING (SIZE(0..20)),
    bcamIfTabIntfAddress5 OCTET STRING (SIZE(0..20)),
    bcamIfTabIntfAddress6 OCTET STRING (SIZE(0..20)),
    bcamIfTabBytesOutHighs Counter ,
    bcamIfTabBytesOutLows Counter ,
    bcamIfTabBytesInHighs Counter ,
    bcamIfTabBytesInLows Counter ,
    bcamIfTabIOsOutHighs Counter ,
    bcamIfTabIOsOutLows Counter ,
    bcamIfTabIOsInHighs Counter ,
    bcamIfTabIOsInLows Counter ,
    bcamIfTabOutputStops Counter ,
    bcamIfTabInputStops Counter ,
    bcamIfTabIOErrorOuts Counter ,
    bcamIfTabIOErrorIns Counter ,
    bcamIfTabPacketsNotReceiveds Counter ,
    bcamIfTabInPacketsLanHighs Counter ,
    bcamIfTabInPacketsLanLows Counter ,
    bcamIfTabOutPacketsLanHighs Counter ,
    bcamIfTabOutPacketsLanLows Counter ,
    bcamIfTabUnicastInHighs Counter ,
    bcamIfTabUnicastInLows Counter ,
    bcamIfTabUnicastOutHighs Counter ,
    bcamIfTabUnicastOutLows Counter ,
    bcamIfTabMulticastInHighs Counter ,
    bcamIfTabMulticastInLows Counter ,
    bcamIfTabMulticastOutHighs Counter ,
    bcamIfTabMulticastOutLows Counter ,
    bcamIfTabErrorPacketIns Counter ,
    bcamIfTabErrorPacketOuts Counter ,
    bcamIfTabDiscardIns Counter ,
    bcamIfTabDiscardOuts Counter ,
    bcamIfTabUnknownProtoIns Counter,
    bcamIfTabTraceState INTEGER,
    bcamIfTabTraceNumberBuffer INTEGER,
    bcamIfTabTraceBufferLen INTEGER


      }

bcamIfTabNumbers OBJECT-TYPE
  SYNTAX INTEGER -- ASSLEN: 2
  ACCESS read-only
  STATUS mandatory
  DESCRIPTION
    "A definite identifier for the interface.
     Serves for references from other tables"
  ::= { bcamIfTabEntry 1 }
-- SNMPNAME: LNUM
-- ASSNAME: YRLSNID
-- implemented

bcamIfTabName OBJECT-TYPE
  SYNTAX DisplayString (SIZE(8)) -- ASSLEN: 9 VAR
  ACCESS read-only
  STATUS mandatory
  DESCRIPTION
    " The name of this interface "
  ::= { bcamIfTabEntry 2 }
-- SNMPNAME: LNAM
-- ASSNAME: YRLSLTGN
-- implemented

bcamIfTabProfile OBJECT-TYPE
  SYNTAX INTEGER {
-- ASSSINGLE:
		 nealkp(1),  -- ASSVAL: YRLS2PNP 1
		 nealke(2),  -- ASSVAL: YRLS2PNE 2
		 nealkeS(3), -- ASSVAL: YRLS2PN3 3
		 nealkh(4),  -- ASSVAL: YRLS2PNH 4
		 llc1(5),    -- ASSVAL: YRLS2PL1 5
		 sinix(6),   -- ASSVAL: YRLS2PSI 6
		 fddi(7)     -- ASSVAL: YRLS2PLF 7
		 }
  ACCESS read-only
  STATUS mandatory
  DESCRIPTION
    " The layer 2 profile of this interface
      nealkp  (1) : proprietary link protocol with permanent dialog.
      nealke  (2) : proprietary event driven link protocol to BS2000 system.
      nealkeS (3) : proprietary event driven link protocol to System 6000.
      nealkh  (4) : a HDLC balanced link protocol (duplex mode).
      llc1    (5) : CSMA/CD link protocol.
      sinix   (6) : proprietary link protocol to SINIX port.
      fddi    (7) : FDDI link protocol.
    "
  ::= { bcamIfTabEntry 3 }
-- SNMPNAME: LPRF
-- ASSNAME: YRLS2PRF
-- implemented

bcamIfTabMnemonicWrite OBJECT-TYPE
  SYNTAX DisplayString (SIZE(4)) -- ASSLEN: 5 VAR
  ACCESS read-only
  STATUS mandatory
  DESCRIPTION
    " The device mnemonic for write operations "
  ::= { bcamIfTabEntry 4 }
-- SNMPNAME: LMWR
-- ASSNAME: YRLSDVWR
-- implemented

bcamIfTabMnemonicRead OBJECT-TYPE
  SYNTAX DisplayString (SIZE(4)) -- ASSLEN: 5 VAR
  ACCESS read-only
  STATUS mandatory
  DESCRIPTION
    " The device mnemonic for read operations "
  ::= { bcamIfTabEntry 5 }
-- SNMPNAME: LMRE
-- ASSNAME: YRLSDVRE
-- implemented

bcamIfTabLanAddress OBJECT-TYPE
  SYNTAX PhysAddress
  ACCESS read-only
  STATUS mandatory
  DESCRIPTION
    " The MAC address (if supported) "
  ::= { bcamIfTabEntry 6 }
-- SNMPNAME: LLAD
-- ASSNAME: YRLSLANA
-- implemented

bcamIfTabConfigUpdate OBJECT-TYPE
  SYNTAX INTEGER {
-- ASSMULTI:
--                 updateAllowed(0),     ASSVAL: YRLSCNAL 0
		 updateForbidden(1),  -- ASSVAL: YRLSCNNO 1
		 primaryRequested(2), -- ASSVAL: YRLSCNPR 2
		 updateAllowed(4)     -- ASSVAL: YRLSCNAL 0
		 }
  ACCESS read-only
  STATUS mandatory
  DESCRIPTION
    "Describes whether assigned routes are allowed to be switched from this to an alternative interface.
     updateAllowed    (4) : switching allowed
     updateForbidden  (1) : switching forbidden
     primaryRequested (2) : switch back of all routes original assigned to this interface requested

      Several bits may be set simultaneous."
  ::= { bcamIfTabEntry 7 }
-- SNMPNAME: LCFU
-- ASSNAME: YRLSCNUP
-- implemented

bcamIfTabMaxLpdu OBJECT-TYPE
  SYNTAX INTEGER {
-- ASSSINGLE :
		 ethernet(1514),  -- ASSVAL: YRLSLPDE 1514
		 fddi(4494),      -- ASSVAL: YRLSLPDF 4494
		 atm(65535),      -- ASSVAL: YRLSLPDA 65535
		 nealkh(65549),   -- ASSVAL: YRLSLPDN 65549
		 nealkp(4113),    -- ASSVAL: YRLSLPDP 4113
--                 oclink(65535),    ASSVAL: YRLSLPDO 65535
--                 sinix(4113),      ASSVAL: YRLSLPDS 4113
		 maxReass(16392)  -- ASSVAL: YRLSLPDR 16392
		 }
  ACCESS read-only
  STATUS mandatory
  DESCRIPTION
    " The maximum length of a link protocol data unit depending on bcamIfTabProfile.
      ethernet  (1514) :  1514 bytes
      fddi      (4494) :  4494 bytes
      atm      (65535) : 65535 bytes
      nealkh   (65549) : 65549 bytes
      nealkp    (4113) :  4113 bytes
      maxReass (16392) : 16392 bytes

    "
  ::= { bcamIfTabEntry 8 }
-- SNMPNAME: LMLP
-- ASSNAME: YRLSLPDU
-- implemented

bcamIfTabL2Monitoring OBJECT-TYPE
  SYNTAX INTEGER {
-- ASSMULTI:
--                 off(0),  ASSVAL: 0 0
		 on(1),  -- ASSVAL: YRLSMMO2 2
		 off(2)  -- ASSVAL: 0 0
		 }
  ACCESS read-only
  STATUS mandatory
  DESCRIPTION
    " The state of the layer 2 monitoring.
      on  (1) : layer 2 monitoring is running.
      off (2) : layer 2 monitoring is not running."
  ::= { bcamIfTabEntry 9 }
-- SNMPNAME: LMON
-- ASSNAME: YRLSMFLG
-- implemented

bcamIfTabDevice OBJECT-TYPE
  SYNTAX INTEGER {
-- ASSMULTI:
		 esconCtc(1) -- ASSVAL: YRLSDVES 128
		 }
  ACCESS read-only
  STATUS mandatory
  DESCRIPTION
    " Describes whether the interface is an ESCON channel to channel adapter.
      esconCtc (1) : the interface is an ESCON channel to channel adapter.
  "
  ::= { bcamIfTabEntry 10 }
-- SNMPNAME: LDEV
-- ASSNAME: YRLSDVFG
-- implemented

bcamIfTabAdminState OBJECT-TYPE
  SYNTAX INTEGER {
-- ASSSINGLE:
--                 none(0),      ASSVAL: YRLSS1NO 0
		 included(1), -- ASSVAL: YRLSSINC 201
		 active(2),   -- ASSVAL: YRLSSACT 193
		 excluded(4)  -- ASSVAL: YRLSSEXC 0
		 }
  ACCESS read-only
  STATUS mandatory
  DESCRIPTION
    " The administrative state of this interface requested by commands.
      excluded (4) : no BCIN command given
      included (1) : BCIN command given
      active   (2) : BCACT command given

      "
  ::= { bcamIfTabEntry 11 }
-- SNMPNAME: LAST
-- ASSNAME: YRLSSTAT
-- implemented

bcamIfTabCurrentState OBJECT-TYPE
  SYNTAX INTEGER {
-- ASSMULTI:
--                 none(0),          ASSVAL: YRLSS2NO X'00'
		 waitForAct(1),   -- ASSVAL: YRLSSWFA X'01'
		 working(2),      -- ASSVAL: YRLSSWOR X'02'
		 waitForDeact(4), -- ASSVAL: YRLSSWFD X'04'
		 none(8)          -- ASSVAL: YRLSS2NO X'00'
		 }
  ACCESS read-only
  STATUS mandatory
  DESCRIPTION
    " The actual working state of this interface
      none         (8) : interface not working
      waitForAct   (1) : activation in progress
      working      (2) : interface working
      waitForDeact (4) : deactivation in progress

      Several bits may be set simultaneous."
  ::= { bcamIfTabEntry 12 }
-- SNMPNAME: LCST
-- ASSNAME: YRLSSTA2
-- implemented

bcamIfTabMode OBJECT-TYPE
  SYNTAX INTEGER {
-- ASSMULTI:
		 stopModeOn(16), -- ASSVAL: YRLSMSTO 128
		 slowPollOn(8),  -- ASSVAL: YRLSMSLO 8
		 shortWaitOn(4), -- ASSVAL: YRLSMSAW 4
		 multicastOn(2), -- ASSVAL: YRLSMMCO 2
		 broadcastOn(1)  -- ASSVAL: YRLSMBCO 1
		 }
  ACCESS read-only
  STATUS mandatory
  DESCRIPTION
    "Describes the operational parameters of this interface
     stopModeOn  (16) : stop PDN in case of abnormal deactivation
     slowPollOn   (8) : automatic reactivation shall be tried
     shortWaitOn  (4) : stop activation after timeout
     multicastOn  (2) : evaluate multicasts
     broadcastOn  (1) : evaluate broadcasts

      Several bits may be set simultaneous."
  ::= { bcamIfTabEntry 13 }
-- SNMPNAME: LMOD
-- ASSNAME: YRLSLSMF
-- implemented

