--
-- Title:   MIB for BCAM -  version 14
-- Date:    04 September 1997
-- Authors: SNI OEC BS DC21


-- MIB-Version: 1




BCAM-MIB DEFINITIONS ::= BEGIN
IMPORTS
		DisplayString FROM SNMPv2-TC
  mgmt, NetworkAddress, IpAddress, Counter, Gauge, TimeTicks, enterprises
  FROM RFC1155-SMI

  OBJECT-TYPE
  FROM RFC-1212
  TRAP-TYPE FROM RFC-1215;

  sni            OBJECT IDENTIFIER ::= { enterprises 231 }
  sniProductMibs OBJECT IDENTIFIER ::= { sni 2 }
  sniBcam        OBJECT IDENTIFIER ::= { sniProductMibs 17 }

PhysAddress ::= OCTET STRING

-- groups in BCAM

bcamTrap       OBJECT IDENTIFIER ::= { sniBcam 1 }

bcamGlobal     OBJECT IDENTIFIER ::= { sniBcam 2 }
-- The global group contains values that are specific for BCAM and cannot be
-- assigned to one of the following groups.

bcamMemory     OBJECT IDENTIFIER ::= { sniBcam 3 }
-- The memory group contains values that describe the memory occupied by BCAM

bcamTrace      OBJECT IDENTIFIER ::= { sniBcam 4 }
-- The trace group describes the state of the BCAM traces
-- SNMPNAME:
-- ASSNAME: YYYTCONB (anchor)

bcamTsap       OBJECT IDENTIFIER ::= { sniBcam 5 }
-- The tsap group contains values that are specific for applications and is
-- divided in two sections. The common part contains values that belong to
-- applications, but cannot be assigned to a specific one. The table part
-- describes every application in more detail.

bcamCep        OBJECT IDENTIFIER ::= { sniBcam 6 }
-- The connection group contains values that are specific for connections and
-- is divided in two sections. The common part contains values that belong to
-- connections, but cannot be assigned to a specific one. The table part
-- describes every connection in more detail.

bcamRoute      OBJECT IDENTIFIER ::= { sniBcam 7 }
-- A route is defined by the pair of the addresses of two hosts.
-- The route group contains values that are specific for routes and is divided in two sections.
-- The common part contains values that belong to routes, but cannot be assigned to a specific one.
-- The table part describes every route in more detail.

bcamIf         OBJECT IDENTIFIER ::= { sniBcam 8 }
-- An interface is an access point to the network.
-- The interface group contains values that are specific for interfaces and is divided in two sections.
-- The common part contains values that belong to interfaces, but cannot be assigned to a specific one.
-- The table part describes every interface in more detail.

bcamRouter     OBJECT IDENTIFIER ::= { sniBcam 9 }
-- A router is a system known by bcam as an intermediate system.
-- The router group contains values that are specific for routers and is divided in two sections.
-- The common part contains values that belong to routers, but cannot be assigned to a specific one.
-- The table part describes every router in more detail.

bcamHost       OBJECT IDENTIFIER ::= { sniBcam 10 }
-- A host is a local endsystem (there may be more than one host).
-- The host group contains values that are specific for hosts and is divided in two sections.
-- The common part contains values that belong to hosts, but cannot be assigned to a specific one.
-- The table part describes every host in more detail.


-- the Trap Group

bcamTrapString OBJECT-TYPE
  SYNTAX DisplayString
  ACCESS not-accessible
  STATUS mandatory
  DESCRIPTION " Textual description of the trap. The trap string contains the values sent with
		the trap in printable form separated by the keywords given at the description
		of the individual traps. "
  ::= { bcamTrap 1 }
-- SNMPNAME: PSTR
-- ASSNAME:

bcamTrapOutPoolOverCurrent OBJECT-TYPE
  SYNTAX INTEGER
  ACCESS not-accessible
  STATUS mandatory
  DESCRIPTION " The number of bytes of resident memory used for output cells
		when the trap condition (output pool over maximum) has been recognized. "
  ::= { bcamTrap 2 }
-- SNMPNAME: POOC
-- ASSNAME:

bcamTrapOutPoolUnderCurrent OBJECT-TYPE
  SYNTAX INTEGER
  ACCESS not-accessible
  STATUS mandatory
  DESCRIPTION " The number of bytes of resident memory used for output cells
		when the trap condition (output pool under maximum) has been recognized. "
  ::= { bcamTrap 3 }
-- SNMPNAME: POUC
-- ASSNAME:

bcamTrapInPoolOverCurrent OBJECT-TYPE
  SYNTAX INTEGER
  ACCESS not-accessible
  STATUS mandatory
  DESCRIPTION " The number of bytes of resident memory used for input cells
		when the trap condition (input pool over maximum) has been recognized. "
  ::= { bcamTrap 4 }
-- SNMPNAME: PIOC
-- ASSNAME:

bcamTrapInPoolUnderCurrent OBJECT-TYPE
  SYNTAX INTEGER
  ACCESS not-accessible
  STATUS mandatory
  DESCRIPTION " The number of bytes of resident memory used for input cells
		when the trap condition (input pool under maximum) has been recognized. "
  ::= { bcamTrap 5 }
-- SNMPNAME: PIUC
-- ASSNAME:

bcamTrapOutPoolOver TRAP-TYPE
  ENTERPRISE sniBcam
  VARIABLES  {
    bcamGlobalUpTime,
    bcamGlobalHostName,
    bcamMemoryPoolOutputLimitTrap,
    bcamTrapOutPoolOverCurrent,
    bcamTrapString
	     }
  DESCRIPTION " This trap will be sent if the resident memory used for
		output cells exceeds the limit (bcamMemoryPoolOutputLimitTrap)
		for more than 5 seconds.
		bcamTrapString has the fields:
		$HOST$:       <hostname>
		$UPTIME$:     <bcamuptime>
		$OUTLIMIT$:   <memlimit>
		$OUTCURRENT$: <memcurrent>
		The following values will be sent together with the trap:
		bcamGlobalHostName
		bcamGlobalUpTime
		bcamMemoryPoolOutputLimitTrap
		bcamMemoryPoolOverCurrent
		bcamTrapString
	      "
  ::= 301

bcamTrapOutPoolUnder TRAP-TYPE
  ENTERPRISE sniBcam
  VARIABLES  {
    bcamGlobalUpTime,
    bcamGlobalHostName,
    bcamMemoryPoolOutputLimitTrap,
    bcamTrapOutPoolUnderCurrent,
    bcamTrapString
	     }
  DESCRIPTION " This trap will be sent if the resident memory used for
		output cells has exceeded the limit (bcamMemoryPoolOutputLimitTrap)
		and falls under the limit for more than 5 seconds.
		bcamTrapString has the fields:
		$HOST$:       <hostname>
		$UPTIME$:     <bcamuptime>
		$OUTLIMIT$:   <memlimit>
		$OUTCURRENT$: <memcurrent>
		The following values will be sent together with the trap:
		bcamGlobalHostName
		bcamGlobalUpTime
		bcamMemoryPoolOutputLimitTrap
		bcamMemoryPoolUnderCurrent
		bcamTrapString
	      "
  ::= 302

bcamTrapInPoolOver TRAP-TYPE
  ENTERPRISE sniBcam
  VARIABLES  {
    bcamGlobalUpTime,
    bcamGlobalHostName,
    bcamMemoryPoolInputLimitTrap,
    bcamTrapInPoolOverCurrent,
    bcamTrapString
	     }
  DESCRIPTION " This trap will be sent if the resident memory used for
		input cells exceeds the limit (bcamMemoryPoolInputLimitTrap)
		for more than 5 seconds.
		bcamTrapString has the fields:
		$HOST$:       <hostname>
		$UPTIME$:     <bcamuptime>
		$INLIMIT$:    <memlimit>
		$INCURRENT$:  <memcurrent>
		The following values will be sent together with the trap:
		bcamGlobalHostName
		bcamGlobalUpTime
		bcamMemoryPoolInputLimitTrap
		bcamTrapInPoolOverCurrent
		bcamTrapString
	      "
  ::= 303

bcamTrapInPoolUnder TRAP-TYPE
  ENTERPRISE sniBcam
  VARIABLES  {
    bcamGlobalUpTime,
    bcamGlobalHostName,
    bcamMemoryPoolInputLimitTrap,
    bcamTrapInPoolUnderCurrent,
    bcamTrapString
	     }
  DESCRIPTION " This trap will be sent if the resident memory used for
		input cells has exceeded the limit (bcamMemoryPoolInputLimitTrap)
		and falls under the limit for more than 5 seconds.
		bcamTrapString has the fields:
		$HOST$:       <hostname>
		$UPTIME$:     <bcamuptime>
		$INLIMIT$:    <memlimit>
		$INCURRENT$:  <memcurrent>
		The following values will be sent together with the trap:
		bcamGlobalHostName
		bcamGlobalUpTime
		bcamMemoryPoolInputLimitTrap
		bcamTrapInPoolUnderCurrent
		bcamTrapString
	      "
  ::= 304

bcamTrapLinkUp TRAP-TYPE
  ENTERPRISE sniBcam
  VARIABLES  {
    bcamGlobalUpTime,
    bcamGlobalHostName,
    bcamIfTabNumbers,
    bcamIfTabName,
    bcamTrapString
	     }
  DESCRIPTION " This trap will be sent if an interface enters the state active
		(bcamIfAdminState).
		bcamTrapString has the fields:
		$HOST$:       <hostname>
		$UPTIME$:     <bcamuptime>
		$NUMBER$:     <linknumber>
		$NAME$:       <linkname>
		The following values will be sent together with the trap:
		bcamGlobalHostName
		bcamGlobalUpTime
		bcamIfTabNumbers
		bcamIfTabName
		bcamTrapString
	      "
  ::= 305

bcamTrapLinkDown TRAP-TYPE
  ENTERPRISE sniBcam
  VARIABLES  {
    bcamGlobalUpTime,
    bcamGlobalHostName,
    bcamIfTabNumbers,
    bcamIfTabName,
    bcamTrapString
	     }
  DESCRIPTION " This trap will be sent if an interface leaves the state active
		(bcamIfAdminState).
		bcamTrapString has the fields:
		$HOST$:       <hostname>
		$UPTIME$:     <bcamuptime>
		$NUMBER$:     <linknumber>
		$NAME$:       <linkname>
		The following values will be sent together with the trap:
		bcamGlobalHostName
		bcamGlobalUpTime
		bcamIfTabNumbers
		bcamIfTabName
		bcamTrapString
	      "
  ::= 306

bcamTrapSystemUp TRAP-TYPE
  ENTERPRISE sniBcam
  VARIABLES  {
    bcamGlobalUpTime,
    bcamGlobalHostName,
    bcamTrapString
	     }
  DESCRIPTION " This trap will be sent after a successful start of BCAM.
		bcamTrapString has the fields:
		$HOST$:       <hostname>
		$UPTIME$:     <bcamuptime>
		The following values will be sent together with the trap:
		bcamGlobalHostName
		bcamGlobalUpTime
		bcamTrapString
	      "
  ::= 307



-- the Global group

bcamGlobalBcamVersion OBJECT-TYPE
  SYNTAX DisplayString (SIZE(6)) -- ASSLEN: 7 VAR
  ACCESS read-only
  STATUS mandatory
  DESCRIPTION
    "The version of BCAM in the form xx.yy
     xx: mainversion
     yy: subversion"
  ::= { bcamGlobal 1 }
-- SNMPNAME: GBCV
-- ASSNAME: YYY#BCAM

bcamGlobalMibVersion OBJECT-TYPE
  SYNTAX INTEGER
  ACCESS read-only
  STATUS mandatory
  DESCRIPTION
    "Version of BCAM specific MIB (beginning with 1).
     Will be increased with every enhancement of this MIB"
  ::= { bcamGlobal 2 }
-- SNMPNAME: GMBV
-- ASSNAME:

  bcamGlobalUpTime OBJECT-TYPE
    SYNTAX INTEGER
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
      "Time in seconds since start of BCAM"
  ::= { bcamGlobal 3 }
-- SNMPNAME: GBUT
-- ASSNAME: YBCSUTIM



  bcamGlobalPortNonpriv OBJECT-TYPE
    SYNTAX INTEGER -- ASSLEN: 2
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
      "The lowest port number that may be used by non privileged
       applications"
  ::= { bcamGlobal 4 }
-- SNMPNAME: GPNP
-- ASSNAME: YYSPORT#

  bcamGlobalPortFree OBJECT-TYPE
    SYNTAX INTEGER -- ASSLEN: 2
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
      "The lowest port number that will be assigned dynamical by BCAM"
  ::= { bcamGlobal 5 }
-- SNMPNAME: GPNF
-- ASSNAME: YYSPFPO#

  bcamGlobalNumBitmap OBJECT-TYPE
    SYNTAX INTEGER -- ASSLEN: 2
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
      "The number of bitmaps to administer slots in the data slot
       pool. Every bitmap can administer 2048 slots. So the maximum
       number of slots in a pool is bcamGlobalNumBitmap*2048"
  ::= { bcamGlobal 6 }
-- SNMPNAME: GNBM
-- ASSNAME: YFDBITM#

  bcamGlobalMaxRemoteIp OBJECT-TYPE
    SYNTAX INTEGER
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
      "The maximum length in bytes BCAM uses for an NSDU
       for an endsystem that is reached over at least one router"
  ::= { bcamGlobal 7 }
-- SNMPNAME: GMRI
-- ASSNAME: YYSPRIPN



bcamGlobalEsCreationIp OBJECT-TYPE
  SYNTAX INTEGER {
-- ASSMULTI:
	   ipOff(1),            -- ASSVAL: 0 0
	   ipInData(2),         -- ASSVAL: YBTAECID 16
	   ipOutData(4),        -- ASSVAL: YBTAECOD 32
	   ipRoutingProt(8),    -- ASSVAL: YBTAECRP 64
	   ipRoutingData(16)    -- ASSVAL: YBTAECRD 128
		 }
  ACCESS read-only
  STATUS mandatory
  DESCRIPTION
    " The types of IP protocols that will lead to automatic endsystem creation:
      ipOff (1)          : no automatic endsystem creation due to IP protocols.
      ipInData (2)       : automatic endsystem creation due to IP data received.
      ipOutData (4)      : automatic endsystem creation due to IP data sent.
      ipRoutingProt (8)  : automatic endsystem creation due to IP routing protocols.
      ipRoutingData (16) : automatic endsystem creation due to routed IP protocols.
    "
  ::= { bcamGlobal 8 }
-- SNMPNAME: GECP
-- ASSNAME: YBTAECIP

bcamGlobalEsCreationIso OBJECT-TYPE
  SYNTAX INTEGER {
-- ASSMULTI:
	   isoOff(1),          -- ASSVAL: 0 0
	   isoInData(2),       -- ASSVAL: YBTAECID 16
	   isoOutData(4),      -- ASSVAL: YBTAECOD 32
	   isoRoutingProt(8),  -- ASSVAL: YBTAECRP 64
	   isoRoutingData(16)  -- ASSVAL: YBTAECRD 128
		 }
  ACCESS read-only
  STATUS mandatory
  DESCRIPTION
    " The types of ISO 8473 and ISO 9542 protocols that will lead to automatic endsystem creation:
      isoOff (1)          : no automatic endsystem creation due to ISO protocols.
      isoInData (2)       : automatic endsystem creation due to ISO data received.
      isoOutData (4)      : automatic endsystem creation due to ISO data sent.
      isoRoutingProt (8)  : automatic endsystem creation due to ISO routing protocols.
      isoRoutingData (16) : automatic endsystem creation due to routed ISO protocols.
    "
  ::= { bcamGlobal 9 }
-- SNMPNAME: GECS
-- ASSNAME:  YBTAECIS

bcamGlobalBroadcast OBJECT-TYPE
  SYNTAX INTEGER {
-- ASSMULTI:
		 off(1), -- ASSVAL: 0 0
		 on(2)   -- ASSVAL: YBTBROAD 4
		 }
  ACCESS read-only
  STATUS mandatory
  DESCRIPTION
    "Describes how incoming broadcasts will be treated.
     on (2)  : broadcasts will be evaluated and answered if necessary.
     off (1) : broadcasts will be ignored. "
  ::= { bcamGlobal 10 }
-- SNMPNAME: GBRC
-- ASSNAME: YBTIPOPT

bcamGlobalArp OBJECT-TYPE
  SYNTAX INTEGER {
-- ASSMULTI:
		 off(1),   -- ASSVAL: YBTARPNO 128
		 quiet(2), -- ASSVAL: YBTARPQU 64
		 on(4)     -- ASSVAL: YBTARPTI 32
		 }
  ACCESS read-only
  STATUS mandatory
  DESCRIPTION
    "Describes if the address resolution protocol is used.
     off (1)   : no ARP will be used.
     quiet (2) : ARP will be used the first time a route is used and periodically if there exists a
	     transport connection to the system described by this route and there was no data transfer
	     in the last supervision interval.
     on (4)    : ARP will be used the first time a route is used and periodically after the supervision
	     interval."
  ::= { bcamGlobal 11 }
-- SNMPNAME: GARP
-- ASSNAME: YBTARPFG

bcamGlobalRarp OBJECT-TYPE
  SYNTAX INTEGER {
-- ASSMULTI:
		 off(1), -- ASSVAL: 0 0
		 on(2)   -- ASSVAL: YBTRARP 1
		 }
  ACCESS read-only
  STATUS mandatory
  DESCRIPTION
    "Describes if the reverse address resolution protocol will be supported.
     off (1) : RARP not supported.
     on (2)  : RARP supported."
  ::= { bcamGlobal 12 }
-- SNMPNAME: GRAR
-- ASSNAME: YBTIPOPT


bcamGlobalInWaitLimit1 OBJECT-TYPE
  SYNTAX INTEGER
  ACCESS read-write
  STATUS mandatory
  DESCRIPTION
    " The lowest bucket limit for the recording of the input wait times in milliseconds.
      The input wait time is the time interval between the
      announcement of a message to an application and corresponding
      receive call. Every change will reset all counters for the input wait time."
  ::= { bcamGlobal 13 }
-- SNMPNAME: GWL1
-- ASSNAME: YBTMWLI1
-- implemented

bcamGlobalInWaitLimit2 OBJECT-TYPE
  SYNTAX INTEGER
  ACCESS read-write
  STATUS mandatory
  DESCRIPTION
    " The second bucket limit for the recording of the input wait times in milliseconds.
      The input wait time is the time interval between the
      announcement of a message to an application and corresponding
      receive call. Every change will reset all counters for the input wait time."
  ::= { bcamGlobal 14 }
-- SNMPNAME: GWL2
-- ASSNAME: YBTMWLI2
-- implemented

bcamGlobalInWaitLimit3 OBJECT-TYPE
  SYNTAX INTEGER
  ACCESS read-write
  STATUS mandatory
  DESCRIPTION
    " The third bucket limit for the recording of the input wait times in milliseconds.
      The input wait time is the time interval between the
      announcement of a message to an application and corresponding
      receive call. Every change will reset all counters for the input wait time."
  ::= { bcamGlobal 15 }
-- SNMPNAME: GWL3
-- ASSNAME: YBTMWLI3
-- implemented

bcamGlobalInWaitLimit4 OBJECT-TYPE
  SYNTAX INTEGER
  ACCESS read-write
  STATUS mandatory
  DESCRIPTION
    " The highest bucket limit for the recording of the input wait times in milliseconds.
      The input wait time is the time interval between the
      announcement of a message to an application and corresponding
      receive call. Every change will reset all counters for the input wait time."
  ::= { bcamGlobal 16 }
-- SNMPNAME: GWL4
-- ASSNAME: YBTMWLI4
-- implemented

bcamGlobalInWaitChange OBJECT-TYPE
  SYNTAX INTEGER (0..255) -- ASSLEN: 1
  ACCESS read-only
  STATUS mandatory
  DESCRIPTION
    " The number of times the limits of the input wait time buckets have
      been changed."
  ::= { bcamGlobal 17 }
-- SNMPNAME: GWCT
-- ASSNAME: YBTMWCNT
-- implemented

bcamGlobalInWaitSet OBJECT-TYPE
  SYNTAX INTEGER {
-- ASSMULTI:
--                 off(0),
		 on(1),  -- ASSVAL: YBTMAWT 32
		 off(2)  -- ASSVAL: 0 0
		 }
  ACCESS read-write
  STATUS mandatory
  DESCRIPTION
    " The state of counting into the input wait time buckets.
      off (2) : the counting is stopped.
      on  (1) : the counting is running. "
  ::= { bcamGlobal 18 }
-- SNMPNAME: GWST
-- ASSNAME: YBTMESFL
-- implemented


bcamGlobalReactLimit1 OBJECT-TYPE
  SYNTAX INTEGER
  ACCESS read-write
  STATUS mandatory
  DESCRIPTION
    " The lowest bucket limit for the recording of the application reaction times in milliseconds.
      The application reaction time is the time interval between a receive call
      and a directly following send call. Every change will reset all counters for the
      application reaction time. "
  ::= { bcamGlobal 19 }
-- SNMPNAME: GRL1
-- ASSNAME: YBTMRLI1
-- implemented

