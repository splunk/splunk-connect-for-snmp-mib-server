bcamRouteTabRouteNetTyp OBJECT-TYPE
  SYNTAX INTEGER {
-- ASSMULTI:
		 gatewayRouter(1), -- ASSVAL: YRRORNGR 1
		 neaRouter(2),     -- ASSVAL: YRRORNNR 2
