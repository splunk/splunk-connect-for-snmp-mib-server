  SYNTAX INTEGER -- ASSLEN: 4
  ACCESS read-only
  STATUS mandatory
  DESCRIPTION
    " The number of rejected tsap openings because of reached maximum for one task (see bcamTsapMaxTsapTask). "
  ::= { bcamTsap 20 }
-- SNMPNAME: ARTT
-- ASSNAME: YBC#AMTA

bcamTsapRejCepTsap OBJECT-TYPE
  SYNTAX INTEGER -- ASSLEN: 4
  ACCESS read-only
  STATUS mandatory
  DESCRIPTION
    " The number of connection requests rejected because the maximum number of connections for this tsap
      has been reached (see bcamTsapMaxCepTsap). "
  ::= { bcamTsap 21 }
-- SNMPNAME: ARCT
-- ASSNAME: YBC#AMCO

bcamTsapTab OBJECT-TYPE
  SYNTAX SEQUENCE OF TsapEntry
  ACCESS not-accessible
  STATUS mandatory
  DESCRIPTION
    " Table of all active TSAPs "
  ::= { bcamTsap 22 }
-- SNMPNAME:
-- ASSNAME:

bcamTsapTabEntry OBJECT-TYPE
  SYNTAX TsapEntry
  ACCESS not-accessible
  STATUS mandatory
  DESCRIPTION
    " Information about a current active TSAP "
  INDEX {
    bcamTsapTabNumbers
    }
  ::= { bcamTsapTab 1 }
-- SNMPNAME:
-- ASSNAME:




TsapEntry ::=
  SEQUENCE {
    bcamTsapTabNumbers Counter ,
    bcamTsapTabState INTEGER ,
    bcamTsapTabDuration Gauge ,
    bcamTsapTabDateApplEnable DisplayString (SIZE(8)),
    bcamTsapTabTimeApplEnable DisplayString (SIZE(6)),
    bcamTsapTabTypName INTEGER ,
    bcamTsapTabName DisplayString (SIZE(0..78)),
    bcamTsapTabPortnumber INTEGER (0..65535),
    bcamTsapTabOsiTsel OCTET STRING (SIZE(0..32)),
    bcamTsapTabNeaTsel OCTET STRING (SIZE(8)),
    bcamTsapTabHost DisplayString (SIZE(8)),
    bcamTsapTabDiagnostic OCTET STRING (SIZE(4)),
    bcamTsapTabTsduSends Counter ,
    bcamTsapTabByteSends Counter ,
    bcamTsapTabTsduReceiveds Counter ,
    bcamTsapTabByteReceiveds Counter ,
    bcamTsapTabSendCallOverMaxs Counter ,
    bcamTsapTabLetterTimeouts Counter ,
    bcamTsapTabInWaitBuck1Hist INTEGER (0..65535),
    bcamTsapTabInWaitBuck2Hist INTEGER (0..65535),
    bcamTsapTabInWaitBuck3Hist INTEGER (0..65535),
    bcamTsapTabInWaitBuck4Hist INTEGER (0..65535),
    bcamTsapTabInWaitBuck5Hist INTEGER (0..65535),
    bcamTsapTabReactBuck1Hist INTEGER (0..65535),
    bcamTsapTabReactBuck2Hist INTEGER (0..65535),
    bcamTsapTabReactBuck3Hist INTEGER (0..65535),
    bcamTsapTabReactBuck4Hist INTEGER (0..65535),
    bcamTsapTabReactBuck5Hist INTEGER (0..65535),
    bcamTsapTabInWaitBuck1 INTEGER (0..65535),
    bcamTsapTabInWaitBuck2 INTEGER (0..65535),
    bcamTsapTabInWaitBuck3 INTEGER (0..65535),
    bcamTsapTabInWaitBuck4 INTEGER (0..65535),
    bcamTsapTabInWaitBuck5 INTEGER (0..65535),
    bcamTsapTabReactBuck1 INTEGER (0..65535),
    bcamTsapTabReactBuck2 INTEGER (0..65535),
    bcamTsapTabReactBuck3 INTEGER (0..65535),
    bcamTsapTabReactBuck4 INTEGER (0..65535),
    bcamTsapTabReactBuck5 INTEGER (0..65535),
    bcamTsapTabTsduSendHists Counter ,
    bcamTsapTabByteSendHists Counter ,
    bcamTsapTabTsduReceivedHists Counter ,
    bcamTsapTabByteReceivedHists Counter ,
    bcamTsapTabSendCallOverMaxHists Counter ,
    bcamTsapTabLetterTimeoutHists Counter ,
    bcamTsapTabFunction INTEGER ,
    bcamTsapTabCurrConn Gauge ,
    bcamTsapTabCloseConns Counter ,
    bcamTsapTabClessSendBytes Counter ,
    bcamTsapTabClessRecvBytes Counter ,
    bcamTsapTabClessSendCalls Counter ,
    bcamTsapTabClessRecvCalls Counter ,
    bcamTsapTabOutbufTsdu Gauge ,
    bcamTsapTabOutbufByte Gauge ,
    bcamTsapTabInbufTsdu Gauge ,
    bcamTsapTabInbufByte Gauge ,
    bcamTsapTabOutbufTsduCless Gauge ,
    bcamTsapTabOutbufByteCless Gauge ,
    bcamTsapTabInbufTsduCless Gauge ,
    bcamTsapTabInbufByteCless Gauge ,
    bcamTsapTabClessTimeout INTEGER
    }

bcamTsapTabNumbers OBJECT-TYPE
  SYNTAX Counter
  ACCESS read-only
  STATUS mandatory
  DESCRIPTION
    " The definite identifier for this application. Even if the same application is disabled and then
      reenabled, the identfier will get a new value. "
  ::= { bcamTsapTabEntry 1 }
-- SNMPNAME: AANR
-- ASSNAME: YAP2@KEY
-- implemented

bcamTsapTabState OBJECT-TYPE
  SYNTAX INTEGER {
-- ASSSINGLE:
		 opening(1), -- ASSVAL: YAP2OPNG 2
		 open(2),    -- ASSVAL: YAP2OPEN 3
		 closing(3), -- ASSVAL: YAP2CLNG 4
		 closed(4),  -- ASSVAL: YAP2CLOS 1
		 stopped(8)  -- ASSVAL: 0 0
		 }
  ACCESS read-only
  STATUS mandatory
  DESCRIPTION
    " The current state of this application.
      opening (1) : the application enableing is in progress.
      open    (2): the application is ready for requesting or accepting connections.
      closing (3): the termination of the application is in progress.
      closed  (4): the application has been closed.
      Several bits may be set simultaneous."
  ::= { bcamTsapTabEntry 2 }
-- SNMPNAME: ASTA
-- ASSNAME: YAP2STAT
-- implemented

bcamTsapTabDuration OBJECT-TYPE
  SYNTAX Gauge
  ACCESS read-only
  STATUS mandatory
  DESCRIPTION
    " The time in seconds the application was enabled. "
  ::= { bcamTsapTabEntry 3 }
-- SNMPNAME: ADUR
-- ASSNAME: YAP2ODUR
-- implemented

bcamTsapTabDateApplEnable OBJECT-TYPE
  SYNTAX DisplayString (SIZE(8)) -- ASSLEN: 9 VAR
  ACCESS read-only
  STATUS mandatory
  DESCRIPTION
    " The date of the application enableing.
      Format: YYYYMMDD "
  ::= { bcamTsapTabEntry 4 }
-- SNMPNAME: ADAE
-- ASSNAME: YAP2DATE
-- implemented

bcamTsapTabTimeApplEnable OBJECT-TYPE
  SYNTAX DisplayString (SIZE(6)) -- ASSLEN: 7 VAR
  ACCESS read-only
  STATUS mandatory
  DESCRIPTION
    " The time of the application enableing.
      Format: HHMMSS "
  ::= { bcamTsapTabEntry 5 }
-- SNMPNAME: ATAE
-- ASSNAME: YAP2TIME
-- implemented

bcamTsapTabTypName OBJECT-TYPE
  SYNTAX INTEGER {
-- ASSSINGLE:
		 neaName(1),    -- ASSVAL: YAP2NAPP 2
		 socketName(2), -- ASSVAL: YAP2NPT# 6
		 isoName(3)     -- ASSVAL: YAP2NISO 7
		 }
  ACCESS read-only
  STATUS mandatory
  DESCRIPTION
    " The type of name of this application.
      NeaName    (1) : the name consists of 8 byte application name and 8 byte name of endsystem.
      SocketName (2) : the name consists of up to 32 byte socket name.
      IsoName    (3) : the name consists of up to 78 byte iso name. "
  ::= { bcamTsapTabEntry 6 }
-- SNMPNAME: ATNM
-- ASSNAME: YAP2NTYP
-- implemented

bcamTsapTabName OBJECT-TYPE
  SYNTAX DisplayString (SIZE(0..78)) -- ASSLEN: 79 VAR
  ACCESS read-only
  STATUS mandatory
  DESCRIPTION
    " The name of this application according the type of the name (bcamTsapTabTypName). "
  ::= { bcamTsapTabEntry 7 }
-- SNMPNAME: ANAM
-- ASSNAME: YAP2NNAI
-- implemented

bcamTsapTabPortnumber OBJECT-TYPE
  SYNTAX INTEGER (0..65535) -- ASSLEN: 2
  ACCESS read-only
  STATUS mandatory
  DESCRIPTION
    " The port number used by this application (if known). "
