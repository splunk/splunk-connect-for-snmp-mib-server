-- ASSNAME: YBC#CPAF


bcamCepNumTable OBJECT-TYPE
  SYNTAX INTEGER
  ACCESS read-only
  STATUS mandatory
  DESCRIPTION
    "The number of table entries. "
  ::= { bcamCep 9 }
-- SNMPNAME: CNTB
-- ASSNAME: YBCC7OCC
-- implemented

  bcamCepTab OBJECT-TYPE
    SYNTAX SEQUENCE OF ConnEntry
    ACCESS not-accessible
    STATUS mandatory
    DESCRIPTION
      "Table of all active connections"
  ::= { bcamCep 10 }
-- SNMPNAME:
-- ASSNAME:

  bcamCepTabEntry OBJECT-TYPE
    SYNTAX ConnEntry
    ACCESS not-accessible
    STATUS mandatory
    DESCRIPTION
      "Information about a connection"
    INDEX {
	  bcamCepTabConnectionNumbers
	  }
  ::= { bcamCepTab 1 }
-- SNMPNAME:
-- ASSNAME:

ConnEntry ::=
  SEQUENCE {
    bcamCepTabProtocolClass INTEGER ,
    bcamCepTabConnectionNumbers Counter ,
    bcamCepTabDisconCommand OCTET STRING (SIZE(4)),
    bcamCepTabDisconInfoWord OCTET STRING (SIZE(4)),
    bcamCepTabTsduSends Counter ,
    bcamCepTabByteSends Counter ,
    bcamCepTabTsduReceiveds Counter ,
    bcamCepTabByteReceiveds Counter ,
    bcamCepTabSendCallOverMaxs Counter ,
    bcamCepTabLetterTimeouts Counter ,
    bcamCepTabOutbufTsduSend Gauge ,
    bcamCepTabOutbufByteSend Gauge ,
    bcamCepTabOutbufMaxTsduSend Gauge ,
    bcamCepTabOutbufMaxByteSend Gauge ,
    bcamCepTabInbufTsdu Gauge ,
    bcamCepTabInbufByte Gauge ,
    bcamCepTabInbufMaxTsduReceived Gauge ,
    bcamCepTabInbufMaxByteReceived Gauge ,
    bcamCepTabPacketsDataSends Counter ,
    bcamCepTabPacketsWindowSends Counter ,
    bcamCepTabPacketsDataReceiveds Counter ,
    bcamCepTabPacketsWindowReceiveds Counter ,
    bcamCepTabGlobalZeroWindowSends Counter ,
    bcamCepTabConnectionZeroWindowSends Counter ,
    bcamCepTabZeroWindowReceiveds Counter ,
    bcamCepTabRoundTripTime INTEGER (0..65535),
    bcamCepTabRetransmitPacketsSends Counter ,
    bcamCepTabDetectedGapsReceiveds Counter ,
    bcamCepTabDuplicatePacketsReceiveds Counter ,
    bcamCepTabErrorPacketsReceiveds Counter ,
    bcamCepTabConnectionState INTEGER ,
    bcamCepTabApplicationNumber INTEGER ,
    bcamCepTabRouteNumber INTEGER ,
    bcamCepTabDuration Gauge ,
    bcamCepTabDateConnectionEstablishment DisplayString (SIZE(8)),
    bcamCepTabTimeConnectionEstablishment DisplayString (SIZE(6)),
    bcamCepTabTypPartnerName INTEGER ,
    bcamCepTabPartnerName DisplayString (SIZE(0..78)),
    bcamCepTabLocalName DisplayString (SIZE(0..78)),
    bcamCepTabTypeL4Addr INTEGER ,
    bcamCepTabL4AddrPartner OCTET STRING (SIZE(0..32)),
    bcamCepTabL4AddrLocal OCTET STRING (SIZE(0..32)),
    bcamCepTabPartnerEndsystem DisplayString (SIZE(8)),
    bcamCepTabInWaitBuck1 INTEGER (0..65535),
    bcamCepTabInWaitBuck2 INTEGER (0..65535),
    bcamCepTabInWaitBuck3 INTEGER (0..65535),
    bcamCepTabInWaitBuck4 INTEGER (0..65535),
    bcamCepTabInWaitBuck5 INTEGER (0..65535),
    bcamCepTabReactBuck1 INTEGER (0..65535),
    bcamCepTabReactBuck2 INTEGER (0..65535),
    bcamCepTabReactBuck3 INTEGER (0..65535),
    bcamCepTabReactBuck4 INTEGER (0..65535),
    bcamCepTabReactBuck5 INTEGER (0..65535),
    bcamCepTabOutProcBuck1 INTEGER (0..65535),
    bcamCepTabOutProcBuck2 INTEGER (0..65535),
    bcamCepTabOutProcBuck3 INTEGER (0..65535),
    bcamCepTabOutProcBuck4 INTEGER (0..65535),
    bcamCepTabOutProcBuck5 INTEGER (0..65535),
    bcamCepTabInProcBuck1 INTEGER (0..65535),
    bcamCepTabInProcBuck2 INTEGER (0..65535),
    bcamCepTabInProcBuck3 INTEGER (0..65535),
    bcamCepTabInProcBuck4 INTEGER (0..65535),
    bcamCepTabInProcBuck5 INTEGER (0..65535),
    bcamCepTabMaxSendLen INTEGER ,
    bcamCepTabMaxIndLen INTEGER ,
    bcamCepTabLocalEndsystem DisplayString (SIZE(8))
    }

bcamCepTabProtocolClass OBJECT-TYPE
  SYNTAX INTEGER {
-- ASSMULTI:
		 local(1), -- ASSVAL: YC1LOCCN 2
		 nea(2),   -- ASSVAL: YC1NEACN 32
		 iso(3),   -- ASSVAL: YC1ISOCN 64
		 tcp(4)    -- ASSVAL: YC1TCPCN 128
		 }
  ACCESS read-only
  STATUS mandatory
  DESCRIPTION
    " The protocol class used for this connection.
      local (1) : the partner application resides in the same host.
      nea   (2) : the proprietary NEA protocol is used as transport protocol.
      iso   (3) : the ISO protocol is used as transport protocol.
      tcp   (4) : the TCP protocol is used as transport protocol."
  ::= { bcamCepTabEntry 1 }
-- SNMPNAME: CPCL
-- ASSNAME: YC1PRTCL
-- implemented
-- how about convergence protocols ?

