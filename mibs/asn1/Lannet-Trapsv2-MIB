-- 1.3.6.1.4.1.81.38.10008
        lntNotificationAgentCapabilities OBJECT IDENTIFIER ::= { lntTraps 10008 }
        
--      1.3.6.1.4.1.81.38.10008.1
        lntP330L3NotificationAgentCapabilities AGENT-CAPABILITIES
PRODUCT-RELEASE 
            ""
            STATUS current
            DESCRIPTION 
                "Trap Capabilities of P330 L3 Device."
            SUPPORTS Lannet-Trapsv2-MIB
                INCLUDES { lntSecurityNotificationGroup, lntConfigChangeVarbinds, lntLagNotificationGroup, lntSWRedundancyNotificationGroup, lntPSUFaultNotificationGroup, 
                    lntL3FaultNotificationGroup, lntLinkDownNotificationGroup, lntCascadeNotificationGroup, lntEthPortNotificationGroup, lntCAMNotificationGroup, 
                    lntConfigChangeNotificationGroup, lntPolicyNotificationGroup, lntFansNotificationGroup }
            ::= { lntNotificationAgentCapabilities 1 }
        
