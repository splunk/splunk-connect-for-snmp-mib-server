-- Home Agent MIP Interface Events Settings Group
	usrMipHaUpperThreshold OBJECT-TYPE
		SYNTAX INTEGER (1..100)
		MAX-ACCESS read-write
		STATUS current
		DESCRIPTION
       			"Specifies the upper Water Mark for Active Session
                         Threshold. If the active call capacity exceeds this
                         limit, then a trap is sent."
		--CONFIGURABLE
	::= { usrMipHaTrapsSettings 1 }
	usrMipHaLowerThreshold OBJECT-TYPE
		SYNTAX INTEGER (1..100)
		MAX-ACCESS read-write
		STATUS current
		DESCRIPTION
       			"Specifies the lower Water Mark for Active Session
                         Threshold. If the active call capacity touches 
                         this lower threshold after touching the Upper
                         threshold, then only a trap will be sent whenever
                         next time it exceeds the Upper Threshold."
		--CONFIGURABLE
	::= { usrMipHaTrapsSettings 2 }
	usrMipHaTrapThreshold OBJECT-TYPE
   		SYNTAX INTEGER {
                    enabled(1),
                    disabled(2)
                    }
   		MAX-ACCESS  read-write
   		STATUS  current
   		DESCRIPTION
	        "Threshold Trap Enabled or Disabled."
        --CONFIGURABLE
	::= { usrMipHaTrapsSettings 3 }
	usrMipHaTrapIdleTimer OBJECT-TYPE
   		SYNTAX INTEGER {
                    enabled(1),
                    disabled(2)
                    }
   		MAX-ACCESS  read-write
   		STATUS  current
   		DESCRIPTION
	        "Idle Timer Trap Enabled or Disabled."
        --CONFIGURABLE
	::= { usrMipHaTrapsSettings 4 }
-- Home Agent MIP Interface Events Group
    usrMipHaTrapsV2 OBJECT-IDENTITY
        STATUS  current
        DESCRIPTION "The Traps for MIP"
        ::= {usrMipHaTraps 0}
	usrMipHaActiveSessionThresholdReached NOTIFICATION-TYPE
		OBJECTS {    
                    usrMipHaUpperThreshold,
                    usrMipHaLowerThreshold,
	                usrMipHaCurrentSessions
		}
        STATUS  current
		DESCRIPTION
			"This trap is generated when Active Session Threshold 
             is reached based on the calculation -
             (ActiveSessions * 100) / MaxTunnels being greater
             than the UpperThreshold."
	    --#TYPE	"Interface Capacity Alarm"
	    --#SUMMARY	"Total Sessions %d reached upper threshold of %d"
	    --#ARGUMENTS { 2 0 }
	    --#SEVERITY MAJOR
	    --#STATE	OPERATIONAL
	    --#PROBABLECAUSE "resourceAtOrNearingCapacity"
        ::= {usrMipHaTrapsV2 1}
	usrMipHaIdleTimerExpired NOTIFICATION-TYPE
		OBJECTS {    
		        usrMipHaMobilityBindingMN
		}
        STATUS  current
		DESCRIPTION
			"This trap is generated when the Idle Timer has expired
             and the tunnel has been disconnected. The trap contains
             the MobileNode's home IP address."
	    --#TYPE	"Timeout"
	    --#SUMMARY	"Tunnel disconnected for MN %d"
	    --#ARGUMENTS { 0 }
	    --#SEVERITY MINOR
	    --#STATE	OPERATIONAL
	    --#PROBABLECAUSE "communicationsProblem"
        ::= {usrMipHaTrapsV2 2}
-- Home Agent status
	usrMipHaStatusHacnInterfaceStatus OBJECT-TYPE
		SYNTAX DisplayString
		MAX-ACCESS read-only
		STATUS current
   		DESCRIPTION
       		"HACN Interface status"
	::= { usrMipHaStatus 1 }
	usrMipHaStatusHaRole OBJECT-TYPE
		SYNTAX DisplayString
		MAX-ACCESS read-only
		STATUS current
   		DESCRIPTION
       		"HA Role. This makes sense only if HACN interface is enabled. If
            HACN interface is not enabled, then HA role will be unassigned."
	::= { usrMipHaStatus 2 }
	usrMipHaStatus2EthPortStatus OBJECT-TYPE
		SYNTAX DisplayString
		MAX-ACCESS read-only
		STATUS current
   		DESCRIPTION
       		"Status of second ethernet port. If both port are in use and
            second ethernet port goes down then HA stops accepting reg req."
	::= { usrMipHaStatus 3 }
	usrMipHaStatusHaRunIp OBJECT-TYPE
		SYNTAX IpAddress
		MAX-ACCESS read-only
		STATUS deprecated
		DESCRIPTION
			"Home Agent RUN IP Address.
			DEPRECATED:
			This object has been replaced by usrMipHacnSettingsHaRunIp"
	::= { usrMipHaStatus 4 }
	usrMipHaStatusHacnResponding OBJECT-TYPE
		SYNTAX DisplayString
		MAX-ACCESS read-only
		STATUS current
   		DESCRIPTION
       		"This indicates whether HACN is responding to heartbeats or not."
	::= { usrMipHaStatus 5 }
	usrMipHaRunIPNetworkStatus OBJECT-TYPE
   		SYNTAX INTEGER {
                    enabled(1),
                    disabled(2)
                    }
   		MAX-ACCESS  read-write
   		STATUS  current
   		DESCRIPTION
            "HA RunIP Network added on HA Enabled or Disabled.
             If the HA role is not active then no action, enabled
             or disabled, is allowed; an error will be returned.
             An error will also be returned if an attempt is made
             to enable/disable and active calls exist."
        --CONFIGURABLE
	::= { usrMipHaStatus 6 }
-- Home Agent Control Node settings
	usrMipHacnSettingsHaRunIp OBJECT-TYPE
		SYNTAX IpAddress
		MAX-ACCESS read-only
		STATUS current
		DESCRIPTION
			"Home Agent RUN IP Address."
	::= { usrMipHacnSettings 1 }
	usrMipHacnSettingsHeartbeatPeriod OBJECT-TYPE
		SYNTAX Unsigned32
		MAX-ACCESS read-only
		STATUS current
		DESCRIPTION
			"Heartbeat period set by HACN. This value is set in HACN and is
            sent to HA during Role Assignment and in each Heartbeat ACK. This
            is the time period after which HA is supposed to send a heartbeat
            message to HACN."
	::= { usrMipHacnSettings 2 }
	usrMipHacnSettingsPacketRetryPeriod OBJECT-TYPE
		SYNTAX Unsigned32
		MAX-ACCESS read-only
		STATUS current
		DESCRIPTION
			"Packet Retry period set by HACN. This value is set in HACN and is
            sent to HA during Role Assignment and in each Heartbeat ACK. This
            is the time period after which HA is supposed to retry a message
            in case HA does not gets a ack for the previous message."
	::= { usrMipHacnSettings 3 }
	usrMipHacnSettingsPacketRetryCount OBJECT-TYPE
		SYNTAX Unsigned32
		MAX-ACCESS read-only
		STATUS current
		DESCRIPTION
			"Packet Retry count set by HACN. This value is set in HACN and is
            sent to HA during Role Assignment and in each Heartbeat ACK. This
            is the number of times a retry message will be sent to HACN."
	::= { usrMipHacnSettings 4 }
-- Home Agent Control Node statistics
	usrMipHacnStatsHeartbeatSent OBJECT-TYPE
		SYNTAX Unsigned32
		MAX-ACCESS read-only
		STATUS current
		DESCRIPTION
			"Total Number of heartbeats sent to HACN."
	::= { usrMipHacnStats 1 }
	usrMipHacnStatsHeartbeatAckRcvd OBJECT-TYPE
		SYNTAX Unsigned32
		MAX-ACCESS read-only
		STATUS current
		DESCRIPTION
			"Total Number of heartbeat ack received from HACN."
	::= { usrMipHacnStats 2 }
	usrMipHacnStatsHeartbeatRetried OBJECT-TYPE
		SYNTAX Unsigned32
		MAX-ACCESS read-only
		STATUS current
		DESCRIPTION
			"Total Number of heartbeats retried."
	::= { usrMipHacnStats 3 }
    usrMipHacnStatsResetTime OBJECT-TYPE
        SYNTAX	Unsigned32 
		MAX-ACCESS  read-write
		STATUS  current
		DESCRIPTION
                "Reading:
				Time the HACN statistics counters were last 
			 	reset. The HACN statistics are valid from 
				usrMipHacnStatsResetTime until present.
	        		Resetting counters causes a discontinuity in 
				the monotonically increasing value of counters.
				Management stations that sample these counters
				need also to sample this object so that they 
				can know that a discontinuity occurred with 
				respect to the previous sample.  
                Its value is in NTP format.
		        Writing:
		   	    Writing 0 causes update of usrMipHacnStatsResetTime
				and the HACN counters to be reset to 0."
	::= { usrMipHacnStats 4 }
-- Mobile Node Information
        usrMobileInfoTable OBJECT-TYPE
		SYNTAX SEQUENCE OF UsrMobileInfoEntry
		MAX-ACCESS not-accessible
		STATUS current
		DESCRIPTION
                        "A table containing Mobile Node Information."
	::= {usrMipHaMobileInfo 1 }
	usrMobileInfoEntry OBJECT-TYPE
		SYNTAX UsrMobileInfoEntry
		MAX-ACCESS not-accessible
		STATUS current
		DESCRIPTION
			"An entry in the Mobile Node Information list."
		INDEX { usrMipHaMobilityBindingIndex }
	::= { usrMobileInfoTable 1 }
        UsrMobileInfoEntry ::= SEQUENCE {
                usrMipHaMobilePacketOut    Unsigned32,
                usrMipHaMobilePacketIn     Unsigned32,
                usrMipHaMobileByteOut      Unsigned32,
                usrMipHaMobileByteIn       Unsigned32
        }
	usrMipHaMobilePacketOut OBJECT-TYPE
		SYNTAX Unsigned32
		MAX-ACCESS read-only
		STATUS current
		DESCRIPTION
			"Number of packets sent out on the tunnel."
	::= {usrMobileInfoEntry 1 }
	usrMipHaMobilePacketIn OBJECT-TYPE
		SYNTAX Unsigned32
		MAX-ACCESS read-only
		STATUS current
		DESCRIPTION
			"Number of packets received from the tunnel."
	::= {usrMobileInfoEntry 2 }
	usrMipHaMobileByteOut OBJECT-TYPE
		SYNTAX Unsigned32
		MAX-ACCESS read-only
		STATUS current
		DESCRIPTION
			"Number of data bytes sent out on the tunnel."
	::= {usrMobileInfoEntry 3 }
	usrMipHaMobileByteIn OBJECT-TYPE
		SYNTAX Unsigned32
		MAX-ACCESS read-only
		STATUS current
		DESCRIPTION
			"Number of data bytes received from the tunnel."
	::= {usrMobileInfoEntry 4 }
END	-- USR-MIP-MIB Definitions
