-- More statistics counter after Home agent binding list
-- Home Agent mobility binding list
	usrMipHaMobilityBindingTable OBJECT-TYPE
		SYNTAX SEQUENCE OF UsrMipHaMobilityBindingEntry
		MAX-ACCESS not-accessible
		STATUS current
		DESCRIPTION
			"A table containing the home agent's mobility
			 binding list.  The home agent updates this
			 table in response to registration events from
			 mobile nodes."
	::= { usrMipHaStats 18 }
	usrMipHaMobilityBindingEntry OBJECT-TYPE
		SYNTAX UsrMipHaMobilityBindingEntry
		MAX-ACCESS not-accessible
		STATUS current
		DESCRIPTION
			"An entry in the mobility binding list."
		INDEX { usrMipHaMobilityBindingIndex }
	::= { usrMipHaMobilityBindingTable 1 }
	UsrMipHaMobilityBindingEntry ::= SEQUENCE {
		usrMipHaMobilityBindingIndex         INTEGER,
		usrMipHaMobilityBindingMN            IpAddress,
		usrMipHaMobilityBindingCOA           IpAddress,
		usrMipHaMobilityBindingRegFlags      INTEGER,
		usrMipHaMobilityBindingRegIDLow      UsrSpecialU32,
		usrMipHaMobilityBindingRegIDHigh     UsrSpecialU32,
		usrMipHaMobilityBindingTimeGranted   Integer32,
		usrMipHaMobilityBindingTimeRemaining Gauge32,
		usrMipHaMobilityBindingMtu           Unsigned32,
		usrMipHaMobilityBindingNAI           DisplayString,
        usrMipHaMobilitySetupTime            Unsigned32,
        usrMipHaMobilityBindingNextHop       IpAddress
		}
	usrMipHaMobilityBindingIndex OBJECT-TYPE
		SYNTAX INTEGER (1..10000)
		MAX-ACCESS read-only
		STATUS current
		DESCRIPTION
			"Index of the entry in the Mobility Binding Table."
	::= { usrMipHaMobilityBindingEntry 1 }
	usrMipHaMobilityBindingMN OBJECT-TYPE
		SYNTAX IpAddress
		MAX-ACCESS read-only
		STATUS current
		DESCRIPTION
			"Mobile node's home (IP) address."
	::= { usrMipHaMobilityBindingEntry 2 }
	usrMipHaMobilityBindingCOA OBJECT-TYPE
		SYNTAX IpAddress
		MAX-ACCESS read-only
		STATUS current
		DESCRIPTION
			"Mobile node's care-of-address.  One mobile node
			 can have multiple bindings with different
			 care-of-addresses."
	::= { usrMipHaMobilityBindingEntry 3 }
	usrMipHaMobilityBindingRegFlags OBJECT-TYPE
		SYNTAX INTEGER (0..255)
		MAX-ACCESS read-only
		STATUS current
		DESCRIPTION
			"Registration flags sent by mobile node.
			 This is a bit-mapped value with the following
			 representation:
			   7   6   5   4   3   2   1   0
			 +---+---+---+---+---+---+---+---+
			 | S | B | D | M | G | V | T | R |
			 +---+---+---+---+---+---+---+---+
			 where,
			 S - Simultaneous Bindings
			 B - Broadcast Datagrams
			 D - Decapsulation by mobile node
			 M - Minimal Encapsulation
			 G - GRE Encapsulation
			 V - V.J. Compression
			 T - Reverse Tunneling
			 R - Reserved Bit"
	::= { usrMipHaMobilityBindingEntry 4 }
	usrMipHaMobilityBindingRegIDLow OBJECT-TYPE
		SYNTAX UsrSpecialU32
		MAX-ACCESS read-only
		STATUS current
		DESCRIPTION
			"Low 32 bits of Identification used in that
			 binding by the mobile node."
	::= { usrMipHaMobilityBindingEntry 5 }
	usrMipHaMobilityBindingRegIDHigh OBJECT-TYPE
		SYNTAX UsrSpecialU32
		MAX-ACCESS read-only
		STATUS current
		DESCRIPTION
			"High 32 bits of Identification used in that
			 binding by the mobile node."
	::= { usrMipHaMobilityBindingEntry 6 }
	usrMipHaMobilityBindingTimeGranted OBJECT-TYPE
		SYNTAX Integer32
		UNITS "seconds"
		MAX-ACCESS read-only
		STATUS current
		DESCRIPTION
			"The lifetime in seconds granted to the mobile
			 node for this registration."
	::= { usrMipHaMobilityBindingEntry 7 }
	usrMipHaMobilityBindingTimeRemaining OBJECT-TYPE
		SYNTAX Gauge32
		UNITS "seconds"
		MAX-ACCESS read-only
		STATUS current
		DESCRIPTION
			"The number of seconds remaining until the
			 registration is expired. It has the same initial
			 value as usrMipHaMobilityBindingTimeGranted,
			 and is counted down by the home agent."
	::= {usrMipHaMobilityBindingEntry 8 }
	usrMipHaMobilityBindingMtu OBJECT-TYPE
		SYNTAX Unsigned32
		MAX-ACCESS read-only
		STATUS current
		DESCRIPTION
			"The size of the MTU."
	::= {usrMipHaMobilityBindingEntry 9 }
	usrMipHaMobilityBindingNAI OBJECT-TYPE
		SYNTAX DisplayString
		MAX-ACCESS read-only
		STATUS current
		DESCRIPTION
			"User name ( NAI )."
	::= {usrMipHaMobilityBindingEntry 10 }
	usrMipHaMobilitySetupTime OBJECT-TYPE
		SYNTAX Unsigned32
		MAX-ACCESS read-only
		STATUS current
		DESCRIPTION
			"Actual time when the tunnel was setup on this HA. Time is in NTP 
            format."
	::= {usrMipHaMobilityBindingEntry 11 }
    usrMipHaMobilityBindingNextHop OBJECT-TYPE
		SYNTAX IpAddress
		MAX-ACCESS read-only
		STATUS current
		DESCRIPTION
			"Next hop gateway address returned by AAA, used for VLAN tagging
            feature."
	::= { usrMipHaMobilityBindingEntry 14 }
