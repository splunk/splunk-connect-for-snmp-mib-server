USR-MIP-MIB DEFINITIONS ::= BEGIN
	IMPORTS
		OBJECT-TYPE,
        MODULE-IDENTITY,
		Integer32,
		Counter32,
		IpAddress,
		Unsigned32,
		Gauge32
			FROM SNMPv2-SMI
		RowStatus,
		TruthValue,
        DisplayString
			FROM SNMPv2-TC
		NOTIFICATION-TYPE
			FROM SNMPv2-SMI
		OBJECT-IDENTITY
			FROM SNMPv2-SMI
        common,
		UsrConfigStatus,
        UsrSecretDisplay,
        UsrSpecialU32
			FROM USR-DEFINITIONS-MIB;
usrMIP  MODULE-IDENTITY
        LAST-UPDATED "200301221300Z"
        ORGANIZATION "CommWorks Corporation, a 3Com Company"
        CONTACT-INFO "Customer Support"
        DESCRIPTION   "Added usrMipHaSendICMPforPMTUDiscovery"
--      REVISION      "200301221300Z"
--      DESCRIPTION   "Added usrMipMultipleLoginsStatus,
--         usrMipHaDuplicateNaiDiscs, usrMipHaClearMBRDiscs."
--      REVISION      "200211061300Z"
--      DESCRIPTION   "Added usrMipHaDuplicateIPDiscs"
--      REVISION      "200211011330Z"
--      DESCRIPTION   "Added usrMipHaGarpUponRereg"
--      REVISION      "200208301200Z"
--      DESCRIPTION   "Added usrMipHaGarpUponRereg"
--      REVISION      "200208231825Z"
--      DESCRIPTION   "Clarified usage of objects usrMipHaTunnelDeleteAll,
--         usrMipHaTunDelByNAI, usrMipHaMnSaIndexKey, usrMipHaFaSaKey,
--         usrMipHaHacnSecret, usrMipHaMobilityBindingRegIDLow, 
--         usrMipHaMobilityBindingRegIDHigh, usrMipHaMobilityBindingNAI,
--         usrMipHaHacnStatus, usrMipHaIdleTimerStatus,
--         usrMipHaUsingBothPorts, usrMipHaBroadcastBit, usrMipHaStatusIPSec,
--         usrMipHaTrapThreshold, usrMipHaTrapIdleTimer, 
--         usrMipHaRunIPNetworkStatus."
--      REVISION      "200208151730Z"
--      DESCRIPTION   "Added structured trap comments"
--      REVISION      "200208021630Z"
--      DESCRIPTION   "Corrected definitions of usrMipHaMnSaIndexKey  
--                     and usrMipHaFaSaKey"
--      REVISION      "200208011400Z"
--      DESCRIPTION   "Added usrMipMNHAKeyPadding"
--      REVISION      "200207181445Z"
--      DESCRIPTION   "obsoleted usrMipHaTunnelByAddr"
--      REVISION      "200205051330Z"
--      DESCRIPTION   "Created MODULE-IDENTITY for Mobile IP MIB"
        ::= { common 82 }
	usrMipHaConfig           OBJECT IDENTIFIER ::= { usrMIP 1 }
	usrMipHaStats            OBJECT IDENTIFIER ::= { usrMIP 2 }
	usrMipHaCfmConfig        OBJECT IDENTIFIER ::= { usrMIP 3 }
