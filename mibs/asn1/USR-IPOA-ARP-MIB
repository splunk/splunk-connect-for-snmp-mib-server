	::= { usrMipHaConfig 21 }
    usrMipHaStatsResetTime OBJECT-TYPE
        SYNTAX	Unsigned32 
		MAX-ACCESS  read-write
		STATUS  current
		DESCRIPTION
                "Reading:
				Time the HA statistics counters were last 
			 	reset. The HA statistics are valid from 
				usrMipHAStatsResetTime until present.
	        		Resetting counters causes a discontinuity in 
				the monotonically increasing value of counters.
				Management stations that sample these counters
				need also to sample this object so that they 
				can know that a discontinuity occurred with 
				respect to the previous sample.  
                Its value is in NTP format.
		        Writing:
		   	    Writing 0 causes update of usrMipHAStatsResetTime
				and the HA counters to be reset to 0."
	::= { usrMipHaConfig 22 }
	usrMipHaUsingBothPorts OBJECT-TYPE
		SYNTAX INTEGER {
                    enabled(1),
                    disabled(2)
                    }
		MAX-ACCESS read-write
		STATUS current
		DESCRIPTION
   			"Specifies if HA is using both ports. Port 1 (eth:1) will
             always be used for interfacing with PDSN/FA/HACN"
		--CONFIGURABLE
	::= { usrMipHaConfig 23 }
	usrMipHaTunDelByNAI OBJECT-TYPE
		SYNTAX     DisplayString 
		MAX-ACCESS read-write
		STATUS current
		DESCRIPTION
			"Delete tunnel(s) specified by this NAI. Reading this
             value returns NULL."
	::= { usrMipHaConfig 24 }
	usrMipHaBroadcastBit OBJECT-TYPE
		SYNTAX INTEGER {
                    enabled(1),
                    disabled(2)
                    }
		MAX-ACCESS read-write
		STATUS current
		DESCRIPTION
   			"This flag specifies if HA should accept a registration
             request with B bit set in registration flags or not. By
             default this bit is set and HA will accept reg req with 
             B bit set."
		--CONFIGURABLE
	::= { usrMipHaConfig 25 }
	usrMipHaNAICheck OBJECT-TYPE
		SYNTAX INTEGER {
                    enabled(1),
                    disabled(2)
                    }
		MAX-ACCESS read-write
		STATUS current
		DESCRIPTION
   			"Specifies if HA is checking NAI for re-registration requests
             or not. If it is enable then NAI is checked for each
             Re-Registration Requests otherwise NAI is not checked. By
             default NAI check is enabled."
		--CONFIGURABLE
	::= { usrMipHaConfig 26 }
	usrMipHaFaSaTable OBJECT-TYPE
		SYNTAX SEQUENCE OF UsrMipHaFaSaEntry
		MAX-ACCESS not-accessible
		STATUS current
		DESCRIPTION
			"A table for configuring FA security
			 associations between PDSN/FA and the
			 Home Agent."
	::= { usrMipHaConfig 27 }
	usrMipHaFaSaEntryIndex OBJECT-TYPE
		SYNTAX UsrMipHaFaSaEntry
		MAX-ACCESS not-accessible
		STATUS current
		DESCRIPTION
			"An entry in the FA-HA Security Association
			 Table."
		INDEX { usrMipHaFaSaIndexNum }
	::= { usrMipHaFaSaTable 1 }
	UsrMipHaFaSaEntry ::= SEQUENCE {
		usrMipHaFaSaIndexNum    INTEGER,
		usrMipHaFaSaIpAddress   IpAddress,
		usrMipHaFaSaSpi         Unsigned32,
		usrMipHaFaSaKey         UsrSecretDisplay,
		usrMipHaFaSaStatus      RowStatus
	}
	usrMipHaFaSaIndexNum OBJECT-TYPE
		SYNTAX INTEGER (1..100)
		MAX-ACCESS read-only
		STATUS current
		DESCRIPTION
			"Home Agent can support up to 100 FA-HA
			 security associations."
	::= { usrMipHaFaSaEntryIndex 1 }
	usrMipHaFaSaIpAddress OBJECT-TYPE
		SYNTAX IpAddress 
		MAX-ACCESS read-write
		STATUS current
		DESCRIPTION
			"The IP address of PDSN/FA."
		--CONFIGURABLE
	::= { usrMipHaFaSaEntryIndex 2 }
	usrMipHaFaSaSpi OBJECT-TYPE
		SYNTAX Unsigned32 (256..4294967295) 
		MAX-ACCESS read-write
		STATUS current
		DESCRIPTION
			"The SPI is the 4-byte opaque index within
			 the FA-HA Security Association which
			 selects the specific security parameters
			 to be used to authenticate the peer."
		--CONFIGURABLE
	::= { usrMipHaFaSaEntryIndex 3 }
	usrMipHaFaSaKey OBJECT-TYPE
		SYNTAX     UsrSecretDisplay
		MAX-ACCESS read-write
		STATUS current
		DESCRIPTION
			"The shared secret key for the security associations."
		--CONFIGURABLE
	::= { usrMipHaFaSaEntryIndex 4 }
	usrMipHaFaSaStatus OBJECT-TYPE
		SYNTAX RowStatus
		MAX-ACCESS read-write
		STATUS current
		DESCRIPTION
			"This object is used to add, modify, and delete
			 rows for the security association table."
		--CONFIGURABLE
	::= { usrMipHaFaSaEntryIndex 5 }
