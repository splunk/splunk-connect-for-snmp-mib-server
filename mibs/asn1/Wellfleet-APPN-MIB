    		INTEGER,
    	    wfAppnNodeEndptSessionPolarity
    		INTEGER,
    	    wfAppnNodeEndptSessionContention
    		INTEGER,
    	    wfAppnNodeEndptSessionRscv
    		OCTET STRING,
    	    wfAppnNodeEndptSessionSendRuSize
    		INTEGER,
    	    wfAppnNodeEndptSessionRcvRuSize
    		INTEGER,
    	    wfAppnNodeEndptSessionMaxSendBtuSize
