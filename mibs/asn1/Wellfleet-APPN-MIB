        DEFVAL  { uplink }
        ::= { wfAppnNodeLsEntry 36 }

     wfAppnNodeLsAdjBrNNLinkSupp OBJECT-TYPE
        SYNTAX  INTEGER {
                allowed(1),
                required(2),
                prohibited(3)
                        }
        ACCESS  read-write
        STATUS  mandatory
        DESCRIPTION
                " Specifies whether the adjacent node is required, allowed or 
                  prohibited from being a BrNN ( i.e. an BrNN showing its NN face )."                                                           
        DEFVAL  { allowed }
        ::= { wfAppnNodeLsEntry 37 }

      wfAppnNodeLsDefaultNNS OBJECT-TYPE
        SYNTAX  INTEGER {
	            yes(1),
    		    no(2)        
        }
        ACCESS  read-write
