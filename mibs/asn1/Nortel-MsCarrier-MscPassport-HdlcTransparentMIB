mscHtdsLCoPathDataTable OBJECT-TYPE
   SYNTAX     SEQUENCE OF MscHtdsLCoPathDataEntry
   ACCESS     not-accessible
   STATUS     mandatory
   DESCRIPTION
     "This group contains all operational attributes for the path oriented 
      LCo."
::= { mscHtdsLCo 10 }

mscHtdsLCoPathDataEntry OBJECT-TYPE
   SYNTAX     MscHtdsLCoPathDataEntry
   ACCESS     not-accessible
   STATUS     mandatory
   DESCRIPTION
     "An entry in the mscHtdsLCoPathDataTable."
   INDEX {
      mscHtdsIndex,
      mscHtdsLCoIndex }
::= { mscHtdsLCoPathDataTable 1 }

MscHtdsLCoPathDataEntry ::= SEQUENCE {
   mscHtdsLCoState
      INTEGER,
   mscHtdsLCoOverrideRemoteName
      AsciiString,
   mscHtdsLCoEnd
      INTEGER,
   mscHtdsLCoCostMetric
      Unsigned32,
   mscHtdsLCoDelayMetric
      Unsigned32,
   mscHtdsLCoRoundTripDelay
      Unsigned32,
