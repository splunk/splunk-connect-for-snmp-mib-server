	::= { usrMipHaConfig 7 }
	usrMipHaTimeDrift OBJECT-TYPE
		SYNTAX INTEGER (0..255)
		UNITS "seconds"
		MAX-ACCESS read-write
		STATUS current
		DESCRIPTION
			"The value represents how close the timestamp,
			 contained in the Identification information element
			 of the Registration Request or the Registration
			 Acknowledge message, should be to the PDSN's time
			 of day clock.  This time tolerance is used in
			 conjunction with timestamp replay protection
			 against replay attacks."
		--CONFIGURABLE
	::= { usrMipHaConfig 8 }
	usrMipHaMaxTunnels OBJECT-TYPE
		SYNTAX INTEGER (0..10000)
		MAX-ACCESS read-write
		STATUS current
		DESCRIPTION
			"Maximum number of tunnels that can be established
			 by this Home Agent."
		--CONFIGURABLE
	::= { usrMipHaConfig 9 }
	usrMipHaHacnStatus OBJECT-TYPE
		SYNTAX INTEGER {
                    enabled(1),
                    disabled(2)
                    }
		MAX-ACCESS read-write
		STATUS current
		DESCRIPTION
       		"Specifies if the HACN Interface is available.
             When set for the first time all active HA tunnels will
             be deleted prior to initialization. Enabling an already
             active interface has no action and returns no error.
             Disabling an active interface will cause deleteion of
             all tunnels and services provided on the interface.
             Disabling an inactive interface has no action and
             returns no error."
		--CONFIGURABLE
	::= { usrMipHaConfig 10 }
	usrMipHaMobileRegDump OBJECT-TYPE
		SYNTAX TruthValue
		MAX-ACCESS read-write
		STATUS deprecated
		DESCRIPTION
			"Specifies if the MIP debugging is enabled.
            DEPRECATED:
            The CLI command Set Mobile Reg_dump on/off removed."
		--CONFIGURABLE
	::= { usrMipHaConfig 11 }
	usrMipHaMnSaTable OBJECT-TYPE
		SYNTAX SEQUENCE OF UsrMipHaMnSaEntry
		MAX-ACCESS not-accessible
		STATUS current
		DESCRIPTION
			"A table for configuring mobility security
			 associations between mobile nodes and the
			 Home Agent."
	::= { usrMipHaConfig 12 }
	usrMipHaMnSaEntryIndex OBJECT-TYPE
		SYNTAX UsrMipHaMnSaEntry
		MAX-ACCESS not-accessible
		STATUS current
		DESCRIPTION
			"An entry in the Home Agent Security Association
			 Table."
		INDEX { usrMipHaMnSaIndexNum }
	::= { usrMipHaMnSaTable 1 }
	UsrMipHaMnSaEntry ::= SEQUENCE {
		usrMipHaMnSaIndexNum    INTEGER,
		usrMipHaMnSaIndexSpi    Unsigned32,
		usrMipHaMnSaIndexKey    UsrSecretDisplay,
		usrMipHaMnSaIndexStatus RowStatus
	}
	usrMipHaMnSaIndexNum OBJECT-TYPE
		SYNTAX INTEGER (1..1000)
		MAX-ACCESS read-only
		STATUS current
		DESCRIPTION
			"Home Agent can support up to 1000 mobility
			 security associations."
	::= { usrMipHaMnSaEntryIndex 1 }
	usrMipHaMnSaIndexSpi OBJECT-TYPE
		SYNTAX Unsigned32 (256..4294967295) 
