		--CONFIGURABLE
	::= { usrMipHaMnSaEntryIndex 4 }
	usrMipHaTunIPToDel OBJECT-TYPE
		SYNTAX IpAddress
		MAX-ACCESS read-write
		STATUS current
		DESCRIPTION
			"Delete a tunnel specified by this IP address."
	::= { usrMipHaConfig 13 }
	usrMipHaTunnelDeleteAll OBJECT-TYPE
		SYNTAX TruthValue
		MAX-ACCESS read-write
		STATUS current
		DESCRIPTION
		"When this flag is FALSE it indicates that HACN should not be 
             updated with a delete MBR. This case will happen if the HACN 
             interface is disabled on HA and re-enabled before HACN can 
             find out that it happened. If this happens then at least HACN 
             can download MBRs to the this HA. If this flag is set to TRUE
             all MBR's are deleted from HA. This flag will always return
             FALSE on a read."
	::= { usrMipHaConfig 14 }
	usrMipHaHacnSoftbusy OBJECT-TYPE
   		SYNTAX  INTEGER {
			disabled(0),
			enabled(1)
		}
   		MAX-ACCESS  read-write
   		STATUS  current
   		DESCRIPTION
       		"HA is softbusy or not.
            From 3.0 onwards it means that this HA will try to forward the
            Registration Request received by it and in case it can not find 
            a HA to which Registration Request can be forwarded, this HA 
            will process that request.
            Softbusy state can be changed only for ACTIVE non IPSec HA."
	::= { usrMipHaConfig 15 }
	usrMipHaHacnSecret OBJECT-TYPE
   		SYNTAX  UsrSecretDisplay
   		MAX-ACCESS  read-write
   		STATUS  current
   		DESCRIPTION
       		"Secret shared between HA and HACN."
		--CONFIGURABLE
	::= { usrMipHaConfig 16 }
	usrMipHaIdleTimer OBJECT-TYPE
		SYNTAX Unsigned32 (5..600)
		UNITS "seconds"
		MAX-ACCESS read-write
		STATUS current
		DESCRIPTION
			"The value represents the time interval when a process 
             will be run to delete the MBRs for the MNs on which 
             there have been no activity for certain time (which
             is equal to maximum idle time). This value should be
			 less than MaxIdleTime. The default is 10 secs."
		--CONFIGURABLE
	::= { usrMipHaConfig 17 }
	usrMipHaIdleTimerStatus OBJECT-TYPE
   		SYNTAX INTEGER {
                    enabled(1),
                    disabled(2)
                    } 
   		MAX-ACCESS  read-write
   		STATUS  current
   		DESCRIPTION
       		"Idle Timer Enabled or Disabled. The default is Disabled.
             Attempting to enable/disable the Idle TImer while any
             call is active will return an error."
        --CONFIGURABLE
	::= { usrMipHaConfig 18 }
	usrMipHaMaxIdleTime OBJECT-TYPE
		SYNTAX Unsigned32 (10..65535)
		UNITS "seconds"
		MAX-ACCESS read-write
		STATUS current
		DESCRIPTION
			"The value represents the time for the maximum idle
             time. If for this much time there is no activity  
             (data transfer), then the MBR for this MN will be 
             deleted. This value should be greater than 
             IdleTimer. The default value is 65535 secs."
		--CONFIGURABLE
	::= { usrMipHaConfig 19 }
	usrMipHaWAPCCStatus OBJECT-TYPE
		SYNTAX INTEGER {
                    enabled(1),
                    disabled(2)
                    }
		MAX-ACCESS read-write
		STATUS current
		DESCRIPTION
       		"Specifies if WAP Call Clearing is enabled.
             Enabling if already enabled has no action and no
             error is returned. Disabling this feature while
             any update message is pending will return an error."
		--CONFIGURABLE
	::= { usrMipHaConfig 20 }
	usrMipHaTunnelByAddr OBJECT-TYPE
		SYNTAX IpAddress
		MAX-ACCESS read-write
		STATUS obsolete
		DESCRIPTION
			"Display a tunnel specified by this IP address.
			 OBSOLETE:
			 This object is never used on HA."
	::= { usrMipHaConfig 21 }
    usrMipHaStatsResetTime OBJECT-TYPE
        SYNTAX	Unsigned32 
		MAX-ACCESS  read-write
		STATUS  current
		DESCRIPTION
                "Reading:
				Time the HA statistics counters were last 
			 	reset. The HA statistics are valid from 
				usrMipHAStatsResetTime until present.
	        		Resetting counters causes a discontinuity in 
				the monotonically increasing value of counters.
				Management stations that sample these counters
				need also to sample this object so that they 
				can know that a discontinuity occurred with 
				respect to the previous sample.  
                Its value is in NTP format.
		        Writing:
		   	    Writing 0 causes update of usrMipHAStatsResetTime
				and the HA counters to be reset to 0."
	::= { usrMipHaConfig 22 }
	usrMipHaUsingBothPorts OBJECT-TYPE
		SYNTAX INTEGER {
                    enabled(1),
                    disabled(2)
                    }
		MAX-ACCESS read-write
		STATUS current
		DESCRIPTION
   			"Specifies if HA is using both ports. Port 1 (eth:1) will
             always be used for interfacing with PDSN/FA/HACN"
		--CONFIGURABLE
	::= { usrMipHaConfig 23 }
	usrMipHaTunDelByNAI OBJECT-TYPE
		SYNTAX     DisplayString 
		MAX-ACCESS read-write
		STATUS current
		DESCRIPTION
			"Delete tunnel(s) specified by this NAI. Reading this
             value returns NULL."
	::= { usrMipHaConfig 24 }
	usrMipHaBroadcastBit OBJECT-TYPE
		SYNTAX INTEGER {
                    enabled(1),
                    disabled(2)
                    }
		MAX-ACCESS read-write
		STATUS current
		DESCRIPTION
   			"This flag specifies if HA should accept a registration
             request with B bit set in registration flags or not. By
             default this bit is set and HA will accept reg req with 
             B bit set."
		--CONFIGURABLE
	::= { usrMipHaConfig 25 }
	usrMipHaNAICheck OBJECT-TYPE
		SYNTAX INTEGER {
                    enabled(1),
                    disabled(2)
                    }
		MAX-ACCESS read-write
		STATUS current
		DESCRIPTION
   			"Specifies if HA is checking NAI for re-registration requests
             or not. If it is enable then NAI is checked for each
             Re-Registration Requests otherwise NAI is not checked. By
             default NAI check is enabled."
		--CONFIGURABLE
	::= { usrMipHaConfig 26 }
	usrMipHaFaSaTable OBJECT-TYPE
