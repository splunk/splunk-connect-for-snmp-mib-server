-- Home Agent MIP Interface Events Settings Group
	usrMipHaUpperThreshold OBJECT-TYPE
		SYNTAX INTEGER (1..100)
		MAX-ACCESS read-write
		STATUS current
		DESCRIPTION
       			"Specifies the upper Water Mark for Active Session
                         Threshold. If the active call capacity exceeds this
                         limit, then a trap is sent."
		--CONFIGURABLE
	::= { usrMipHaTrapsSettings 1 }
	usrMipHaLowerThreshold OBJECT-TYPE
		SYNTAX INTEGER (1..100)
		MAX-ACCESS read-write
		STATUS current
		DESCRIPTION
       			"Specifies the lower Water Mark for Active Session
                         Threshold. If the active call capacity touches 
                         this lower threshold after touching the Upper
                         threshold, then only a trap will be sent whenever
                         next time it exceeds the Upper Threshold."
		--CONFIGURABLE
	::= { usrMipHaTrapsSettings 2 }
	usrMipHaTrapThreshold OBJECT-TYPE
   		SYNTAX INTEGER {
                    enabled(1),
                    disabled(2)
                    }
   		MAX-ACCESS  read-write
   		STATUS  current
   		DESCRIPTION
	        "Threshold Trap Enabled or Disabled."
        --CONFIGURABLE
	::= { usrMipHaTrapsSettings 3 }
	usrMipHaTrapIdleTimer OBJECT-TYPE
   		SYNTAX INTEGER {
                    enabled(1),
                    disabled(2)
                    }
   		MAX-ACCESS  read-write
   		STATUS  current
   		DESCRIPTION
	        "Idle Timer Trap Enabled or Disabled."
        --CONFIGURABLE
	::= { usrMipHaTrapsSettings 4 }
-- Home Agent MIP Interface Events Group
    usrMipHaTrapsV2 OBJECT-IDENTITY
        STATUS  current
        DESCRIPTION "The Traps for MIP"
        ::= {usrMipHaTraps 0}
	usrMipHaActiveSessionThresholdReached NOTIFICATION-TYPE
		OBJECTS {    
                    usrMipHaUpperThreshold,
                    usrMipHaLowerThreshold,
	                usrMipHaCurrentSessions
		}
        STATUS  current
		DESCRIPTION
			"This trap is generated when Active Session Threshold 
             is reached based on the calculation -
             (ActiveSessions * 100) / MaxTunnels being greater
             than the UpperThreshold."
	    --#TYPE	"Interface Capacity Alarm"
	    --#SUMMARY	"Total Sessions %d reached upper threshold of %d"
	    --#ARGUMENTS { 2 0 }
	    --#SEVERITY MAJOR
	    --#STATE	OPERATIONAL
	    --#PROBABLECAUSE "resourceAtOrNearingCapacity"
        ::= {usrMipHaTrapsV2 1}
	usrMipHaIdleTimerExpired NOTIFICATION-TYPE
		OBJECTS {    
		        usrMipHaMobilityBindingMN
		}
        STATUS  current
		DESCRIPTION
			"This trap is generated when the Idle Timer has expired
             and the tunnel has been disconnected. The trap contains
             the MobileNode's home IP address."
	    --#TYPE	"Timeout"
	    --#SUMMARY	"Tunnel disconnected for MN %d"
	    --#ARGUMENTS { 0 }
	    --#SEVERITY MINOR
	    --#STATE	OPERATIONAL
	    --#PROBABLECAUSE "communicationsProblem"
        ::= {usrMipHaTrapsV2 2}
