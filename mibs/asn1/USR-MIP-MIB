	usrMipHaLoadInfo         OBJECT IDENTIFIER ::= { usrMIP 8 } 
	usrMipHacnSettings       OBJECT IDENTIFIER ::= { usrMIP 9 }
	usrMipHacnStats          OBJECT IDENTIFIER ::= { usrMIP 10 }
-- Home Agent MIP configurable parameters
	usrMipHaMaxLifetime OBJECT-TYPE
		SYNTAX Integer32 (5..65535)
		UNITS "seconds"
		MAX-ACCESS read-write
		STATUS current
		DESCRIPTION
			"The longest lifetime in seconds that the Home
			 Agent is willing to accept in any Registration
			 Request."
		--CONFIGURABLE
	::= { usrMipHaConfig 1 }
	usrMipHaDomainName OBJECT-TYPE
		SYNTAX OCTET STRING (SIZE(0..128))
		MAX-ACCESS read-write
		STATUS obsolete
		DESCRIPTION
			"The HA domain name.
			 OBSOLETE:
			 This object is never used on HA."
		--CONFIGURABLE
	::= { usrMipHaConfig 2 }
	usrMipHaDomainSpi OBJECT-TYPE
		SYNTAX Unsigned32 (0..4294967295) 
		MAX-ACCESS read-write
		STATUS obsolete
		DESCRIPTION
			"The SPI is the 4-byte opaque index within
			 the Mobility Security Association which
			 selects the specific security parameters
			 to be used to authenticate the peer.
			 OBSOLETE:
			 This object is never used on HA."
		--CONFIGURABLE
	::= { usrMipHaConfig 3 }
	usrMipHaDomainKey OBJECT-TYPE
		SYNTAX OCTET STRING (SIZE(0..16))
		MAX-ACCESS read-write
		STATUS obsolete
		DESCRIPTION
			"The shared secret key for the security associations.
			 Reading this object will always return zero length
			 value.
			 OBSOLETE:
			 This object is never used on HA."
		--CONFIGURABLE
	::= { usrMipHaConfig 4 }
	usrMipHaHacnAddress OBJECT-TYPE
		SYNTAX IpAddress
