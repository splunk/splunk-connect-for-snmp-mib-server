        ncmcsuIntervalEntry OBJECT-TYPE
        SYNTAX NcmcsuIntervalEntry
        ACCESS not-accessible
        STATUS mandatory
        DESCRIPTION
               "An entry in the Interval Entry table."
        INDEX   { ncmcsuNIDIntervalIndex,ncmcsuIntervalIndex, ncmcsuIntervalEndType, ncmcsuIntervalNumber }
        ::= { ncmcsuIntervalTable 1 }



                NcmcsuIntervalEntry ::=
                SEQUENCE {
		ncmcsuNIDIntervalIndex
		INTEGER,
                ncmcsuIntervalIndex
                INTEGER,
		ncmcsuIntervalEndType
		INTEGER,
	        ncmcsuIntervalNumber
                INTEGER,
                ncmcsuIntervalNETOOFS
                Gauge,
                ncmcsuIntervalNETLOSS
                Gauge,
                ncmcsuIntervalNETAISS
                Gauge,
                 ncmcsuIntervalNETBERS
                Gauge,
                ncmcsuIntervalNETYELS
                Gauge,
                ncmcsuIntervalNETLOFS
                Gauge,
                ncmcsuIntervalNETESL
                Gauge,
                ncmcsuIntervalNETSEFS
                Gauge,
                ncmcsuIntervalEQOOFS
                Gauge,
                ncmcsuIntervalEQDTED
                Gauge,
                ncmcsuIntervalEQDBER
                Gauge,
		ncmcsuIntervalEQES
		Gauge,
		ncmcsuIntervalEQUAS
		Gauge,
		ncmcsuIntervalEQESL
		Gauge

         
                }

