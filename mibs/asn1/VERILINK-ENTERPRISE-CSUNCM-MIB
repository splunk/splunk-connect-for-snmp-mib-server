            INDEX   { ncmcsuNIDIntervalIndex1, ncmcsuIntervalIndex1,ncmcsuIntervalEndType1, ncmcsuIntervalNumber1 }
            ::= { ncmcsuIntervalOneTable 1 }
             
     NcmcsuIntervalOneEntry ::=
         SEQUENCE {
               
             ncmcsuNIDIntervalIndex1
                INTEGER,
             ncmcsuIntervalIndex1
                 INTEGER,
	     ncmcsuIntervalEndType1
		INTEGER,
             ncmcsuIntervalNumber1
                 INTEGER,
	     ncmcsuIntervalESs
                 Gauge,
             ncmcsuIntervalSESs
                 Gauge,
             ncmcsuIntervalSEFSs
                 Gauge,
             ncmcsuIntervalUASs
                 Gauge,
