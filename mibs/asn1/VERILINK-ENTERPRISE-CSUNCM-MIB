	 -- the ncm csu Enhanced Interval Table

     -- The ncm csu Enhanced  Interval Table contains various statistics
     -- collected by each ncmcsu Interface over the previous 24 hours of
     -- operation.  The past 24 hours are broken into 96 completed
     -- 15 minute intervals.

         ncmcsuEnhancedIntervalTable OBJECT-TYPE
             SYNTAX  SEQUENCE OF NcmcsuEnhancedIntervalEntry
             ACCESS  not-accessible
             STATUS  mandatory
             DESCRIPTION
                "The ncmcsu Interval table1."
			 --DEFAULT test-function    csucommoninterval-test
                        --DEFAULT set-function     null-set-proc
                        --DEFAULT next-function    csucommoninterval-next
	
            ::= { ncm-csu 6009 }


         ncmcsuEnhancedIntervalEntry OBJECT-TYPE
             SYNTAX  NcmcsuEnhancedIntervalEntry
             ACCESS  not-accessible
             STATUS  mandatory
             DESCRIPTION
                "An entry in the ncmcsu Enhanced Interval table."
            INDEX   { ncmcsuNIDEnhancedIntervalIndex, ncmcsuEnhancedIntervalIndex,ncmcsuEnhancedIntervalEndType, ncmcsuEnhancedIntervalNumber }
            ::= { ncmcsuEnhancedIntervalTable 1 }

    	 NcmcsuEnhancedIntervalEntry ::=
         SEQUENCE {

             ncmcsuNIDEnhancedIntervalIndex
                INTEGER,
             ncmcsuEnhancedIntervalIndex
                 INTEGER,
             ncmcsuEnhancedIntervalEndType
                INTEGER,
             ncmcsuEnhancedIntervalNumber
                 INTEGER,
             ncmcsuEnhancedIntervalESs
                 Gauge,
             ncmcsuEnhancedIntervalSESs
                 Gauge,
             ncmcsuEnhancedIntervalUASs
                 Gauge,
             ncmcsuEnhancedIntervalCSSs
                 Gauge,
             ncmcsuEnhancedIntervalBESs
                 Gauge,
	     ncmcsuEnhancedIntervalLOFC
                 Gauge

	     
                      }


 	ncmcsuNIDEnhancedIntervalIndex OBJECT-TYPE
             SYNTAX  INTEGER (1..'7fffffff'h)
             ACCESS  read-only
             STATUS  mandatory
             DESCRIPTION
                "This variable specifies the node id of the node"
            ::= { ncmcsuEnhancedIntervalEntry 1 }
          
         
         
        ncmcsuEnhancedIntervalIndex OBJECT-TYPE
             SYNTAX  INTEGER (1..'7fffffff'h)
             ACCESS  read-only
             STATUS  mandatory
             DESCRIPTION
                "The index value which uniquely identifies  the
                ncmcsu  interface  to which this entry is applica-
                ble.  The interface identified by a  particular
                value  of  this  index is the same interface as
                identified by the same value as a ncmcsuLineIndex
                object instance."
            ::= { ncmcsuEnhancedIntervalEntry 2 }
 
 
         ncmcsuEnhancedIntervalEndType OBJECT-TYPE
             SYNTAX INTEGER {
 
                       near-End(1),
                       far-End(2)
 
                        }
             ACCESS read-only
             STATUS mandatory
             DESCRIPTION
             "The variable specifies the End Type   ."
 
             ::= { ncmcsuEnhancedIntervalEntry 3 }
 
         
 
         ncmcsuEnhancedIntervalNumber OBJECT-TYPE
             SYNTAX  INTEGER (1..96)
             ACCESS  read-only
             STATUS  mandatory
             DESCRIPTION
                "A number between 1 and 96, where 1 is the most
                recently completed 15 minute interval and 96 is
                the least recently completed 15 minutes  inter-
                val   (assuming   that  all  96  intervals  are
                valid)."
            ::= { ncmcsuEnhancedIntervalEntry 4 }

	

	 ncmcsuEnhancedIntervalESs OBJECT-TYPE
             SYNTAX  Gauge
             ACCESS  read-only
             STATUS  mandatory
             DESCRIPTION
                "The number of Errored Seconds encountered by a
                ncmcsu  interface in one of the previous 96, indi-
                vidual 15 minute, intervals."
            ::= { ncmcsuEnhancedIntervalEntry 5 }
          
 
         ncmcsuEnhancedIntervalSESs OBJECT-TYPE
             SYNTAX  Gauge
             ACCESS  read-only
             STATUS  mandatory
             DESCRIPTION
                "The number of Severely Errored Seconds encoun-
                tered by a ncmcsu interface in one of the previous
                96, individual 15 minute, intervals."
            ::= { ncmcsuEnhancedIntervalEntry 6 }

	 ncmcsuEnhancedIntervalUASs OBJECT-TYPE
             SYNTAX  Gauge
             ACCESS  read-only
             STATUS  mandatory
             DESCRIPTION
                "The number of Unavailable Seconds  encountered
                by  a  ncmcsu interface in one of the previous 96,
                individual 15 minute, intervals."
            ::= { ncmcsuEnhancedIntervalEntry 7 }
          
 
         ncmcsuEnhancedIntervalCSSs OBJECT-TYPE
             SYNTAX  Gauge
             ACCESS  read-only
             STATUS  mandatory
             DESCRIPTION
                "The number of Controlled Slip Seconds  encoun-
                tered by a ncmcsu interface in one of the previous
                96, individual 15 minute, intervals."
          ::= { ncmcsuEnhancedIntervalEntry 8 }


	 ncmcsuEnhancedIntervalBESs OBJECT-TYPE
             SYNTAX Gauge
             ACCESS read-only
             STATUS mandatory
             DESCRIPTION
                "The number of Bursty  Errored  Seconds  (BESs)
                encountered  by  a  ncmcsu interface in one of the
                previous 96, individual 15 minute, intervals."
            ::= {ncmcsuEnhancedIntervalEntry 9 }


	

	 ncmcsuEnhancedIntervalLOFC OBJECT-TYPE
             SYNTAX Gauge
             ACCESS read-only
             STATUS mandatory
             DESCRIPTION
                "The number of LOFC
                encountered  by  a  ncmcsu interface in one of the
                previous 96, individual 15 minute, intervals."
            ::= {ncmcsuEnhancedIntervalEntry 10 }

 



