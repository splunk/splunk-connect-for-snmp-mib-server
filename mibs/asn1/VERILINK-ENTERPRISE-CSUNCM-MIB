 VERILINK-ENTERPRISE-CSUNCM-MIB

        -- set up MIB-wide defaults
        -- all functions can use %n for object name and
        -- %t for object type, %p for parent name,
        -- %d for name of default-bearing node and %% for %

          DEFINITIONS ::= BEGIN

     IMPORTS
             Gauge
                     FROM RFC1155-SMI
             OBJECT-TYPE
                     FROM RFC-1212
	      DisplayString
                     FROM RFC1213-MIB

	     ncm-csu
		     FROM 
             VERILINK-ENTERPRISE-NCMALARM-MIB;

             

     --  This MIB module uses the extended OBJECT-TYPE macro as
     --  defined in RFC 1212.
     --  This MIB module uses the TRAP-TYPE macro as
     --  defined in RFC 1215.




        

         
         -- The ncm csu Main configuration table

        ncmcsuMainConfigTable OBJECT-TYPE
        SYNTAX SEQUENCE OF NcmcsuMainConfigEntry
        ACCESS not-accessible
        STATUS mandatory
	DESCRIPTION
        " The ncm csu main configuration table."

	 --DEFAULT test-function csu-test
        --DEFAULT next-function csu-next
	
	
	

                  ::= { ncm-csu 6000 }


        ncmcsuMainConfigEntry OBJECT-TYPE
        SYNTAX NcmcsuMainConfigEntry
        ACCESS not-accessible
        STATUS mandatory
        DESCRIPTION
               "An entry in the ncm csu main configuration table."
        INDEX   { ncmcsuNIDMainConfigIndex, ncmcsuMainLineIndex }
        ::= { ncmcsuMainConfigTable 1 }


	NcmcsuMainConfigEntry ::=
        SEQUENCE {
		
	     ncmcsuNIDMainConfigIndex
		  INTEGER,	
             ncmcsuMainLineIndex
                 INTEGER,
             ncmcsuFormat
                 INTEGER,
	     ncmcsuLineCode
		INTEGER,
             ncmcsuEQCableLength
                 INTEGER,
             ncmcsuTiming
                 INTEGER,
             ncmdsudatabus
                 INTEGER,
             ncmcsuNetLineBuildOut
                 INTEGER,
	     ncmcsuValidIntervals
		 INTEGER,
	     ncmcsuCurrentIntervalSec
		INTEGER,
  	     ncmcsuJitterBuf
                INTEGER,
	     ncmcsuTestSigCfgEnable
		INTEGER,
	     ncmcsuTestSigCfgFrameSignal
		INTEGER,
	     ncmcsuRLBTimeoutIndex
		INTEGER,
	     ncmcsuCfgRptSendPRM
		INTEGER,
	     ncmcsuCfgRptPollFarEnd
		INTEGER,
	     ncmcsuCfgRptDataLinkUnsolicit
		INTEGER,
	     ncmcsuCfgRptAlmReporting
		INTEGER,
	     ncmcsuCfgRptPRMType
		INTEGER,
	     ncmcsuCfgCodeRegenCRC
		INTEGER,
	     ncmcsuCfgCodeXYellowAlarm
		INTEGER,
	     ncmcsuCfgCodeEQFIFO
		INTEGER,
	     ncmcsuCfgCodeNETFIFO
		INTEGER,
	     ncmcsuCfgCodeTranMode
		INTEGER,
	     ncmcsuCfgCodeSend1sLnkIdle
		INTEGER,
	     ncmcsuCfgAlmSelfTest
		INTEGER,
	     ncmcsuCfgAlmEnableTestState
		INTEGER,
	     ncmcsuCfgAlmUnframedMode
		INTEGER,
	     ncmcsuCfgAlmOnEqLoop
		INTEGER,
	     ncmcsuCfgAlmOnNetLoop
		INTEGER,
	     ncmcsuCfgAlmOnPowerUpLoop
		INTEGER,
	     ncmcsuCfgLoopRespLLB
                INTEGER,
             ncmcsuCfgLoopRespPLB
                INTEGER,
             ncmcsuCfgLoopRespELB
                INTEGER,
             ncmcsuCfgLoopRespRLB
                INTEGER,
             ncmcsuCfgLoopRespLLBTONE
                INTEGER,
             ncmcsuCfgLoopRespPLBTONE
                INTEGER,
             ncmcsuCfgSendReceiveInBandCode
                INTEGER
 

           
                }


	ncmcsuNIDMainConfigIndex OBJECT-TYPE
        SYNTAX  INTEGER
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION
	"This variable specifies the node id."          
        ::= { ncmcsuMainConfigEntry 1 }

	
 
 
        ncmcsuMainLineIndex OBJECT-TYPE
        SYNTAX  INTEGER
        ACCESS  read-only
        STATUS  mandatory
	DESCRIPTION
	"This variable specifies the line index value."           
        ::= { ncmcsuMainConfigEntry 2 }
 
        ncmcsuFormat OBJECT-TYPE
         SYNTAX INTEGER{
		       unframed(1),
                       sf(2),
                       esf(3),
                       zbtsi(4)
 
               } 
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION
               "The ncmcsu Net/Eq format."
 
        ::= { ncmcsuMainConfigEntry 3 }
 
 
 
        ncmcsuLineCode OBJECT-TYPE
         SYNTAX INTEGER {
			ami(1),
                       b8zs(2)
           
 
               } 
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION
               "The ncmcsu Net/Eq Line Code."
 
        ::= { ncmcsuMainConfigEntry 4 }
 
 
	 ncmcsuEQCableLength OBJECT-TYPE
        SYNTAX INTEGER  {
                ft-0-133(1),
                fT-133-266(2),
                fT-266-399(3),
                fT-399-533(4),
                fT-533-655(5)
			}
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION
               "The ncmcsu EQ Cable Length."
        ::= { ncmcsuMainConfigEntry 5 }
          
 
        ncmcsuTiming OBJECT-TYPE
        SYNTAX INTEGER  {
                through (1),
                internal (2),
                external-422 (3),
                ext-ttl (4),
                net(5),
                eq(6),
                tiu(7),
                dsu(8)
                                }
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION
               "The ncmcsu timing."
        ::= { ncmcsuMainConfigEntry 6 }
          
 
         
        ncmdsudatabus OBJECT-TYPE
        SYNTAX INTEGER  {
                        none(1),
                        bus-a(2),
                        bus-b(3),
                        bus-c(4),
        		drop-uses-bus-b(5),   
        		insert-uses-bus-a(6)   
 
                        }
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION
               "The dsu data bus reserved for future."
        ::= { ncmcsuMainConfigEntry 7 }
          

	ncmcsuNetLineBuildOut OBJECT-TYPE
        SYNTAX INTEGER
                        {
                        db-Zero(1),
                        db-Seven-point-five(2),
                        db-Fifteen(3)
                        }
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION
               "The ncmcsu net line build out."
        ::= { ncmcsuMainConfigEntry 8 }

	
	ncmcsuValidIntervals OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
               "The ncmcsu valid intervals."
        ::= { ncmcsuMainConfigEntry 9 }


        ncmcsuCurrentIntervalSec  OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
               "The ncmcsu seconds in current 15 minutes interval. "
        ::= { ncmcsuMainConfigEntry 10 }


        ncmcsuJitterBuf OBJECT-TYPE
         SYNTAX INTEGER {
                        eq-Net-16-16(1),
                        eq-Net-40-16(2),
                        eq-Net-16-40(3),
                        eq-Net-40-40(4)

               } 
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION
               "The ncmcsu Net/Eq Jitter buffer in bits."
        ::= { ncmcsuMainConfigEntry 11 }


	ncmcsuTestSigCfgEnable    OBJECT-TYPE
        SYNTAX INTEGER  {
			no(1),
                        yes(2)
        }                
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION
               "bit 1 of MSGOUT+47 for read or of MSGIN+47 for write is test signal enable
		in this test signal configuration message."
        ::= { ncmcsuMainConfigEntry 12 }


        ncmcsuTestSigCfgFrameSignal  OBJECT-TYPE
        SYNTAX INTEGER  {
                        no(1),
                        yes(2)
        }       
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION
               "bit 7 of MSGOUT+47 for read or of MSGIN+47 for write is frame test signal
		in this test signal configuration message."
        ::= { ncmcsuMainConfigEntry 13 }
 

	ncmcsuRLBTimeoutIndex      OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION
               "The rlb timeout index."
        ::= { ncmcsuMainConfigEntry 14 }


	ncmcsuCfgRptSendPRM	OBJECT-TYPE
        SYNTAX INTEGER  {
			no(1),
                        yes(2)
	}
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION
               "bit 0 of MSGOUT+52 for read or MSGIN+52 for write is send prm
		in this configure report message."
        ::= { ncmcsuMainConfigEntry 15 }


	ncmcsuCfgRptPollFarEnd     OBJECT-TYPE
        SYNTAX INTEGER  {
			no(1),
                        yes(2)
        }
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION
               "bit 1 of MSGOUT+52 for read or MSGIN+52 for write is poll far end
		in this configure report message."
        ::= { ncmcsuMainConfigEntry 16 }


	ncmcsuCfgRptDataLinkUnsolicit   OBJECT-TYPE
        SYNTAX INTEGER  {
			no(1),
                        yes(2)
        }
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION
               "bit 2 of MSGOUT+52 for read or MSGIN+52 for write is data link unsolicited report
		in this configure report message."
        ::= { ncmcsuMainConfigEntry 17 }


	ncmcsuCfgRptAlmReporting    OBJECT-TYPE
        SYNTAX INTEGER  {
			no(1),
                        yes(2)
        }
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION
               "bit 3 of MSGOUT+52 for read or MSGIN+52 for write is alarm reporting
		in this configure report message."
        ::= { ncmcsuMainConfigEntry 18 }


	ncmcsuCfgRptPRMType	OBJECT-TYPE
        SYNTAX INTEGER  {
                        nO-PRM(1),
                        uSER(2)
        }
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION
               "bit 4,5 of MSGOUT+52 for read or of MSGIN+52 for write is PRM type
		in this configure report message."
        ::= { ncmcsuMainConfigEntry 19 }


	ncmcsuCfgCodeRegenCRC  	OBJECT-TYPE
        SYNTAX INTEGER  {
                        pass(1),
                        net-to-Eq(2),
                        eq-to-Net(3),
                        both(4)
        }
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION
               "bit 0,1 of MSGOUT+53 for read or MSGIN+53 for write is regenerate 
		crc from net to dte or eq to net in this configure coding message. "
        ::= { ncmcsuMainConfigEntry 20 }


	ncmcsuCfgCodeXYellowAlarm  OBJECT-TYPE
        SYNTAX INTEGER  {
                        off(1),
                        net-to-Eq(2),
                        eq-to-Net(3),
                        both(4)
        }
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION
               "bit 2,3 of MSGOUT+53 for read or MSGIN+53 for write is transcode yellow
                alarm from net to dte or eq to net in this configure coding message. "
        ::= { ncmcsuMainConfigEntry 21 }


	ncmcsuCfgCodeEQFIFO        OBJECT-TYPE
        SYNTAX INTEGER  {
                        fifteen-bits(1),
                        forty-bits(2)
        }
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION
               "bit 4 of MSGOUT+53 for read or MSGIN+53 for write is eq fifo
                in this configure coding message. "
        ::= { ncmcsuMainConfigEntry 22 }


        ncmcsuCfgCodeNETFIFO        OBJECT-TYPE
        SYNTAX INTEGER  {
                        fifteen-bits(1),
                        forty-bits(2)
        }
        ACCESS read-write
        STATUS mandatory 
        DESCRIPTION
               "bit 5 of MSGOUT+53 for read or MSGIN+53 for write is net fifo
                in this configure coding message. "
        ::= { ncmcsuMainConfigEntry 23 }


	ncmcsuCfgCodeTranMode	OBJECT-TYPE
        SYNTAX INTEGER  {
			no(1),
                        yes(2)
        }
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION
               "bit 6 of MSGOUT+53 for read or MSGIN+53 for write is transparent 
		mode in this configure report message."
        ::= { ncmcsuMainConfigEntry 24 }


        ncmcsuCfgCodeSend1sLnkIdle   OBJECT-TYPE 
        SYNTAX INTEGER  { 
			no(1),
                        yes(2) 
        } 
        ACCESS read-write
        STATUS mandatory 
        DESCRIPTION 
               "bit 7 of MSGOUT+53 for read or MSGIN+53 for write is send ones
                data link idle code in this configure report message."
        ::= { ncmcsuMainConfigEntry 25 }


        ncmcsuCfgAlmSelfTest       OBJECT-TYPE
        SYNTAX INTEGER  {
			no(1),
                        yes(2)
        }                
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION
               "bit 0 of MSGOUT+54 for read or MSGIN+54 for write is enable
                power up self test in this configure alarm message."
        ::= { ncmcsuMainConfigEntry 26 }
 
 
        ncmcsuCfgAlmEnableTestState   OBJECT-TYPE
        SYNTAX INTEGER  {
			no(1),
                        yes(2)
        }                
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION
               "bit 1 of MSGOUT+54 for read or MSGIN+54 for write is enable
                alarms in test state in this configure alarm message."
        ::= { ncmcsuMainConfigEntry 27 }
 
 
        ncmcsuCfgAlmUnframedMode   OBJECT-TYPE
        SYNTAX INTEGER  {
                        no(1),
                        yes(2)
        }
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION
               "bit 2 of MSGOUT+54 for read or MSGIN+54 for write is
                alarms in unframe mode in this configure alarm message."
        ::= { ncmcsuMainConfigEntry 28 }
 
 
        ncmcsuCfgAlmOnEqLoop       OBJECT-TYPE
        SYNTAX INTEGER  {
			no(1),
                        yes(2)
        }
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION
               "bit 3 of MSGOUT+54 for read or MSGIN+54 for write is alarm
                on eq loop loop in this configure alarm message."
        ::= { ncmcsuMainConfigEntry 29 }
 

        ncmcsuCfgAlmOnNetLoop       OBJECT-TYPE
        SYNTAX INTEGER  {
			no(1),
                        yes(2)
        }
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION
               "bit 4 of MSGOUT+54 for read or MSGIN+54 for write is alarm
                on net loop in this configure alarm message."
        ::= { ncmcsuMainConfigEntry 30 }


        ncmcsuCfgAlmOnPowerUpLoop       OBJECT-TYPE 
        SYNTAX INTEGER  { 
			no(1),
                        yes(2)
        }
        ACCESS read-write
        STATUS mandatory 
        DESCRIPTION 
               "bit 5 of MSGOUT+54 for read or MSGIN+54 for write is alarm 
                power up loop in this configure alarm message." 
        ::= { ncmcsuMainConfigEntry 31 }


	ncmcsuCfgLoopRespLLB      OBJECT-TYPE
        SYNTAX INTEGER  {
                        no(1),
                        yes(2)
        }
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION
               "bit 0 of MSGOUT+55 for read or MSGIN+55 for respond to LLB
                in this configure Loop Response Message."
        ::= { ncmcsuMainConfigEntry 32 }


        ncmcsuCfgLoopRespPLB      OBJECT-TYPE
        SYNTAX INTEGER  {
                        no(1),
                        yes(2)
        }
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION
               "bit 1 of MSGOUT+55 for read or MSGIN+55 for respond to PLB
                in this configure Loop Response Message."
        ::= { ncmcsuMainConfigEntry 33 }
 
 
        ncmcsuCfgLoopRespELB     OBJECT-TYPE
        SYNTAX INTEGER  {
                        no(1),
                        yes(2)
        }
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION
               "bit 2 of MSGOUT+55 for read or MSGIN+55 for respond to ELB
                in this configure Loop Response Message."
        ::= { ncmcsuMainConfigEntry 34 }
 
 
        ncmcsuCfgLoopRespRLB     OBJECT-TYPE
        SYNTAX INTEGER  {
                        no(1),
                        yes(2)
        }
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION
               "bit 3 of MSGOUT+55 for read or MSGIN+55 for respond to RLB
                in this configure Loop Response Message."
        ::= { ncmcsuMainConfigEntry 35 }
 
 
        ncmcsuCfgLoopRespLLBTONE     OBJECT-TYPE
        SYNTAX INTEGER  {
                        no(1),
                        yes(2)
        }
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION
               "bit 4 of MSGOUT+55 for read or MSGIN+55 for respond to LLB (T1.403)
                in this configure Loop Response Message."
        ::= { ncmcsuMainConfigEntry 36 }


	ncmcsuCfgLoopRespPLBTONE     OBJECT-TYPE
        SYNTAX INTEGER  {
                        no(1),
                        yes(2)
        }
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION
               "bit 5 of MSGOUT+55 for read or MSGIN+55 for respond to PLB (T1.403)
                in this configure Loop Response Message."
        ::= { ncmcsuMainConfigEntry 37 }
 
 
        ncmcsuCfgSendReceiveInBandCode     OBJECT-TYPE
        SYNTAX INTEGER  {
                        disable(1),
                        enable(2)
        }
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION
               "bit 6 of MSGOUT+55 for read or MSGIN+55 for enable to Send/Receive
                of in-band code."
        ::= { ncmcsuMainConfigEntry 38 }

 


	-- The ncmcsu Enhance Configuration table

        ncmcsuEnhanceConfigTable OBJECT-TYPE
        SYNTAX SEQUENCE OF NcmcsuEnhanceConfigEntry
        ACCESS not-accessible
        STATUS mandatory
	DESCRIPTION
        "The ncmcsu enhance configuration table."



	 --DEFAULT test-function csu-test
        --DEFAULT next-function csu-next
	
        ::= { ncm-csu 6001 }


        ncmcsuEnhanceConfigEntry OBJECT-TYPE
        SYNTAX NcmcsuEnhanceConfigEntry
        ACCESS not-accessible
        STATUS mandatory
        DESCRIPTION
               "An entry in the ncmcsu enhance configuration table."
        INDEX   { ncmcsuNIDEnhanceIndex, ncmcsuEnhanceLineIndex }
        ::= { ncmcsuEnhanceConfigTable 1 }


        NcmcsuEnhanceConfigEntry ::=
        SEQUENCE {

	     ncmcsuNIDEnhanceIndex
		  INTEGER,
             ncmcsuEnhanceLineIndex
                 INTEGER,
             ncmcsuNetworkKeepAlive
                 INTEGER,
             ncmcsuAlarmSetDelay
                 INTEGER,
             ncmcsuAlarmClearDelay
                 INTEGER,
             ncmcsuAlarmEnable
                INTEGER,
             ncmcsuNetDensityEnforcement
                 INTEGER,
             ncmcsuLoopback
                INTEGER,
	     ncmcsuTestPattern
		INTEGER,
	     ncmcsuResetPerfReg
		INTEGER,
	     ncmcsuTestErrorCounter
		Gauge,
	     ncmcsuTestSecondsRemain
		Gauge,
	     ncmcsuTestTimeSeconds
                INTEGER,
             ncmcsuNetLOFCIndexTime
                INTEGER,
             ncmcsuChannelMask
                DisplayString,
             ncmcsuApplication
                INTEGER,
             ncmcsuTestIntervalIndex
                INTEGER

 

                        }




	ncmcsuNIDEnhanceIndex OBJECT-TYPE
        SYNTAX  INTEGER 
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION
	"This variable specifies the nid index."         
        ::= { ncmcsuEnhanceConfigEntry 1 }


 	ncmcsuEnhanceLineIndex OBJECT-TYPE
        SYNTAX  INTEGER 
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION
	"This variable specifies the line index."         
        ::= { ncmcsuEnhanceConfigEntry 2 }

        ncmcsuNetworkKeepAlive OBJECT-TYPE
        SYNTAX INTEGER {
 
                       none(1),
                       loop(2),
                       aIS(3),
                       framed-all-ones(4)
                        
               } 
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION
               "The ncmcsu  network keep alive varaible ."
                  
        ::= { ncmcsuEnhanceConfigEntry 3 }
 
 
        ncmcsuAlarmSetDelay OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION
               "The ncmcsu alarm set delay."
        ::= { ncmcsuEnhanceConfigEntry 4 }
 
 
        ncmcsuAlarmClearDelay OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION
               "The ncmcsu alarm clear delay."
        ::= { ncmcsuEnhanceConfigEntry 5 }
 
        ncmcsuAlarmEnable OBJECT-TYPE
        SYNTAX INTEGER {
 
                       disable(1),
                       enable(2)
                        
               }        
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION
               "The ncmcsu alarm reporting."
                  
        ::= { ncmcsuEnhanceConfigEntry 6 }
 

	ncmcsuNetDensityEnforcement OBJECT-TYPE
        SYNTAX INTEGER {
                        none(1),
                        fcc-part-68(2),
                        pub-62411(3),
                        eighty-zeroes(4),
                        fifteen-zeroes(5)
 
                        }
         ACCESS read-write
         STATUS mandatory
         DESCRIPTION
               "The ncmcsu net density enforcement."
         ::= { ncmcsuEnhanceConfigEntry 7 }
         
 
 
        ncmcsuLoopback OBJECT-TYPE
        SYNTAX INTEGER {
                csu-Payload-Loop-Back(1),
                csu-Line-Loop-Back(2),
                repeater-Loop-Back(3),
                csu-Equip-Loop-Back(4),
                csu-No-Loop-Back(5),
                deactivate-LLB-and-PLB(6),
                deactivate-ELB-and-RLB(7),
                send-Inband-Loop-Up(9),
                send-Inband-Loop-Down(10)
                
                        }
                
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION
            " The ncmcsu loop back config. "
 
        ::= { ncmcsuEnhanceConfigEntry 8 }



	ncmcsuTestPattern OBJECT-TYPE
        SYNTAX INTEGER {

		no-test(1),
		qrss-to-net(2),
		one-in-eight-to-net(3),
		three-in-twenty-four-to-net(4),
		all-ones-to-net(5),
		qrss-to-eq(6),
		one-in-eight-to-eq(7),
		three-in-twenty-four-to-eq(8),
		all-ones-to-eq(9)
		
                        }
                
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION
            " The ncmcsu test pattern config. "
 
        ::= { ncmcsuEnhanceConfigEntry 9 }
 

	ncmcsuResetPerfReg OBJECT-TYPE 
        SYNTAX INTEGER { 
 
                no(1),     
                yes(2)       
                        } 
                 
        ACCESS read-write 
        STATUS mandatory 
        DESCRIPTION    
            " The ncmcsu  reset performance monitoring register " 
  
        ::= { ncmcsuEnhanceConfigEntry 10 }


	ncmcsuTestErrorCounter OBJECT-TYPE 
        SYNTAX Gauge 
        ACCESS read-only
        STATUS mandatory 
        DESCRIPTION 
            " The ncmcsu test error counter in the test status  " 
  
        ::= { ncmcsuEnhanceConfigEntry 11 } 
 



	ncmcsuTestSecondsRemain OBJECT-TYPE
        SYNTAX Gauge 
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
            " The ncmcsu test seconds remaining in the test status  "
 
        ::= { ncmcsuEnhanceConfigEntry 12 }



	ncmcsuTestTimeSeconds OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION
            " The csu test time duration. Enter Value (0-142, 255)
              0         corresponds to 0 Sec (Default)
              1 - 59    corresponds to 1-59 Seconds
              60 - 118  corresponds to 1-59 Min
              119 - 142 corresponds to 1-24 HR
              255       corresponds to (Forever) option "

        ::= { ncmcsuEnhanceConfigEntry 13 }


        ncmcsuNetLOFCIndexTime OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION
            " The csu net lofc index time  "

        ::= { ncmcsuEnhanceConfigEntry 14 }



        ncmcsuChannelMask OBJECT-TYPE
        SYNTAX DisplayString (SIZE (0..255))
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION
            " The csu channel mask for the 24 channels  "
 
        ::= { ncmcsuEnhanceConfigEntry 15 }
 
 
        ncmcsuApplication OBJECT-TYPE
        SYNTAX INTEGER  {
                         none(1),
                         csu(2),
                         smds(3)
                        }
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION
            " The csu application mode  "
 
        ::= { ncmcsuEnhanceConfigEntry 16 }
 
 
 
        ncmcsuTestIntervalIndex OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION
            " The csu Test Inteval Index value used for test pattern (0 - config default) " 
        ::= { ncmcsuEnhanceConfigEntry 17 }




  
	  -- The ncmcsu Element Status table
    
        ncmcsuElementStatusOneTable OBJECT-TYPE
        SYNTAX SEQUENCE OF NcmcsuElementStatusOneEntry
        ACCESS not-accessible
        STATUS mandatory
	DESCRIPTION
               " the ncmcsu element status table."
	       --DEFAULT test-function csucommon-test
               --DEFAULT next-function csucommon-next
        ::= { ncm-csu 6002 }

        ncmcsuElementStatusOneEntry OBJECT-TYPE
        SYNTAX NcmcsuElementStatusOneEntry
        ACCESS not-accessible
        STATUS mandatory
        DESCRIPTION
               "An entry in the ncmcsu element status table."
        INDEX   { ncmcsuNIDElementStatusIndex1,ncmcsuElementStatusIndex1 }
        ::= { ncmcsuElementStatusOneTable 1 }


        NcmcsuElementStatusOneEntry ::= SEQUENCE {
	     ncmcsuNIDElementStatusIndex1
		 INTEGER, 
             ncmcsuElementStatusIndex1
                 INTEGER,
             ncmcsuExcessiveError
                 INTEGER,
             ncmcsuOutOfFrame
                 INTEGER,
             ncmcsuNetLossOfSignal
                 INTEGER
                }


	ncmcsuNIDElementStatusIndex1 OBJECT-TYPE
        SYNTAX  INTEGER
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION
	"This variable specifies the node id of the node."        
        ::= { ncmcsuElementStatusOneEntry 1 }

	

        ncmcsuElementStatusIndex1 OBJECT-TYPE
        SYNTAX  INTEGER
        ACCESS  read-only
        STATUS  mandatory
	DESCRIPTION
	"This variable specifies the interface index ."
         ::= { ncmcsuElementStatusOneEntry 2 }
 
 
