	-- the ncm csu Enhanced Current Table

     -- The ncm csu enhanced current table contains various statistics being
     -- collected for the current 15 minute interval.

         ncmcsuEnhancedCurrentTable OBJECT-TYPE
             SYNTAX  SEQUENCE OF NcmcsuEnhancedCurrentEntry
             ACCESS  not-accessible
             STATUS  mandatory
             DESCRIPTION
                "The ncm csu enhanced Current table."
			 --DEFAULT test-function    csucommon-test
                        --DEFAULT set-function    null-set-proc
                        --DEFAULT next-function    csucommon-next
		
            ::= { ncm-csu 6008 }


         ncmcsuEnhancedCurrentEntry OBJECT-TYPE
             SYNTAX  NcmcsuEnhancedCurrentEntry
             ACCESS  not-accessible
             STATUS  mandatory
             DESCRIPTION
                "An entry in the ncmcsu Enhanced Current table."
            INDEX   { ncmcsuNIDEnhancedCurrentIndex, ncmcsuEnhancedCurrentIndex, ncmcsuEnhancedCurrentEndType }
            ::= { ncmcsuEnhancedCurrentTable 1 }

         NcmcsuEnhancedCurrentEntry ::=
         SEQUENCE {
         
             ncmcsuNIDEnhancedCurrentIndex
                INTEGER,
             ncmcsuEnhancedCurrentIndex
                 INTEGER,
	     ncmcsuEnhancedCurrentEndType
		 INTEGER,
             ncmcsuEnhancedCurrentESs
                 Gauge,
             ncmcsuEnhancedCurrentSESs
                 Gauge,           
             ncmcsuEnhancedCurrentUASs
                 Gauge,
	     ncmcsuEnhancedCurrentCSSs
                 Gauge,
             ncmcsuEnhancedCurrentBESs
                 Gauge,
	      ncmcsuEnhancedCurrentLOFC
                Gauge

                     }

	ncmcsuNIDEnhancedCurrentIndex OBJECT-TYPE
             SYNTAX  INTEGER (1..'7fffffff'h)
             ACCESS  read-only
             STATUS  mandatory
             DESCRIPTION
                "This variable specifies the node id of the node"
            ::= { ncmcsuEnhancedCurrentEntry 1 }
 
 
         ncmcsuEnhancedCurrentIndex OBJECT-TYPE
             SYNTAX  INTEGER (1..'7fffffff'h)
             ACCESS  read-only
             STATUS  mandatory
             DESCRIPTION
                "The index value which uniquely identifies  the
                ncmcsu  interface  to which this entry is applica-
                ble.  The interface identified by a  particular
                value  of  this  index is the same interface as
                identified by the same value as a ncmcsuLineIndex
                object instance."
            ::= { ncmcsuEnhancedCurrentEntry 2 }

