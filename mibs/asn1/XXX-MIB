				MAX-ACCESS		read-write
				STATUS			mandatory
				DESCRIPTION		"Center card's current speed mode"
				::= { mc4-25G-OEOCardEntry 2 } 						
				
mc4-25G-OEOLoopback		OBJECT-TYPE
				SYNTAX			INTEGER  {enable(1),disable(2),not-support(3)}
				MAX-ACCESS		read-write
				STATUS			current
