EQUIPE-ATM-MIB DEFINITIONS ::= BEGIN

    --
    -- Equipe ATM MIB
    --

    IMPORTS
        enterprises    
            FROM RFC1155-SMI
        MODULE-IDENTITY, OBJECT-TYPE, Counter32, Counter64
            FROM SNMPv2-SMI
        TEXTUAL-CONVENTION
            FROM SNMPv2-TC;

    equipe        OBJECT IDENTIFIER ::= { enterprises 5022 }

    eqAtmMib MODULE-IDENTITY
        LAST-UPDATED "0009170000Z"
        ORGANIZATION ""
        CONTACT-INFO
            "Jas Parmar
            Equipe Communications
            100 Nagog Park
            Acton MA 01720"
        DESCRIPTION
            "This is the Equipe ATM MIB module."
        ::= { equipe 2 }

    --
    --  ATM Cause codes TC.
    --

    AtmCauseCode ::= TEXTUAL-CONVENTION
        STATUS    current
        DESCRIPTION
                "The atm last cause code values."
        SYNTAX    INTEGER {
                none(1),                    -- none 
                unallocNumber(2),           -- unallocated number
                noRouteToTransitNet(3),     -- no route to tansit net
                noRouteToDest(4),           -- no route to dest
                vccUnacceptable30(5),       -- VPI/VCI unacceptable (UNI 3.0)
                normalCallCleared31(6),     -- normal call cleared
                userBusy(7),                -- user busy
                noUserResponse(8),          -- no user response
                callRejected(9),            -- call rejected
                numberChanged(10),           -- number changed
                callRejectCLIR(11),         -- user rejects all calls with CLIR
                destOutOfOrder(12),         -- destn out of order
                invalidNumberFormat(13),    -- invalid number format
                respToStatusEnquiry(14),    -- response to status enquiry
                normalUnspecified(15),      -- normal unspecified
                reqVccUnavailable(16),      -- requested VPI/VCI unavailable
                vccFail31(17),              -- VPI/VCI assignment failure
                rateUnavailable31(18),      -- rate unavailable
                networkOutOfOrder(19),      -- network out of order
                tempFailure(20),            -- temp failure
                accessInfoDiscarded(21),    -- access info discarded
                resourcesUnavailable(22),   -- resource unavailable
                qosUnavailable(23),         -- QoS unavailable
                rateUnavailable(24),        -- call rate unavailable
                bcapNotAuthorised(25),      -- bearer caps not authorised
                bcapUnavailable(26),        -- bearer caps unavailable
                serviceUnavailable(27),     -- service or option unavailable
                bcapNotImplemented(28),     -- bearer cap not implemented
                combinationUnsupported(29), -- unsupported mix of traffic params
                aalParamsUnsupported31(30), -- unsupported aal params
                invalidCallReference(31),   -- invalid call reference
                noChannel(32),              -- channel does not exist
                destIncompatible(33),       -- incomptible destination
                invalidEndpointRef(34),     -- invalid endpoint
                invalidTransitNet(35),      -- invalid transit net
                tooManyAddPartyRequests(36),-- too many add party requests
                aalParamsUnsupported30(37), -- aal params unsupported
                infoELementMissing(38),     -- mandatory info element missing
                msgTypeNotImplemented(39),  -- msg type not implemented
                infoElementNotImplemented(40), -- info element not implemented
                invalidInfoElement(41),     -- invalid info element
                msgNotCompatible(42),       -- msg not comptaible with call state
                timerRecovery(43),          -- timer expired recovery
                invalidMsgLen(44),          -- invalid msg len
                protocolError(45),          -- protocol error
                opticalElementError(46),    -- optical element error 
                noRouteToNextNode(47),      -- no route to next node
                dtlNotMyNode(48)            -- not my node
        }

    --
    --  ATM Logical Port Stats Table.
    --

    eqAtmStatsTable OBJECT-TYPE
        SYNTAX      SEQUENCE OF EqAtmStatsEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
           "This table contains stats for an ATM logical port."
        ::= { eqAtmMib 1 }
        
    eqAtmStatsEntry OBJECT-TYPE
        SYNTAX      EqAtmStatsEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
                "An ATM logical port stats entry."
        INDEX    { eqAtmStatsIfIndex }
        ::= { eqAtmStatsTable 1 }

    EqAtmStatsEntry ::=
        SEQUENCE {
            eqAtmStatsIfIndex    
                    INTEGER,
            eqAtmStatsInCells        
                    Counter64,
            eqAtmStatsInCellsPerSecond        
                    Counter64,
            eqAtmStatsInBitsPerSecond        
                    Counter64,
            eqAtmStatsInCellsClp0        
                    Counter64,
            eqAtmStatsInCellsClp1
                    Counter64,
            eqAtmStatsInCellsTagged
                    Counter64,
            eqAtmStatsInCellsClp0Discards
                    Counter64,
            eqAtmStatsInCellsClp1Discards
                    Counter64,
            eqAtmStatsInCellsOam
                    Counter64,
            eqAtmStatsInCellsClp0Oam
                    Counter64,
            eqAtmStatsInCellsClp1Oam
                    Counter64,
            eqAtmStatsInCellsLoopback
                    Counter64,
            eqAtmStatsInCellsLoopbackOam
                    Counter64,
            eqAtmStatsOutCells        
                    Counter64,
            eqAtmStatsOutCellsPerSecond        
                    Counter64,
            eqAtmStatsOutBitsPerSecond        
                    Counter64,
            eqAtmStatsOutCellsClp0        
                    Counter64,
            eqAtmStatsOutCellsClp1
                    Counter64,
            eqAtmStatsOutCellsTagged
                    Counter64,
            eqAtmStatsOutCellsClp0Discards
                    Counter64,
            eqAtmStatsOutCellsClp1Discards
                    Counter64,
            eqAtmStatsOutCellsOam
                    Counter64,
            eqAtmStatsOutCellsClp0Oam
                    Counter64,
            eqAtmStatsOutCellsClp1Oam
                    Counter64,
            eqAtmStatsOutCellsLoopback
                    Counter64,
            eqAtmStatsOutCellsLoopbackOam
                    Counter64,
            eqAtmStatsCLR        
                    INTEGER
        }

    eqAtmStatsIfIndex OBJECT-TYPE
        SYNTAX      INTEGER (1..2147483647)
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "The ifIndex for the corresponding atm logical port."
        ::= { eqAtmStatsEntry 1 }

    eqAtmStatsInCells OBJECT-TYPE
        SYNTAX      Counter64 
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "The number of cells received."
        ::= { eqAtmStatsEntry 2 }

    eqAtmStatsInCellsPerSecond OBJECT-TYPE
        SYNTAX      Counter64 
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "The number of cells received per second."
        ::= { eqAtmStatsEntry 3 }

    eqAtmStatsInBitsPerSecond OBJECT-TYPE
        SYNTAX      Counter64 
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "The number of bits received per second."
        ::= { eqAtmStatsEntry 4 }

    eqAtmStatsInCellsClp0 OBJECT-TYPE
        SYNTAX      Counter64 
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "The number of CLP-0 cells received."
        ::= { eqAtmStatsEntry 5 }

    eqAtmStatsInCellsClp1 OBJECT-TYPE
        SYNTAX      Counter64 
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "The number of CLP-1 cells received."
        ::= { eqAtmStatsEntry 6 }

    eqAtmStatsInCellsTagged OBJECT-TYPE
        SYNTAX      Counter64 
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "The number of tagged cells received."
        ::= { eqAtmStatsEntry 7 }

    eqAtmStatsInCellsClp0Discards OBJECT-TYPE
        SYNTAX      Counter64 
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "The number of CLP-0 cells received and discarded."
        ::= { eqAtmStatsEntry 8 }

    eqAtmStatsInCellsClp1Discards OBJECT-TYPE
        SYNTAX      Counter64 
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "The number of CLP-1 cells received and discarded."
        ::= { eqAtmStatsEntry 9 }

    eqAtmStatsInCellsOam OBJECT-TYPE
        SYNTAX      Counter64 
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "The number of OAM cells received."
        ::= { eqAtmStatsEntry 10 }

    eqAtmStatsInCellsClp0Oam OBJECT-TYPE
        SYNTAX      Counter64 
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "The number of CLP-0 OAM cells received."
        ::= { eqAtmStatsEntry 11 }

    eqAtmStatsInCellsClp1Oam OBJECT-TYPE
        SYNTAX      Counter64 
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "The number of CLP-1 OAM cells received."
        ::= { eqAtmStatsEntry 12 }

    eqAtmStatsInCellsLoopback OBJECT-TYPE
        SYNTAX      Counter64 
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "The number of loopback cells received."
        ::= { eqAtmStatsEntry 13 }

    eqAtmStatsInCellsLoopbackOam OBJECT-TYPE
        SYNTAX      Counter64 
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "The number of loopback OAM cells received."
        ::= { eqAtmStatsEntry 14 }

    eqAtmStatsOutCells OBJECT-TYPE
        SYNTAX      Counter64 
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "The number of cells transmitted."
        ::= { eqAtmStatsEntry 15 }

    eqAtmStatsOutCellsPerSecond OBJECT-TYPE
        SYNTAX      Counter64 
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "The number of cells transmitted per second."
        ::= { eqAtmStatsEntry 16 }

    eqAtmStatsOutBitsPerSecond OBJECT-TYPE
        SYNTAX      Counter64 
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "The number of bits transmitted per second."
        ::= { eqAtmStatsEntry 17 }

    eqAtmStatsOutCellsClp0 OBJECT-TYPE
        SYNTAX      Counter64 
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "The number of CLP-0 cells transmitted."
        ::= { eqAtmStatsEntry 18 }

    eqAtmStatsOutCellsClp1 OBJECT-TYPE
        SYNTAX      Counter64 
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "The number of CLP-1 cells transmitted."
        ::= { eqAtmStatsEntry 19 }

    eqAtmStatsOutCellsTagged OBJECT-TYPE
        SYNTAX      Counter64 
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "The number of tagged cells transmitted."
        ::= { eqAtmStatsEntry 20 }

    eqAtmStatsOutCellsClp0Discards OBJECT-TYPE
        SYNTAX      Counter64 
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "The number of CLP-0 cells discarded due to 
            lack of resources."
        ::= { eqAtmStatsEntry 21 }

    eqAtmStatsOutCellsClp1Discards OBJECT-TYPE
        SYNTAX      Counter64 
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "The number of CLP-1 cells discarded due to 
            lack of resources."
        ::= { eqAtmStatsEntry 22 }

    eqAtmStatsOutCellsOam OBJECT-TYPE
        SYNTAX      Counter64 
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "The number of OAM cells transmitted."
        ::= { eqAtmStatsEntry 23 }

    eqAtmStatsOutCellsClp0Oam OBJECT-TYPE
        SYNTAX      Counter64 
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "The number of CLP-0 OAM cells transmitted."
        ::= { eqAtmStatsEntry 24 }

    eqAtmStatsOutCellsClp1Oam OBJECT-TYPE
        SYNTAX      Counter64 
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "The number of CLP-1 OAM cells transmitted."
        ::= { eqAtmStatsEntry 25 }

    eqAtmStatsOutCellsLoopback OBJECT-TYPE
        SYNTAX      Counter64 
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "The number of loopback cells transmitted."
        ::= { eqAtmStatsEntry 26 }

    eqAtmStatsOutCellsLoopbackOam OBJECT-TYPE
        SYNTAX      Counter64 
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "The number of loopback OAM cells transmitted."
        ::= { eqAtmStatsEntry 27 }

    eqAtmStatsCLR OBJECT-TYPE
        SYNTAX      INTEGER (0..512)
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "The cell loss ratio requirement, expressed as a negative
            exponent of 10. The default value of 512 indicates any
            cell loss ratio is acceptable."
        ::= { eqAtmStatsEntry 28 }


    --
    --  ATM Logical Port SVC Stats Table.
    --

    eqAtmSvcStatsTable OBJECT-TYPE
        SYNTAX      SEQUENCE OF EqAtmSvcStatsEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
           "This table contains svc stats for an ATM logical 
            port." 
        ::= { eqAtmMib 2 }
        
    eqAtmSvcStatsEntry OBJECT-TYPE
        SYNTAX      EqAtmSvcStatsEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
                "An ATM logical port svc stats entry."
        INDEX    { eqAtmSvcStatsIfIndex }
        ::= { eqAtmSvcStatsTable 1 }

    EqAtmSvcStatsEntry ::=
        SEQUENCE {
            eqAtmSvcStatsIfIndex    
                    INTEGER,
            eqAtmSvcTotal
                    Counter32,
            eqAtmSvcConnected
                    Counter32,
            eqAtmSvcFailures
                    Counter32,
            eqAtmSvcInLastCauseCode
                    AtmCauseCode,
            eqAtmSvcInSetupPdus
                    Counter32,
            eqAtmSvcInCallProceeding
                    Counter32,
            eqAtmSvcInPdusConnect
                    Counter32,
            eqAtmSvcInPdusConnectAck
                    Counter32,
            eqAtmSvcInPdusReleaseComplete
                    Counter32,
            eqAtmSvcInPdusAddParty
                    Counter32,
            eqAtmSvcInPdusAddPartyAck
                    Counter32,
            eqAtmSvcInPdusAddPartyReject
                    Counter32,
            eqAtmSvcInPdusDropParty
                    Counter32,
            eqAtmSvcInPdusDropPartyAck
                    Counter32,
            eqAtmSvcInPdusStatusEnquiry
                    Counter32,
            eqAtmSvcInPdusStatus
                    Counter32,
            eqAtmSvcInPdusRestart
                    Counter32,
            eqAtmSvcInPdusRestartAck
                    Counter32,
            eqAtmSvcInPdusNotify
                    Counter32,
            eqAtmSvcInPdusProgress
                    Counter32,
            eqAtmSvcInPdusAlerting
                    Counter32,
            eqAtmSvcInPdusPartyAlerting
                    Counter32,
            eqAtmSvcOutLastCauseCode
                    AtmCauseCode,
            eqAtmSvcOutSetupPdus
                    Counter32,
            eqAtmSvcOutCallProceeding
                    Counter32,
            eqAtmSvcOutPdusConnect
                    Counter32,
            eqAtmSvcOutPdusConnectAck
                    Counter32,
            eqAtmSvcOutPdusReleaseComplete
                    Counter32,
            eqAtmSvcOutPdusAddParty
                    Counter32,
            eqAtmSvcOutPdusAddPartyAck
                    Counter32,
            eqAtmSvcOutPdusAddPartyReject
                    Counter32,
            eqAtmSvcOutPdusDropParty
                    Counter32,
            eqAtmSvcOutPdusDropPartyAck
                    Counter32,
            eqAtmSvcOutPdusStatusEnquiry
                    Counter32,
            eqAtmSvcOutPdusStatus
                    Counter32,
            eqAtmSvcOutPdusRestart
                    Counter32,
            eqAtmSvcOutPdusRestartAck
                    Counter32,
            eqAtmSvcOutPdusNotify
                    Counter32,
            eqAtmSvcOutPdusProgress
                    Counter32,
            eqAtmSvcOutPdusAlerting
                    Counter32,
            eqAtmSvcOutPdusPartyAlerting
                    Counter32
        }

    eqAtmSvcStatsIfIndex OBJECT-TYPE
        SYNTAX      INTEGER (1..2147483647)
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "The ifIndex for the corresponding atm logical port."
        ::= { eqAtmSvcStatsEntry 1 }

    eqAtmSvcTotal OBJECT-TYPE
        SYNTAX      Counter32 
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "The total number of svc's established on this port."
        ::= { eqAtmSvcStatsEntry 2 }

    eqAtmSvcConnected OBJECT-TYPE
        SYNTAX      Counter32 
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "The number of svc's currently active on this port."
        ::= { eqAtmSvcStatsEntry 3 }

    eqAtmSvcFailures OBJECT-TYPE
        SYNTAX      Counter32 
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "The number of svc configuration failures on this port."
        ::= { eqAtmSvcStatsEntry 4 }

    eqAtmSvcInLastCauseCode OBJECT-TYPE
        SYNTAX      AtmCauseCode 
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "The last cause code received on this port."
        ::= { eqAtmSvcStatsEntry 5 }

    eqAtmSvcInSetupPdus OBJECT-TYPE
        SYNTAX      Counter32 
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
