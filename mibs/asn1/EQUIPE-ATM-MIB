    --
    --  ATM OAM Performance Monitoring Table.
    --

    eqAtmOamPmTable OBJECT-TYPE
        SYNTAX      SEQUENCE OF EqAtmOamPmEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
           "This table contains OAM Performance Monitoring
            stats for an ATM OAM PM Session."
        ::= { eqAtmMib 5 }

    eqAtmOamPmEntry OBJECT-TYPE
        SYNTAX      EqAtmOamPmEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
                "An ATM logical port stats entry."
        INDEX    { eqAtmOamPmIfIndex,
                   eqAtmOamPmVpi,
                   eqAtmOamPmVci }
        ::= { eqAtmOamPmTable 1 }

    EqAtmOamPmEntry ::=
        SEQUENCE {
            eqAtmOamPmIfIndex
                    INTEGER,
            eqAtmOamPmVpi
                    INTEGER,
            eqAtmOamPmVci
                    INTEGER,
            eqAtmOamPmStatus
                    INTEGER,
            eqAtmOamPmSessionType
                    INTEGER,
            eqAtmOamPmSinkSource
                    INTEGER,
            eqAtmOamPmBlockSize
                    INTEGER,
            eqAtmOamPmEndPt
                    INTEGER,
            eqAtmOamPmFlow
                    INTEGER,
            eqAtmOamPmDirection
                    INTEGER,
            eqAtmOamPmTimeout
                    INTEGER,
            eqAtmOamPmRetryCount
                    INTEGER,
            eqAtmOamPmFwdTxUserCellsClp0
                    Counter32,
            eqAtmOamPmFwdTxUserCellsClp01
                    Counter32,
            eqAtmOamPmFwdTxLostCellsClp0
                    Counter32,
            eqAtmOamPmFwdTxLostCellsClp01
                    Counter32,
            eqAtmOamPmFwdSECBs
                    Counter32,
            eqAtmOamPmBwdTxUserCellsClp0
                    Counter32,
            eqAtmOamPmBwdTxUserCellsClp01
                    Counter32,
            eqAtmOamPmBwdTxLostCellsClp0
                    Counter32,
            eqAtmOamPmBwdTxLostCellsClp01
                    Counter32,
            eqAtmOamPmBwdSECBs
                    Counter32
        }

    eqAtmOamPmIfIndex OBJECT-TYPE
        SYNTAX      INTEGER (1..2147483647)
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "The ifIndex for the corresponding atm logical port."
        ::= { eqAtmOamPmEntry 1 }

    eqAtmOamPmVpi OBJECT-TYPE
        SYNTAX      INTEGER (1..4095)
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "The Virtual Path Identifier."
        ::= { eqAtmOamPmEntry 2 }

    eqAtmOamPmVci OBJECT-TYPE
        SYNTAX      INTEGER (1..65535)
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "The Virtual Channel Identifier."
        ::= { eqAtmOamPmEntry 3 }

    eqAtmOamPmStatus OBJECT-TYPE
        SYNTAX      INTEGER {
                activating(1),
                connected(2),
                failConnect(3),
                failNoResource(4)
        }
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "Indicates whether the activation process is in
             the middle of activating, has successfully
             connected, has failed to connect because the
             remote end did not respond, or couldn't setup
             the connection because of lack of local resources."
        ::= { eqAtmOamPmEntry 4 }

    eqAtmOamPmSessionType OBJECT-TYPE
        SYNTAX      INTEGER {
                contCheck(1),
                perfMonitoring(2)
        }
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "Identifies the PM session to perform Continuity Check
             or Performance Monitoring."
        ::= { eqAtmOamPmEntry 5 }

    eqAtmOamPmSinkSource OBJECT-TYPE
        SYNTAX      INTEGER {
                sink(1),
                source(2),
                bidirectional(3)
        }
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "Identifies the PM session to be sink, source, or both."
        ::= { eqAtmOamPmEntry 6 }

    eqAtmOamPmBlockSize OBJECT-TYPE
        SYNTAX      INTEGER {
                bytes128(1),
                bytes256(2),
                bytes512(3),
                bytes1K(4),
                bytes2K(5),
                bytes4K(6),
                bytes8K(7),
                bytes16K(8),
                bytes32K(9)
        }
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "The Block Size of User cells transmitted before an
             OAM PM cell is transmitted."
        ::= { eqAtmOamPmEntry 7 }

    eqAtmOamPmEndPt OBJECT-TYPE
        SYNTAX      INTEGER {
                segment(1),
                endToEnd(2)
        }
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "Identifies the PM Session as either a Segment Endpoint
             or an End-to-End Point"
        ::= { eqAtmOamPmEntry 8 }

    eqAtmOamPmFlow OBJECT-TYPE
        SYNTAX      INTEGER {
                f4(1),
                f5(2)
        }
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "Identifies the Flow as either an F4 (Virtual Path)
             or F5 (Virtual Channel) flow."
        ::= { eqAtmOamPmEntry 9 }

    eqAtmOamPmDirection OBJECT-TYPE
        SYNTAX      INTEGER {
                segment(1),
                endToEnd(2)
        }
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "Identifies the direction as either
                Forward (Egress, toward the outside netword)
             or
                Backward (Ingress, toward the switch fabric)"
        ::= { eqAtmOamPmEntry 10 }

    eqAtmOamPmTimeout OBJECT-TYPE
        SYNTAX      INTEGER (1..60)
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "Identifies the Time Period in which to timeout an
             Activation/DeActivation Cell (in which case another
             cell may be sent depending on the Retry Count)."
        ::= { eqAtmOamPmEntry 11 }

    eqAtmOamPmRetryCount OBJECT-TYPE
        SYNTAX      INTEGER (1..10)
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "Number of times to retry Activation in the event
             of a failure (no response)."
        ::= { eqAtmOamPmEntry 12 }

    eqAtmOamPmFwdTxUserCellsClp0 OBJECT-TYPE
        SYNTAX      Counter32
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "The number of Clp0 cells transmitted."
        ::= { eqAtmOamPmEntry 13 }

    eqAtmOamPmFwdTxUserCellsClp01 OBJECT-TYPE
        SYNTAX      Counter32
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "The number of Clp0 plus Clp1 cells transmitted."
        ::= { eqAtmOamPmEntry 14 }

    eqAtmOamPmFwdTxLostCellsClp0 OBJECT-TYPE
        SYNTAX      Counter32
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "The number of Clp0 cells Lost."
        ::= { eqAtmOamPmEntry 15 }

    eqAtmOamPmFwdTxLostCellsClp01 OBJECT-TYPE
        SYNTAX      Counter32
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "The number of Clp0 plus Clp1 cells Lost."
        ::= { eqAtmOamPmEntry 16 }

    eqAtmOamPmFwdSECBs OBJECT-TYPE
        SYNTAX      Counter32
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "The number of Severely Errored Cell Blocks."
        ::= { eqAtmOamPmEntry 17 }

    eqAtmOamPmBwdTxUserCellsClp0 OBJECT-TYPE
        SYNTAX      Counter32
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "The number of Clp0 cells transmitted."
        ::= { eqAtmOamPmEntry 18 }

    eqAtmOamPmBwdTxUserCellsClp01 OBJECT-TYPE
        SYNTAX      Counter32
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "The number of Clp0 plus Clp1 cells transmitted."
        ::= { eqAtmOamPmEntry 19 }

    eqAtmOamPmBwdTxLostCellsClp0 OBJECT-TYPE
        SYNTAX      Counter32
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "The number of Clp0 cells Lost."
        ::= { eqAtmOamPmEntry 20 }

    eqAtmOamPmBwdTxLostCellsClp01 OBJECT-TYPE
        SYNTAX      Counter32
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "The number of Clp0 plus Clp1 cells Lost."
        ::= { eqAtmOamPmEntry 21 }

    eqAtmOamPmBwdSECBs OBJECT-TYPE
        SYNTAX      Counter32
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "The number of Severely Errored Cell Blocks."
        ::= { eqAtmOamPmEntry 22 }


    --
    --  ATM OAM Loopback Table.
    --

    eqAtmOamLoopbackTable OBJECT-TYPE
        SYNTAX      SEQUENCE OF EqAtmOamLoopbackEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
           "This table contains OAM Performance Monitoring
            stats for an ATM OAM PM Session."
        ::= { eqAtmMib 6 }

    eqAtmOamLoopbackEntry OBJECT-TYPE
        SYNTAX      EqAtmOamLoopbackEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
                "An ATM logical port stats entry."
        INDEX    { eqAtmOamLoopbackIfIndex,
                   eqAtmOamLoopbackVpi,
                   eqAtmOamLoopbackVci }
        ::= { eqAtmOamLoopbackTable 1 }

    EqAtmOamLoopbackEntry ::=
        SEQUENCE {
            eqAtmOamLoopbackIfIndex
                    INTEGER,
            eqAtmOamLoopbackVpi
                    INTEGER,
            eqAtmOamLoopbackVci
                    INTEGER,
            eqAtmOamLoopbackStatus
                    INTEGER,
            eqAtmOamLoopbackSendCount
                    INTEGER,
            eqAtmOamLoopbackSendTrap
                    INTEGER,
            eqAtmOamLoopbackEndPoint
                    INTEGER,
            eqAtmOamLoopbackFlow
                    INTEGER,
            eqAtmOamLoopbackDirection
                    INTEGER,
            eqAtmOamLoopbackTimeout
                    INTEGER,
            eqAtmOamLoopbackCellsReceived
                    Counter32,
            eqAtmOamLoopbackCellsTimedOut
                    Counter32,
            eqAtmOamLoopbackAveCellTime
                    INTEGER,
            eqAtmOamLoopbackMinCellTime
                    INTEGER,
            eqAtmOamLoopbackMaxCellTime
                    INTEGER,
            eqAtmOamLoopbackTotalCellTime
                    INTEGER
        }

    eqAtmOamLoopbackIfIndex OBJECT-TYPE
        SYNTAX      INTEGER (1..2147483647)
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "The ifIndex for the corresponding atm logical port."
        ::= { eqAtmOamLoopbackEntry 1 }

    eqAtmOamLoopbackVpi OBJECT-TYPE
        SYNTAX      INTEGER (1..4095)
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "The Virtual Path Identifier."
        ::= { eqAtmOamLoopbackEntry 2 }

    eqAtmOamLoopbackVci OBJECT-TYPE
        SYNTAX      INTEGER (1..65535)
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "The Virtual Channel Identifier."
        ::= { eqAtmOamLoopbackEntry 3 }

    eqAtmOamLoopbackStatus OBJECT-TYPE
        SYNTAX      INTEGER {
                inProgress(1),
                completed(2)
        }
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "Indicates whether the Loopback request is being worked on
             or has completed."
        ::= { eqAtmOamLoopbackEntry 4 }

    eqAtmOamLoopbackSendCount OBJECT-TYPE
        SYNTAX      INTEGER (1..100)
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "Indicates the number of Loopback Cells to send."
        ::= { eqAtmOamLoopbackEntry 5 }

    eqAtmOamLoopbackSendTrap OBJECT-TYPE
        SYNTAX      INTEGER {
                send(1),
                noAction(2)
        }
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "Indicates whether or not to send an SNMP trap when finished."
        ::= { eqAtmOamLoopbackEntry 6 }

    eqAtmOamLoopbackEndPoint OBJECT-TYPE
        SYNTAX      INTEGER {
                segment(1),
                endToEnd(2)
        }
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "Indicates whether the Loopback Cell to be sent is a Segment
             or End-to-End Point type cell."
        ::= { eqAtmOamLoopbackEntry 7 }

    eqAtmOamLoopbackFlow OBJECT-TYPE
        SYNTAX      INTEGER {
                f4(1),
                f5(2)
        }
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "Identifies the Flow as either an F4 (Virtual Path)
             or F5 (Virtual Channel) flow."
        ::= { eqAtmOamLoopbackEntry 8 }

    eqAtmOamLoopbackDirection OBJECT-TYPE
        SYNTAX      INTEGER {
                segment(1),
                endToEnd(2)
        }
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "Identifies the direction as either
                Forward (Egress, toward the outside netword)
             or
                Backward (Ingress, toward the switch fabric)"
        ::= { eqAtmOamLoopbackEntry 9 }

    eqAtmOamLoopbackTimeout OBJECT-TYPE
        SYNTAX      INTEGER (1..60)
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "Indicates the Timeout (in seconds) to wait for a Loopback
             response before sending another (or finishing)."
        ::= { eqAtmOamLoopbackEntry 10 }

    eqAtmOamLoopbackCellsReceived OBJECT-TYPE
        SYNTAX      Counter32
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "Count of the number of Loopback cell responses received
             succesfully."
        ::= { eqAtmOamLoopbackEntry 11 }

    eqAtmOamLoopbackCellsTimedOut OBJECT-TYPE
        SYNTAX      Counter32
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "Count of the number of Loopback cell responses not received
             succesfully."
        ::= { eqAtmOamLoopbackEntry 12 }

    eqAtmOamLoopbackAveCellTime OBJECT-TYPE
        SYNTAX      INTEGER (1..2147483647)
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "Average response time (in milliseconds) for the Loopback Cell."
        ::= { eqAtmOamLoopbackEntry 13 }

    eqAtmOamLoopbackMinCellTime OBJECT-TYPE
        SYNTAX      INTEGER (1..2147483647)
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "Minimum response time (in milliseconds) for the Loopback Cell."
        ::= { eqAtmOamLoopbackEntry 14 }

    eqAtmOamLoopbackMaxCellTime OBJECT-TYPE
        SYNTAX      INTEGER (1..2147483647)
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "Maximum response time (in milliseconds) for the Loopback Cell."
        ::= { eqAtmOamLoopbackEntry 15 }

    eqAtmOamLoopbackTotalCellTime OBJECT-TYPE
        SYNTAX      INTEGER (1..2147483647)
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "Total response time (in milliseconds) for all Loopback Cells
             of the session."
        ::= { eqAtmOamLoopbackEntry 16 }



END
