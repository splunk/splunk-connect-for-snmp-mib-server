--
-- OMNIS Trap group
--
omnisStopTrap     TRAP-TYPE
        ENTERPRISE sniOmnisTraps
        VARIABLES  {  omnisSettingsAppName    }
        DESCRIPTION "Sending a TRAP, if an OMNIS has been terminated "
      ::= 301
omnisStartTrap     TRAP-TYPE
        ENTERPRISE sniOmnisTraps
        VARIABLES  {  omnisSettingsAppName    }
        DESCRIPTION "Sending a TRAP, if an OMNIS has been activated "
      ::= 302
omnisConnStopTrap     TRAP-TYPE
        ENTERPRISE sniOmnisTraps
        VARIABLES  {omnisSettingsAppName,omnisPartnerInfoPtnName,omnisPartnerInfoProName,omnisPartnerInfoPAC}
        DESCRIPTION "Sending a TRAP, if a critical Connection has been deactivated "
      ::= 303
omnisDstConnStopTrap  TRAP-TYPE
        ENTERPRISE sniOmnisTraps
        VARIABLES  {omnisSettingsAppName,omnisTerminalInfoPtnName,omnisTerminalInfoProName,omnisTerminalInfoTID}
        DESCRIPTION "Sending a TRAP, if an OMNIS Session has been normally terminated"
      ::= 304
omnisEventTrap     TRAP-TYPE
        ENTERPRISE sniOmnisTraps
        VARIABLES  {omnisSettingsAppName,omnisPartnerInfoPtnName,omnisPartnerInfoProName,omnisPartnerInfoPAC}
        DESCRIPTION "Sending a TRAP, if a critical Omnis message arrived "
      ::= 305
omnisDstLevelTrap     TRAP-TYPE
        ENTERPRISE sniOmnisTraps
        VARIABLES  {omnisSettingsAppName}
        DESCRIPTION "Sending a TRAP, if DSTMAX has been reached "
      ::= 306
omnisPacLevelTrap     TRAP-TYPE
        ENTERPRISE sniOmnisTraps
        VARIABLES  {omnisSettingsAppName}
        DESCRIPTION "Sending a TRAP, if PACMAX has been reached "
      ::= 307
omnisPtnLevelTrap     TRAP-TYPE
        ENTERPRISE sniOmnisTraps
        VARIABLES  {omnisSettingsAppName}
        DESCRIPTION "Sending a TRAP, if PTNMAX has been reached "
      ::= 308
omnisMuxConnStopTrap     TRAP-TYPE
        ENTERPRISE sniOmnisTraps
        VARIABLES  {omnisSettingsAppName,omnisPartnerInfoPtnName,omnisPartnerInfoProName,omnisPartnerInfoPAC}
        DESCRIPTION "Sending a TRAP, if a critical MUX Connection has been deactivated "
      ::= 309
omnisOmnConnStopTrap     TRAP-TYPE
        ENTERPRISE sniOmnisTraps
        VARIABLES  {omnisSettingsAppName,omnisPartnerInfoPtnName,omnisPartnerInfoProName,omnisPartnerInfoPAC}
        DESCRIPTION "Sending a TRAP, if a critical OMNIS-OMNIS Connection has been deactivated"
      ::= 310
omnisHcConnStopTrap     TRAP-TYPE
        ENTERPRISE sniOmnisTraps
        VARIABLES  {omnisSettingsAppName,omnisPartnerInfoPtnName,omnisPartnerInfoProName,omnisPartnerInfoPAC}
        DESCRIPTION "Sending a TRAP, if a critical Hardcopy Connection has been deactivated"
      ::= 311
omnisDumpWriteTrap     TRAP-TYPE
        ENTERPRISE sniOmnisTraps
        VARIABLES  {omnisSettingsAppName}
        DESCRIPTION "Sending a TRAP, if OMNIS has started writing a Dump"
      ::= 312
omnisDumpEndTrap     TRAP-TYPE
        ENTERPRISE sniOmnisTraps
        VARIABLES  {omnisSettingsAppName}
        DESCRIPTION "Sending a TRAP, if OMNIS has finished writing a Dump"
      ::= 313
omnisEndTrap     TRAP-TYPE
        ENTERPRISE sniOmnisTraps
        VARIABLES  {omnisSettingsAppName}
        DESCRIPTION "Sending a TRAP,if OMNIS has been normally terminated "
      ::= 314
 
--
-- OMNIS User defined Traps
--
 
omnisUserdefinedTrapData   OBJECT IDENTIFIER ::= { sniOmnisUserdefinedTraps 1 }
 
omnisTrapMsgText      OBJECT-TYPE
            SYNTAX  DisplayString
            ACCESS  read-only
            STATUS  mandatory
            DESCRIPTION "OMNIS Message-Text generating a Trap"
            ::= {omnisUserdefinedTrapData  1}
 
omnisUserdefinedTrap   OBJECT IDENTIFIER ::= { sniOmnisUserdefinedTraps 10 }
 
omnisGeneralTrap     TRAP-TYPE
        ENTERPRISE omnisUserdefinedTrap
        VARIABLES  {omnisSettingsAppName,omnisTrapMsgText}
        DESCRIPTION "Sending a TRAP, in all user-defined cases"
      ::= 320
 
END
