        ::= {sniOmnisMux  1}
omnisMuxTabNum             OBJECT-TYPE
        SYNTAX  INTEGER
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION "The number of entries in the omnisMuxTable"
        ::= {sniOmnisMux 2}
omnisMuxTable           OBJECT-TYPE
        SYNTAX  SEQUENCE OF OmnisMuxEntry
        ACCESS  not-accessible
        STATUS  mandatory
        DESCRIPTION "The OMNIS Mux table"
        ::= {sniOmnisMux  3}
omnisMuxEntry        OBJECT-TYPE
        SYNTAX  OmnisMuxEntry
        ACCESS  not-accessible
        STATUS  mandatory
        DESCRIPTION "An entry in the table"
        INDEX {omnisMuxID}
        ::= {omnisMuxTable 1}
OmnisMuxEntry ::=
        SEQUENCE
        {
            omnisMuxID                      INTEGER,
            omnisMuxPtnName                 DisplayString,
            omnisMuxProName                 DisplayString,
            omnisMuxState                   INTEGER,
            omnisMuxConnect                 INTEGER,
            omnisMuxLPass                   INTEGER,
            omnisMuxSessions                INTEGER,
            omnisMuxAvailability            INTEGER
        }
omnisMuxID             OBJECT-TYPE
        SYNTAX  INTEGER
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION "Identifier of MUX"
        ::= {omnisMuxEntry 1}
omnisMuxPtnName             OBJECT-TYPE
        SYNTAX  DisplayString
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION "Partner name"
        ::= {omnisMuxEntry 2 }
omnisMuxProName             OBJECT-TYPE
        SYNTAX  DisplayString
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION "Processor name"
        ::= {omnisMuxEntry 3 }
omnisMuxState              OBJECT-TYPE
        SYNTAX  INTEGER {
                         cls-p   (1),
                         opn     (2),
                         act     (3),
                         los     (4),
                         cls     (5),
                         inact   (6),
                         cancel  (7),
                         unknown  (99)
                        }
        ACCESS  read-write
        STATUS  mandatory
        DESCRIPTION "The state of the MUX"
        ::= {omnisMuxEntry 4 }
omnisMuxConnect             OBJECT-TYPE
        SYNTAX  INTEGER {
                         start     (1),
                         opncon    (2),
                         unknown  (99)
                        }
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION "The state of connection"
        ::= {omnisMuxEntry 5 }
omnisMuxLPass             OBJECT-TYPE
        SYNTAX  INTEGER {
                         yes      (1),
                         no       (2),
                         unknown  (99)
                        }
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION "The connection password"
        ::= {omnisMuxEntry 6 }
omnisMuxSessions         OBJECT-TYPE
        SYNTAX  INTEGER
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION "number of actual sessions"
        ::= {omnisMuxEntry 7 }
omnisMuxAvailability     OBJECT-TYPE
        SYNTAX  INTEGER {
                         yes      (1),
                         no       (2),
                         unknown  (99)
                        }
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION "The availability"
        ::= {omnisMuxEntry 8 }
