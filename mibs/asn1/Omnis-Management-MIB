Omnis-Management-MIB DEFINITIONS ::= BEGIN
--
-- Title:   MIB for the BS2000 Omnis Management
-- Date:    Januar, 1997
-- Authors: Rudolph           BS2000 DC3
--
-- Update am 28.7.1997
--  Transportparameter in Trace Gruppe hinzugefuegt
--
-- Update am 09.10.1997
-- Objekte, die sich nur durch Gross-/Kleinschreibung unterscheiden
-- modifiziert.
--
-- Update am 21.10.1997
-- GlobalData Gruppe erweitert
-- Neu Gruppe GlobalDataEntry erzeugt
-- Author: Veges
--
-- Update am 12.11.1997
-- sniOmnisGlobalData: Subagent Version wieder eingefuehrt
-- Trap Nummerierung neu gemacht
-- Author: Rudolph
--
-- Update am 13.11.1997
-- sniOmnisGlobalData: SubagentVersion Name und Typ korrigiert
-- Author: Veges
--
-- Update am 19.1.1998
-- sniomnisTerminalInfo: Objekte SNMP, Trans-Prot, HardcopyForm
--                       hinzugefuegt
-- sniomnisPartnerInfo:  Objekte SNMP, Connect, Berid, pac-prefix
--                       hinzugefuegt
-- Author: Rudolph
--
-- Update am 20.1.1998
-- omnisGlobalDataActName und omnisGlobalDataActID auf read-write gesetzt
--
--
-- Update am 06.02.1998
-- Beschreibung der einzelnen Objekte
-- Author: Vaziri
--
--
-- Update am 20.10.1998
-- Neue Objekte: omnisSettingsEnd, omnisSettingsEndPassw
-- Auf read-write gesetzt: omnisOmnisState,omnisMuxState
-- Korrigierte Konstantennamen fuer: omnisSettingsExitPri,omnisSettingsLoggPri
-- Konstantenmenge mit ..._omnis erweitert fuer:
--    omnisSettingsLine25,omnisTerminalInfoLine25,omnisPartnerInfoLine25
-- Author: Veges
--
--
-- Update am 22.10.1998
-- Neue Traps: 309-314,320
-- Neue Gruppe:UserdefinedTraps,Neues Objekt:TrapMsgText
-- Author: Veges
--
--
-- MIB-Version: 1
IMPORTS
	TRAP-TYPE FROM RFC-1215
        enterprises
            FROM RFC1155-SMI
        DisplayString
            FROM RFC1213-MIB
        OBJECT-TYPE
            FROM RFC-1212;
sni                         OBJECT IDENTIFIER ::= { enterprises 231 }
sniProductMibs              OBJECT IDENTIFIER ::= { sni 2 }
sniOmnis                    OBJECT IDENTIFIER ::= { sniProductMibs 31}
sniOmnisGlobalData          OBJECT IDENTIFIER ::= { sniOmnis 1 }
sniOmnisSettings            OBJECT IDENTIFIER ::= { sniOmnis 2 }
sniOmnisParameters          OBJECT IDENTIFIER ::= { sniOmnis 3 }
sniOmnisTerminals           OBJECT IDENTIFIER ::= { sniOmnis 4 }
sniOmnisTerminalInfo        OBJECT IDENTIFIER ::= { sniOmnis 5 }
sniOmnisPartners            OBJECT IDENTIFIER ::= { sniOmnis 6 }
sniOmnisPartnerInfo         OBJECT IDENTIFIER ::= { sniOmnis 7 }
sniOmnisGroups              OBJECT IDENTIFIER ::= { sniOmnis 8 }
sniOmnisHardCopy            OBJECT IDENTIFIER ::= { sniOmnis 9 }
sniOmnisHardCopyPIDs        OBJECT IDENTIFIER ::= { sniOmnis 10 }
sniOmnisHardCopyTIDs        OBJECT IDENTIFIER ::= { sniOmnis 11 }
sniOmnisHardCopyCreate      OBJECT IDENTIFIER ::= { sniOmnis 12 }
sniOmnisOmnis               OBJECT IDENTIFIER ::= { sniOmnis 13 }
sniOmnisMux                 OBJECT IDENTIFIER ::= { sniOmnis 14 }
sniOmnisExits               OBJECT IDENTIFIER ::= { sniOmnis 15 }
sniOmnisExitPIDs            OBJECT IDENTIFIER ::= { sniOmnis 16 }
sniOmnisExitTIDs            OBJECT IDENTIFIER ::= { sniOmnis 17 }
sniOmnisExitCreate          OBJECT IDENTIFIER ::= { sniOmnis 18 }
sniOmnisTrace               OBJECT IDENTIFIER ::= { sniOmnis 19 }
sniOmnisTraps               OBJECT IDENTIFIER ::= { sniOmnis 20 }
sniOmnisUserdefinedTraps    OBJECT IDENTIFIER ::= { sniOmnis 21 }
--------------------------------------------------------------
--
--   Globale  OMNIS-Informationen
--
--------------------------------------------------------------
omnisGlobalDataSubagentVersion      OBJECT-TYPE
        SYNTAX  DisplayString
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION "Version of OMNIS-Subagent"
        ::= {sniOmnisGlobalData 1}
omnisGlobalDataTabNum        OBJECT-TYPE
        SYNTAX  INTEGER
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION "The total number of OMNISes"
        ::= {sniOmnisGlobalData 4}
omnisGlobalDataActID         OBJECT-TYPE
        SYNTAX  INTEGER
        ACCESS  read-write
        STATUS  mandatory
        DESCRIPTION "The ID of the current OMNIS"
        ::= {sniOmnisGlobalData 5}
omnisGlobalDataActName       OBJECT-TYPE
        SYNTAX  DisplayString
        ACCESS  read-write
        STATUS  mandatory
        DESCRIPTION "The name of  the current OMNIS"
        ::= {sniOmnisGlobalData 6}
omnisGlobalDataTable       OBJECT-TYPE
        SYNTAX  SEQUENCE OF OmnisGlobalDataEntry
        ACCESS  not-accessible
        STATUS  mandatory
        DESCRIPTION "The OMNIS GlobalData-Table"
        ::= {sniOmnisGlobalData 7}
omnisGlobalDataEntry        OBJECT-TYPE
        SYNTAX  OmnisGlobalDataEntry
        ACCESS  not-accessible
        STATUS  mandatory
        DESCRIPTION "An entry in the table"
        INDEX {omnisGlobalDataOmnID}
        ::= {omnisGlobalDataTable 1}
OmnisGlobalDataEntry ::=
        SEQUENCE
        {
            omnisGlobalDataOmnID              INTEGER,
            omnisGlobalDataVersion            DisplayString,
            omnisGlobalDataOmnName            DisplayString,
            omnisGlobalDataState              INTEGER
        }
omnisGlobalDataOmnID               OBJECT-TYPE
        SYNTAX  INTEGER
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION "OMNIS ID"
        ::= {omnisGlobalDataEntry 1}
omnisGlobalDataVersion             OBJECT-TYPE
        SYNTAX  DisplayString
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION "OMNIS version"
        ::= {omnisGlobalDataEntry 2}
omnisGlobalDataOmnName             OBJECT-TYPE
        SYNTAX  DisplayString
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION "OMNIS name"
        ::= {omnisGlobalDataEntry 3}
omnisGlobalDataState               OBJECT-TYPE
        SYNTAX  INTEGER {
                          open        (1),
                          close       (2)
                        }
        ACCESS  read-write
        STATUS  mandatory
        DESCRIPTION "OMNIS state"
        ::= {omnisGlobalDataEntry 4}
