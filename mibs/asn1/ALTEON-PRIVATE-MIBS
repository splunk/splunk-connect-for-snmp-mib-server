        SYNTAX  DisplayString (SIZE (0..32))
        ACCESS  read-write
        STATUS  mandatory
        DESCRIPTION
                "The SNMP community string to use with the
                 trap host"
        ::= { agNewCfgTrapHostEntry 3 }

