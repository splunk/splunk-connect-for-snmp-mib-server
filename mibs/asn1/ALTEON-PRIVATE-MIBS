        SYNTAX INTEGER {
                other(1),
                delete(2)
                }
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION
                "By setting the value to delete(2), the entire row is deleted."
        ::= { gslbNewCfgNetworkTableEntry 12}

