        SYNTAX  INTEGER {
                enabled(1),
                disabled(2)
                }
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION
                "Enable or disable URL redirection.  The URL expressions are
                 defined in slbCurCfgUrlTable."
        ::= { fltCurCfgTableEntry 21 }


