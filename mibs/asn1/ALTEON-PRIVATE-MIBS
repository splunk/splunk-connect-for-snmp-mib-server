        ::= { vrrpCurCfgVirtRtrTableEntry 14 }


vrrpCurCfgVirtRtrTckHsrp OBJECT-TYPE
        SYNTAX  INTEGER {
                enabled(1),
                disabled(2)
                }
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION
                "Enable or disable tracking HSRP advertisements for 
		 priority adjustment.  the priority increment is defined 
