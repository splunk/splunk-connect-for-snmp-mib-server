	slbFailOverInfoPrimaryStatus   INTEGER,
	slbFailOverInfoPrimaryState    INTEGER,
        slbFailOverInfoSecondaryIp     IpAddress,
	slbFailOverInfoSecondaryStatus INTEGER,
	slbFailOverInfoSecondaryState  INTEGER
	}

