

                    XYPLEX-SYSTEM-MIB DEFINITIONS ::= BEGIN
            
                    IMPORTS
                            enterprises, Counter, Gauge, TimeTicks,
                            IpAddress
                                    FROM RFC1155-SMI
                            DisplayString
                                    FROM RFC1213-MIB
                            OBJECT-TYPE
                                    FROM RFC1212
                            TRAP-TYPE
                                    FROM RFC1215-DEFINING-TRAPS;
            
            xyplex          OBJECT IDENTIFIER ::= { enterprises 33 }
            xSystem         OBJECT IDENTIFIER ::= { xyplex 1 }
            
            -- This is a MIB module for all Xyplex systems
            
            -- Copyright 1992 Xyplex, Inc..  All Rights Reserved.
            -- Reproduction of this document is authorized on
            -- condition that this copyright notice is included.
