                  Counter,
               grIpxVSapRxNearRs
                  Counter,
               grIpxVSapTxGenQs
                  Counter,
               grIpxVSapTxNearQs
                  Counter,
               grIpxVSapTxGenRs
                  Counter,
               grIpxVSapTxNearRs
                  Counter,
               grIpxVSapRxUnknGenQs
                  Counter,
               grIpxVSapRxUnknNearQs
                  Counter,
               grIpxVSapRxUnknGenRs
                  Counter,
               grIpxVSapRxUnknNearRs
                  Counter,
               grIpxVSapAgedOuts
                  Counter,
               grIpxVSapPeriodics
                  Counter,
               grIpxVSapUpdates
                  Counter,
               grIpxVSapWrongNets
                  Counter,
               grIpxVSapBadInfos
                  Counter
            }


