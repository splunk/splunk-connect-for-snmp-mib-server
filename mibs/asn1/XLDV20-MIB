     --                 TLM STM1 near-end PPTP Table
     -- --------------------------------------------------------------
     --
     -- This managed entity represents the extension for the PPTP at
     -- the STM1 near-end interface. It includes the additional
     -- attributes to configure the STM1 link.
     --

     xldv20Stm1NePPTPTable OBJECT-TYPE
        -- info-IMPL:    V2.0->          
        SYNTAX        SEQUENCE OF Xldv20Stm1NePPTPEntry
        ACCESS        not-accessible
        STATUS        mandatory
        DESCRIPTION
             " The physical path termination point table for the STM1 IF. "
        --DEFAULT test-function-async     fsnS1nEntryTest
        --DEFAULT next-function-async     fsnS1nEntryNext
        --DEFAULT set-function-async      fsnS1nEntrySet
        --DEFAULT get-function-async      fsnS1nEntryGet
        ::= { xldv20Tlm 9 }

     xldv20Stm1NePPTPEntry OBJECT-TYPE
        -- info-IMPL:    V2.0->          
        SYNTAX        Xldv20Stm1NePPTPEntry
        ACCESS        not-accessible
        STATUS        mandatory
        DESCRIPTION
             " An entry describing a termination point in the PPTP table.
               Notifications : see xldv20IfEntry. "
        INDEX         { xldv20IfIndex }
        ::= { xldv20Stm1NePPTPTable 1 }

     Xldv20Stm1NePPTPEntry ::=
        SEQUENCE { xldv20S1nLineType                 Xldv20LineType,
                   xldv20S1nPathTraceSend            OCTET STRING,
                   xldv20S1nPathTraceSendDefault     Xldv20OperState,
                   xldv20S1nPathTraceExpect          OCTET STRING,
                   xldv20S1nPathTraceTIMDetectionActivate  Xldv20OperState,
                   xldv20S1nPathTraceReceive         OCTET STRING,
                   xldv20S1nHpEberThreshold          Xldv20S1nS3nEberThreshold,
                   xldv20S1nMsEberThreshold          Xldv20S1nS3nEberThreshold,
                   xldv20S1nRsEberThreshold          Xldv20S1nS3nEberThreshold,
                   xldv20S1nSDThreshold              Xldv20S1nS3nSDThreshold,
                   xldv20S1nRDIAISOnEber             Xldv20OperState,
                   xldv20S1nSignalLabelReceive       Integer32,
                   xldv20S1nEmptyCellType            Xldv20EmptyCellType,
                   xldv20S1nBandwidthUsage           INTEGER }

     xldv20S1nLineType OBJECT-TYPE
        -- info-VALUE:   mediumSingleMode,
        -- info-VALUE:   longSingleMode, coax
        -- info-ROU:     GET db
        -- info-IMPL:    V2.0->          
        SYNTAX        Xldv20LineType
        ACCESS        read-only
        STATUS        mandatory
        DESCRIPTION
             " The physical medium of the line used for transmission. "
        ::= { xldv20Stm1NePPTPEntry 1 }

     xldv20S1nPathTraceSend OBJECT-TYPE
        -- info-VALUE:   1'st byte: any value (CRC incl. Sync-Bit)
        -- info-VALUE:   1'st byte: inserted value will be ignored by DSLAM and
        -- info-VALUE:   generated itself by DSLAM.                      
        -- info-VALUE:   2'nd-16'th byte: value <= 0x7F
        -- info-ROU:     GET db  SET tm 
        -- info-IMPL:    V2.0->          
        -- info-VERCTRL: -
        SYNTAX        OCTET STRING ( SIZE (0..16) )
        ACCESS        read-write
        STATUS        mandatory
        DESCRIPTION
             " Pattern to be sent to the LE (CRC + 15 bytes) used for the path
               trace send functionality. "
        ::= { xldv20Stm1NePPTPEntry 2 }

     xldv20S1nPathTraceSendDefault OBJECT-TYPE
        -- info-VALUE:   enabled(1), disabled(2)
        -- info-VALUE:   default: enabled
        -- info-ROU:     GET db  SET tm 
        -- info-IMPL:    V2.0->          
        -- info-VERCTRL: -
        SYNTAX        Xldv20OperState
        ACCESS        read-write
        STATUS        mandatory
        DESCRIPTION
             " To configure the path trace send functionality.
               A default path trace value (enabled) or the pattern
               defined in the xldv20S1nPathTraceSend attribute (disabled) will
               be sent. "
        ::= { xldv20Stm1NePPTPEntry 3 }

     xldv20S1nPathTraceExpect OBJECT-TYPE
        -- info-VALUE:   1'st byte: any value (CRC incl. Sync-Bit)
        -- info-VALUE:   1'st byte: inserted value will be ignored by DSLAM and
        -- info-VALUE:   generated itself by DSLAM.                        
        -- info-VALUE:   2'nd-16'th byte: value <= 0x7F
        -- info-ROU:     GET db  SET tm
        -- info-IMPL:    V2.0->          
        -- info-VERCTRL: -
        SYNTAX        OCTET STRING ( SIZE (0..16) )
        ACCESS        read-write
        STATUS        mandatory
        DESCRIPTION
             " The expected result of a path trace operation.
	       This signal will be compared with the
               S1nPathTraceReceive, when the Attribute
               S1nPathTraceTIMDetectionActivate is enabled. If the comparison 
               detects unequality an alarm notification (TIM) is sent. "
        ::= { xldv20Stm1NePPTPEntry 4 }

     xldv20S1nPathTraceTIMDetectionActivate OBJECT-TYPE
        -- info-VALUE:   enabled(1), disabled(2) (default)
        -- info-ROU:     GET db  SET tm
        -- info-IMPL:    V2.0->          
        -- info-VERCTRL: -
        SYNTAX        Xldv20OperState
        ACCESS        read-write
        STATUS        mandatory
        DESCRIPTION
             " Enables / disables (default) the comparison of xldv20S1nPathTraceExpect
	     and xldv20S1nPathTraceReceive attribute. "
        ::= { xldv20Stm1NePPTPEntry 5 }

     xldv20S1nPathTraceReceive OBJECT-TYPE
        -- info-VALUE:   1'st byte: CRC (incl. Sync-Bit)
        -- info-VALUE:   2'nd-16'th byte: value <= 0x7F
        -- info-ROU:     GET tm
        -- info-IMPL:    V2.0->          
        -- info-VERCTRL: -
        SYNTAX        OCTET STRING ( SIZE (0..16) )
        ACCESS        read-only
        STATUS        mandatory
        DESCRIPTION
             " Received path trace value from the LE. "
        ::= { xldv20Stm1NePPTPEntry 6 }

     xldv20S1nHpEberThreshold OBJECT-TYPE
        -- info-VALUE:   exp10E3(1), exp10E4(2), exp10E5(3), noSupervision(255)
        -- info-VALUE:   default: exp10E3
        -- info-ROU:     GET db  SET tm 
        -- info-IMPL:    V2.0->          
        -- info-VERCTRL: -
        SYNTAX        Xldv20S1nS3nEberThreshold
        ACCESS        read-write
        STATUS        mandatory
        DESCRIPTION
             " The threshold for the higher-order path excessive bit error check. "
        ::= { xldv20Stm1NePPTPEntry 7 }

     xldv20S1nMsEberThreshold OBJECT-TYPE
        -- info-VALUE:   exp10E3(1), exp10E4(2), exp10E5(3), noSupervision(255)
        -- info-VALUE:   default: exp10E3
        -- info-ROU:     GET db  SET tm 
        -- info-IMPL:    V2.0->          
        -- info-VERCTRL: -
        SYNTAX        Xldv20S1nS3nEberThreshold
        ACCESS        read-write
        STATUS        mandatory
        DESCRIPTION
             " The threshold for the multiplexer section excessive bit error check. "
        ::= { xldv20Stm1NePPTPEntry 8 }

     xldv20S1nRsEberThreshold OBJECT-TYPE
        -- info-VALUE:   exp10E3(1), exp10E4(2), exp10E5(3), noSupervision(255)
        -- info-VALUE:   default: exp10E3
        -- info-ROU:     GET db  SET tm 
        -- info-IMPL:    V2.0->          
        -- info-VERCTRL: -
        SYNTAX        Xldv20S1nS3nEberThreshold
        ACCESS        read-write
        STATUS        mandatory
        DESCRIPTION
             " The threshold for the repeater section excessive bit error check. "
        ::= { xldv20Stm1NePPTPEntry 9 }

     xldv20S1nSDThreshold OBJECT-TYPE
        -- info-VALUE:   exp10E5(3), exp10E6(4), exp10E7(5), exp10E8(6), exp10E9(7),
        -- info-VALUE:   noSupervision(255)
        -- info-VALUE:   default: exp10E5
        -- info-ROU:     GET db  SET tm 
        -- info-IMPL:    V2.0->          
        -- info-VERCTRL: -
        SYNTAX        Xldv20S1nS3nSDThreshold
        ACCESS        read-write
        STATUS        mandatory
        DESCRIPTION
             " The threshold for the signal degration check. "
        ::= { xldv20Stm1NePPTPEntry 10 }

     xldv20S1nRDIAISOnEber OBJECT-TYPE
        -- info-VALUE:   enabled(1) (default), disabled(2)
        -- info-ROU:     GET db  SET tm 
        -- info-IMPL:    V2.0->          
        -- info-VERCTRL: -
        SYNTAX        Xldv20OperState
        ACCESS        read-write
        STATUS        mandatory
        DESCRIPTION
             " Enables (default) / disables the RDI-and VP-AIS generation
               on defect excessive bit ratio. "
        ::= { xldv20Stm1NePPTPEntry 11 }

     xldv20S1nSignalLabelReceive OBJECT-TYPE
        -- info-VALUE:   -2147483648..2147483647
        -- info-ROU:     GET tm 
        -- info-IMPL:    V2.0->          
        -- info-VERCTRL: -
        SYNTAX        Integer32
        ACCESS        read-only
        STATUS        mandatory
        DESCRIPTION
              " Received signal label from the LE. "
        ::= { xldv20Stm1NePPTPEntry 12 }

     xldv20S1nEmptyCellType OBJECT-TYPE
        -- info-VALUE:   idle (default), unassigned
        -- info-ROU:     GET db  SET tm 
        -- info-IMPL:    V2.0->          
        -- info-VERCTRL: -
        SYNTAX        Xldv20EmptyCellType
        ACCESS        read-write
        STATUS        mandatory
        DESCRIPTION
             " The type of empty cells. "
        ::= { xldv20Stm1NePPTPEntry 13 }

        xldv20S1nBandwidthUsage OBJECT-TYPE
        -- info-VALUE:   0..100, Default 0
        -- info-ROU:     GET db
        -- info-IMPL:    V2.0->
        -- info-VERCTRL: -
        SYNTAX        INTEGER (0..100)
        ACCESS        read-only
        STATUS        mandatory
        DESCRIPTION
             " Used bandwidth in percent. "
        ::= { xldv20Stm1NePPTPEntry 14 }

     -- --------------------------------------------------------------
     --                 TLM STS3c near-end PPTP Table
     -- --------------------------------------------------------------
     --
     -- This managed entity represents the extension for the PPTP at
     -- the STS3 near end interface. It includes the additional
     -- attributes to configure the STS3 link.
     --

     xldv20Sts3NePPTPTable OBJECT-TYPE
        -- info-IMPL:    V2.0->          
        SYNTAX        SEQUENCE OF Xldv20Sts3NePPTPEntry
        ACCESS        not-accessible
        STATUS        mandatory
        DESCRIPTION
             " The physical path termination point table for the STS3c IF. "
        --DEFAULT test-function-async     fsnS3nEntryTest
        --DEFAULT next-function-async     fsnS3nEntryNext
        --DEFAULT set-function-async      fsnS3nEntrySet
        --DEFAULT get-function-async      fsnS3nEntryGet
        ::= { xldv20Tlm 10 }

     xldv20Sts3NePPTPEntry OBJECT-TYPE
        -- info-IMPL:    V2.0->          
        SYNTAX        Xldv20Sts3NePPTPEntry
        ACCESS        not-accessible
        STATUS        mandatory
        DESCRIPTION
             " An entry describing a termination point in the PPTP table.
               Notifications : see xldv20IfEntry. "
        INDEX         { xldv20IfIndex }
        ::= { xldv20Sts3NePPTPTable 1 }

     Xldv20Sts3NePPTPEntry ::=
        SEQUENCE { xldv20S3nLineType                 Xldv20LineType,
                   xldv20S3nPathTraceSend            OCTET STRING ( SIZE (0..16) ),
                   xldv20S3nPathTraceSendDefault     Xldv20OperState,
                   xldv20S3nPathTraceExpect          OCTET STRING ( SIZE (0..16) ),
                   xldv20S3nPathTraceTIMDetectionActivate  Xldv20OperState,
                   xldv20S3nPathTraceReceive         OCTET STRING ( SIZE (0..16) ),
                   xldv20S3nPathTraceSync            Xldv20OperState,
                   xldv20S3nPathTraceSendCRCActivate     Xldv20OperState,
                   xldv20S3nPathTraceReceiveCRCActivate  Xldv20OperState,
                   xldv20S3nHpEberThreshold          Xldv20S1nS3nEberThreshold,
                   xldv20S3nMsEberThreshold          Xldv20S1nS3nEberThreshold,
                   xldv20S3nRsEberThreshold          Xldv20S1nS3nEberThreshold,
                   xldv20S3nSDThreshold              Xldv20S1nS3nSDThreshold,
                   xldv20S3nSignalLabelReceive       Integer32,
                   xldv20S3nEmptyCellType            Xldv20EmptyCellType,
                   xldv20S3nBandwidthUsage           INTEGER }

     xldv20S3nLineType OBJECT-TYPE
        -- info-VALUE:   other(1), shortSingleMode(2),
        -- info-VALUE:   mediumSingleMode(3) (default)
        -- info-VALUE:   longSingleMode(4), coax(5), utp(6)
        -- info-ROU:     GET db 
        -- info-IMPL:    V2.0->          
        -- info-VERCTRL: GET db
        -- info-VERCTRL: -
        SYNTAX        Xldv20LineType
        ACCESS        read-only
        STATUS        mandatory
        DESCRIPTION
             " The physical medium of the line used for transmission. "
        ::= { xldv20Sts3NePPTPEntry 1 }

     xldv20S3nPathTraceSend OBJECT-TYPE
        -- info-VALUE:   16 times <= 0x7F
        -- info-VALUE:   (case CRC enabled / Sync-Bit enabled):
        -- info-VALUE:   * 1'st byte: CRC+Sync-Bit +
        -- info-VALUE:     15 times less or equal 0x7F
        -- info-VALUE:     (default: 16 * 0x00)
        -- info-VALUE:   (case CRC enabled / Sync-Bit disabled):
        -- info-VALUE:   * 1'st byte: CRC-no-Sync-Bit +
        -- info-VALUE:     15 times less or equal 0x7F
        -- info-VALUE:     (default: 16 * 0x00)
        -- info-VALUE:   (case CRC disabled / Sync-Bit enabled):
        -- info-VALUE:   * 16 times less or equal 0x7F
        -- info-VALUE:     (default: 16 * 0x00)
        -- info-VALUE:     == value from set-request + Sync-Bit
        -- info-VALUE:   * The first bit of the first byte is set
        -- info-VALUE:   (case CRC disabled / Sync-Bit disabled):
        -- info-VALUE:   * 16 times less or equal 0x7F
        -- info-VALUE:     (default: 16 * 0x00)
        -- info-VALUE:      == value from set-request / no Sync-Bit
        -- info-VALUE:   * The first bit of the first byte is not set
        -- info-ROU:     GET db  SET tm 
        -- info-IMPL:    V2.0->          
        -- info-VERCTRL: GET db  SET asw
        -- info-VERCTRL: -
        SYNTAX        OCTET STRING ( SIZE (0..16) )
        ACCESS        read-write
        STATUS        mandatory
        DESCRIPTION
             " The input signal used for the path trace send functionality. "
        ::= { xldv20Sts3NePPTPEntry 2 }

     xldv20S3nPathTraceSendDefault OBJECT-TYPE
        -- info-VALUE:   enabled(1) (default), disabled(2)
        -- info-ROU:     GET db  SET tm 
        -- info-IMPL:    V2.0->          
        -- info-VERCTRL: -
        SYNTAX        Xldv20OperState
        ACCESS        read-write
        STATUS        mandatory
        DESCRIPTION
             " Enables/disables the path trace send functionality.
               A default path trace value (enabled) or the pattern
               defined in the xldv20S3nPathTraceSend attribute (disabled) will
               be sent. "
        ::= { xldv20Sts3NePPTPEntry 3 }

     xldv20S3nPathTraceExpect OBJECT-TYPE
        -- info-VALUE:   (case received CRC enabled):  
        -- info-VALUE:   * 1'st byte: any value (will be ignored by the DSLAM and generated itself)
        -- info-VALUE:   (case received CRC disabled): * 16 times less or equal 0x7F.
        -- info-VALUE:   + 15 times less or equal 0x7F (default: 16 * 0x00).
        -- info-ROU:     GET db  SET tm 
        -- info-IMPL:    V2.0->          
        -- info-VERCTRL: -
        SYNTAX        OCTET STRING ( SIZE (0..16) )
        ACCESS        read-write
        STATUS        mandatory
        DESCRIPTION
             " The expected result of a path trace operation.
	     This signal will be compared with the
               S3nPathTraceReceive, when the attribute
               S3nPathTraceTIMDetectionActivate is enabled. If the comparison detects
               unequality an alarm notification (TIM) is sent. When CRC is
               activated, only the first 15 bytes are compared. "
        ::= { xldv20Sts3NePPTPEntry 4 }

     xldv20S3nPathTraceTIMDetectionActivate OBJECT-TYPE
        -- info-VALUE:   enabled(1), disabled(2) (default)
        -- info-ROU:     GET db  SET tm 
        -- info-IMPL:    V2.0->          
        -- info-VERCTRL: -
        SYNTAX        Xldv20OperState
        ACCESS        read-write
        STATUS        mandatory
        DESCRIPTION
             " Enables/disables the comparsion of the signals
               S3nPathTraceExpect and S3nPathTraceReceive.  "
        ::= { xldv20Sts3NePPTPEntry 5 }

     xldv20S3nPathTraceReceive OBJECT-TYPE
        -- info-VALUE:   1'st byte: maybe CRC (incl. sync-bit) + 15 times <= 0x7F
        -- info-ROU:     GET tm 
        -- info-IMPL:    V2.0->          
        -- info-VERCTRL: -
        SYNTAX        OCTET STRING ( SIZE (0..16) )
        ACCESS        read-only
        STATUS        mandatory
        DESCRIPTION
             " Received path trace value from the LE. "
        ::= { xldv20Sts3NePPTPEntry 6 }

     xldv20S3nPathTraceSync OBJECT-TYPE
        -- info-VALUE:   enabled(1), disabled(2) (default: without sync-bit)
        -- info-ROU:     GET db  SET tm 
        -- info-IMPL:    V2.0->          
        -- info-VERCTRL: -
        SYNTAX        Xldv20OperState
        ACCESS        read-write
        STATUS        mandatory
        DESCRIPTION
             " Enables/disables the path trace synchronization 
	     (the sync-bit generation for the path trace send attribute). "
        ::= { xldv20Sts3NePPTPEntry 7 }

     xldv20S3nPathTraceSendCRCActivate OBJECT-TYPE
        -- info-VALUE:   enabled(1), disabled(2) (default == DSLAM uses 1'st
        -- info-VALUE:   Byte of MIB-attribute xldv20S3nPathTraceSend )
        -- info-ROU:     GET db  SET tm 
        -- info-IMPL:    V2.0->          
        -- info-VERCTRL: -
        SYNTAX        Xldv20OperState
        ACCESS        read-write
        STATUS        mandatory
        DESCRIPTION
             " Enables/disbales the CRC generation
	     for the path trace send attribute.
	     First Trace Send byte ignored and used for CRC when CRC is enabled. "
        ::= { xldv20Sts3NePPTPEntry 8 }

     xldv20S3nPathTraceReceiveCRCActivate OBJECT-TYPE
        -- info-VALUE:   enabled(1), disabled(2) (default)
        -- info-ROU:     GET db  SET tm 
        -- info-IMPL:    V2.0->          
        -- info-VERCTRL: -
        SYNTAX        Xldv20OperState
        ACCESS        read-write
        STATUS        mandatory
        DESCRIPTION
             " Enables/disables the CRC generation
               for the path trace reveive attribute. "
        ::= { xldv20Sts3NePPTPEntry 9 }

     xldv20S3nHpEberThreshold OBJECT-TYPE
        -- info-VALUE:   exp10E3(1), exp10E4(2), exp10E5(3), noSupervision(255)
        -- info-VALUE:   default: noSupervision
        -- info-ROU:     GET db  SET tm 
        -- info-IMPL:    V2.0->          
        -- info-VERCTRL: -
        SYNTAX        Xldv20S1nS3nEberThreshold
        ACCESS        read-write
        STATUS        mandatory
        DESCRIPTION
             " Threshold for the higher-order path
               excessive bit error check. "
        ::= { xldv20Sts3NePPTPEntry 10 }

     xldv20S3nMsEberThreshold OBJECT-TYPE
        -- info-VALUE:   exp10E3(1), exp10E4(2), exp10E5(3), noSupervision(255)
        -- info-VALUE:   default: noSupervision
        -- info-ROU:     GET db  SET tm
        -- info-IMPL:    V2.0->          
        -- info-VERCTRL: -
        SYNTAX        Xldv20S1nS3nEberThreshold
        ACCESS        read-write
        STATUS        mandatory
        DESCRIPTION
             " Threshold for the multiplexer
               section excessive bit error check. "
        ::= { xldv20Sts3NePPTPEntry 11 }

     xldv20S3nRsEberThreshold OBJECT-TYPE
        -- info-VALUE:   exp10E3(1), exp10E4(2), exp10E5(3), noSupervision(255)
        -- info-VALUE:   default: noSupervision
        -- info-ROU:     GET db  SET tm 
        -- info-IMPL:    V2.0->          
        -- info-VERCTRL: -
        SYNTAX        Xldv20S1nS3nEberThreshold
        ACCESS        read-write
        STATUS        mandatory
        DESCRIPTION
             " Threshold for the repeater section
               excessive bit error check. "
        ::= { xldv20Sts3NePPTPEntry 12 }

     xldv20S3nSDThreshold OBJECT-TYPE
        -- info-VALUE:   exp10E5(1), exp10E6(2), exp10E7(3),exp10E8(2), exp10E9(3),
        -- info-VALUE:   noSupervision(255)
        -- info-VALUE:   default: exp10E5
        -- info-ROU:     GET db  SET tm 
        -- info-IMPL:    V2.0->          
        -- info-VERCTRL: -
        SYNTAX        Xldv20S1nS3nSDThreshold
        ACCESS        read-write
        STATUS        mandatory
        DESCRIPTION
             " Threshold for the signal degration check.  "
        ::= { xldv20Sts3NePPTPEntry 13 }

     xldv20S3nSignalLabelReceive OBJECT-TYPE
        -- info-VALUE:   0 ... 255
        -- info-ROU:     GET tm
        -- info-IMPL:    V2.0->          
        -- info-VERCTRL: -
        SYNTAX        Integer32
        ACCESS        read-only
        STATUS        mandatory
        DESCRIPTION
              " Received signal label value from the LE. "
        ::= { xldv20Sts3NePPTPEntry 14 }

     xldv20S3nEmptyCellType OBJECT-TYPE
        -- info-VALUE:   idle, unassigned (default)
        -- info-ROU:     GET db  SET tm
        -- info-IMPL:    V2.0->          
        -- info-VERCTRL: -
        SYNTAX        Xldv20EmptyCellType
        ACCESS        read-write
        STATUS        mandatory
        DESCRIPTION
             " Type of empty cells. "
        ::= { xldv20Sts3NePPTPEntry 15 }

        xldv20S3nBandwidthUsage OBJECT-TYPE
        -- info-VALUE:   0..100, Default 0
        -- info-ROU:     GET db
        -- info-IMPL:    V2.0->
        -- info-VERCTRL: -
        SYNTAX        INTEGER (0..100)
        ACCESS        read-only
        STATUS        mandatory
        DESCRIPTION
             " Used bandwidth in percent. "
        ::= { xldv20Sts3NePPTPEntry 16 }

     -- --------------------------------------------------------------
     --                 The DS1 transmission subtree (RFC1406)
     -- --------------------------------------------------------------
     -- The DS1 Near End Group consists of
     -- the DS1 Configuration Table
 
     xldv20Dsx1ConfigTable OBJECT-TYPE
        -- info-IMPL:    V2.0->
        SYNTAX  SEQUENCE OF Xldv20Dsx1ConfigEntry
        ACCESS  not-accessible
        STATUS  mandatory
        DESCRIPTION
        "The DS1 Configuration table. "
        --DEFAULT next-function-async     fsnDsxEntryNext
        --DEFAULT get-function-async      fsnDsxEntryGet
        ::= { xldv20Tlm 11 }
 
     xldv20Dsx1ConfigEntry OBJECT-TYPE
        -- info-IMPL:    V2.0->
        SYNTAX  Xldv20Dsx1ConfigEntry
        ACCESS  not-accessible
        STATUS  mandatory
        DESCRIPTION
        "An entry in the DS1 Configuration table (RFC1406) .
        Index is IfIndex of dsx1nePPTP (e1nIma). "
        INDEX   { xldv20IfIndex }
        ::= { xldv20Dsx1ConfigTable 1 }

     Xldv20Dsx1ConfigEntry ::=
         SEQUENCE {
             xldv20Dsx1LineType            INTEGER,
             xldv20Dsx1LineCoding          INTEGER,
             xldv20Dsx1LoopbackConfig      INTEGER,
             xldv20Dsx1SignalMode          INTEGER,
             xldv20Dsx1TransmitClockSource INTEGER,
             xldv20Dsx1Fdl                 INTEGER
         }
 
        xldv20Dsx1LineType OBJECT-TYPE
        -- info-VALUE:   other(1), dsx1ESF(2),
        -- info-VALUE:   dsx1D4(3), dsx1E1(4)
        -- info-VALUE:   dsx1E1-CRC(5) (default), dsx1E1-MF(6),
        -- info-VALUE:   dsx1E1-CRC-MF(7)
        -- info-ROU:     GET db
        -- info-IMPL:    V2.0->
        -- info-VERCTRL: -
        SYNTAX  INTEGER {
                   other(1),
                   dsx1ESF(2),
                   dsx1D4(3),
                   dsx1E1(4),
                   dsx1E1-CRC(5),
                   dsx1E1-MF(6),
                   dsx1E1-CRC-MF(7)
                }
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION
           " This variable indicates  the  variety  of  DS1
           Line  implementing  this  circuit.  The type of
           circuit affects the number of bits  per  second
           that  the circuit can reasonably carry, as well
           as the interpretation of the  usage  and  error
           statistics.  The values, in sequence.
 
                dsx1ESF       Extended SuperFrame DS1.
                dsx1D4        AT&T D4 format DS1.
                dsx1E1        CCITT Recommendation G704
                                (Table 4a).
                dsx1E1-CRC    CCITT Recommendation G704
                                (Table 4b).
                dsxE1-MF      G704 (Table 4a) with TS16
                                multiframing enabled.
                dsx1E1-CRC-MF G704 (Table 4b) with TS16.
                                multiframing enabled. "
        ::= { xldv20Dsx1ConfigEntry 1 }

        xldv20Dsx1LineCoding OBJECT-TYPE
        -- info-VALUE:   dsx1JBZS (1), dsx1B8ZS (2),
        -- info-VALUE:   dsx1HDB3 (3) (default), dsx1ZBTSI (4),
        -- info-VALUE:   dsx1AMI (5), other(6)
        -- info-ROU:     GET db
        -- info-IMPL:    V2.0->
        -- info-VERCTRL: -
        SYNTAX  INTEGER {
                    dsx1JBZS (1),
                    dsx1B8ZS (2),
                    dsx1HDB3 (3),
                    dsx1ZBTSI (4),
                    dsx1AMI (5),
                    other(6)
                }
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION
           "This variable describes the  variety  of  Zero
           Code  Suppression  used  on  the link, which in
           turn affects a number of its characteristics.
 
           dsx1JBZS refers the Jammed  Bit  Zero  Suppres-
           sion,  in  which  the  AT&T specification of at
           least one pulse every 8 bit periods is literal-
           ly  implemented  by forcing a pulse in bit 8 of
           each channel.  Thus, only seven bits per  chan-
           nel, or 1.344 Mbps, is available for data.
 
           dsx1B8ZS refers to the use of a specified  pat-
           tern  of  normal  bits  and  bipolar violations
           which are used to replace a sequence  of  eight
           zero bits.
 
           ANSI Clear Channels may use dsx1ZBTSI, or  Zero
           Byte Time Slot Interchange.
 
           E1 links, with or without CRC, use dsx1HDB3  or
           dsx1AMI.
 
 
           dsx1AMI refers to a mode wherein no  zero  code
           suppression  is  present  and the line encoding
           does not solve the problem directly.   In  this
           application, the higher layer must provide data
           which meets or exceeds the  pulse  density  re-
           quirements, such as inverting HDLC data. "
       ::= { xldv20Dsx1ConfigEntry 2 }
       
        xldv20Dsx1LoopbackConfig OBJECT-TYPE
        -- info-VALUE:   dsx1OtherLoop(4)
        -- info-ROU:     GET db
        -- info-IMPL:    V2.0->
        -- info-VERCTRL: -
        SYNTAX  INTEGER {
                    dsx1NoLoop(1),
                    dsx1PayloadLoop(2),
                    dsx1LineLoop(3),
                    dsx1OtherLoop(4)
                }
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION
        "This variable represents the  loopback  confi-
           guration of the DS1 interface.  Agents support-
           ing read/write access should return badValue in
           response to a requested loopback state that the
           interface does not support.  The values mean:.
 
 
                dsx1NoLoop
                     Not in the loopback state.  A device  that
                     is not capable of performing a loopback on
                     the interface shall always return this  as
                     it's value.
 
                dsx1PayloadLoop
                     The received signal at this  interface  is
                     looped  through  the device. Typically the
                     received signal is  looped  back  for  re-
                     transmission  after  it has passed through
                     the device's framing function.
 
                dsx1LineLoop
                     The received signal at this interface does
                     not  go  through the device (minimum pene-
                     tration) but is looped back out.
 
                dsx1OtherLoop
                     Loopbacks that are not defined here. "
       ::= { xldv20Dsx1ConfigEntry 3 }
 
        xldv20Dsx1SignalMode OBJECT-TYPE
        -- info-VALUE:   none (1)
        -- info-ROU:     GET db
        -- info-IMPL:    V2.0->
        -- info-VERCTRL: -
        SYNTAX  INTEGER {
                    none (1),
                    robbedBit (2),
                    bitOriented (3),
                    messageOriented (4)
                }
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION
                " The signal mode.
		
		'none' indicates that no bits are reserved for
                signaling on this channel.
 
                 'robbedBit' indicates that T1 Robbed Bit  Sig-
                naling is in use.
 
                 'bitOriented' indicates that E1 Channel  Asso-
                ciated Signaling is in use.
 
                 'messageOriented' indicates that Common  Chan-
                nel Signaling is in use either on channel 16 of
                an E1 link or channel 24 of a T1. "
        ::= { xldv20Dsx1ConfigEntry 4 }

        xldv20Dsx1TransmitClockSource OBJECT-TYPE
        -- info-VALUE:   localTiming (2),
        -- info-ROU:     GET db
        -- info-IMPL:    V2.0->
        -- info-VERCTRL: -
        SYNTAX  INTEGER {
                    loopTiming (1),
                    localTiming (2),
                    throughTiming (3)
                }
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION
           "The source of Transmit Clock.
 
                 'loopTiming' indicates that the recovered  re-
                ceive clock is used as the transmit clock.
 
                 'localTiming' indicates  that  a  local  clock
                source is used.
 
                 'throughTiming' indicates that  recovered  re-
                ceive  clock  from another interface is used as
                the transmit clock. "
       ::= { xldv20Dsx1ConfigEntry 5 }

        xldv20Dsx1Fdl OBJECT-TYPE
        -- info-VALUE:   dsx1Att-54016(4)
        -- info-ROU:     GET db
        -- info-IMPL:    V2.0->
        -- info-VERCTRL: -
        SYNTAX  INTEGER {
                      other(1),
                      dsx1Ansi-T1-403(2),
                      dsx1Att-54016(4),
                      dsx1Fdl-none(8)
                }
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION
           "This bitmap describes the use of  the  facili-
           ties data link, and is the sum of the capabili-
           ties:
 
            'other' indicates that a protocol  other  than
           one following is used.
 
            'dsx1Ansi-T1-403' refers to the  FDL  exchange
                recommended by ANSI.
 
            'dsx1Att-54016' refers to ESF FDL exchanges.
 
            'dsx1Fdl-none' indicates that the device  does
           not use the FDL. "
       ::= { xldv20Dsx1ConfigEntry 6 }

