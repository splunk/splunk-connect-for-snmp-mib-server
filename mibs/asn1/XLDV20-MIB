     -- --------------------------------------------------------------
     --                 Subtree for T L M  O A M  functions
     --                 Table of Loop location Points (at TCA) 
     -- --------------------------------------------------------------

     xldv20LoopBackPointTable  OBJECT-TYPE
        -- info-IMPL:    V2.0->
        SYNTAX        SEQUENCE OF Xldv20LbpEntry
        ACCESS        not-accessible
        STATUS        mandatory
        DESCRIPTION
             " The loop back point table"
        --DEFAULT test-function-async     fsnLbpEntryTest
        --DEFAULT next-function-async     fsnLbpEntryNext
        --DEFAULT set-function-async      fsnLbpEntrySet
        --DEFAULT get-function-async      fsnLbpEntryGet
        ::= { xldv20TlmOam 1 }
 
     xldv20LbpEntry   OBJECT-TYPE
        -- info-IMPL:    V2.0->
        SYNTAX        Xldv20LbpEntry
        ACCESS        not-accessible
        STATUS        mandatory
        DESCRIPTION
             " An entry in the loop back point table.
               The entry holds parameters of loop back points
               (as the loop location ID for loop back points or
               the loop mode).
               Loopback points are either the TCA at the LU* or SU
               or the IMA group at the LUIMA (within the DSLAM),
               or the TCA of adrPPTP at the NTAD.
               Notifications : see xldv20IfEntry. "
        INDEX         { xldv20IfIndex }
        ::= { xldv20LoopBackPointTable 1 }
 
     Xldv20LbpEntry ::=
        SEQUENCE { xldv20LbpLoopLocId   OCTET STRING,
                   xldv20LbpLoopMode    Xldv20LoopMode
                   }

     xldv20LbpLoopLocId   OBJECT-TYPE
        -- info-VALUE:   default: 16 times FF
        -- info-VALUE:   hexadecimal at TMN-OS
        -- info-ROU:     GET tm  SET tm
        -- info-IMPL:    V2.0->
        -- info-VERCTRL: -
        SYNTAX        OCTET STRING ( SIZE (0..16) )
        ACCESS        read-write
        STATUS        mandatory
        DESCRIPTION
             " A unique number known by the crafts person specifying
              the location where the OAM loopback cell shall be
              looped back.
              Loopback point is either the TCA at the LU* or SU (within the DSLAM)
              or the TCA of adrPPTP at NTAD.
              NOTE : Due to the functionality of ASPEN at SU the loop location ID
              only of port 1 is configurable. The IDs for port2-16 are calculated
              from this value by the ASPEN. TMN will store these values to the
              corresponding  table entries . "
        ::= {xldv20LbpEntry 1 }
 
 
     xldv20LbpLoopMode   OBJECT-TYPE
        -- info-VALUE:   forward(default), loopAndForward
        -- info-ROU:     GET tm  SET tm
        -- info-IMPL:    V2.0->
        -- info-VERCTRL: -
        SYNTAX        Xldv20LoopMode
        ACCESS        read-write
        STATUS        mandatory
        DESCRIPTION
             " This attribute selects for connecting points
             whether the received loopback cell shall be
             forwarded or forwarded and looped back . "
        ::= {xldv20LbpEntry 2 }

     -- --------------------------------------------------------------
     --                 TLM Physical Loop Test subgroup
     -- --------------------------------------------------------------

     xldv20PhyLoopTest OBJECT IDENTIFIER 
        -- info-IMPL:    V2.0->          
        -- info-VERCTRL: -
     ::= { xldv20TlmOam 2 }

     -- This group handles the TMN control request for starting a service
     -- interrupting physical layer loop diagnosis.
     --

     xldv20PltIfIndex OBJECT-TYPE
        -- info-VALUE:   0..65535
        -- info-ROU:     GET db  SET db 
        -- info-IMPL:    V2.0->          
        -- info-VERCTRL: -
        SYNTAX        Unsigned16
        ACCESS        read-write
        STATUS        mandatory
        DESCRIPTION
             " This attribute labels the location of the physical loopback. It
               can be the ifIndex of a PPTP at LI25, LU, SU . "
        --DEFAULT test-function-async     fsnPltTest
        --DEFAULT next-function-async     fsnPltNext
        --DEFAULT set-function-async      fsnPltSet
        --DEFAULT get-function-async      fsnPltGet
        ::= { xldv20PhyLoopTest 1 }

     xldv20PltControlReq OBJECT-TYPE
