     -- --------------------------------------------------------------
     --                     Alarm Control group 
     -- --------------------------------------------------------------

     xldv20AlmControl OBJECT IDENTIFIER 
        -- info-IMPL:    V2.0->          
        -- info-VERCTRL: -
     ::= { xldv20Alarming 7 }

     -- This managed entity manages the update of the alarm list
     -- (xldv20AlarmListTable) and the alarming filtering table.
     -- The craft person has to update the alarm list when he/she has changed
     -- alarm severities.
     -- The craft person has to update the alarm filtering table when he/she has
     -- changed temporal filter in the alarm filtering table.
     --
     -- The TMN-OS can request the DSLAM to send Atm layer multi alarm traps.
     --

     xldv20AlmControlReq OBJECT-TYPE
        -- info-VALUE:   updateAlmList
        -- info-VALUE:   updateFiltList
        -- info-VALUE:   commandFailed
        -- info-VALUE:   startSendingAlarms
        -- info-ROU:     GET db SET gf
        -- info-IMPL:    V2.0->          
        -- info-VERCTRL: -
        SYNTAX        Xldv20ControlReq
        ACCESS        read-write
        STATUS        mandatory
        DESCRIPTION
             " Marks the received command to update the alarm list
               (updateAlmList) or the alarming filtering table
               (updateFiltList) or the request of Atm layer multi 
               alarm traps (startSendingAlarms). "
        --DEFAULT test-function-async     fsnAlmTest
        --DEFAULT next-function-async     fsnAlmNext
        --DEFAULT set-function-async      fsnAlmSet
        --DEFAULT get-function-async      fsnAlmGet
        ::= { xldv20AlmControl 1 }

     xldv20AlmControlStatus OBJECT-TYPE
        -- info-VALUE:   requestIdle(1), requestBusy(2), requestPassed(3)
        -- info-VALUE:   requestFailed(4)
        -- info-ROU:     GET db
        -- info-IMPL:    V2.0->          
        -- info-VERCTRL: -
        SYNTAX        Xldv20ControlStatus
        ACCESS        read-only
        STATUS        mandatory
        DESCRIPTION
             " The status of the control request. The value requestIdle
               will be set after startup/recovery or after a running
               test was aborted by the SNMP manager. "
        --DEFAULT next-function-async     fsnAlmNext
        --DEFAULT get-function-async      fsnAlmGet
        ::= { xldv20AlmControl 2 }

     xldv20AlmControlReqResult OBJECT-TYPE
        -- info-VALUE:   updateAlmList (command)
        -- info-VALUE:   updateFiltList(command)
        -- info-VALUE:   commandFailed
        -- info-VALUE:   startSendingAlarms(command)
        -- info-VALUE:   requestBusy,setDataFailed
        -- info-VALUE:   aswTimeout
        -- info-ROU:     GET db
        -- info-IMPL:    V2.0->          
        -- info-VERCTRL: -
        SYNTAX        Xldv20ControlReq
        ACCESS        read-only
        STATUS        mandatory
        DESCRIPTION
              " The result of the last user command. "
        --DEFAULT next-function-async     fsnAlmNext
        --DEFAULT get-function-async      fsnAlmGet
        ::= { xldv20AlmControl 3 }

      xldv20AlmControlTimeStamp OBJECT-TYPE
         -- info-VALUE:   0..2Giga
         -- info-ROU:     GET db
         -- info-IMPL:    V2.0->
         -- info-VERCTRL: -
         SYNTAX        TimeTicks
         ACCESS        read-only
         STATUS        mandatory
         DESCRIPTION
            " The time stamp of the last command (end of command). "
        --DEFAULT next-function-async     fsnAlmNext
        --DEFAULT get-function-async      fsnAlmGet
        ::= { xldv20AlmControl 4 }

     -- --------------------------------------------------------------
     --                         Reset group 
     -- --------------------------------------------------------------

     xldv20Reset OBJECT IDENTIFIER 
        -- info-IMPL:    V2.0->          
        -- info-VERCTRL: -
     ::= { xldv20Alarming 8 }


   -- Performs the reset of one plugInUnit or of all applicable plug in units.
   -- Not all kinds of reset can be performed on every unit.

     xldv20RstControlReq OBJECT-TYPE
        -- info-VALUE:   warm,cold,reload,dbReload
        -- info-VALUE:   noRequest,
        -- info-ROU:     GET db SET gf
        -- info-IMPL:    V2.0->          
        -- info-VERCTRL: -
        SYNTAX        Xldv20ControlReq
        ACCESS        read-write
        STATUS        mandatory
        DESCRIPTION
             " Request to perform a reset.
               Notifications : 
               xldv20TrReplaceableUnitReset at beginning of reset PIU.
               xldv20TrReplaceableUnitResetEnd at end of reset PIU.
               xldv20TrRstResult for reset of CI in case of a failure occured
               after the reset has been accepted. "
        --DEFAULT test-function-async     fsnRstTest
        --DEFAULT next-function-async     fsnRstNext
        --DEFAULT set-function-async      fsnRstSet
        --DEFAULT get-function-async      fsnRstGet
        ::= { xldv20Reset 1 }

     xldv20RstControlStatus OBJECT-TYPE
        -- info-VALUE:   requestIdle(1), requestBusy(2), requestPassed(3)
        -- info-VALUE:   requestFailed(4)
        -- info-ROU:     GET db
        -- info-IMPL:    V2.0->          
        -- info-VERCTRL: -
        SYNTAX        Xldv20ControlStatus
        ACCESS        read-only
        STATUS        mandatory
        DESCRIPTION
             " The actual status of the reset procedure. "
        --DEFAULT next-function-async     fsnRstNext
        --DEFAULT get-function-async      fsnRstGet
        ::= { xldv20Reset 2 }

     xldv20RstLevel OBJECT-TYPE
        -- info-VALUE:   plugInUnit,system
        -- info-ROU:     GET db SET db
        -- info-IMPL:    V2.0->          
        -- info-VERCTRL: -
        SYNTAX        Xldv20ResetLevel
        ACCESS        read-write
        STATUS        mandatory
        DESCRIPTION
             " The reset level.
	     With this attribute the TMN-OS can deside to make a reset for one
               unit or the whole system. "
        --DEFAULT test-function-async     fsnRstTest
        --DEFAULT next-function-async     fsnRstNext
        --DEFAULT set-function-async      fsnRstSet
        --DEFAULT get-function-async      fsnRstGet
        ::= { xldv20Reset 3 }

     xldv20RstHwUnitIndex OBJECT-TYPE
        -- info-VALUE:   0..65535
        -- info-ROU:     GET db SET db
        -- info-IMPL:    V2.0->          
        -- info-VERCTRL: -
        SYNTAX        Unsigned16
        ACCESS        read-write
        STATUS        mandatory
