             " The pathname of SAPS content file. "
        --DEFAULT test-function-async     fsnSucTest
        --DEFAULT next-function-async     fsnSucNext
        --DEFAULT set-function-async      fsnSucSet
        --DEFAULT get-function-async      fsnSucGet
        ::= { xldv20SwmUpgradeControl 5 }

     xldv20SucFileName OBJECT-TYPE
        -- info-VALUE:   -
        -- info-ROU:     GET db SET db
        -- info-IMPL:    V2.0->          
        -- info-VERCTRL: -
        SYNTAX        OCTET STRING ( SIZE (0..33) )
        ACCESS        read-write
        STATUS        mandatory
        DESCRIPTION
             " The filename of SAPS content file. "
        --DEFAULT test-function-async     fsnSucTest
        --DEFAULT next-function-async     fsnSucNext
        --DEFAULT set-function-async      fsnSucSet
        --DEFAULT get-function-async      fsnSucGet
        ::= { xldv20SwmUpgradeControl 6 }

     xldv20SucControlReq OBJECT-TYPE
        -- info-VALUE:   sucUpgrStartRequest
        -- info-VALUE:   sucActivateLoad, sucReadSAPSContentFile
        -- info-VALUE:   sucGetSwVersions
        -- info-VALUE:   sucUpgrEndRequest
