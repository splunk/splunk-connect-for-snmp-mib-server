         -- --------------------------------------------------------------
         --            Hardware Plug-In Unit Table
         -- --------------------------------------------------------------

         xldv20HwPlugInUnitTable OBJECT-TYPE
            -- info-IMPL:    V2.0->
            -- info-VERCTRL: -
            SYNTAX        SEQUENCE OF Xldv20HwPlugInUnitEntry
            ACCESS        not-accessible
            STATUS        mandatory
            DESCRIPTION
                 " Table containing the plug-in unit entries . "
            --DEFAULT test-function-async     fsnPiuEntryTest
            --DEFAULT next-function-async     fsnPiuEntryNext
            --DEFAULT set-function-async      fsnPiuEntrySet
            --DEFAULT get-function-async      fsnPiuEntryGet
            ::= { xldv20Hwm 6 }

         xldv20HwPlugInUnitEntry OBJECT-TYPE
            -- info-IMPL:    V2.0->
            -- info-VERCTRL: -
            SYNTAX        Xldv20HwPlugInUnitEntry
            ACCESS        not-accessible
            STATUS        mandatory
            DESCRIPTION
                 " Entry of plug in unit table.
		 Each entry is used to represent equipment that is inserted
                   (plugged into) and removed from slots of the ATM NE .
                   Notifications : 
                     xldv20TrStateChangeOper.
                     xldv20TrStateChangeAdmin.
                     xldv20TrTrObjCreate.
		     xldv20TrObjDelete.
                     Alarm traps when a fault has been detected or cleared. "
            INDEX { xldv20HwUnitIndex }
            ::= { xldv20HwPlugInUnitTable 1 }

         Xldv20HwPlugInUnitEntry ::=
            SEQUENCE { xldv20PiuAdminState          Xldv20AdminState,
                       xldv20PiuAvailStatus         Xldv20AvailStatus,
                       xldv20PiuOperState           Xldv20OperState,
                       xldv20PiuType                DisplayString,
                       xldv20PiuAlarmSeverityIndex  Xldv20AlmSevProfileIndex,
                       xldv20PiuAlarmFilteringIndex Xldv20AlmFiltProfileIndex,
                       xldv20PiuUpgradeResult       Xldv20ControlReq
                     }

         xldv20PiuAdminState OBJECT-TYPE
            -- info-VALUE:   unlocked(1), locked(2)
            -- info-ROU:     GET db  SET hm 
            -- info-IMPL:    V2.0->
            -- info-VERCTRL: -
            SYNTAX        Xldv20AdminState
            ACCESS        read-write
            STATUS        mandatory
            DESCRIPTION
                 " This attribute is used to activate (unlock) and deactivate
                   (lock) the functions performed by the plug-in unit.
                   Default is locked except for LU155:OM, LU155:OL, LUPON:O,
                   LUDSX3:C. For LU155:OM, LU155:OL, LUPON:O, LUDSX3:C locking
                   is only allowed from LCT.
                   In the transition from lock to unlock the Plug-In Unit is
                   diagnosed. "
            ::= { xldv20HwPlugInUnitEntry 1 }

         xldv20PiuAvailStatus OBJECT-TYPE
            -- info-VALUE:   available(1), failed(2), degraded(3), offline(4)
            -- info-VALUE:   notInstalled(5), dependency(7)
            -- info-ROU:     GET db
            -- info-IMPL:    V2.0->
            -- info-VERCTRL: -
            SYNTAX        Xldv20AvailStatus
            ACCESS        read-only
            STATUS        mandatory
            DESCRIPTION
                 " The availability status of a plug-in unit is described with
                   this attribute. "
            ::= { xldv20HwPlugInUnitEntry 2 }

         xldv20PiuOperState OBJECT-TYPE
            -- info-VALUE:   enabled(1), disabled(2)
            -- info-ROU:     GET db
            -- info-IMPL:    V2.0->
            -- info-VERCTRL: -
            SYNTAX        Xldv20OperState
            ACCESS        read-only
            STATUS        mandatory
            DESCRIPTION
                 " The operational state of plug in unit. "
            ::= { xldv20HwPlugInUnitEntry 3 }

         xldv20PiuType OBJECT-TYPE
            -- info-VALUE:   see xldv20EqhAccPiuTypes
            -- info-ROU:     GET db
            -- info-IMPL:    V2.0->
            -- info-VERCTRL: -
            SYNTAX        DisplayString (SIZE (0..20))
            ACCESS        read-only 
            STATUS        mandatory
            DESCRIPTION
                 " Identifier for the type of a plug-in unit. 
                   See description of xldv20EqhAccPiuTypes for possible string values. "
            ::= { xldv20HwPlugInUnitEntry 4 }

         xldv20PiuAlarmSeverityIndex OBJECT-TYPE
            -- info-VALUE:   1..40
            -- info-ROU:     GET db  SET db
            -- info-IMPL:    V2.0->
            -- info-VERCTRL: -
            SYNTAX        Xldv20AlmSevProfileIndex
            ACCESS        read-write
            STATUS        mandatory
            DESCRIPTION
                 " It is a pointer to the instance of the alarm severity
                   assignment profile managed entity that contains the severity
                   assignments for the alarms reported by this managed entity.
                   Default is 1. "
            ::= { xldv20HwPlugInUnitEntry 5 }

         xldv20PiuAlarmFilteringIndex OBJECT-TYPE
            -- info-VALUE:   1..40
            -- info-ROU:     GET db
            -- info-IMPL:    V2.0->
            -- info-VERCTRL: -
            SYNTAX        Xldv20AlmFiltProfileIndex
            ACCESS        read-only
            STATUS        mandatory
            DESCRIPTION
                 " It is a pointer to the instance of the alarm filtering
                   assignment profile managed entity that contains the filtering
                   assignments for the alarms reported by this managed entity.
                   (Not implemented yet) . "
            ::= { xldv20HwPlugInUnitEntry 6 }

    xldv20PiuUpgradeResult OBJECT-TYPE
       -- info-VALUE:   SW upgrade task states :
       -- info-VALUE:   sutInitialized,sutToDo
       -- info-VALUE:   sutStarted,sutCrashed 
       -- info-VALUE:   sutFtpDone,sutDone
       -- info-VALUE:   sutReadyForReset,sutObsolete
       -- info-VALUE:   Result values at end of task :
       -- info-VALUE:   sucUpgrStartRequest,
       -- info-VALUE:   sucActivateLoad, sucReadSAPSContentFile,
       -- info-VALUE:   sucGetSwVersions, sucUpgrMissParameter,
       -- info-VALUE:   sucGetSwVersionsFailed
       -- info-VALUE:   sucOk ,sucNotOk
       -- info-VALUE:   sucDbAccessFailed ,sucPiuUnlocked
       -- info-VALUE:   sucTransmissionError ,sucWrongSwVersion
       -- info-VALUE:   sucUnitTypeMismatch
       -- info-VALUE:   sucFtpTransError ,sucFtpCrcError
       -- info-VALUE:   sucSwHwMismatch ,sucUpgrWrongAvState
       -- info-VALUE:   sucFepromCopyFault ,sucUpdateDescFileFailed
       -- info-VALUE:   sucFtpServerNoteAvailable ,sucFtpSendError
       -- info-VALUE:   sucDbBackupFault ,sucUpgradeComplete
       -- info-VALUE:   sucUpgradeNotComplete
       -- info-VALUE:   sucUpgrTaskCreationFault
       -- info-VALUE:   sucUpgrDbAccessFault
       -- info-ROU:     GET db
       -- info-IMPL:    V2.0->
       -- info-VERCTRL: -
       SYNTAX        Xldv20ControlReq
       ACCESS        read-only
       STATUS        mandatory
       DESCRIPTION
          " Informs about the status of the SW upgrade task during
	  loading and the result at the end of loading the current plug-in unit. "
        ::= { xldv20HwPlugInUnitEntry 7 }


         -- --------------------------------------------------------------
         --                    HW Unit control subgroup
         -- --------------------------------------------------------------

         xldv20HwuControl OBJECT IDENTIFIER ::= { xldv20Hwm 7 }
            -- info-IMPL:    V2.0->
            -- info-VERCTRL: -

         -- This managed entity manages the creation and deletion of Hw Units.
         -- XLDV2.0 : The XLD NE starts with static pre-configuration of HWM and
         -- TLM tables. 
         -- Only the command createHwUnit for HwUnits of type plugInUnit is supported.
         -- This command only modifies the xldv20PiuType attribute.
         -- Command deleteHwUnit is not supported.

         xldv20HwuControlReq OBJECT-TYPE
            -- info-VALUE:   createHwUnit, deleteHwUnit
            -- info-ROU:     GET db  SET hm
            -- info-IMPL:    V2.0->
            -- info-VERCTRL: -
            SYNTAX        Xldv20ControlReq
            ACCESS        read-write
            STATUS        mandatory
            DESCRIPTION
	         " This attribute identifies the control command for creating or
                   deleting a HW unit.
                   Notifications : xldv20TrHwuControl when command is finished. "
            --DEFAULT test-function-async     fsnHwuTest
            --DEFAULT next-function-async     fsnHwuNext
            --DEFAULT set-function-async      fsnHwuSet
            --DEFAULT get-function-async      fsnHwuGet
            ::= { xldv20HwuControl 1 }

         xldv20HwuControlStatus OBJECT-TYPE
            -- info-VALUE:   requestIdle(1), requestBusy(2), requestPassed(3)
            -- info-VALUE:   requestFailed(4)
            -- info-ROU:     GET db
            -- info-IMPL:    V2.0->
            -- info-VERCTRL: -
            SYNTAX        Xldv20ControlStatus
            ACCESS        read-only
            STATUS        mandatory
            DESCRIPTION
                 " The status of a control request. "
            --DEFAULT next-function-async     fsnHwuNext
            --DEFAULT get-function-async      fsnHwuGet
            ::= { xldv20HwuControl 2 }

         xldv20HwuUnitType OBJECT-TYPE
            -- info-VALUE:   equipment(V2.0->), equipmentHolder(V2.0->), plugInUnit
            -- info-ROU:     GET db SET db
            -- info-IMPL:    V2.0->
            -- info-VERCTRL: -
            SYNTAX        Xldv20HwUnitType
            ACCESS        read-write
            STATUS        mandatory
            DESCRIPTION
                 " Identifies the type of hardware unit which should be created.
                   The values are equipment, equipmentHolder and plugInUnit .
                   The following entities can not be created: 
		   atmNe.
                   equipment (DSLAM). 
		   equipmentHolder (basic shelf).
                   equipmentHolder (slot1-slot32 of basic shelf). "
            --DEFAULT test-function-async     fsnHwuTest
            --DEFAULT next-function-async     fsnHwuNext
            --DEFAULT set-function-async      fsnHwuSet
            --DEFAULT get-function-async      fsnHwuGet
            ::= { xldv20HwuControl 3 }

         xldv20HwuEquType OBJECT-TYPE
        -- info-VALUE:   ntAdsl, ntVdsl, ntCpe
        -- info-ROU:     GET db  SET db
        -- info-IMPL:    V2.0->
        -- info-VERCTRL: -
        SYNTAX        OCTET STRING ( SIZE (0..21) )
        ACCESS        read-only
        STATUS        mandatory
        DESCRIPTION
             " Indicates the equipment which should be created
               (ntAdsl or ntVdsl or ntCpe). Default is \0. "
        --DEFAULT next-function-async     fsnHwuNext
        --DEFAULT get-function-async      fsnHwuGet
        ::= { xldv20HwuControl 4 }

     xldv20HwuEqhType OBJECT-TYPE
        -- info-VALUE:   extendedShelf(4), ntAdslShelf(5), ntVdslShelf(6)
        -- info-VALUE:   cpeShelf(7)
        -- info-ROU:     GET db  SET db
        -- info-VERCTRL: -
        SYNTAX        Xldv20EqhType
        ACCESS        read-only
        STATUS        mandatory
        DESCRIPTION
             " Indicates the equipmentHolder which should be created (extendedShelf or
