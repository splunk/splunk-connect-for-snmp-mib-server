        ::= { xldv20ImaGroupEntry 18 }

        xldv20ImaGroupDiffDelayMax OBJECT-TYPE
        -- info-VALUE:   only : 25 supported!
        -- info-ROU:     GET db
        -- info-IMPL:    V2.0->
        -- info-VERCTRL: -
        SYNTAX      MilliSeconds
        ACCESS      read-only
        STATUS      mandatory
        DESCRIPTION
            " The maximum number of milliseconds of differential delay among
        the links that will be tolerated on this interface. "
        ::= { xldv20ImaGroupEntry 19 }

        xldv20ImaGroupLeastDelayLink OBJECT-TYPE
        -- info-VALUE:   
        -- info-ROU:     GET tm
        -- info-IMPL:    V2.0->
        -- info-VERCTRL: -
        SYNTAX      Unsigned16
        ACCESS      read-only
        STATUS      mandatory
        DESCRIPTION
            " The ifIndex of the link configured in the IMA group which has
        the smallest link propagation delay. This value has meaning only
        if at least one link has been configured in the IMA group. "
        ::= { xldv20ImaGroupEntry 20 }

        xldv20ImaGroupDiffDelayMaxObs OBJECT-TYPE
        -- info-VALUE:   
        -- info-ROU:     GET tm
        -- info-IMPL:    V2.0->
        -- info-VERCTRL: -
        SYNTAX      MilliSeconds 
        ACCESS      read-only
        STATUS      mandatory
        DESCRIPTION
            " The latest maximum differential delay observed (in milliseconds)
        between the links having the least and most link propagation delay,
        among the receive links that are currently configured in the IMA
        group. "
        ::= { xldv20ImaGroupEntry 21 }

        xldv20ImaGroupAlphaValue OBJECT-TYPE
        -- info-VALUE:   2 only!
        -- info-ROU:     GET tm
        -- info-IMPL:    V2.0->
        -- info-VERCTRL: -
        SYNTAX      INTEGER (1..2)
        ACCESS      read-only
        STATUS      mandatory
        DESCRIPTION
            " This indicates the 'alpha' value used to specify the number
        of consecutive invalid ICP cells to be detected before moving
        to the IMA Hunt state from the IMA Sync state. "
        ::= { xldv20ImaGroupEntry 22 }

        xldv20ImaGroupBetaValue OBJECT-TYPE
        -- info-VALUE:   2 only!
        -- info-ROU:     GET tm
        -- info-IMPL:    V2.0->
        -- info-VERCTRL: -
        SYNTAX      INTEGER (1..5)
        ACCESS      read-only
        STATUS      mandatory
        DESCRIPTION
            " This indicates the 'beta' value used to specify the number
        of consecutive errored ICP cells to be detected before moving
        to the IMA Hunt state from the IMA Sync state. "
        ::= { xldv20ImaGroupEntry 23 }

        xldv20ImaGroupGammaValue OBJECT-TYPE
        -- info-VALUE:   1 only!
        -- info-ROU:     GET tm
        -- info-IMPL:    V2.0->
        -- info-VERCTRL: -
        SYNTAX      INTEGER (1..5)
        ACCESS      read-only
        STATUS      mandatory
        DESCRIPTION
            " This indicates the 'gamma' value used to specify the number
        of consecutive valid ICP cells to be detected before moving
        to the IMA Sync state from the IMA PreSync state. "
        ::= { xldv20ImaGroupEntry 24 }

        xldv20ImaGroupRunningSecs  OBJECT-TYPE
        -- info-VALUE:   
        -- info-ROU:     GET tm
        -- info-IMPL:    V2.0->
        -- info-VERCTRL: -
        SYNTAX      Gauge32
        ACCESS      read-only
        STATUS      mandatory
        DESCRIPTION
            " The amount of time (in seconds) since this IMA group has
        been in the operation (up or down). "
        ::= { xldv20ImaGroupEntry 25 }

        xldv20ImaGroupUnavailSecs OBJECT-TYPE
        -- info-VALUE:   
        -- info-ROU:     GET tm
        -- info-IMPL:    V2.0->
        -- info-VERCTRL: -
        SYNTAX      Counter32
        ACCESS      read-only
        STATUS      mandatory
        DESCRIPTION
            " Count of one second intervals where the IMA Group Traffic
        State Machine is Down. "
        ::= { xldv20ImaGroupEntry 26 }

        xldv20ImaGroupNeNumFailures OBJECT-TYPE
        -- info-VALUE:   
        -- info-ROU:     GET tm
        -- info-IMPL:    V2.0->
        -- info-VERCTRL: -
        SYNTAX      Counter32
        ACCESS      read-only
        STATUS      mandatory
        DESCRIPTION
            " The number of times a near-end group failure (Config-Aborted,
        Insufficient-Links) has been reported since power-up or reboot. "
        ::= { xldv20ImaGroupEntry 27 }

        xldv20ImaGroupFeNumFailures OBJECT-TYPE
        -- info-VALUE:   
        -- info-ROU:     GET tm
        -- info-IMPL:    V2.0->
        -- info-VERCTRL: -
        SYNTAX      Counter32
        ACCESS      read-only
        STATUS      mandatory
        DESCRIPTION
            " The number of times a far-end group failure (Config-Aborted,
        Insufficient-Links) has been reported since power-up or reboot. "
        ::= { xldv20ImaGroupEntry 28 }

        xldv20ImaGroupNumTxCfgLinks OBJECT-TYPE
        -- info-VALUE:   
        -- info-ROU:     GET tm
        -- info-IMPL:    V2.0->
        -- info-VERCTRL: -
        SYNTAX      Gauge32
        ACCESS      read-only
        STATUS      mandatory
        DESCRIPTION
            " The number of links that are configured to transmit in this IMA
        group. This attribute overwrites the value of the
        imaGroupNumRxActLinks attribute when the IMA group is configured
        in the Symmetrical Configuration group symmetry mode. "
        ::= { xldv20ImaGroupEntry 29 }

        xldv20ImaGroupNumRxCfgLinks OBJECT-TYPE
        -- info-VALUE:   
        -- info-ROU:     GET tm
        -- info-IMPL:    V2.0->
        -- info-VERCTRL: -
        SYNTAX      Gauge32
        ACCESS      read-only
        STATUS      mandatory
        DESCRIPTION
            " The number of links that are configured to receive in this IMA
        group. This attribute is overwritten by the value of the
        imaGroupNumTxActLinks attribute when the IMA group is configured
        in the Symmetrical Configuration group symmetry mode. "
        ::= { xldv20ImaGroupEntry 30 }

        xldv20ImaGroupNumTxActLinks OBJECT-TYPE
        -- info-VALUE:   
        -- info-ROU:     GET tm
        -- info-IMPL:    V2.0->
        -- info-VERCTRL: -
        SYNTAX      Gauge32
        ACCESS      read-only
        STATUS      mandatory
        DESCRIPTION
            " The number of links which are configured to transmit and are
        currently Active in this IMA group. "
        ::= { xldv20ImaGroupEntry 31 }

        xldv20ImaGroupNumRxActLinks OBJECT-TYPE
        -- info-VALUE:   
        -- info-ROU:     GET tm
        -- info-IMPL:    V2.0->
        -- info-VERCTRL: -
        SYNTAX      Gauge32
        ACCESS      read-only
        STATUS      mandatory
        DESCRIPTION
            " The number of links which are configured to receive and are
        currently Active in this IMA group. "
        ::= { xldv20ImaGroupEntry 32 }

        xldv20ImaGroupBandwidthUsage OBJECT-TYPE
        -- info-VALUE:   0..100, Default 0
        -- info-ROU:     GET db
        -- info-IMPL:    V2.0->
        -- info-VERCTRL: -
        SYNTAX        INTEGER (0..100)
        ACCESS        read-only
        STATUS        mandatory
        DESCRIPTION
                 "  Used bandwidth in percent. "
        ::= { xldv20ImaGroupEntry 33 }

     -- --------------------------------------------------------------
     --                 The IMA group mapping table
     -- --------------------------------------------------------------

     -- The IMA Group Mapping Table subtree consists of a table of mappings
     -- from 'ifIndex' values of IMA groups to their 'imaGroupIndex' values.
     -- This table allows the Unit Management to perform easy look-ups (no
     -- searches and sorts).
     --
     -- Note that the Group Index is different from the ifIndex.
     --

     xldv20ImaGroupMappingTable OBJECT-TYPE
        -- info-IMPL:    V2.0->
        SYNTAX        SEQUENCE OF Xldv20ImaGroupMappingEntry
        ACCESS        not-accessible
        STATUS        mandatory
        DESCRIPTION
            " A table mapping the 'ifIndex' values of 'imaGroupIfIndex'
        to the 'imaGroupIndex' values of the corresponding IMA group.
	Index is ifIndex of IMA group (atmIma).  "
        --DEFAULT next-function-async     fsnImmEntryNext
        --DEFAULT get-function-async      fsnImmEntryGet
        ::= { xldv20ImaMibObjects 4 }
 
     xldv20ImaGroupMappingEntry OBJECT-TYPE
        -- info-IMPL:    V2.0->
        SYNTAX        Xldv20ImaGroupMappingEntry
        ACCESS        not-accessible
        STATUS        mandatory
        DESCRIPTION
            " An entry in the IMA Group Mapping table. "
        INDEX       { xldv20IfIndex }
        ::= { xldv20ImaGroupMappingTable 1 }

     Xldv20ImaGroupMappingEntry ::= SEQUENCE {
      xldv20ImaGroupMappingIndex    Unsigned32 }

     xldv20ImaGroupMappingIndex OBJECT-TYPE
        -- info-VALUE:   currentrly 1 only
        -- info-ROU:     GET db
        -- info-IMPL:    V2.0->
        -- info-VERCTRL: -
        SYNTAX      Unsigned32
        ACCESS      read-only
        STATUS      mandatory
        DESCRIPTION
            " The imaGroupIndex of the IMA Group which implements the
        specified interface. "
      ::= { xldv20ImaGroupMappingEntry 1 }

     -- --------------------------------------------------------------
     --                 The IMA link subtree
     -- --------------------------------------------------------------
     --
     -- The IMA Link subtree consists of a table of IMA links.  Each entry
     -- in the table contains status information about a link which is part
     -- of an IMA group, and also contains a configuration object to select
     -- to which IMA group the link belongs.
     --
 
     xldv20ImaLinkTable OBJECT-TYPE
        -- info-IMPL:    V2.0->
        SYNTAX        SEQUENCE OF Xldv20ImaLinkEntry
        ACCESS        not-accessible
        STATUS        mandatory
        DESCRIPTION
                " The IMA group Link Status and Configuration table. "
        --DEFAULT test-function-async     fsnImlEntryTest
        --DEFAULT next-function-async     fsnImlEntryNext
        --DEFAULT set-function-async      fsnImlEntrySet
        --DEFAULT get-function-async      fsnImlEntryGet
        ::= { xldv20ImaMibObjects 5 }
 
      xldv20ImaLinkEntry OBJECT-TYPE
        SYNTAX        Xldv20ImaLinkEntry
        ACCESS        not-accessible
        STATUS        mandatory
        DESCRIPTION
                " An entry in the IMA Group Link table.
	    Index is IfIndex of dsx1nePPTP (e1nIma). "
        INDEX       { xldv20IfIndex }
        ::= { xldv20ImaLinkTable 1 }

      Xldv20ImaLinkEntry ::= SEQUENCE {
        xldv20ImaLinkRowStatus            Xldv20RowStatus,
        xldv20ImaLinkGroupIndex           Integer32,
        xldv20ImaLinkNeTxState            Xldv20ImaLinkState,
        xldv20ImaLinkNeRxState            Xldv20ImaLinkState,
        xldv20ImaLinkFeTxState            Xldv20ImaLinkState,
        xldv20ImaLinkFeRxState            Xldv20ImaLinkState,
        xldv20ImaLinkNeRxFailureStatus    Xldv20ImaLinkFailureStatus,
        xldv20ImaLinkFeRxFailureStatus    Xldv20ImaLinkFailureStatus,
        xldv20ImaLinkTxLid                INTEGER (0..7),
        xldv20ImaLinkRxLid                INTEGER (0..7),
        xldv20ImaLinkImaViolations        Counter32,
        xldv20ImaLinkOifAnomalies         Counter32,
        xldv20ImaLinkNeSevErroredSecs     Counter32,
        xldv20ImaLinkFeSevErroredSecs     Counter32,
        xldv20ImaLinkNeUnavailSecs        Counter32,
        xldv20ImaLinkFeUnavailSecs        Counter32,
        xldv20ImaLinkNeTxUnusableSecs     Counter32,
        xldv20ImaLinkNeRxUnusableSecs     Counter32,
        xldv20ImaLinkFeTxUnusableSecs     Counter32,
        xldv20ImaLinkFeRxUnusableSecs     Counter32,
        xldv20ImaLinkNeTxNumFailures      Counter32,
        xldv20ImaLinkNeRxNumFailures      Counter32,
        xldv20ImaLinkFeTxNumFailures      Counter32,
        xldv20ImaLinkFeRxNumFailures      Counter32,
        xldv20ImaLinkTxStuffs             Counter32,
        xldv20ImaLinkRxStuffs             Counter32
        }
 
      xldv20ImaLinkRowStatus OBJECT-TYPE
        -- info-VALUE:   created
        -- info-ROU:     GET db
        -- info-IMPL:    V2.0->
        -- info-VERCTRL: 
        SYNTAX      Xldv20RowStatus
        ACCESS      read-only
        STATUS      mandatory
        DESCRIPTION
            " The imaLinkRowStatus object allows create, change, and delete
            operations on imaLinkTable entries.
            In the first version the IMA  links for the LU-IMA will be
            created during start-up. "
      ::= { xldv20ImaLinkEntry 1 }

      xldv20ImaLinkGroupIndex OBJECT-TYPE
        -- info-VALUE:   currently 1 only
        -- info-ROU:     GET db
        -- info-IMPL:    V2.0->
        -- info-VERCTRL: 
        SYNTAX      Integer32
        ACCESS      read-only
        STATUS      mandatory
      DESCRIPTION
        " The value which identifies the IMA group (imaGroupIndex)
        of which this link is a member. "
      ::= { xldv20ImaLinkEntry 2 }
 
      xldv20ImaLinkNeTxState OBJECT-TYPE
        -- info-VALUE:   notInGroup(1), unusableNoGivenReason(2),
        -- info-VALUE:   unusableFault(3), unusableMisconnected(4),
        -- info-VALUE:   unusableInhibited(5), unusableFailed(6),
        -- info-VALUE:   usable(7), active(8) }
        -- info-ROU:     GET tm
        -- info-IMPL:    V2.0->
        -- info-VERCTRL: -
        SYNTAX      Xldv20ImaLinkState
        ACCESS      read-only
        STATUS      mandatory
        DESCRIPTION
            " The current state of the near-end transmit link. "
      ::= { xldv20ImaLinkEntry 3 }
 
      xldv20ImaLinkNeRxState OBJECT-TYPE
        -- info-VALUE:   notInGroup(1), unusableNoGivenReason(2),
        -- info-VALUE:   unusableFault(3), unusableMisconnected(4),
        -- info-VALUE:   unusableInhibited(5), unusableFailed(6),
        -- info-VALUE:   usable(7), active(8) }
        -- info-ROU:     GET tm
        -- info-IMPL:    V2.0->
        -- info-VERCTRL: -
        SYNTAX      Xldv20ImaLinkState
        ACCESS      read-only
        STATUS      mandatory
        DESCRIPTION
            " The current state of the near-end receive link. "
      ::= { xldv20ImaLinkEntry 4 }
 
      xldv20ImaLinkFeTxState OBJECT-TYPE
        -- info-VALUE:   notInGroup(1), unusableNoGivenReason(2),
        -- info-VALUE:   unusableFault(3), unusableMisconnected(4),
        -- info-VALUE:   unusableInhibited(5), unusableFailed(6),
        -- info-VALUE:   usable(7), active(8) }
        -- info-ROU:     GET tm
        -- info-IMPL:    V2.0->
        -- info-VERCTRL: -
        SYNTAX      Xldv20ImaLinkState
        ACCESS      read-only
        STATUS      mandatory
        DESCRIPTION
            " The current state of the far-end transmit link as reported
        via ICP cells. "
      ::= { xldv20ImaLinkEntry 5 }
 
      xldv20ImaLinkFeRxState OBJECT-TYPE
        -- info-VALUE:   notInGroup(1), unusableNoGivenReason(2),
        -- info-VALUE:   unusableFault(3), unusableMisconnected(4),
        -- info-VALUE:   unusableInhibited(5), unusableFailed(6),
        -- info-VALUE:   usable(7), active(8) }
        -- info-ROU:     GET tm
        -- info-IMPL:    V2.0->
        -- info-VERCTRL: -
