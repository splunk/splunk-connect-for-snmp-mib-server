     xldv20AdcCtrlMinMarginDn OBJECT-TYPE
        -- info-VALUE:   dB-6 (-6),
        -- info-VALUE:   dB-5 (-5),
        -- info-VALUE:   dB-4 (-4),
        -- info-VALUE:   dB-3 (-3),
        -- info-VALUE:   dB-2 (-2),
        -- info-VALUE:   dB-1 (-1),
        -- info-VALUE:   dB0  (0 ),
        -- info-VALUE:   dB1  (1 ),
        -- info-VALUE:   dB2  (2 ),
        -- info-VALUE:   dB3  (3 ),
        -- info-VALUE:   dB4  (4 ),
        -- info-VALUE:   dB5  (5 ),
        -- info-VALUE:   dB6  (6 ),
        -- info-VALUE:   dB7  (7 ),
        -- info-VALUE:   dB8  (8 ),
        -- info-VALUE:   dB9  (9 ),
        -- info-VALUE:   dB10 (10),
        -- info-VALUE:   dB11 (11),
        -- info-VALUE:   dB12 (12),
        -- info-VALUE:   auto (255)
        -- info-ROU:     GET db SET db
        -- info-IMPL:    V2.0->
        -- info-VERCTRL: -
        SYNTAX        Xldv20AdslMinMargin
        ACCESS        read-write
        STATUS        mandatory
        DESCRIPTION
             " Threshold for downstream margin when an alarm
               should be sent. In units of dB.
	       The value auto is the default value and means that 
	       the threshold is calculated from the configured margin :
	       threshold margin down = configured margin down - 6 dB.  "
        --DEFAULT test-function-async     fsnAdcCtrlTest
        --DEFAULT next-function-async     fsnAdcCtrlNext
        --DEFAULT set-function-async      fsnAdcCtrlSet
        --DEFAULT get-function-async      fsnAdcCtrlGet
