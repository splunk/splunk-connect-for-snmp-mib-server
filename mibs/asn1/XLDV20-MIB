            ::= { xldv20SnmpControl 9 }

         -- The following two polling flag attributes are used
         -- in the context of polling, discovery and alarm processing scenario.
         -- Discovery is a process where the TMN reads out the NE data to
         -- perform data sychronization between its own and the NE data.
         -- The NE sets these attributes when alarm traps have been sent or
         -- an LCT session has been terminated. 
         -- Under the assumption that traps can get lost these polling flags
         -- inform about DSLAM data changes and the TMN can discover the NE
         -- data.
         
         xldv20SnmCallpPollingFlag OBJECT-TYPE   
            -- info-VALUE:   flagSet(1), flagNotSet(2)
            -- info-ROU:     GET db
            -- info-IMPL:    V2.0->
            -- info-VERCTRL: -
            SYNTAX        Xldv20PollingFlagType
            ACCESS        read-only
            STATUS        mandatory
            DESCRIPTION
                 " The TMN can poll this global alarm flag to see whether for
                 any COH object an alarm is present. "
            --DEFAULT next-function-async     fsnSnmNext
            --DEFAULT get-function-async      fsnSnmGet
            ::= { xldv20SnmpControl 10 }

         xldv20SnmLctSessionPollingFlag OBJECT-TYPE   
            -- info-VALUE:   flagSet(1), flagNotSet(2)
            -- info-ROU:     GET db SET sn
            -- info-IMPL:    V2.0->
            -- info-VERCTRL: -
            SYNTAX        Xldv20PollingFlagType
            ACCESS        read-write
            STATUS        mandatory
            DESCRIPTION
                 " The TMN can poll this flag to see whether an LCT session
                 has been terminated within the last polling period (value set).
                 During the LCT session DSLAM configuration data may have been
                 changed and therefore the TMN database is inconsistent.
                 The TMN can perform a discovery to sychronize the data.
                 The TMN should set the value to notSet before the discovery.
		 "
            --DEFAULT test-function-async     fsnSnmTest
            --DEFAULT next-function-async     fsnSnmNext
            --DEFAULT set-function-async      fsnSnmSet
            --DEFAULT get-function-async      fsnSnmGet
            ::= { xldv20SnmpControl 11 }

         xldv20SnmControlReqResult OBJECT-TYPE
            -- info-VALUE:   snmUpdateComplete, aswTimeout
            -- info-VALUE:   snmUpdateFailed, snmFtpError       
            -- info-ROU:     GET db
            -- info-IMPL:    V2.0->
            -- info-VERCTRL: -
            SYNTAX        Xldv20ControlReq
            ACCESS        read-only
            STATUS        mandatory
            DESCRIPTION
               " Result of the last user command. "
             --DEFAULT next-function-async     fsnSnmNext
             --DEFAULT get-function-async      fsnSnmGet
             ::= { xldv20SnmpControl 12 }

         xldv20SnmStartupResult OBJECT-TYPE
            -- info-VALUE:   suFinished, suFailed,
            -- info-VALUE:   suStandaloneStartupFinished,
            -- info-VALUE:   suInProgress
            -- info-ROU:     GET db
            -- info-IMPL:    V2.0->
            -- info-VERCTRL: -
