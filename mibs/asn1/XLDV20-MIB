        --DEFAULT next-function-async     fsnImcNext
        --DEFAULT get-function-async      fsnImcGet
        ::= { xldv20ImaControl 7 }

     -- --------------------------------------------------------------
     --                 The IMA group subtree (IMA-MIB)
     -- --------------------------------------------------------------
     --
     -- The IMA Group subtree consists of the table of IMA groups.
     -- Each entry in the table of IMA groups contains
     -- information (configuration and status) specific to each group.
     --
     xldv20ImaGroupTable OBJECT-TYPE
        -- info-IMPL:    V2.0->
        SYNTAX        SEQUENCE OF Xldv20ImaGroupEntry
        ACCESS        not-accessible
        STATUS        mandatory
        DESCRIPTION
             " The IMA Group configuration Table.
	      Index is ImaGroupIndex. "
        --DEFAULT test-function-async     fsnImgEntryTest
        --DEFAULT next-function-async     fsnImgEntryNext
        --DEFAULT set-function-async      fsnImgEntrySet
        --DEFAULT get-function-async      fsnImgEntryGet
        ::= { xldv20ImaMibObjects 3 }
 
     xldv20ImaGroupEntry OBJECT-TYPE
        -- info-IMPL:    V2.0->
        SYNTAX      Xldv20ImaGroupEntry
        ACCESS      not-accessible
        STATUS      mandatory
        DESCRIPTION
            " An entry in the IMA Group table. "
        INDEX       { xldv20ImaGroupIndex }
      ::= { xldv20ImaGroupTable 1 }
 
      Xldv20ImaGroupEntry ::= SEQUENCE {
        xldv20ImaGroupIndex               INTEGER (1..2147483647),
        xldv20ImaGroupRowStatus           Xldv20RowStatus,
        xldv20ImaGroupIfIndex             Unsigned16,
        xldv20ImaGroupNeState             Xldv20ImaGroupState,
        xldv20ImaGroupFeState             Xldv20ImaGroupState,
        xldv20ImaGroupFailureStatus       Xldv20ImaGroupFailureStatus,
        xldv20ImaGroupSymmetry            Xldv20ImaGroupSymmetry,
        xldv20ImaGroupMinNumTxLinks       INTEGER (1..8),
        xldv20ImaGroupMinNumRxLinks       INTEGER (1..8),
        xldv20ImaGroupNeTxClkMode         Xldv20ImaGroupTxClkMode,
        xldv20ImaGroupFeTxClkMode         Xldv20ImaGroupTxClkMode,
        xldv20ImaGroupTxTimingRefLink     Unsigned16,
        xldv20ImaGroupRxTimingRefLink     Unsigned16,
        xldv20ImaGroupLastChange          TimeTicks,
        xldv20ImaGroupTxImaId             INTEGER (0..255),
        xldv20ImaGroupRxImaId             INTEGER (0..255),
        xldv20ImaGroupTxFrameLength       Xldv20ImaFrameLength,
        xldv20ImaGroupRxFrameLength       Xldv20ImaFrameLength,
        xldv20ImaGroupDiffDelayMax        MilliSeconds,
        xldv20ImaGroupLeastDelayLink      Unsigned16,
        xldv20ImaGroupDiffDelayMaxObs     MilliSeconds,
        xldv20ImaGroupAlphaValue          INTEGER (1..2),
        xldv20ImaGroupBetaValue           INTEGER (1..5),
        xldv20ImaGroupGammaValue          INTEGER (1..5),
        xldv20ImaGroupRunningSecs         Gauge32,
        xldv20ImaGroupUnavailSecs         Counter32,
        xldv20ImaGroupNeNumFailures       Counter32,
        xldv20ImaGroupFeNumFailures       Counter32,
        xldv20ImaGroupNumTxCfgLinks       Gauge32,
        xldv20ImaGroupNumRxCfgLinks       Gauge32,
        xldv20ImaGroupNumTxActLinks       Gauge32,
        xldv20ImaGroupNumRxActLinks       Gauge32,
        xldv20ImaGroupBandwidthUsage      INTEGER 
        }

      xldv20ImaGroupIndex OBJECT-TYPE
        -- info-VALUE:   currently 1
        -- info-ROU:     GET db
        -- info-IMPL:    V2.0->
        -- info-VERCTRL: -
        SYNTAX      INTEGER (1..2147483647)
        ACCESS      read-only
        STATUS      mandatory
        DESCRIPTION
             " The index of the table identifying an IMA Group. "
        ::= { xldv20ImaGroupEntry 1 }
 
      xldv20ImaGroupRowStatus OBJECT-TYPE
        -- info-VALUE:   created
        -- info-ROU:     GET db
        -- info-IMPL:    V2.0->
        -- info-VERCTRL: 
        SYNTAX      Xldv20RowStatus
        ACCESS      read-only
        STATUS      mandatory
        DESCRIPTION
             " The imaGroupRowStatus object allows create, change, and delete
        operations on imaGroupTable entries.
        In the first version only one IMA group for the LU-IMA will be
        created during start-up. "
        ::= { xldv20ImaGroupEntry 2 }
 
        xldv20ImaGroupIfIndex OBJECT-TYPE
        -- info-VALUE:   
        -- info-ROU:     GET db
        -- info-IMPL:    V2.0->
        -- info-VERCTRL: -
        SYNTAX      Unsigned16
        ACCESS      read-only
        STATUS      mandatory
        DESCRIPTION
             " The IfIndex of the IMA group. "
        ::= { xldv20ImaGroupEntry 3 }
 
        xldv20ImaGroupNeState OBJECT-TYPE
        -- info-VALUE:   notConfigured(1), startUp(2),
        -- info-VALUE:   startUpAck(3),
        -- info-VALUE:   configAbortUnsupported(4),
        -- info-VALUE:   configAbortIncompatibleSymmetry(5),
        -- info-VALUE:   configAbortOther(6),
        -- info-VALUE:   insufficientLinks(7),
        -- info-VALUE:   blocked(8),
        -- info-VALUE:   operational(9),
        -- info-VALUE:   configAbortUnsupportedImaVersion(10)
        -- info-ROU:     GET tm
        -- info-IMPL:    V2.0->
        -- info-VERCTRL: -
