        STATUS        mandatory
        DESCRIPTION
             " Description of further information about an alarm. "
        ::= { xldv20AlarmListEntry 3 }

     xldv20AlmSpecProblemText OBJECT-TYPE
        -- info-VALUE:   0..50 
        -- info-ROU:     GET db
        -- info-IMPL:    V2.0->          
        -- info-VERCTRL: -
