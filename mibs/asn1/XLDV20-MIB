     -- --------------------------------------------------------------
     --                 TLM SDSL central PPTP Table
     -- --------------------------------------------------------------
 
     -- This managed entity represents the extension for the PPTP at
     -- the SDSL central interface. It includes additional attributes to
     -- configure the SDSL link.
     --
 
     xldv20SdcPPTPTable OBJECT-TYPE
        -- info-IMPL:    V2.0->
        -- info-VERCTRL: -
        SYNTAX        SEQUENCE OF Xldv20SdcPPTPEntry
        ACCESS        not-accessible
        STATUS        mandatory
        DESCRIPTION
             " The physical path termination point table for SDSL C. "
        --DEFAULT test-function-async     fsnSdcEntryTest
        --DEFAULT next-function-async     fsnSdcEntryNext
        --DEFAULT get-function-async      fsnSdcEntryGet
        --DEFAULT set-function-async      fsnSdcEntrySet
        ::= { xldv20Tlm 13 }
 
     xldv20SdcPPTPEntry OBJECT-TYPE
        -- info-IMPL:    V2.0->
        -- info-VERCTRL: -
        SYNTAX        Xldv20SdcPPTPEntry
        ACCESS        not-accessible
        STATUS        mandatory
        DESCRIPTION
             " An entry describing a termination point in the PPTP table.
               Notifications : see xldv20IfEntry. "
        INDEX         { xldv20IfIndex }
        ::= { xldv20SdcPPTPTable 1 }
 
     Xldv20SdcPPTPEntry ::=
        SEQUENCE { xldv20SdcRateCfg            Xldv20SdslDataRate,
                   xldv20SdcXdslServiceTypeCfg Xldv20SdslServiceType,
                   xldv20SdcRate               Xldv20SdslDataRateCurrent,
                   xldv20SdcMarginDn           Xldv20AdslMargin,
                   xldv20SdcMarginUp           Xldv20AdslMargin,
                   xldv20SdcAttenuationDn      Xldv20AdslAttenuation,
                   xldv20SdcAttenuationUp      Xldv20AdslAttenuation,
                   xldv20SdcLinkState          Xldv20LinkState,
                   xldv20SdcAISOnLOS           Xldv20OperState,
                   xldv20SdcAISOnACT           Xldv20OperState ,
                   xldv20SdcGuaranteedBandwidthUsage INTEGER,
                   xldv20SdcInitStatus         Xldv20XdslInitStatus,
                   xldv20SdcMinMarginUpCfg           INTEGER,
	           xldv20SdcTransceiverOutputPower Xldv20AdslOutputPower,
	           xldv20SdcXdslServiceType    Xldv20XdslServiceTypeCurrent
                   }
 
     xldv20SdcRateCfg OBJECT-TYPE
        -- info-VALUE:   for values see Xldv20SdcCtrlRate
        -- info-ROU:     GET db
        -- info-IMPL:    V2.0->
        -- info-VERCTRL: -
        SYNTAX        Xldv20SdslDataRate
