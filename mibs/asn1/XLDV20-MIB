            ::= { xldv20VcCrossConnectEntry 9 }

         xldv20VcxAlarmState OBJECT-TYPE
            -- info-VALUE:   noAlarm(1), alarmPresent(2)
            -- info-ROU:     GET db
            -- info-IMPL:    V2.0->
            -- info-VERCTRL: -
            SYNTAX        Xldv20CallpAlarmState
            ACCESS        read-only
            STATUS        mandatory
            DESCRIPTION
                 " This attribute indicates whether for the cross connect
                   an alarm is present or not. "
            ::= { xldv20VcCrossConnectEntry 10 }

         xldv20VcxAdminState OBJECT-TYPE
            -- info-VALUE:   unlocked(1), locked(2)
            -- info-ROU:     GET db  SET cp 
            -- info-IMPL:    V2.0->
            -- info-VERCTRL: -
            SYNTAX        Xldv20AdminState
            ACCESS        read-write
            STATUS        mandatory
            DESCRIPTION
                 " This attribute is used to activate (unlock) and deactivate
                   (lock) the VCL cross connection. The cross connect is
                   removed within the ATM ASIC, the configuration data of the
                   cross connect are still available within the DSLAM . "
            ::= { xldv20VcCrossConnectEntry 11 }

         xldv20VcxAtmfVplIndex OBJECT-TYPE
            -- info-VALUE:   0..4000
            -- info-ROU:     GET db
            -- info-IMPL:    V2.0->
            -- info-VERCTRL: -
            SYNTAX        INTEGER (0..4000)
            ACCESS        read-only
            STATUS        mandatory
            DESCRIPTION
                 " For cross connects at the ATMF port of NT this value
                 identifies the belonging VplTp .
                 The value is the entry number in the xldv20VplTpTable.
                 For all other cross connects the value is always 0. "
            ::= { xldv20VcCrossConnectEntry 12 }


         -- --------------------------------------------------------------
         --                        VCL TP Table
         -- --------------------------------------------------------------

         xldv20VclTpTable OBJECT-TYPE
            -- info-IMPL:    V2.0->
            SYNTAX        SEQUENCE OF Xldv20VclTpEntry
            ACCESS        not-accessible
            STATUS        mandatory
            DESCRIPTION
                 " This table contains the entries for the VC links. "
            --DEFAULT test-function-async     fsnVclEntryTest
            --DEFAULT next-function-async     fsnVclEntryNext
            --DEFAULT set-function-async      fsnVclEntrySet
            --DEFAULT get-function-async      fsnVclEntryGet
            ::= { xldv20CallP 7 }

         xldv20VclTpEntry OBJECT-TYPE
            -- info-VALUE:   -
            -- info-ROU:     -
            -- info-IMPL:    V2.0->
            -- info-VERCTRL: -
            SYNTAX        Xldv20VclTpEntry
            ACCESS        not-accessible
            STATUS        mandatory
            DESCRIPTION
                 " The entries represent the termination of VCLs on an ATM NE.
                   An instance of the VC Cross Connection managed entity may be
                   used to relate two instances of the VCL Termination Point
                   managed entity (i.e. for point-to-point cross connection).
                   Instances of this managed entity may be created automatically
                   by the ATM NE or as a result of an VC cross connect request.
                   Similarly instances of this managed entity may be deleted
                   automatically by the ATM NE or as a result of an VC  Cross
                   Connect release request.
                   Notifications: xldv20TrStateChangeOper. "
            INDEX         { xldv20VclIndex }
            ::= { xldv20VclTpTable 1 }

         Xldv20VclTpEntry ::=
            SEQUENCE { xldv20VclOperationalState          Xldv20OperState,
                       xldv20VclVpiValue                  INTEGER,
                       xldv20VclVciValue                  INTEGER,
                       xldv20VclHwUnitIndex               Unsigned16,
                       xldv20VclIfIndex                   Unsigned16,
                       xldv20VclPeakCellRateUpstream      Unsigned32,
                       xldv20VclPeakCellRateDownstream    Unsigned32,
                       xldv20VclCellDelayVariationToleranceUpstream    INTEGER,
                       xldv20VclCellDelayVariationToleranceDownstream  INTEGER,
                       xldv20VclTrafficType               Xldv20TrafficType,
                       xldv20VclTrafficDirection          Xldv20TrafficDirection,
                       xldv20VclConnectivityPointer       Unsigned16,
                       xldv20VclLineIndex                 Unsigned16,
                       xldv20VclSegmentEndPoint           INTEGER,
                       xldv20VclSustainableCellRateUpstream   Unsigned32,
                       xldv20VclSustainableCellRateDownstream Unsigned32,
                       xldv20VclMaximumBurstSizeUpstream      INTEGER,
                       xldv20VclMaximumBurstSizeDownstream    INTEGER,
                       xldv20VclAlarmSeverityIndex            Xldv20AlmSevProfileIndex,
                       xldv20VclAlarmFilteringIndex           Xldv20AlmFiltProfileIndex,
                       xldv20VclCvcIndexSegment               Unsigned16, -- continuity check
                       xldv20VclIndex                         INTEGER,
                       xldv20VclLOCAlarm                  Xldv20TpAlarmState,
                       xldv20VclVpcIndex                      INTEGER,
                       xldv20VclVbrCDVTUpstream               INTEGER,
                       xldv20VclVbrCDVTDownstream             INTEGER
                       }

         xldv20VclOperationalState OBJECT-TYPE
            -- info-VALUE:   enabled, disabled
            -- info-ROU:     GET db
            -- info-IMPL:    V2.0->
            -- info-VERCTRL: -
