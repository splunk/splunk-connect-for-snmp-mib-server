        ACCESS      read-only
        STATUS      mandatory
        DESCRIPTION
             " The current operational state of the near-end IMA Group State
        Machine. "
        ::= { xldv20ImaGroupEntry 4 }

        xldv20ImaGroupFeState OBJECT-TYPE
        -- info-VALUE:   notConfigured(1), startUp(2),
        -- info-VALUE:   startUpAck(3),
        -- info-VALUE:   configAbortUnsupported(4),
        -- info-VALUE:   configAbortIncompatibleSymmetry(5),
        -- info-VALUE:   configAbortOther(6),
        -- info-VALUE:   insufficientLinks(7),
