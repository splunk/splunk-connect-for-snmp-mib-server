            ACCESS           read-only
            STATUS           mandatory
            DESCRIPTION
                  " The IP Address of the NT,
                    parameter for ipForwarding - mode. "
            ::= { xldv20EthernetConfigEntry 3 }

         xldv20EthIpAddressSubnetMaskNt   OBJECT-TYPE
            -- info-VALUE:   Default: 0.0.0.0
            -- info-ROU:     GET db
            -- info-IMPL:    V2.0->
            -- info-VERCTRL: -
