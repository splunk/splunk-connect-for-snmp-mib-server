        --DEFAULT next-function-async     fsnRstNext
        --DEFAULT get-function-async      fsnRstGet
        ::= { xldv20Reset 5 }

     xldv20RstControlReqResult OBJECT-TYPE
        -- info-VALUE:   noRequest, reload, cold, dbReload
        -- info-VALUE:   warm, commExecStarted,
        -- info-VALUE:   rjUpgRunning,rjRstRunning,rjPiuNotAvail
        -- info-VALUE:   rjResetNotSupported,paramFaulty
