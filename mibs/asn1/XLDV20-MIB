            ACCESS        read-write
            STATUS        mandatory
            DESCRIPTION
                 " Identifies the specific or general location of the
                   ATM NE component . "
            ::= { xldv20HwEquipEntry 2 }

         xldv20EquUserLabel OBJECT-TYPE
            -- info-VALUE:   e.g.: "XLD Testbed"
            -- info-ROU:     GET db  SET db
            -- info-IMPL:    V2.0->
            -- info-VERCTRL: -
