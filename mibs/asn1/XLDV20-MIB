                 " The Ethernet Configuration Table. "
            --DEFAULT get-function-async      fsnEthConfigEntryGet
            --DEFAULT next-function-async     fsnEthConfigEntryNext
            ::= { xldv20CallP 10 }

         xldv20EthernetConfigEntry OBJECT-TYPE
            -- info-IMPL:    V2.0->
            -- info-VERCTRL: -
            SYNTAX        Xldv20EthernetConfigEntry
            ACCESS        not-accessible
            STATUS        mandatory
            DESCRIPTION
                 " The entry of the Ethernet Configuration Table. "
            INDEX         { xldv20IfIndex }
            ::= { xldv20EthernetConfigTable 1 }

          Xldv20EthernetConfigEntry ::=
            SEQUENCE { xldv20EthMode              Xldv20EthNtMode,
                       xldv20EthE164Isp           OCTET STRING,
                       xldv20EthIpAddressNt       IpAddress,
                       xldv20EthIpAddressSubnetMaskNt IpAddress,
                       xldv20EthIpAddressCo               IpAddress,
                       xldv20EthIpAddressSubnetMaskCo IpAddress,
                       xldv20EthIpAddressRemoteRouter IpAddress
                       }

         xldv20EthMode    OBJECT-TYPE
            -- info-VALUE:   bridging, routing
            -- info-VALUE:   default: bridging
            -- info-ROU:     GET db
            -- info-IMPL:    V2.0->
            -- info-VERCTRL: -
            SYNTAX        Xldv20EthNtMode 
            ACCESS        read-only
            STATUS        mandatory
            DESCRIPTION
                  " The operational mode of the ethernet configuration:
                    Available modes: bridging, routing.
                    "
            ::= { xldv20EthernetConfigEntry 1 }

         xldv20EthE164Isp     OBJECT-TYPE
            -- info-VALUE:   -
            -- info-ROU:     GET db
            -- info-IMPL:    V2.0->
            -- info-VERCTRL: -
            SYNTAX            OCTET STRING ( SIZE (0..20) ) 
            ACCESS            read-only
            STATUS            mandatory
            DESCRIPTION
                  " The telephon number of the Internet Service Provider,
                    parameter for ipForwarding- mode"
            ::= { xldv20EthernetConfigEntry 2 }

         xldv20EthIpAddressNt   OBJECT-TYPE
            -- info-VALUE:   Default: 0.0.0.0
            -- info-ROU:     GET db
            -- info-IMPL:    V2.0->
            -- info-VERCTRL: -
