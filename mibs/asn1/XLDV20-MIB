         -- --------------------------------------------------------------
         --                VCX Table for Vcl cross connections
         -- --------------------------------------------------------------

         xldv20VcCrossConnectTable OBJECT-TYPE
            -- info-IMPL:    V2.0->
            -- info-VERCTRL: -
            SYNTAX        SEQUENCE OF Xldv20VcCrossConnectEntry
            ACCESS        not-accessible
            STATUS        mandatory
            DESCRIPTION
                 " The VC cross connect table for Virtual Channel
                    point-to-point cross connections. 
                   This managed entity is used to represent
                   the VC-cross-connect relationship between two termination
                   points.
                   Instances of this managed entity shall be automatically
                   created and deleted on VCX Connect and VCX Disconnect
		   actions . "
            --DEFAULT test-function-async     fsnVcxEntryTest
            --DEFAULT next-function-async     fsnVcxEntryNext
            --DEFAULT set-function-async      fsnVcxEntrySet
            --DEFAULT get-function-async      fsnVcxEntryGet
            ::= { xldv20CallP 6 }

         xldv20VcCrossConnectEntry OBJECT-TYPE
            -- info-IMPL:    V2.0->
            -- info-VERCTRL: -
            SYNTAX        Xldv20VcCrossConnectEntry
            ACCESS        not-accessible
            STATUS        mandatory
            DESCRIPTION
                 " An entry in the VC cross connect table.
                   Notifications : xldv20TrStateChangeOper. "
            INDEX         { xldv20VcxIndex }
            ::= { xldv20VcCrossConnectTable 1 }

         Xldv20VcCrossConnectEntry ::=
            SEQUENCE { xldv20VcxOperationalState   Xldv20OperState,
                       xldv20VcxTerminationPointA  INTEGER,
                       xldv20VcxTerminationPointZ  INTEGER,
                       xldv20VcxLineIndex          Unsigned16,
                       xldv20VcxEthVpcIndex        INTEGER,
                       xldv20VcxEthVccIndex        INTEGER,
                       xldv20VcxVpcNniIndex        Unsigned16,
                       xldv20VcxVpcUniIndex        Unsigned16,
                       xldv20VcxIndex              INTEGER,
                       xldv20VcxAlarmState         Xldv20CallpAlarmState,
                       xldv20VcxAdminState         Xldv20AdminState,
                       xldv20VcxAtmfVplIndex       INTEGER }

         xldv20VcxOperationalState OBJECT-TYPE
            -- info-VALUE:   enabled, disabled
            -- info-ROU:     GET db
            -- info-IMPL:    V2.0->
            -- info-VERCTRL: -
            SYNTAX        Xldv20OperState
            ACCESS        read-only
            STATUS        mandatory
            DESCRIPTION
                 " The Operational State of Vc cross connect. "
            ::= { xldv20VcCrossConnectEntry 1 }

         xldv20VcxTerminationPointA OBJECT-TYPE
            -- info-VALUE:   1..4096
            -- info-ROU:     GET db
