           -- --------------------------------------------------------------
           --                    continuity check control group
           -- --------------------------------------------------------------
      
           xldv20ContinuityCheckControl OBJECT IDENTIFIER ::= { xldv20CallP 13 }
            -- info-IMPL:    V2.0->
            -- info-VERCTRL: -
      
           -- This group handles the TMN control request for activating/deactivating 
           -- the continuity check function .
           --
      
           xldv20CocControlReq OBJECT-TYPE
              -- info-VALUE:   noRequest(command), continuityCheckRequest(command)  
              -- info-ROU:     GET db  SET cp 
            -- info-IMPL:    V2.0->
            SYNTAX        Xldv20ControlReq
            ACCESS        read-write
            STATUS        mandatory
            DESCRIPTION
                 "Control group for activating/deactivating
		  the continuity check at the source/sink of
		  a VPL or VCL connection.
		 The user command attribute to activate a continuity check
                   (continuityCheckRequest) or to deactivate (noRequest).
                     Notifications : none . "
            --DEFAULT test-function-async     fsnCocTest
            --DEFAULT next-function-async     fsnCocNext
            --DEFAULT set-function-async      fsnCocSet
            --DEFAULT get-function-async      fsnCocGet
            ::= { xldv20ContinuityCheckControl 1 }
      
           xldv20CocControlStatus OBJECT-TYPE
              -- info-VALUE:   requestIdle(1), requestBusy(2), requestPassed(3)
              -- info-VALUE:   requestFailed(4)
              -- info-ROU:     GET db
            -- info-IMPL:    V2.0->
            -- info-VERCTRL: -
            SYNTAX        Xldv20ControlStatus
            ACCESS        read-only
            STATUS        mandatory
            DESCRIPTION
                 " The status of the control request. The value requestIdle will be
                   set after startup/recovery or after a running test was aborted by
                   the SNMP manager. "
            --DEFAULT next-function-async     fsnCocNext
            --DEFAULT get-function-async      fsnCocGet
            ::= { xldv20ContinuityCheckControl 2 }
      
           xldv20CocEndpointType OBJECT-TYPE
              -- info-VALUE:   source (1), sink (2)
              -- info-VALUE:   both (3)
              -- info-ROU:     GET db  SET db 
            -- info-IMPL:    V2.0->
            -- info-VERCTRL: -
            SYNTAX        Xldv20EndpointType
            ACCESS        read-write
            STATUS        mandatory
            DESCRIPTION
                 " This attribute specifies whether the source or sink of a
                 should be enabled/disabled for countinuity check . "
            --DEFAULT test-function-async     fsnCocTest
            --DEFAULT next-function-async     fsnCocNext
            --DEFAULT set-function-async      fsnCocSet
            --DEFAULT get-function-async      fsnCocGet
            ::= { xldv20ContinuityCheckControl 3 }
           
           xldv20CocObjectType OBJECT-TYPE
              -- info-VALUE:   vplTp,vpcTp,vclTp
              -- info-ROU:     GET db  SET db 
            -- info-IMPL:    V2.0->
            -- info-VERCTRL: -
            SYNTAX        Xldv20RepSource
            ACCESS        read-write
            STATUS        mandatory
            DESCRIPTION
                 " Selects the type of COH object. "
            --DEFAULT test-function-async     fsnCocTest
            --DEFAULT next-function-async     fsnCocNext
            --DEFAULT set-function-async      fsnCocSet
            --DEFAULT get-function-async      fsnCocGet
            ::= { xldv20ContinuityCheckControl 4 }
           
           xldv20CocTestType OBJECT-TYPE
              -- info-VALUE:   segment(1), endToEnd(2) 
              -- info-ROU:     GET db  SET db 
            -- info-IMPL:    V2.0->
            -- info-VERCTRL: -
            SYNTAX        Xldv20TestType
            ACCESS        read-write
            STATUS        mandatory
            DESCRIPTION
                 " The type of continuity check (segment or endToEnd) . "   
            --DEFAULT test-function-async     fsnCocTest
            --DEFAULT next-function-async     fsnCocNext
            --DEFAULT set-function-async      fsnCocSet
            --DEFAULT get-function-async      fsnCocGet
            ::= { xldv20ContinuityCheckControl 5 }
           
           xldv20CocIndex OBJECT-TYPE
              -- info-VALUE:   0..65535
              -- info-ROU:     GET db  SET db
              -- info-IMPL:    V2.0->
              -- info-VERCTRL: -
            SYNTAX        Unsigned16
            ACCESS        read-write
            STATUS        mandatory
            DESCRIPTION
                   " If the continuity check should be enabled it is
                   the index of the selected COH object.
                   If the continuity check should be disabled it is
                   the index xldv20VplCvpIndexSegment or xldv20VpcCvpIndexSegment.
                   xldv20VpcCvpIndexEndToEnd or xldv20VclCvcIndexSegment. "
            --DEFAULT test-function-async     fsnCocTest
            --DEFAULT next-function-async     fsnCocNext
            --DEFAULT set-function-async      fsnCocSet
            --DEFAULT get-function-async      fsnCocGet
            ::= { xldv20ContinuityCheckControl 6 }
      
         xldv20CocControlReqResult OBJECT-TYPE
            -- info-VALUE:   noRequest (disable command)
            -- info-VALUE:   continuityCheckRequest(enable command)  
            -- info-VALUE:   paramFaulty, continuityCheckBusy, 
            -- info-VALUE:   continuityCheckAlreadyEnabled
            -- info-VALUE:   continuityCheckAlreadyDisabled
            -- info-VALUE:   continuityCheckReject
            -- info-VALUE:   continuityCheckTestTypeConflict
            -- info-VALUE:   continuityCheckNotSupportedOnNt
            -- info-VALUE:   requestBusy, setDataFailed
            -- info-VALUE:   aswTimeout, commandFailed, commandSucceeded
            -- info-ROU:     GET db
            -- info-IMPL:    V2.0->
            -- info-VERCTRL: -
