         xldv20VplTpEntry OBJECT-TYPE
            -- info-IMPL:    V2.0->
            -- info-VERCTRL: -
            SYNTAX        Xldv20VplTpEntry
            ACCESS        not-accessible
            STATUS        mandatory
            DESCRIPTION
                 " Represent the VPL termination points on an ATM NE. "
            INDEX         { xldv20VplIndex }
            ::= { xldv20VplTpTable 1 }

         Xldv20VplTpEntry ::=
            SEQUENCE { xldv20VplOperationalState          Xldv20OperState,
                       xldv20VplVpiValue                  INTEGER,
                       xldv20VplHwUnitIndex               Unsigned16,
                       xldv20VplIfIndex                   Unsigned16,
                       xldv20VplPeakCellRateUpstream      Unsigned32,
                       xldv20VplPeakCellRateDownstream    Unsigned32,
                       xldv20VplCellDelayVariationToleranceUpstream  INTEGER,
                       xldv20VplCellDelayVariationToleranceDownstream  INTEGER,
                       xldv20VplTrafficType                   Xldv20TrafficType,
                       xldv20VplTrafficDirection              Xldv20TrafficDirection,
                       xldv20VplConnectivityPointer           Unsigned16,
                       xldv20VplLineIndex                     Unsigned16,
                       xldv20VplSegmentEndPoint               INTEGER,
                       xldv20VplSustainableCellRateUpstream   Unsigned32,
                       xldv20VplSustainableCellRateDownstream Unsigned32,
                       xldv20VplMaximumBurstSizeUpstream      INTEGER,
                       xldv20VplMaximumBurstSizeDownstream    INTEGER,
                       xldv20VplAlarmSeverityIndex            Xldv20AlmSevProfileIndex,
                       xldv20VplAlarmFilteringIndex           Xldv20AlmFiltProfileIndex,
                       xldv20VplCvpIndexSegment               INTEGER,    -- continuity check
                       xldv20VplIndex                         INTEGER,
                       xldv20VplLOCAlarm                  Xldv20TpAlarmState,
                       xldv20VplVbrCDVTUpstream               INTEGER,
                       xldv20VplVbrCDVTDownstream             INTEGER
                       }

         xldv20VplOperationalState OBJECT-TYPE
            -- info-VALUE:   enabled(1)..disabled(2)
            -- info-ROU:     GET db  SET db 
            -- info-IMPL:    V2.0->
