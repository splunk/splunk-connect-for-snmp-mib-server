            ACCESS        read-write
            STATUS        mandatory
            DESCRIPTION
                 " Assignis user friendly name to the
                   associated managed entity . "
            ::= { xldv20HwEquipEntry 3 }

         xldv20EquConnectedPiuIndex OBJECT-TYPE
            -- info-VALUE:   0..65535
            -- info-ROU:     GET db
            -- info-IMPL:    V2.0->
            -- info-VERCTRL: -
            SYNTAX        Unsigned16
            ACCESS        read-only
            STATUS        mandatory
            DESCRIPTION
                 " This attribute is used to show the PIU to which the NT is
                   connected (HwUnitIndex) . In case of DSLAM value is 0. "
            ::= { xldv20HwEquipEntry 4 }

         xldv20EquConnectedPiuPort OBJECT-TYPE
            -- info-VALUE:   0..255
            -- info-VALUE:   Default value: 0
            -- info-ROU:     GET db
            -- info-IMPL:    V2.0->
            SYNTAX        INTEGER (0..255)
            ACCESS        read-only
            STATUS        mandatory
            DESCRIPTION
                 " The port ID of the connected PIU.  "
            ::= { xldv20HwEquipEntry 5 }


         -- --------------------------------------------------------------
         --             Hardware Equipment Holder Table
         -- --------------------------------------------------------------

         xldv20HwEquipHolderTable OBJECT-TYPE
            -- info-IMPL:    V2.0->
            -- info-VERCTRL: -
            SYNTAX        SEQUENCE OF Xldv20HwEquipHolderEntry
            ACCESS        not-accessible
            STATUS        mandatory
            DESCRIPTION
                 " The equipment holder table . "
            --DEFAULT test-function-async     fsnEqhEntryTest
            --DEFAULT next-function-async     fsnEqhEntryNext
            --DEFAULT set-function-async      fsnEqhEntrySet
            --DEFAULT get-function-async      fsnEqhEntryGet
            ::= { xldv20Hwm 5 }

         xldv20HwEquipHolderEntry OBJECT-TYPE
            -- info-IMPL:    V2.0->
            -- info-VERCTRL: -
            SYNTAX        Xldv20HwEquipHolderEntry
            ACCESS        not-accessible
            STATUS        mandatory
            DESCRIPTION
                 " Entry of the equipment holder table.
		 The entries represent physical resources of the ATM NE
                   holding other physical resources. Examples of resources
                   represented by instances of this managed entity are
                   shelves and slots.
                   An instance of this managed entity shall exist for each
                   shelf and slot of the ATM NE. Instances shall be auto-
                   matically created and reported by the ATM NE once
                   system initialization is complete.
                   Notifications :
		   xldv20TrObjCreate .
                   xldv20TrObjDelete .
                   xldv20TrSwVersionSet when xldv20EqhSwVersion.
                   xldv20EqhMnemoCode,xldv20EqhFwItemNumber set. "
            INDEX         { xldv20HwUnitIndex }
            ::= { xldv20HwEquipHolderTable 1 }

         Xldv20HwEquipHolderEntry ::=
            SEQUENCE { xldv20EqhType            Xldv20EqhType,
                       xldv20EqhAccPiuTypes     DisplayString,
                       xldv20EqhSlotStatus      Xldv20SlotStatus,
                       xldv20EqhSwVersion       OCTET STRING,
                       xldv20EqhMnemoCode       OCTET STRING,
                       xldv20EqhFwCode          OCTET STRING,
                       xldv20EqhRiMnemoCode     DisplayString,
                       xldv20EqhRiFwItemNumber  DisplayString,
                       xldv20EqhRiFwIssue       DisplayString,
                       xldv20EqhRiHwItemNumber  DisplayString,
                       xldv20EqhRiHwIssue       DisplayString, 
                       xldv20EqhRiSerialNumber  DisplayString } 


         xldv20EqhType OBJECT-TYPE
            -- info-VALUE:   slot, basicShelf
            -- info-VALUE:   extendedShelf, ntAdslShelf
            -- info-VALUE:   cpeShelf  (for CPE customer premise equipment)
            -- info-VALUE:   ntVdslShelf(V2.x->) 
            -- info-ROU:     GET db
            -- info-IMPL:    V2.0->
            -- info-VERCTRL: -
            SYNTAX        Xldv20EqhType
            ACCESS        read-only
            STATUS        mandatory
            DESCRIPTION
                 " Type of the equipment holder. "
            ::= { xldv20HwEquipHolderEntry 1 }

         xldv20EqhAccPiuTypes OBJECT-TYPE
            -- info-VALUE:   'CI:A'
            -- info-VALUE:   'LU155:OS', 'LU155:OL', 'LU155:C', 'LUDSX3:C'
            -- info-VALUE:   'LUIMAE1:C', 'LUIMADS1:C'
            -- info-VALUE:   'SUADSL:16P' (POTS), 'SUADSL:16I' (ISDN),
            -- info-VALUE:   'SUSDSL:16'
            -- info-VALUE:   'NTAD:1I', 'NTAD:1P'
            -- info-VALUE:   'LIETH25:11', 'LI25:1L' (loadable),
            -- info-VALUE:   'CPE'
            -- info-VALUE:   'UPL:A'
            -- info-ROU:     GET db
            -- info-IMPL:    V2.0->
            -- info-VERCTRL: -
            SYNTAX        DisplayString (SIZE (0..200))
            ACCESS        read-only
            STATUS        mandatory
            DESCRIPTION
                 " Accepted PIU Types of this slot.
		   This attribute identifies the types of Plug-In Units to be
                   supported by this slot. They are separated by newline
                   characters. That means 'CR LF'.
                   This attribute only supplies if the Equipment holder
                   instance represents a slot.  "
            ::= { xldv20HwEquipHolderEntry 2 }

         xldv20EqhSlotStatus OBJECT-TYPE
            -- info-VALUE:   unknown(0) (default), used(1), empty(2)
            -- info-ROU:     GET db
            -- info-IMPL:    V2.0->
            -- info-VERCTRL: -
            SYNTAX        Xldv20SlotStatus
            ACCESS        read-only
            STATUS        mandatory
            DESCRIPTION
                 " Indicates whether a slot is empty or used.
                   This attribute only supplies if the Equipment holder
                   instance represents a slot. "
            ::= { xldv20HwEquipHolderEntry 3 }

         xldv20EqhSwVersion OBJECT-TYPE
            -- info-VALUE:   Version of SW load
            -- info-ROU:     GET db
            -- info-IMPL:    V2.0->
            -- info-VERCTRL: -
            SYNTAX        OCTET STRING ( SIZE (0..10) )
            ACCESS        read-only
            STATUS        mandatory
            DESCRIPTION
                 " Identifies the software load of this PIU.
		 This attribute identifies the software load, if there
                   is any, which is currently designated as the one to
                   be loaded to the plug-in-unit whenever an automatic
                   reload of software is needed.
                   This attribute only applies if the equipment holder
                   instance represents a slot.
                   Default value for not loadable units is \0 . "
            ::= { xldv20HwEquipHolderEntry 4 }

         xldv20EqhMnemoCode OBJECT-TYPE
            -- info-VALUE:   Mnemo Code of PIU.
            -- info-ROU:     GET db
            -- info-IMPL:    V2.0->
            -- info-VERCTRL: -
            SYNTAX        OCTET STRING ( SIZE (0..19) )
            ACCESS        read-only
            STATUS        mandatory
            DESCRIPTION
                 " The mnemo code used to retrieve the SW
                   loadfile for the plug-in unit.
                   Default value for not loadable units is \0 . "
            ::= { xldv20HwEquipHolderEntry 5 }

         xldv20EqhFwCode OBJECT-TYPE
            -- info-VALUE:   FW code
            -- info-ROU:     GET db
            -- info-IMPL:    V2.0->
            -- info-VERCTRL: -
            SYNTAX        OCTET STRING ( SIZE (0..10) )
            ACCESS        read-only
            STATUS        mandatory
            DESCRIPTION
                 " The FW code used to retrieve the SW
                   loadfile for the plug-in unit.
                   Default value for not loadable units is \0 . "
            ::= { xldv20HwEquipHolderEntry 6 }

         xldv20EqhRiMnemoCode OBJECT-TYPE
            -- info-VALUE:   Default: "\0"
            -- info-ROU:     GET hm
            -- info-IMPL:    V2.0->
            -- info-VERCTRL: -
            SYNTAX        DisplayString (SIZE (0..20))
            ACCESS        read-only
            STATUS        mandatory
            DESCRIPTION
                 " This attribute specifies the mnemo code of the currently plugged
                   PIU. "
            ::= { xldv20HwEquipHolderEntry 7 }

         xldv20EqhRiFwItemNumber OBJECT-TYPE
            -- info-VALUE:   Default: "\0"
            -- info-ROU:     GET hm
            -- info-IMPL:    V2.0->
            -- info-VERCTRL: -
            SYNTAX        DisplayString (SIZE (0..20))
            ACCESS        read-only
            STATUS        mandatory
            DESCRIPTION
                 " This attribute specifies the FW item number of the currently
                   plugged PIU. "
            ::= { xldv20HwEquipHolderEntry 8 }

         xldv20EqhRiFwIssue OBJECT-TYPE
            -- info-VALUE:   Default: "\0"
            -- info-ROU:     GET hm
