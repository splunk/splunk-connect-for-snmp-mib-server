     Xldv20ExternalAlarmEntry ::=
          SEQUENCE { xldv20ExtAlarmIndex           INTEGER,
                     xldv20ExtAlarmAdminState      Xldv20AdminState,
                     xldv20ExtAlarmOperState       Xldv20OperState,
                     xldv20ExtAlarmActivityState   Xldv20ExtAlarmActivityState,
                     xldv20ExtAlarmSeverityIndex   Xldv20AlmSevProfileIndex,
                     xldv20ExtAlarmFilteringIndex  Xldv20AlmFiltProfileIndex,
                     xldv20ExtAlarmName            OCTET STRING }

      xldv20ExtAlarmIndex  OBJECT-TYPE
        -- info-VALUE:   1..3
        -- info-ROU:     GET db
        -- info-IMPL:    V2.0->          
        -- info-VERCTRL: -
        SYNTAX        INTEGER (1..3)
        ACCESS        read-only
        STATUS        mandatory
