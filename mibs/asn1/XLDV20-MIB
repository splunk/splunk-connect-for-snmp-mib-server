        SYNTAX        Xldv20DayLightSavingTime
        ACCESS        read-write
        STATUS        mandatory
        DESCRIPTION
             " Specifies whether summer time (day light saving time DST)
             or winter time is valid. Normally the clock is turned 
             1 hour ahead at begin of summer time and 1 hour back 
             at end. Default is winterTime."
        --DEFAULT test-function-async     fsnNeTest
        --DEFAULT next-function-async     fsnNeNext
        --DEFAULT set-function-async      fsnNeSet
        --DEFAULT get-function-async      fsnNeGet
        ::= { xldv20AtmNe 8 }



         -- --------------------------------------------------------------
         --               Hardware Unit Table 
         -- --------------------------------------------------------------

         xldv20HwUnitTable OBJECT-TYPE
            -- info-IMPL:    V2.0->
            -- info-VERCTRL: -
            SYNTAX        SEQUENCE OF Xldv20HwUnitEntry
            ACCESS        not-accessible
            STATUS        mandatory
            DESCRIPTION
                 " The XLD hardware unit table. This table models the Equipment,
                   Equipment Holder and Plug-In Unit management entities . "
            --DEFAULT next-function-async     fsnHwUnitEntryNext
            --DEFAULT get-function-async      fsnHwUnitEntryGet
            ::= { xldv20Hwm 2 }

         xldv20HwUnitEntry OBJECT-TYPE
            -- info-IMPL:    V2.0->
            -- info-VERCTRL: -
            SYNTAX        Xldv20HwUnitEntry
            ACCESS        not-accessible
            STATUS        mandatory
            DESCRIPTION
                 " An entry in the xldv20HwUnitTable.
                   Notifications : none. "
            INDEX { xldv20HwUnitIndex }
            ::= { xldv20HwUnitTable 1 }

         Xldv20HwUnitEntry ::=
            SEQUENCE { xldv20HwUnitIndex    Unsigned16,
                       xldv20HwUnitType     Xldv20HwUnitType }

         xldv20HwUnitIndex OBJECT-TYPE
            -- info-VALUE:   0..65535
            -- info-ROU:     GET db
            -- info-IMPL:    V2.0->
            -- info-VERCTRL: -
            SYNTAX        Unsigned16
            ACCESS        read-only
            STATUS        mandatory
            DESCRIPTION
                 " A unique value to identify a hardware unit within
                   the ATM agent. It is used as index for the tables
                   xldv20HwEquipTable, xldv20HwEquipHolderTable and
                   xldv20HwPlugInUnitTable . "
            ::= { xldv20HwUnitEntry 1 }

         xldv20HwUnitType OBJECT-TYPE
            -- info-VALUE:   equipment(1), equipmentHolder(2), plugInUnit(3)
            -- info-ROU:     GET db
            -- info-IMPL:    V2.0->
            -- info-VERCTRL: -
            SYNTAX        Xldv20HwUnitType
            ACCESS        read-only
            STATUS        mandatory
            DESCRIPTION
                 " Identifies the general type of this hardware unit.
                   The values are equipment, equipmentHolder and plugInUnit . "
            ::= { xldv20HwUnitEntry 2 }


         -- --------------------------------------------------------------
         --            Hardware Containment Table
         -- --------------------------------------------------------------

         xldv20HwContainmentTable OBJECT-TYPE
            -- info-IMPL:    V2.0->
            -- info-VERCTRL: -
            SYNTAX        SEQUENCE OF Xldv20HwContainmentEntry
            ACCESS        not-accessible
            STATUS        mandatory
            DESCRIPTION
                 " The hardware unit containment table. This table describes
                   the containment relationships that maintain between Equipment,
                   Equipment Holder and Plug-In Unit entities . "
            --DEFAULT next-function-async     fsnHwContEntryNext
            --DEFAULT get-function-async      fsnHwContEntryGet
            ::= { xldv20Hwm 3 }

         xldv20HwContainmentEntry OBJECT-TYPE
            -- info-IMPL:    V2.0->
            -- info-VERCTRL: -
            SYNTAX        Xldv20HwContainmentEntry
            ACCESS        not-accessible
            STATUS        mandatory
            DESCRIPTION
                 " An entry in the hardware unit containment table. Each entry
                   of this type identifies two hardware units, one of which is
                   contained directly within the other .
                   Notifications : none. "
            INDEX { xldv20HwContainingUnitIndex,
                    xldv20HwContainedUnitAddr }
            ::= { xldv20HwContainmentTable 1 }

         Xldv20HwContainmentEntry ::=
            SEQUENCE { xldv20HwContainingUnitIndex Unsigned16,
                       xldv20HwContainedUnitAddr   INTEGER,
                       xldv20HwContainedUnitIndex  Unsigned16 }

         xldv20HwContainingUnitIndex OBJECT-TYPE
            -- info-VALUE:   0..65535
            -- info-ROU:     GET db
            -- info-IMPL:    V2.0->
            -- info-VERCTRL: -
            SYNTAX        Unsigned16
            ACCESS        read-only
            STATUS        mandatory
            DESCRIPTION
                 " The HwUnitIndex (in the xldv20HwUnitTable), of the containing hardware
                   unit in this pair . "
            ::= { xldv20HwContainmentEntry 1 }

         xldv20HwContainedUnitAddr OBJECT-TYPE
            -- info-VALUE:   1..512
            -- info-ROU:     GET db
            -- info-IMPL:    V2.0->
            -- info-VERCTRL: -
            SYNTAX        INTEGER (1..512)
            ACCESS        read-only
            STATUS        mandatory
            DESCRIPTION
                 " A number uniquely identifying, within the containing hardware
                   unit, the contained unit.
                   Contained unit should be addressed starting at 1. If the
                   contained component is an Equipment Holder, this object
                   represents its physical location (e.g.  slot number). If a
                   single piece of Equipment contains both Equipment Holder and
                   other Equipment objects, the Equipment Holders should have
                   the lowest-valued addresses, so this relationship can be
                   maintained. A Plug-In Unit always has an address of 1 .  "

            ::= { xldv20HwContainmentEntry 2 }

         xldv20HwContainedUnitIndex OBJECT-TYPE
            -- info-VALUE:   0..65535
            -- info-ROU:     GET db
            -- info-IMPL:    V2.0->
            -- info-VERCTRL: -
            SYNTAX        Unsigned16
            ACCESS        read-only
            STATUS        mandatory
            DESCRIPTION
                 " The hwUnitIndex of the contained unit. "
            ::= { xldv20HwContainmentEntry 3 }


         -- --------------------------------------------------------------
         --                  Hardware Equipment Table
         -- --------------------------------------------------------------

         xldv20HwEquipTable OBJECT-TYPE
            -- info-IMPL:    V2.0->
            -- info-VERCTRL: -
            SYNTAX        SEQUENCE OF Xldv20HwEquipEntry
            ACCESS        not-accessible
            STATUS        mandatory
            DESCRIPTION
                 " The hardware equipment table. "
            --DEFAULT test-function-async     fsnEquEntryTest
            --DEFAULT next-function-async     fsnEquEntryNext
            --DEFAULT set-function-async      fsnEquEntrySet
            --DEFAULT get-function-async      fsnEquEntryGet
