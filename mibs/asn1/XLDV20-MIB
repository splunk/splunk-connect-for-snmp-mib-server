         ------------------------------------------------------------------

         xldv20TmnTable OBJECT-TYPE
            -- info-IMPL:    V2.0->
            -- info-VERCTRL: -
            SYNTAX        SEQUENCE OF Xldv20TmnEntry
            ACCESS        not-accessible
            STATUS        mandatory
            DESCRIPTION
                 " Table for TMN specific data.
                   Instances of this managed entity shall be automatically
                   SNMP configuration file has been read. "
            --DEFAULT next-function-async     fsnTmnEntryNext
            --DEFAULT get-function-async      fsnTmnEntryGet
            ::= { xldv20SnmpMgmt 2 }

         xldv20TmnEntry OBJECT-TYPE
            -- info-IMPL:    V2.0->
            -- info-VERCTRL: -
            SYNTAX        Xldv20TmnEntry
            ACCESS        not-accessible
            STATUS        mandatory
            DESCRIPTION
                 " An entry in the xldv20TmnTable.
                   Notifications : none. "
            INDEX         { xldv20TmnIndex }
            ::= { xldv20TmnTable 1 }

         Xldv20TmnEntry ::=
            SEQUENCE { xldv20TmnIndex                 INTEGER,
                       xldv20TmnOrLctTerminal         Xldv20TerminalType,
                       xldv20TmnInOrOutBand           Xldv20ChannelType,
                       xldv20TmnConnected             INTEGER,
                       xldv20TmnTrapFlowControl       Xldv20SuppressionType,
                       xldv20TmnLctSessionPollingFlag Xldv20PollingFlagType
                       }

         xldv20TmnIndex OBJECT-TYPE
            -- info-VALUE:   1..11 
            -- info-ROU:     GET db
            -- info-IMPL:    V2.0->
            -- info-VERCTRL: -
            SYNTAX        INTEGER (1..11)
            ACCESS        read-only
            STATUS        mandatory
            DESCRIPTION
                 " Index of TmnTable.
                 Entries of the table with same index belong to the same TMN.  "
            ::= { xldv20TmnEntry 1 }

         xldv20TmnOrLctTerminal OBJECT-TYPE
            -- info-VALUE:   tmn(1), lct(2), unknown(10)
            -- info-ROU:     GET db
            -- info-IMPL:    V2.0->
            -- info-VERCTRL: -
            SYNTAX        Xldv20TerminalType
            ACCESS        read-only
            STATUS        mandatory
            DESCRIPTION
                 " Specifies a LCT or TMN terminal. "
            ::= { xldv20TmnEntry 2 }
     
         xldv20TmnInOrOutBand OBJECT-TYPE
            -- info-VALUE:   inband(1), outband(2), unknown(10)
            -- info-ROU:     GET db
            -- info-IMPL:    V2.0->
            -- info-VERCTRL: -
            SYNTAX        Xldv20ChannelType
            ACCESS        read-only
            STATUS        mandatory
            DESCRIPTION
                 " Specifies whether communication is in or outband. "
            ::= { xldv20TmnEntry 3 }
     
         xldv20TmnConnected OBJECT-TYPE
            -- info-VALUE:   yes(1)..no(2) (default)
            -- info-ROU:     GET db
            -- info-IMPL:    V2.0->
            -- info-VERCTRL: -
            SYNTAX        INTEGER {
                             yes(1),
                             no(2) }
            ACCESS        read-only
            STATUS        mandatory
            DESCRIPTION
                 " The attribute indicates whether this TMN/LCT is connected.
		 "
            ::= { xldv20TmnEntry 4 }

         xldv20TmnTrapFlowControl OBJECT-TYPE
            -- info-VALUE:   noTraps(1) (default), allTraps(2),
            -- info-VALUE:   noEvents(3), noAlarms(4)
            -- info-ROU:     GET db
            -- info-IMPL:    V2.0->
            -- info-VERCTRL: -
            SYNTAX        Xldv20SuppressionType
            ACCESS        read-only
            STATUS        mandatory
            DESCRIPTION
                 " To suppress or not autonomous messages to TMN-OS.
                 Default : noTraps after reload or first time power up. "
            ::= { xldv20TmnEntry 5 }

         xldv20TmnLctSessionPollingFlag OBJECT-TYPE
            -- info-VALUE:   flagSet(1), flagNotSet(2) (default)
            -- info-ROU:     GET db
            -- info-IMPL:    V2.0->
            -- info-VERCTRL: -
            SYNTAX        Xldv20PollingFlagType
            ACCESS        read-only
            STATUS        mandatory
            DESCRIPTION
                 " TMN specific LCT session polling flag. "
            ::= { xldv20TmnEntry 6 }

         -- ==============================================================
         --           C o n n e c t i o n  H a n d l i n g  G r o u p
         -- ==============================================================

         -- --------------------------------------------------------------
         --           ATM Cross Connection Command Control subgroup
         -- --------------------------------------------------------------

         xldv20AtmCcControl OBJECT IDENTIFIER ::= { xldv20CallP 1 }
            -- info-IMPL:    V2.0->
            -- info-VERCTRL: -

         -- This managed entity manages the establishment and release
         -- of VP cross-connections ( e.g. VPI ) in the ATM NE.
         --
         -- An instance of this managed entity shall be automatically
         -- created by the ATM NE imnmediately following ATM NE initialization.

         xldv20ActOperationalState OBJECT-TYPE
            -- info-VALUE:   enabled(1)..disabled(2)
            -- info-ROU:     GET db 
            -- info-IMPL:    V2.0->
            -- info-VERCTRL: -
            SYNTAX        Xldv20OperState
            ACCESS        not-accessible
            STATUS        mandatory
            DESCRIPTION
                 " Unused. "
            --DEFAULT next-function-async     fsnActNext
            --DEFAULT get-function-async      fsnActGet
            ::= { xldv20AtmCcControl 1 }

         xldv20ActControlReq OBJECT-TYPE
            -- info-VALUE:   createVplCC, deleteVplCC
            -- info-VALUE:
            -- info-VALUE:   createEthVplCC, deleteEthVplCC
            -- info-VALUE:
            -- info-VALUE:   createVpcTp(V2.0->) ,deleteVpcTp(V2.0->)
