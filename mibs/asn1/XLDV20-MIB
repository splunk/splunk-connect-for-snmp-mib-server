        -- info-VALUE:   sucDbBackupFault ,sucUpgradeComplete
        -- info-VALUE:   sucUpgradeNotComplete
        -- info-VALUE:   sucUpgrTaskCreationFault
        -- info-VALUE:   sucUpgrDbAccessFault
        -- info-VALUE:   sucAlreadyLoaded
        -- info-VALUE:   commandSucceeded, commandFailed
        -- info-ROU:     GET db
