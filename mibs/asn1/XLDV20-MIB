        via ICP cells. "
      ::= { xldv20ImaLinkEntry 6 }
 
      xldv20ImaLinkNeRxFailureStatus OBJECT-TYPE
        -- info-VALUE:   noFailure(1), imaLinkFailure(2),
        -- info-VALUE:   lifFailure(3), lodsFailure(4),
        -- info-VALUE:   misConnected(5), blocked(6),
        -- info-VALUE:   fault(7),
        -- info-VALUE:   farEndTxLinkUnusable(8),
        -- info-VALUE:   farEndRxLinkUnusable(9)
