                  " The Index of this table. "
            ::= { xldv20ContinuityCheckVpEntry 1 }
      
         xldv20CvpEndpointType OBJECT-TYPE
            -- info-VALUE:   source (1), sink (2), both(3)
            -- info-ROU:     GET db
            -- info-IMPL:    V2.0->
            -- info-VERCTRL: -
            SYNTAX        Xldv20EndpointType
            ACCESS        read-only
            STATUS        mandatory
            DESCRIPTION
                 " The configured endpoint type (source or sink or
                 both) for countinuity check . "
            ::= { xldv20ContinuityCheckVpEntry 2 }

           xldv20CvpTestType OBJECT-TYPE
              -- info-VALUE:   segment(1), endToEnd(2) 
