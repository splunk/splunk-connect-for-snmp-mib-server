            -- info-TRAPTYPE:  alarm
            -- info-FAILEDSRC: tm   
            -- info-FAILEDOBJ: atmIma
            -- info-IMPL:    V2.0->          
            ENTERPRISE xldv20Traps 
            VARIABLES { xldv20TvTimeAndDate,
                        xldv20TvRepEntity,
                        xldv20TvRepSource,
                        xldv20TvAlmSpecProblem,
                        xldv20TvAlmNatureOfAlarm,
                        xldv20TvAlmFailedComponent,
                        xldv20TvAlmFailedComponentRepSource,
                        xldv20TvAlmSeverityOfFailure,
                        xldv20TvAlmStatus,
                        xldv20TvAlmPropRepairAction,
                        xldv20TvAlmSpecProblemText }
            DESCRIPTION
                 " IMA group reports less than minimum number of links are active
		 (detected on far-end). "
            ::= 453
            
         xldv20TrInsufficientLinksFe TRAP-TYPE
            -- info-TRAPTYPE:  alarm
            -- info-FAILEDSRC: tm   
            -- info-FAILEDOBJ: atmIma
