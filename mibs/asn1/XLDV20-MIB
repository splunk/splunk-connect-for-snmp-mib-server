         xldv20ActControlReqResult OBJECT-TYPE
            -- info-VALUE:   COMMANDS:
            -- info-VALUE:   createVplCC, deleteVplCC
            -- info-VALUE:
            -- info-VALUE:   createEthVplCC, deleteEthVplCC
            -- info-VALUE:
            -- info-VALUE:   createVpcTp(V2.0->) ,deleteVpcTp(V2.0->)
            -- info-VALUE:
            -- info-VALUE:   createEthVpcTp(V2.0->), deleteEthVpcTp(V2.0->)
            -- info-VALUE:
            -- info-VALUE:   createVclCC, deleteVclCC
            -- info-VALUE:
            -- info-VALUE:   createEthVclCC, deleteEthVclCC
            -- info-VALUE:
            -- info-VALUE:   configEth
            -- info-VALUE:   configInbandTmn
            -- info-VALUE:
            -- info-VALUE:   modifyNumberOfVcs(V2.0->)
            -- info-VALUE:
            -- info-VALUE:   RESULTS:
            -- info-VALUE:
            -- info-VALUE:   actVpiNniFaulty
            -- info-VALUE:   actVpiUniFaulty ,actPortLocationIdFaulty
            -- info-VALUE:   actControlReqFaulty   ,lineNotEnabled       
            -- info-VALUE:   ccExists            
            -- info-VALUE:   actPeakCellRateUpstreamFaulty
            -- info-VALUE:   actPeakCellRateDownstreamFaulty 
            -- info-VALUE:   actCellDelayVariationUpstreamFaulty
            -- info-VALUE:   actCellDelayVariationDownstreamFaulty
            -- info-VALUE:   actTrafficTypeFaulty
            -- info-VALUE:   actSCRUpstreamFaulty
            -- info-VALUE:   actSCRDownstreamFaulty ,actMBSUpstreamFaulty 
            -- info-VALUE:   actMBSDownstreamFaulty
            -- info-VALUE:   actVpcTpNotExisting, actVpcTpExists 
            -- info-VALUE:   actTrafficDirectionFaulty ,actVciNniFaulty
            -- info-VALUE:   actVciUniFaulty ,actLineBlocked
            -- info-VALUE:   vpiUniNotExisting
            -- info-VALUE:   vciTrafficTypeInvalid ,vciUniValueFaulty    
            -- info-VALUE:   actWrongLiType
            -- info-VALUE:   actSegmentEndPointUniFaulty  
            -- info-VALUE:   actSegmentEndPointNniFaulty 
            -- info-VALUE:   actSegmentEndPointNtFaulty
            -- info-VALUE:   actVclCCExists             
            -- info-VALUE:   actNumberOfVcsFaulty
            -- info-VALUE:   actMaxCcsPerPortExceeded
            -- info-VALUE:   actMaxNrOfCcsInDbExceeded
            -- info-VALUE:   actMaxNrOfCcsOnHwExceeded
            -- info-VALUE:   actMaxNumberOfVcsToSmall
            -- info-VALUE:   configEthFailed ,ethModeInvalid 
            -- info-VALUE:   ethVpiUniNotExisting ,ethVciUniNotExisting
            -- info-VALUE:   ethVciTrafficTypeFaulty  
            -- info-VALUE:   ethVciPeakCellRateUpstreamFaulty
            -- info-VALUE:   actEthVpcTpDeletionNotAllowed  
            -- info-VALUE:   continuityCheckRequest
            -- info-VALUE:   continuityCheckAlreadyEnabled
            -- info-VALUE:   continuityCheckAlreadyDisabled
            -- info-VALUE:   continuityCheckTestTypeConflict
            -- info-VALUE:   continuityCheckNotSupportedOnNt
            -- info-VALUE:   continuityCheckReject
            -- info-VALUE:   continuityCheckTrafficDirectionConflict
            -- info-VALUE:   configInbandTmn ,configInbandTmnFailed 
            -- info-VALUE:   inbandVpiExists ,inbandVciExists
            -- info-VALUE:   cacBwOverflowUp
            -- info-VALUE:   cacBwOverflowDown ,cacLineBlockedByTlm
            -- info-VALUE:   cacTrafficTypeConflict 
            -- info-VALUE:   cacTrafficDirectionConflict
            -- info-VALUE:   cacMbsOverflowUp ,cacMbsOverflowDown
            -- info-VALUE:   cacBwWarningUp, cacBwWarningDown
            -- info-VALUE:   cacMbsWarningUp, cacMbsWarningDown
            -- info-VALUE:   cacMbsScrError ,cacInternalError
            -- info-ROU:     GET db
            -- info-IMPL:    V2.0->
            -- info-VERCTRL: -
            SYNTAX        Xldv20ControlReq
            ACCESS        read-only
            STATUS        mandatory
            DESCRIPTION
                 " The result of the last user command. "
            --DEFAULT next-function-async     fsnActNext
            --DEFAULT get-function-async      fsnActGet
            ::= { xldv20AtmCcControl 34 }

