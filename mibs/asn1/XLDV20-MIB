        ACCESS        read-write
        STATUS        mandatory
        DESCRIPTION
              " The IMA control request object for IMA MIB objects. "
        --DEFAULT test-function-async     fsnImcTest
        --DEFAULT next-function-async     fsnImcNext
        --DEFAULT set-function-async      fsnImcSet
        --DEFAULT get-function-async      fsnImcGet
      ::= { xldv20ImaControl 1 }

     xldv20ImaControlMinNumLinks OBJECT-TYPE
        -- info-VALUE:   1..8
        -- info-ROU:     GET db  SET db
