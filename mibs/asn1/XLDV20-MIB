             " The timer value for trap response. "
        --DEFAULT next-function-async     fsnDbuNext
        --DEFAULT get-function-async      fsnDbuGet
        ::= { xldv20SwmLogHandler 6 }

     xldv20DbuControlReqResult OBJECT-TYPE
        -- info-VALUE:   noRequest, commExecStarted
        -- info-VALUE:   paramFaulty, dbuReadRequest
        -- info-VALUE:   dbuReadComplete,dbuReadFailed
        -- info-VALUE:   dbuFileCreationFailed,dbuFileTransmissionFailed
        -- info-VALUE:   commandFailed, commandSucceeded
        -- info-ROU:     GET db
