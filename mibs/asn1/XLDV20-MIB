             -- Do not change enumeration !!!
	     --
             Xldv20StartupType ::=
                 INTEGER {
                     warm         (6300),
                     cold         (6301),
                     reload       (6302),
                     cmuUpgrade   (6303),
                     powerUp      (6304)
                 }

