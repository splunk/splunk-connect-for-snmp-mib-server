     -- --------------------------------------------------------------
     --                 TLM Multiple ATM Loop Back Technique
     --                 Multiple Loopback Result table
     -- --------------------------------------------------------------

     xldv20MultipleLoopbackResultTable  OBJECT-TYPE
        -- info-IMPL:    V2.0->          
        SYNTAX        SEQUENCE OF Xldv20MlbEntry
        ACCESS        not-accessible
        STATUS        mandatory
        DESCRIPTION
             " The Multiple Loopback Result table.
             Used for the multiple loopback functionality to collect
             the IDs of connecting points which
             looped back to the XLD.
             When an ATM looptest is started the table is empty.
             An entry is created when an OAM cell is received from a
             connecting point. "
        --DEFAULT next-function-async     fsnMlbEntryNext
        --DEFAULT get-function-async      fsnMlbEntryGet
        ::= { xldv20TlmOam 4 }

     xldv20MlbEntry   OBJECT-TYPE
        -- info-IMPL:    V2.0->          
        SYNTAX        Xldv20MlbEntry
        ACCESS        not-accessible
        STATUS        mandatory
        DESCRIPTION
             " An entry in the Multiple Loopback Result Table. "
        INDEX         { xldv20MlbIndex }
        ::= { xldv20MultipleLoopbackResultTable 1 }

     Xldv20MlbEntry ::=
        SEQUENCE { xldv20MlbIndex       Unsigned16,
                   xldv20MlbLoopLocId   OCTET STRING
                 }

     xldv20MlbIndex OBJECT-TYPE
