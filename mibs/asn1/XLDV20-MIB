             --DEFAULT next-function-async     fsnSnmNext
             --DEFAULT get-function-async      fsnSnmGet
             ::= { xldv20SnmpControl 13 }

         xldv20SnmControlTimeStamp OBJECT-TYPE
            -- info-VALUE:   0..2Giga
            -- info-ROU:     GET db
            -- info-IMPL:    V2.0->
            -- info-VERCTRL: -
            SYNTAX        TimeTicks
            ACCESS        read-only
            STATUS        mandatory
            DESCRIPTION
               " The time stamp of the last command (end of command). "
             --DEFAULT next-function-async     fsnSnmNext
             --DEFAULT get-function-async      fsnSnmGet
             ::= { xldv20SnmpControl 14 }
         ------------------------------------------------------------------
         --
         -- This table holds TMN specific attributes. 
