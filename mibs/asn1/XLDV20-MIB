     --    T r u n k / L i n e   M a i n t e n a n c e   G r o u p
     -- ==============================================================

     -- --------------------------------------------------------------
     --                   TLM Interface Table
     -- --------------------------------------------------------------

     xldv20IfTable OBJECT-TYPE
        -- info-IMPL:    V2.0->
        -- info-VERCTRL: -
        SYNTAX        SEQUENCE OF Xldv20IfEntry
        ACCESS        not-accessible
        STATUS        mandatory
        DESCRIPTION
             " The TLM interface table. "
        --DEFAULT test-function-async     fsnIfEntryTest
        --DEFAULT next-function-async     fsnIfEntryNext
        --DEFAULT set-function-async      fsnIfEntrySet
        --DEFAULT get-function-async      fsnIfEntryGet
        ::= { xldv20Tlm 1 }

     xldv20IfEntry OBJECT-TYPE
        -- info-IMPL:    V2.0->
        -- info-VERCTRL: -
        SYNTAX        Xldv20IfEntry
        ACCESS        not-accessible
        STATUS        mandatory
        DESCRIPTION
             " An entry in the TLM interface table. Each entry represents
               either a physical path termination point (PPTP) or a
               TC adaptor (tcAdaptor).
               Notifications :
               alarm traps when a fault has been detected or cleared.
               xldv20TrStateChangeOper.
               xldv20TrStateChangeAdmin. "
        INDEX         { xldv20IfIndex }
        ::= { xldv20IfTable 1 }

     Xldv20IfEntry ::=
          SEQUENCE { xldv20IfIndex                Unsigned16,
                     xldv20IfType                 Xldv20IfType,
                     xldv20IfLineIndex            Unsigned16,
                     xldv20IfHwUnitIndex          Unsigned16,
                     xldv20IfHwPortId             INTEGER,
                     xldv20IfConnectivityIndex    Unsigned16,
                     xldv20IfOperState            Xldv20OperState,
                     xldv20IfAdminState           Xldv20AdminState,
                     xldv20IfAlarmState           Xldv20AlarmState,
                     xldv20IfAlarmSeverityIndex   Xldv20AlmSevProfileIndex,
                     xldv20IfAlarmFilteringIndex  Xldv20AlmFiltProfileIndex }

     xldv20IfIndex OBJECT-TYPE
        -- info-VALUE:   0..65535
        -- info-ROU:     GET db
        -- info-IMPL:    V2.0->
        -- info-VERCTRL: -
        SYNTAX        Unsigned16
        ACCESS        read-only
        STATUS        mandatory
        DESCRIPTION
             " Index of the table. Identifier of a TLM interface . "
        ::= { xldv20IfEntry 1 }

     xldv20IfType OBJECT-TYPE
        -- info-VALUE:   s1n(1),s1f(2),adc(3),adr(4)
        -- info-VALUE:   ibm(5),tca(6),pon(7) (V2.x) 
        -- info-VALUE:   vdc(8)(V2.x),vdr(9)(V2.x)
        -- info-VALUE:   eth(10)
        -- info-VALUE:   d3n(11),d3f(12)
        -- info-VALUE:   e3n(13), e3f(14)
        -- info-VALUE:   s3n(15), s3f(16)
        -- info-VALUE:   e1nIma(20) (E1 near end)
        -- info-VALUE:   e1fIma(21) (E1 far end)
        -- info-VALUE:   d1nIma(22) (Ds1 near end) (V2.x)
        -- info-VALUE:   d1fIma(23) (Ds1 far end) (V2.x)
        -- info-VALUE:   sdc(24), sdr(25) (SDSL)
        -- info-VALUE:   atmIma(107)  (IMA group)
        -- info-ROU:     GET db
        -- info-IMPL:    V2.0->
        -- info-VERCTRL: -
        SYNTAX        Xldv20IfType
        ACCESS        read-only
        STATUS        mandatory
        DESCRIPTION
             " The type of a TLM interface.
               Supported interface types :
               s1n, s1f. 
               adc, adr.
               ibm, eth.
               s3n, s3f.
               d3n, d3f.
               e3n, e3f.
               e1nIma, e1fIma.
               d1nIma, d1fIma.
               sdc, sdr.
               atmIma  (IMA group).
	       tca.
               In XLDV20 unused :
	       pon.
	       vdc.
	       vdr.
	       d1nIma.
	       d1fIma.  "
        ::= { xldv20IfEntry 2 }

     xldv20IfLineIndex OBJECT-TYPE
        -- info-VALUE:   -
        -- info-ROU:     GET db
        -- info-IMPL:    V2.0->
        -- info-VERCTRL: -
        SYNTAX        Unsigned16
        ACCESS        read-only
        STATUS        mandatory
        DESCRIPTION
             " A unique value to identify interfaces belonging to the
               same line. "
        ::= { xldv20IfEntry 3 }

     xldv20IfHwUnitIndex OBJECT-TYPE
        -- info-VALUE:   0..65535
        -- info-ROU:     GET db
        -- info-IMPL:    V2.0->
        -- info-VERCTRL: -
        SYNTAX        Unsigned16
        ACCESS        read-only
        STATUS        mandatory
        DESCRIPTION
             " The index to identify the hardware unit the physical
               path/TC layer terminates on. "
        ::= { xldv20IfEntry 4 }

     xldv20IfHwPortId OBJECT-TYPE
        -- info-VALUE:   0..255
        -- info-ROU:     GET db
        -- info-IMPL:    V2.0->
        -- info-VERCTRL: -
        SYNTAX        INTEGER (0..255)
        ACCESS        read-only
        STATUS        mandatory
        DESCRIPTION
             " For PPTP it is the associated HW port ID. For TC adaptor
               it is the IfIndex of the PPTP the TC adaptor is connected to. "
        ::= { xldv20IfEntry 5 }

     xldv20IfConnectivityIndex OBJECT-TYPE
        -- info-LEAF:    xldv20ConnectivityIndex
        -- info-TREE:    xldv20Tlm
        -- info-ACC:     ro
        -- info-SYNTAX:  Unsigned16
        -- info-VALUE:   0..65535
        -- info-ROU:     GET db
        -- info-IMPL:    V2.0->
        -- info-VERCTRL: -
        SYNTAX        Unsigned16
        ACCESS        read-only
        STATUS        mandatory
        DESCRIPTION
             " For PPTP it is the associated HW port ID. For TC adaptor
               it is the IfIndex of the PPTP the TC adaptor is connected to. "
        ::= { xldv20IfEntry 6 }

     xldv20IfOperState OBJECT-TYPE
        -- info-VALUE:   enabled(1), disabled(2)
        -- info-ROU:     GET db
        -- info-IMPL:    V2.0->
        -- info-VERCTRL: -
        SYNTAX        Xldv20OperState
        ACCESS        read-only
        STATUS        mandatory
        DESCRIPTION
             " The operational state of this unit. "
        ::= { xldv20IfEntry 7 }

     xldv20IfAdminState OBJECT-TYPE
        -- info-VALUE:   unlocked(1), locked(2)
        -- info-ROU:     GET db  SET tm
        -- info-IMPL:    V2.0->
        -- info-VERCTRL: -
        SYNTAX        Xldv20AdminState
        ACCESS        read-write
        STATUS        mandatory
        DESCRIPTION
             " Activates(unlock) or deactivates(lock) the functionality
               of this managed entity. It must be locked before physical
               looptest has been started. The attribute is not
               configurable during looptest.
               The PPTP of type s1f, s3f, e3f, d3f, e1nIma, e1fIma, 
	       d1nIma, d1fIma and atmIma
	       and the associated tcAdaptors can only be locked from a
               connected LCT. "
        ::= { xldv20IfEntry 8 }

     xldv20IfAlarmState OBJECT-TYPE
        -- info-VALUE:   noAlarm(0), hpEBER(1), hpUNEQ(2), lcd(3)
        -- info-VALUE:   lof(4), lop(5), los(6)
        -- info-VALUE:   msAIS(7), msEBER(8),msSD(10)
        -- info-VALUE:   pAIS(11),pRDI(12), rsEBER(13), slm(14)
        -- info-VALUE:   tim(15), actFault(16),
        -- info-VALUE:   xmissionErr(105), ais(114)
        -- info-VALUE:   plcpLof(116), immMNR(130, lpr(131), lol(132)
        -- info-VALUE:   msRDI(9),
        -- info-VALUE:   rdi(115),plcpRDI(117),internalFault(123)
        -- info-VALUE:   wrongServiceConfigData(141)
        -- info-VALUE:   
        -- info-VALUE:   E1/Ds1 near-end:
        -- info-VALUE:   excBER( 407 ), lif( 408 ), lods( 409 ),
        -- info-VALUE:   
        -- info-VALUE:   E1/Ds1 far-end :
        -- info-VALUE:   txUnusableFe( 410 ), rxUnusableFe( 411 ),
        -- info-VALUE:   rfiIma( 412 ), rai( 413 ),
        -- info-VALUE:   
        -- info-VALUE:   IMA group :
        -- info-VALUE:   startUpFe( 450 ),
        -- info-VALUE:   configAbort( 451 ), configAbortFe( 452 ),
        -- info-VALUE:   insufficientLinks( 453 ), insufficientLinksFe( 454 ),
        -- info-VALUE:   blockedFe( 455 ), imaMinNumOfLinks( 456 )
        -- info-ROU:     GET db
        -- info-IMPL:    V2.0->
        -- info-VERCTRL: -
        SYNTAX        Xldv20AlarmState
        ACCESS        read-only
        STATUS        mandatory
        DESCRIPTION
             " The nature of an alarm at a TLM interface.
	     Values correspond to alarm trap ID`s.
	     Default is noAlarm. "
        ::= { xldv20IfEntry 9 }

     xldv20IfAlarmSeverityIndex OBJECT-TYPE
        -- info-VALUE:   1..40 (default is 1)
        -- info-ROU:     GET db  SET db
        -- info-IMPL:    V2.0->
        -- info-VERCTRL: -
        SYNTAX        Xldv20AlmSevProfileIndex
        ACCESS        read-write
        STATUS        mandatory
        DESCRIPTION
             " The index of the associated entry in the alarm severity
               profile table. These entries contain the severity
               assigments for the alarms reported by this managed
               entity.
               The range of values is:
               1,.., 10: for possible pre-configured alarm severity
                         profiles .
               Default is profile 1. "
        ::= { xldv20IfEntry 10 }

     xldv20IfAlarmFilteringIndex OBJECT-TYPE
        -- info-VALUE:   1..40
        -- info-ROU:     GET db
        -- info-IMPL:    V2.0->
        -- info-VERCTRL: -
        SYNTAX        Xldv20AlmFiltProfileIndex
        ACCESS        read-only
        STATUS        mandatory
        DESCRIPTION
             " It is a pointer to the instance of the alarm filtering
               assignment profile managed entity that contains the filtering
               assignments for the alarms reported by this managed entity. "
        ::= { xldv20IfEntry 11 }

