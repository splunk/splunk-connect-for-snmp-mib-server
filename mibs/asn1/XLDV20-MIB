     -- --------------------------------------------------------------
     --                 TLM ADSL central PPTP Table
     -- --------------------------------------------------------------

     -- This managed entity represents the extension for the PPTP at
     -- the ADSL central interface. It includes additional attributes to
     -- configure the ADSL link.
     --

     xldv20AdcPPTPTable OBJECT-TYPE
            -- info-IMPL:    V2.0->
            -- info-VERCTRL: -
        SYNTAX        SEQUENCE OF Xldv20AdcPPTPEntry
        ACCESS        not-accessible
        STATUS        mandatory
        DESCRIPTION
             " The physical path termination point table for ADSL Central
	     interface. "
        --DEFAULT test-function-async     fsnAdcEntryTest
        --DEFAULT next-function-async     fsnAdcEntryNext
        --DEFAULT get-function-async      fsnAdcEntryGet
        --DEFAULT set-function-async      fsnAdcEntrySet
        ::= { xldv20Tlm 4 }

     xldv20AdcPPTPEntry OBJECT-TYPE
            -- info-IMPL:    V2.0->
            -- info-VERCTRL: -
        SYNTAX        Xldv20AdcPPTPEntry
        ACCESS        not-accessible
        STATUS        mandatory
        DESCRIPTION
             " An entry describing a termination point in the PPTP table.
               Notifications : see xldv20IfEntry. "
        INDEX         { xldv20IfIndex }
        ::= { xldv20AdcPPTPTable 1 }

     Xldv20AdcPPTPEntry ::=
        SEQUENCE { xldv20AdcMinRateDnCfg                    Xldv20AdslDataRateDown,
                   xldv20AdcMinRateUpCfg                    Xldv20AdslDataRateUp,
                   xldv20AdcMaxRateDnCfg                    Xldv20AdslDataRateDown,
                   xldv20AdcMaxRateUpCfg                    Xldv20AdslDataRateUp,
                   xldv20AdcMarginDnCfg                           INTEGER,
                   xldv20AdcMarginUpCfg                           INTEGER,
                   xldv20AdcMinMarginDnCfg                  Xldv20AdslMinMargin,
                   xldv20AdcMinMarginUpCfg                  Xldv20AdslMinMargin,
                   xldv20AdcLatencyDnCfg                    Xldv20Latency,
                   xldv20AdcLatencyUpCfg                    Xldv20Latency,
                   xldv20AdcRateDn                          Xldv20AdslDataRateDownCurrent,
                   xldv20AdcRateUp                          Xldv20AdslDataRateUpCurrent,
                   xldv20AdcMarginDn                        Xldv20AdslMargin,
                   xldv20AdcMarginUp                        Xldv20AdslMargin,
                   xldv20AdcAttenuationDn                   Xldv20AdslAttenuation,
                   xldv20AdcAttenuationUp                   Xldv20AdslAttenuation,
                   xldv20AdcLinkState                       Xldv20LinkState,
                   xldv20AdcAISOnLOS                        Xldv20OperState,
                   xldv20AdcAISOnACT                        Xldv20OperState ,
                   xldv20AdcGuaranteedBandwidthUsage              INTEGER,
                   xldv20AdcXdslServiceTypeCfg              Xldv20XdslServiceType,
                   xldv20AdcInitStatus                      Xldv20XdslInitStatus,
	           xldv20AdcTransceiverOutputPower          Xldv20AdslOutputPower,
	           xldv20AdcFirstUsedSubCarrierUpstream           INTEGER,
	           xldv20AdcFirstUsedSubCarrierDownstream         INTEGER,
	           xldv20AdcLastUsedSubCarrierUpstream            INTEGER,
	           xldv20AdcLastUsedSubCarrierDownstream          INTEGER,
                   xldv20AdcXdslServiceType                 Xldv20XdslServiceTypeCurrent
                   }

     xldv20AdcMinRateDnCfg OBJECT-TYPE
        -- info-VALUE:   1..253 (default 1)
        -- info-VALUE:   for values see Xldv20AdcCtrlMinRateDn
        -- info-ROU:     GET db
        -- info-IMPL:    V2.0->
        -- info-VERCTRL: -
        SYNTAX        Xldv20AdslDataRateDown
        ACCESS        read-only
        STATUS        mandatory
        DESCRIPTION
             " The desired lower value of the downstream data rate. The data
               rate ranges from 32 to 8096 Kbit/sec and 
	       is splitted into units of 32 Kbit/sec.
               This corresponds to a cell rate between 75 and 19094 cells/sec.
               Default is 1 (32 kbps/75 cps). "
        ::= { xldv20AdcPPTPEntry 1 }

     xldv20AdcMinRateUpCfg OBJECT-TYPE
        -- info-VALUE:   1..24 (default 1)
        -- info-VALUE:   for values see Xldv20AdcCtrlMinRateDn
        -- info-ROU:     GET db
        -- info-IMPL:    V2.0->
        -- info-VERCTRL: -
        SYNTAX        Xldv20AdslDataRateUp
        ACCESS        read-only
        STATUS        mandatory
        DESCRIPTION
             " The desired lower value of the upstream data rate. The data
               rate ranges from 32 and 768 Kbit/sec and 
	       is splitted into units of 32 Kbit/sec.
               This corresponds to a cell rate between 75 and 1811 cells/sec.
               Default is 1 (32 kbps/75 cps). "
        ::= { xldv20AdcPPTPEntry 2 }

     xldv20AdcMaxRateDnCfg OBJECT-TYPE
        -- info-VALUE:   1..253 (default 253)
        -- info-VALUE:   for values see Xldv20AdcCtrlMinRateDn
        -- info-ROU:     GET db
        -- info-IMPL:    V2.0->
        -- info-VERCTRL: -
        SYNTAX        Xldv20AdslDataRateDown
        ACCESS        read-only
        STATUS        mandatory
        DESCRIPTION
             " The desired upper value of the downstream data rate. The data
               rate ranges from 32 and 8160 Kbit/sec and is splitted into units
               of 32 Kbit/sec.
               This corresponds to a cell rate between 75 and 1811 cells/sec.
               Default is 253 (8096 kbps/19094 cps). "
        ::= { xldv20AdcPPTPEntry 3 }

     xldv20AdcMaxRateUpCfg OBJECT-TYPE
