        -- info-VALUE:   dBm-10     (-10),
        -- info-VALUE:   dBm-9      (-9),
        -- info-VALUE:   dBm-8      (-8),
        -- info-VALUE:   dBm-7      (-7),
        -- info-VALUE:   dBm-6      (-6),
        -- info-VALUE:   dBm-5      (-5),
        -- info-VALUE:   dBm-4      (-4),
        -- info-VALUE:   dBm-3      (-3),
        -- info-VALUE:   dBm-2      (-2),
        -- info-VALUE:   dBm-1      (-1),
        -- info-VALUE:   dBm0       (0 ),
        -- info-VALUE:   dBm1       (1 ),
        -- info-VALUE:   dBm2       (2 ),
        -- info-VALUE:   dBm3       (3 ),
        -- info-VALUE:   dBm4       (4 ),
        -- info-VALUE:   dBm5       (5 ),
        -- info-VALUE:   dBm6       (6 ),
        -- info-VALUE:   dBm7       (7 ),
        -- info-VALUE:   dBm8       (8 ),
        -- info-VALUE:   dBm9       (9 ),
        -- info-VALUE:   dBm10      (10),
        -- info-VALUE:   dBm11      (11),
        -- info-VALUE:   dBm12      (12),
        -- info-VALUE:   dBm13      (13),
        -- info-VALUE:   dBm14      (14),
        -- info-VALUE:   dBm15      (15),
        -- info-VALUE:   dBm16      (16),
        -- info-VALUE:   dBm17      (17),
        -- info-VALUE:   dBm18      (18),
        -- info-VALUE:   dBm19      (19),
        -- info-VALUE:   dBm20      (20),
        -- info-VALUE:   dBm21      (21),
        -- info-VALUE:   dBm22      (22),
        -- info-VALUE:   dBm23      (23),
        -- info-VALUE:   dBm24      (24),
        -- info-VALUE:   dBm25      (25),
        -- info-VALUE:   dBm26      (26),
        -- info-VALUE:   dBm27      (27),
        -- info-VALUE:   dBm28      (28),
        -- info-VALUE:   dBm29      (29),
        -- info-VALUE:   dBm30      (30),
        -- info-VALUE:   dBm31      (31),
        -- info-VALUE:   dBm32      (32),
        -- info-VALUE:   outputPowerInvalid (32767)
        -- info-ROU:     GET tm
        -- info-IMPL:    V2.0->
        -- info-VERCTRL: -
        SYNTAX        Xldv20AdslOutputPower
        ACCESS        read-only
        STATUS        mandatory
        DESCRIPTION
             " The SDSL tranceiver output power. Granularity 1.0 dBm.
	     Default value is outputPowerInvalid. "
        ::= { xldv20SdcPPTPEntry 14 }

	xldv20SdcXdslServiceType OBJECT-TYPE
        -- info-VALUE:   gDotDmtMode  (1),
        -- info-VALUE:   ansiT1Dot413 (2),
        -- info-VALUE:   gDotLiteMode (3)
        -- info-VALUE:   serviceTypeUnknown (10)
        -- info-ROU:     GET tm
        -- info-IMPL:    V2.0->
        -- info-VERCTRL: -
        SYNTAX        Xldv20XdslServiceTypeCurrent
        ACCESS        read-only
        STATUS        mandatory
        DESCRIPTION
             " The current service type resulting from the configured service
	     type. It depends on the actual hardware.
	     Configuring gDotHsMode can result either in gDotLiteMode or gDotDmtMode.
	     gDotAll can result in gDotDmtMode, ansiT1dot413 or gDotLiteMode.
	     Default value is serviceTypeUnknown (10). "
        ::= { xldv20SdcPPTPEntry 15 }

     -- --------------------------------------------------------------
     --                 The IMA subtree (IMA-MIB)
     -- --------------------------------------------------------------
     --

     xldv20ImaMibObjects OBJECT IDENTIFIER 
        -- info-IMPL:    V2.0->
        -- info-VERCTRL: -
     ::= { xldv20TlmIma 1 }


     xldv20ImaGroupNumber OBJECT-TYPE
        -- info-VALUE:   currently 1
        -- info-ROU:     GET db
        -- info-IMPL:    V2.0->
        SYNTAX      INTEGER (0..2147483647)
        ACCESS      read-only
        STATUS      mandatory
        DESCRIPTION
            " The number of IMA groups configured on this system. "
        --DEFAULT next-function-async     fsnImnNext
        --DEFAULT get-function-async      fsnImnGet
      ::= { xldv20ImaMibObjects 1 }

         
     -- --------------------------------------------------------------
     --                 The IMA Control subtree (IMA-MIB)
     -- --------------------------------------------------------------
     --
     -- The IMA Control subtree consists of the number of IMA groups and 
     -- all attributes used for user commands on IMA MIB objects.
     --

     xldv20ImaControl OBJECT IDENTIFIER 
        -- info-IMPL:    V2.0->          
        -- info-VERCTRL: -
     ::= { xldv20ImaMibObjects 2 }

     xldv20ImaControlReq OBJECT-TYPE
        -- info-VALUE:   imaSetMinNumTxLinks
        -- info-ROU:     GET db SET tm  
