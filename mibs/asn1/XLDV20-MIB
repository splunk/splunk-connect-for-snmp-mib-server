        --DEFAULT get-function-async      fsnImcGet
        ::= { xldv20ImaControl 3 }

     xldv20ImaControlReqResult OBJECT-TYPE
        -- info-VALUE:   imaSetMinNumTxLinks(command)
        -- info-VALUE:   reject reasons :
        -- info-VALUE:   imaBwError(CAC)
        -- info-VALUE:   imaSetMinNumOk      (9202)
        -- info-VALUE:   imaSetMinNumFailed  (9203)
        -- info-VALUE:   paramFaulty, commExecStarted
        -- info-VALUE:   requestBusy,setDataFailed,
        -- info-VALUE:   aswTimeout,
