         -- --------------------------------------------------------------
         --                   VCC TP Table
         -- --------------------------------------------------------------

         xldv20VccTpTable OBJECT-TYPE
            -- info-IMPL:    V2.0->
            -- info-VERCTRL: -
            SYNTAX        SEQUENCE OF Xldv20VccTpEntry
            ACCESS        not-accessible
            STATUS        mandatory
            DESCRIPTION
                " VccTp table for VC termination points.
                  Entries are created for Ethernet NTs
                  when cross connects are created.
                  Entries are deleted with the cross connect. "
        --DEFAULT test-function-async     fsnVccEntryTest
        --DEFAULT next-function-async     fsnVccEntryNext
        --DEFAULT set-function-async      fsnVccEntrySet
        --DEFAULT get-function-async      fsnVccEntryGet
            ::= { xldv20CallP 5 }

         xldv20VccTpEntry OBJECT-TYPE
            -- info-IMPL:    V2.0->
            -- info-VERCTRL: -
            SYNTAX        Xldv20VccTpEntry
            ACCESS        not-accessible
            STATUS        mandatory
            DESCRIPTION
                   "Each entry represents the point in the switch
                   where the VCC and its associated overhead (F4 OAM cells) are 
                   terminated or originated.
                   Instances of this managed entity may be created automatically
                   by the ATM NE or as a result of an Vpl and Vcl cross connect request
		   at an Ethernet port.
                   Similarly instances of this managed entity may be deleted
                   automatically by the ATM NE as a result of an cross
                   connect deletion request.
                   Notifications : 
                     xldv20TrStateChangeOper. 
                     xldv20TrObjCreate.
                     xldv20TrObjDelete.
                     xldv20TrRDI.
                     xldv20TrAIS.
                     xldv20TrLOC alarms when fault is detected or cleared. "
            INDEX         { xldv20VccIndex }
            ::= { xldv20VccTpTable 1 }

         Xldv20VccTpEntry ::=
            SEQUENCE { xldv20VccOperationalState     Xldv20OperState,
                       xldv20VccVciValue             INTEGER,
                       xldv20VccVpcIndex             INTEGER,
                       xldv20VccHwUnitIndex          Unsigned16,
                       xldv20VccAlarmSeverityIndex   Xldv20AlmSevProfileIndex,
                       xldv20VccAlarmFilteringIndex  Xldv20AlmFiltProfileIndex,
                       xldv20VccLineIndex            Unsigned16,
                       xldv20VccIndex                INTEGER,
                       xldv20VccAISAlarm             Xldv20TpAlarmState,
                       xldv20VccRDIAlarm             Xldv20TpAlarmState,
                       xldv20VccLOCAlarm             Xldv20TpAlarmState,
                       xldv20VccCvcIndexSegment      Unsigned16,
                       xldv20VccCvcIndexEndToEnd     Unsigned16
                       }

     xldv20VccOperationalState OBJECT-TYPE
        -- info-VALUE:   enabled, disabled
        -- info-ROU:     GET db
        -- info-IMPL:    V2.0->
        -- info-VERCTRL: -
        SYNTAX        Xldv20OperState
        ACCESS        read-only
        STATUS        mandatory
        DESCRIPTION
             " The operational state identifies whether or not the VPL
               termination is capable of performing its normal functions
               (in-service or out-of-service). "
        ::= { xldv20VccTpEntry 1 }

         xldv20VccVciValue OBJECT-TYPE
