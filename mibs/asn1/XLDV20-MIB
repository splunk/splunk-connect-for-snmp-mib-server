             " The table of alarm severity profiles.
               Profiles can be created or deleted dynamically.
               Creating or deleting an entry in this table automatically creates
               or deletes the corresponding entries in the
               xldv20AlarmSevTable. "
        --DEFAULT test-function-async     fsnAlmSevProfileEntryTest
        --DEFAULT next-function-async     fsnAlmSevProfileEntryNext
        --DEFAULT set-function-async      fsnAlmSevProfileEntrySet
        --DEFAULT get-function-async      fsnAlmSevProfileEntryGet
        ::= { xldv20Alarming 2 }

     xldv20AlarmSeverityProfileEntry OBJECT-TYPE
        -- info-IMPL:    V2.0->          
        -- info-VERCTRL: -
        SYNTAX        Xldv20AlarmSeverityProfileEntry
        ACCESS        not-accessible
        STATUS        mandatory
        DESCRIPTION
             " A group of severities, one for each alarm type in
               the communications alarm group. "
        INDEX         { xldv20AlmSevProfileIndex }
        ::= { xldv20AlarmSeverityProfileTable 1 }

     Xldv20AlarmSeverityProfileEntry ::=
        SEQUENCE { xldv20AlmSevProfileIndex      Xldv20AlmSevProfileIndex,
                   xldv20AlmSevProfileRowStatus  Xldv20RowStatus }

     xldv20AlmSevProfileIndex OBJECT-TYPE
        -- info-VALUE:   1..40
        -- info-ROU:     GET db
        -- info-VERCTRL: -
        SYNTAX        Xldv20AlmSevProfileIndex
        ACCESS        read-only
        STATUS        mandatory
        DESCRIPTION
             " A number identifying this alarm severity profile. "
        ::= { xldv20AlarmSeverityProfileEntry 1 }

     xldv20AlmSevProfileRowStatus OBJECT-TYPE
        -- info-VALUE:   create(1), delete(2)
        -- info-ROU:     GET db SET db
        -- info-IMPL:    V2.0->          
        -- info-VERCTRL: -
        SYNTAX        Xldv20RowStatus
        ACCESS        read-write
        STATUS        mandatory
        DESCRIPTION
             " Used to create a new row or to delete
               an existing row in the table.
               Default value:
                   xldv20AlmSevProfileIndex = 1..10:  created
                   xldv20AlmSevProfileIndex = 11..40: deleted . "
        ::= { xldv20AlarmSeverityProfileEntry 2 }

     -- --------------------------------------------------------------
     --                    Alarm Severity Table
     -- --------------------------------------------------------------

     xldv20AlarmSeverityTable OBJECT-TYPE
        -- info-IMPL:    V2.0->          
        SYNTAX        SEQUENCE OF Xldv20AlarmSeverityEntry
        ACCESS        not-accessible
        STATUS        mandatory
        DESCRIPTION
             " The alarm severity table associates
               profile index and trap ID pairs with severities to be
               used for alarm traps that have occurred.
               (Note that this table does not apply to cleared alarms). "
        --DEFAULT test-function-async     fsnAlmSevEntryTest
        --DEFAULT next-function-async     fsnAlmSevEntryNext
        --DEFAULT set-function-async      fsnAlmSevEntrySet
        --DEFAULT get-function-async      fsnAlmSevEntryGet
        ::= { xldv20Alarming 3 }

     xldv20AlarmSeverityEntry OBJECT-TYPE
        -- info-IMPL:    V2.0->          
        SYNTAX        Xldv20AlarmSeverityEntry
        ACCESS        not-accessible
        STATUS        mandatory
        DESCRIPTION
             " An entry in this table associates an alarm severity
               profile index/trap ID pair with a severity to be used
               for alarm traps.
               The alarm severity profile index is referenced by the
               'Alarm Severity assignment Profile Pointer' attribute
               in the alarm-reporting managed entity e.g. plug-in unit
               and TLM interface (tcAdaptor, Physical Path Termination
               Point etc.).
               Notifications : none. "
        INDEX         { xldv20AlmSevProfileIndex,
	                xldv20AlmSevTrapId }
        ::= { xldv20AlarmSeverityTable 1 }

     Xldv20AlarmSeverityEntry ::=
        SEQUENCE { xldv20AlmSevTrapId  Xldv20TrapIds,
                   xldv20AlmSeverity   Xldv20AlarmSeverity }

     xldv20AlmSevTrapId OBJECT-TYPE
        -- info-VALUE:   trHpExcBER ( 1 ),
        -- info-VALUE:   trHpUNEQ( 2 ), trLcd( 3 ),
        -- info-VALUE:   trLof( 4 ),trLop( 5 ),
        -- info-VALUE:   trLos( 6 ),trMsAIS( 7 ),
        -- info-VALUE:   trMsExcBER( 8 ),trMsRDI( 9 ),
        -- info-VALUE:   trMsSD( 10 ),trPAIS( 11 ),
        -- info-VALUE:   trPRDI( 12 ),trRsExcBER( 13 ),
        -- info-VALUE:   trSlm( 14 ), trTim( 15 ),
        -- info-VALUE:   trActFault( 16 ),
        -- info-VALUE:   trReplaceableUnitTypeMismatch ( 17 ),
        -- info-VALUE:   trReplaceableUnitFailure      ( 18 ),
        -- info-VALUE:   trReplaceableUnitProblem      ( 19 ),
        -- info-VALUE:   trReplaceableUnitNotInstalled ( 20 ),
        -- info-VALUE:   trReplaceableUnitSwMismatch   ( 21 ),
        -- info-VALUE:   trXmissionErr( 105 ),
        -- info-VALUE:   trReplaceableUnitRemoved( 113 ),
        -- info-VALUE:   trAIS( 114 ),trRDI( 115 ),
        -- info-VALUE:   trPlcpLof( 116 ),trPlcpRDI( 117 ),
        -- info-VALUE:   trReplaceableUnitSwMissing( 122 )
        -- info-VALUE:   trInternalFault( 123 ),
        -- info-VALUE:   trAttributeValueChange( 124 ),
        -- info-VALUE:   trImmMNR( 130 ), trLpr( 131 ),
        -- info-VALUE:   trLol( 132 ),
        -- info-VALUE:   trAtmLayerMultiAlarm( 135 ),
        -- info-VALUE:   trWrongServiceConfigData( 141 ),
        -- info-VALUE:   trLOC( 145 ), 
        -- info-VALUE:   Trap ID's for LUIMA :
        -- info-VALUE:   E1 near-end :
        -- info-VALUE:   trExcBER( 407 ), trLif( 408 ),
        -- info-VALUE:   trLods( 409 ),
        -- info-VALUE:   E1 far-end :
        -- info-VALUE:   trTxUnusableFe( 410 ),
        -- info-VALUE:   trRxUnusableFe( 411 ),
        -- info-VALUE:   trRfiIma( 412 ),
        -- info-VALUE:   trRAI( 413 ),
        -- info-VALUE:   IMA group  :
        -- info-VALUE:   trStartUp( 450 ),trStartUpFe( 451 ),
        -- info-VALUE:   trConfigAbort( 452 ),
        -- info-VALUE:   trConfigAbortFe( 453 ),
        -- info-VALUE:   trInsufficientLinks( 454 ),
        -- info-VALUE:   trInsufficientLinksFe( 455 ),
        -- info-VALUE:   trBlockedFe( 456 ),
        -- info-VALUE:   trGrTimingMismatch( 457 ),
        -- info-ROU:     GET db
        -- info-IMPL:    V2.0->          
        -- info-VERCTRL: -
        SYNTAX        Xldv20TrapIds
        ACCESS        read-only
        STATUS        mandatory
        DESCRIPTION
             " The trap ID of the alarm trap type this entry applies to.
               NOTE : the severity of xldv20TrAtmLayerMultiAlarm 
               is not configurable. "
        ::= { xldv20AlarmSeverityEntry 1 }

     xldv20AlmSeverity OBJECT-TYPE
        -- info-VALUE:   critical(1), major(2)
        -- info-VALUE:   minor(3), warning(4), noAlarming(5)
        -- info-ROU:     GET db SET db
        -- info-IMPL:    V2.0->          
        -- info-VERCTRL: -
        SYNTAX        Xldv20AlarmSeverity
        ACCESS        read-write
        STATUS        mandatory
        DESCRIPTION
             " The severity to be used for this trap type when the
               entry is selected. "
        ::= { xldv20AlarmSeverityEntry 2 }

     -- --------------------------------------------------------------
     --                Alarm Filtering Profile Table
     -- --------------------------------------------------------------

     xldv20AlarmFilteringProfileTable OBJECT-TYPE
        -- info-IMPL:    V2.0->          
        SYNTAX        SEQUENCE OF Xldv20AlarmFilteringProfileEntry
        ACCESS        not-accessible
        STATUS        mandatory
        DESCRIPTION
             " The table of alarm filtering profiles.
             Creating or deleting an entry in this table automatically creates
             or deletes the corresponding entries in the
             xldv20AlarmFilteringTable. "
        --DEFAULT next-function-async     fsnAlmFiltProfileEntryNext
        --DEFAULT get-function-async      fsnAlmFiltProfileEntryGet
        ::= { xldv20Alarming 4 }

     xldv20AlarmFilteringProfileEntry OBJECT-TYPE
        -- info-IMPL:    V2.0->          
        SYNTAX        Xldv20AlarmFilteringProfileEntry
        ACCESS        not-accessible
        STATUS        mandatory
        DESCRIPTION
             " An entry of the alarm filtering profile table. "
        INDEX         { xldv20AlmFiltProfileIndex }
        ::= { xldv20AlarmFilteringProfileTable 1 }

     Xldv20AlarmFilteringProfileEntry ::=
        SEQUENCE { xldv20AlmFiltProfileIndex      Xldv20AlmFiltProfileIndex,
                   xldv20AlmFiltProfileRowStatus  Xldv20RowStatus }

     xldv20AlmFiltProfileIndex OBJECT-TYPE
        -- info-VALUE:   1..40 (default 1)
        -- info-ROU:     GET db
        -- info-IMPL:    V2.0->          
        -- info-VERCTRL: -
        SYNTAX        Xldv20AlmFiltProfileIndex
        ACCESS        read-only 
        STATUS        mandatory
        DESCRIPTION
             " A number identifying this alarm filtering profile. "
        ::= { xldv20AlarmFilteringProfileEntry 1 }

     xldv20AlmFiltProfileRowStatus OBJECT-TYPE
        -- info-VALUE:   create(1), delete(2)
        -- info-ROU:     GET db
        -- info-IMPL:    V2.0->          
        -- info-VERCTRL: -
        SYNTAX        Xldv20RowStatus
        ACCESS        read-only
        STATUS        mandatory
        DESCRIPTION
             " This object is used to create a new row or to delete
               an existing row in the table.
               Default value: xldv20AlmFiltProfileIndex = 1: created
                              xldv20AlmFiltProfileIndex = 2 .. 300: deleted . "
        ::= { xldv20AlarmFilteringProfileEntry 2 }

     -- --------------------------------------------------------------
     --                   Alarm Filtering Table 
     -- --------------------------------------------------------------

     xldv20AlarmFilteringTable OBJECT-TYPE
        -- info-IMPL:    V2.0->          
        SYNTAX        SEQUENCE OF Xldv20AlarmFilteringEntry
        ACCESS        not-accessible
        STATUS        mandatory
        DESCRIPTION
             " The alarm filtering table is used for
               temporairy filtering of alarms. "
        --DEFAULT next-function-async     fsnAlmFiltEntryNext
        --DEFAULT get-function-async      fsnAlmFiltEntryGet
        ::= { xldv20Alarming 5 }

     xldv20AlarmFilteringEntry OBJECT-TYPE
        -- info-IMPL:    V2.0->          
        SYNTAX        Xldv20AlarmFilteringEntry
        ACCESS        not-accessible
        STATUS        mandatory
        DESCRIPTION
             " An entry in this table associates a trap id/ temporal
               filter parameter with a xldv20AlmFiltProfileIndex. "
        INDEX   { xldv20AlmFiltProfileIndex,
                  xldv20AlmFiltTrapId }
        ::= { xldv20AlarmFilteringTable 1 }

     Xldv20AlarmFilteringEntry ::=
        SEQUENCE { xldv20AlmFiltTrapId   Xldv20TrapIds,
                   xldv20AlmTempFilter   INTEGER }

     xldv20AlmFiltTrapId OBJECT-TYPE
        -- info-VALUE:   see xldv20AlmSevTrapId
        -- info-ROU:     GET db
        -- info-IMPL:    V2.0->          
        -- info-VERCTRL: -
        SYNTAX        Xldv20TrapIds
        ACCESS        read-only 
        STATUS        mandatory
        DESCRIPTION
             " The ID of the trap type this entry applies to. "
        ::= { xldv20AlarmFilteringEntry 1 }

     xldv20AlmTempFilter OBJECT-TYPE
        -- info-VALUE:   1..86400
        -- info-ROU:     GET db
        -- info-IMPL:    V2.0->          
        -- info-VERCTRL: -
        SYNTAX        INTEGER (0..86400)
        ACCESS        read-only
        STATUS        mandatory
        DESCRIPTION
             " The temporal filtering period in seconds.
               The value 0 means no filtering. "
        ::= { xldv20AlarmFilteringEntry 2 }

     -- --------------------------------------------------------------
     --                      Alarm List Table
     -- --------------------------------------------------------------

     xldv20AlarmListTable OBJECT-TYPE
        -- info-IMPL:    V2.0->          
        SYNTAX        SEQUENCE OF Xldv20AlarmListEntry
        ACCESS        not-accessible
        STATUS        mandatory
        DESCRIPTION
             " The alarm list table is used to store all reported alarms.  "
        --DEFAULT next-function-async     fsnAlmListEntryNext
        --DEFAULT get-function-async      fsnAlmListEntryGet
        ::= { xldv20Alarming 6 }

     xldv20AlarmListEntry OBJECT-TYPE
        -- info-IMPL:    V2.0->          
        SYNTAX        Xldv20AlarmListEntry
        ACCESS        not-accessible
        STATUS        mandatory
        DESCRIPTION
             " Entry of the alarm list table.
               Notifications : none. "
        INDEX         { xldv20AlmIndex }
        ::= { xldv20AlarmListTable 1 }

     Xldv20AlarmListEntry ::=
        SEQUENCE { xldv20AlmIndex               INTEGER,
                   xldv20AlmNatureOfAlarm       Xldv20TrapIds,
                   xldv20AlmSpecProblem         INTEGER,
                   xldv20AlmSpecProblemText     OCTET STRING,
                   xldv20AlmRepEntityId         Integer32,
                   xldv20AlmRepSource           Xldv20RepSource,
                   xldv20AlmFailedComponent     Integer32,
                   xldv20AlmFailedComponentRepSource  Xldv20RepSource,
                   xldv20AlmSeverityOfFailure   Xldv20AlarmSeverity,
                   xldv20AlmPropRepairAction    Integer32,
                   xldv20AlmTimeAndDate         Integer32 }

     xldv20AlmIndex OBJECT-TYPE
        -- info-VALUE:   1..700
        -- info-ROU:     GET db
        -- info-IMPL:    V2.0->          
        -- info-VERCTRL: -
        SYNTAX        INTEGER (1..700)
        ACCESS        read-only
        STATUS        mandatory
        DESCRIPTION
             " Unique value to identify an entry in the alarm list when the
               entry is read or cleared, when the entry is created. "
        ::= { xldv20AlarmListEntry 1 }

     xldv20AlmNatureOfAlarm OBJECT-TYPE
        -- info-VALUE:   see xldv20AlmSevTrapId
        -- info-ROU:     GET db
        -- info-IMPL:    V2.0->          
        -- info-VERCTRL: -
        SYNTAX        Xldv20TrapIds
        ACCESS        read-only
        STATUS        mandatory
        DESCRIPTION
             " Generic trouble description of an alarm (the trap ID). "
        ::= { xldv20AlarmListEntry 2 }

     xldv20AlmSpecProblem OBJECT-TYPE
        -- info-VALUE:   0..255 
        -- info-ROU:     GET db
        -- info-IMPL:    V2.0->          
        -- info-VERCTRL: -
