        --DEFAULT set-function-async      fsnRstSet
        --DEFAULT get-function-async      fsnRstGet
        ::= { xldv20Reset 4 }

     xldv20RstControlTimer OBJECT-TYPE
        -- info-VALUE:   0..2147483647
        -- info-ROU:     GET db
        -- info-IMPL:    V2.0->          
        -- info-VERCTRL: -
        SYNTAX        Unsigned32
        ACCESS        read-only
        STATUS        mandatory
