             " Indicates whether or not the external alarm is
               capable of performing its normal functions. "
        ::= { xldv20ExternalAlarmEntry 3 }

     xldv20ExtAlarmActivityState OBJECT-TYPE
        -- info-VALUE:   undeterminate(0), on(1), off(2)
        -- info-ROU:     GET db
        -- info-IMPL:    V2.0->          
        -- info-VERCTRL: -
        SYNTAX        Xldv20ExtAlarmActivityState
        ACCESS        read-only
        STATUS        mandatory
        DESCRIPTION
             " The status of the external alarm.
	     Only if the operational state is enabled
               and the alarm state is on, an external alarm is generated
               for this object. "
        ::= { xldv20ExternalAlarmEntry 4 }

     xldv20ExtAlarmSeverityIndex OBJECT-TYPE
        -- info-VALUE:   1..40 (default is 1)
        -- info-ROU:     GET db  SET db
        -- info-IMPL:    V2.0->
        -- info-VERCTRL: -
        SYNTAX        Xldv20AlmSevProfileIndex
        ACCESS        read-write
        STATUS        mandatory
        DESCRIPTION
             " The index of the associated entry in the alarm severity
               profile table. These entries contain the severity
               assigments for the alarms reported by this managed
               entity. "
        ::= { xldv20ExternalAlarmEntry 5 }

     xldv20ExtAlarmFilteringIndex OBJECT-TYPE
        -- info-VALUE:   1..40
        -- info-ROU:     GET db
        -- info-IMPL:    V2.x
        -- info-VERCTRL: -
        SYNTAX        Xldv20AlmFiltProfileIndex
        ACCESS        read-only
        STATUS        mandatory
        DESCRIPTION
             " It is a pointer to the instance of the alarm filtering
               assignment profile managed entity that contains the filtering
               assignments for the alarms reported by this managed entity. "
        ::= { xldv20ExternalAlarmEntry 6 }

        xldv20ExtAlarmName OBJECT-TYPE
        -- info-VALUE:   -
        -- info-ROU:     GET db SET db
        -- info-IMPL:    V2.0->
        -- info-VERCTRL: -
        SYNTAX        OCTET STRING ( SIZE (0..50) )
        ACCESS        read-write
        STATUS        mandatory
        DESCRIPTION
             " Describe the input line and the type of external alarm.
               Default values on startup are :
               index=1 : Fan 1 Fail
               index=2 : Fan 2 Fail
               index=3 : High Temperature. "
        ::= { xldv20ExternalAlarmEntry 7 }

     -- --------------------------------------------------------------
     --               Alarm Severity Profile Table
     -- --------------------------------------------------------------

     xldv20AlarmSeverityProfileTable OBJECT-TYPE
        -- info-IMPL:    V2.0->          
        -- info-VERCTRL: -
