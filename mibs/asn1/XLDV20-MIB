     -- --------------------------------------------------------------
     --                      SW Upgrade Task Table
     -- --------------------------------------------------------------
       
     xldv20SwUpgradeTaskTable OBJECT-TYPE 
        -- info-IMPL:    V2.0->
        SYNTAX        SEQUENCE OF Xldv20SutTaskTableEntry
        ACCESS        not-accessible
        STATUS        mandatory
        DESCRIPTION
             " Table of the current active or failed tasks of SW upgrade.
               Entries for successfull loading tasks are removed
               from the table.
               This table is used to display the upgrade status for
               single plugin units at the TMN. " 
        --DEFAULT next-function-async     fsnSutEntryNext
        --DEFAULT get-function-async      fsnSutEntryGet
        ::= { xldv20Swm 3 } 
 
     xldv20SutTaskTableEntry OBJECT-TYPE
        -- info-IMPL:    V2.0->
        SYNTAX        Xldv20SutTaskTableEntry 
        ACCESS        not-accessible
        STATUS        mandatory
        DESCRIPTION
             " Entry of the SW upgrade task table.
               Notifications : none. "
        INDEX         { xldv20SutIndex }
        ::= { xldv20SwUpgradeTaskTable 1 }
 
     Xldv20SutTaskTableEntry ::=
        SEQUENCE { xldv20SutIndex               Unsigned16,
                   xldv20SutPiuType             OCTET STRING,
                   xldv20SutHwUnitIndex         Unsigned16,
                   xldv20SutSwVersion           OCTET STRING,
                   xldv20SutRequester           Xldv20Requester,
                   xldv20SutUpgradeState        Xldv20ControlReq,
                   xldv20SutUpgradeResult       Xldv20ControlReq,
		   xldv20SutTimeStamp           TimeTicks
                   }

     xldv20SutIndex OBJECT-TYPE
        -- info-VALUE:   -
        -- info-ROU:     GET db
        -- info-IMPL:    V2.0->
        -- info-VERCTRL: -
        SYNTAX        Unsigned16
        ACCESS        read-only
        STATUS        mandatory
        DESCRIPTION
             " Unique value to identify an entry of this table. "
        ::= { xldv20SutTaskTableEntry 1 }
 
     xldv20SutPiuType OBJECT-TYPE
        -- info-VALUE:   -
        -- info-ROU:     GET db
        -- info-IMPL:    V1.3N->
        -- info-VERCTRL: -
        SYNTAX        OCTET STRING ( SIZE (0..20) )
        ACCESS        read-only
        STATUS        mandatory
        DESCRIPTION
         " Indicates the plug-in unit type to be upgraded. "
        ::= { xldv20SutTaskTableEntry 2 }
 
     xldv20SutHwUnitIndex OBJECT-TYPE
        -- info-VALUE:   -
        -- info-ROU:     GET db
        -- info-IMPL:    V2.0->
        -- info-VERCTRL: -
        SYNTAX        Unsigned16
        ACCESS        read-only
        STATUS        mandatory
        DESCRIPTION
         " The HwUnitIndex of PIU to be loaded.
           From this value the TMN calculates the location shelf/slot
           of the plug-in unit and displays it at the GUI. "
        ::= { xldv20SutTaskTableEntry 3 }
 
    xldv20SutSwVersion OBJECT-TYPE
       -- info-VALUE:   -
       -- info-ROU:     GET db
       -- info-IMPL:    V2.0->
       -- info-VERCTRL: -
       SYNTAX        OCTET STRING ( SIZE (0..10) )
       ACCESS        read-only
       STATUS        mandatory
       DESCRIPTION
         " String containing the APS version which is loaded
           into the plug-in unit . "
        ::= { xldv20SutTaskTableEntry 4 }
 
     xldv20SutRequester OBJECT-TYPE
        -- info-VALUE:   tmn, hwm
        -- info-ROU:     GET db
        -- info-IMPL:    V2.0->
        -- info-VERCTRL: -
        SYNTAX        Xldv20Requester
        ACCESS        read-only
        STATUS        mandatory
        DESCRIPTION
             " Identifies the requesting unit of
               the current upgrade task. "
        ::= { xldv20SutTaskTableEntry 5 }
 
     xldv20SutUpgradeState OBJECT-TYPE
        -- info-VALUE:   sutInitialized, sutToDo
        -- info-VALUE:   sutStarted, sutCrashed, sutFtpDone
        -- info-VALUE:   sutDone, sutReadyForReset
        -- info-VALUE:   sutObsolete
        -- info-ROU:     GET db
        -- info-IMPL:    V2.0->
        -- info-VERCTRL: -
        SYNTAX        Xldv20ControlReq
        ACCESS        read-only
        STATUS        mandatory
        DESCRIPTION
             " Identifies the current processing state of
               the upgrade task. "
        ::= { xldv20SutTaskTableEntry 6 }
 
    xldv20SutUpgradeResult OBJECT-TYPE
        -- info-VALUE:   commExecStarted sucUpgrStartRequest,
        -- info-VALUE:   sucActivateLoad, sucReadSAPSContentFile,
        -- info-VALUE:   sucGetSwVersions, sucUpgrMissParameter,
        -- info-VALUE:   sucGetSwVersionsFailed
        -- info-VALUE:   sucOk ,sucNotOk
        -- info-VALUE:   sucDbAccessFailed ,sucPiuUnlocked
        -- info-VALUE:   sucTransmissionError ,sucWrongSwVersion
        -- info-VALUE:   sucUnitTypeMismatch
        -- info-VALUE:   sucFtpTransError ,sucFtpCrcError
        -- info-VALUE:   sucSwHwMismatch ,sucUpgrWrongAvState
        -- info-VALUE:   sucFepromCopyFault ,sucUpdateDescFileFailed
        -- info-VALUE:   sucFtpServerNoteAvailable ,sucFtpSendError
        -- info-VALUE:   sucDbBackupFault ,sucUpgradeComplete
        -- info-VALUE:   sucUpgradeNotComplete
        -- info-VALUE:   sucUpgrTaskCreationFault
        -- info-VALUE:   sucUpgrDbAccessFault
        -- info-ROU:     GET db
        -- info-IMPL:    V2.0->
        -- info-VERCTRL: -
        SYNTAX        Xldv20ControlReq
        ACCESS        read-only
        STATUS        mandatory
        DESCRIPTION
          " This read-only attribute informs about the result of
            the performes upgrade (in case of failed upgrade
            the error reason). "
        ::= { xldv20SutTaskTableEntry 7 }
 
  xldv20SutTimeStamp OBJECT-TYPE
     -- info-VALUE:   0..2Giga
     -- info-ROU:     GET db
     -- info-IMPL:    V2.0->
     -- info-VERCTRL: -
     SYNTAX        TimeTicks
     ACCESS        read-only
     STATUS        mandatory
     DESCRIPTION
        " The time stamp of the task. It is updated anytime the
        upgrade task state changes. "
        ::= { xldv20SutTaskTableEntry 8 }
 
 -- --------------------------------------------------------------
 --               Secure Telnet control subgroup
 -- --------------------------------------------------------------
 
 -- This managed entity represents the control group for Secure Telnet.
 -- The secure telnet session will be enabled / disabled via a SET
 -- request to the attribute xldv20StcTelnetAccess.
 
 
     xldv20StcSecureTelnetControl OBJECT IDENTIFIER 
        -- info-IMPL:    V2.x->          
     ::= { xldv20Swm 4 }
 
     xldv20StcTelnetAccess OBJECT-TYPE
        -- info-VALUE:   read(1), trace(2),
        -- info-VALUE:   write(3), noAccess(4)
        -- info-ROU:     GET db  SET st
        -- info-IMPL:    V2.x->          
        -- info-VERCTRL: SET blocked
        SYNTAX        Xldv20TelnetAccess
        ACCESS        read-write
        STATUS        mandatory
        DESCRIPTION
             " Feature not supported. The access mode of a telnet session.
             The setting of values read, trace, write will enable 
             a secure telnet session with the corresponding access right.
             The setting of value noAccess will disable a telnet session. "
        --DEFAULT test-function-async     fsnStcTest
        --DEFAULT next-function-async     fsnStcNext
        --DEFAULT set-function-async      fsnStcSet
        --DEFAULT get-function-async      fsnStcGet
        ::= { xldv20StcSecureTelnetControl 1 }

     -- ==============================================================
     --       D a t a B a s e   M a n a g e m e n t   G r o u p
     -- ==============================================================

     -- --------------------------------------------------------------
     --             DataBase Management control subgroup
     -- --------------------------------------------------------------

     xldv20DbmControl OBJECT IDENTIFIER 
        -- info-IMPL:    V2.0->          
     ::= { xldv20Dbm 1 }

     -- This managed entity manages the control request for storage
     -- of the database.

     xldv20DbmControlReq OBJECT-TYPE
        -- info-VALUE:   backupDb
        -- info-ROU:     GET db SET dm
        -- info-IMPL:    V2.0->          
        -- info-VERCTRL: -
        SYNTAX        Xldv20ControlReq
        ACCESS        read-write
        STATUS        mandatory
        DESCRIPTION
             " The command to store the database
               (backupDb) into the FEPROM of the DSLAM.
               The return value commExecStarted indicates the beginning
               of DB storage.
               Notifications : xldv20TrDbBackup indicates the end and 
               the result of the database storage. "
        --DEFAULT set-function-async      fsnDbmSet
        --DEFAULT next-function-async     fsnDbmNext
        --DEFAULT test-function-async     fsnDbmTest
        --DEFAULT get-function-async      fsnDbmGet
        ::= { xldv20DbmControl 1 }

     xldv20DbmControlStatus OBJECT-TYPE
        -- info-VALUE:   requestIdle(1), requestBusy(2), requestPassed(3)
        -- info-VALUE:   requestFailed(4)
        -- info-ROU:     GET db
        -- info-IMPL:    V2.0->          
        -- info-VERCTRL: -
        SYNTAX        Xldv20ControlStatus
        ACCESS        read-only
        STATUS        mandatory
        DESCRIPTION
             " The status of the control request. "
        --DEFAULT next-function-async     fsnDbmNext
        --DEFAULT get-function-async      fsnDbmGet
        ::= { xldv20DbmControl 2 }

     xldv20DbmControlTimer OBJECT-TYPE
        -- info-VALUE:   0..2147483647
        -- info-ROU:     GET db
        -- info-IMPL:    V2.0->          
        -- info-VERCTRL: -
        SYNTAX        Unsigned32
        ACCESS        read-only
        STATUS        mandatory
        DESCRIPTION
              " The maximum wait time for the manager when a DB storage
                request cannot be executed immediately (value commExecStarted
                in the result attribute xldv20DbmControlReqResult).
                The manager reads this value from the DSLAM.
                The end of request indicates the trap xldv20TrDbBackup. "
        --DEFAULT next-function-async     fsnDbmNext
        --DEFAULT get-function-async      fsnDbmGet
        ::= { xldv20DbmControl 3 }

     xldv20DbmBackupPeriod OBJECT-TYPE
        -- info-VALUE:   12..48 (unit = hour)
        -- info-VALUE:   default = 24
        -- info-ROU:     GET db SET dm
        -- info-IMPL:    V2.0->          
        -- info-VERCTRL: -
        SYNTAX        INTEGER (12..48)
            ACCESS        read-write
            STATUS        mandatory
            DESCRIPTION
                  " Interval between two backup operations in hours. "
            --DEFAULT test-function-async     fsnDbmTest
            --DEFAULT next-function-async     fsnDbmNext
            --DEFAULT set-function-async      fsnDbmSet
            --DEFAULT get-function-async      fsnDbmGet
            ::= { xldv20DbmControl 4 }

     xldv20DbmBackupStartTime OBJECT-TYPE
        -- info-VALUE:   0..86400
        -- info-ROU:     GET db SET db
        -- info-IMPL:    V2.0->          
        -- info-VERCTRL: -
            SYNTAX        INTEGER (0..86400)
            ACCESS        read-write
            STATUS        mandatory
            DESCRIPTION
                  " Start time of next DB backup in seconds. "
