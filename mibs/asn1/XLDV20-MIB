        -- info-VALUE:   dBm-20     (-20),
        -- info-VALUE:   dBm-19     (-19),
        -- info-VALUE:   dBm-18     (-18),
        -- info-VALUE:   dBm-17     (-17),
        -- info-VALUE:   dBm-16     (-16),
        -- info-VALUE:   dBm-15     (-15),
        -- info-VALUE:   dBm-14     (-14),
        -- info-VALUE:   dBm-13     (-13),
