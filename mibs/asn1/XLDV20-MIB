         -- --------------------------------------------------------------
         --            ATM Cross Connect Table for VPL Cross Connection
         -- -------------------------------------------------------------

         xldv20AtmCrossConnectTable OBJECT-TYPE
            -- info-IMPL:    V2.0->
            -- info-VERCTRL: -
            SYNTAX        SEQUENCE OF Xldv20AtmCrossConnectEntry
            ACCESS        not-accessible
            STATUS        mandatory
            DESCRIPTION
                 " The ATM VP cross connect table.
                   This managed entity is used to represent
                   the cross-connect relationship between two Vpl termination
                   points.
                   Instances of this managed entity shall be automatically
                   created and deleted during ATM Connect and ATM Disconnect
		   actions. "
            --DEFAULT test-function-async     fsnAccEntryTest
            --DEFAULT next-function-async     fsnAccEntryNext
            --DEFAULT set-function-async      fsnAccEntrySet
            --DEFAULT get-function-async      fsnAccEntryGet
            ::= { xldv20CallP 2 }

         xldv20AtmCrossConnectEntry OBJECT-TYPE
            -- info-IMPL:    V2.0->
            -- info-VERCTRL: -
            SYNTAX        Xldv20AtmCrossConnectEntry
            ACCESS        not-accessible
            STATUS        mandatory
            DESCRIPTION
                 " An entry in the ATM cross connect table.  "
            INDEX         { xldv20AccVpiNniIndex }
            ::= { xldv20AtmCrossConnectTable 1 }

         Xldv20AtmCrossConnectEntry ::=
            SEQUENCE { xldv20AccOperationalState   Xldv20OperState,
                       xldv20AccTerminationPointA  INTEGER,
                       xldv20AccTerminationPointZ  INTEGER,
                       xldv20AccLineIndex          Unsigned16,
                       xldv20AccEthVpcIndex        INTEGER,
                       xldv20AccEthVccIndex        INTEGER,
                       xldv20AccVpiNniIndex        INTEGER,
                       xldv20AccAlarmState         Xldv20CallpAlarmState,
                       xldv20AccAdminState         Xldv20AdminState,
                       xldv20AccAtmfVplIndex       INTEGER
                       }

         xldv20AccOperationalState OBJECT-TYPE
            -- info-VALUE:   enabled(1)..disabled(2)
            -- info-ROU:     GET db
            -- info-IMPL:    V2.0->
            -- info-VERCTRL: -
            SYNTAX        Xldv20OperState
            ACCESS        read-only
            STATUS        mandatory
            DESCRIPTION
                 " Identifies whether or not this instance of
                   the ATM Cross Connection managed entity is capable of
                   performing its normal function ( i.e. switching cells ).
                   Altering this attribute is without reporting this state
                   change via the trap 'xldv20TrStateChangeOper'. "
            ::= { xldv20AtmCrossConnectEntry 1 }

         xldv20AccTerminationPointA OBJECT-TYPE
            -- info-VALUE:   1..4096
            -- info-ROU:     GET db
            -- info-IMPL:    V2.0->
            -- info-VERCTRL: -
            SYNTAX        INTEGER (1..4096)
            ACCESS        read-only
            STATUS        mandatory
            DESCRIPTION
                 " The instance of the VPL termination point within the DSLAM at the LE side. "
            ::= { xldv20AtmCrossConnectEntry 2 }

         xldv20AccTerminationPointZ OBJECT-TYPE
            -- info-VALUE:   1..4096
            -- info-ROU:     GET db
            -- info-IMPL:    V2.0->
            -- info-VERCTRL: -
            SYNTAX        INTEGER (1..4096)
            ACCESS        read-only
            STATUS        mandatory
            DESCRIPTION
                 " The instance of the VPL termination point within the DSLAM 
		 at the NT side. "
            ::= { xldv20AtmCrossConnectEntry 3 }

         xldv20AccLineIndex OBJECT-TYPE
            -- info-VALUE:   0..65535
            -- info-ROU:     GET db
            -- info-IMPL:    V2.0->
            -- info-VERCTRL: -
            SYNTAX        Unsigned16
            ACCESS        read-only
            STATUS        mandatory
            DESCRIPTION
                 " The external line index. The TLM interfaces (see IFTable)
		 with same line index blong to the same line. "
            ::= { xldv20AtmCrossConnectEntry 4 }

         xldv20AccEthVpcIndex OBJECT-TYPE
            -- info-VALUE:   0..2000
            -- info-ROU:     GET db
            -- info-IMPL:    V2.0->
            -- info-VERCTRL: -
            SYNTAX        INTEGER (0..2000)
            ACCESS        read-only
            STATUS        mandatory
            DESCRIPTION
                 " For cross connects at the LIETH a pointer to the
                   belonging VpcTp. The value is the entry number in the
                   xldv20VpcTpTable.
                   For all other cross connects the value is always 0. "
            ::= { xldv20AtmCrossConnectEntry 5 }

         xldv20AccEthVccIndex OBJECT-TYPE
            -- info-VALUE:   0..1000
            -- info-ROU:     GET db
            -- info-IMPL:    V2.0->
            -- info-VERCTRL: -
            SYNTAX        INTEGER (0..1000)
            ACCESS        read-only
            STATUS        mandatory
            DESCRIPTION
                 " For cross connects at the LIETH a pointer to the
                   belonging VccTp. The value is the entry number in the
                   xldv20VccTpTable.
                   For all other cross connects the value is always 0. "
            ::= { xldv20AtmCrossConnectEntry 6 }
     
         xldv20AccVpiNniIndex OBJECT-TYPE
            -- info-VALUE:   1..2000
            -- info-ROU:     GET db
            -- info-IMPL:    V2.0->
            -- info-VERCTRL: -
            SYNTAX        INTEGER (1..2000)
            ACCESS        read-only
            STATUS        mandatory
            DESCRIPTION
                 " Index of this table. "
            ::= { xldv20AtmCrossConnectEntry 7 }

         xldv20AccAlarmState OBJECT-TYPE
            -- info-VALUE:   noAlarm(1), alarmPresent(2)
            -- info-ROU:     GET db
            -- info-IMPL:    V2.0->
            -- info-VERCTRL: -
            SYNTAX        Xldv20CallpAlarmState
            ACCESS        read-only
            STATUS        mandatory
            DESCRIPTION
                 " Indicates whether for the cross connect
                   an alarm is present or not. "
            ::= { xldv20AtmCrossConnectEntry 8 }

         xldv20AccAdminState OBJECT-TYPE
            -- info-VALUE:   unlocked(1), locked(2)
            -- info-ROU:     GET db  SET cp 
            -- info-IMPL:    V2.0->
            -- info-VERCTRL: -
            SYNTAX        Xldv20AdminState
            ACCESS        read-write
            STATUS        mandatory
            DESCRIPTION
                 " This attribute is used to activate (unlock) and deactivate
                   (lock) the VPL cross connection. The cross connect is
                   removed within the ATM ASIC, the configuration data of the
                   cross connect are still available within the DSLAM . "
            ::= { xldv20AtmCrossConnectEntry 9 }

         xldv20AccAtmfVplIndex OBJECT-TYPE
            -- info-VALUE:   0..4000
            -- info-ROU:     GET db
            -- info-IMPL:    V2.0->
            -- info-VERCTRL: -
            SYNTAX        INTEGER (0..4000)
            ACCESS        read-only
            STATUS        mandatory
            DESCRIPTION
                 " For cross connects at the ATMF port of NT this value
                 identifies the belonging VplTp . The value is the 
                 entry number in the xldv20VplTpTable.
                 For all other cross connects the value is always 0. "
            ::= { xldv20AtmCrossConnectEntry 10 }


         -- --------------------------------------------------------------
         --                        VPL TP Table
         -- --------------------------------------------------------------

         xldv20VplTpTable OBJECT-TYPE
            -- info-IMPL:    V2.0->
            -- info-VERCTRL: -
            SYNTAX        SEQUENCE OF Xldv20VplTpEntry
            ACCESS        not-accessible
            STATUS        mandatory
            DESCRIPTION
                 " This table contains the entries for Vpl termination
                 points (VplTp). "
            --DEFAULT test-function-async     fsnVplEntryTest
