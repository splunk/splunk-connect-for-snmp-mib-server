        ACCESS        read-only
        STATUS        mandatory
        DESCRIPTION
             " Description of further information about an alarm in text form. "
        ::= { xldv20AlarmListEntry 4 }

     xldv20AlmRepEntityId OBJECT-TYPE
        -- info-VALUE:   -
        -- info-ROU:     GET db
        -- info-IMPL:    V2.0->          
        -- info-VERCTRL: -
        SYNTAX        Integer32
        ACCESS        read-only
        STATUS        mandatory
        DESCRIPTION
             " The managed entity that reported the alarm. In case of a HWM /
               TLM / COH / GFH object it is the HW unit index / interface
               index / vpcTP or vccTp index /  External Alarm index. "
        ::= { xldv20AlarmListEntry 5 }

     xldv20AlmRepSource OBJECT-TYPE
        -- info-VALUE:   hwm, tlm, vplNni, vplUni
        -- info-VALUE:   extAlarm, vccTp, vpcTp
        -- info-VALUE:   line, vclTp, vplTp
        -- info-VALUE:   vclNni, vclUni
        -- info-ROU:     GET db
        -- info-IMPL:    V2.0->          
