             -- --------------------------------------------------------------
             -- **************************************************************
             -- **************************************************************


             -- **************************************************************
             --             Information Block for Version Control
             --             of MIB nodes and attributes (template)
             -- **************************************************************
             --
             -- info-TRAPTYPE:  alarm/event            ==>> for traps only
             -- info-VALUE:     minValue..maxValue; attrValue[(V2.0->)]
             -- info-ROU:       GET  SET [db,gf,up,tm,hm,ut,so,sn,cp]
             -- info-FAILEDSRC: hwm, tlm, callp       ==>> for alarm traps only
             -- info-FAILEDOBJ: [hwm] LU155,LUDSX3,LUPON,CI ,SUAD,
             -- info-FAILEDOBJ: [hwm] LIETH,LI25,NTAD,
             -- info-FAILEDOBJ: [tlm] S1n(= STM-1 NePPTP),S1f,S3n,E3n,D3n,S3f,
             -- info-FAILEDOBJ: [tlm] D3f,E3f,Pon,Adc(= Adc-PPTP),Adr,Vdc,Vdr,
             -- info-FAILEDOBJ: [tlm] Ibm(= Ibm-PPTP),Eth,Tca  
             -- info-FAILEDOBJ: [callp] VpcTP, VccTP  
             -- info-IMPL:      [V2.0,V2.x]:[rw,ro,*]
             -- info-IMPL:      e.g.: V1.1U-> (= all vers. begin. with V1.1U)
             -- info-IMPL:      e.g.: V1.0->V1.1U V1.2B:rw->V1.3A V1.3B 
             -- info-VERCTRL:   [GET,SET [db,asw,agent,blocked] or "-" 
             --                             (blocked == blocked by SNMP agent)
             --                             ("-"     == no special handling)
             -- **************************************************************



             xldv20 OBJECT IDENTIFIER
                -- info-IMPL:    V2.0->
                -- info-VERCTRL: -
             ::= { onu 4 }

             -- **************************************************************
             --             TEXTUAL-CONVENTIONs for XLDV20-MIB
             --
             -- use of textual convention syntax necessary for the exported
             -- types Xldv20ControlStatus, Xldv20AdminState, Unsigned16 and
             -- Xldv20FlowDirection in order to create the file snmpinfo.dat
             -- For load production the textual convention definitions are
             -- hidden by comment signs.
             --
             -- ATTENTION  use TCON: after comment sign for mosy tools
             --            to prepare the snmpinfo.dat file           

             -- **************************************************************
             --
             -- --------------------------------------------------------------
             --                            States
             -- --------------------------------------------------------------

             -- The control status of an entity.
             --
             --TCON: Xldv20ControlStatus ::= TEXTUAL-CONVENTION
             --TCON:      STATUS current
             --TCON:      DESCRIPTION
             --TCON:        "This data type is used as the syntax for the control status 
             --TCON:         attributes."
             --TCON:      SYNTAX INTEGER {
             --TCON:        requestIdle(1),
             --TCON:        requestBusy(2),
             --TCON:        requestPassed(3),
             --TCON:        requestFailed(4)
             --TCON:      }

             Xldv20ControlStatus ::=
                 INTEGER {
                     requestIdle(1),
                     requestBusy(2),
                     requestPassed(3),
                     requestFailed(4)
                 }

             -- The operating state of an entity.
             --
             Xldv20OperState ::=
                 INTEGER {
                     enabled(1),
                     disabled(2)
                 }

             -- Indicates whether a slot is empty or not.
             --
             Xldv20SlotStatus ::=
                 INTEGER {
                     unknown(0),
                     used(1),
                     empty(2)
                 }

             -- The administrative state of an entity.
             --
             --TCON: Xldv20AdminState ::= TEXTUAL-CONVENTION
             --TCON:      STATUS current
             --TCON:      DESCRIPTION
             --TCON:        "This data type is used as the syntax for the admin state 
             --TCON:         attributes. "
             --TCON:      SYNTAX INTEGER {
             --TCON:        unlocked(1),
             --TCON:        locked(2)
             --TCON:      }

             Xldv20AdminState ::=
                 INTEGER {
                     unlocked(1),
                     locked(2)
                 }

             -- The availability status of a hardware unit
             --
             Xldv20AvailStatus ::=
                 INTEGER {
                         available(1),
                         failed(2),
                         degraded(3),
                         offline(4),
                         notInstalled(5),
                         dependency(7)
            -- only internal states
            --               noDependency(6),
            --               noRestart(8),
            --               warmRestart(9),
            --               coldRestart(10),
            --               reloadRestart(11),
            --               noUpgrade(12),
            --               upgrade(13)
                }

             -- The attribute reflects upon the internal states of the
             -- Central Office Modem.
             --
             Xldv20LinkState ::=
                 INTEGER {
                     quiet(0),
                     idle(1),
                     train(2),
                     active(3),
                     download(4),
                     remoteDownload(6),
                     sleepMode(7),
                     unknown(255)
                 }

             -- The log read status.
             --
             Xldv20DbuReadStatus ::=
                 INTEGER {
                    dbuReadComplete                     (7081),
                    dbuReadFailed                       (7082),
                    dbuFileCreationFailed               (7083),
                    dbuFileTransmissionFailed           (7084)
                 }
     
             -- The access mode of a telnet session
             --
             Xldv20TelnetAccess ::=
                 INTEGER {
                     read(1),
                     trace(2),
                     write(3),
                     noAccess(4)
                 }

             -- The status of a telnet session
             --
             Xldv20TvTelnetSessionStatus ::=
                 INTEGER {
                     running(1),
                     stopped(2)
                 }

             -- The type of endpoint for continuity check
             --
             Xldv20EndpointType ::=
                 INTEGER { 
                     source  (1),
                     sink    (2),
                     both    (3)
                     }

             -- The type of reset result
             --
             Xldv20RstResult ::=
                 INTEGER { 
                     requestFailed (4)
                     }

             -- State of IMA group
             --
             Xldv20ImaGroupState ::=
                 INTEGER {
                   notConfigured(1),
                   startUp(2),
                   startUpAck(3),
                   configAbortUnsupported(4),
                   configAbortIncompatibleSymmetry(5),
                   configAbortOther(6),
                   insufficientLinks(7),
                   blocked(8),
                   operational(9),
                   configAbortUnsupportedImaVersion(10) }
 
              -- Failure Reason of an IMA group
              -- ATM Forum IMA v1.1, Section 10.2.1, page 55
              --
              Xldv20ImaGroupFailureStatus ::=
                  INTEGER {
                    noFailure(1), -- unit is up
                    startUpNe(2),
                    startUpFe(3),
                    invalidValueNe(4),
                    invalidValueFe(5),
                    failedAssymetricNe(6),
                    failedAssymetricFe(7),
                    insufficientLinksNe(8),
                    insufficientLinksFe(9),
                    blockedNe(10),
                    blockedFe(11),
                    otherFailure(12),
                    invalidImaVersionNe(13),
                    invalidImaVersionFe(14) }
 
              -- Failure Reason of an IMA group
              -- Indicate the transmit clock mode of the IMA group.
              -- There are two possible modes: the Common Transmit
              -- Clock (CTC) and the Independent Transmit Clock (ITC).
              -- The CTC mode corresponds to the case when the transmit clock
              -- of all IMA links are derived from the same source.  The ITC
              -- configuration corresponds to the case where there is at least
              -- one IMA link whose transmit clock is derived from a source
              -- different than at least another link transmit clock. "
              -- ATM Forum IMA v1.1, Section 7 on page 38
              --
              Xldv20ImaGroupTxClkMode  ::=
                  INTEGER {
                    ctc(1),
                    itc(2) }
 
              -- Group Symmetry of IMA group
              -- adjusted during group start-up
              -- ATM Forum IMA v1.1, Section 5.2.2.7 on page 35
              --
              Xldv20ImaGroupSymmetry ::=
                  INTEGER {
                    symmetricOperation(1),
                    asymmetricOperation(2),
                    asymmetricConfiguration(3) }
 
              -- Length of the IMA frames
              -- ATM Forum IMA v1.1, Section 5.2.2.4.2 on page 34
              --
              Xldv20ImaFrameLength ::=
                  INTEGER {
                    m32(32),
                    m64(64),
                    m128(128),
                    m256(256) }
 
              -- State of IMA link
              -- ATM Forum IMA v1.1, Section 10.1.2 on page 48
              --
              Xldv20ImaLinkState ::=
                  INTEGER {
                    notInGroup(1),
                    unusableNoGivenReason(2),
                    unusableFault(3),
                    unusableMisconnected(4),
                    unusableInhibited(5),
                    unusableFailed(6),
                    usable(7),
                    active(8) }
 
              -- Local Failure status of an IMA link
              -- ATM Forum IMA v1.1, Section 10.1.2 on page 48
              --
              Xldv20ImaLinkFailureStatus ::=
                  INTEGER {
                    noFailure(1),
                    imaLinkFailure(2),
                    lifFailure(3),
                    lodsFailure(4),
                    misConnected(5),
                    blocked(6),
                    fault(7),
                    farEndTxLinkUnusable(8),
                    farEndRxLinkUnusable(9) }


             -- --------------------------------------------------------------
             --                           Types
             -- --------------------------------------------------------------

             --
             --
             -- The textual convention like SNMPv2-TC. See rfc1903.
             -- In the Xld system we terminate all strings with '\0'.
             --
             --
             DisplayString ::= OCTET STRING (SIZE (0..255))




             -- The textual convention for a 32 bit integer index , 
              -- positive, beginning with value 1.
                      Unsigned16 ::= TEXTUAL-CONVENTION
                         STATUS current
                         DESCRIPTION
                           "Textual convention for a 16 bit integer value. "
                         SYNTAX INTEGER (0..65535)
 

             -- Following type as in IMA-MIB
             --
             MilliSeconds ::= Integer32

             -- The following types as in SNMPv2-SMIi
             -- (IMPORT makes problems during compilation)
             --

             -- A general hardware unit type identifier.
             --
             Xldv20HwUnitType ::=
                 INTEGER {
                     equipment(1),
                     equipmentHolder(2),
                     plugInUnit(3)
                 }

             -- The type of a TLM interface.
             --
             Xldv20IfType ::=
                 INTEGER {
                     s1n(1),         -- stm1nePPTP
                     s1f(2),         -- stm1fePPTP
                     adc(3),         -- adslcPPTP
                     adr(4),         -- adslrPPTP
                     ibm(5),         -- ibmPPTP
                     tca(6),         -- TC Adaptors
                     pon(7),         -- BPON           not supported
                     vdc(8),         -- vdslcPPTP      not supported
                     vdr(9),         -- vdslrPPTP      not supported
                     eth(10),        -- LIETH PPTP
                     d3n(11),        -- d3nePPTP
                     d3f(12),        -- d3fePPTP
                     e3n(13),        -- e3nePPTP
                     e3f(14),        -- e3fePPTP
                     s3n(15),        -- sts3nePPTP
                     s3f(16),        -- sts3fePPTP
                     e1nIma(20),     -- E1 near end
                     e1fIma(21),     -- E1 far end
                     d1nIma(22),     -- Ds1 near end   not supported
                     d1fIma(23),     -- Ds1 far end    not supported
                     sdc(24),        -- sdslcPPTP
                     sdr(25),        -- sdslrPPTP
                                     --
                     atmIma(107)     -- IMA group (value according
                                     -- AF-Phy-0086.000)
                 }

             -- The alarm state of a TLM entity.
             --
             Xldv20AlarmState ::=
                 INTEGER {
                     noAlarm                       (0),
                     hpEBER                        (1),
                     hpUNEQ                        (2),
                     lcd                           (3),
                     lof                           (4),
                     lop                           (5),
                     los                           (6),
                     msAIS                         (7),
                     msEBER                        (8),
                     msRDI                         (9),
                     msSD                          (10),
                     pAIS                          (11),
                     pRDI                          (12),
                     rsEBER                        (13),
                     slm                           (14),
                     tim                           (15),
                     actFault                      (16),
                     xmissionErr                   (105),
                     ais                           (114),
                     rdi                           (115),
                     plcpLof                       (116),
                     plcpRDI                       (117),
                     internalFault                 (123),
                     immMNR                        (130),          
                     lpr                           (131),
                     lol                           (132),
                     wrongServiceConfigData        (141),

                     -- E1/Ds1 near-end
                     excBER                        ( 407 ),
                     lif                           ( 408 ),
                     lods                          ( 409 ),
                           
                     -- E1/Ds1 far-end
                     txUnusableFe                  ( 410 ),
                     rxUnusableFe                  ( 411 ),
                     rfiIma                        ( 412 ),
		     rai                           ( 413 ),

                     -- IMA group 
                    startUpFe                     ( 450 ),
                    configAbort                   ( 451 ),
                    configAbortFe                 ( 452 ),
                    insufficientLinks             ( 453 ),
                    insufficientLinksFe           ( 454 ),
                    blockedFe                     ( 455 ),
                    imaMinNumOfLinks              ( 456)
                 }

             -- This type selects the mode for the trap flow control :
             --   noTraps  : no traps are sent
             --   allTraps : all traps are sent
             --   noEvents : only alarms but no events are sent
             --   noAlarms : only events but no alarms are sent
             --
             Xldv20SuppressionType ::=
                 INTEGER {
                     noTraps(1),
                     allTraps(2),
                     noEvents(3),
                     noAlarms(4)
                 }
             --
             -- This type selects the traffic type of a VPC.    
             -- for the trap flow control
             --
             Xldv20TrafficType ::=
                 INTEGER {
                     cbr    (1),              -- constant bit rate, default
                     ubr    (2),              -- unspecified bit rate,
                     nrtVbr (3),              
                     rtVbr  (4)              
                 }


             --  this type selects the traffic direction
             --
            Xldv20TrafficDirection ::=
                   INTEGER {
                         bidirectional (1),
                         upstream      (2),
                         downstream    (3) }

             -- 
             -- This type selects the equipment holder type.    
             --
             Xldv20EqhType ::=
                 INTEGER {
                     shelf(1),    -- unused 
                     slot(2),   
                     basicShelf(3),
                     extendedShelf(4),
                     ntAdslShelf(5), 
                     ntVdslShelf(6),
                     cpeShelf(7)
                 }
             --
             --
             -- This type specifies the different line types
             --
             Xldv20LineType ::=
                 INTEGER {
                     other(1),
                     shortSingleMode(2),          -- s1n, s3n
                     mediumSingleMode(3),         -- s1n, s3n
                     longSingleMode(4),           -- s1n, s3n
                     coax(5),                     -- s1n, s3n
                     utp(6),                      -- s1n, s3n
                     e3direct(7),                 -- e3n 
                     ds3plcp(8)                   -- d3n
                 }

         
              -- Default value of all current xldv20Adc... attributes of table
              -- xldv20AdcPPTPTable
              --
              -- THIS CUR DEFAULT WILL NOT BE USED IN THE MIB, BUT WILL BE USED BY 
              -- THE DSLAM SW TO SIGNALIZE A DEFAULT VALUE TO THE TMN_OS
              -- Xldv20AdcCurDefault ::=
              --     INTEGER {
              --             adcCurDefault(32767)
              --          }


             -- TLM S1n/S3n excessive bit error check threshold
             --
             Xldv20S1nS3nEberThreshold ::=
                 INTEGER {
                     exp10E3(1),
                     exp10E4(2),
                     exp10E5(3),
                     noSupervision(255)
                 }

             -- TLM S1n/S3n signal degration check threshold
             --
             Xldv20S1nS3nSDThreshold ::=
                 INTEGER {
                     exp10E5(3),
                     exp10E6(4),
                     exp10E7(5),
                     exp10E8(6),
                     exp10E9(7),
                     noSupervision(255)
                 }

             -- The value of this object identifies the severity
             -- of an alarm that has occured.
             --
             Xldv20AlarmSeverity ::=
                 INTEGER {
                     critical(1),
                     major(2),
                     minor(3),
                     warning(4),
                     noAlarming(5)
                 }

             -- The value of this object identifies the range
             -- of the alarm severity index.
             --
             Xldv20AlmSevProfileIndex ::= INTEGER (1..40) 

             -- The value of this object identifies the range
             -- of the alarm filtering index.
             --
             Xldv20AlmFiltProfileIndex ::= INTEGER (1..40) 

             -- This type specifies remote inventory result type
             --
             Xldv20RiResultType ::=
                 INTEGER {
                     riRemoteInventoryComplete(1),
                     riFileCreationFailed(2),
                     riFileTransmissionFailed(3)
                 }
             --
