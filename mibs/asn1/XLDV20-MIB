         xldv20HwEquipEntry OBJECT-TYPE
            -- info-IMPL:    V2.0->
            -- info-VERCTRL: -
            SYNTAX        Xldv20HwEquipEntry
            ACCESS        not-accessible
            STATUS        mandatory
            DESCRIPTION
                 "  Entry of the HW equipment table.
		 The entries represent the various manageable physical
                   components of the ATM NE that are not modelled via the
                   Plug-In Unit managed entity.
                   An instance of this managed entity shall be automati-
                   cally created by the ATM NE immediately following the
                   initialization/installation of the externally
                   manageable physical component in the ATM NE.
                   Notifications : 
		   xldv20TrObjCreate.
		   xldv20TrObjDelete . "
            INDEX         { xldv20HwUnitIndex }
            ::= { xldv20HwEquipTable 1 }

         Xldv20HwEquipEntry ::=
            SEQUENCE { xldv20EquType               DisplayString,
                       xldv20EquLocation           OCTET STRING,
                       xldv20EquUserLabel          OCTET STRING,
                       xldv20EquConnectedPiuIndex  Unsigned16,
                       xldv20EquConnectedPiuPort   INTEGER }

         xldv20EquType OBJECT-TYPE
            -- info-VALUE:   onu, ntAdsl, ntVdsl, ntCpe
            -- info-ROU:     GET db
            -- info-IMPL:    V2.0->
            -- info-VERCTRL: -
            SYNTAX        DisplayString (SIZE (0..10))
            ACCESS        read-only
            STATUS        mandatory
            DESCRIPTION
                 " Indicates whether the equipment is a DSLAM,
                   ntAdsl, ntVdsl or a ntCpe ( customer premise equipment) . "
            ::= { xldv20HwEquipEntry 1 }

         xldv20EquLocation OBJECT-TYPE
            -- info-VALUE:   e.g.: "Rack 23"
            -- info-ROU:     GET db  SET db
            -- info-IMPL:    V2.0->
            -- info-VERCTRL: -
