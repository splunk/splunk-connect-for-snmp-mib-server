             " The current operational state of the far-end IMA Group State
        Machine. "
        ::= { xldv20ImaGroupEntry 5 }

        xldv20ImaGroupFailureStatus OBJECT-TYPE
        -- info-VALUE:   noFailure(1),
        -- info-VALUE:   startUpNe(2),
        -- info-VALUE:   startUpFe(3), invalidValueNe(4),
        -- info-VALUE:   invalidValueFe(5), failedAssymetricNe(6),
        -- info-VALUE:   failedAssymetricFe(7),
