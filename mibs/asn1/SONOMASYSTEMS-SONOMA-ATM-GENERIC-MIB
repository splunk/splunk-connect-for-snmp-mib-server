    -- Sonoma ATM ASI Adapter Specific Group MIB groups

    -- This group contains all informat specificly for
    -- Sonoma Systems ASI adapter. It includes the configuration and
    -- some statistics information

    -- Sonoma ATM ASI Adapter Specific Group MIB groups
       atmAsiConfGroup OBJECT IDENTIFIER ::= { atmAsiPhysGroup 1 }
       atmAsiStatsGroup OBJECT IDENTIFIER ::= { atmAsiPhysGroup 2 }
    

    --  atmAsiConfGroup

    atmAsiConfPhyTable  OBJECT-TYPE
        SYNTAX  SEQUENCE OF AtmAsiConfPhyEntry
        ACCESS  not-accessible
        STATUS  mandatory
        DESCRIPTION
            "A table of physical layer configuration for the ASI
         interface"
        ::= { atmAsiConfGroup 1 }

    atmAsiConfPhyEntry  OBJECT-TYPE
        SYNTAX  AtmAsiConfPhyEntry
        ACCESS  not-accessible
        STATUS  mandatory
        DESCRIPTION
          "A entry in the table, containing information about the
           physical layer of an ASI interface"
        INDEX  { atmAsiConfPhysIndex }
        ::= { atmAsiConfPhyTable 1 }
    
    AtmAsiConfPhyEntry ::= SEQUENCE {
      atmAsiConfPhysIndex     INTEGER,
      atmAsiConfLineType      INTEGER,
      atmAsiConfLineCoding    INTEGER,
      atmAsiConfTxClkSelect   INTEGER,
      atmAsiConfSignalMode    INTEGER,      
      atmAsiConfLIUType       INTEGER,
      atmAsiConfLBOType       INTEGER,
      atmAsiConfIdlePattern   INTEGER,
      atmAsiConfLoopbackMode  INTEGER,
      atmAsiConfStopMode      INTEGER,
      atmAsiConfRecoveredClkSource    INTEGER,
      atmAsiDebugAddr         INTEGER,
      atmAsiDebugReadValue    INTEGER,
      atmAsiDebugWriteValue   INTEGER,
      atmAsiDebugRead         INTEGER
    }

    atmAsiConfPhysIndex   OBJECT-TYPE
        SYNTAX  INTEGER
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION
            "The physical interface index."
        ::= { atmAsiConfPhyEntry 1 }   

    atmAsiConfLineType OBJECT-TYPE
        SYNTAX  INTEGER { 
                        esf(2), 
                        d4(3),
                        e1(4),
                        e1-crc(5),
                        e1-cas(6),
                        e1-crc-cas(7)
                        }
        ACCESS  read-write
        STATUS  mandatory
        DESCRIPTION
            "Configure the line type."
          DEFVAL { esf }
        ::= { atmAsiConfPhyEntry 2 }


    atmAsiConfLineCoding OBJECT-TYPE
        SYNTAX  INTEGER {  
            b8zs(2),
            hdb3(3),
            zbtsi(4),
            ami(5)
            }
        ACCESS  read-write
        STATUS  mandatory
        DESCRIPTION
            "Configure the line coding."
            DEFVAL { b8zs }
        ::= { atmAsiConfPhyEntry 4 }

    atmAsiConfTxClkSelect OBJECT-TYPE
        SYNTAX  INTEGER {
                        internal(1),
                        line(2),
                        through(3)
                        }
        ACCESS  read-write
        STATUS  mandatory
        DESCRIPTION
            "This parameter configures the source of the Tx clock."
            DEFVAL { line }
        ::= { atmAsiConfPhyEntry 5 }
        
    atmAsiConfSignalMode OBJECT-TYPE
        SYNTAX  INTEGER {
                        none(1),
                        robbed-bit(2),
                        bit-oriented (3)
                        }
        ACCESS  read-write
        STATUS  mandatory
        DESCRIPTION
            "This parameter configures the signal mode."
            DEFVAL { none }
        ::= { atmAsiConfPhyEntry 6 }
        


	atmAsiConfLIUType OBJECT-TYPE
        SYNTAX  INTEGER {
                        t1-long(1),
                        itu-120(2),
                        itu-75(3),
                        dsx1-short(4)
                        }
        ACCESS  read-write
        STATUS  mandatory
        DESCRIPTION
            "This parameter configures the Line Interface Unit type."
            DEFVAL { dsx1-short }
        ::= { atmAsiConfPhyEntry 7 }

	atmAsiConfLBOType OBJECT-TYPE
        SYNTAX  INTEGER {
                        short-133(1),
                        short-266(2),
                        short-399(3),
                        short-533(4),
                        short-655(5),
                        long-0(8),
                        long-75(9),
                        long-150(10),
                        long-225(11)
                        }
        ACCESS  read-write
        STATUS  mandatory
        DESCRIPTION
            "This parameter configures the Line Build Out."
            DEFVAL { short-133 }
        ::= { atmAsiConfPhyEntry 8 }

atmAsiConfIdlePattern OBJECT-TYPE
        SYNTAX  INTEGER(0..255)
        ACCESS  read-write
        STATUS  mandatory
        DESCRIPTION
            "This parameter configures the idle pattern for trunk
             conditioning."
            DEFVAL { 127 }
        ::= { atmAsiConfPhyEntry 9 }

atmAsiConfLoopbackMode OBJECT-TYPE
        SYNTAX  INTEGER {
                        disable(1),
                        internal(2),
                        external(3)
                        }
        ACCESS  read-write
        STATUS  mandatory
        DESCRIPTION
            "This parameter configures the loopback mode."
            DEFVAL { disable }
        ::= { atmAsiConfPhyEntry 10 }

atmAsiConfStopMode OBJECT-TYPE
        SYNTAX  INTEGER {
                        immediate(1),
                        on-idle(2)
                        }
        ACCESS  read-write
        STATUS  mandatory
        DESCRIPTION
            "This parameter configures the mode used for stop commands."
            DEFVAL { immediate }
        ::= { atmAsiConfPhyEntry 11 }

atmAsiConfRecoveredClkSource OBJECT-TYPE
        SYNTAX  INTEGER {
			fourth-port(1),
			third-port(2),
			second-port(3),
			first-port(4),
			phy-port(5)			
			}
        ACCESS  read-write
        STATUS  mandatory
        DESCRIPTION
            "This parameter configures the source of the recovered clock used for
             through timing."
            DEFVAL { fourth-port }
        ::= { atmAsiConfPhyEntry 12 }

atmAsiDebugAddr  OBJECT-TYPE
        SYNTAX  INTEGER 
        ACCESS  read-write
        STATUS  mandatory
        DESCRIPTION
            "PCI Phy address for DS1 port register."
        DEFVAL { 0 }
        ::= { atmAsiConfPhyEntry 13 }

    atmAsiDebugReadValue  OBJECT-TYPE
        SYNTAX  INTEGER 
        ACCESS  read-write
        STATUS  mandatory
        DESCRIPTION
            "Value read from DS1 port register."
        DEFVAL { 0 }
        ::= { atmAsiConfPhyEntry 14 }

    atmAsiDebugWriteValue  OBJECT-TYPE
        SYNTAX  INTEGER 
        ACCESS  read-write
        STATUS  mandatory
        DESCRIPTION
            "Value written to DS1 port register."
        DEFVAL { 0 }
        ::= {atmAsiConfPhyEntry 15 }

    atmAsiDebugRead  OBJECT-TYPE
        SYNTAX  INTEGER {
                        read(1),
                        write(2)
                        }
        ACCESS  read-write
        STATUS  mandatory
        DESCRIPTION
            "Read or Write of DS1 port register (TRUE = READ)."
        DEFVAL { read }
        ::= { atmAsiConfPhyEntry 16 }



    --  atmAsiStatsGroup

    atmAsiStatsTable  OBJECT-TYPE
        SYNTAX  SEQUENCE OF AtmAsiStatsEntry
        ACCESS  not-accessible
        STATUS  mandatory
        DESCRIPTION
            "A table of physical layer statistics information for the ASI 
         interface"
        ::= { atmAsiStatsGroup 1 }

    atmAsiStatsEntry  OBJECT-TYPE
        SYNTAX  AtmAsiStatsEntry
        ACCESS  not-accessible
        STATUS  mandatory
        DESCRIPTION
          "A entry in the table, containing information about the
           physical layer of an ASI interface"
        INDEX  { atmAsiStatsPhysIndex }
        ::= { atmAsiStatsTable 1 }
    
    AtmAsiStatsEntry ::= SEQUENCE {
      atmAsiStatsPhysIndex      INTEGER,
      atmAsiStatsNoSignals      Counter,
      atmAsiStatsAISDetects     Counter,
      atmAsiStatsYelAlarmCount  Counter,
      atmAsiStatsLCVErrors      Counter,
      atmAsiStatsPCVErrors      Counter,
      atmAsiStatsMOSErrors      Counter,
      atmAsiStatsSyncLossCount  Counter,
      atmAsiStatsHECErrors      Counter,
      atmAsiStatsSignalLoss       INTEGER,
      atmAsiStatsAISDetect        INTEGER,
      atmAsiStatsYellowAlarm      INTEGER,
      atmAsiStatsSyncLoss          INTEGER,
      atmAsiStatsTxClockLoss         INTEGER,
      atmAsiStatsClearCounters     INTEGER
    }

    atmAsiStatsPhysIndex  OBJECT-TYPE
        SYNTAX  INTEGER
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION
            "The physical interface index."
        ::= { atmAsiStatsEntry 1 }  

    atmAsiStatsNoSignals  OBJECT-TYPE
        SYNTAX  Counter
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION
            "No signal error counter."
        ::= { atmAsiStatsEntry 2 }

    atmAsiStatsAISDetects  OBJECT-TYPE
        SYNTAX  Counter
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION
            "AIS Detect counter."
        ::= {atmAsiStatsEntry 3 }
    
    atmAsiStatsYelAlarmCount  OBJECT-TYPE
        SYNTAX  Counter
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION
            "A count of the number of Yellow Alarms."
        ::= { atmAsiStatsEntry 4 }

    atmAsiStatsLCVErrors  OBJECT-TYPE
        SYNTAX  Counter
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION
            "LCV (Line Code Violation) error counter."
        ::= { atmAsiStatsEntry 5 }

    atmAsiStatsPCVErrors  OBJECT-TYPE
        SYNTAX  Counter
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION
            "PCV (Path Code Violation) error counter."
        ::= { atmAsiStatsEntry 6 }

    atmAsiStatsMOSErrors  OBJECT-TYPE
        SYNTAX  Counter
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION
            "Multiframe out of sync error counter."
        ::= { atmAsiStatsEntry 7 }

    atmAsiStatsSyncLossCount  OBJECT-TYPE
        SYNTAX  Counter
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION
            "Sync Loss counter."
        ::= { atmAsiStatsEntry 8 }

    atmAsiStatsHECErrors  OBJECT-TYPE
        SYNTAX  Counter
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION
            "HEC error counter."
        ::= { atmAsiStatsEntry 9 }

    atmAsiStatsSignalLoss  OBJECT-TYPE
        SYNTAX  INTEGER {
                        true (1), 
                        false (2)
                        }
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION
            "Signal loss indication."
        ::= { atmAsiStatsEntry 10 }

    atmAsiStatsAISDetect  OBJECT-TYPE
        SYNTAX  INTEGER {
                        true (1), 
                        false (2)
                        }
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION
            "AIS indication."
        ::= { atmAsiStatsEntry 11 }

    atmAsiStatsYellowAlarm  OBJECT-TYPE
        SYNTAX  INTEGER {
                        true (1), 
                        false (2)
                        }
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION
            "Yellow Alarm indication."
        ::= { atmAsiStatsEntry 12 }

    atmAsiStatsSyncLoss  OBJECT-TYPE
        SYNTAX  INTEGER {
                        true (1), 
                        false (2)
                        }
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION
            "Loss of sync indication."
        ::= { atmAsiStatsEntry 13 }

    atmAsiStatsTxClockLoss  OBJECT-TYPE
        SYNTAX  INTEGER {
                    true (1), 
                    false (2)
                    }
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION
            "Loss of transmit clock indication."
        ::= { atmAsiStatsEntry 14 }

    atmAsiStatsClearCounters  OBJECT-TYPE
        SYNTAX  INTEGER {
                        true (1), 
                        false (2)
                        }
        ACCESS  read-write
        STATUS  mandatory
        DESCRIPTION
            "Clear all counters in this group ONLY."
        DEFVAL { false }
        ::= { atmAsiStatsEntry 15 }

    -----------------------------------------------------------------------

    -- Sonoma ATM ASM Adapter Specific Group

    -- This group contains all information specifically for the
    -- Sonoma Systems ASM adapter. Currently it includes an object
    -- to set the physical port's line usage.

       atmAsmPhyConfGroup OBJECT IDENTIFIER ::= { atmAsmPhysGroup 1 }

    --  atmAsmPhyConfGroup

    atmAsmPhyConfTable  OBJECT-TYPE
        SYNTAX  SEQUENCE OF AtmAsmPhyConfEntry
        ACCESS  not-accessible
        STATUS  mandatory
        DESCRIPTION
          "A table of physical layer configuration for the ASM
           interfaces."
        ::= { atmAsmPhyConfGroup 1 }

    atmAsmPhyConfEntry  OBJECT-TYPE
        SYNTAX  AtmAsmPhyConfEntry
        ACCESS  not-accessible
        STATUS  mandatory
        DESCRIPTION
          "A entry in the table, containing information about the
           physical layer of an ASM interfaces."
        INDEX  { atmAsmPhyConfIndex }
        ::= { atmAsmPhyConfTable 1 }
    
    AtmAsmPhyConfEntry ::= SEQUENCE {
      atmAsmPhyConfIndex          INTEGER,
      atmAsmPhyConfAdminLineUsage INTEGER,
      atmAsmPhyConfOperLineUsage  INTEGER
    }

    atmAsmPhyConfIndex   OBJECT-TYPE
        SYNTAX  INTEGER
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION
            "The physical interface index."
        ::= { atmAsmPhyConfEntry 1 }

    atmAsmPhyConfAdminLineUsage OBJECT-TYPE
        SYNTAX  INTEGER {
                        ces-udt(1), 
                        ces-sdt(2),
                        uni(4),
                        ima(5) 
                        } 
        ACCESS  read-write
        STATUS  mandatory
        DESCRIPTION
            "The desired physical port's line usage. For some port 
             types, the desired line usage will become effective 
             after the system is rebooted."
            DEFVAL { uni }
        ::= { atmAsmPhyConfEntry 2 }

    atmAsmPhyConfOperLineUsage OBJECT-TYPE
        SYNTAX  INTEGER {
                        ces-udt(1), 
                        ces-sdt(2),
                        uni(4),
                        ima(5) 
                        } 
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION
            "The physical port's current line usage."
            DEFVAL { uni }
        ::= { atmAsmPhyConfEntry 3 }

END
