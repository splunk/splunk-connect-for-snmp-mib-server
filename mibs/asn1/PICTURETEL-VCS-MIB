	SYNTAX CallStateType
	MAX-ACCESS read-only
	STATUS          current
	DESCRIPTION     " Current or most recent H.320 call state"
	::= { ptSysH320CallStat 3 }



ptH320CallDuration  OBJECT-TYPE
	SYNTAX Gauge32
	MAX-ACCESS read-only
	STATUS          current
	DESCRIPTION     " Current or most recent H.320 call duration (sec)"
	::= { ptSysH320CallStat 4 }


