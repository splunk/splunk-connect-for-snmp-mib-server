TRAX-MIB DEFINITIONS ::= BEGIN

IMPORTS
   enterprises,
   experimental,
   IpAddress,
   TimeTicks,
   Gauge,
   Counter
      FROM RFC1155-SMI
   DisplayString
      FROM RFC1213-MIB
   OBJECT-TYPE
      FROM RFC-1212;


usr OBJECT IDENTIFIER ::= { enterprises 429 }

nas OBJECT IDENTIFIER ::= { usr 1 }

trax OBJECT IDENTIFIER ::= { nas 31 }

-- The TRAX Card Identification Group.

traxId OBJECT IDENTIFIER ::= { trax 1 }

traxIdTable OBJECT-TYPE
SYNTAX SEQUENCE OF TraxIdEntry
ACCESS not-accessible
STATUS mandatory
DESCRIPTION
"A table that contains information that identifies the 
hardware and software that make up the specified Hiper TRAX 
card."
::= { traxId 1 }

traxIdEntry OBJECT-TYPE
SYNTAX TraxIdEntry
ACCESS not-accessible
STATUS mandatory
DESCRIPTION
"There is one entry in this table for every Hiper TRAX card 
in the chassis."
INDEX { traxIdIndex }
::= { traxIdTable 1 }

TraxIdEntry ::=
SEQUENCE {
traxIdIndex
    INTEGER,
traxIdHardwareSerNum
    DisplayString,
traxIdHardwareRev
    DisplayString,
traxIdSoftwareRev
    DisplayString,
traxIdCpuType
    INTEGER,
traxIdRamInstalled
    INTEGER,
traxIdFlashInstalled
    INTEGER,
traxIdSelfTestResult
    INTEGER,
traxIdMgmtConnect
    INTEGER
}

traxIdIndex OBJECT-TYPE
SYNTAX INTEGER
ACCESS read-only
STATUS mandatory
DESCRIPTION
"An index that uniquely identifies the Hiper TRAX card for 
which this row in the table pertains.  The value of this 
index matches the value of the index for the corresponding 
entity in the entity table of the chassis MIB."
::= { traxIdEntry 1 }

traxIdHardwareSerNum OBJECT-TYPE
SYNTAX DisplayString
ACCESS read-only
STATUS mandatory
DESCRIPTION
"The Hiper TRAX card's hardware serial number as stored in 
EEPROM."
::= { traxIdEntry 2 }

traxIdHardwareRev OBJECT-TYPE
SYNTAX DisplayString
ACCESS read-only
STATUS mandatory
DESCRIPTION
"The hardware revision of the specified Hiper TRAX card as 
stored in its EEPROM."
::= { traxIdEntry 3 }

traxIdSoftwareRev OBJECT-TYPE
SYNTAX DisplayString
ACCESS read-only
STATUS mandatory
DESCRIPTION
"The revision of the software being executed in the 
specified Hiper TRAX card."
::= { traxIdEntry 4 }

traxIdCpuType OBJECT-TYPE
SYNTAX INTEGER{
  i80386(1),
  i80486(2),
  powerPC603(3)
}
ACCESS read-only
STATUS mandatory
DESCRIPTION
"Defines the type of CPU present on the Hiper TRAX NAC."
::= { traxIdEntry 5 }

traxIdRamInstalled OBJECT-TYPE
SYNTAX INTEGER
ACCESS read-only
STATUS mandatory
DESCRIPTION
"Specifies the amount of DRAM installed on the Hiper TRAX 
Gateway NAC in Kbytes."
::= { traxIdEntry 6 }

traxIdFlashInstalled OBJECT-TYPE
SYNTAX INTEGER
ACCESS read-only
STATUS mandatory
DESCRIPTION
"Specifies the amount of Flash Rom installed on the Hiper 
TRAX NAC in Kbytes."
::= { traxIdEntry 7 }

traxIdSelfTestResult OBJECT-TYPE
SYNTAX INTEGER
ACCESS read-only
STATUS mandatory
DESCRIPTION
"Displays the results of the most resent self test performed 
by the Hiper TRAX NAC.  If all tests have passed a value of 
0 is returned."
::= { traxIdEntry 9 }

traxIdMgmtConnect OBJECT-TYPE
SYNTAX INTEGER{
  inactive(1),
  active(2)
}
ACCESS read-only
STATUS mandatory
DESCRIPTION
"Indicates the current status of Hiper TRAX Card user 
interface."
::= { traxIdEntry 10 }

-- The TRAX Card Command Group.

traxCmd OBJECT IDENTIFIER ::= { trax 2 }

traxCmdTable OBJECT-TYPE
SYNTAX SEQUENCE OF TraxCmdEntry
ACCESS not-accessible
STATUS mandatory
DESCRIPTION
"The Hiper TRAX Card Command table contains an entry for 
each of the manageable Hiper TRAX Cards in the chassis.  It 
provides a means through which to take specific actions on 
one or more Hiper TRAX cards in the NAS chassis."
::= { traxCmd 1 }

traxCmdEntry OBJECT-TYPE
SYNTAX TraxCmdEntry
ACCESS not-accessible
STATUS mandatory
DESCRIPTION
"There is one Hiper TRAX Card Command entry per Hiper TRAX 
Card in the chassis."
INDEX { traxCmdIndex }
::= { traxCmdTable 1 }

TraxCmdEntry ::=
SEQUENCE {
traxCmdIndex
    INTEGER,
traxCmdMgtStationId
    OCTET STRING,
traxCmdReqId
    INTEGER,
traxCmdFunction
    INTEGER,
traxCmdForce
    INTEGER,
traxCmdParam
    OCTET STRING,
traxCmdResult
    INTEGER,
traxCmdCode
    INTEGER
}

traxCmdIndex OBJECT-TYPE
SYNTAX INTEGER
ACCESS read-only
STATUS mandatory
DESCRIPTION
"A unique value for each Hiper TRAX Card in the chassis.  
The value of this object matches the value of the index of 
the corresponding Hiper TRAX Card's entry in the entity 
table of the chassis MIB."
::= { traxCmdEntry 1 }

traxCmdMgtStationId OBJECT-TYPE
SYNTAX OCTET STRING (SIZE(0..8))
ACCESS read-write
STATUS mandatory
DESCRIPTION
"This object is a generic read-write variable that a 
Management Station (MS) can use to guarantee that the 
results from a given command are the results of a command 
issued by that specific MS.  Each MS must SET a unique 
value to this object"
::= { traxCmdEntry 2 }

traxCmdReqId OBJECT-TYPE
SYNTAX INTEGER
ACCESS read-only
STATUS mandatory
DESCRIPTION
"This object contains the value of the request-id field in 
the SNMP PDU which invoked the current or most recent 
command or test on this X.25 Card.  If the request-id is 
unknown or undefined, this object contains the value zero."
::= { traxCmdEntry 3 }

traxCmdFunction OBJECT-TYPE
SYNTAX INTEGER{
  noCommand(1),
  saveToNVRAM(2),
  restoreFromNVRAM(3),
  restoreFromDefault(4),
  nonDisruptSelfTest(5),
  disruptSelfTest(6),
  softwareReset(7),
  downloadCfgFile(8),
  uploadCfgFile(9),
  resetStsCounters(10)
}
ACCESS read-write
STATUS mandatory
DESCRIPTION
"This object contains a value which describes the command 
which is being invoked."
::= { traxCmdEntry 4 }

traxCmdForce OBJECT-TYPE
SYNTAX INTEGER
ACCESS read-write
STATUS mandatory
DESCRIPTION
"In some cases, the Hiper TRAX Card may be in a state such 
that certain commands could adversely affect connections.  
In such cases, a command request with this object not 
present or set to noForce will result in a warning"
::= { traxCmdEntry 5 }

traxCmdParam OBJECT-TYPE
SYNTAX OCTET STRING
ACCESS read-write
STATUS mandatory
DESCRIPTION
"This object contains parameters that are specific to the 
particular command being issued.  In some cases, there will 
be no aditional parameters required."
::= { traxCmdEntry 6 }

traxCmdResult OBJECT-TYPE
SYNTAX INTEGER{
  none(1),
  success(2),
  inProgress(3),
  notSupported(4),
  unAbleToRun(5),
  aborted(6),
  failed(7)
}
ACCESS read-only
STATUS mandatory
DESCRIPTION
"This object contains the result of the most recently 
requested command or test, or the value none(1) if no 
commands have been requested since the last reset."
::= { traxCmdEntry 7 }

traxCmdCode OBJECT-TYPE
SYNTAX INTEGER{
  noError(1),
  unable(2),
  unrecognizedCommand(6),
  slotEmpty(8),
  noResponse(12),
  unsupportedCommand(20),
  deviceDisabled(22),
  fileTooBig(46),
  userInterfaceActive(58),
  badFlashRomID(61),
  badFlashVoltage(62),
  flashEraseError(63),
  eraseSequenceError(64),
  eraseExecutionError(65),
  receiveBufferOverflow(66),
  badProgramVoltage(68),
  programmingDataError(69),
  programCodeError(70),
  invalidCodeError(71),
  romCrcBad(72),
  pendingSoftwareDownload(73),
  ramCrcBad(74),
  invalidRomId(75),
  cardIdMismatch(84),
  cardIdUnknown(85),
  tftpTimeout(86),
  flashEraseTimeout(87),
  invalidFileHeader(88),
  bulkTransferInProcess(93)
}
ACCESS read-only
STATUS mandatory
DESCRIPTION
"The value of this object is noError(1) if the command was 
successful, and indicates a further description of what 
went wrong if it was unsuccessful."
::= { traxCmdEntry 8 }

-- The TRAX Card Configuration Group.

traxCfg OBJECT IDENTIFIER ::= { trax 3 }

traxCfgTable OBJECT-TYPE
SYNTAX SEQUENCE OF TraxCfgEntry
ACCESS not-accessible
STATUS mandatory
DESCRIPTION
"A table of configurable parameters specific to the Hiper 
TRAX NAC."
::= { traxCfg 1 }

traxCfgEntry OBJECT-TYPE
SYNTAX TraxCfgEntry
ACCESS not-accessible
STATUS mandatory
DESCRIPTION
""
INDEX { traxCfgIndex }
::= { traxCfgTable 1 }

TraxCfgEntry ::=
SEQUENCE {
traxCfgIndex
    INTEGER,
traxCfgCardIPAddress
    IpAddress,
traxCfgIPMask
    IpAddress,
traxCfgGatewayIpAddr
    IpAddress,
traxCfgHostIPAddr
    IpAddress,
traxCfgX25Addr
    DisplayString,
traxCfgNicTrapDestination
    IpAddress,
traxCfgReadMemAddr
    INTEGER,
traxCfgReadMemData
    OCTET STRING,
traxCfgReadMemLength
    INTEGER,
traxCfgWriteMemAddr
    INTEGER,
traxCfgWriteMemLen
    INTEGER,
traxCfgWriteMemData
    OCTET STRING,
traxFailureReason
    INTEGER,
traxDNIFailureReason
    OCTET STRING,
traxCfgX25AddrPort1
    DisplayString
}

traxCfgIndex OBJECT-TYPE
SYNTAX INTEGER
ACCESS read-only
STATUS mandatory
DESCRIPTION
"A unique value for each Hiper TRAX NAC installed in the 
chassis.  The value of this index is identical to the index 
in the USR chassis MIB associates with the Hiper TRAX Card 
entity."
::= { traxCfgEntry 1 }

traxCfgCardIPAddress OBJECT-TYPE
SYNTAX IpAddress
ACCESS read-write
STATUS mandatory
DESCRIPTION
"Specifies the local IP address used for the Hiper TRAX Card 
Mgmt"
::= { traxCfgEntry 2 }

traxCfgIPMask OBJECT-TYPE
SYNTAX IpAddress
ACCESS read-write
STATUS mandatory
DESCRIPTION
"Specifies the subnet mask for the Hiper TRAX Card"
::= { traxCfgEntry 3 }

traxCfgGatewayIpAddr OBJECT-TYPE
SYNTAX IpAddress
ACCESS read-write
STATUS mandatory
DESCRIPTION
"Specifies Hiper TRAX gateway IP Address"
::= { traxCfgEntry 4 }

traxCfgHostIPAddr OBJECT-TYPE
SYNTAX IpAddress
ACCESS read-write
STATUS mandatory
DESCRIPTION
"Specifies Hiper TRAX Cards LAN HOST IP Address"
::= { traxCfgEntry 5 }

traxCfgX25Addr OBJECT-TYPE
SYNTAX DisplayString (SIZE(0..15))
ACCESS read-write
STATUS mandatory
DESCRIPTION
"Specifies Hiper TRAX network address port 0 for card."
::= { traxCfgEntry 6 }

traxCfgNicTrapDestination OBJECT-TYPE
SYNTAX IpAddress
ACCESS read-write
STATUS mandatory
DESCRIPTION
"Specifies alternative trap destination for the NIC IP Port"
::= { traxCfgEntry 7 }

traxCfgReadMemAddr OBJECT-TYPE
SYNTAX INTEGER
ACCESS read-write
STATUS mandatory
DESCRIPTION
"Memory Address for retrieving data"
::= { traxCfgEntry 8 }

traxCfgReadMemData OBJECT-TYPE
SYNTAX OCTET STRING (SIZE(0..32))
ACCESS read-write
STATUS mandatory
DESCRIPTION
""
::= { traxCfgEntry 9 }

traxCfgReadMemLength OBJECT-TYPE
SYNTAX INTEGER
ACCESS read-write
STATUS mandatory
DESCRIPTION
"Specifies number of byte of the ReadMemoryAddr to Read"
::= { traxCfgEntry 10 }

traxCfgWriteMemAddr OBJECT-TYPE
SYNTAX INTEGER
ACCESS read-write
STATUS mandatory
DESCRIPTION
"Memory address for setting data"
::= { traxCfgEntry 11 }

traxCfgWriteMemLen OBJECT-TYPE
SYNTAX INTEGER
ACCESS read-write
STATUS mandatory
DESCRIPTION
"Specifies the number of bytes of the WriteMemoryData string 
to write"
::= { traxCfgEntry 12 }

traxCfgWriteMemData OBJECT-TYPE
SYNTAX OCTET STRING (SIZE(0..32))
ACCESS read-write
STATUS mandatory
DESCRIPTION
"Specifies the data to be written to processor memory"
::= { traxCfgEntry 13 }

traxFailureReason OBJECT-TYPE
SYNTAX INTEGER
ACCESS not-accessible
STATUS mandatory
DESCRIPTION
"Specifies the Failure Reason for the TRAX Traps."
::= { traxCfgEntry 14 }

traxDNIFailureReason OBJECT-TYPE
SYNTAX OCTET STRING (SIZE(0..20))
ACCESS not-accessible
STATUS mandatory
DESCRIPTION
"Specifies the Failure Reason for the TRAX Traps."
::= { traxCfgEntry 15 }

traxCfgX25AddrPort1 OBJECT-TYPE
SYNTAX DisplayString (SIZE(0..15))
ACCESS read-write
STATUS mandatory
DESCRIPTION
"Specifies Hiper TRAX network address port 1 for card"
::= { traxCfgEntry 16 }

-- The TRAX Trap Enable Group.

traxTrapEna OBJECT IDENTIFIER ::= { trax 4 }

traxTrapEnaTable OBJECT-TYPE
SYNTAX SEQUENCE OF TraxTrapEnaEntry
ACCESS not-accessible
STATUS mandatory
DESCRIPTION
"Table containing objects to enable traps on the Hiper TRAX 
Cards in the chassis."
::= { traxTrapEna 1 }

traxTrapEnaEntry OBJECT-TYPE
SYNTAX TraxTrapEnaEntry
ACCESS not-accessible
STATUS mandatory
DESCRIPTION
"There is one entry for each Hiper TRAX Card in the chassis"
INDEX { traxTrapEnaIndex }
::= { traxTrapEnaTable 1 }

TraxTrapEnaEntry ::=
SEQUENCE {
traxTrapEnaIndex
    INTEGER,
traxTrapEnaNicMissing
    INTEGER,
traxTrapEnaX25FrameLevelUp
    INTEGER,
traxTrapEnaX25FrameLevelDown
    INTEGER,
traxTrapEnaX25PacketLevelUp
    INTEGER,
traxTrapEnaX25PacketLevelDn
    INTEGER,
traxTrapEnaX25LostCalls
    INTEGER,
traxTrapEnaX25LinkUp
    INTEGER,
traxTrapEnaX25LinkDn
    INTEGER,
traxTrapEnaDnisLookUp
    INTEGER,
traxTrapEnaSvcSetUp
    INTEGER,
traxTrapEnaSvcAlarm
    INTEGER,
traxTrapDupTrans
    INTEGER
}

traxTrapEnaIndex OBJECT-TYPE
SYNTAX INTEGER
ACCESS read-only
STATUS mandatory
DESCRIPTION
"A unique index identifying the Hiper TRAX Card to which the 
trap enable objects pertain."
::= { traxTrapEnaEntry 1 }

traxTrapEnaNicMissing OBJECT-TYPE
SYNTAX INTEGER{
  enable(1),
  disable(2)
}
ACCESS read-write
STATUS mandatory
DESCRIPTION
"Enables reporting of NIC missing event"
::= { traxTrapEnaEntry 2 }

traxTrapEnaX25FrameLevelUp OBJECT-TYPE
SYNTAX INTEGER{
  enable(1),
  disable(2)
}
