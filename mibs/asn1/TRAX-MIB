::= { traxTrapEnaEntry 13 }

--The TRAX Card Visa Statistics.

traxVisaSts OBJECT IDENTIFIER ::= { trax 5 }

traxVisaStsTable OBJECT-TYPE
SYNTAX SEQUENCE OF TraxVisaStsEntry
ACCESS not-accessible
STATUS mandatory
DESCRIPTION
""
::= { traxVisaSts 1 }

traxVisaStsEntry OBJECT-TYPE
SYNTAX TraxVisaStsEntry
ACCESS not-accessible
STATUS mandatory
DESCRIPTION
"Entries of Hiper TRAX  VISA  Statistics"
INDEX { traxVisaStsIndex }
::= { traxVisaStsTable 1 }

TraxVisaStsEntry ::=
SEQUENCE {
traxVisaStsIndex
    INTEGER,
traxVisaCallCount
    Counter,
traxVisaTransactionCount
    Counter,
traxVisaRetransCount
    Counter,
traxVisaDuplicateTransCount
    Counter,
traxVisaDiscNormal
    Counter,
traxVisaDiscPosRequest
    Counter,
traxVisaDiscHostRequest
    Counter,
traxVisaDiscMaxEnqs
    Counter,
traxVisaDiscMaxRetranToPos
    Counter,
traxVisaDiscMaxRetranToHost
    Counter,
traxVisaDiscMaxBadFrames
    Counter,
traxVisaDiscPosWriteTimeouts
    Counter,
traxVisaDiscHostWriteTimeouts
    Counter,
traxVisaDiscSynDelayTooLong
    Counter,
traxVisaDiscBadCurrentState
    Counter,
traxVisaDiscUnexpectedEvent
    Counter,
traxVisaDiscNdcLogonFailures
    Counter,
traxVisaDiscPosRcvTimeouts
    Counter,
traxVisaDiscPosFrameTooBig
    Counter,
traxVisaDiscHostFrameTooBig
    Counter,
traxVisaDiscHostRespTimeouts
    Counter,
traxVisaDiscNumBadMsg
    Counter,
traxVisaDiscNumSystem
    Counter
}

traxVisaStsIndex OBJECT-TYPE
SYNTAX INTEGER
ACCESS read-only
STATUS mandatory
DESCRIPTION
"Index into Hiper TRAX Visa Table"
::= { traxVisaStsEntry 1 }

traxVisaCallCount OBJECT-TYPE
SYNTAX Counter
ACCESS read-only
STATUS mandatory
DESCRIPTION
"Number of calls in which VISA protocol was initiated"
::= { traxVisaStsEntry 2 }

traxVisaTransactionCount OBJECT-TYPE
SYNTAX Counter
ACCESS read-only
STATUS mandatory
DESCRIPTION
