SYNTAX INTEGER{
  enable(1),
  disable(2)
}
ACCESS read-write
STATUS mandatory
DESCRIPTION
"Enable Hiper TRAX Packet Level Down Event"
::= { traxTrapEnaEntry 6 }

traxTrapEnaX25LostCalls OBJECT-TYPE
SYNTAX INTEGER{
  enable(1),
  disable(2)
}
ACCESS read-write
STATUS mandatory
DESCRIPTION
"Enables Hiper TRAX Lost Calls Event"
::= { traxTrapEnaEntry 7 }

traxTrapEnaX25LinkUp OBJECT-TYPE
SYNTAX INTEGER{
  enable(1),
  disable(2)
}
ACCESS read-write
STATUS mandatory
DESCRIPTION
"Enables Hiper TRAX link Up Event"
::= { traxTrapEnaEntry 8 }

