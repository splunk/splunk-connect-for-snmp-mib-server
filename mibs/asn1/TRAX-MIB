    Counter,
traxSyncDiscMaxRetrans
    Counter,
traxSyncDiscMaxSnrms
    Counter,
traxSyncDiscHostFrameTooBig
    Counter,
traxSyncDiscPosFrameTooSmall
    Counter,
traxSyncDiscPosFrameTooBig
    Counter,
