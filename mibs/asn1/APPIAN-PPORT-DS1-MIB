     --
     -- The Appian DS1  Threshold Table
     -- 


     acDs1ThresholdTable OBJECT-TYPE
          SYNTAX SEQUENCE OF AcDs1ThresholdEntry
          MAX-ACCESS  not-accessible
          STATUS  current
          DESCRIPTION
                 "The Appian DS1  Threshold Table allows the 
                 user to configure the threshold values for each of the 
                 statistics found in the interval table base on
                 the 15 minute interval on the day.  This table is
                 created automatically."
          ::= { acDs1 6 }

     acDs1ThresholdEntry OBJECT-TYPE
          SYNTAX  AcDs1ThresholdEntry
          MAX-ACCESS  not-accessible
          STATUS  current
          DESCRIPTION
                 "An entry in the Appian DS1 Threshold table."
          INDEX   { acDs1ThresholdNodeId,
                    acDs1ThresholdSlot,  
                    acDs1ThresholdPort}
          ::= { acDs1ThresholdTable 1 }

     AcDs1ThresholdEntry ::=
          SEQUENCE {
              acDs1ThresholdNodeId                 AcNodeId,
              acDs1ThresholdSlot                   AcSlotNumber,
              acDs1ThresholdPort                   AcPortNumber,
              acDs1ThresholdNEIntervalESs          INTEGER,
              acDs1ThresholdNEIntervalSESs         INTEGER,
              acDs1ThresholdNEIntervalSEFSs        INTEGER,
              acDs1ThresholdNEIntervalUASs         INTEGER,
              acDs1ThresholdNEIntervalCSSs         INTEGER,
              acDs1ThresholdNEIntervalPCVs         INTEGER,
              acDs1ThresholdNEIntervalLESs         INTEGER,
              acDs1ThresholdNEIntervalBESs         INTEGER,
              acDs1ThresholdNEIntervalDMs          INTEGER,
              acDs1ThresholdNEIntervalLCVs         INTEGER,
              acDs1ThresholdNEDayESs               INTEGER,
              acDs1ThresholdNEDaySESs              INTEGER,
              acDs1ThresholdNEDaySEFSs             INTEGER,
              acDs1ThresholdNEDayUASs              INTEGER,
              acDs1ThresholdNEDayCSSs              INTEGER,
              acDs1ThresholdNEDayPCVs              INTEGER,
              acDs1ThresholdNEDayLESs              INTEGER,
              acDs1ThresholdNEDayBESs              INTEGER,
              acDs1ThresholdNEDayDMs               INTEGER,
              acDs1ThresholdNEDayLCVs              INTEGER,
              acDs1ThresholdFEIntervalESs          INTEGER,
              acDs1ThresholdFEIntervalSESs         INTEGER,
              acDs1ThresholdFEIntervalSEFSs        INTEGER,
              acDs1ThresholdFEIntervalUASs         INTEGER,
              acDs1ThresholdFEIntervalCSSs         INTEGER,
              acDs1ThresholdFEIntervalLESs         INTEGER,
              acDs1ThresholdFEIntervalPCVs         INTEGER,
              acDs1ThresholdFEIntervalBESs         INTEGER,
              acDs1ThresholdFEIntervalDMs          INTEGER,
              acDs1ThresholdFEDayESs               INTEGER,
              acDs1ThresholdFEDaySESs              INTEGER,
              acDs1ThresholdFEDaySEFSs             INTEGER,
              acDs1ThresholdFEDayUASs              INTEGER,
              acDs1ThresholdFEDayCSSs              INTEGER,
              acDs1ThresholdFEDayLESs              INTEGER,
              acDs1ThresholdFEDayPCVs              INTEGER,
              acDs1ThresholdFEDayBESs              INTEGER,
              acDs1ThresholdFEDayDMs               INTEGER
     }


     acDs1ThresholdNodeId OBJECT-TYPE
          SYNTAX  AcNodeId
          MAX-ACCESS  accessible-for-notify
          STATUS  current
          DESCRIPTION
                 "The node id is the id for this specific node in the OSAP 
                 ring."
          ::= { acDs1ThresholdEntry 1 }

     acDs1ThresholdSlot OBJECT-TYPE
          SYNTAX  AcSlotNumber
          MAX-ACCESS  accessible-for-notify
          STATUS  current
          DESCRIPTION
                 "The physical slot number of the port."
          ::= { acDs1ThresholdEntry 2 }

     acDs1ThresholdPort OBJECT-TYPE
          SYNTAX  AcPortNumber
          MAX-ACCESS  accessible-for-notify
          STATUS  current
          DESCRIPTION
                 "The physical port number of the port."
          ::= { acDs1ThresholdEntry 3 }

     acDs1ThresholdNEIntervalESs OBJECT-TYPE
          SYNTAX  INTEGER
          MAX-ACCESS  read-write
          STATUS  current
          DESCRIPTION
                 "The threshold value for interval number of Errored 
                 Seconds.  A value of 0 disables the threshold mechanism 
                 for this attribute."
          DEFVAL { 0 }
          ::= { acDs1ThresholdEntry 4 }

     acDs1ThresholdNEIntervalSESs OBJECT-TYPE
          SYNTAX  INTEGER
          MAX-ACCESS  read-write
