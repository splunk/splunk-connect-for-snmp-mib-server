-- 
--   Ds1 TRAPS
--
--

acDs1Traps   OBJECT IDENTIFIER ::= { acDs1 0 }


acDs1LineStatusChangeTrap NOTIFICATION-TYPE
    OBJECTS { acChassisCurrentTime, acChassisRingId, acDs1ConfigNodeId,
              acDs1ConfigSlot,
              acDs1ConfigPort,
              acDs1ConfigLineStatus, 
              acDs1ConfigLineStatusLastChange }
    STATUS  current
    DESCRIPTION
    "An acLogcialDs1LineStatusChange trap is sent when the value of an 
    instance acDs1ConfigLineStatus changes. It can be utilized by 
    an NMS to trigger polls."

    ::= { acDs1Traps 1 }

acDs1StatsResetTrap  NOTIFICATION-TYPE 
    OBJECTS { acChassisCurrentTime, acChassisRingId, acDs1ConfigNodeId,
              acDs1ConfigSlot,
              acDs1ConfigPort }
    STATUS  current
    DESCRIPTION
    "The  Ds1 statistics for this interface have been reset to zero (0) 
    which may effect long term trending functions monitoring this interface."
    
    ::= { acDs1Traps 2 }

acDs1CfgErrorTrap  NOTIFICATION-TYPE 
    OBJECTS { acChassisCurrentTime, acChassisRingId, acDs1ConfigNodeId,
              acDs1ConfigSlot,
              acDs1ConfigPort }
    STATUS  current
    DESCRIPTION
    "A configuration request to the  Ds1 configuration table for an 
    interface has been detected as invalid or corrupt."
    
    ::= { acDs1Traps 3 }


acDs1LinkDownTrap  NOTIFICATION-TYPE
    OBJECTS { acChassisCurrentTime, acChassisRingId, acDs1ConfigNodeId,
              acDs1ConfigSlot,
              acDs1ConfigPort }
    STATUS  current
    DESCRIPTION
    "The link for this physical ds1 interface has obtained sync."

    ::= { acDs1Traps 4 }

acDs1LinkUpTrap  NOTIFICATION-TYPE
    OBJECTS { acChassisCurrentTime, acChassisRingId, acDs1ConfigNodeId,
              acDs1ConfigSlot,
              acDs1ConfigPort }
    STATUS  current
    DESCRIPTION
    "The link for this physical ds1 interface has been lost."

    ::= { acDs1Traps 5 }

acDs1ExceededThresholdNEIntervalESsTrap NOTIFICATION-TYPE
    OBJECTS { acChassisCurrentTime, acChassisRingId, acDs1ThresholdNodeId,
              acDs1ThresholdSlot,
              acDs1ThresholdPort }
    STATUS  current
    DESCRIPTION
    "The threshold value speficed has been exceeded for this interface."

    ::= { acDs1Traps 7 }
        
acDs1ExceededThresholdNEIntervalSESsTrap NOTIFICATION-TYPE
    OBJECTS { acChassisCurrentTime, acChassisRingId, acDs1ThresholdNodeId,
              acDs1ThresholdSlot,
