SErrorcontrol ::= SEQUENCE {
	a152Selfid                              	DmiInteger,
	a152Fatalcount                          	DmiCounter,
	a152Majorcount                          	DmiCounter,
	a152Warningcount                        	DmiCounter,
	a152Errstatus                           	DmiInteger,
	a152Errstatustype                       	DmiInteger
}

