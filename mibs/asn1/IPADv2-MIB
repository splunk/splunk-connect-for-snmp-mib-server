    SYNTAX    IpadEndpointTableEntry
    MAX-ACCESS  not-accessible
    STATUS  current
    DESCRIPTION
        "An entry in the ipad endpoint table."
    INDEX   { ipadEndpointIndex }
    ::= { ipadEndpointTable 1 }

