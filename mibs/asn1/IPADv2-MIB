    ::= { ipadDLCITableEntry 6 }

ipadDLCIremoteEquipActive    OBJECT-TYPE
    SYNTAX  INTEGER
    {
    other(1),
    inactive(2),
    active(3),
    sosAlarm(4)
    }
    MAX-ACCESS  read-only
    STATUS  current
    DESCRIPTION
