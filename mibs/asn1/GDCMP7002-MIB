 --
 -- ********************************************************************
 -- GDC Dsx-1 Alarm Object Identifier Definitions
 -- ********************************************************************

 mp7002AlarmData                	OBJECT IDENTIFIER ::= { mp7002Alarms 1 }

 mp7002NoResponseAlm	OBJECT IDENTIFIER ::= { mp7002AlarmData 1  }
 mp7002DiagRxErrAlm	OBJECT IDENTIFIER ::= { mp7002AlarmData 2  }
 mp7002PowerUpAlm	OBJECT IDENTIFIER ::= { mp7002AlarmData 3  }
 mp7002NvRamCorrupt	OBJECT IDENTIFIER ::= { mp7002AlarmData 4  }
 mp7002UnitFailure      OBJECT IDENTIFIER ::= { mp7002AlarmData 5  }
 mp7002TimingLoss       OBJECT IDENTIFIER ::= { mp7002AlarmData 6  }
 mp7002LCV		OBJECT IDENTIFIER ::= { mp7002AlarmData 7  }
 mp7002LossOfSignal     OBJECT IDENTIFIER ::= { mp7002AlarmData 8  }
 mp7002LossOfFrame      OBJECT IDENTIFIER ::= { mp7002AlarmData 9  }
 mp7002AlarmIndSignal	OBJECT IDENTIFIER ::= { mp7002AlarmData 10 }
 mp7002NEES		OBJECT IDENTIFIER ::= { mp7002AlarmData 11 }
 mp7002NEBBE		OBJECT IDENTIFIER ::= { mp7002AlarmData 12 }
 mp7002NESES		OBJECT IDENTIFIER ::= { mp7002AlarmData 13 }
 mp7002NEUAS		OBJECT IDENTIFIER ::= { mp7002AlarmData 14 }
 mp7002FEES		OBJECT IDENTIFIER ::= { mp7002AlarmData 15 }
 mp7002FEBBE		OBJECT IDENTIFIER ::= { mp7002AlarmData 16 }
 mp7002FESES		OBJECT IDENTIFIER ::= { mp7002AlarmData 17 }
 mp7002FEUAS		OBJECT IDENTIFIER ::= { mp7002AlarmData 18 }
 mp7002RAI		OBJECT IDENTIFIER ::= { mp7002AlarmData 19 }

 -- ALARM                     Maskable            Valid interfaces         window/thres
 -- ====================================================================================
 -- mp7002NoResponseAlm		no			unit			no
 -- mp7002DiagRxErrAlm		no			unit			no
 -- mp7002PowerUpAlm		no			unit			no
 -- mp7002NvRamCorrupt		no			unit			no
 -- mp7002UnitFailure		no			unit			no
 -- mp7002TimingLoss		yes			unit			no
 -- mp7002LCV			yes			E1			no
 -- mp7002LossofSignal		yes			E1/E1X			no
 -- mp7002LossOfFrame		yes			E1/E1X			no
 -- mp7002Ais			yes			E1/E1X			no
 -- mp7002NEES			yes			E1/E1X			yes
 -- mp7002NEBBE			yes			E1/E1X			yes
 -- mp7002NESES			yes			E1/E1X			yes
 -- mp7002NEUAS			yes			E1/E1X			yes
 -- mp7002FEES			yes			E1/E1X			yes
 -- mp7002FEBBE			yes			E1/E1X			yes
 -- mp7002FESES			yes			E1/E1X			yes
 -- mp7002FEUAS			yes			E1/E1X			yes
 -- mp7002RAI			yes			E1			no
 -- 
 --
 -- ********************************************************************
 -- GDC MP7002 Alarm Configuration
 -- ********************************************************************

 mp7002NearEndAlarmConfigTable OBJECT-TYPE
     SYNTAX SEQUENCE OF Mp7002NearEndAlarmConfigEntry
     ACCESS not-accessible
     STATUS mandatory
     DESCRIPTION
	 "The mp7002NearEndAlarmConfigTable contains entries that configure alarm
	 reporting.  The structure of the table is such that alarm
	 configuration is supported on a unit and interface basis, and
	 then on an alarm type basis within the interface.  For
	 simplicity sake alarms, be they unit or interface
	 related, are represented in one table."
     ::= { mp7002Alarms 2 }

 mp7002NearEndAlarmConfigEntry OBJECT-TYPE
     SYNTAX Mp7002NearEndAlarmConfigEntry
     ACCESS not-accessible
     STATUS mandatory
     DESCRIPTION
	 "An entry in the MP7002 Alarm Configuration table."
     INDEX { mp7002NearEndAlarmConfigIndex, mp7002NearEndAlarmConfigIdentifier }
     ::= { mp7002NearEndAlarmConfigTable 1 }

 Mp7002NearEndAlarmConfigEntry ::=
     SEQUENCE {
	 mp7002NearEndAlarmConfigIndex
	     SCinstance,
	 mp7002NearEndAlarmConfigIdentifier
	     OBJECT IDENTIFIER,
	 mp7002NearEndAlarmCountWindow
	     INTEGER,
	 mp7002NearEndAlarmCountThreshold
	     INTEGER
     }

 mp7002NearEndAlarmConfigIndex OBJECT-TYPE
     SYNTAX SCinstance
     ACCESS read-only
     STATUS mandatory
     DESCRIPTION
	 "The index value which uniquely identifies the interface to
	 which this entry is applicable."
     ::= { mp7002NearEndAlarmConfigEntry 1 }

 --
 mp7002NearEndAlarmConfigIdentifier OBJECT-TYPE
     SYNTAX OBJECT IDENTIFIER
     ACCESS read-only
     STATUS mandatory
     DESCRIPTION
	 "The unique alarm identifier assigned to this alarm type.
	 The format of this identifier is an OBJECT IDENTIFIER that has
	 the following format: {iso(1) org(3) dod(6) internet(1)
	 private(4) enterprises(1) gdc(498) xxx(x) alarm(z) yyy(y) 
	 where xxx(x) is the administratively assigned family object 
	 identifier (z) is the object identifier for alarms in the family
	 defined MIB and yyy(y) is the administratively assigned alarm 
	 type identifier for this alarm."

     ::= { mp7002NearEndAlarmConfigEntry 2 }

 mp7002NearEndAlarmCountWindow OBJECT-TYPE
     SYNTAX INTEGER{
			disabled(1),
			last1sec(2),
			last10sec(3),
			last30sec(4),
			last1min(5),
			last15min(6),
			last1hr(7),
			last24hr(8),
			infinite(9)
		}
	ACCESS  read-write
	STATUS  mandatory
	DESCRIPTION
		"This variable sets/reads the alarm window.

		This window is used with the alarm threshold to determine
		how long the alarm should be active before reporting.

		Default: disabled(1)"
    ::= { mp7002NearEndAlarmConfigEntry 3 }

 mp7002NearEndAlarmCountThreshold OBJECT-TYPE
    SYNTAX INTEGER {
			thresGT1(1),
			thresGT3(2),
			thresGT10(3),
			thresGT100(4),
			thresGT1000(5),
			thresGT10000(6)
		}
	ACCESS  read-write
	STATUS  mandatory
	DESCRIPTION
		"This function sets/reads the alarm threshold criteria.

		This threshold is used along with the the alarm window to 
		determine the number of instances in a given time frame for
		an alarm to occur before the alarm is considered active.

		Default: thresGT1(1)"
    ::= { mp7002NearEndAlarmConfigEntry 4 }

 mp7002FarEndAlarmConfigTable OBJECT-TYPE
     SYNTAX SEQUENCE OF Mp7002FarEndAlarmConfigEntry
     ACCESS not-accessible
     STATUS mandatory
     DESCRIPTION
	 "The mp7002FarEndAlarmConfigTable contains entries that configure alarm
	 reporting.  The structure of the table is such that alarm
	 configuration is supported on a unit and interface basis, and
	 then on an alarm type basis within the interface.  For
	 simplicity sake alarms, be they unit or interface
	 related, are represented in one table."
     ::= { mp7002Alarms 3 }

 mp7002FarEndAlarmConfigEntry OBJECT-TYPE
     SYNTAX Mp7002FarEndAlarmConfigEntry
     ACCESS not-accessible
     STATUS mandatory
     DESCRIPTION
	 "An entry in the MP7002 Alarm Configuration table."
     INDEX { mp7002FarEndAlarmConfigIndex, mp7002FarEndAlarmConfigIdentifier }
     ::= { mp7002FarEndAlarmConfigTable 1 }

 Mp7002FarEndAlarmConfigEntry ::=
     SEQUENCE {
	 mp7002FarEndAlarmConfigIndex
	     SCinstance,
	 mp7002FarEndAlarmConfigIdentifier
	     OBJECT IDENTIFIER,
	 mp7002FarEndAlarmCountWindow
	     INTEGER,
	 mp7002FarEndAlarmCountThreshold
	     INTEGER
     }

 --
 mp7002FarEndAlarmConfigIndex OBJECT-TYPE
     SYNTAX SCinstance
     ACCESS read-only
     STATUS mandatory
     DESCRIPTION
	 "The index value which uniquely identifies the interface to
	 which this entry is applicable."
     ::= { mp7002FarEndAlarmConfigEntry 1 }

 mp7002FarEndAlarmConfigIdentifier OBJECT-TYPE
     SYNTAX OBJECT IDENTIFIER
     ACCESS read-only
     STATUS mandatory
     DESCRIPTION
	 "The unique alarm identifier assigned to this alarm type.
	 The format of this identifier is an OBJECT IDENTIFIER that has
	 the following format: {iso(1) org(3) dod(6) internet(1)
	 private(4) enterprises(1) gdc(498) xxx(x) alarm(z) yyy(y) 
	 where xxx(x) is the administratively assigned family object 
	 identifier (z) is the object identifier for alarms in the family
	 defined MIB and yyy(y) is the administratively assigned alarm 
	 type identifier for this alarm."

     ::= { mp7002FarEndAlarmConfigEntry 2 }

 mp7002FarEndAlarmCountWindow OBJECT-TYPE
     SYNTAX INTEGER{
			disabled(1),
			last1sec(2),
			last10sec(3),
			last30sec(4),
			last1min(5),
			last15min(6),
			last1hr(7),
			last24hr(8),
			infinite(9)
		}
	ACCESS  read-write
	STATUS  mandatory
	DESCRIPTION
		"This variable sets/reads the alarm window.

		This window is used with the alarm threshold to determine
		how long the alarm should be active before reporting.

		Default: disabled(1)"
    ::= { mp7002FarEndAlarmConfigEntry 3 }

 --
mp7002FarEndAlarmCountThreshold OBJECT-TYPE
    SYNTAX INTEGER {
			thresGT1(1),
			thresGT3(2),
			thresGT10(3),
			thresGT100(4),
			thresGT1000(5),
			thresGT10000(6)
		}
	ACCESS  read-write
	STATUS  mandatory
	DESCRIPTION
		"This function sets/reads the alarm threshold criteria.

		This threshold is used along with the the alarm window to 
		determine the number of instances in a given time frame for
		an alarm to occur before the alarm is considered active.

		Default: thresGT1(1)"
    ::= { mp7002FarEndAlarmConfigEntry 4 }

 --
 -- ********************************************************************
 -- GDC Local MP7002 Alarm Configuration
 -- ********************************************************************

 mp7002LocalAlarmConfigTable OBJECT-TYPE
     SYNTAX SEQUENCE OF Mp7002LocalAlarmConfigEntry
     ACCESS not-accessible
     STATUS mandatory
     DESCRIPTION
	 "The mp7002LocalAlarmConfigTable contains entries that configure 
	 alarm
	 reporting.  The structure of the table is such that alarm
	 configuration is supported on a unit and interface basis, and
	 then on an alarm type basis within the interface.  For
	 simplicity sake alarms, be they unit or interface
	 related, are represented in one table."
     ::= { mp7002Alarms 4 }

 mp7002LocalAlarmConfigEntry OBJECT-TYPE
     SYNTAX Mp7002LocalAlarmConfigEntry
     ACCESS not-accessible
     STATUS mandatory
     DESCRIPTION
	 "An entry in the GDC Local Alarm Configuration table."
     INDEX { mp7002LocalAlarmConfigIndex }
     ::= { mp7002LocalAlarmConfigTable 1 }

 Mp7002LocalAlarmConfigEntry ::=
     SEQUENCE {
	 mp7002LocalAlarmConfigIndex
	     SCinstance,
	 mp7002UASNE
	     INTEGER,
	 mp7002SESNE
	     INTEGER,
	 mp7002BBENE
	     INTEGER,
	 mp7002ESNE
	     INTEGER,
	 mp7002UASFE
	     INTEGER,
	 mp7002SESFE
	     INTEGER,
	 mp7002BBEFE
	     INTEGER,
	 mp7002ESFE
	     INTEGER,
	 mp7002LOS
	     INTEGER,
	 mp7002LOF
	     INTEGER,
	 mp7002AIS
	     INTEGER,
	 mp7002TmgLoss
	     INTEGER,
	 mp7002LCVs
	     INTEGER,
	 mp7002NtwkRAI
	     INTEGER
     }

 mp7002LocalAlarmConfigIndex OBJECT-TYPE
     SYNTAX SCinstance
     ACCESS read-only
     STATUS mandatory
     DESCRIPTION
	 "The index value which uniquely identifies the interface to
	 which this entry is applicable."
     ::= { mp7002LocalAlarmConfigEntry 1 }

 mp7002UASNE       OBJECT-TYPE
	SYNTAX  INTEGER {
			disabled(1),
			enabledMajor(2),
			enabledMinor(3)
			}
	ACCESS  read-write
	STATUS  mandatory
	DESCRIPTION
		"Disables or enables the alarm on the Major buss or Minor
		buss."
     ::= { mp7002LocalAlarmConfigEntry 2 }
 --

 mp7002SESNE       OBJECT-TYPE
