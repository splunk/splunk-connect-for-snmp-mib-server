    SYNTAX  INTEGER {
        enabled(1),
        disabled(2)
        }
    MAX-ACCESS  read-only
    STATUS  current
    DESCRIPTION
        "Enable/Disable Symantec IPS Processing per port."
    ::= { agCfgSecurityPortTableEntry 15 }

