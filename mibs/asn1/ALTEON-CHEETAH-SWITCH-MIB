    SYNTAX  Integer32
    MAX-ACCESS  read-only
    STATUS  current
    DESCRIPTION
        "The current memory of SP in bytes."
    ::= { spMemStatsTableEntry 3 }

spMemStatsAllocs OBJECT-TYPE
    SYNTAX  Counter32 
    MAX-ACCESS  read-only
    STATUS  current
    DESCRIPTION
        "The total number of dynamic memory allocations in SP."
    ::= { spMemStatsTableEntry 4 }

