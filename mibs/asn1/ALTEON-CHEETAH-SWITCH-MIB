spMemStatsTable OBJECT-TYPE
    SYNTAX  SEQUENCE OF SpMemStatsTableEntry 
    MAX-ACCESS  not-accessible
    STATUS  current
    DESCRIPTION
        "The table of SP memory statistics."
    ::= { spStats 3 }

spMemStatsTableEntry OBJECT-TYPE
    SYNTAX  SpMemStatsTableEntry
    MAX-ACCESS  not-accessible
    STATUS  current
    DESCRIPTION
        "A row in the SP memory statistics table."
    INDEX   { spMemStatsIndex }
    ::= { spMemStatsTable 1 }

SpMemStatsTableEntry ::= SEQUENCE {
    spMemStatsIndex             Integer32,
    spMemStatsTotal             Integer32,
    spMemStatsCurr              Integer32,
    spMemStatsAllocs            Counter32,
    spMemStatsFrees             Counter32,
    spMemStatsAllocsFailures    Counter32,
    spMemStatsHiWat             Integer32 
    }

