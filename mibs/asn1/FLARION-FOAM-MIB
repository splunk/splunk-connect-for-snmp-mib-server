    MAX-ACCESS          read-only
    STATUS              current
    DESCRIPTION         "MCU serial number."
    ::= { foamMcuEntry 6 }

foamMcuProductionDate   OBJECT-TYPE
    SYNTAX              DisplayString
    MAX-ACCESS          read-only
    STATUS              current
    DESCRIPTION         "MCU production date."
    ::= { foamMcuEntry 7 }

