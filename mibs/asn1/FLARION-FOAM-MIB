    SYNTAX              INTEGER
    MAX-ACCESS          read-only
    STATUS              current
    DESCRIPTION         "Sector Id."
    ::= {foamDistributionDelayEntry 1}

foamForwardDisDelay     OBJECT-TYPE
    SYNTAX              INTEGER (0..1024)
    UNITS               "1/8chip"
    MAX-ACCESS          read-only
    STATUS              current
    DESCRIPTION         "Forward distribution delay."
    ::= {foamDistributionDelayEntry 2}

foamReverseDisDelay     OBJECT-TYPE
    SYNTAX              INTEGER (0..1024)
    UNITS               "1/8chip"
    MAX-ACCESS          read-only
    STATUS              current
    DESCRIPTION         "Reverse distribution delay."
    ::= {foamDistributionDelayEntry 3}


