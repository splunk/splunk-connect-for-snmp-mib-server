    SYNTAX                  INTEGER
    MAX-ACCESS              read-only
    STATUS                  current
    DESCRIPTION             "MBP's even second signal is not synchronized."
    ::= {foamHsspc 6}

foamPLLNotLocked        OBJECT-TYPE
    SYNTAX              INTEGER
    MAX-ACCESS          read-only
    STATUS              current
    DESCRIPTION         "CEM Interconnect Board's 32 fc frequency synthesizer (PLL) 
                        is not locked to its 52 fc reference."
    ::= {foamHsspc 7}

