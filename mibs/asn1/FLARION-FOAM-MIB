    SYNTAX          INTEGER {
                            dcgReset(1),
                            mainbtenanceFromCraftperson(2),
                            faultDetectedByDCG(3),
                            spuriousReset(4),
                            foamHsspcLinkFailed(5),
                            dcgSWACT(6),
                            invalidOrUnknown(255)
                            }
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     "Reset reason from DCG."
    ::= {foamOam 7}


