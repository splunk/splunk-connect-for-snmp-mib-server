    SYNTAX           DisplayString
    MAX-ACCESS       read-only
    STATUS           current
    DESCRIPTION      "MCU part number."
    ::= { foamMcuEntry 8 }

foamMcuDescription   OBJECT-TYPE
    SYNTAX           DisplayString
    MAX-ACCESS       read-only
    STATUS           current
    DESCRIPTION      "MCU description."
