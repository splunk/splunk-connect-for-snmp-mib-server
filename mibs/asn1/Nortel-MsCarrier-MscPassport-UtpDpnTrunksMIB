utpDpnTrunksMIB OBJECT IDENTIFIER ::= { mscPassportMIBs 67 }










-- DpnGateway/n Utp
-- The Utp component provides the Utp Level2 link protocol, an 
-- acknowledged protocol that provides selective retransmission of 
-- frames. A Framer subcomponent is automatically added providing 
-- the underlying HDLC framing and associated hardware that the Utp 
-- component will use.

mscDpnGateUtp OBJECT IDENTIFIER ::= { mscDpnGate 2 }



mscDpnGateUtpRowStatusTable OBJECT-TYPE
   SYNTAX     SEQUENCE OF MscDpnGateUtpRowStatusEntry
   ACCESS     not-accessible
   STATUS     mandatory
   DESCRIPTION
     "This entry controls the addition and deletion of 
      mscDpnGateUtp components."
::= { mscDpnGateUtp 1 }

mscDpnGateUtpRowStatusEntry OBJECT-TYPE
   SYNTAX     MscDpnGateUtpRowStatusEntry
   ACCESS     not-accessible
   STATUS     mandatory
   DESCRIPTION
     "A single entry in the table represents a single 
      mscDpnGateUtp component."
   INDEX {
      mscDpnGateIndex,
      mscDpnGateUtpIndex }
::= { mscDpnGateUtpRowStatusTable 1 }

