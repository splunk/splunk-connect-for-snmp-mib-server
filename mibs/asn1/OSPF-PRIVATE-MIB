            cjnOspfLsaHdrAreaId
					AreaID,
	    	cjnOspfLsaHdrLsaType
			        INTEGER,
		    cjnOspfLsaHdrAdvRtrId
            		IpAddress,
            cjnOspfLsaHdrLsId
            		IpAddress,
        	cjnOspfLsaHdrLsaAge
            		INTEGER,
        	cjnOspfLsaHdrChecksum
            		INTEGER,
        	cjnOspfLsaHdrSequence
            		INTEGER
              }

    	cjnOspfLsaHdrAreaId OBJECT-TYPE
           SYNTAX   AreaID
           MAX-ACCESS   read-only
           STATUS   current
           DESCRIPTION
