    -- dot11PhyFHSSTable		::=  {dot11phy 4}

    -- dot11PhyDSSSTable		::=  {dot11phy 5}

    -- dot11PhyIRTable			::=  {dot11phy 6}

    -- dot11RegDomainsSupportedTable	::=  {dot11phy 7}
