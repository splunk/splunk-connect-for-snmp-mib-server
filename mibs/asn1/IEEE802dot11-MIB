	    dot11RxSTBCOptionEnabled			TruthValue,
	    dot11BeamFormingOptionImplemented		TruthValue,
	    dot11BeamFormingOptionEnabled		TruthValue,
	    dot11HighestSupportedDataRate		INTEGER,
	    dot11TxMCSSetDefined			TruthValue,
	    dot11TxRxMCSSetNotEqual			TruthValue,
	    dot11TxMaximumNumberSpatialStreamsSupported	INTEGER,
	    dot11TxUnequalModulationSupported		TruthValue }
	    
