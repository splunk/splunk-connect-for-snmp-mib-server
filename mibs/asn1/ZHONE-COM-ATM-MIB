                                zhoneAtmStatsTotalInitialCellsTx
                                        Counter64,
                                zhoneAtmStatsTotalFabricCellsTx
                                        Counter64,
                                zhoneAtmStatsTotalFinalCLP0CellsTx
                                        Counter64,
                                zhoneAtmStatsTotalFinalCLP1CellsTx
                                        Counter64
                         }

                -- 1.3.6.1.4.1.5504.4.2.2.5.1.1
