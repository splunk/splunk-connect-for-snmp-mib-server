        hcxCnfgBackupAction OBJECT-TYPE
                SYNTAX INTEGER {
                                nil(1),
                                backupToSaved(2),
				revertToSaved(3) }
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "
			This attribute is provided to allow a user to
                        trigger a backup of ComController configuration 
			data on to the boot server. backupToSaved is
			used to create a user controlled backup on NMAPS.
			revertToSaved is used retrieve a saved database
			from NMAPS."
                ::= { com21HcxControlGroup 21 }

        hcxCnfgBackupComplete NOTIFICATION-TYPE
                OBJECTS { hcxAlmSeverity, hcxEventLogTime,
                            hcxCnfgBackupResult }
                STATUS current
                DESCRIPTION "
                        This notification is generated whenever the CC
                        completes the configuration data backup. This an event 
                        with no clear condition. Only severity is WARNING."
                ::= { com21Traps 44 }

        hcxCnfgBackupResult OBJECT-TYPE
                SYNTAX INTEGER { 
				inprogress(1),
				success(2), 
                                serverNoResponse(3),
				writeFailed(4),
				uninitiated(5) }
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "
                        This attribute contains the result of the last
                        configuration data backup operation."
                ::= { com21HcxControlGroup 22 }

	hcxPhyConfigType OBJECT-TYPE
                SYNTAX INTEGER { 
				singleShelf(1), 
				fullConfig(2), 
				compactShelf(3) }
		MAX-ACCESS read-only
		STATUS current
                DESCRIPTION "
                        This attribute details the physical configuration
			type of the ComController."
                ::= { com21HcxControlGroup 23 }
 
        hcxNumOfShelves OBJECT-TYPE
                SYNTAX INTEGER (1..2)
		MAX-ACCESS read-only 
                STATUS current 
                DESCRIPTION " 
                        This attribute specifies the number of shelves
			in the system. Presently one or two."
                ::= { com21HcxControlGroup 24 }

	hcxBootpServer OBJECT-TYPE
                SYNTAX DisplayString (SIZE(32))
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "
                        This attribute contains bootp server hostname."
                ::= { com21HcxControlGroup 25 }
 
        hcxBootpFailed NOTIFICATION-TYPE
                OBJECTS { hcxAlmSeverity, hcxEventLogTime,
                            hcxBootpResult }
                STATUS current
                DESCRIPTION "
                        This notification is generated whenever the CC
                        fails to successfully complete a bootp operation.
                        Only severity is WARNING."
                ::= { com21Traps 45 }

        hcxBootpResult OBJECT-TYPE
                SYNTAX INTEGER {
                                inprogress(1),
                                success(2),
                                serverNoResponse(3),
                                serverUnknown(4),
				imageNotFound(5),
				versionMismatch(6),
                                serverConnectionLost(7) }
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "
                        This attribute contains the result of the last
                        bootp operation."
                ::= { com21HcxControlGroup 26 }
 
        hcxPowerFailDetected NOTIFICATION-TYPE
                OBJECTS { hcxAlmSeverity, hcxEventLogTime, hcxShelfId }
                STATUS current
                DESCRIPTION "
                        This notification is generated whenever a power
                        failure condition is detected. Default severity
                        is MAJOR."
                ::= { com21Traps 46 }
 
        hcxPowerFailClear NOTIFICATION-TYPE 
                OBJECTS { hcxAlmSeverity, hcxEventLogTime, hcxShelfId }
                STATUS current
                DESCRIPTION " 
                        This notification is generated whenever a power
                        failure condition is removed. Severity is
                        CLEARED." 
                ::= { com21Traps 47 }

        hcxFanFailDetected NOTIFICATION-TYPE 
                OBJECTS { hcxAlmSeverity, hcxEventLogTime, hcxShelfId }
                STATUS current
                DESCRIPTION " 
                        This notification is generated whenever a fan
                        failure condition is detected. Default severity 
                        is MAJOR." 
                ::= { com21Traps 48 }
  
        hcxFanFailClear NOTIFICATION-TYPE  
                OBJECTS { hcxAlmSeverity, hcxEventLogTime, hcxShelfId } 
                STATUS current
                DESCRIPTION "  
                        This notification is generated whenever a fan
                        failure condition is removed. Severity is 
                        CLEARED."  
                ::= { com21Traps 49 }

        hcxFreqHopPause OBJECT-TYPE
                SYNTAX INTEGER (0..300)
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "
                        This attribute defines the number of seconds
                        between a failed frequency hopping operation
                        and a subsequent attempt. If a given upstream
                        frequency is found to be in an error state, then
                        the HCX will cycle through the configured
                        alternates. If all these alternates are also
                        found to be in a degraded state, then the HCX
                        shall pause for the number of seconds defined
                        by this attribute before attempting another hop.
                        Default 30secs."
                ::= { com21HcxControlGroup 28 }

        hcxMaxDnstrmCBRAlloc OBJECT-TYPE
                SYNTAX INTEGER (0..100)
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "
                        This attribute defines the maximum percentage
			of downstream bandwidth that can be allocated
			to CBR traffic. This control mechanism will
			denied service to acquiring ComPorts if the
			addition of the configured CBR bandwidth 
			would exceed the defined percentage. Note
			that this is a limit. i.e. if this attribute
			is set to 50%, then UP TO half the bandwidth
			can be CBR. It does not mean that half WILL
			be CBR. 100%, the default, means in effect 
			there is no control."
                ::= { com21HcxControlGroup 29 }

        hcxMaxUpstrmCBRAlloc OBJECT-TYPE 
                SYNTAX INTEGER (0..100)  
                MAX-ACCESS read-write  
                STATUS current
                DESCRIPTION "  
                        This attribute defines the maximum percentage   
                        of upstream bandwidth that can be allocated
                        to CBR traffic. Default 100."
                ::= { com21HcxControlGroup 30 }

        hcxStatsControl OBJECT-TYPE
                SYNTAX INTEGER { previousCounts(1), wrapCurr(2) }
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "
                        This attribute controls the statistics collection
			mechanism. If previousCounts is selected then the
			current stats shall be copied into the previous
			stats counts every 15minutes. If wrapCurr is
			selected, then there shall be no copying in to
			previous counts and the current counters shall
			wrap. If NMAPS is utilized then previousCounts
			MUST be selected. Default previousCounts."
                ::= { com21HcxControlGroup 33 }

	hcxTrapFormat OBJECT-TYPE 
                SYNTAX INTEGER { snmpv1 (1), snmpv2 (2) }
		MAX-ACCESS read-write 
                STATUS current 
                DESCRIPTION "  
                        specifies the format of the traps generated by the
			Common Controller. Default is snmpv1."
                ::= { com21HcxControlGroup 35 }

        hcxSyncClkConfigure OBJECT-TYPE
                SYNTAX INTEGER { internalClk (1), networkClk (2) }
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "
                        specifies the clock used for synchronization.
                        if networkClk is selected, one of the
                        OC3 card should be selected as the primary
                        source. Optionally secondary source can
                        also be selected. The behaviour is as follows:
 
                        If no Oc3 card exists
                                ClockSelect is internalClk
                        if priOc3Card is Online
                                ClockSelect is priNetworkClk
                        if secOc3Card is Online and priOc3Card is Offline
                                ClockSelect is secNetworkClk
 
                        Default is internalClk."
                ::= { com21HcxControlGroup 36 }
 
 
        hcxSyncClkSelect OBJECT-TYPE
                SYNTAX INTEGER { internalClk (1),
                                 priNetworkClk (2),
                                 secNetworkClk (3) }
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "
                        specifies the clock selected for
                        synchronization.
                        Default is internalClk."
                ::= { com21HcxControlGroup 37 }
 
        hcxSyncClkChange NOTIFICATION-TYPE
                OBJECTS { hcxAlmSeverity, hcxEventLogTime,
                          hcxSyncClkSelect }
                STATUS current
                DESCRIPTION "
                        This notification is generated whenever
                        there is a change in Clock
                        used for Synchronization "
                ::= { com21Traps 181 }




        -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- --
        -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- --
        --
        -- com21HcxAcqCnfgGroup
        --
        -- defines the attributes related to the acquisition of ComPorts. It
	-- is anticipated that these values should be set only by COM21 
	-- personnel.
        --
        -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- --
        -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- --

	hcxAcquisitionFreq OBJECT-TYPE
                SYNTAX UpstrmFreqKhz
                MAX-ACCESS read-write
                STATUS deprecated
                DESCRIPTION "
                        No longer supported."
                ::= { com21HcxAcqCnfgGroup 1 }

        hcxAcqMinPower OBJECT-TYPE
                SYNTAX StuGain
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "
                        Contains the minimum power setting to be used by
			the ComPort when hunting for upstream channel. This 
			data is used in the INVITE_CONFIG message. 
			Default 18."
                ::= { com21HcxAcqCnfgGroup 6 }

        hcxAcqMaxPower OBJECT-TYPE
                SYNTAX StuGain
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "
                        Contains the maximum power setting to be used by
			the ComPort when hunting for upstream channel. This 
			data is used in the INVITE_CONFIG message. Default
			58."
                ::= { com21HcxAcqCnfgGroup 7 }
 
         hcxAcqPowerStepSize OBJECT-TYPE
                SYNTAX INTEGER (1..20)
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "
                        Contains the power step size to be used by the
                        ComPort when hunting for upstream channel. This data 
			is used in the INVITE_CONFIG message. Default 1."
                ::= { com21HcxAcqCnfgGroup 8 }

        hcxAltAcqFreq OBJECT-TYPE
                SYNTAX INTEGER (0..40000)
                MAX-ACCESS read-write
                STATUS deprecated
                DESCRIPTION "
                        No longer supported."
                ::= { com21HcxAcqCnfgGroup 9 }

        hcxActiveAcqFreq OBJECT-TYPE
                SYNTAX UpstrmFreqKhz
                MAX-ACCESS read-only
                STATUS deprecated
                DESCRIPTION "
                        No longer supported."
                ::= { com21HcxAcqCnfgGroup 10 }

 
        -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- --
