mcafee_EVENT_INFECTION_ACESSDENIED	      TRAP-TYPE	
	ENTERPRISE mcafeeTVDTrap
    VARIABLES
    {
		naiTrapAgent,				
		naiTrapShortDescription,	
		naiTrapSeverity,            
		mcafee_SOURCEIP,		
		mcafee_GMTTIME,             
		naiTrapAgentVersion,
