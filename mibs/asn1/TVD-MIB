TVD-MIB DEFINITIONS ::= BEGIN

IMPORTS
    enterprises                        FROM RFC1155-SMI
    OBJECT-TYPE                        FROM RFC-1212
    TRAP-TYPE                          FROM RFC-1215
    DisplayString 		       FROM RFC1213-MIB
    nai,
	naiTrapAgent,				
	naiTrapShortDescription,	
	naiTrapSeverity,            
	naiTrapAgentVersion,
	naiTrapLongDescription,		 
	naiTrapProblemResolution,	
	naiTrapDiagID,              
	naiTrapURL,                 
	naiTrapSourceDNSName,		
	naiTrapTargetDNSName		FROM NAI-MIB;
 

-- 
-- McAfee MIB DEFINITION
-- 
-- mcafee was assigned the new ID of 12 (as opposed to the older 4)
-- for SNMP structures conforming to the NAI and mcafee standards


mcafee					OBJECT IDENTIFIER ::= { nai 12 }
-- mcafeeTVDTrapBase			OBJECT IDENTIFIER ::= { mcafee 0 }
mcafeeTVDTrap				OBJECT IDENTIFIER ::= { mcafee 0 }
mcafeeStandardTrapField			OBJECT IDENTIFIER ::= { mcafee 1 }

--
-- The following trap is a sample that contains all of the nai standard 
-- trap fields. 



-- 
-- mcafee MIB DEFINITION
-- 
-- mcafee was assigned the new ID of 12 (as opposed to the older 4)
-- for SNMP structures conforming to the NAI and mcafee standards

--  
 
--mcafeeFields MODULE-IDENTITY
--	LAST-UPDATED  "Wed August 1, 1999"
--    ORGANIZATION "Network Associates Inc. mcafee Div."
--    CONTACT-INFO
--    	"Postal:   Network Associates, Inc.
--                   3965 Freedom Circle
--                   Santa Clara, CA  95054
--        Phone:     +1 408 988-3832
--        Internet:  www.nai.com"
--	DESCRIPTION
--    	"mcafee agent MIB definition"
--        	::= { mcafee 1 }
--
--

--
	-- The nai standard trap group
	-- This group contains objects describing NAI standard alarm variable 
	-- definitions which shall be included in each product's trap extension
	-- resulting in transmission of a SNMP trap message.



mcafee_TRAPID  OBJECT-TYPE	
	SYNTAX	INTEGER
	ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
    "mcafee Event/Alert Id number"		
	::= { mcafeeStandardTrapField 1 }             


mcafee_ENGINEVERSION OBJECT-TYPE		        
        SYNTAX          DisplayString
        ACCESS          read-only
        STATUS          mandatory
        DESCRIPTION
	  "Version string for the current Av Scanner Engine"
            ::= { mcafeeStandardTrapField 2 }

mcafee_DATVERSION OBJECT-TYPE		        
        SYNTAX          DisplayString
        ACCESS          read-only
        STATUS          mandatory
        DESCRIPTION
	  "Version string for the current Av DAT File"
        ::= { mcafeeStandardTrapField 3 }

mcafee_ENGINESTATUS OBJECT-TYPE
	SYNTAX  INTEGER
	ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
		"The status at the end of the last engine scan"
    ::= { mcafeeStandardTrapField 4  }

mcafee_VIRUSNAME OBJECT-TYPE		        
    SYNTAX          DisplayString
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION
	"Name of virus detected"
     ::= { mcafeeStandardTrapField 5 }
    
mcafee_VIRUSTYPE	OBJECT-TYPE	
    SYNTAX          DisplayString
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION
	"Type of infection (virus/trojan/etc)"
	 ::= { mcafeeStandardTrapField 6 }
	 
mcafee_FILENAME	OBJECT-TYPE	
    SYNTAX          DisplayString
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION
	"File name and path"
	 ::= { mcafeeStandardTrapField 7 }
	 
mcafee_USERNAME	OBJECT-TYPE	
    SYNTAX          DisplayString
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION
	"User Name"
	 ::= { mcafeeStandardTrapField 8 }   
	 
mcafee_OS	OBJECT-TYPE	
    SYNTAX          DisplayString
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION
	"Operating System and Version"
	 ::= { mcafeeStandardTrapField 9 }
	 
mcafee_PROCESSORSERIAL	OBJECT-TYPE	
    SYNTAX          DisplayString
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION
	"Processor Serial number"
	 ::= { mcafeeStandardTrapField 10 }
	 
mcafee_TASKNAME	OBJECT-TYPE	
    SYNTAX          DisplayString
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION
	"Sub Task Name"
	 ::= { mcafeeStandardTrapField 11 }

mcafee_NUMVIRS		OBJECT-TYPE	
	SYNTAX  INTEGER
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
    "Number of infections detected."
	 ::= { mcafeeStandardTrapField 15 }
	 
mcafee_NUMCLEANED	OBJECT-TYPE	
	SYNTAX  INTEGER
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
    "Number of infections cleaned."
	 ::= { mcafeeStandardTrapField 16 }
	 
mcafee_NUMDELETED		OBJECT-TYPE	
	SYNTAX  INTEGER
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
    "Number of infected files deleted."
	 ::= { mcafeeStandardTrapField 17 }
	 
mcafee_NUMQUARANTINED	OBJECT-TYPE	
	SYNTAX  INTEGER
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
    "Number of viruses quarantined."
	 ::= { mcafeeStandardTrapField 18 }
	 
mcafee_SCANRETURNCODE	OBJECT-TYPE	
	SYNTAX  INTEGER
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
    "Return Code from Scan Engine"
	 ::= { mcafeeStandardTrapField 19 }
	 
mcafee_MAILFROMNAME	OBJECT-TYPE	
    SYNTAX          DisplayString
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION
	"Mail From Field"
	 ::= { mcafeeStandardTrapField 30 }
	 
mcafee_MAILTONAME	OBJECT-TYPE	
    SYNTAX          DisplayString
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION
	"Mail To Field Contents"
	 ::= { mcafeeStandardTrapField 31 }
	 
mcafee_MAILCCNAME	OBJECT-TYPE	
    SYNTAX          DisplayString
    ACCESS          read-only
    STATUS          mandatory
    DESCRIPTION
	"Mail CC Name"
	 ::= { mcafeeStandardTrapField 32 }
	 
mcafee_MAILSUBJECTLINE		OBJECT-TYPE	
    SYNTAX          DisplayString
    ACCESS          read-only
    STATUS          optional
    DESCRIPTION
	"Mail Subject Line"
	 ::= { mcafeeStandardTrapField 33 }
	 
mcafee_MAILIDENTIFIERINFO	OBJECT-TYPE	
    SYNTAX          DisplayString
    ACCESS          read-only
    STATUS          optional
    DESCRIPTION
	"Mail item identifier info"
	 ::= { mcafeeStandardTrapField 34 }

mcafee_NOTEID	OBJECT-TYPE	
    SYNTAX          DisplayString
    ACCESS          read-only
    STATUS          optional
    DESCRIPTION
	"Notes identifier info"
	 ::= { mcafeeStandardTrapField 35 }

mcafee_NOTESSERVERNAME	OBJECT-TYPE	
    SYNTAX          DisplayString
    ACCESS          read-only
    STATUS          optional
    DESCRIPTION
	"Notes Server Name"
	 ::= { mcafeeStandardTrapField 36 }

mcafee_NOTESDBNAME	OBJECT-TYPE	
    SYNTAX          DisplayString
    ACCESS          read-only
    STATUS          optional
    DESCRIPTION
	"Notes Database Name "
	 ::= { mcafeeStandardTrapField 37 }

mcafee_DOMAIN	OBJECT-TYPE	
    SYNTAX          DisplayString
    ACCESS          read-only
    STATUS          optional
    DESCRIPTION
	"Domain Name."
	 ::= { mcafeeStandardTrapField 38 }

mcafee_OBRULE	OBJECT-TYPE	
    SYNTAX          DisplayString
    ACCESS          read-only
    STATUS          optional
    DESCRIPTION
	"OB Rule "
	 ::= { mcafeeStandardTrapField 39 }

mcafee_LANGUAGECODE	OBJECT-TYPE	
    SYNTAX          DisplayString
    ACCESS          read-only
    STATUS          optional
    DESCRIPTION
	"Language Code of Machine where Event Originated"
	 ::= { mcafeeStandardTrapField 40 }
		
mcafee_CLIENTCOMPUTER	OBJECT-TYPE	
    SYNTAX          DisplayString
    ACCESS          read-only
    STATUS          optional
    DESCRIPTION
	"Name of Terminal Services Client Computer"
	 ::= { mcafeeStandardTrapField 41 }


mcafee_TSCLIENTID	OBJECT-TYPE	
    SYNTAX          INTEGER
    ACCESS          read-only
    STATUS          optional
    DESCRIPTION
	"Terminal Services Client Id number"
	 ::= { mcafeeStandardTrapField 42 }

mcafee_ACCESSPROCESSNAME	OBJECT-TYPE	
    SYNTAX          DisplayString
    ACCESS          read-only
    STATUS          optional
    DESCRIPTION
	"Name of Process triggering on-access scan"
	 ::= { mcafeeStandardTrapField 43 }

mcafee_EVENTNAME	OBJECT-TYPE	
    SYNTAX          DisplayString
    ACCESS          read-only
    STATUS          optional
    DESCRIPTION
	"Name of this event for McAfee Diagnostics"
	 ::= { mcafeeStandardTrapField 44 }

mcafee_GMTTIME	OBJECT-TYPE	
    SYNTAX          DisplayString
    ACCESS          read-only
    STATUS          optional
    DESCRIPTION
	"GMT time at the time the event was sent"
	 ::= { mcafeeStandardTrapField 45 }

mcafee_TIME	OBJECT-TYPE	
    SYNTAX          DisplayString
    ACCESS          read-only
    STATUS          optional
    DESCRIPTION
	"Local Time when the event was sent"
	 ::= { mcafeeStandardTrapField 46 }

mcafee_SOURCEIP	OBJECT-TYPE	
    SYNTAX          DisplayString
    ACCESS          read-only
    STATUS          optional
    DESCRIPTION
	"Source IP where event was generated"
	 ::= { mcafeeStandardTrapField 47 }

mcafee_TARGETIP	OBJECT-TYPE	
    SYNTAX          DisplayString
    ACCESS          read-only
    STATUS          optional
    DESCRIPTION
	"IP of machine which the infection occurred if different from Source IP"
	 ::= { mcafeeStandardTrapField 48 }


-- mcafee TRAP DEFINITIONS
--
-- sample with all fields NOTE all mcafee traps for 4.5 will have all fields




mcafee_EVENT_TEST_TRAP TRAP-TYPE
	ENTERPRISE mcafeeTVDTrap
	VARIABLES
	{
		naiTrapAgent,				
		naiTrapShortDescription,	
		naiTrapSeverity,            
		mcafee_SOURCEIP,		
		mcafee_GMTTIME,             
		naiTrapAgentVersion,
		naiTrapLongDescription,		 
		naiTrapProblemResolution,	
		naiTrapDiagID,              
		naiTrapURL,                 
		naiTrapSourceDNSName,		

		mcafee_TIME,           
		mcafee_TARGETIP,
		naiTrapTargetDNSName,
		
		mcafee_TRAPID,             
		mcafee_ENGINEVERSION,
		mcafee_DATVERSION,
		mcafee_ENGINESTATUS,
		mcafee_VIRUSNAME,
		mcafee_VIRUSTYPE,         
		mcafee_FILENAME,         
		mcafee_USERNAME,          
		mcafee_OS,          
		mcafee_PROCESSORSERIAL,
		
		mcafee_NUMVIRS,  
		mcafee_NUMCLEANED,
		mcafee_NUMDELETED,
		mcafee_NUMQUARANTINED,
		mcafee_SCANRETURNCODE,

		mcafee_MAILFROMNAME,   
		mcafee_MAILTONAME,
		mcafee_MAILCCNAME,
		mcafee_MAILSUBJECTLINE,
		mcafee_MAILIDENTIFIERINFO,

		mcafee_LANGUAGECODE,
		mcafee_CLIENTCOMPUTER,
		mcafee_TSCLIENTID,
		mcafee_ACCESSPROCESSNAME,

		mcafee_NOTEID,
		mcafee_NOTESSERVERNAME,
		mcafee_NOTESDBNAME,
		mcafee_DOMAIN,
		mcafee_OBRULE,
		mcafee_EVENTNAME
	}
	DESCRIPTION
		"mcafee Alert Manager Test Trap"
	::= 9999



