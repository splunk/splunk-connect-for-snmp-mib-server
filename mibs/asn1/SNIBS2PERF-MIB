sm2SDeviceStatus OBJECT-TYPE
	SYNTAX  INTEGER
		{
		  data-supported (1), -- data are supported and valid
		  data-invalid (2),   -- data are supported but invalid
		  prg-inactive (3),    -- measurement program inactive
		  no-subsystem (4),   -- subsystem is not running
		  sm2-not-running(5), -- subsystem SM2 not running
		  unkown (6)          -- program interface error
		}
	ACCESS  read-only
	STATUS  mandatory
	DESCRIPTION
	     "Status associated with Sampling-Device buffer"
	::= { sm2MpSDevice 1 }

sm2SDeviceRealNumber OBJECT-TYPE
	SYNTAX  INTEGER
		{ -- a non negative valid value OR
		     no-data (-1) -- at the moment no data available
		}                 -- or error conditions have occured
	ACCESS  read-only
	STATUS  mandatory
	DESCRIPTION
	     "Real number of available devices"
	::= { sm2MpSDevice 2 }

sm2SDeviceTabNumber OBJECT-TYPE
	SYNTAX  INTEGER
		{ -- a non negative valid value OR
		     no-data (-1) -- at the moment no data available
		}                 -- or error conditions have occured
	ACCESS  read-only
	STATUS  mandatory
	DESCRIPTION
	     "Number of entries in following table; default: 10 entries"
	::= { sm2MpSDevice 3 }

sm2SDeviceTab OBJECT-TYPE
	SYNTAX  SEQUENCE OF Sm2SDeviceTabEntry
	ACCESS  not-accessible
	STATUS  mandatory
	DESCRIPTION
	     "List of devices (terminals, fixed disks, IO-devices, ... );
	      sorted by 'sm2SDeviceTabIO' in descending order."
