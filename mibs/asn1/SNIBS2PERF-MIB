sm2MpTimeIO OBJECT IDENTIFIER ::= { sniSM2Mpoints  2 }
--

sm2TimeIOStatus OBJECT-TYPE
	SYNTAX  INTEGER
		{
		  data-supported (1), -- data are supported and valid
		  data-invalid (2),   -- data are supported but invalid
		  prg-inactive (3),    -- measurement program inactive
		  no-subsystem (4),   -- subsystem is not running
		  sm2-not-running(5), -- subsystem SM2 not running
		  unkown (6)          -- program interface error
		}
	ACCESS  read-only
	STATUS  mandatory
	DESCRIPTION
	     "Status associated with TIME IO buffer"
	::= { sm2MpTimeIO 1 }

sm2TimeIOActMach OBJECT-TYPE
	SYNTAX  INTEGER
		{ -- a non negative valid value OR
		     no-data (-1) -- at the moment no data available
		}                 -- or error conditions have occured
	ACCESS  read-only
	STATUS  mandatory
	DESCRIPTION
	     "Number of active logical machines"
	::= { sm2MpTimeIO 2 }

sm2TimeIOMachTabNumber OBJECT-TYPE
	SYNTAX  INTEGER
		{ -- a non negative valid value OR
		     no-data (-1) -- at the moment no data available
		}                 -- or error conditions have occured
	ACCESS  read-only
	STATUS  mandatory
	DESCRIPTION
	     "Number of entries in following machine table"
	::= { sm2MpTimeIO 3 }

sm2TimeIOMachTab OBJECT-TYPE
	SYNTAX  SEQUENCE OF Sm2TimeIOMachTabEntry
	ACCESS  not-accessible
	STATUS  mandatory
	DESCRIPTION
	     "List about all logical machines and their data"
	::= { sm2MpTimeIO 4 }

sm2TimeIOMachTabEntry OBJECT-TYPE
	SYNTAX  Sm2TimeIOMachTabEntry
	ACCESS  not-accessible
	STATUS  mandatory
	DESCRIPTION
	     "Contains data of a logical machine"
	INDEX { sm2TimeIOMachTabIndex }
	::= { sm2TimeIOMachTab 1 }

Sm2TimeIOMachTabEntry ::= SEQUENCE
	{
	  sm2TimeIOMachTabIndex       INTEGER,
	  sm2TimeIOMachTabIdleTime    INTEGER,
	  sm2TimeIOMachTabTUTime      INTEGER,
	  sm2TimeIOMachTabTPRTime     INTEGER,
	  sm2TimeIOMachTabSIHTime     INTEGER,
	  sm2TimeIOMachTabStopTime    INTEGER,
	  sm2TimeIOMachTabPagingIO    INTEGER,
	  sm2TimeIOMachTabDiskIO      INTEGER,
	  sm2TimeIOMachTabTapeIO      INTEGER,
	  sm2TimeIOMachTabPrinterIO   INTEGER,
	  sm2TimeIOMachTabOtherIO     INTEGER
	}

sm2TimeIOMachTabIndex OBJECT-TYPE
	SYNTAX  INTEGER
		{
		    mach1   (1),  -- logical machine number 1
		    mach2   (2),  -- logical machine number 2
		    mach3   (3),  -- logical machine number 3
		    mach4   (4),  -- logical machine number 4
		    mach5   (5),  -- logical machine number 5
		    mach6   (6),  -- logical machine number 6
		    mach7   (7),  -- logical machine number 7
		    mach8   (8),  -- logical machine number 8
		    mach9   (9),  -- logical machine number 9
		    mach10 (10),  -- logical machine number 10
		    mach11 (11),  -- logical machine number 11
		    mach12 (12),  -- logical machine number 12
		    mach13 (13),  -- logical machine number 13
		    mach14 (14),  -- logical machine number 14
		    mach15 (15),  -- logical machine number 15
		    mach16 (16),  -- logical machine number 16
		    average (100)  -- average values over all logical machines
		}
	ACCESS  read-only
	STATUS  mandatory
	DESCRIPTION
	     "The index starts with 1 and uniquely identifies each entry.
	      Entry 'average' (index 100) will contain average values
	      for all provided measurement times and sums for the
	      remaining io counters over all logical machines.
	      Since the table provides average/sum values
	      'sm2TimeIOMachTabNumber' is regularly by one higher
	      than 'sm2TimeIOActMach'."
	::= { sm2TimeIOMachTabEntry 1 }

sm2TimeIOMachTabIdleTime OBJECT-TYPE
	SYNTAX  INTEGER
		{ -- a non negative valid value OR
		     no-data (-1) -- at the moment no data available
		}                 -- or error conditions have occured
	ACCESS  read-only
	STATUS  mandatory
	DESCRIPTION
	     "Time in which the logical machine was inactive (not stopped)
	      in o/oo (share of time in parts per thousand)."
	::= { sm2TimeIOMachTabEntry 2 }

sm2TimeIOMachTabTUTime OBJECT-TYPE
	SYNTAX  INTEGER
		{ -- a non negative valid value OR
		     no-data (-1) -- at the moment no data available
		}                 -- or error conditions have occured
	ACCESS  read-only
	STATUS  mandatory
	DESCRIPTION
	     "TU time of the logical machine
	      in o/oo (share of time in parts per thousand)."
	::= { sm2TimeIOMachTabEntry 3 }

sm2TimeIOMachTabTPRTime OBJECT-TYPE
	SYNTAX  INTEGER
		{ -- a non negative valid value OR
		     no-data (-1) -- at the moment no data available
		}                 -- or error conditions have occured
	ACCESS  read-only
	STATUS  mandatory
	DESCRIPTION
	     "TPR time of the logical machine
	      in o/oo (share of time in parts per thousand)."
	::= { sm2TimeIOMachTabEntry 4 }

sm2TimeIOMachTabSIHTime OBJECT-TYPE
	SYNTAX  INTEGER
		{ -- a non negative valid value OR
		     no-data (-1) -- at the moment no data available
		}                 -- or error conditions have occured
	ACCESS  read-only
	STATUS  mandatory
	DESCRIPTION
	     "SIH time of the logical machine
	      in o/oo (share of time in parts per thousand)."
	::= { sm2TimeIOMachTabEntry 5 }

sm2TimeIOMachTabStopTime OBJECT-TYPE
	SYNTAX  INTEGER
		{ -- a non negative valid value OR
		     no-data (-1) -- at the moment no data available
		}                 -- or error conditions have occured
	ACCESS  read-only
	STATUS  mandatory
	DESCRIPTION
	     "Stop time of the logical machine
	      in o/oo (share of time in parts per thousand)."
	::= { sm2TimeIOMachTabEntry 6 }

sm2TimeIOMachTabPagingIO OBJECT-TYPE
	SYNTAX  INTEGER
		{ -- a non negative valid value OR
		     no-data (-1) -- at the moment no data available
		}                 -- or error conditions have occured
	ACCESS  read-only
	STATUS  mandatory
	DESCRIPTION
	     "Rate of paging IO concerning the logical machine
	      in (number of paging IOs per second) * 10"
	::= { sm2TimeIOMachTabEntry 7 }

sm2TimeIOMachTabDiskIO OBJECT-TYPE
	SYNTAX  INTEGER
		{ -- a non negative valid value OR
		     no-data (-1) -- at the moment no data available
		}                 -- or error conditions have occured
	ACCESS  read-only
	STATUS  mandatory
	DESCRIPTION
	     "Rate of disk IO concerning the logical machine
	      in (number of disk IOs per second) * 10"
	::= { sm2TimeIOMachTabEntry 8 }

sm2TimeIOMachTabTapeIO OBJECT-TYPE
	SYNTAX  INTEGER
		{ -- a non negative valid value OR
		     no-data (-1) -- at the moment no data available
		}                 -- or error conditions have occured
	ACCESS  read-only
	STATUS  mandatory
	DESCRIPTION
	     "Rate of tape IO concerning the logical machine
	      in (number of tape IOs per second) * 10"
	::= { sm2TimeIOMachTabEntry 9 }

sm2TimeIOMachTabPrinterIO OBJECT-TYPE
	SYNTAX  INTEGER
		{ -- a non negative valid value OR
		     no-data (-1) -- at the moment no data available
		}                 -- or error conditions have occured
	ACCESS  read-only
	STATUS  mandatory
	DESCRIPTION
	     "Rate of printer IO concerning the logical machine
	      in (number of printer IOs per second) * 10"
	::= { sm2TimeIOMachTabEntry 10 }

sm2TimeIOMachTabOtherIO OBJECT-TYPE
	SYNTAX  INTEGER
		{ -- a non negative valid value OR
		     no-data (-1) -- at the moment no data available
		}                 -- or error conditions have occured
	ACCESS  read-only
	STATUS  mandatory
	DESCRIPTION
	     "Rate of other IO concerning the logical machine
	      in (number of other IOs per second) * 10"
	::= { sm2TimeIOMachTabEntry 11 }

