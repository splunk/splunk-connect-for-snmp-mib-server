sm2UTMStatus OBJECT-TYPE
	SYNTAX  INTEGER
		{
		  data-supported (1), -- data are supported and valid
		  data-invalid (2),   -- data are supported but invalid
		  prg-inactive (3),    -- measurement program inactive
		  no-subsystem (4),   -- subsystem is not running
		  sm2-not-running(5), -- subsystem SM2 not running
		  unkown (6)          -- program interface error
		}
	ACCESS  read-only
	STATUS  mandatory
	DESCRIPTION
	     "Status associated with UTM buffer"
	::= { sm2MpUTM 1 }

