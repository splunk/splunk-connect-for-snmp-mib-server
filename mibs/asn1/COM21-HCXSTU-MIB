        com21HcxStuIpFiltStatsTable OBJECT-TYPE
                SYNTAX  SEQUENCE OF HcxStuIpFiltStatsEntry
                MAX-ACCESS  not-accessible
                STATUS  current
                DESCRIPTION "."
                ::= { com21HcxStuIpFiltStatsGroup 1 }

        com21HcxStuIpFiltStatsEntry OBJECT-TYPE
                SYNTAX  HcxStuIpFiltStatsEntry
                MAX-ACCESS  not-accessible
                STATUS  current
                DESCRIPTION "."
                INDEX { hcxStuIpFiltStatsMacAddr }
                ::= { com21HcxStuIpFiltStatsTable 1 }

        HcxStuIpFiltStatsEntry ::= SEQUENCE {
                hcxStuIpFiltStatsMacAddr        MacAddress,
                hcxStuIpCurrArpFiltStat         Gauge32,
                hcxStuIpCurrSrcFiltStat         Gauge32,
                hcxStuIpCurrDstFiltStat         Gauge32,
                hcxStuIpCurrBootpReqFiltStat    Gauge32,
                hcxStuIpCurrBootpReplyFiltStat  Gauge32,
                hcxStuIpCurrDhcpSnoopFiltStat   Gauge32,
                hcxStuL2CurrSnapFiltStat        Gauge32,
                hcxStuL2CurrNonSnapFiltStat     Gauge32,
                hcxStuL2CurrEnetFiltStat        Gauge32,
                hcxStuL2CurrArpIpv4FiltStat     Gauge32,
                hcxStuL2CurrIpv4FiltStat        Gauge32,
                hcxStuL2CurrIpv6FiltStat        Gauge32,
                hcxStuL2CurrIpxFiltStat         Gauge32,
                hcxStuL2CurrAppletalkFiltStat   Gauge32,
                hcxStuL2CurrOthersFiltStat      Gauge32,
                hcxStuIpCurrNetbiosFiltStat     Gauge32, 
                hcxStuIpPrevArpFiltStat         Gauge32,
                hcxStuIpPrevSrcFiltStat         Gauge32,
                hcxStuIpPrevDstFiltStat         Gauge32,
                hcxStuIpPrevBootpReqFiltStat    Gauge32,
                hcxStuIpPrevBootpReplyFiltStat  Gauge32,
                hcxStuIpPrevDhcpSnoopFiltStat   Gauge32,
                hcxStuL2PrevSnapFiltStat        Gauge32,
                hcxStuL2PrevNonSnapFiltStat     Gauge32,
                hcxStuL2PrevEnetFiltStat        Gauge32,
                hcxStuL2PrevArpIpv4FiltStat     Gauge32,
                hcxStuL2PrevIpv4FiltStat        Gauge32,
                hcxStuL2PrevIpv6FiltStat        Gauge32,
                hcxStuL2PrevIpxFiltStat         Gauge32,
                hcxStuL2PrevAppletalkFiltStat   Gauge32,
                hcxStuL2PrevOthersFiltStat      Gauge32,
                hcxStuIpPrevNetbiosFiltStat     Gauge32,
		hcxStuIpClearStats		Gauge }
 
        hcxStuIpFiltStatsMacAddr OBJECT-TYPE
                SYNTAX MacAddress
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "
                        Contains IEEE 802 medium access control address
                        of the ComPort device."
                ::= { com21HcxStuIpFiltStatsEntry 1 }
 
        hcxStuIpCurrArpFiltStat OBJECT-TYPE
                SYNTAX Gauge32
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "
                        Contains the number of ARP packets from this ComPort 
                        dropped due to authentication filtering in the
                        current 15 minute collection period."
                ::= { com21HcxStuIpFiltStatsEntry 2 }
 
