                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "
                        Contains table entry id."
                ::= { com21HcxStuDstIpAddrEntry 5 }
 

 
        -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- --
        -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- --
        --
        -- com21HcxStuIpFiltGroup
        --
        -- this group controls the filter operations performed for each ComPort.
	-- This includes various address, bootp and dhcp filtering.
        --
        -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- --
        -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- --

        com21HcxStuIpFiltTable OBJECT-TYPE
                SYNTAX SEQUENCE OF HcxStuIpFiltEntry
                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION "."
                ::= { com21HcxStuIpFiltGroup 1 }
 
        com21HcxStuIpFiltEntry OBJECT-TYPE
                SYNTAX HcxStuIpFiltEntry
                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION "."
                INDEX { hcxStuIpFiltMacAddr }
                ::= { com21HcxStuIpFiltTable 1 }

        HcxStuIpFiltEntry ::= SEQUENCE {
                hcxStuIpFiltMacAddr        MacAddress,
                hcxStuIpArpFiltEnable      INTEGER,
                hcxStuIpArpFiltInvalidAddr IpAddress,
                hcxStuIpArpFiltBadServAddr IpAddress,
		hcxStuIpSrcFiltEnable      INTEGER,
		hcxStuIpDstFiltEnable      INTEGER,
		hcxStuIpBootpReqFiltEnbl   INTEGER,
		hcxStuIpBootpReplyFiltEnbl INTEGER,
		hcxStuIpDhcpSnoopFiltEnbl  INTEGER,
                hcxStuL2SnapFiltEnbl       INTEGER,
                hcxStuL2NonSnapFiltEnbl    INTEGER,
                hcxStuL2EnetFiltEnbl       INTEGER,
                hcxStuL2ArpIpv4FiltEnbl    INTEGER,
                hcxStuL2Ipv4FiltEnbl       INTEGER,
                hcxStuL2Ipv6FiltEnbl       INTEGER,
                hcxStuL2IpxFiltEnbl        INTEGER,
                hcxStuL2AppletalkFiltEnbl  INTEGER,
                hcxStuL2OthersFiltEnbl     INTEGER,
                hcxStuIpNetbiosFiltEnbl    INTEGER }
 
        hcxStuIpFiltMacAddr OBJECT-TYPE
                SYNTAX MacAddress
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "
                        Contains IEEE 802 medium access control address
                        of the ComPort device."
                ::= { com21HcxStuIpFiltEntry 1 }

        hcxStuIpArpFiltEnable OBJECT-TYPE           
                SYNTAX INTEGER { true (1), false (2) }
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "
                        Specifies whether ARP authentication filtering
                        should be enabled for this ComPort. This filter
                        is applicable to 10BT, 100BT, and OC3 cards.
                        It filters traffic in the upstream direction.
                        Default is false.  This filter can also be
                        enabled for an entire VLAN via the MIB attribute
                        hcxVlanIpArpFiltEnbl."
                ::= { com21HcxStuIpFiltEntry 2 }
         
        hcxStuIpArpFiltInvalidAddr OBJECT-TYPE      
                SYNTAX IpAddress
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "
                        Contains the last invalid IP Address used by
                        the ComPort. This value is cleared on the quarter
                        hour."
                ::= { com21HcxStuIpFiltEntry 3 }
 
        hcxStuArpInvalidAddr NOTIFICATION-TYPE
                OBJECTS { hcxAlmSeverity, hcxEventLogTime,
			    hcxStuIpFiltMacAddr, hcxStuIpArpFiltInvalidAddr }
                STATUS current
                DESCRIPTION "
                        this trap is generated whenever an invalid ARP IP
			source address is detected. Severity is WARNING."
                ::= { com21Traps 106 }
 
        hcxStuIpArpFiltBadServAddr OBJECT-TYPE
                SYNTAX IpAddress
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "
                        Contains the last ComController Server IP Address
                        used by the ComPort. This value is cleared on the
                        quarter hour."
                ::= { com21HcxStuIpFiltEntry 4 }
 
        hcxStuArpBadServerAddr NOTIFICATION-TYPE
                OBJECTS { hcxAlmSeverity, hcxEventLogTime,
			    hcxStuIpFiltMacAddr, hcxStuIpArpFiltBadServAddr }
                STATUS current
                DESCRIPTION "
                        this trap is generated whenever the ComPort is
			detected as utilizing a reserved server address as
			its source IP address. Severity is WARNING."
                ::= { com21Traps 107 }

        hcxStuIpSrcFiltEnable  OBJECT-TYPE
                SYNTAX INTEGER { true (1), false (2) }
                MAX-ACCESS  read-write
                STATUS  current
                DESCRIPTION "
                        Specifies whether IP source address filtering
                        should be enabled for this ComPort. This filter
                        is applicable to 100BT, and OC3 cards.  It
                        is not applicable to 10BT card.  It filters
                        traffic in the upstream direction.  Default
                        is false.  This filter can also be enabled
                        for an entire VLAN via the MIB attribute
                        hcxVlanIpSrcFiltEnbl."
                ::= { com21HcxStuIpFiltEntry 5 }
 
        hcxStuIpDstFiltEnable  OBJECT-TYPE
                SYNTAX INTEGER { true (1), false (2) }
                MAX-ACCESS  read-write
                STATUS  current
                DESCRIPTION "
                        Specifies whether IP destination address
                        filtering should be enabled for this ComPort.
                        This filter is applicable to 100BT, and OC3
                        cards.  It is not applicable to to 10BT card.
                        It filters traffic in the upstream direction.
                        Default is false.  This filter can also be
                        enabled for an entire VLAN via the MIB
                        attribute hcxVlanIpDstFiltEnbl."
                ::= { com21HcxStuIpFiltEntry 6 }
 
        hcxStuIpBootpReqFiltEnbl  OBJECT-TYPE
                SYNTAX INTEGER { true (1), false (2) }
                MAX-ACCESS  read-write
                STATUS  deprecated
                DESCRIPTION "
                        Allows enabling or disabling BOOTP Request Filter
                        for this STU. 
			Default: FALSE"
                ::= { com21HcxStuIpFiltEntry 7 }

        hcxStuIpBootpReplyFiltEnbl  OBJECT-TYPE
                SYNTAX INTEGER { true (1), false (2) }
                MAX-ACCESS  read-write
                STATUS  current
                DESCRIPTION "
                        Specifies whether BOOTP replies, or DHCPOFFER,
                        or DHCPACK packets should be filtered for this
                        ComPort. This filter is applicable to 100BT, and
                        OC3 cards.  It it not applicable to 10BT card.
                        It filters traffic in the upstream direction.
                        Default is false.  This filter can also be
                        enabled for an entire VLAN via the MIB attribute
                        hcxVlanIpBootpReplyFiltEnbl."
                ::= { com21HcxStuIpFiltEntry 8 }
 
 
        hcxStuIpDhcpSnoopFiltEnbl  OBJECT-TYPE
                SYNTAX INTEGER { true (1), false (2) }
                MAX-ACCESS  read-write
                STATUS  current
                DESCRIPTION "
                        Specifies whether DHCP snooping should be
                        enabled for this ComPort. This filter is applicable
                        to 100BT, and OC3 cards.  It is not applicable
                        to 10BT card.  It snoops traffic in the downstream
                        direction for DHCPACK packets, and in the upstream
                        direction for DHCPDECLINE or DHCPRELEASE packets.
                        Dynamic IP address assigments thus learned are
                        stored in the valid source IP address table
                        (com21HcxStuSrcIpAddrGroup.) Default is false.
                        This filter can also be enabled for an entire
                        VLAN via the MIB attribute hcxVlanIpDhcpSnoopFiltEnbl."
                ::= { com21HcxStuIpFiltEntry 9 }

        hcxStuL2SnapFiltEnbl OBJECT-TYPE
                SYNTAX INTEGER { true (1), false (2) }
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "
                        Specifies whether SNAP-encoded IEEE 802.3 packets
                        should be filtered for this ComPort. This filter
                        is applicable to 100BT, and OC3 cards.  It is not
                        applicable to 10BT card.  It filters traffic
                        in both upstream and downstream directions.
                        Default is false.  This filter can also be
                        enabled for an entire VLAN via the MIB
                        attribute hcxVlanL2SnapFiltEnbl."
                ::= { com21HcxStuIpFiltEntry 10 }
 
        hcxStuL2NonSnapFiltEnbl OBJECT-TYPE
                SYNTAX INTEGER { true (1), false (2) }
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "
                        Specifies whether non-SNAP-encoded IEEE 802.3
                        packets should be filtered for this ComPort.
                        This filter is applicable to 10BT, 100BT, and
                        OC3 cards.  It filters traffic in both upstream
                        and downstream directions.  Default is false.
                        This filter can also be enabled for an entire
                        VLAN via the MIB attribute hcxVlanL2NonSnapFiltEnbl."
                ::= { com21HcxStuIpFiltEntry 11 }
 
        hcxStuL2EnetFiltEnbl OBJECT-TYPE
                SYNTAX INTEGER { true (1), false (2) }
                MAX-ACCESS read-write
                STATUS deprecated
                DESCRIPTION "
                        Specifies whether Ethernet packets should be 
			filtered for this ComPort.  This filter is applicable 
			to 100BT and  OC3 cards.  It is not applicable to 10BT 
			card.  It filters traffic in both upstream and 
			downstream directions.  Default is false.
                        This filter can also be enabled for an entire
                        VLAN via the MIB attribute hcxVlanL2EnetFiltEnbl."
                ::= { com21HcxStuIpFiltEntry 12 }
 
        hcxStuL2ArpIpv4FiltEnbl OBJECT-TYPE
                SYNTAX INTEGER { true (1), false (2) }
                MAX-ACCESS read-write
                STATUS deprecated
                DESCRIPTION "
                        Specifies whether ARP packets should be 
                        filtered for this ComPort.  This filter is applicable 
                        to 100BT and  OC3 cards.  It is not applicable to 10BT 
                        card.  It filters traffic in both upstream and 
                        downstream directions.  Default is false.
                        This filter can also be enabled for an entire  
                        VLAN via the MIB attribute hcxVlanL2ArpIpv4FiltEnbl."  
                ::= { com21HcxStuIpFiltEntry 13 }
 
        hcxStuL2Ipv4FiltEnbl OBJECT-TYPE
                SYNTAX INTEGER { true (1), false (2) }
                MAX-ACCESS read-write
                STATUS deprecated
                DESCRIPTION "
                        Specifies whether IPv4 packets should be 
                        filtered for this ComPort.  This filter is applicable 
                        to 100BT and  OC3 cards.  It is not applicable to 10BT 
