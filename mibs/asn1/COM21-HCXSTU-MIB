        hcxStuIpPrevBootpReqFiltStat OBJECT-TYPE
                SYNTAX Gauge32
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "
                        Contains the number of bootp Req packets from this 
                        ComPort dropped due to filtering in the previous 15 
			minute collection period."
                ::= { com21HcxStuIpFiltStatsEntry 11 }
 
        hcxStuIpPrevBootpReplyFiltStat OBJECT-TYPE
                SYNTAX Gauge32
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "
                        Contains the number of bootp Reply packets from this 
                        ComPort dropped due to filtering in the previous 15 
			minute collection period."
                ::= { com21HcxStuIpFiltStatsEntry 12 }
 
        hcxStuIpPrevDhcpSnoopFiltStat OBJECT-TYPE
                SYNTAX Gauge32
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "
                        Contains the number of DHCP Ack packets from this ComPort
                        snooped due to filtering in the previous 15 minute
                        collection period."
                ::= { com21HcxStuIpFiltStatsEntry 13 }

        hcxStuIpClearStats OBJECT-TYPE      
		SYNTAX INTEGER { nil(1), clear(2) }
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "
                        This attribute is used to clear all IP Filter
                        statistics counters in this group. This operation
                        is only possible if the hcxStatsControl attribute
                        is set to wrapCurr."
                ::= { com21HcxStuIpFiltStatsEntry 14 }


