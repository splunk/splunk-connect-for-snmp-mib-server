    SYNTAX          SEQUENCE OF PhyAlarmInputEntry
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION     ""
    ::= { rrPhyShelf 12 }

phyAlarmInputEntry  OBJECT-TYPE
    SYNTAX          PhyAlarmInputEntry
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION     ""
    INDEX           { phyAlarmInputIndex }
    ::= { phyAlarmInputTable 1 }

PhyAlarmInputEntry ::= SEQUENCE {
    phyAlarmInputIndex      INTEGER,
    phyAlarmInputEnable     TruthValue,
    phyAlarmInputName       DisplayString,
    phyAlarmInputState      TruthValue
}

phyAlarmInputIndex  OBJECT-TYPE
    SYNTAX          INTEGER
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     "AlarmInput index."
    ::= { phyAlarmInputEntry 1 }

phyAlarmInputEnable OBJECT-TYPE
    SYNTAX          TruthValue
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION     "AlarmInput enable."
    ::= { phyAlarmInputEntry 2 }

phyAlarmInputName   OBJECT-TYPE
    SYNTAX          DisplayString
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION     "AlarmInput name."
    ::= { phyAlarmInputEntry 3 }

phyAlarmInputState  OBJECT-TYPE
    SYNTAX          TruthValue
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     "AlarmInput state."
    ::= { phyAlarmInputEntry 4 }

--
-- Power Controller Unit (PCU) Table
