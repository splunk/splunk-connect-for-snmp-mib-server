--
-- RX Unit (RXU) Table
--
phyRxuTable         OBJECT-TYPE
    SYNTAX          SEQUENCE OF PhyRxuEntry
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION     ""
    ::= { rrPhyShelf 17 }

phyRxuEntry         OBJECT-TYPE
    SYNTAX          PhyRxuEntry
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION     ""
    INDEX           { phyRxuIndex }
    ::= { phyRxuTable 1 }

PhyRxuEntry ::= SEQUENCE {
    phyRxuIndex             INTEGER,
    phyRxuOpState           CardOpState,
    phyRxuFaultLed          CardFaultLed,
    phyRxuTempStatus        CardTempStatus,
    phyRxuVoltageStatus     CardVoltageStatus,
    phyRxuSerialNumber      DisplayString,
    phyRxuProductionDate    DisplayString,
    phyRxuPartNumber        DisplayString,
    phyRxuDescription       DisplayString,
