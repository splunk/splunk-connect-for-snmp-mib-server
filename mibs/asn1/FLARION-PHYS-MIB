    SYNTAX          CardFaultLed
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     "AIU fault LED color."
    ::= { phyAiuEntry 3 }

phyAiuTempStatus    OBJECT-TYPE
    SYNTAX          CardTempStatus
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     "AIU temperature status."
    ::= { phyAiuEntry 4 }

phyAiuVoltageStatus OBJECT-TYPE
    SYNTAX          CardVoltageStatus
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     "AIU voltage status."
    ::= { phyAiuEntry 5 }

phyAiuSerialNumber  OBJECT-TYPE
    SYNTAX          DisplayString
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     "AIU serial number."
    ::= { phyAiuEntry 6 }

