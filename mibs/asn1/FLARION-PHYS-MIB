    SYNTAX          DisplayString
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     "AIU production date."
    ::= { phyAiuEntry 7 }

phyAiuPartNumber    OBJECT-TYPE
    SYNTAX          DisplayString
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     "AIU part number."
    ::= { phyAiuEntry 8 }

phyAiuDescription   OBJECT-TYPE
    SYNTAX          DisplayString
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     "AIU description."
    ::= { phyAiuEntry 9 }

phyAiuHardwareVersion   OBJECT-TYPE
    SYNTAX          DisplayString
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     "AIU hardware version."
    ::= { phyAiuEntry 10 }

phyAiuIpmiSwVersion OBJECT-TYPE
    SYNTAX          DisplayString
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     "AIU IPMI software version."
    ::= { phyAiuEntry 11 }

phyAiuAssemblyId    OBJECT-TYPE
    SYNTAX          DisplayString
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     "AIU assembly ID."
    ::= { phyAiuEntry 12 }

phyAiuAssemblyIdSrc OBJECT-TYPE
    SYNTAX          AssemblyIdSource
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     "AIU assembly ID source."
    ::= { phyAiuEntry 13 }

phyAiuExcessiveFakeAid  OBJECT-TYPE
    SYNTAX          TruthValue
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     "This object is true(1) if the assembly ID is programmed
                    under more than one source."
    ::= { phyAiuEntry 14 }

phyAiuAdminState    OBJECT-TYPE
    SYNTAX          AdminState
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION     "AIU administrative state."
    ::= { phyAiuEntry 20 }

phyAiuRebootRr      OBJECT-TYPE
    SYNTAX          TruthValue
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION     "Setting this object to true(1) will power cycle the
                     RadioRouter chassis!"
    ::= { phyAiuEntry 21 }

