    phyAiuAssemblyId        DisplayString,
    phyAiuAssemblyIdSrc     AssemblyIdSource,
    phyAiuExcessiveFakeAid  TruthValue,
    phyAiuAdminState        AdminState,
    phyAiuRebootRr          TruthValue
}

