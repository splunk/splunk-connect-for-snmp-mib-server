-- $Id: FLARION-PHYS-MIB.txt,v 1.5 2004/07/14 20:29:51 csmith Exp $



FLARION-PHYS-MIB DEFINITIONS ::= BEGIN

IMPORTS
        MODULE-IDENTITY, OBJECT-TYPE, NOTIFICATION-TYPE,
        IpAddress, TimeTicks, Unsigned32, Counter32
                FROM SNMPv2-SMI

        enterprises
                FROM RFC1155-SMI

        DisplayString, RowStatus, DateAndTime, TruthValue
                FROM SNMPv2-TC

        AdminState, MoType, rrMib, rrManagedObjs, FloatAsString,
        rrBaseBandUnit
                FROM FLARION-MIB;


rrPhyShelf              OBJECT IDENTIFIER ::= { rrMib 3 }
rrPaShelf               OBJECT IDENTIFIER ::= { rrMib 4 }

rrChassis               OBJECT IDENTIFIER ::= { rrManagedObjs 8 }


-- chassis type
ChassisType ::= INTEGER {
    chassisUnknown(0),
    chassisCpciIndoor(1),
    chassisCpciOutdoor(2),
    chassisEnetIndoor(3),
    chassisEnetOutdoor(4)
}


-- Relay state
RelayState ::= INTEGER {
    relayUnknown(0),
    relayOpen(1),
    relayClosed(2)
}

-- Card operational state
CardOpState ::= INTEGER {
    cardOpStateUnknown(0),
    cardOpStateNotPresent(1),
    cardOpStateStandby(2),
    cardOpStateActive(3),
    cardOpStateDown(4)
}

-- Card fault LED colors
CardFaultLed ::= INTEGER {
    faultLedUnknown(0),
    faultLedRed(1),
    faultLedOrange(2),
    faultLedGreen(3)
}

-- Card temperature status
CardTempStatus ::= INTEGER {
    tempUnknown(0),
    tempNominal(1),
    tempHigh(2),
    tempSevere(3),
    tempCritical(4)
}

-- Card voltage status
CardVoltageStatus ::= INTEGER {
    voltageUnknown(0),
    voltageNominal(1),
    voltageLow(2),
    voltageHigh(3)
}

-- RF band
RfBand ::= INTEGER {
    rfBandUnknown(0),
    rfBandUpper700MHz(1),
    rfBandLow800MHz(2),
    rfBandCellBand(3),
    rfBandSmrBand(4),
    rfBandPcsKorea(5),
    rfBandPcsUs(6),
    rfBandUmts(7),
    rfBand2P3GHz(8),
    rfBandMmds(9)
}


-- PowerAmp RF band
PaRfBand ::= INTEGER {
    paRfBandUnknown(0),
    paRfBand747to770MHz(1),
    paRfBand861to894MHz(2),
    paRfBand1930to1990MHz(3),
    paRfBand2370to2400MHz(4),
    paRfBand2110to2170MHz(5),
    paRfBand2620to2686MHz(6),
    paRfBand740Mto746Hz(7)
}

-- PowerAmp Operational State
PaState ::= INTEGER {
    paStateUnknown(0),
    paStateProbing(1),
    paStateNotPresent(2),
    paStateDisabledByPa(3),
    paStateDisabledByBts(4),
    paStateEnabling(5),
    paStateEnabled(6),
    paStateDisabling(7)
}

-- Assembly Id source
AssemblyIdSource ::= INTEGER {
    aidSourceUnknown(0),
    aidSourceDatabase(1),
    aidSourceTarget(2)
}


--
-- Chassis
--
rrChassisType           OBJECT-TYPE
        SYNTAX          ChassisType
        MAX-ACCESS      read-only
        STATUS          deprecated
        DESCRIPTION     "The chassis revision."
        ::= { rrChassis 1 }

rrChassisRev            OBJECT-TYPE
        SYNTAX          DisplayString
        MAX-ACCESS      read-only
        STATUS          deprecated
        DESCRIPTION     "The chassis revision."
        ::= { rrChassis 2 }

rrChassisMaxSlots       OBJECT-TYPE
        SYNTAX          INTEGER
        MAX-ACCESS      read-only
        STATUS          deprecated
        DESCRIPTION     "Maximum number of slots in the chassis."
        ::= { rrChassis 3 }

rrChassisNumCards       OBJECT-TYPE
        SYNTAX          INTEGER
        MAX-ACCESS      read-only
        STATUS          deprecated
        DESCRIPTION     "Number of cards detected in the chassis."
        ::= { rrChassis 4 }

rrChassisTable          OBJECT-TYPE
        SYNTAX          SEQUENCE OF RrChassisTableEntry
        MAX-ACCESS      not-accessible
        STATUS          current
        DESCRIPTION     "Table containing chassis card information."
        ::= { rrChassis 5 }

rrChassisTableEntry     OBJECT-TYPE
        SYNTAX          RrChassisTableEntry
        MAX-ACCESS      not-accessible
        STATUS          current
        DESCRIPTION     "RadioRouter chassis card table entry."
        INDEX           { rrChassisSlotId }
        ::= { rrChassisTable 1 }

RrChassisTableEntry ::= SEQUENCE {
        rrChassisSlotId         INTEGER,
        rrChassisSlotHwType     MoType,
        rrChassisSlotHwName     DisplayString,
        rrChassisSlotHwStatus   CardOpState,
        rrChassisSlotHwVer      DisplayString,
        rrChassisSlotHwEsn      OCTET STRING,
        rrChassisSlotSwVer      DisplayString
}

rrChassisSlotId         OBJECT-TYPE
        SYNTAX          INTEGER
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION     "The chassis slot of the card."
        ::= { rrChassisTableEntry 1 }

rrChassisSlotHwType     OBJECT-TYPE
        SYNTAX          MoType
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION     "The card type."
        ::= { rrChassisTableEntry 2 }

rrChassisSlotHwName     OBJECT-TYPE
        SYNTAX          DisplayString
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION     "The card name."
        ::= { rrChassisTableEntry 3 }

rrChassisSlotHwStatus   OBJECT-TYPE
        SYNTAX          CardOpState
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION     "The card hardware operational status."
        ::= { rrChassisTableEntry 4 }

rrChassisSlotHwVer      OBJECT-TYPE
        SYNTAX          DisplayString
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION     "The card hardware version number."
        ::= { rrChassisTableEntry 5 }

rrChassisSlotHwEsn      OBJECT-TYPE
        SYNTAX          OCTET STRING (SIZE(8))
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION     "The card Electronic Serial Number."
        ::= { rrChassisTableEntry 6 }

rrChassisSlotSwVer      OBJECT-TYPE
        SYNTAX          DisplayString
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION     "The card IPMI (Intelligent Platform Management Interface)
                        firmware version number."
        ::= { rrChassisTableEntry 8 }


--
-- Physical Layout
--
phyChassisType      OBJECT-TYPE
    SYNTAX          ChassisType
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     "Chassis type."
    ::= { rrPhyShelf 1 }

phySerialNumber     OBJECT-TYPE
    SYNTAX          DisplayString
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     "Chassis serial number."
    ::= { rrPhyShelf 2 }

phyProductionDate   OBJECT-TYPE
    SYNTAX          DisplayString
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     "Chassis production date (mm/dd/yyyy)."
    ::= { rrPhyShelf 3 }

phyPartNumber       OBJECT-TYPE
    SYNTAX          DisplayString
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     "Chassis part number."
    ::= { rrPhyShelf 4 }

phyHardwareVersion  OBJECT-TYPE
    SYNTAX          DisplayString
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     "Chassis hardware version."
    ::= { rrPhyShelf 5 }

phyChassisRfBand    OBJECT-TYPE
    SYNTAX          RfBand
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     "Chassis RF band."
    ::= { rrPhyShelf 6 }

phyAssemblyId       OBJECT-TYPE
    SYNTAX          DisplayString
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     "Chassis assembly ID."
    ::= { rrPhyShelf 7 }

--
-- Fan Table
--
phyFanTable         OBJECT-TYPE
    SYNTAX          SEQUENCE OF PhyFanEntry
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION     ""
    ::= { rrPhyShelf 10 }

phyFanEntry         OBJECT-TYPE
    SYNTAX          PhyFanEntry
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION     ""
    INDEX           { phyFanIndex }
    ::= { phyFanTable 1 }

PhyFanEntry ::= SEQUENCE {
    phyFanIndex     INTEGER,
    phyFanOk        TruthValue
}

phyFanIndex         OBJECT-TYPE
    SYNTAX          INTEGER
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     "Fan index."
    ::= { phyFanEntry 1 }

phyFanOk            OBJECT-TYPE
    SYNTAX          TruthValue
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     "Fan status."
    ::= { phyFanEntry 2 }

--
-- Relay Table
--
phyRelayTable       OBJECT-TYPE
    SYNTAX          SEQUENCE OF PhyRelayEntry
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION     ""
    ::= { rrPhyShelf 11 }

phyRelayEntry       OBJECT-TYPE
    SYNTAX          PhyRelayEntry
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION     ""
    INDEX           { phyRelayIndex }
    ::= { phyRelayTable 1 }

PhyRelayEntry ::= SEQUENCE {
    phyRelayIndex   INTEGER,
    phyRelayName    DisplayString,
    phyRelayState   RelayState
}

phyRelayIndex       OBJECT-TYPE
    SYNTAX          INTEGER
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     "Relay index."
    ::= { phyRelayEntry 1 }

phyRelayName        OBJECT-TYPE
    SYNTAX          DisplayString
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION     "Relay name."
    ::= { phyRelayEntry 2 }

phyRelayState       OBJECT-TYPE
    SYNTAX          RelayState
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     "Relay state."
    ::= { phyRelayEntry 3 }

--
-- Alarm Input Table
--
