    SYNTAX          SEQUENCE OF PhyPcuEntry
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION     ""
    ::= { rrPhyShelf 13 }

phyPcuEntry         OBJECT-TYPE
    SYNTAX          PhyPcuEntry
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION     ""
    INDEX           { phyPcuIndex }
    ::= { phyPcuTable 1 }

PhyPcuEntry ::= SEQUENCE {
    phyPcuIndex             INTEGER,
    phyPcuPowerFail         TruthValue
}

phyPcuIndex         OBJECT-TYPE
    SYNTAX          INTEGER
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     "PCU index."
    ::= { phyPcuEntry 1 }

