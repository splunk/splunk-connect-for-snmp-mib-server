--
-- Power Amp Tx Power 
--
paPowerTable        OBJECT-TYPE
    SYNTAX          SEQUENCE OF PaPowerEntry
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION     "This table is used to display PA various Tx power values.
                    These values are store on the system as floating point
                    numbers, but since SNMP cannot display floating point
                    numbers, this table will convert the values to integer."
    ::= { rrPaShelf 7 }

paPowerEntry        OBJECT-TYPE
    SYNTAX          PaPowerEntry
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION     ""
    INDEX           { paPowerIndex }
    ::= { paPowerTable 1 }

PaPowerEntry ::= SEQUENCE {
    paPowerIndex        INTEGER,
    paPowerTxOutput     INTEGER,
    paPowerTxInput      INTEGER,
    paPowerTxReflected  INTEGER
}

paPowerIndex        OBJECT-TYPE
    SYNTAX          INTEGER
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     "PA index."
    ::= { paPowerEntry 1 }

paPowerTxOutput     OBJECT-TYPE
    SYNTAX          INTEGER
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     "PA output Tx power. In order to retain precision in
                    converting the floating point value into integer, the value                     is first multiplied by 100. For example 43.25dB will be
                    represented as 4325."
    ::= { paPowerEntry 2 }

paPowerTxInput      OBJECT-TYPE
    SYNTAX          INTEGER
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     "PA input Tx power. In order to retain precision in
                    converting the floating point value into integer, the value 
                    is first multiplied by 100. For example -50.75dB will be
                    represented as -5075."
    ::= { paPowerEntry 3 }

paPowerTxReflected  OBJECT-TYPE
    SYNTAX          INTEGER
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     "PA reflected Tx power. In order to retain precision in
                    converting the floating point value into integer, the value 
                    is first multiplied by 100. For example 21.00dB will be
                    represented as 2100."
    ::= { paPowerEntry 4 }

--
-- BBU System Table
--
bbuSysTable         OBJECT-TYPE
    SYNTAX          SEQUENCE OF BbuSysEntry
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION     "Base Band Unit system information."
    ::= { rrBaseBandUnit 5 }

bbuSysEntry         OBJECT-TYPE
    SYNTAX          BbuSysEntry
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION     "Base Band Unit system table entry."
    INDEX           { bbuSysId }
    ::= { bbuSysTable 1 }

BbuSysEntry ::= SEQUENCE {
    bbuSysId                INTEGER,
    bbuDspVer               DisplayString,
    bbuLlcVer               DisplayString,
    bbuTxFpgaVer            DisplayString,
    bbuRxFpgaVer            DisplayString,
    bbuLdpcVer              Unsigned32,
    bbuRfChanNum            INTEGER,
    bbuExtGainTx            INTEGER,
    bbuExtGainRx            INTEGER,
    bbuRfGainRx0            INTEGER,
    bbuRfGainRx1            INTEGER,
    bbuRfBand               INTEGER,
    bbuRfPowerTx0           INTEGER,
    bbuRfPowerTx1           INTEGER,
    bbuRfuEnableMask        INTEGER,
    bbuExtTxGain            FloatAsString,
    bbuExtRxGain            FloatAsString,
    bbuRfRx0Gain            FloatAsString,
    bbuRfRx1Gain            FloatAsString,
    bbuRfTx0Power           FloatAsString,
    bbuRfTx1Power           FloatAsString,
    bbuTxPwrDeviationLimit  INTEGER,
    bbuSlope                INTEGER,
    bbuMcastSnr             INTEGER,
    bbuScmType              INTEGER,
    bbuSleepInactivityTimer INTEGER,
    bbuLuTimer              INTEGER
}

bbuSysId            OBJECT-TYPE
    SYNTAX          INTEGER
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     "This identifies a single BBU in a basestation."
    ::= { bbuSysEntry 1 }

bbuDspVer           OBJECT-TYPE
    SYNTAX          DisplayString
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     "The read-only statistic indicates the version number of
                    the DSP software currently operational on the sector in
                    question. It has the major.minor.micro notation."
    ::= { bbuSysEntry 2 }

bbuLlcVer           OBJECT-TYPE
    SYNTAX          DisplayString
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     "The read-only statistic indicates the version number of
                    the Link List Controller software/firmware currently
                    operational on the sector in question. It has the
                    major.minor notation."
    ::= { bbuSysEntry 3 }

bbuTxFpgaVer        OBJECT-TYPE
    SYNTAX          DisplayString
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     "The read-only statistic indicates the version number of
                    the Tx FPGA software/firmware currently operational on the
                    sector in question. It has the  major.minor notation."
    ::= { bbuSysEntry 4 }

bbuRxFpgaVer        OBJECT-TYPE
    SYNTAX          DisplayString
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     "The read-only statistic indicates the version number of
                    the Rx FPGA software/firmware currently operational on the
                    sector in question. It has the  major.minor notation."
    ::= { bbuSysEntry 5 }

bbuLdpcVer          OBJECT-TYPE
    SYNTAX          Unsigned32
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     "The read-only statistic indicates the version number of
                    the LDPC encoder/decoder software/firmware currently
                    operational on the sector in question. It has the
                    major.minor notation."
    ::= { bbuSysEntry 6 }

bbuRfChanNum        OBJECT-TYPE
    SYNTAX          INTEGER (0..2000)
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION     "The RF channel number, in conjunction with the bbuRffBand
                    object defines the frequency of operation. The channel
                    number follows well-known ITU numbering, and represents
                    50 kHz spacing. The step size depends on the band. In the
                    1.9 GHz band, the step size is 4 (200 kHz). Editable
                    offline."
    DEFVAL          { 295 }
    ::= { bbuSysEntry 50 }

bbuExtGainTx        OBJECT-TYPE
    SYNTAX          INTEGER (-1600..2240)
    MAX-ACCESS      read-write
    STATUS          deprecated
    DESCRIPTION     "Represents the total external gain in the Tx path.
                    Typically it equals PA gain minus any attenuators.
                    The value is in dB * 32 (scaled by 32). E.g., default
                    value is 43 dB, which is the PA gain, and is represented as
                    43*32 = 1376. This parameter is used to assist the mobile
                    in making an access attempt at an appropriate power level.
                    The step size is 1 dB. Editable online."
    DEFVAL          { 1376 }
    ::= { bbuSysEntry 51 }

bbuExtGainRx        OBJECT-TYPE
    SYNTAX          INTEGER (-1600..2240)
    MAX-ACCESS      read-write
    STATUS          deprecated
    DESCRIPTION     "Represents the total external gain in the Rx path.
                    Typically it equals LNA gain minus any attenuators.
                    The value is in dB * 32 (scaled by 32). E.g., default
                    value is 31 dB, which is the LNA gain, and is represented
                    as 31*32 = 992. This parameter is used to assist the mobile
                    in making an access attempt at an appropriate power level.
                    The step size is 1 dB. Editable online."

    DEFVAL          { 992 }
    ::= { bbuSysEntry 52 }

bbuRfGainRx0        OBJECT-TYPE
    SYNTAX          INTEGER (-10..75)
    MAX-ACCESS      read-write
    STATUS          deprecated
    DESCRIPTION     "Represents the gain (in dB) provided by the RF Rx
                    blade(RXU) in the Rx0 path. This gain is in addition to the
                    gain provided by the LNA. This value is typically
                    calibrated and set in the factory. The step size is 1 dB.
                    Editable online."
    DEFVAL          { 42 }
    ::= { bbuSysEntry 53 }

bbuRfGainRx1        OBJECT-TYPE
    SYNTAX          INTEGER (-10..75)
    MAX-ACCESS      read-write
    STATUS          deprecated
    DESCRIPTION     "Represents the gain (in dB) provided by the RF Rx
                    blade(RXU) in the Rx1 path. This gain is in addition to the
                    gain provided by the LNA. This value is typically
                    calibrated and set in the factory. The step size is 1 dB.
                    Editable online."
    DEFVAL          { 42 }
    ::= { bbuSysEntry 54 }

bbuRfBand           OBJECT-TYPE
    SYNTAX          INTEGER (0..255)
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION     "The band of operation, represented by an enumeration.
                    E.g., the 1.9 GHz PCS band has a value of 6, while the
                    2.1 GHz UMTS band has a value of 7. This quantity, in
                    conjunction with the bbuRfChanNum decides the frequency
                    of operation. Editable offline."
    DEFVAL          { 0 }
    ::= { bbuSysEntry 55 }

bbuRfPowerTx0       OBJECT-TYPE
    SYNTAX          INTEGER (-20..10)
    MAX-ACCESS      read-write
    STATUS          deprecated
    DESCRIPTION     "Represents the signal strength in dBm of the Tx signal put
                    out on Tx0 by the TXU. An increase in this implies an
                    increase in the signal input to the PA. Step size is 1 dBm.
                    Editable online."
    DEFVAL          { 0 }
    ::= { bbuSysEntry 56 }

bbuRfPowerTx1       OBJECT-TYPE
    SYNTAX          INTEGER (-20..10)
    MAX-ACCESS      read-write
    STATUS          deprecated
    DESCRIPTION     "Represents the signal strength in dBm of the Tx signal put
                    out on Tx1 by the TXU. An increase in this implies an
                    increase in the signal input to the PA. Step size is 1 dBm.
                    Editable online."
    DEFVAL          { 0 }
    ::= { bbuSysEntry 57 }

bbuRfuEnableMask    OBJECT-TYPE
    SYNTAX          INTEGER (0..15)
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION     "This is a bit mask to choose between the 0 and 1 channels
                    for Rx and Tx. The value should be an OR of the channels
                    selected, where: Tx0 = 0001
                                     Tx1 = 0010
                                     Rx0 = 0100
                                     Rx1 = 1000
                    For example, set this object to 6 if Rx0 and Tx1 need to
                    be enabled."
    DEFVAL          { 15 }
    ::= { bbuSysEntry 59 }

bbuExtTxGain        OBJECT-TYPE
    SYNTAX          FloatAsString
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION     "External Tx gain. Valid range is -50.0 to 70.0."
    DEFVAL          { "43.0" }
    ::= { bbuSysEntry 60 }

bbuExtRxGain        OBJECT-TYPE
    SYNTAX          FloatAsString
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION     "External Rx gain. Valid range is -50.0 to 70.0."
    DEFVAL          { "31.0" }
    ::= { bbuSysEntry 61 }

bbuRfRx0Gain        OBJECT-TYPE
    SYNTAX          FloatAsString
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION     "Represents the gain (in dB) provided by the RF Rx
                    blade(RXU) in the Rx0 path. This gain is in addition to the
                    gain provided by the LNA. This value is typically
                    calibrated and set in the factory. The step size is 1 dB.
                    Valid range is -10.0 to 75.0. Editable online."
    DEFVAL          { "42" }
    ::= { bbuSysEntry 62 }

bbuRfRx1Gain        OBJECT-TYPE
    SYNTAX          FloatAsString
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION     "Represents the gain (in dB) provided by the RF Rx
                    blade(RXU) in the Rx1 path. This gain is in addition to the
                    gain provided by the LNA. This value is typically
                    calibrated and set in the factory. The step size is 1 dB.
                    Valid range is -10.0 to 75.0. Editable online."
    DEFVAL          { "42" }
    ::= { bbuSysEntry 63 }

bbuRfTx0Power       OBJECT-TYPE
    SYNTAX          FloatAsString
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION     "Represents the signal strength in dBm of the Tx signal put
                    out on Tx0 by the TXU. An increase in this implies an
                    increase in the signal input to the PA. Step size is 1 dBm.
                    Valid range is -20.0 to 10.0. Editable online."
    DEFVAL          { "0" }
    ::= { bbuSysEntry 64 }

bbuRfTx1Power       OBJECT-TYPE
    SYNTAX          FloatAsString
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION     "Represents the signal strength in dBm of the Tx signal put
                    out on Tx1 by the TXU. An increase in this implies an
                    increase in the signal input to the PA. Step size is 1 dBm.
                    Valid range is -20.0 to 10.0. Editable online."
    DEFVAL          { "0" }
    ::= { bbuSysEntry 65 }

bbuTxPwrDeviationLimit  OBJECT-TYPE
    SYNTAX          INTEGER (1..10)
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION     ""
    DEFVAL          { 2 }
    ::= { bbuSysEntry 66 }

bbuSlope            OBJECT-TYPE
    SYNTAX          INTEGER (1..112)
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION     "Represents the slope of the RR sector. It is an identifier
                    which allows a mobile to distinguish between signals from
                    different basestations. There are 112 possible slope values,
                    however, only 80 of these are considered valid values.
                    Editable offline."
    DEFVAL          { 10 }
    ::= { bbuSysEntry 70 }

bbuMcastSnr         OBJECT-TYPE
    SYNTAX          INTEGER (-20..35)
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION     ""
    DEFVAL          { 8 }
    ::= { bbuSysEntry 71 }

bbuScmType          OBJECT-TYPE
    SYNTAX          INTEGER (0..1)
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION     ""
    DEFVAL          { 1 }
    ::= { bbuSysEntry 72 }

bbuSleepInactivityTimer OBJECT-TYPE
    SYNTAX          INTEGER (0..3600)
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION     "Represents the amount of time of data inactivity (on the
                    forward or reverse links) after which a mobile moves to the
                    Sleep state. In units of 1/3 seconds [ eg. Value = 180
                    represents 60 seconds]. This time period is conveyed to the
                    mobile at the time of establishing connectivity with the
                    basestation. This quantity is non-linearly quantized, so
                    the actual duration of inactivity before sleep may be
                    slightly different from this value. Step size is 1/3 second.
                    Range is 0 to 20 minutes. 0 means never go to sleep.
                    Recommended value is 180 (i.e., 60sec). Editable online."
    DEFVAL          { 0 }
    ::= { bbuSysEntry 91 }

bbuLuTimer          OBJECT-TYPE
    SYNTAX          INTEGER (0..86400)
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION     "Periodicity, in seconds, of location update done by a
                    stationary mobile. This time period is conveyed to the
                    mobile at the time of establishing connectivity with the RR.
                    This quantity is non-linearly quantized, so the actual
                    duration of location update may be slightly different from
                    this value. Step size is 1 second. Range is 5 seconds to 18
                    hours. Editable online."
    DEFVAL          { 900 }
    ::= { bbuSysEntry 100 }

END
