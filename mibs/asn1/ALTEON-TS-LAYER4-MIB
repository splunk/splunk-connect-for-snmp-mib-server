    SYNTAX  Gauge
    ACCESS  read-only
    STATUS  mandatory 
    DESCRIPTION 
        "The current sessions for the real server."
    ::= { slbStatPortRealServerEntry 3 }

slbStatPortRealServerTotalSessions OBJECT-TYPE 
    SYNTAX  Counter 
    ACCESS  read-only
    STATUS  mandatory 
    DESCRIPTION 
        "The total number of sessions for the real server."
    ::= { slbStatPortRealServerEntry 4 }

slbStatPortRealServerHCOctets OBJECT-TYPE 
    SYNTAX  Counter64
    ACCESS  read-only
    STATUS  obsolete
    DESCRIPTION 
        "The total number of octets received and transmitted out of the real 
         server on a particular port."
