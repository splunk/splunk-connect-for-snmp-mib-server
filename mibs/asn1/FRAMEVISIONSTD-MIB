				::= { fribFrlmCfgEntry 2 }

fribFrlmCfgProtocol		OBJECT-TYPE
				SYNTAX	INTEGER {
				none		(1),
				lmi			(2),
				ansiT1617D	(3),
				ccittQ933A	(4)
