	-- the 24-Hour AlertThreshold table
 
          ncmhdmTCADayTable OBJECT-TYPE
              SYNTAX  SEQUENCE OF NcmhdmTCADayEntry
              ACCESS  not-accessible
              STATUS  mandatory
              DESCRIPTION
                      "The 24 hour Alert Threshold Table."
                --DEFAULT test-function   ds3-test
--              DEFAULT set-function    null-set-proc
                --DEFAULT next-function   ds3-next
              ::= { ncm-ds3 9008 }
 
          ncmhdmTCADayEntry OBJECT-TYPE
              SYNTAX  NcmhdmTCADayEntry
              ACCESS  not-accessible
              STATUS  mandatory
              DESCRIPTION
                      "An entry in the 24 hour Alert Threshold Table."
              INDEX   { ncmhdmTCADayNIDIndex,  ncmhdmTCADayIndex }
              ::= { ncmhdmTCADayTable 1 }
 
          NcmhdmTCADayEntry ::=
              SEQUENCE {
                  ncmhdmTCADayNIDIndex
                      INTEGER,
                  ncmhdmTCADayIndex
                      INTEGER,
                  ncmhdmTCADayDs3FarEndCCV
                      INTEGER,
                  ncmhdmTCADayDs3FarEndCES
                      INTEGER,
                  ncmhdmTCADayDs3FarEndCSES
                      INTEGER,
                  ncmhdmTCADayDs3FarEndLUAS
                      INTEGER,
                  ncmhdmTCADayDs3LCV
                      INTEGER,
                  ncmhdmTCADayDs3LES
                      INTEGER,
                  ncmhdmTCADayDs3LSESs
                      INTEGER,
                  ncmhdmTCADayDs3PCV
                      INTEGER,
                  ncmhdmTCADayDs3PES
                      INTEGER,
                  ncmhdmTCADayDs3PSES
                      INTEGER,
                  ncmhdmTCADayDs3CCV
                      INTEGER,
                  ncmhdmTCADayDs3CES
                      INTEGER,
                  ncmhdmTCADayDs3CSES
                      INTEGER,
                  ncmhdmTCADayDs3SEFS
                      INTEGER,
                  ncmhdmTCADayDs3AISS
                      INTEGER,
                  ncmhdmTCADayDs3LUASs
                      INTEGER,
                  ncmhdmTCADayDs3LOSSs
                      INTEGER,
                  ncmhdmTCADayDs3LOFSs
                      INTEGER
              }


	ncmhdmTCADayNIDIndex OBJECT-TYPE
          SYNTAX  INTEGER (1..'7fffffff'h)
          ACCESS  read-only
          STATUS  mandatory
          DESCRIPTION
                "This variable indicates the node id value of the node."
            ::= { ncmhdmTCADayEntry 1 }
               
          ncmhdmTCADayIndex OBJECT-TYPE
              SYNTAX  INTEGER (1..65535)
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      "The index value which uniquely identifies the
                      alert crossing threshold interface to which
                      this entry is applicable.  The interface
                      identified by a particular value of this
                      index is the same interface as identified
                      by the same value an plcpLineIndex object
                      instance."
              ::= { ncmhdmTCADayEntry 2 }
               
 
