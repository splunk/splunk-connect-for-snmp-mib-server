	 -- Ds3 Transmit Alarm table
 
          ncmhdmTxAlarmTable OBJECT-TYPE
              SYNTAX  SEQUENCE OF NcmhdmTxAlarmEntry
              ACCESS  not-accessible
              STATUS  mandatory
              DESCRIPTION
                "DS3 Transmit alarm Table."
                --DEFAULT test-function   ds3-test
--              DEFAULT set-function    null-set-proc
                --DEFAULT next-function   ds3-next
              ::= { ncm-ds3 9009 }
 
          ncmhdmTxAlarmEntry OBJECT-TYPE
              SYNTAX  NcmhdmTxAlarmEntry
              ACCESS  not-accessible
              STATUS  mandatory
              DESCRIPTION
                      "An entry in the DS3/E3 Transmit Alarm table."
              INDEX   { ncmhdmTxAlarmNIDIndex, ncmhdmTxAlarmIndex }
              ::= { ncmhdmTxAlarmTable 1 }
 
          NcmhdmTxAlarmEntry ::=
              SEQUENCE {
                  ncmhdmTxAlarmNIDIndex
                      INTEGER,
                  ncmhdmTxAlarmIndex
                      INTEGER,
                  ncmhdmTxYellowAlarm
                      INTEGER,
                  ncmhdmTxAIS
                      INTEGER,
                  ncmhdmTxIdleSignal
                      INTEGER,
                  ncmhdmTxFEBE
                      INTEGER,
                  ncmhdmTxFEACAlarm
                      INTEGER
              }
 
        ncmhdmTxAlarmNIDIndex OBJECT-TYPE
          SYNTAX  INTEGER (1..'7fffffff'h)
          ACCESS  read-only
          STATUS  mandatory
          DESCRIPTION
                "This variable indicates the node id value of the node."
            ::= { ncmhdmTxAlarmEntry 1 }


	 ncmhdmTxAlarmIndex OBJECT-TYPE
              SYNTAX  INTEGER (1..65535)
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      "The index value which uniquely identifies the
                      Transmit Alarm interface to which
                      this entry is applicable.  The interface
                      identified by a particular value of this
                      index is the same interface as identified
                      by the same value an Dsx3LineIndex object
                      instance."
              ::= { ncmhdmTxAlarmEntry 2 }
               
          ncmhdmTxYellowAlarm OBJECT-TYPE
              SYNTAX  INTEGER{
                        eNABLE(1),
                        dISABLE(2)
                      }
              ACCESS  read-write
              STATUS  mandatory
              DESCRIPTION
                      "Transmit the near-end yellow alarm."
              ::= { ncmhdmTxAlarmEntry 3 }
 
          ncmhdmTxAIS   OBJECT-TYPE
              SYNTAX  INTEGER{
                        eNABLE(1),
                        dISABLE(2)
                      }
              ACCESS  read-write
              STATUS  mandatory
              DESCRIPTION
                      "Transmit the near-end AIS alarm."
              ::= { ncmhdmTxAlarmEntry 4 }
 
          ncmhdmTxIdleSignal OBJECT-TYPE
              SYNTAX  INTEGER{
                        eNABLE(1),
                        dISABLE(2)
                      }
              ACCESS  read-write
              STATUS  mandatory
              DESCRIPTION
                      "Transmit the near-end Idle alarm."
              ::= { ncmhdmTxAlarmEntry 5 }
 
          ncmhdmTxFEBE OBJECT-TYPE
              SYNTAX  INTEGER{
                        eNABLE(1),
                        dISABLE(2)
                      }
              ACCESS  read-write
              STATUS  mandatory
              DESCRIPTION
                      "Near end unit transmits the FEBE alarm."
              ::= { ncmhdmTxAlarmEntry 6 }


	 ncmhdmTxFEACAlarm OBJECT-TYPE
              SYNTAX  INTEGER{
                        nO-FEAC-Alarm-enabled (1),
                        eqptFailureSA-Alarm (2),
                        lOSHBER-Alarm (3),
                        oOF-Alarm (4),
                        aISReceived-Alarm (5),
                        idleReceived-Alarm (6),
                        eqptFailureNSA-Alarm (7),
                        commonEqptFailureNSA-Alarm (8)
                      }
              ACCESS  read-write
              STATUS  mandatory
              DESCRIPTION
                      "This object describes which, if any, FEAC
                        Alarm is enabled. "
              ::= { ncmhdmTxAlarmEntry 7 }
               


	--- Hdm Stats
---
     ncmhdmDs3StatTable OBJECT-TYPE
          SYNTAX SEQUENCE OF NcmhdmDs3StatEntry
          ACCESS not-accessible
          STATUS mandatory
          DESCRIPTION
               "The HDM Ds3 Stats."
                --DEFAULT test-function   ds3-test
--              DEFAULT set-function    null-set-proc
                --DEFAULT next-function   ds3-next
          ::= { ncm-ds3 9010 }
 
     ncmhdmDs3StatEntry OBJECT-TYPE
          SYNTAX NcmhdmDs3StatEntry
          ACCESS not-accessible
          STATUS mandatory
          DESCRIPTION
               "An entry in the ds3 stats table."
          INDEX   { ncmhdmDs3StatNIDIndex, ncmhdmDs3StatIndex }
          ::= { ncmhdmDs3StatTable 1 }
 
     NcmhdmDs3StatEntry ::= SEQUENCE {
             ncmhdmDs3StatNIDIndex
                 INTEGER,
             ncmhdmDs3StatIndex
                 INTEGER,
             ncmhdmDs3StatAIS
                 INTEGER,
             ncmhdmDs3StatIdle
                 INTEGER,
             ncmhdmDs3StatYellowAlm
                 INTEGER,
             ncmhdmDs3StatFrameLoss
                 INTEGER,
             ncmhdmDs3StatSigLoss
                 INTEGER,
             ncmhdmDs3StatPLCPYellow
                 INTEGER,
             ncmhdmDs3StatPLCPFrmLoss
                 INTEGER,
             ncmhdmDs3StatPLCPOOF
                 INTEGER,
             ncmhdmDs3LpbkLocal
                 INTEGER,
             ncmhdmDs3LpbkPayload
                 INTEGER,
             ncmhdmDs3LpbkFEAC
                 INTEGER,
             ncmhdmDs3LpbkFarEnd
                 INTEGER
     }


