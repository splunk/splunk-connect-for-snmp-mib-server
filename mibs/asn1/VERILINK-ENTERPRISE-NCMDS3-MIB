	--  ncm HDM DS3/E3 24 Hour SnapShot
 
-- The HDM DS3/E3 24 Hour SnapShot table contains various statistics being
-- collected for the last 24 Hours.
 
 
         ncmDs3PerformanceSnapShotTable OBJECT-TYPE
             SYNTAX  SEQUENCE OF NcmDs3PerformanceSnapShotEntry
             ACCESS  not-accessible
             STATUS  mandatory
                --DEFAULT test-function   ds3-test
--              DEFAULT set-function    null-set-proc
                --DEFAULT next-function   ds3-next
            ::= { ncm-ds3 9006 }
 
 
         ncmDs3PerformanceSnapShotEntry OBJECT-TYPE
             SYNTAX  NcmDs3PerformanceSnapShotEntry
             ACCESS  not-accessible
             STATUS  mandatory
             DESCRIPTION
             "An entry in the  Ds3 Performance Snap Shot Entry table."
             INDEX   { ncmDs3SnapShotNIDIndex, ncmDs3SnapShotIndex }
            ::= { ncmDs3PerformanceSnapShotTable 1 }
 
 
        NcmDs3PerformanceSnapShotEntry ::=
         SEQUENCE {
           ncmDs3SnapShotNIDIndex
                INTEGER,
           ncmDs3SnapShotIndex
                 INTEGER,
           ncmDs3SnapShotCRC4Status
                INTEGER,
           ncmDs3SnapShot
                INTEGER,
           ncmDs3TimeStampSec
                INTEGER,
           ncmDs3TimeStampMilliSec
                 INTEGER,
           ncmDs3SnapShotSecs
                 INTEGER,
           ncmDs3ResetPerfReg
                 INTEGER
                }
          
          
       ncmDs3SnapShotNIDIndex OBJECT-TYPE
             SYNTAX  INTEGER (1..'7fffffff'h)
             ACCESS  read-only
             STATUS  mandatory
             DESCRIPTION
                "The object specifies the node id index of the node."
            ::= { ncmDs3PerformanceSnapShotEntry 1 }
 
	
        ncmDs3SnapShotIndex OBJECT-TYPE
             SYNTAX  INTEGER (1..'7fffffff'h)
             ACCESS  read-only
             STATUS  mandatory
             DESCRIPTION
                "The index value which uniquely identifies  the
                DS1  interface  to which this entry is applica-
                ble.  The interface identified by a  particular
                value  of  this  index is the same interface as
                identified by the same value as a LineIndex
                object instance."
            ::= { ncmDs3PerformanceSnapShotEntry 2 }
 
 
          ncmDs3SnapShotCRC4Status OBJECT-TYPE
               SYNTAX INTEGER {
                       disabled (1)
               } 
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      "CRC4 Status."
              ::= { ncmDs3PerformanceSnapShotEntry 3 }
 
        ncmDs3SnapShot OBJECT-TYPE
            SYNTAX INTEGER {
                                no(1),
                                yes(2)
                       } 
           ACCESS read-write
           STATUS mandatory
           DESCRIPTION
           "The variable specifies the option to initiate snap shot
               command."
 
            ::= { ncmDs3PerformanceSnapShotEntry 4 }
 
        ncmDs3TimeStampSec OBJECT-TYPE
             SYNTAX  INTEGER (1..'7fffffff'h)
           ACCESS read-only
           STATUS mandatory
           DESCRIPTION
           "The variable specifies the Time Stamp in Seconds ."
 
           ::= { ncmDs3PerformanceSnapShotEntry 5 }
 
 
        ncmDs3TimeStampMilliSec OBJECT-TYPE
            SYNTAX  INTEGER (1..65535)
           ACCESS read-only
           STATUS mandatory
           DESCRIPTION
           "The variable specifies the Time Stamp in milli  Seconds ."
 
           ::= { ncmDs3PerformanceSnapShotEntry  6 }


	
         ncmDs3SnapShotSecs OBJECT-TYPE
            SYNTAX  INTEGER (1..65535)
           ACCESS read-only
            STATUS mandatory
           DESCRIPTION
           "The variable specifies number of seconds in the current interval."
            ::= { ncmDs3PerformanceSnapShotEntry 7 }
 
         ncmDs3ResetPerfReg OBJECT-TYPE
             SYNTAX  INTEGER {
                         no(1),
                         yes(2)
                     }
             ACCESS  read-write
             STATUS  mandatory
             DESCRIPTION
                "Reset Performance Monitoring registers."
            ::= { ncmDs3PerformanceSnapShotEntry 8 }
 
	-- the Quarter-Hour AlertThreshold table
 
          ncmhdmTCAQtrTable OBJECT-TYPE
              SYNTAX  SEQUENCE OF NcmhdmTCAQtrEntry
              ACCESS  not-accessible
              STATUS  mandatory
              DESCRIPTION
                      "The quarter hour Alert Threshold Table."
                --DEFAULT test-function   ds3-test
--              DEFAULT set-function    null-set-proc
                --DEFAULT next-function   ds3-next
              ::= { ncm-ds3 9007  }
 
          ncmhdmTCAQtrEntry OBJECT-TYPE
              SYNTAX  NcmhdmTCAQtrEntry
              ACCESS  not-accessible
              STATUS  mandatory
              DESCRIPTION
                      "An entry in the DS3/E3 table."
              INDEX   { ncmhdmTCAQtrNIDIndex, ncmhdmTCAQtrIndex }
              ::= { ncmhdmTCAQtrTable 1 }
 
          NcmhdmTCAQtrEntry ::=
              SEQUENCE {
                  ncmhdmTCAQtrNIDIndex
                      INTEGER,
                  ncmhdmTCAQtrIndex
                      INTEGER,
                  ncmhdmTCAQtrDs3FarEndCCV
                      INTEGER,
                  ncmhdmTCAQtrDs3FarEndCES
                      INTEGER,
                  ncmhdmTCAQtrDs3FarEndCSES
                      INTEGER,
                  ncmhdmTCAQtrDs3FarEndLUAS
                      INTEGER,
                  ncmhdmTCAQtrDs3LCV
                      INTEGER,
                  ncmhdmTCAQtrDs3LES
                      INTEGER,
                  ncmhdmTCAQtrDs3LSESs
                      INTEGER,
                  ncmhdmTCAQtrDs3PCV
                      INTEGER,
                  ncmhdmTCAQtrDs3PES
                      INTEGER,
                  ncmhdmTCAQtrDs3PSES
                      INTEGER,
                  ncmhdmTCAQtrDs3CCV
                      INTEGER,
                  ncmhdmTCAQtrDs3CES
                      INTEGER,
                  ncmhdmTCAQtrDs3CSES
                      INTEGER,
                  ncmhdmTCAQtrDs3SEFS
                      INTEGER,
                  ncmhdmTCAQtrDs3AISS
                      INTEGER,
                  ncmhdmTCAQtrDs3LUASs
                      INTEGER,
                  ncmhdmTCAQtrDs3LOSSs
                      INTEGER,
                  ncmhdmTCAQtrDs3LOFSs
                      INTEGER
              }

	ncmhdmTCAQtrNIDIndex OBJECT-TYPE
          SYNTAX  INTEGER (1..'7fffffff'h)
          ACCESS  read-only
          STATUS  mandatory
          DESCRIPTION
                "This variable indicates the node id value of the node."
            ::= { ncmhdmTCAQtrEntry 1 }
               
          ncmhdmTCAQtrIndex OBJECT-TYPE
              SYNTAX  INTEGER (1..65535)
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      "The index value which uniquely identifies the
                      alert crossing threshold interface to which
                      this entry is applicable.  The interface
                      identified by a particular value of this
                      index is the same interface as identified
                      by the same value an plcpLineIndex object
                      instance."
              ::= { ncmhdmTCAQtrEntry 2 }
               
          ncmhdmTCAQtrDs3FarEndCCV OBJECT-TYPE
              SYNTAX  INTEGER (0..4294967295)
              ACCESS  read-write
              STATUS  mandatory
              DESCRIPTION
                      "The performance alert threshold for DS3
                        layer Far End C-bit Code Violation in 1/4 hour
                        boundary."
              ::= { ncmhdmTCAQtrEntry 3 }
               
          ncmhdmTCAQtrDs3FarEndCES OBJECT-TYPE
              SYNTAX  INTEGER (0..4294967295)
              ACCESS  read-write
              STATUS  mandatory
              DESCRIPTION
                      "The performance alert threshold for DS3
                        Far End C-bit Errored Seconds,
                        in 1/4 hour boundary."
              ::= { ncmhdmTCAQtrEntry 4 }
               
          ncmhdmTCAQtrDs3FarEndCSES OBJECT-TYPE
              SYNTAX  INTEGER (0..4294967295)
              ACCESS  read-write
              STATUS  mandatory
              DESCRIPTION
                      "The performance alert threshold for DS3
                        Far End C-bit Severely Errored Seconds,
                        in 1/4 hour boundary."
 
              ::= { ncmhdmTCAQtrEntry 5 }
 
          ncmhdmTCAQtrDs3FarEndLUAS OBJECT-TYPE
              SYNTAX  INTEGER (0..4294967295)
              ACCESS  read-write
              STATUS  mandatory
              DESCRIPTION
                      "The performance alert threshold for DS3 layer
                      Far End Line Unavailable Seconds in 1/4 hour
                        boundary."
              ::= { ncmhdmTCAQtrEntry 6 }


	ncmhdmTCAQtrDs3LCV OBJECT-TYPE
              SYNTAX  INTEGER (0..4294967295)
              ACCESS  read-write
              STATUS  mandatory
              DESCRIPTION
                      "The performance alert threshold for DS3
                        layer Line code Violation in 1/4 hour
                        boundary."
              ::= { ncmhdmTCAQtrEntry 7 }
               
