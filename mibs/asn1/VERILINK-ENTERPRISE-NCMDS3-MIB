	VERILINK-ENTERPRISE-NCMDS3-MIB

        -- set up MIB-wide defaults
        -- all functions can use %n for object name and
        -- %t for object type, %p for parent name,
        -- %d for name of default-bearing node and %% for %

          DEFINITIONS ::= BEGIN

     IMPORTS
                Gauge
                        FROM RFC1155-SMI
            DisplayString
                     FROM RFC1213-MIB
        OBJECT-TYPE
                     FROM RFC-1212
        ncm-ds3
             FROM VERILINK-ENTERPRISE-NCMALARM-MIB;


     --  This MIB module uses the extended OBJECT-TYPE macro as
     --  defined in RFC 1212.
     --  This MIB module uses the TRAP-TYPE macro as
     --  defined in RFC 1215.


     --   verilink                    OBJECT IDENTIFIER ::= { enterprises 321 }
     --   as2000                      OBJECT IDENTIFIER ::= { verilink 1 }
     --   ncm-ds3                     OBJECT IDENTIFIER ::= { as2000 3036 }



	--
     --ncm HDM DS3 Port table
     --  
     ncmhdmDs3PortTable OBJECT-TYPE
          SYNTAX SEQUENCE OF NcmhdmDs3PortEntry
          ACCESS not-accessible
          STATUS mandatory
          DESCRIPTION
               "The HDM DS3 Port table."
                --DEFAULT test-function   ds3-test
--              DEFAULT set-function    null-set-proc
                --DEFAULT next-function   ds3-next
          ::= { ncm-ds3 9000 }

     ncmhdmDs3PortEntry OBJECT-TYPE
          SYNTAX NcmhdmDs3PortEntry
          ACCESS not-accessible
          STATUS mandatory
          DESCRIPTION
               "An entry in the dsu information table."
          INDEX   { ncmhdmDs3PortNIDIndex, ncmhdmDs3PortIndex }
          ::= { ncmhdmDs3PortTable 1 }

     NcmhdmDs3PortEntry ::= SEQUENCE {
             ncmhdmDs3PortNIDIndex
                INTEGER,
             ncmhdmDs3PortIndex
                INTEGER,
             ncmhdmDs3PerfControl
                INTEGER,
             ncmhdmDs3LBOSelection
                INTEGER,
             ncmhdmDs3DataRateMode
                INTEGER,
	     ncmhdmDs3Rate
		INTEGER,
	     ncmhdmDs3LineType
		INTEGER,
	      ncmhdmDs3LineCode
                INTEGER,
             ncmhdmDs3AISCBit
                INTEGER,
             ncmhdmDs3EquipCode
                 DisplayString,
             ncmhdmDs3LocationIDCode
                 DisplayString,
             ncmhdmDs3FrameIDCode
                 DisplayString,
             ncmhdmDs3UnitCode
                 DisplayString,
             ncmhdmDs3FacilityIDCode
                 DisplayString,
             ncmhdmDs3PortIdCode
                 DisplayString,
             ncmhdmDs3CircuitID
                 DisplayString,
             ncmhdmDs3GenIDCode
                 DisplayString,
             ncmhdmDs3Inband
                INTEGER
     }


	ncmhdmDs3PortNIDIndex OBJECT-TYPE
          SYNTAX  INTEGER (1..'7fffffff'h)
          ACCESS  read-only
          STATUS  mandatory
          DESCRIPTION
                "This variable indicates the node id value of the node."
            ::= { ncmhdmDs3PortEntry 1 }
 
     ncmhdmDs3PortIndex OBJECT-TYPE
          SYNTAX  INTEGER (1..65535)
          ACCESS  read-only
          STATUS  mandatory
          DESCRIPTION
               "The index value which uniquely identifies  the
                DS1  interface  to which this entry is applica-
                ble.  The interface identified by a  particular
                value  of  this  index is the same interface as
                identified by the same value as a dsx1LineIndex
                object instance."
            ::= { ncmhdmDs3PortEntry 2 }
 
     ncmhdmDs3PerfControl OBJECT-TYPE
          SYNTAX  INTEGER {
                disabled (1),
                enabled (2)
          }
          ACCESS  read-write
          STATUS  mandatory
          DESCRIPTION
                "Performance Control Enabled/Disabled."
            ::= { ncmhdmDs3PortEntry 3 }
 
     ncmhdmDs3LBOSelection  OBJECT-TYPE
          SYNTAX  INTEGER{
                nORMAL-CABLE (1),
                lONG-CABLE(2)
          }
          ACCESS  read-write
          STATUS  mandatory
          DESCRIPTION
               "Select the length of DS3 cable to be Normal
                (0-255 feet) or Long cable (225-450 feet)."
            ::= { ncmhdmDs3PortEntry 4 }
 
     ncmhdmDs3DataRateMode OBJECT-TYPE
          SYNTAX  INTEGER {
                only-NearEnd (1),
                both-NearEndORFarEnd (2),
		not-applicable (3)
          }
          ACCESS  read-write
          STATUS  mandatory
          DESCRIPTION
                "Data Rate Mode."
            ::= { ncmhdmDs3PortEntry 5 }


	ncmhdmDs3Rate OBJECT-TYPE
	SYNTAX INTEGER 
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION
	"the date rate fro ds3 ."
	::= { ncmhdmDs3PortEntry 6 }
	


	 ncmhdmDs3LineType OBJECT-TYPE
          SYNTAX  INTEGER {
                dsx3M13(2),
                dSX3CBitParity (4)
          }
          ACCESS  read-write
          STATUS  mandatory
          DESCRIPTION
                "Line Type."
            ::= { ncmhdmDs3PortEntry 7 }
 
     ncmhdmDs3LineCode OBJECT-TYPE
          SYNTAX  INTEGER {
                dSX3CB3ZS (2)
          }
          ACCESS  read-write
          STATUS  mandatory
          DESCRIPTION
                "Line Code."
            ::= { ncmhdmDs3PortEntry 8 }
 
     ncmhdmDs3AISCBit OBJECT-TYPE
          SYNTAX  INTEGER {
                aISCBit (1)
          }
          ACCESS  read-write
          STATUS  mandatory
          DESCRIPTION
                "AISCBit."
            ::= { ncmhdmDs3PortEntry 9 }
 
     ncmhdmDs3EquipCode OBJECT-TYPE
          SYNTAX  DisplayString (SIZE (0..10))
          ACCESS  read-write
          STATUS  mandatory
          DESCRIPTION
               "This is the Far End Equipment Identification code
                that describes the specific piece of equipment.
                It is sent within the Path
                Identification Message."
          ::= { ncmhdmDs3PortEntry 10 }
 
     ncmhdmDs3LocationIDCode OBJECT-TYPE
          SYNTAX  DisplayString (SIZE (0..11))
          ACCESS  read-write
          STATUS  mandatory
          DESCRIPTION
               "This is the Far End Location Identification code
                that describes the specific location of the
                equipment.  It is sent within the Path
                Identification Message."
          ::= { ncmhdmDs3PortEntry 11 }
 
     ncmhdmDs3FrameIDCode OBJECT-TYPE
          SYNTAX  DisplayString (SIZE (0..10))
          ACCESS  read-write
          STATUS  mandatory
          DESCRIPTION
               "This is the Far End Frame Identification code
                that identifies where the equipment is located
                within a building at a given location.
                It is sent within the Path Identification Message."
          ::= { ncmhdmDs3PortEntry 12 }


	 ncmhdmDs3UnitCode OBJECT-TYPE
          SYNTAX  DisplayString (SIZE (0..6))
          ACCESS  read-write
          STATUS  mandatory
          DESCRIPTION
               "This is the Far End code
                that identifies the equipment location within a bay.
                It is sent within the Path
                Identification Message."
          ::= { ncmhdmDs3PortEntry 13 }
 
     ncmhdmDs3FacilityIDCode OBJECT-TYPE
          SYNTAX  DisplayString (SIZE (0..38))
          ACCESS  read-write
          STATUS  mandatory
          DESCRIPTION
               "This code identifies a specific Far End DS3 path.
                It is sent within the Path Identification Message."
          ::= { ncmhdmDs3PortEntry 14}
 
     ncmhdmDs3PortIdCode OBJECT-TYPE
          SYNTAX  DisplayString (SIZE (0..38))
          ACCESS read-write
          STATUS mandatory
          DESCRIPTION
               "Port ID."
          ::= { ncmhdmDs3PortEntry 15 }
 
     ncmhdmDs3CircuitID OBJECT-TYPE
          SYNTAX  DisplayString (SIZE (0..26))
          ACCESS read-write
          STATUS mandatory
          DESCRIPTION
               "Circuit ID."
          ::= { ncmhdmDs3PortEntry 16 }
 
     ncmhdmDs3GenIDCode OBJECT-TYPE
          SYNTAX  DisplayString (SIZE (0..38))
          ACCESS read-write
          STATUS mandatory
          DESCRIPTION
               "Test Signal Generator ID."
          ::= { ncmhdmDs3PortEntry 17 }
 
     ncmhdmDs3Inband OBJECT-TYPE
          SYNTAX  INTEGER {
                disabled (1),
                enabled (2)
          }
          ACCESS  read-write
          STATUS  mandatory
          DESCRIPTION
                "Inband signalling control."
            ::= { ncmhdmDs3PortEntry 18 }
 


	

 --  
     -- ncm HDM HSSI Port table
     --   

     ncmhdmHssi1PortTable OBJECT-TYPE
          SYNTAX SEQUENCE OF NcmhdmHssi1PortEntry
          ACCESS not-accessible
          STATUS mandatory
          DESCRIPTION
               "The HDM HSSI Port table."
                --DEFAULT test-function   ds3-hssi-test
--              DEFAULT set-function    null-set-proc
                --DEFAULT next-function   ds3-hssi-next
          ::= { ncm-ds3  9001 }

     ncmhdmHssi1PortEntry OBJECT-TYPE
          SYNTAX NcmhdmHssi1PortEntry
          ACCESS not-accessible
          STATUS mandatory
          DESCRIPTION
               "An entry in the dsu information table."
          INDEX   { ncmhdmDs3Hssi1NIDIndex, ncmhdmHssi1PortIndex }
          ::= { ncmhdmHssi1PortTable 1 }

     NcmhdmHssi1PortEntry ::= SEQUENCE {
             ncmhdmDs3Hssi1NIDIndex
                 INTEGER,
             ncmhdmHssi1PortIndex
                 INTEGER,
             ncmhdmHssi1ConfigMode
                 INTEGER,
             ncmhdmHssi1PortConfig
                 INTEGER,
             ncmhdmHssi1TestMode
                 INTEGER,
             ncmhdmHssi1DataRateMode
                 INTEGER,
             ncmhdmHssi1HssiRate
                 INTEGER,
	     ncmhdmHssi1CircuitID
 		 DisplayString
        
     }
       
     	ncmhdmDs3Hssi1NIDIndex OBJECT-TYPE
          SYNTAX  INTEGER (1..'7fffffff'h)
          ACCESS  read-only
          STATUS  mandatory
          DESCRIPTION
                "This variable indicates the node id value of the node."
            ::= { ncmhdmHssi1PortEntry 1 }
               
	ncmhdmHssi1PortIndex OBJECT-TYPE
          SYNTAX  INTEGER (1..65535)
          ACCESS  read-only
          STATUS  mandatory
          DESCRIPTION
               "The index value which uniquely identifies  the
                DS1  interface  to which this entry is applica-
                ble.  The interface identified by a  particular
                value  of  this  index is the same interface as
                identified by the same value as a dsx1LineIndex
                object instance."
            ::= { ncmhdmHssi1PortEntry 2 }
 
     	ncmhdmHssi1ConfigMode OBJECT-TYPE
          SYNTAX INTEGER {
                       aUTOMATIC(1),
                       mANUAL(2)
               } 
          ACCESS read-write
          STATUS mandatory
          DESCRIPTION
               "Set the Hssi Port to be automatic configuration
                or manual configuration. "
          ::= { ncmhdmHssi1PortEntry 3 }
 
     	ncmhdmHssi1PortConfig OBJECT-TYPE
          SYNTAX INTEGER {
                       oUT-OF-SERVICE(1),
                       iN-SERVICE(2)
               } 
          ACCESS read-write
          STATUS mandatory
          DESCRIPTION
               "Configure System - In-Service or Out of Service."
          ::= { ncmhdmHssi1PortEntry 4 }
 
     	ncmhdmHssi1TestMode OBJECT-TYPE
          SYNTAX  INTEGER{
                enabled(1),
                disabled(2)
          }
          ACCESS  read-only
          STATUS  mandatory
          DESCRIPTION
               "Report status of the test mode of HSSI port."
          ::= { ncmhdmHssi1PortEntry 5 }
 
        ncmhdmHssi1DataRateMode OBJECT-TYPE
          SYNTAX  INTEGER {
                only-NearEnd (1),
                both-NearEndORFarEnd (2),
		not-applicable (3)
          }
          ACCESS  read-write
          STATUS  mandatory
          DESCRIPTION
                "Data Rate Mode for the Hssi Ports ."
            ::= { ncmhdmHssi1PortEntry 6 }


	 ncmhdmHssi1HssiRate OBJECT-TYPE
          SYNTAX  INTEGER (0..28)
          ACCESS  read-write
          STATUS  mandatory
          DESCRIPTION
                "Data Rate in # of T1."
          ::= { ncmhdmHssi1PortEntry 7 }


	 ncmhdmHssi1CircuitID OBJECT-TYPE
           SYNTAX  DisplayString (SIZE (0..20))
           ACCESS  read-write
           STATUS  mandatory
           DESCRIPTION
                "This is the Circuit ID for the Hssi port."
           ::= { ncmhdmHssi1PortEntry 8 }



 
 
--- ncm HSSI Stats
---
     ncmhdmHssiStatTable OBJECT-TYPE
          SYNTAX SEQUENCE OF NcmhdmHssiStatEntry
          ACCESS not-accessible
          STATUS mandatory
          DESCRIPTION
               "The HDM HSSI Stats."
                --DEFAULT test-function   ds3-hssi-test
--              DEFAULT set-function    null-set-proc
                --DEFAULT next-function   ds3-hssi-next
                ::= { ncm-ds3 9002 }
 
     ncmhdmHssiStatEntry OBJECT-TYPE
          SYNTAX NcmhdmHssiStatEntry
          ACCESS not-accessible
          STATUS mandatory
          DESCRIPTION
               "An entry in the Hssi ds3 stats table."
          INDEX   { ncmhdmHssiStatNIDIndex, ncmhdmHssiStatIndex }
          ::= { ncmhdmHssiStatTable 1 }
 
     NcmhdmHssiStatEntry ::= SEQUENCE {
             ncmhdmHssiStatNIDIndex
                 INTEGER,
             ncmhdmHssiStatIndex
                 INTEGER,
             ncmhdmStatDCEReady
                 INTEGER,
             ncmhdmStatDTEReady
                 INTEGER,
             ncmhdmStatLpbkA
                 INTEGER,
             ncmhdmStatLpbkB
                 INTEGER,
             ncmhdmStatTestMode
                 INTEGER,
             ncmhdmStatPortState
                 INTEGER,
             ncmhdmStatCIMType
                 INTEGER,
             ncmhdmStatDiagStatus
                 INTEGER
     }
 
     ncmhdmHssiStatNIDIndex OBJECT-TYPE
          SYNTAX  INTEGER (1..'7fffffff'h)
          ACCESS  read-only
          STATUS  mandatory
          DESCRIPTION
                "This variable indicates the node id value of the node."
            ::= { ncmhdmHssiStatEntry 1 }


	ncmhdmHssiStatIndex OBJECT-TYPE
          SYNTAX  INTEGER (1..65535)
          ACCESS  read-only
          STATUS  mandatory
          DESCRIPTION
               "The index value which uniquely identifies  the
                DS1  interface  to which this entry is applica-
                ble.  The interface identified by a  particular
                value  of  this  index is the same interface as
                identified by the same value as a dsx1LineIndex
                object instance."
            ::= { ncmhdmHssiStatEntry 2 }
 
 
     	ncmhdmStatDCEReady OBJECT-TYPE
          SYNTAX INTEGER {
                       oFF (1),
                       oN (2)
               } 
          ACCESS read-only
          STATUS mandatory
          DESCRIPTION
               "Hssi Port DCE Ready OFF/ON. "
          ::= { ncmhdmHssiStatEntry  3 }
 
     	ncmhdmStatDTEReady OBJECT-TYPE
          SYNTAX INTEGER {
                       oFF (1),
                       oN (2)
               } 
          ACCESS read-only
          STATUS mandatory
          DESCRIPTION
               "Hssi Port DTE Ready OFF/ON. "
          ::= { ncmhdmHssiStatEntry  4 }
 
 
     	ncmhdmStatLpbkA OBJECT-TYPE
          SYNTAX INTEGER {
                       oFF (1),
                       oN (2)
               } 
          ACCESS read-only
          STATUS mandatory
          DESCRIPTION
               "Hssi Port - Loopback A OFF/ON."
          ::= { ncmhdmHssiStatEntry  5 }
 
 
     	ncmhdmStatLpbkB OBJECT-TYPE
          SYNTAX INTEGER {
                       oFF (1),
                       oN (2)
               } 
          ACCESS read-only
          STATUS mandatory
          DESCRIPTION
               "Hssi Port - Loopback B OFF/ON."
          ::= { ncmhdmHssiStatEntry  6 }


	ncmhdmStatTestMode OBJECT-TYPE
          SYNTAX INTEGER {
                       oFF (1),
                       oN (2)
               } 
          ACCESS read-only
          STATUS mandatory
          DESCRIPTION
               "Hssi Port Test Mode OFF/ON."
          ::= { ncmhdmHssiStatEntry  7  }
 
 
     	ncmhdmStatPortState OBJECT-TYPE
          SYNTAX INTEGER {
                       oUT-OF-SERVICE(1),
                       iN-SERVICE(2)
               } 
          ACCESS read-only
          STATUS mandatory
          DESCRIPTION
               "Hssi Port - In-Service or Out of Service."
          ::= { ncmhdmHssiStatEntry  8 }
 
 
     	ncmhdmStatCIMType OBJECT-TYPE
          SYNTAX INTEGER {
                       cim-2080 (44),
                       cdm-2080 (59),
		       cdm-2182 (63)
               } 
          ACCESS read-only
          STATUS mandatory
          DESCRIPTION
               "Ds3 Cim Type."
          ::= { ncmhdmHssiStatEntry 9 }
 
     	ncmhdmStatDiagStatus OBJECT-TYPE
          SYNTAX INTEGER {
                       oFF (1),
                       oN (2)
               } 
          ACCESS read-only
          STATUS mandatory
          DESCRIPTION
               "Hssi User Equipment Loopback OFF/ON."
          ::= { ncmhdmHssiStatEntry 10 }


-- the ncm HDM DS3/E3 Current
 
-- The ncm HDM DS3/E3 current table contains various statistics being
-- collected for the current 15 minute interval.
 
          ncmhdmCurrentTable OBJECT-TYPE
              SYNTAX  SEQUENCE OF NcmhdmCurrentEntry
              ACCESS  not-accessible
              STATUS  mandatory
              DESCRIPTION
                      "The DS3/E3 Current table."
                --DEFAULT test-function   ds3-test
--              DEFAULT set-function    null-set-proc
                --DEFAULT next-function   ds3-next
              ::= { ncm-ds3 9003 }
 
          ncmhdmCurrentEntry OBJECT-TYPE
              SYNTAX  NcmhdmCurrentEntry
              ACCESS  not-accessible
              STATUS  mandatory
              DESCRIPTION
                      "An entry in the DS3/E3 Current table."
              INDEX   { ncmhdmCurrentNIDIndex, ncmhdmCurrentIndex }
              ::= { ncmhdmCurrentTable 1 }
 
          NcmhdmCurrentEntry ::=
              SEQUENCE {
                  ncmhdmCurrentNIDIndex
                      INTEGER,
                  ncmhdmCurrentIndex
                      INTEGER,
                  ncmhdmCurrentCRC4Status
                      INTEGER,
                  ncmhdmCurrentTimeStampSecs
                      INTEGER,
                  ncmhdmCurrentTimeStampMSecs
                      INTEGER,
                  ncmhdmCurrentSecsElaps
                      INTEGER,
                  ncmhdmCurrentFarEndCCV
                      Gauge,
                  ncmhdmCurrentFarEndCES
                      Gauge,
                  ncmhdmCurrentFarEndCSES
                      Gauge,
                  ncmhdmCurrentFarEndLUAS
                      Gauge,
                  ncmhdmCurrentLCV
                      Gauge,
                  ncmhdmCurrentLES
                      Gauge,
                  ncmhdmCurrentLSESs
                      Gauge,
                  ncmhdmCurrentPCV
                      Gauge,
                  ncmhdmCurrentPES
                      Gauge,
                  ncmhdmCurrentPSES
                      Gauge,
                  ncmhdmCurrentCCV
                      Gauge,
                  ncmhdmCurrentCES
                      Gauge,
                  ncmhdmCurrentCSES
                      Gauge,
		 ncmhdmCurrentSEFS
                      Gauge,
                  ncmhdmCurrentAISS
                      Gauge,
                  ncmhdmCurrentOOSSs
                      Gauge,
                  ncmhdmCurrentLOSSs
                      Gauge,
                  ncmhdmCurrentLOFSs
                      Gauge
              }
 
        ncmhdmCurrentNIDIndex OBJECT-TYPE
          SYNTAX  INTEGER (1..'7fffffff'h)
          ACCESS  read-only
          STATUS  mandatory
          DESCRIPTION
                "This variable indicates the node id value of the node."
            ::= { ncmhdmCurrentEntry 1 }
               
          ncmhdmCurrentIndex OBJECT-TYPE
              SYNTAX  INTEGER (1..65535)
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      "The index value which uniquely identifies the
                      DS3/E3 interface to which this entry is
                      applicable.  The interface identified by a
                      particular value of this index is the same
                      interface as identified by the same value an
                      ncmhdmLineIndex object instance."
              ::= { ncmhdmCurrentEntry 2 }
 
          ncmhdmCurrentCRC4Status OBJECT-TYPE
                SYNTAX INTEGER {
                       disabled (1)
               } 
          ACCESS read-only
          STATUS mandatory
          DESCRIPTION
               "Current CRC4 Status."
          ::= { ncmhdmCurrentEntry  3  }
 
 
          ncmhdmCurrentTimeStampSecs OBJECT-TYPE
              SYNTAX  INTEGER (1..65535)
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      "Current Time in seconds."
              ::= { ncmhdmCurrentEntry 4 }
 
          ncmhdmCurrentTimeStampMSecs OBJECT-TYPE
              SYNTAX  INTEGER (1..65535)
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      "Current Time in milliseconds."
              ::= { ncmhdmCurrentEntry 5 }


	 ncmhdmCurrentSecsElaps OBJECT-TYPE
              SYNTAX  INTEGER (1..65535)
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      "Seconds elapsed in the current interval."
              ::= { ncmhdmCurrentEntry 6 }
 
          ncmhdmCurrentFarEndCCV OBJECT-TYPE
              SYNTAX  Gauge
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      "The counter associated with the number of
                        Far End C-bit Code Violations,
                       in the current 15 minute interval."
              ::= { ncmhdmCurrentEntry 7 }
               
          ncmhdmCurrentFarEndCES OBJECT-TYPE
              SYNTAX  Gauge
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      "The counter associated with the number of
                        Far End C-bit Errored Seconds,
                       in the current 15 minute interval."
              ::= { ncmhdmCurrentEntry 8 }
               
          ncmhdmCurrentFarEndCSES OBJECT-TYPE
              SYNTAX  Gauge
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      "The counter associated with the number of
                        Far End C-bit Severely Errored Seconds,
                       in the current 15 minute interval."
 
              ::= { ncmhdmCurrentEntry 9 }
 
          ncmhdmCurrentFarEndLUAS OBJECT-TYPE
              SYNTAX  Gauge
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      "Far End Line Unavailable Seconds. The counter associated
                      with the number of Seconds the Far End DS3 interface
                      was Out Of Service in the current 15 minute interval."
              ::= { ncmhdmCurrentEntry 10 }
               
          ncmhdmCurrentLCV OBJECT-TYPE
              SYNTAX  Gauge
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      "The counter associated with the number of Line
                      Code Violations, a bipolar or excesive zero violation,
                       in the current 15 minute interval."
              ::= { ncmhdmCurrentEntry 11 }


	 ncmhdmCurrentLES OBJECT-TYPE
              SYNTAX  Gauge
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      "Line Errored Second Total.  The counter associated
                      with the number of Seconds of in which one or more
                      LCV occurred in the current 15 minute interval."
              ::= { ncmhdmCurrentEntry 12 }
               
          ncmhdmCurrentLSESs OBJECT-TYPE
              SYNTAX  Gauge
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      "The counter associated with the number of
                       Line Severely Errored Seconds (LSES) in
                       the current 15 minute interval."
              ::= { ncmhdmCurrentEntry 13 }
               
          ncmhdmCurrentPCV OBJECT-TYPE
              SYNTAX  Gauge
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      "The counter associated with the number
                      of Parity Code Violations (locally calculated P-bits
                      disagree with those received),
                       in the current 15 minute interval."
              ::= { ncmhdmCurrentEntry 14 }
               
          ncmhdmCurrentPES OBJECT-TYPE
              SYNTAX  Gauge
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      "The counter associated with the number of Seconds
                      having at least one Parity Code Violation,
                       in the current 15 minute interval."
              ::= { ncmhdmCurrentEntry 15 }
               
          ncmhdmCurrentPSES OBJECT-TYPE
              SYNTAX  Gauge
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      "The counter associated with the number of Seconds
                      having 44 or more Parity Code Violations,
                       in the current 15 minute interval."
              ::= { ncmhdmCurrentEntry 16 }
               
          ncmhdmCurrentCCV OBJECT-TYPE
              SYNTAX  Gauge
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      "The counter associated with the number of
                        C-bit Code Violations,
                       in the current 15 minute interval."
              ::= { ncmhdmCurrentEntry 17 }


	ncmhdmCurrentCES OBJECT-TYPE
              SYNTAX  Gauge
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      "The counter associated with the number of
                        C-bit Errored Seconds,
                       in the current 15 minute interval."
              ::= { ncmhdmCurrentEntry 18 }
               
          ncmhdmCurrentCSES OBJECT-TYPE
              SYNTAX  Gauge
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      "The counter associated with the number of
                        C-bit Severely Errored Seconds,
                       in the current 15 minute interval."
              ::= { ncmhdmCurrentEntry 19 }
               
          ncmhdmCurrentSEFS OBJECT-TYPE
              SYNTAX  Gauge
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      "The counter associated with the number of seconds
                      in which one or more Out of Frame occurred
                       in the current 15 minute interval."
              ::= { ncmhdmCurrentEntry 20 }
               
          ncmhdmCurrentAISS OBJECT-TYPE
              SYNTAX  Gauge
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      "The counter associated with the number of Seconds
                      in which one or more incoming AIS was detected
                       in the current 15 minute interval."
              ::= { ncmhdmCurrentEntry 21 }
               
          ncmhdmCurrentOOSSs OBJECT-TYPE
              SYNTAX  Gauge
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      "The counter associated with the number of Seconds
                      the DS3 interface was Out Of Service
                       in the current 15 minute interval."
              ::= { ncmhdmCurrentEntry 22 }
               
          ncmhdmCurrentLOSSs OBJECT-TYPE
              SYNTAX  Gauge
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      "The counter associated with the number of Seconds
                      Of system Loss Of Signal (LOS) in the current
                      15 minute interval."
              ::= { ncmhdmCurrentEntry 23 }


	 ncmhdmCurrentLOFSs OBJECT-TYPE
              SYNTAX  Gauge
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      "The counter associated with the number of Seconds
                      Of system Loss Of Frame (LOF) in the current
                      15 minute interval."
                
              ::= { ncmhdmCurrentEntry 24 }
               
 
-- ncm DS3/E3 Specific Interval table
     -- The HDM DS3/E3 Interval table contains various statistics
     -- collected by each DS3 Interface over the previous 24 hours of
     -- operation.  The past 24 hours are broken into 96 completed
     -- 15 minute intervals.
 
 
          ncmhdmIntervalTable OBJECT-TYPE
              SYNTAX  SEQUENCE OF NcmhdmIntervalEntry
              ACCESS  not-accessible
              STATUS  mandatory
              DESCRIPTION
                      "The DS3/E3 Interval table."
                --DEFAULT test-function   ds3interval-test
                --DEFAULT next-function   ds3interval-next
              ::= { ncm-ds3 9004 }
 
          ncmhdmIntervalEntry OBJECT-TYPE
              SYNTAX  NcmhdmIntervalEntry
              ACCESS  not-accessible
              STATUS  mandatory
              DESCRIPTION
                      "An entry in the DS3/E3 Interval table."
              INDEX   { ncmhdmIntervalNIDIndex, ncmhdmIntervalIndex, ncmhdmIntervalNumber }
              ::= { ncmhdmIntervalTable 1 }
 
          NcmhdmIntervalEntry ::=
              SEQUENCE {
                  ncmhdmIntervalNIDIndex
                      INTEGER,
                  ncmhdmIntervalIndex
                      INTEGER,
                  ncmhdmIntervalNumber
                      INTEGER,
                  ncmhdmIntervalFarEndCCV
                      Gauge,
                  ncmhdmIntervalFarEndCES
                      Gauge,
                  ncmhdmIntervalFarEndCSES
                      Gauge,
                  ncmhdmIntervalFarEndLUAS
                      Gauge,
                  ncmhdmIntervalLCV
                      Gauge,
                  ncmhdmIntervalLES
                      Gauge,
                  ncmhdmIntervalLSESs
                      Gauge,
                  ncmhdmIntervalPCV
                      Gauge,
                  ncmhdmIntervalPES
                      Gauge,
		ncmhdmIntervalPSES
                      Gauge,
                  ncmhdmIntervalCCV
                      Gauge,
                  ncmhdmIntervalCES
                      Gauge,
                  ncmhdmIntervalCSES
                      Gauge,
                  ncmhdmIntervalSEFS
                      Gauge,
                  ncmhdmIntervalAISS
                      Gauge,
                  ncmhdmIntervalOOSSs
                      Gauge,
                  ncmhdmIntervalLOSSs
                      Gauge,
                  ncmhdmIntervalLOFSs
                      Gauge
              }
 
        ncmhdmIntervalNIDIndex OBJECT-TYPE
          SYNTAX  INTEGER (1..'7fffffff'h)
          ACCESS  read-only
          STATUS  mandatory
          DESCRIPTION
                "This variable indicates the node id value of the node."
            ::= { ncmhdmIntervalEntry 1 }
               
 
          ncmhdmIntervalIndex OBJECT-TYPE
              SYNTAX  INTEGER (1..65535)
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      "The index value which uniquely identifies the
                      DS3/E3 interface to which this entry is
                      applicable.  The interface identified by a
                      particular value of this index is the same
                      interface as identified by the same value an
                      ncmhdmLineIndex object instance."
              ::= { ncmhdmIntervalEntry 2 }
 
         ncmhdmIntervalNumber OBJECT-TYPE
             SYNTAX  INTEGER (1..96)
             ACCESS  read-only
             STATUS  mandatory
             DESCRIPTION
                "A number between 1 and 96, where 1 is the most
                recently completed 15 minute interval and 96 is
                the least recently completed 15 minutes  inter-
                val   (assuming   that  all  96  intervals  are
                valid)."
            ::= { ncmhdmIntervalEntry 3 }
 
 
          ncmhdmIntervalFarEndCCV OBJECT-TYPE
              SYNTAX  Gauge
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      "The counter associated with the number of
                        Far End C-bit Code Violations,
                       in the previous 15 minute interval."
              ::= { ncmhdmIntervalEntry 4 }


	ncmhdmIntervalFarEndCES OBJECT-TYPE
              SYNTAX  Gauge
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      "The counter associated with the number of
                        Far End C-bit Errored Seconds,
                       in the current 15 minute interval."
              ::= { ncmhdmIntervalEntry 5 }
               
          ncmhdmIntervalFarEndCSES OBJECT-TYPE
              SYNTAX  Gauge
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      "The counter associated with the number of
                        Far End C-bit Severely Errored Seconds,
                       in the current 15 minute interval."
 
              ::= { ncmhdmIntervalEntry 6 }
 
          ncmhdmIntervalFarEndLUAS OBJECT-TYPE
              SYNTAX  Gauge
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      "Far End Line Unavailable Seconds. The counter associated
                      with the number of Seconds the Far End DS3 interface
                      was Out Of Service in the current 15 minute interval."
              ::= { ncmhdmIntervalEntry 7 }
               
          ncmhdmIntervalLCV OBJECT-TYPE
              SYNTAX  Gauge
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      "The counter associated with the number of Line
                      Code Violations, a bipolar or excesive zero violation,
                       in the current 15 minute interval."
              ::= { ncmhdmIntervalEntry 8 }
               
          ncmhdmIntervalLES OBJECT-TYPE
              SYNTAX  Gauge
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      "Line Errored Second Total.  The counter associated
                      with the number of Seconds of in which one or more
                      LCV occurred in the current 15 minute interval."
              ::= { ncmhdmIntervalEntry 9 }
               
          ncmhdmIntervalLSESs OBJECT-TYPE
              SYNTAX  Gauge
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      "The counter associated with the number of
                       Line Severely Errored Seconds (LSES) in
                       the current 15 minute interval."
              ::= { ncmhdmIntervalEntry 10 }


	ncmhdmIntervalPCV OBJECT-TYPE
              SYNTAX  Gauge
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      "The counter associated with the number
                      of Parity Code Violations (locally calculated P-bits
                      disagree with those received),
                       in the current 15 minute interval."
              ::= { ncmhdmIntervalEntry 11 }
               
          ncmhdmIntervalPES OBJECT-TYPE
              SYNTAX  Gauge
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      "The counter associated with the number of Seconds
                      having at least one Parity Code Violation,
                       in the current 15 minute interval."
              ::= { ncmhdmIntervalEntry 12 }
               
          ncmhdmIntervalPSES OBJECT-TYPE
              SYNTAX  Gauge
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      "The counter associated with the number of Seconds
                      having 44 or more Parity Code Violations,
                       in the current 15 minute interval."
              ::= { ncmhdmIntervalEntry 13 }
               
          ncmhdmIntervalCCV OBJECT-TYPE
              SYNTAX  Gauge
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      "The counter associated with the number of
                        C-bit Code Violations,
                       in the current 15 minute interval."
              ::= { ncmhdmIntervalEntry 14 }
               
          ncmhdmIntervalCES OBJECT-TYPE
              SYNTAX  Gauge
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      "The counter associated with the number of
                        C-bit Errored Seconds,
                       in the current 15 minute interval."
              ::= { ncmhdmIntervalEntry 15 }
               
          ncmhdmIntervalCSES OBJECT-TYPE
              SYNTAX  Gauge
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      "The counter associated with the number of
                        C-bit Severely Errored Seconds,
                       in the current 15 minute interval."
              ::= { ncmhdmIntervalEntry 16 }


	 ncmhdmIntervalSEFS OBJECT-TYPE
              SYNTAX  Gauge
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      "The counter associated with the number of seconds
                      in which one or more Out of Frame occurred
                       in the current 15 minute interval."
              ::= { ncmhdmIntervalEntry 17 }
               
          ncmhdmIntervalAISS OBJECT-TYPE
              SYNTAX  Gauge
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      "The counter associated with the number of Seconds
