	-- ncm HDM DS3/E3 Loopback Activate/Deactivate
 
          ncmhdmds3LPBKTable OBJECT-TYPE
              SYNTAX  SEQUENCE OF Ncmhdmds3LPBKEntry
              ACCESS  not-accessible
              STATUS  mandatory
              DESCRIPTION
                      "The DS3/E3 Current table."
                --DEFAULT test-function   ds3-test
--              DEFAULT set-function    null-set-proc
                --DEFAULT next-function   ds3-next
 
              ::= { ncm-ds3 9011 }
 
          ncmhdmds3LPBKEntry OBJECT-TYPE
              SYNTAX  Ncmhdmds3LPBKEntry
              ACCESS  not-accessible
              STATUS  mandatory
              DESCRIPTION
                      "An entry in the DS3 Loopback table."
              INDEX   { ncmhdmLPBKNIDIndex, ncmhdmds3LPBKIndex }
              ::= { ncmhdmds3LPBKTable 1 }
 
          Ncmhdmds3LPBKEntry ::=
              SEQUENCE {
                  ncmhdmLPBKNIDIndex
                      INTEGER,
                  ncmhdmds3LPBKIndex
                      INTEGER,
                  ncmhdmDs3LPBKActivation
                      INTEGER
              }
 
        ncmhdmLPBKNIDIndex OBJECT-TYPE
          SYNTAX  INTEGER (1..'7fffffff'h)
          ACCESS  read-only
          STATUS  mandatory
          DESCRIPTION
                "This variable indicates the node id value of the node."
            ::= { ncmhdmds3LPBKEntry 1 }
               
          ncmhdmds3LPBKIndex OBJECT-TYPE
              SYNTAX  INTEGER (1..65535)
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      "The index value which uniquely identifies the
                      DS3/E3 interface to which this entry is
                      applicable.  The interface identified by a
                      particular value of this index is the same
                      interface as identified by the same value an
                      ncmhdmLineIndex object instance."
              ::= { ncmhdmds3LPBKEntry 2 }


	ncmhdmDs3LPBKActivation OBJECT-TYPE
          SYNTAX INTEGER {
                        no-Loopback (1),
                        dS3-Internal-Activate (2),
                        dS3-Internal-De-activate (3),
                        dS3-Payload-Activate (4),
                        dS3-Payload-De-activate (5),
                        hSSI-A-Activate (6),
                        hSSI-A-De-activate (7),
                        hSSI-B-Activate (8),
                        hSSI-B-De-activate (9),
                        fEAC-Activate (10),
                        fEAC-De-activate (11),
                        far-End-Lpbk-Activate (12),
                        far-End-Lpbk-De-activate (13)
               }
          ACCESS read-write
          STATUS mandatory
          DESCRIPTION
               "Ds3 Loopback enabled/disabled, and Ds3 Loopback Type."
          ::= { ncmhdmds3LPBKEntry 3 }
 
     END
