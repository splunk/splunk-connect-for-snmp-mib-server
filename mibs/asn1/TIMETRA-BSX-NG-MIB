        tmnxBsxHttpEnrichName           TNamedItem,
        tmnxBsxHttpEnrichRowStatus      RowStatus,
        tmnxBsxHttpEnrichRowLastCh      TimeStamp,
        tmnxBsxHttpEnrichEnabled        TmnxEnabledDisabled,
        tmnxBsxHttpEnrichDescription    TItemDescription
    }

