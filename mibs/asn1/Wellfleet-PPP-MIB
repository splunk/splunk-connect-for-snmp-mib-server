		INTEGER,	
            wfPppCircuitLampreyCompDisable
		INTEGER,	
            wfPppCircuitMsgLevel
                INTEGER,
	    wfPppCircuitType
		INTEGER,
	    wfPppCircuitWRCompatability
                INTEGER
        }

