	  v51MlUserActiveVarReq Integer32,
	  v51MlDeleteVarReq     Integer32,
	  v51MlSaveVariant      Integer32,
	  v51MlReadVariant      Integer32,
	  v51MlDeactivateVar    Integer32
	  }
v51MlSltIdx OBJECT-TYPE
      SYNTAX  INTEGER
      {
      io1   (5),
      io2   (6),
      io3   (7),
      io4   (8),
      io5   (9),
      io6  (10),
      io7  (11),
      io8  (12),
      io9  (13),
