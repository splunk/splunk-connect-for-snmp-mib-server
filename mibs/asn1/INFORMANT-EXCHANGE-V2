ex2CatalogsTable OBJECT-TYPE
    SYNTAX     SEQUENCE OF MSFTESQLExchangeCatalogsEntry
    MAX-ACCESS not-accessible
    STATUS     current
    DESCRIPTION
	    "A table containing performance information for
	    instances of 'MSFTESQL-Exchange:Catalogs'
	    objects."
    ::= { exchangeV2 72 }

ex2CatalogsEntry OBJECT-TYPE
    SYNTAX     MSFTESQLExchangeCatalogsEntry
    MAX-ACCESS not-accessible
    STATUS     current
    DESCRIPTION
	    "Performance information for one particular
	    instance of 'MSFTESQL-Exchange:Catalogs'
	    object."
    INDEX      { ex2catInstance }
    ::= { ex2CatalogsTable 1 }

MSFTESQLExchangeCatalogsEntry ::= SEQUENCE {
    ex2catInstance InstanceName,
    ex2catBatchesFPC Gauge32,
    ex2catBatchesAborted Gauge32,
    ex2catBatchesCompletedSuccess Gauge32,
    ex2catBatchesCompletedWPerErrors Gauge32,
    ex2catBatchesCompleteWPerWarning Gauge32,
    ex2catBatchesDone Gauge32,
    ex2catBatchesInProgress Gauge32,
    ex2catBatchesReceived Gauge32,
    ex2catPhaseComplete Gauge32,
    ex2catPhaseInHDFD Gauge32,
    ex2catPhaseInMTFD Gauge32,
    ex2catPhaseInSTFD Gauge32,
    ex2catPhaseInPipeline Gauge32,
    ex2catPhaseInReadyQueue Gauge32,
    ex2catPhaseOnHold Gauge32,
    ex2catPhasePendingCompletion Gauge32,
    ex2catPhaseRedundant Gauge32,
    ex2catRequestAbort Gauge32,
    ex2catRequestAsyncFlush Gauge32,
    ex2catRequestFlushPerDrain Gauge32,
    ex2catRequestForceMerge Gauge32,
    ex2catRequestPause Gauge32,
    ex2catRequestReset Gauge32,
    ex2catRequestResume Gauge32,
    ex2catState Gauge32,
    ex2catStateCallerComponent Gauge32,
    ex2catStateReasonCode Gauge32,
    ex2catStateResultCode Gauge32,
    ex2catTransDeletes Gauge32,
    ex2catTransDone Gauge32,
    ex2catTransErrored Gauge32,
    ex2catTransInProgress Gauge32,
    ex2catTransModifies Gauge32,
    ex2catTransRerouted Gauge32
}

ex2catInstance OBJECT-TYPE
    SYNTAX     InstanceName
    MAX-ACCESS read-only
    STATUS     current
    DESCRIPTION
	    "Instance name."
    ::= { ex2CatalogsEntry 1 }

ex2catBatchesFPC OBJECT-TYPE
    SYNTAX     Gauge32
    MAX-ACCESS read-only
    STATUS     current
    DESCRIPTION
	    "Number of current batches past filter phase
	    complete.  This counter is incremented after
	    all the transactions in the batch are filtered
	    by the filter daemon and given to the plugin(s)
	    for processing."
    ::= { ex2CatalogsEntry 2 }

ex2catBatchesAborted OBJECT-TYPE
    SYNTAX     Gauge32
    MAX-ACCESS read-only
    STATUS     current
    DESCRIPTION
	    "Number of batches aborted to date.  By
	    definition the state of an aborted batch, how
	    far some or all of its transactions got through
	    the indexing process is unknown.  These batches
	    are retried by SQL."
    ::= { ex2CatalogsEntry 3 }

ex2catBatchesCompletedSuccess OBJECT-TYPE
    SYNTAX     Gauge32
    MAX-ACCESS read-only
    STATUS     current
    DESCRIPTION
	    "Number of batches completed without warnings
	    or errors, to date."
    ::= { ex2CatalogsEntry 4 }

