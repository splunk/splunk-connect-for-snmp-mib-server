    ex2sidDocumentIndexingRate Gauge32,
    ex2sidFullCrawlModeStatus Gauge32,
    ex2sidNumContentConversionsDone Gauge32,
    ex2sidNumCreateNotifications Gauge32,
    ex2sidNumCreateNotificationPrSec Gauge32,
    ex2sidNumDeleteNotifications Gauge32,
    ex2sidNumDeleteNotificationPrSec Gauge32,
    ex2sidNumDocumentsSuccessIndexed Gauge32,
    ex2sidNumDocumentFailDuringIndex Gauge32,
    ex2sidNumberOfFailedRetries Gauge32,
    ex2sidNumberOfHTMLMessageBodies Gauge32,
    ex2sidNumberOfIndexedAttachments Gauge32,
    ex2sidNumItemInNotificationQueue Gauge32,
    ex2sidNumMailboxesLeftToCrawl Gauge32,
    ex2sidNumberOfMoveNotifications Gauge32,
    ex2sidNumMoveNotificationsPerSec Gauge32,
    ex2sidNumberOfOutstandingBatches Gauge32,
    ex2sidNumOutstandingDocuments Gauge32,
    ex2sidNumPlainTextMessageBodies Gauge32,
    ex2sidNumberOfRTFMessageBodies Gauge32,
    ex2sidNumRecentMoveMailboxCrawl Gauge32,
    ex2sidNumberOfRetries Gauge32,
    ex2sidNumberOfSuccessfulRetries Gauge32,
    ex2sidNumUpdateNotifications Gauge32,
    ex2sidNumUpdateNotificationPrSec Gauge32,
    ex2sidPercentageNotificOptimize Gauge32,
    ex2sidRecentAvgLatencyRPCUseObta Gauge32,
    ex2sidThrottlingDelayValue Gauge32
}

