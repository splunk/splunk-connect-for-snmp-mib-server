	    "Recipients sent is the number of recipients
	    sent by the SMTPSend Connector."
    ::= { ex2TransportSmtpSendEntry 15 }

ex2UMAutoAttendantTable OBJECT-TYPE
    SYNTAX     SEQUENCE OF Ex2UMAutoAttendantEntry
    MAX-ACCESS not-accessible
    STATUS     current
    DESCRIPTION
	    "A table containing performance information for
	    instances of 'MSExchangeUMAutoAttendant'
	    objects."
    ::= { exchangeV2 63 }

ex2UMAutoAttendantEntry OBJECT-TYPE
    SYNTAX     Ex2UMAutoAttendantEntry
    MAX-ACCESS not-accessible
    STATUS     current
    DESCRIPTION
	    "Performance information for one particular
	    instance of 'MSExchangeUMAutoAttendant'
	    object."
    INDEX      { ex2umaaInstance }
    ::= { ex2UMAutoAttendantTable 1 }

Ex2UMAutoAttendantEntry ::= SEQUENCE {
    ex2umaaInstance InstanceName,
    ex2umaaPercentSuccessfulCalls Gauge32,
    ex2umaaAmbiguousNameTransfers Gauge32,
    ex2umaaAverageCallTime Gauge32,
    ex2umaaAverageRecentCallTime Gauge32,
    ex2umaaBusinessHoursCalls Gauge32,
    ex2umaaCallsDisconUMIrreExtError Gauge32,
    ex2umaaCallsWithDTMFFallback Gauge32,
    ex2umaaCallsWithSentMessage Gauge32,
    ex2umaaCallsWithSpeechInput Gauge32,
    ex2umaaCallsWithSpokenName Gauge32,
    ex2umaaCustomMenuOptions Gauge32,
    ex2umaaDirectoryAccessed Gauge32,
    ex2umaaDirAccsdSuccByDialByName Gauge32,
    ex2umaaDirAccsdSuccBySpokenName Gauge32,
    ex2umaaDirAccsdByDialByName Gauge32,
    ex2umaaDirAccsdByExtension Gauge32,
    ex2umaaDirAccsdBySpokenName Gauge32,
    ex2umaaDisallowedTransfers Gauge32,
    ex2umaaDisconnectedWithoutInput Gauge32,
    ex2umaaMenuOption1Used Gauge32,
    ex2umaaMenuOption2Used Gauge32,
    ex2umaaMenuOption3Used Gauge32,
    ex2umaaMenuOption4Used Gauge32,
    ex2umaaMenuOption5Used Gauge32,
    ex2umaaMenuOption6Used Gauge32,
    ex2umaaMenuOption7Used Gauge32,
    ex2umaaMenuOption8Used Gauge32,
    ex2umaaMenuOption9Used Gauge32,
    ex2umaaMenuOptionTimedOut Gauge32,
    ex2umaaOperatorTransfers Gauge32,
    ex2umaaOperTransReqByUser Gauge32,
    ex2umaaOperTransReqByUseOpenMenu Gauge32,
    ex2umaaOutOfHoursCalls Gauge32,
    ex2umaaSentToAutoAttendant Gauge32,
    ex2umaaTotalCalls Gauge32,
    ex2umaaTransferredCount Gauge32
}

ex2umaaInstance OBJECT-TYPE
    SYNTAX     InstanceName
    MAX-ACCESS read-only
    STATUS     current
    DESCRIPTION
	    "Instance name."
    ::= { ex2UMAutoAttendantEntry 1 }

ex2umaaPercentSuccessfulCalls OBJECT-TYPE
    SYNTAX     Gauge32
    MAX-ACCESS read-only
    STATUS     current
    DESCRIPTION
	    "This counter calculates the success rate of
	    the auto attendant."
    ::= { ex2UMAutoAttendantEntry 2 }

ex2umaaAmbiguousNameTransfers OBJECT-TYPE
    SYNTAX     Gauge32
    MAX-ACCESS read-only
    STATUS     current
    DESCRIPTION
	    "Ambiguous Name Transfers is the number of
	    times that a caller was transferred to the
	    operator because the name that they spelled or
	    spoke was too common in the search results."
    ::= { ex2UMAutoAttendantEntry 3 }

ex2umaaAverageCallTime OBJECT-TYPE
    SYNTAX     Gauge32
    MAX-ACCESS read-only
    STATUS     current
    DESCRIPTION
	    "Average Call Time is the average length of
	    time that callers interacted with the auto
	    attendant."
    ::= { ex2UMAutoAttendantEntry 4 }

ex2umaaAverageRecentCallTime OBJECT-TYPE
    SYNTAX     Gauge32
    MAX-ACCESS read-only
    STATUS     current
    DESCRIPTION
	    "Average Recent Call Time is the average length
	    of time, in seconds, of the last 50 auto
	    attendant calls."
    ::= { ex2UMAutoAttendantEntry 5 }

ex2umaaBusinessHoursCalls OBJECT-TYPE
    SYNTAX     Gauge32
    MAX-ACCESS read-only
    STATUS     current
    DESCRIPTION
	    "Business Hours Calls is the number of calls
	    processed by this auto attendant during
	    business hours."
    ::= { ex2UMAutoAttendantEntry 6 }

