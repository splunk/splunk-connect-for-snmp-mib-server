ex2OledbResourceEntry OBJECT-TYPE
    SYNTAX     Ex2OledbResourceEntry
    MAX-ACCESS not-accessible
    STATUS     current
    DESCRIPTION
	    "Performance information for one particular
	    instance of 'MSExchange Oledb Resource'
	    object."
    INDEX      { ex2orInstance }
    ::= { ex2OledbResourceTable 1 }

Ex2OledbResourceEntry ::= SEQUENCE {
    ex2orInstance InstanceName,
    ex2orActiveCommands Gauge32,
    ex2orActiveDataSources Gauge32,
    ex2orActiveRows Gauge32,
    ex2orActiveRowsets Gauge32,
    ex2orActiveSessions Gauge32,
    ex2orActiveStreams Gauge32,
    ex2orResourceBindingsRate Gauge32,
    ex2orResourceBindingsTotal Gauge32,
    ex2orRowsetsOpenedRate Gauge32,
    ex2orRowsetsOpenedTotal Gauge32,
    ex2orTransactionsAbortedRate Gauge32,
    ex2orTransactionsAbortedTotal Gauge32,
    ex2orTransactionsCommittedRate Gauge32,
    ex2orTransactionsCommittedTotal Gauge32,
    ex2orTransactionsStartedRate Gauge32,
    ex2orTransactionsStartedTotal Gauge32
}

ex2orInstance OBJECT-TYPE
    SYNTAX     InstanceName
    MAX-ACCESS read-only
    STATUS     current
    DESCRIPTION
	    "Instance name."
    ::= { ex2OledbResourceEntry 1 }

