    ::= { exchangeV2 13 }

ex2DatabaseInstancesEntry OBJECT-TYPE
    SYNTAX     Ex2DatabaseInstancesEntry
    MAX-ACCESS not-accessible
    STATUS     current
    DESCRIPTION
	    "Performance information for one particular
	    instance of 'MSExchange Database ==> Instances'
	    object."
    INDEX      { ex2dbiInstance }
    ::= { ex2DatabaseInstancesTable 1 }

Ex2DatabaseInstancesEntry ::= SEQUENCE {
    ex2dbiInstance InstanceName,
    ex2dbiIODatabaseRdAvgLatency Gauge32,
    ex2dbiIODatabaseReadsPerSec Gauge32,
    ex2dbiIODatabsWriteAvgLatency Gauge32,
    ex2dbiIODatabaseWritesPerSec Gauge32,
    ex2dbiIOLogReadsPerSec Gauge32,
    ex2dbiIOLogWritesAvgLatency Gauge32,
    ex2dbiIOLogWritesPerSec Gauge32,
    ex2dbiLogBytesWritePerSec Gauge32,
    ex2dbiLogFileCurrentGeneration Gauge32,
    ex2dbiLogFilesGenerated Gauge32,
    ex2dbiLogFilesGeneratedPremature Gauge32,
    ex2dbiLogGenerationCheckptDepth Gauge32,
    ex2dbiLogGeneratnCheckptDepthMax Gauge32,
    ex2dbiLogGeneratLossResilient Gauge32,
    ex2dbiLogRecordStallsPerSec Gauge32,
    ex2dbiLogThreadsWaiting Gauge32,
    ex2dbiLogWritesPerSec Gauge32,
    ex2dbiPagesConverted Gauge32,
    ex2dbiPagesConvertedPerSec Gauge32,
    ex2dbiRecordsConverted Gauge32,
    ex2dbiRecordsConvertedPerSec Gauge32,
    ex2dbiSessionsPercentUsed Gauge32,
    ex2dbiSessionsInUse Gauge32,
    ex2dbiStreamBackupPageReadPerSec Gauge32,
    ex2dbiTableOpenCachePercentHit Gauge32,
    ex2dbiTableOpenCacheHitsPerSec Gauge32,
    ex2dbiTableOpenCacheMissesPerSec Gauge32,
    ex2dbiTableOpensPerSec Gauge32,
    ex2dbiVersionBucketsAllocated Gauge32
}

ex2dbiInstance OBJECT-TYPE
    SYNTAX     InstanceName
    MAX-ACCESS read-only
    STATUS     current
    DESCRIPTION
	    "Instance name."
    ::= { ex2DatabaseInstancesEntry 1 }

