ex2ADAccessProcessesTable OBJECT-TYPE
    SYNTAX     SEQUENCE OF Ex2ADAccessProcessesEntry
    MAX-ACCESS not-accessible
    STATUS     current
    DESCRIPTION
	    "One instance per client process"
    ::= { exchangeV2 5 }

ex2ADAccessProcessesEntry OBJECT-TYPE
    SYNTAX     Ex2ADAccessProcessesEntry
    MAX-ACCESS not-accessible
    STATUS     current
    DESCRIPTION
	    "Performance information for one particular
	    instance of 'MSExchange ADAccess Processes'
	    object."
    INDEX      { ex2aapInstance }
    ::= { ex2ADAccessProcessesTable 1 }

Ex2ADAccessProcessesEntry ::= SEQUENCE {
    ex2aapInstance InstanceName,
    ex2aapCriticlValidFailuresPerMin Gauge32,
    ex2aapIgnorValidatFailuresPerMin Gauge32,
    ex2aapLDAPNotFndConfigRdCallSec Gauge32,
    ex2aapLDAPPagesPerSec Gauge32,
    ex2aapLDAPReadTime Gauge32,
    ex2aapLDAPReadCallsPerSec Gauge32,
    ex2aapLDAPSearchTime Gauge32,
    ex2aapLDAPSearchCallsPerSec Gauge32,
    ex2aapLDAPTimeoutErrorsPerSec Gauge32,
    ex2aapLDAPVLVRequestsPerSec Gauge32,
    ex2aapLDAPWriteTime Gauge32,
    ex2aapLDAPWriteCallsPerSec Gauge32,
    ex2aapLDAPNotificaReportedPerSec Gauge32,
    ex2aapLDAPNotificaReceivedPerSec Gauge32,
    ex2aapLongRunningLDAPOperaPerMin Gauge32,
    ex2aapNonCriticalValidFailPerMin Gauge32,
    ex2aapNumberOfOutstandingRequest Gauge32,
    ex2aapOpenConnectionToDomainCtrl Gauge32,
    ex2aapOpenConnectionsToGlobalCat Gauge32,
    ex2aapProcessID Gauge32,
    ex2aapTopologyVersion Gauge32
}

ex2aapInstance OBJECT-TYPE
    SYNTAX     InstanceName
    MAX-ACCESS read-only
    STATUS     current
    DESCRIPTION
	    "Instance name."
    ::= { ex2ADAccessProcessesEntry 1 }

ex2aapCriticlValidFailuresPerMin OBJECT-TYPE
    SYNTAX     Gauge32
    MAX-ACCESS read-only
    STATUS     current
    DESCRIPTION
	    "Number of critical validation failures (that
	    cause DataValidationException to be thrown) per
	    minute"
    ::= { ex2ADAccessProcessesEntry 2 }

ex2aapIgnorValidatFailuresPerMin OBJECT-TYPE
    SYNTAX     Gauge32
    MAX-ACCESS read-only
    STATUS     current
    DESCRIPTION
	    "Number of non-critical validation failures
	    that cause an object to be removed from the
	    result set per minute"
    ::= { ex2ADAccessProcessesEntry 3 }

ex2aapLDAPNotFndConfigRdCallSec OBJECT-TYPE
    SYNTAX     Gauge32
    MAX-ACCESS read-only
    STATUS     current
    DESCRIPTION
	    "LDAP Not Found configuration read calls/Sec is
	    the number of Depth 0 LDAP search calls made by
	    this process that failed with 'Not found' per
	    second"
    ::= { ex2ADAccessProcessesEntry 4 }

