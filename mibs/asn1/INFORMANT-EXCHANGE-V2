ex2ISClientEntry OBJECT-TYPE
    SYNTAX     Ex2ISClientEntry
    MAX-ACCESS not-accessible
    STATUS     current
    DESCRIPTION
	    "Performance information for one particular
	    instance of 'MSExchangeIS Client' object."
    INDEX      { ex2ISCInstance }
    ::= { ex2ISClientTable 1 }

Ex2ISClientEntry ::= SEQUENCE {
    ex2ISCInstance InstanceName,
    ex2ISCDirAccCacheEntryAddPerSec Gauge32,
    ex2ISCDirAccCacheEntryExpiredSec Gauge32,
    ex2ISCDirAccCacheHitsPercent Gauge32,
    ex2ISCDirAccLDAPReadsPerSec Gauge32,
    ex2ISCDirAccLDAPSearchesPerSec Gauge32,
    ex2ISCJETLogRecordBytesPerSec Gauge32,
    ex2ISCJETLogRecordsPerSec Gauge32,
    ex2ISCJETPagesModifiedPerSec Gauge32,
    ex2ISCJETPagesPrereadPerSec Gauge32,
    ex2ISCJETPagesReadPerSec Gauge32,
    ex2ISCJETPagesReferencedPerSec Gauge32,
    ex2ISCJETPagesRemodifiedPerSec Gauge32,
    ex2ISCRPCAverageLatency Gauge32,
    ex2ISCRPCBytesReceivedPerSec Gauge32,
    ex2ISCRPCBytesSentPerSec Gauge32,
    ex2ISCRPCOperationsPerSec Gauge32,
    ex2ISCRPCPacketsPerSec Gauge32
}

ex2ISCInstance OBJECT-TYPE
    SYNTAX     InstanceName
    MAX-ACCESS read-only
    STATUS     current
    DESCRIPTION
	    "Instance name."
    ::= { ex2ISClientEntry 1 }

