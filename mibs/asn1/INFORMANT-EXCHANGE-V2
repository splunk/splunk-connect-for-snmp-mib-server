    ::= { ex2Pop3Table 1 }

Ex2Pop3Entry ::= SEQUENCE {
    ex2p3Instance InstanceName,
    ex2p3AUTHFailures Gauge32,
    ex2p3AUTHRate Gauge32,
    ex2p3AUTHTotal Gauge32,
    ex2p3ActiveSSLConnections Gauge32,
    ex2p3AvgComndProcessTimeMillisec Gauge32,
    ex2p3CAPAFailures Gauge32,
    ex2p3CAPARate Gauge32,
    ex2p3CAPATotal Gauge32,
    ex2p3ConnectionsCurrent Gauge32,
    ex2p3ConnectionsFailed Gauge32,
    ex2p3ConnectionsRejected Gauge32,
    ex2p3ConnectionsTotal Gauge32,
    ex2p3DELEFailures Gauge32,
    ex2p3DELERate Gauge32,
    ex2p3DELETotal Gauge32,
    ex2p3InvalidCommands Gauge32,
    ex2p3InvalidCommandsRate Gauge32,
    ex2p3LISTFailures Gauge32,
    ex2p3LISTRate Gauge32,
    ex2p3LISTTotal Gauge32,
    ex2p3NOOPFailures Gauge32,
    ex2p3NOOPRate Gauge32,
    ex2p3NOOPTotal Gauge32,
    ex2p3PASSFailures Gauge32,
    ex2p3PASSRate Gauge32,
    ex2p3PASSTotal Gauge32,
    ex2p3QUITFailures Gauge32,
    ex2p3QUITRate Gauge32,
    ex2p3QUITTotal Gauge32,
    ex2p3RETRFailures Gauge32,
    ex2p3RETRRate Gauge32,
    ex2p3RETRTotal Gauge32,
    ex2p3RSETFailures Gauge32,
    ex2p3RSETRate Gauge32,
    ex2p3RSETTotal Gauge32,
    ex2p3SSLConnections Gauge32,
    ex2p3STATFailures Gauge32,
    ex2p3STATRate Gauge32,
    ex2p3STATTotal Gauge32,
    ex2p3STLSFailures Gauge32,
    ex2p3STLSRate Gauge32,
    ex2p3STLSTotal Gauge32,
    ex2p3TOPFailures Gauge32,
    ex2p3TOPRate Gauge32,
    ex2p3TOPTotal Gauge32,
    ex2p3UIDLFailures Gauge32,
    ex2p3UIDLRate Gauge32,
    ex2p3UIDLTotal Gauge32,
    ex2p3USERFailures Gauge32,
    ex2p3USERRate Gauge32,
    ex2p3USERTotal Gauge32
}

