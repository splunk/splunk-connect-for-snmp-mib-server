ex2StoreInterfaceEntry OBJECT-TYPE
    SYNTAX     Ex2StoreInterfaceEntry
    MAX-ACCESS not-accessible
    STATUS     current
    DESCRIPTION
	    "Performance information for one particular
	    instance of 'MSExchange Store Interface'
	    object."
    INDEX      { ex2sifInstance }
    ::= { ex2StoreInterfaceTable 1 }

Ex2StoreInterfaceEntry ::= SEQUENCE {
    ex2sifInstance InstanceName,
    ex2sifConnCacheActiveConns Gauge32,
    ex2sifConnCacheIdleConns Gauge32,
    ex2sifConnectionCacheNumCaches Gauge32,
    ex2sifConnCacheOutLimitCreations Gauge32,
    ex2sifConnCacheTotalCapacity Gauge32,
    ex2sifExRpcConnCreationEvents Gauge32,
    ex2sifExRpcConnDisposalEvents Gauge32,
    ex2sifExRpcConnectionOutstanding Gauge32,
    ex2sifROPRequestsComplete Gauge32,
    ex2sifROPRequestsOutstanding Gauge32,
    ex2sifROPRequestsSent Gauge32,
    ex2sifRPCBytesReceived Gauge32,
    ex2sifRPCBytesReceivedAverage Gauge32,
    ex2sifRPCBytesSent Gauge32,
    ex2sifRPCBytesSentAverage Gauge32,
    ex2sifRPCLatencyAverageMsec Gauge32,
    ex2sifRPCLatencyTotalMsec Gauge32,
    ex2sifRPCRequestsFailed Gauge32,
    ex2sifRPCRequestsFailedPercent Gauge32,
    ex2sifRPCRequestsFailedException Gauge32,
    ex2sifRPCRequestsOutstanding Gauge32,
    ex2sifRPCRequestsSent Gauge32,
    ex2sifRPCRequestsSentPerSec Gauge32,
    ex2sifRPCRequestsSucceeded Gauge32,
    ex2sifRPCSlowRequests Gauge32,
    ex2sifRPCSlowRequestsPercent Gauge32,
    ex2sifRPCSlowReqLatencyAvgMsec Gauge32,
    ex2sifRPCSlowReqLatencyTotalMsec Gauge32,
    ex2sifUNKFolders Gauge32,
    ex2sifUNKLogons Gauge32,
    ex2sifUNKMessages Gauge32,
    ex2sifUNKObjectsTotal Gauge32
}

