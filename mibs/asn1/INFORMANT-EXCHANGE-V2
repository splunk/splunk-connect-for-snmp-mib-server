ex2UMClientAccessEntry OBJECT-TYPE
    SYNTAX     Ex2UMClientAccessEntry
    MAX-ACCESS not-accessible
    STATUS     current
    DESCRIPTION
	    "Performance information for one particular
	    instance of 'MSExchangeUMClientAccess' object."
    INDEX      { ex2umcacInstance }
    ::= { ex2UMClientAccessTable 1 }

Ex2UMClientAccessEntry ::= SEQUENCE {
    ex2umcacInstance InstanceName,
    ex2umcacPID Gauge32,
    ex2umcacTotalNumPINResetRequests Gauge32,
    ex2umcacTotalNumFailPINResetReq Gauge32,
    ex2umcacTotalNumFailPlayPhoneReq Gauge32,
    ex2umcacTotalNumPlayPhoneRequest Gauge32
}

ex2umcacInstance OBJECT-TYPE
    SYNTAX     InstanceName
    MAX-ACCESS read-only
    STATUS     current
    DESCRIPTION
	    "Instance name."
    ::= { ex2UMClientAccessEntry 1 }

ex2umcacPID OBJECT-TYPE
    SYNTAX     Gauge32
    MAX-ACCESS read-only
    STATUS     current
    DESCRIPTION
	    "Process ID that is hosting the UM client
	    access component"
    ::= { ex2UMClientAccessEntry 2 }

ex2umcacTotalNumPINResetRequests OBJECT-TYPE
    SYNTAX     Gauge32
    MAX-ACCESS read-only
    STATUS     current
    DESCRIPTION
