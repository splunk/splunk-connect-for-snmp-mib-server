INFORMANT-EXCHANGE-V2 DEFINITIONS ::= BEGIN

IMPORTS
    MODULE-IDENTITY, OBJECT-TYPE, OBJECT-IDENTITY, Gauge32
        FROM SNMPv2-SMI
    InstanceName, informant
        FROM WTCS;

exchangeV2 MODULE-IDENTITY
    LAST-UPDATED "200804080009Z"
    ORGANIZATION "Informant Systems, Inc."
    CONTACT-INFO
            "Garth Williams
             11135-23A Ave
             Edmonton, AB  T6J4W5  Canada
             Tel: +1 780 434 4113
             E-mail: garth.williams@wtcs.org"
    DESCRIPTION
            "The MIB module for informant standard entities."
    REVISION      "200804080009Z"
    DESCRIPTION
            "The initial revision of this MIB module."
    ::= { informant 15 }

ex2ADRMSPrelicensingAgent OBJECT-IDENTITY
    STATUS     current
    DESCRIPTION
	    "Performance counters for Exchange AD RMS
	    Prelicensing Agent"
    ::= { exchangeV2 1 }

ex2apaAvgProcessTimeLicenseRqst OBJECT-TYPE
    SYNTAX     Gauge32
    MAX-ACCESS read-only
    STATUS     current
    DESCRIPTION
	    "The Average Processing Time Per License
	    Request (ms) is the average amount of time in
	    milliseconds that it takes the AD RMS
	    Prelicensing agent to process a license."
    ::= { ex2ADRMSPrelicensingAgent 1 }

ex2apaMessageProcessSuccessfully OBJECT-TYPE
    SYNTAX     Gauge32
    MAX-ACCESS read-only
    STATUS     current
    DESCRIPTION
	    "Messages Processed Successfully is the total
	    number of protected messages that were
	    successfully processed by the AD RMS
	    Prelicensing agent."
    ::= { ex2ADRMSPrelicensingAgent 2 }

ex2apaPermanentFailLicenseRqsts OBJECT-TYPE
    SYNTAX     Gauge32
    MAX-ACCESS read-only
    STATUS     current
    DESCRIPTION
	    "Permanent Failure License Requests is the
	    total number of license requests that were made
	    by the AD RMS Prelicensing agent and resulted
	    in permanent failures."
    ::= { ex2ADRMSPrelicensingAgent 3 }

ex2apaSuccessfulLicenseRequests OBJECT-TYPE
    SYNTAX     Gauge32
    MAX-ACCESS read-only
    STATUS     current
    DESCRIPTION
	    "Successful License Requests is the total
	    number of successful license requests that were
	    made by the AD RMS Prelicensing agent."
    ::= { ex2ADRMSPrelicensingAgent 4 }

ex2apaTotalLicenseRequests OBJECT-TYPE
    SYNTAX     Gauge32
    MAX-ACCESS read-only
    STATUS     current
    DESCRIPTION
	    "Total License Requests is the total number of
	    license requests that were made by the AD RMS
	    Prelicensing agent."
    ::= { ex2ADRMSPrelicensingAgent 5 }

ex2apaTotalLicenseRqstsProcTime OBJECT-TYPE
    SYNTAX     Gauge32
    MAX-ACCESS read-only
    STATUS     current
    DESCRIPTION
	    "Total License Requests Processing Time (ms) is
	    the total time spent on license requests by the
	    AD RMS Prelicensing agent in milliseconds."
    ::= { ex2ADRMSPrelicensingAgent 6 }

ex2apaTotalLicenseRequestsPerSec OBJECT-TYPE
    SYNTAX     Gauge32
    MAX-ACCESS read-only
    STATUS     current
    DESCRIPTION
	    "Total License Requests/sec is the total number
	    of license requests that were made by the AD
	    RMS Prelicensing agent per second."
    ::= { ex2ADRMSPrelicensingAgent 7 }

ex2apaTotalMessages OBJECT-TYPE
    SYNTAX     Gauge32
    MAX-ACCESS read-only
    STATUS     current
    DESCRIPTION
	    "Total Messages is the total number of
	    protected messages that were processed by the
	    AD RMS Prelicensing agent."
    ::= { ex2ADRMSPrelicensingAgent 8 }

ex2apaTotalRecipients OBJECT-TYPE
    SYNTAX     Gauge32
    MAX-ACCESS read-only
    STATUS     current
    DESCRIPTION
	    "Total Recipients is the total number of
	    recipients from all protected e-mails that were
	    processed by the AD RMS Prelicensing agent."
    ::= { ex2ADRMSPrelicensingAgent 9 }

ex2ADAccessCachesTable OBJECT-TYPE
    SYNTAX     SEQUENCE OF Ex2ADAccessCachesEntry
    MAX-ACCESS not-accessible
    STATUS     current
    DESCRIPTION
	    "One instance per Cache Instance: global Cache
	    Instance data."
    ::= { exchangeV2 2 }

ex2ADAccessCachesEntry OBJECT-TYPE
    SYNTAX     Ex2ADAccessCachesEntry
    MAX-ACCESS not-accessible
    STATUS     current
    DESCRIPTION
	    "Performance information for one particular
	    instance of 'MSExchange ADAccess Caches'
	    object."
    INDEX      { ex2aacInstance }
    ::= { ex2ADAccessCachesTable 1 }

Ex2ADAccessCachesEntry ::= SEQUENCE {
    ex2aacInstance InstanceName,
    ex2aacCacheExpireTotalConfigData Gauge32,
    ex2aacCacheExpiriesTotalUserData Gauge32,
    ex2aacCacheExpiriesSecConfigData Gauge32,
    ex2aacCacheExpirePerSecUserData Gauge32,
    ex2aacCacheHitsTotal Gauge32,
    ex2aacCacheHitsPerSec Gauge32,
    ex2aacCacheInsertTotalConfigData Gauge32,
    ex2aacCacheInsertsTotalUserData Gauge32,
    ex2aacCacheInsertSecConfigurData Gauge32,
    ex2aacCacheInsertsPerSecUserData Gauge32,
    ex2aacCacheMissesTotal Gauge32,
    ex2aacCacheMissesPerSec Gauge32,
    ex2aacDNEntriesConfigurationData Gauge32,
    ex2aacDNEntriesUserData Gauge32,
    ex2aacDNEntriesMemoryConfigData Gauge32,
    ex2aacDNEntriesMemoryUserData Gauge32,
    ex2aacLDAPSearchesTotal Gauge32,
    ex2aacLDAPSearchesPerSec Gauge32,
    ex2aacNotFndDNEntriesConfigData Gauge32,
    ex2aacNotFoundDNEntriesUserData Gauge32,
    ex2aacNotFndDNEntryMemConfigData Gauge32,
    ex2aacNotFndDNEntriesMemUserData Gauge32,
    ex2aacNotFndGUIDEntryConfigData Gauge32,
    ex2aacNotFndGUIDEntriesUserData Gauge32,
    ex2aacNotFndGUIDEntryMemConfigDa Gauge32,
    ex2aacNotFndGUIDEntryMemUserData Gauge32,
    ex2aacOutstandingAsyncNotifies Gauge32,
    ex2aacOutstandingAsyncReads Gauge32,
    ex2aacOutstandingAsyncSearches Gauge32,
    ex2aacSearchEntriesConfigData Gauge32,
    ex2aacSearchEntriesUserData Gauge32,
    ex2aacSearchEntriesMemConfigData Gauge32,
    ex2aacSearchEntryMemoryUserData Gauge32,
    ex2aacTotalEntriesConfigData Gauge32,
    ex2aacTotalEntriesUserData Gauge32,
    ex2aacTotalEntriesMemConfigData Gauge32,
    ex2aacTotalEntriesMemoryUserData Gauge32
}

ex2aacInstance OBJECT-TYPE
    SYNTAX     InstanceName
    MAX-ACCESS read-only
    STATUS     current
    DESCRIPTION
	    "Instance name."
    ::= { ex2ADAccessCachesEntry 1 }

ex2aacCacheExpireTotalConfigData OBJECT-TYPE
    SYNTAX     Gauge32
    MAX-ACCESS read-only
    STATUS     current
    DESCRIPTION
	    "Cache Expiries Total (Configuration Data) is
	    the total number of configuration data objects
	    expired from the cache since system
	    initialization."
    ::= { ex2ADAccessCachesEntry 2 }

ex2aacCacheExpiriesTotalUserData OBJECT-TYPE
    SYNTAX     Gauge32
    MAX-ACCESS read-only
    STATUS     current
    DESCRIPTION
	    "Cache Expiries Total (User Data) is the total
	    number of user data objects expired from the
	    cache since system initialization."
    ::= { ex2ADAccessCachesEntry 3 }

ex2aacCacheExpiriesSecConfigData OBJECT-TYPE
    SYNTAX     Gauge32
    MAX-ACCESS read-only
    STATUS     current
    DESCRIPTION
	    "Cache Expiries/Sec (Configuration Data) is the
	    number of configuration data objects expired
	    from the cache per second."
    ::= { ex2ADAccessCachesEntry 4 }

ex2aacCacheExpirePerSecUserData OBJECT-TYPE
    SYNTAX     Gauge32
    MAX-ACCESS read-only
    STATUS     current
    DESCRIPTION
	    "Cache Expiries/Sec (User Data) is the number
	    of user data objects expired from the cache per
	    second."
    ::= { ex2ADAccessCachesEntry 5 }

ex2aacCacheHitsTotal OBJECT-TYPE
    SYNTAX     Gauge32
    MAX-ACCESS read-only
    STATUS     current
    DESCRIPTION
	    "Cache Hits Total is the total number of
	    'object found in cache' events since system
	    initialization."
    ::= { ex2ADAccessCachesEntry 6 }

ex2aacCacheHitsPerSec OBJECT-TYPE
    SYNTAX     Gauge32
    MAX-ACCESS read-only
    STATUS     current
    DESCRIPTION
	    "Cache Hits/Sec is the number of  'object found
	    in cache' events per second."
    ::= { ex2ADAccessCachesEntry 7 }

ex2aacCacheInsertTotalConfigData OBJECT-TYPE
    SYNTAX     Gauge32
    MAX-ACCESS read-only
    STATUS     current
    DESCRIPTION
	    "Cache Inserts Total (Configuration Data) is
	    the total number of configuration data objects
	    inserted into the cache since system
	    initialization."
    ::= { ex2ADAccessCachesEntry 8 }

ex2aacCacheInsertsTotalUserData OBJECT-TYPE
    SYNTAX     Gauge32
    MAX-ACCESS read-only
    STATUS     current
    DESCRIPTION
	    "Cache Inserts Total (User Data) is the total
	    number of user data objects inserted into the
	    cache since system initialization."
    ::= { ex2ADAccessCachesEntry 9 }

ex2aacCacheInsertSecConfigurData OBJECT-TYPE
    SYNTAX     Gauge32
    MAX-ACCESS read-only
    STATUS     current
    DESCRIPTION
