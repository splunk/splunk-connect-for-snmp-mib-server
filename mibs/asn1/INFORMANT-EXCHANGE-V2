ex2TransportDumpster OBJECT IDENTIFIER
    ::= { exchangeV2 56 }

ex2tdDumpsterDeletesPerSec OBJECT-TYPE
    SYNTAX     Gauge32
    MAX-ACCESS read-only
    STATUS     current
    DESCRIPTION
	    "Delete Rate is the rate at which items are
	    deleted from the Transport Dumpster on this
	    server."
    ::= { ex2TransportDumpster 1 }

ex2tdDumpsterInsertsPerSec OBJECT-TYPE
    SYNTAX     Gauge32
    MAX-ACCESS read-only
    STATUS     current
    DESCRIPTION
	    "Insert Rate is the rate at which items are
	    inserted into the Transport Dumpster on this
	    server."
    ::= { ex2TransportDumpster 2 }

ex2tdDumpsterItemCount OBJECT-TYPE
    SYNTAX     Gauge32
    MAX-ACCESS read-only
    STATUS     current
    DESCRIPTION
	    "Item Count is the total number of mail  items
	    that are currently in the Transport Dumpster on
	    this server."
    ::= { ex2TransportDumpster 3 }

ex2tdDumpsterSize OBJECT-TYPE
    SYNTAX     Gauge32
    MAX-ACCESS read-only
    STATUS     current
    DESCRIPTION
	    "Item Size is the total size  (in bytes) of
	    mail items that are currently in the Transport
	    Dumpster on this server."
    ::= { ex2TransportDumpster 4 }

ex2tdRedeliveryCount OBJECT-TYPE
    SYNTAX     Gauge32
    MAX-ACCESS read-only
    STATUS     current
    DESCRIPTION
	    "Redelivery Count is the number of mail items
	    that were re-delivered from the Transport
	    Dumpster on this server."
    ::= { ex2TransportDumpster 5 }

ex2TransportPickupTable OBJECT-TYPE
    SYNTAX     SEQUENCE OF Ex2TransportPickupEntry
    MAX-ACCESS not-accessible
    STATUS     current
    DESCRIPTION
	    "A table containing performance information for
	    instances of 'MSExchangeTransport Pickup'
	    objects."
    ::= { exchangeV2 57 }

ex2TransportPickupEntry OBJECT-TYPE
    SYNTAX     Ex2TransportPickupEntry
    MAX-ACCESS not-accessible
    STATUS     current
    DESCRIPTION
	    "Performance information for one particular
	    instance of 'MSExchangeTransport Pickup'
	    object."
    INDEX      { ex2tpInstance }
    ::= { ex2TransportPickupTable 1 }

Ex2TransportPickupEntry ::= SEQUENCE {
    ex2tpkInstance InstanceName,
    ex2tpkMessagesBadmailed Gauge32,
    ex2tpkMessagesNDRed Gauge32,
    ex2tpkMessagesSubmitted Gauge32
}

ex2tpkInstance OBJECT-TYPE
    SYNTAX     InstanceName
    MAX-ACCESS read-only
    STATUS     current
    DESCRIPTION
	    "Instance name."
    ::= { ex2TransportPickupEntry 1 }

ex2tpkMessagesBadmailed OBJECT-TYPE
    SYNTAX     Gauge32
    MAX-ACCESS read-only
    STATUS     current
    DESCRIPTION
	    "Messages Badmailed is the number of messages
	    that were submitted to the Pickup directory but
	    were classified as badmail and not delivered."
    ::= { ex2TransportPickupEntry 2 }

ex2tpkMessagesNDRed OBJECT-TYPE
    SYNTAX     Gauge32
    MAX-ACCESS read-only
    STATUS     current
    DESCRIPTION
	    "Messages NDRed is the number of messages
	    processed by the Pickup directory that causes
	    NDRs to be created."
    ::= { ex2TransportPickupEntry 3 }

ex2tpkMessagesSubmitted OBJECT-TYPE
    SYNTAX     Gauge32
    MAX-ACCESS read-only
    STATUS     current
    DESCRIPTION
	    "Messages Submitted is the number of messages
	    that were successfully submitted by the Pickup
	    directory."
    ::= { ex2TransportPickupEntry 4 }

ex2TransportQueuesTable OBJECT-TYPE
    SYNTAX     SEQUENCE OF Ex2TransportQueuesEntry
    MAX-ACCESS not-accessible
    STATUS     current
    DESCRIPTION
	    "A table containing performance information for
	    instances of 'MSExchangeTransport Queues'
	    objects."
    ::= { exchangeV2 58 }

ex2TransportQueuesEntry OBJECT-TYPE
    SYNTAX     Ex2TransportQueuesEntry
    MAX-ACCESS not-accessible
    STATUS     current
    DESCRIPTION
	    "Performance information for one particular
	    instance of 'MSExchangeTransport Queues'
	    object."
    INDEX      { ex2tqInstance }
    ::= { ex2TransportQueuesTable 1 }

Ex2TransportQueuesEntry ::= SEQUENCE {
    ex2tqInstance InstanceName,
    ex2tqActiveMailboxDelivQueLength Gauge32,
    ex2tqActiveNonSmtpDelivQueLength Gauge32,
    ex2tqActiveRemoteDelivQueLength Gauge32,
    ex2tqAggregDelivQueLengthAllQues Gauge32,
    ex2tqItemCompletedDelivPerSecond Gauge32,
    ex2tqItemsCompletedDeliveryTotal Gauge32,
    ex2tqItemsDeletedByAdminTotal Gauge32,
    ex2tqItemQuedForDelivExpireTotal Gauge32,
    ex2tqItemsQueuedForDeliveryTotal Gauge32,
    ex2tqItemsQuedForDelivPerSecond Gauge32,
    ex2tqItemsResubmittedTotal Gauge32,
    ex2tqLargestDeliveryQueueLength Gauge32,
    ex2tqMsgsCompletedDelivPerSecond Gauge32,
    ex2tqMsgsCompletedDelivTotal Gauge32,
    ex2tqMessagesQueuedForDelivery Gauge32,
    ex2tqMsgsQuedForDelivTotal Gauge32,
    ex2tqMsgsQuedForDelivPerSecond Gauge32,
    ex2tqMessagesSubmittedPerSecond Gauge32,
    ex2tqMessagesSubmittedTotal Gauge32,
    ex2tqPoisonQueueLength Gauge32,
    ex2tqRetryMailboxDelivQueLength Gauge32,
    ex2tqRetryNonSmtpDelivQueLength Gauge32,
    ex2tqRetryRemoteDelivQueLength Gauge32,
    ex2tqSubmissQueueItemExpireTotal Gauge32,
    ex2tqSubmissionQueueLength Gauge32,
    ex2tqUnreachableQueueLength Gauge32
}

ex2tqInstance OBJECT-TYPE
    SYNTAX     InstanceName
    MAX-ACCESS read-only
    STATUS     current
    DESCRIPTION
	    "Instance name."
    ::= { ex2TransportQueuesEntry 1 }

ex2tqActiveMailboxDelivQueLength OBJECT-TYPE
    SYNTAX     Gauge32
    MAX-ACCESS read-only
    STATUS     current
    DESCRIPTION
	    "Active Mailbox Delivery Queue Length is the
	    number of messages in the active mailbox
	    queues."
    ::= { ex2TransportQueuesEntry 2 }

