ex2ISPublicTable OBJECT-TYPE
    SYNTAX     SEQUENCE OF Ex2ISPublicEntry
    MAX-ACCESS not-accessible
    STATUS     current
    DESCRIPTION
	    "A table containing performance information for
	    instances of 'MSExchangeIS Public' objects."
    ::= { exchangeV2 48 }

ex2ISPublicEntry OBJECT-TYPE
    SYNTAX     Ex2ISPublicEntry
    MAX-ACCESS not-accessible
    STATUS     current
    DESCRIPTION
	    "Performance information for one particular
	    instance of 'MSExchangeIS Public' object."
    INDEX      { ex2ispInstance }
    ::= { ex2ISPublicTable 1 }

Ex2ISPublicEntry ::= SEQUENCE {
    ex2ispInstance InstanceName,
    ex2ispActiveClientLogons Gauge32,
    ex2ispAverageDeliveryTime Gauge32,
    ex2ispCategorizationCount Gauge32,
    ex2ispClientLogons Gauge32,
    ex2ispEventHistoryDeletes Gauge32,
    ex2ispEventHistoryDeletesPerSec Gauge32,
    ex2ispEventHistEventCacheHitPerc Gauge32,
    ex2ispEventHistoryEventsCount Gauge32,
    ex2ispEventHistEvntEmptyCntClass Gauge32,
    ex2ispEventHisEventEmptyMsgClass Gauge32,
    ex2ispEventHistEventTrunConClass Gauge32,
    ex2ispEventHistEventTrunMsgClass Gauge32,
    ex2ispEventHistoryReads Gauge32,
    ex2ispEventHistoryReadsPerSec Gauge32,
    ex2ispEventHistUncommitTranCount Gauge32,
    ex2ispEventHistWatermarksCount Gauge32,
    ex2ispEventHistWatermarksDeletes Gauge32,
    ex2ispEventHistWatermarkDelPrSec Gauge32,
    ex2ispEventHistWatermarksReads Gauge32,
    ex2ispEventHistWatermarkRdPerSec Gauge32,
    ex2ispEventHistWatermarksWrites Gauge32,
    ex2ispEventHistWatermarkWriteSec Gauge32,
    ex2ispEventHistoryWrites Gauge32,
    ex2ispEventHistoryWritesPerSec Gauge32,
    ex2ispFolderOpensPerSec Gauge32,
    ex2ispHTTPPerDAVCurrentPendNotif Gauge32,
    ex2ispHTTPPerDAVCurrentSubscript Gauge32,
    ex2ispHTTPPerDAVCurrentTransLock Gauge32,
    ex2ispHTTPPerDAVNotifyReqPerSec Gauge32,
    ex2ispHTTPPerDAVTotalLockCreated Gauge32,
    ex2ispHTTPPerDAVTotalNotifyReqst Gauge32,
    ex2ispHTTPPerDAVTotalSubsCreated Gauge32,
    ex2ispHTTPPerDAVTotalSubsExpired Gauge32,
    ex2ispLogonOperationsPerSec Gauge32,
    ex2ispMessageOpensPerSec Gauge32,
    ex2ispMessageRecipientsDelivered Gauge32,
    ex2ispMsgRecipientsDeliverPerSec Gauge32,
    ex2ispMessagesDelivered Gauge32,
    ex2ispMessagesDeliveredPerSec Gauge32,
    ex2ispMsgsQueuedForSubmission Gauge32,
    ex2ispMessagesSent Gauge32,
    ex2ispMessagesSentPerSec Gauge32,
    ex2ispMessagesSubmitted Gauge32,
    ex2ispMessagesSubmittedPerSec Gauge32,
    ex2ispNumMsgsExpireFrmPublicFold Gauge32,
    ex2ispPeakClientLogons Gauge32,
    ex2ispReceiveQueueSize Gauge32,
    ex2ispReplIDCount Gauge32,
    ex2ispReplicBackfillDataMsgsRecv Gauge32,
    ex2ispReplicBackfillDataMsgsSent Gauge32,
    ex2ispReplicBackfillRequestRecvd Gauge32,
    ex2ispReplicBackfillRequestsSent Gauge32,
    ex2ispReplicFolderChangeReceived Gauge32,
    ex2ispReplicFolderChangesSent Gauge32,
    ex2ispReplicFolderDataMsgReceive Gauge32,
    ex2ispReplicFolderDataMsgsSent Gauge32,
    ex2ispReplicFolderTreeMsgReceive Gauge32,
    ex2ispReplicFolderTreeMsgsSent Gauge32,
    ex2ispReplicMessageChangeReceive Gauge32,
    ex2ispReplicMessageChangesSent Gauge32,
    ex2ispReplicMsgsReceived Gauge32,
    ex2ispReplicationMessagesSent Gauge32,
    ex2ispReplicReceiveQueueSize Gauge32,
    ex2ispReplicStatusMsgsReceived Gauge32,
    ex2ispReplicStatusMsgsSent Gauge32,
    ex2ispRestrictedViewCacheHitRate Gauge32,
    ex2ispRestricteViewCacheMissRate Gauge32,
    ex2ispSearchTaskRate Gauge32,
    ex2ispSingleInstanceRatio Gauge32,
    ex2ispSlowFindRowRate Gauge32,
    ex2ispTotalCountRecoverableItems Gauge32,
    ex2ispTotalSizeOfRecoverableItem Gauge32,
    ex2ispVirusScanBackgrndScnThread Gauge32,
    ex2ispVirusScanBackgrndMsgsScan Gauge32,
    ex2ispVirusScanBackgrndMsgSkippd Gauge32,
    ex2ispVirusScanBackgrndMsgUpDate Gauge32,
    ex2ispVirusScanExtResultAccepted Gauge32,
    ex2ispVirusScanExtResultNotAccpt Gauge32,
    ex2ispVirusScanExtResultsNotPres Gauge32
}

ex2ispInstance OBJECT-TYPE
    SYNTAX     InstanceName
    MAX-ACCESS read-only
    STATUS     current
    DESCRIPTION
	    "Instance name."
    ::= { ex2ISPublicEntry 1 }

ex2ispActiveClientLogons OBJECT-TYPE
    SYNTAX     Gauge32
    MAX-ACCESS read-only
    STATUS     current
    DESCRIPTION
	    "Active Client Logons is the number of clients
	    that performed any action within the last ten
	    minute time interval."
    ::= { ex2ISPublicEntry 2 }

ex2ispAverageDeliveryTime OBJECT-TYPE
    SYNTAX     Gauge32
    MAX-ACCESS read-only
    STATUS     current
    DESCRIPTION
	    "Average Delivery Time is the average time in
	    miliseconds between the submission of a message
	    to the public store and the delivery to all
	    local recipients (recipients on the same
	    server) for the last 10 messages."
    ::= { ex2ISPublicEntry 3 }

ex2ispCategorizationCount OBJECT-TYPE
    SYNTAX     Gauge32
    MAX-ACCESS read-only
    STATUS     current
    DESCRIPTION
	    "Categorization Count is the number of
	    categorizations that exist in the public store.
	    Categorizations are created when a user creates
	    a filtered view or performs a search. When the
	    information store must maintain an excessive
	    number of categorizations, performance can be
	    affected."
    ::= { ex2ISPublicEntry 4 }

ex2ispClientLogons OBJECT-TYPE
    SYNTAX     Gauge32
    MAX-ACCESS read-only
    STATUS     current
    DESCRIPTION
	    "Client Logons is the number of clients
	    (including system processes) currently logged
	    on."
    ::= { ex2ISPublicEntry 5 }

