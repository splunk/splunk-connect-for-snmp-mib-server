    ex2msInstance InstanceName,
    ex2msFailedSubmissions Gauge32,
    ex2msFailedSubmissionsPerSecond Gauge32,
    ex2msHubServers Gauge32,
    ex2msHubServersInRetry Gauge32,
    ex2msSuccessfulSubmissions Gauge32,
    ex2msSuccessfulSubmsPerSecond Gauge32,
    ex2msTemporarySubmissionFailures Gauge32,
    ex2msTemporarySubmFailuresPerSec Gauge32
}

