    ex2esjScanJobFailNotExtLockTotal Gauge32,
    ex2esjScanJobFailDirctErrorTotal Gauge32,
    ex2esjScanJobNotStartNotLockTotl Gauge32,
    ex2esjScanJobsStartedTotal Gauge32,
    ex2esjSourceObjectsScannedTotal Gauge32,
    ex2esjSourceObjectsScannedPerSec Gauge32,
    ex2esjTargetObjectsScannedTotal Gauge32,
    ex2esjTargetObjectsScannedPerSec Gauge32
