    ex2FDSOABInstance InstanceName,
    ex2FDSOABBytesDownloaded Gauge32,
    ex2FDSOABDownloadTaskQueued Gauge32,
    ex2FDSOABDownloadTasksCompleted Gauge32,
    ex2FDSOABTotalBytesToDownload Gauge32
}

