ex2Imap4Table OBJECT-TYPE
    SYNTAX     SEQUENCE OF Ex2Imap4Entry
    MAX-ACCESS not-accessible
    STATUS     current
    DESCRIPTION
	    "A table containing performance information for
	    instances of 'MSExchangeImap4' objects."
    ::= { exchangeV2 49 }

ex2Imap4Entry OBJECT-TYPE
    SYNTAX     Ex2Imap4Entry
    MAX-ACCESS not-accessible
    STATUS     current
    DESCRIPTION
	    "Performance information for one particular
	    instance of 'MSExchangeImap4' object."
    INDEX      { ex2im4Instance }
    ::= { ex2Imap4Table 1 }

Ex2Imap4Entry ::= SEQUENCE {
    ex2im4Instance InstanceName,
    ex2im4ActiveSSLConnections Gauge32,
    ex2im4AppendFailures Gauge32,
    ex2im4AppendRate Gauge32,
    ex2im4AppendTotal Gauge32,
    ex2im4AuthenticateFailures Gauge32,
    ex2im4AuthenticateRate Gauge32,
    ex2im4AuthenticateTotal Gauge32,
    ex2im4AvgCommProcessTimeMillisec Gauge32,
    ex2im4CapabilityFailures Gauge32,
    ex2im4CapabilityRate Gauge32,
    ex2im4CapabilityTotal Gauge32,
    ex2im4CheckFailures Gauge32,
    ex2im4CheckRate Gauge32,
    ex2im4CheckTotal Gauge32,
    ex2im4CloseFailures Gauge32,
    ex2im4CloseRate Gauge32,
    ex2im4CloseTotal Gauge32,
    ex2im4ConnectionsFailed Gauge32,
    ex2im4ConnectionsRejected Gauge32,
    ex2im4CopyFailures Gauge32,
    ex2im4CopyRate Gauge32,
    ex2im4CopyTotal Gauge32,
    ex2im4CreateFailures Gauge32,
    ex2im4CreateRate Gauge32,
    ex2im4CreateTotal Gauge32,
    ex2im4CurrentConnections Gauge32,
    ex2im4DeleteFailures Gauge32,
    ex2im4DeleteRate Gauge32,
    ex2im4DeleteTotal Gauge32,
    ex2im4ExamineFailures Gauge32,
    ex2im4ExamineRate Gauge32,
    ex2im4ExamineTotal Gauge32,
    ex2im4ExpungeFailures Gauge32,
    ex2im4ExpungeRate Gauge32,
    ex2im4ExpungeTotal Gauge32,
    ex2im4FetchFailures Gauge32,
    ex2im4FetchRate Gauge32,
    ex2im4FetchTotal Gauge32,
    ex2im4IdleFailures Gauge32,
    ex2im4IdleRate Gauge32,
    ex2im4IdleTotal Gauge32,
    ex2im4InvalidCommands Gauge32,
    ex2im4InvalidCommandsRate Gauge32,
    ex2im4LSUBFailures Gauge32,
    ex2im4LSUBRate Gauge32,
    ex2im4LSUBTotal Gauge32,
    ex2im4ListFailures Gauge32,
    ex2im4ListRate Gauge32,
    ex2im4ListTotal Gauge32,
