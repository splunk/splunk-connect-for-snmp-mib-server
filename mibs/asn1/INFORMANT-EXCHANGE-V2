    ::= { exchangeV2 47 }

ex2ISMailboxEntry OBJECT-TYPE
    SYNTAX     Ex2ISMailboxEntry
    MAX-ACCESS not-accessible
    STATUS     current
    DESCRIPTION
	    "Performance information for one particular
	    instance of 'MSExchangeIS Mailbox' object."
    INDEX      { ex2mbInstance }
    ::= { ex2ISMailboxTable 1 }

Ex2ISMailboxEntry ::= SEQUENCE {
    ex2mbInstance InstanceName,
    ex2mbActiveClientLogons Gauge32,
    ex2mbAverageDeliveryTime Gauge32,
    ex2mbCategorizationCount Gauge32,
    ex2mbClientLogons Gauge32,
    ex2mbEventHistoryDeletes Gauge32,
    ex2mbEventHistoryDeletesPerSec Gauge32,
    ex2mbEventHistEventCacheHitsPerc Gauge32,
    ex2mbEventHistoryEventsCount Gauge32,
    ex2mbEventHistEventEmptyConClass Gauge32,
    ex2mbEventHistEventEmptyMsgClass Gauge32,
    ex2mbEventHistEventsTrunConClass Gauge32,
    ex2mbEventHistEventsTrunMsgClass Gauge32,
    ex2mbEventHistoryReads Gauge32,
    ex2mbEventHistoryReadsPerSec Gauge32,
    ex2mbEventHistUncommitTransCount Gauge32,
    ex2mbEventHistoryWatermarksCount Gauge32,
    ex2mbEventHistWatermarksDeletes Gauge32,
    ex2mbEventHistWatermarkDelPerSec Gauge32,
    ex2mbEventHistoryWatermarksReads Gauge32,
    ex2mbEventHistWatermarkReadPrSec Gauge32,
    ex2mbEventHistWatermarksWrites Gauge32,
    ex2mbEventHistWatermarkWritPrSec Gauge32,
    ex2mbEventHistoryWrites Gauge32,
    ex2mbEventHistoryWritesPerSec Gauge32,
    ex2mbFolderOpensPerSec Gauge32,
    ex2mbHTTPPerDAVCurrentPendNotif Gauge32,
    ex2mbHTTPPerDAVCurrentSubscript Gauge32,
    ex2mbHTTPPerDAVCurrentTransLocks Gauge32,
    ex2mbHTTPPerDAVNotifyRequestPSec Gauge32,
    ex2mbHTTPPerDAVTotalLocksCreated Gauge32,
    ex2mbHTTPPerDAVTotalNotifyRequst Gauge32,
    ex2mbHTTPPerDAVTotalSubscCreated Gauge32,
    ex2mbHTTPPerDAVTotalSubscExpired Gauge32,
    ex2mbLocalDeliveries Gauge32,
    ex2mbLocalDeliveryRate Gauge32,
    ex2mbLogonOperationsPerSec Gauge32,
    ex2mbMessageOpensPerSec Gauge32,
    ex2mbMessageRecipientsDelivered Gauge32,
    ex2mbMessageRecipientDelivPerSec Gauge32,
    ex2mbMessagesDelivered Gauge32,
    ex2mbMessagesDeliveredPerSec Gauge32,
    ex2mbMessagesQueuedForSubmission Gauge32,
    ex2mbMessagesSent Gauge32,
    ex2mbMessagesSentPerSec Gauge32,
    ex2mbMessagesSubmitted Gauge32,
    ex2mbMessagesSubmittedPerSec Gauge32,
    ex2mbPeakClientLogons Gauge32,
    ex2mbReceiveQueueSize Gauge32,
    ex2mbReplIDCount Gauge32,
    ex2mbRestrictedViewCacheHitRate Gauge32,
    ex2mbRestrictedViewCacheMissRate Gauge32,
    ex2mbSearchTaskRate Gauge32,
    ex2mbSingleInstanceRatio Gauge32,
    ex2mbSlowFindRowRate Gauge32,
    ex2mbTotalCountOfRecoverableItem Gauge32,
    ex2mbTotalSizeOfRecoverableItems Gauge32,
    ex2mbVirusScanBackgrndScanThread Gauge32,
    ex2mbVirusScanBackgrndMsgScanned Gauge32,
    ex2mbVirusScanBackgrndMsgSkipped Gauge32,
