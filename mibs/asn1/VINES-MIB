		--#SUMMARY "	exceeded %lu per min"
		--#ARGUMENTS { 3 }
		--#SEVERITY INFORMATIONAL
		--#TIMEINDEX 0
		--#SYMBOL SNM_ALLOCFAIL_HIGH

		::= 10


nmCacheHits-Low	TRAP-TYPE
