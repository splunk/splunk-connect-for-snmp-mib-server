		ENTERPRISE  mib2
		VARIABLES  {  tpSvrName, tpSvcName, tpAlertTime  }
	
		DESCRIPTION
			"An AppleTalk stack error was detected."
                --#TYPE ""
		--#SUMMARY "AppleTalk stack error detected"
		--#ARGUMENTS { }
		--#SEVERITY INFORMATIONAL
		--#TIMEINDEX 0
		--#SYMBOL AFP_ATALK_DOWN

		::= 70


