        ::= { cdx6500PSTT1E1TGStatCurrent15MinutesReportEntry 11 }

        -- The Interval Error Statistics Table

        cdx6500PSTT1E1TGStatIntervalReportTable OBJECT-TYPE
            SYNTAX SEQUENCE OF Cdx6500PSTT1E1TGStatIntervalReportEntry
            ACCESS not-accessible
            STATUS mandatory
            DESCRIPTION
                "The T1E1 Interval Table contains various
                 statistics collected by each T1E1 Interface over
                 the previous 24 hours of operation.  The past 24
                 hours are broken into 96 completed 15 minute
                 intervals.  "
        ::= { cdx6500PSTT1E1TGPortTable 4 }

        cdx6500PSTT1E1TGStatIntervalReportEntry OBJECT-TYPE
            SYNTAX Cdx6500PSTT1E1TGStatIntervalReportEntry
            ACCESS not-accessible
            STATUS mandatory
            DESCRIPTION
                "Each entry contains statistics information for one T1E1TG Interface."
            INDEX  { cdx6500T1E1TGStatIntervalReportPortNumber,cdx6500T1E1TGStatIntervalNumber }
        ::= { cdx6500PSTT1E1TGStatIntervalReportTable 1 }
        Cdx6500PSTT1E1TGStatIntervalReportEntry ::=
            SEQUENCE {
                cdx6500T1E1TGStatIntervalReportPortNumber
                    INTEGER,
                cdx6500T1E1TGStatIntervalNumber
                    INTEGER,
                cdx6500T1E1TGStatIntervalReportLES
                    Gauge,
                cdx6500T1E1TGStatIntervalReportLCV
                    Gauge,
                cdx6500T1E1TGStatIntervalReportPCV
                    Gauge,
                cdx6500T1E1TGStatIntervalReportCSS
                    Gauge,
                cdx6500T1E1TGStatIntervalReportES
                    Gauge,
                cdx6500T1E1TGStatIntervalReportBES
                    Gauge,
                cdx6500T1E1TGStatIntervalReportSES
                    Gauge,
                cdx6500T1E1TGStatIntervalReportSEFS
                    Gauge,
                cdx6500T1E1VgStatIntervalReportUAS
                    Gauge
                }

        cdx6500T1E1TGStatIntervalReportPortNumber OBJECT-TYPE
            SYNTAX INTEGER
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
                "The index which identifies the Port number of the T1E1."
        ::= { cdx6500PSTT1E1TGStatIntervalReportEntry 1 }

        cdx6500T1E1TGStatIntervalNumber OBJECT-TYPE
            SYNTAX INTEGER (1..96)
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
                 "A number between 1 and 96, where 1 is the most
                 recently completed 15 minute interval and 96 is
                 the 15 minutes interval completed 23 hours and 45
                 minutes prior to interval 1."
        ::= { cdx6500PSTT1E1TGStatIntervalReportEntry 2 }

        cdx6500T1E1TGStatIntervalReportLES OBJECT-TYPE
            SYNTAX Gauge
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
                "The number of Line Errored Seconds (LESs)
                 encountered by the T1E1TG interface in the
                 15 minute interval."
        ::= { cdx6500PSTT1E1TGStatIntervalReportEntry 3 }
        cdx6500T1E1TGStatIntervalReportLCV OBJECT-TYPE
            SYNTAX Gauge
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
                "The number of Line Code Violations (LCVs)
                 encountered by the T1E1TG interface in the
                 15 minute interval."
        ::= { cdx6500PSTT1E1TGStatIntervalReportEntry 4 }

        cdx6500T1E1TGStatIntervalReportPCV OBJECT-TYPE
            SYNTAX Gauge
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
                "The number of Path Coding  Violations (PCVs)
                 encountered by the T1E1TG interface in the
                 15 minute interval."
        ::= { cdx6500PSTT1E1TGStatIntervalReportEntry 5 }

        cdx6500T1E1TGStatIntervalReportCSS OBJECT-TYPE
            SYNTAX Gauge
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
                "The number of Controlled Slip Seconds (CSSs)
                 encountered by the T1E1TG interface in the
                 15 minute interval."
        ::= { cdx6500PSTT1E1TGStatIntervalReportEntry 6 }

        cdx6500T1E1TGStatIntervalReportES OBJECT-TYPE
            SYNTAX Gauge
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
                "The number of Errored Seconds (ESs)
                 encountered by the T1E1TG interface in the
                 15 minute interval."
        ::= { cdx6500PSTT1E1TGStatIntervalReportEntry 7 }

        cdx6500T1E1TGStatIntervalReportBES OBJECT-TYPE
            SYNTAX Gauge
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
                "The number of Bursty Errored Seconds (BESs)
                 encountered by the T1E1TG interface in the
                 15 minute interval."
        ::= { cdx6500PSTT1E1TGStatIntervalReportEntry 8 }
        cdx6500T1E1TGStatIntervalReportSES OBJECT-TYPE
            SYNTAX Gauge
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
                "The number of Severely Errored Seconds (SESs)
                 encountered by the T1E1TG interface in the
                 15 minute interval."
        ::= { cdx6500PSTT1E1TGStatIntervalReportEntry 9 }

        cdx6500T1E1TGStatIntervalReportSEFS OBJECT-TYPE
            SYNTAX Gauge
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
                "The number of Severely Errored Framing Seconds (SEFSs)
                 encountered by the T1E1TG interface in the 
                 15 minute interval."
        ::= { cdx6500PSTT1E1TGStatIntervalReportEntry 10 }

        cdx6500T1E1TGStatIntervalReportUAS OBJECT-TYPE
            SYNTAX Gauge
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
                "The number of Unavailable Seconds (UASs)
                 encountered by the T1E1TG interface in the
                 15 minute interval."
        ::= { cdx6500PSTT1E1TGStatIntervalReportEntry 11 }

        -- The T1E1TG Control Table
        cdx6500ContT1E1TGTable OBJECT IDENTIFIER
        ::= { cdx6500Controls 22 }

        cdx6500T1E1TGContEntry OBJECT-TYPE
            SYNTAX Cdx6500T1E1TGContEntry
            ACCESS not-accessible
            STATUS mandatory
            DESCRIPTION
                "Each control applies to a single T1E1TG Port."
            INDEX { t1e1tgContPortNumber }
        ::= { cdx6500ContT1E1TGTable 1 }

        Cdx6500T1E1TGContEntry ::=
            SEQUENCE {
                t1e1tgContPortNumber
                    INTEGER,
                t1e1tgContPortBoot
                    INTEGER
                }

        t1e1tgContPortNumber OBJECT-TYPE
            SYNTAX INTEGER
            ACCESS not-accessible
            STATUS mandatory
            DESCRIPTION
                "Port number of the T1E1TG."
        ::= { cdx6500T1E1TGContEntry 1 }

        t1e1tgContPortBoot OBJECT-TYPE
            SYNTAX INTEGER {
                   boot       (1),
                   noboot     (2)
                   }
            ACCESS write-only
            STATUS mandatory
            DESCRIPTION
                "Boot operations for the specified T1E1TG Port.
                 boot   : Boots the T1E1TG Port 
                 noboot : Do not Boots the T1E1TG Port."
        ::= { cdx6500T1E1TGContEntry 2 }

        END
