    writeXCalls OBJECT-TYPE
        SYNTAX Unsigned32
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The total number of 'WriteX' CIFS calls received since 
             power up."
        ::= { cifsStatsEntry 26 }

    trans2Calls OBJECT-TYPE
        SYNTAX Unsigned32
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The total number of 'Trans2' CIFS calls received since
             power up."
        ::= { cifsStatsEntry 27 }

    findCloseCalls OBJECT-TYPE
        SYNTAX Unsigned32
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The total number of 'FindClose' CIFS calls received since 
             power up."
        ::= { cifsStatsEntry 28 }

    tdisCalls OBJECT-TYPE
        SYNTAX Unsigned32
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The total number of 'Tdis' CIFS calls received since 
             power up."
        ::= { cifsStatsEntry 29 }

    negProtCalls OBJECT-TYPE
        SYNTAX Unsigned32
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The total number of 'NegProt' CIFS calls received since 
             power up."
        ::= { cifsStatsEntry 30 }

    sessSetupXCalls OBJECT-TYPE
        SYNTAX Unsigned32
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The total number of 'SessSetupX' CIFS calls received since 
             power up."
        ::= { cifsStatsEntry 31 }

    ulogoffXCalls OBJECT-TYPE
        SYNTAX Unsigned32
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The total number of 'UlogoffX' CIFS calls received since 
             power up."
        ::= { cifsStatsEntry 32 }

    tconXCalls OBJECT-TYPE
        SYNTAX Unsigned32
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The total number of 'TconX' CIFS calls received since 
             power up."
        ::= { cifsStatsEntry 33 }

    dskattrCalls OBJECT-TYPE
        SYNTAX Unsigned32
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The total number of 'Dskattr' CIFS calls received since 
             power up."
        ::= { cifsStatsEntry 34 }

    searchCalls OBJECT-TYPE
        SYNTAX Unsigned32
        MAX-ACCESS read-only
        STATUS current
