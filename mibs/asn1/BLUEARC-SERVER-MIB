--------------------------------------------------------------------------------
--
-- Network / Name Service / DNS
--
--------------------------------------------------------------------------------

    dnsServerNumber OBJECT-TYPE
        SYNTAX Integer32
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The number of configured DNS servers."
        ::= { dns 1 }

    dnsServerTable OBJECT-TYPE
        SYNTAX SEQUENCE OF DnsServerEntry
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION
            "A table containing the IP addresses of each of the
             servers which have been configured as DNS servers
             for this system.

             The number of entries in this table is given by the value of 
             dnsServerNumber."
        ::= { dns 2 }

    dnsServerEntry OBJECT-TYPE
        SYNTAX DnsServerEntry
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION
            "Information about the configured DNS server."
        INDEX { dnsServerIndex }
        ::= { dnsServerTable 1 }

    DnsServerEntry ::=
        SEQUENCE {
            dnsServerIndex
                Integer32,
            dnsServerIpAddress
                IpAddress
        }

    dnsServerIndex OBJECT-TYPE
        SYNTAX Integer32 (1..10)
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "A unique value for each configured DNS server.
             This value defines the DNS search ordering.

             Its value ranges between 1 and the value of dnsServerNumber."
        ::= { dnsServerEntry 1 }

    dnsServerIpAddress OBJECT-TYPE
        SYNTAX IpAddress
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The IP address of the DNS server."
        ::= { dnsServerEntry 2 }

    dnsSearchNumber OBJECT-TYPE
        SYNTAX Integer32
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The number of DNS search list entries which are configured 
             for the system."
        ::= { dns 3 }

    dnsSearchTable OBJECT-TYPE
        SYNTAX SEQUENCE OF DnsSearchEntry
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION
            "A table containing the DNS search list which is a list of
             domain suffices.

             The number of entries in this table is given by the value of 
             dnsSearchNumber."
        ::= { dns 4 }

    dnsSearchEntry OBJECT-TYPE
        SYNTAX DnsSearchEntry
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION
            "DNS search information."
        INDEX { dnsSearchIndex }
        ::= { dnsSearchTable 1 }

    DnsSearchEntry ::=
        SEQUENCE {
            dnsSearchIndex
                Integer32,
            dnsSearchString
                DisplayString
        }

    dnsSearchIndex OBJECT-TYPE
        SYNTAX Integer32 (1..10)
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "A unique value for each configured DNS search string.
             This value defines the DNS search ordering.

             Its value ranges between 1 and the value of dnsSearchNumber."
        ::= { dnsSearchEntry 1 }

    dnsSearchString OBJECT-TYPE
        SYNTAX DisplayString
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The DNS search string."
        ::= { dnsSearchEntry 2 }

--------------------------------------------------------------------------------
--
-- Network / Name Service / Name Service Ordering
--
--------------------------------------------------------------------------------

    nameServiceNumber OBJECT-TYPE
        SYNTAX Integer32
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The number of name services configured to be used for hostname
             lookups."
        ::= { nsOrder 1 }

    nameServiceTable OBJECT-TYPE
        SYNTAX SEQUENCE OF NameServiceEntry
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION
            "A table containing the name services which have been
             configured for hostname lookups.

             The number of entries in this table is given by the value of 
             nameServiceNumber."
        ::= { nsOrder 2 }

    nameServiceEntry OBJECT-TYPE
        SYNTAX NameServiceEntry
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION
            "Information about the name service."
        INDEX { nameServiceOrder }
        ::= { nameServiceTable 1 }

    NameServiceEntry ::=
        SEQUENCE {
            nameServiceOrder
                Integer32,
            nameServiceType
                INTEGER
        }

    nameServiceOrder OBJECT-TYPE
        SYNTAX Integer32 (1..4)
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The order in which the lookup is made.

             Its value ranges between 1 and the value of nameServiceNumber."
        ::= { nameServiceEntry 1 }

    nameServiceType OBJECT-TYPE
        SYNTAX INTEGER {
            dns(1),
            wins(2),
            nis(3),
            unknown(4)
        }
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "Indicates the configured service type.

             dns(1)  - domain name service.
             wins(2) - the windows service.
             nis(3)  - network information service."
        ::= { nameServiceEntry 2 }

