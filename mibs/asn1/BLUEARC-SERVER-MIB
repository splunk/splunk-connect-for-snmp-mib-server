    snapshotRulesTable OBJECT-TYPE
        SYNTAX SEQUENCE OF SnapshotRulesEntry
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION
            "A table containing information about each snapshot rule.

             The number of entries in this table is given by the value of
             snapshotRuleNumber."
        ::= { snapshot 5 }

    snapshotRulesEntry OBJECT-TYPE
        SYNTAX SnapshotRulesEntry
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION
            "Information about a snapshot rule."
        INDEX { snapshotRulesEVS, snapshotRulesName }
        ::= { snapshotRulesTable 1 }

    SnapshotRulesEntry ::=
