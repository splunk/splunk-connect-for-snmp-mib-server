--------------------------------------------------------------------------------
--
-- File Protocol / NFS / Statistics
--
--------------------------------------------------------------------------------

    nfsMounts OBJECT-TYPE
        SYNTAX Counter32
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "The total number of NFS mounts since power up.
             
             This object is obsolete.  It could not be measured."
        ::= { nfsStats 3 }

    nfsClients OBJECT-TYPE
        SYNTAX Counter32
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "The total number of NFS clients since power up.

             This object is obsolete.  It could not be measured."
        ::= { nfsStats 4 }

--------------------------------------------------------------------------------
--
-- File Protocol / NFS / NFS Service Configuration
--
--------------------------------------------------------------------------------

    nfsServiceEnabled OBJECT-TYPE
        SYNTAX INTEGER {
            enabled(1),
            disabled(2),
            unknown(3)
        }
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "Indicates whether the NFS service is enabled or not."
        ::= { nfsService 1 }

    nfsServiceMaxUsers OBJECT-TYPE
        SYNTAX Unsigned32
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The maximum number of NFS users the service will allow."
        ::= { nfsService 2 }

--------------------------------------------------------------------------------
--
-- File Protocol / FTP
--
--------------------------------------------------------------------------------

    ftpTimeout OBJECT-TYPE
        SYNTAX Integer32
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "The FTP session timeout in minutes.
             NOTE: this value applies to all FTP sessions.

            This object is obsolete."
        ::= { ftp 1 }

    ftpMountNumber OBJECT-TYPE
        SYNTAX Integer32
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "The number of FTP mount points which are configured
             for the system.

            This object is obsolete."
        ::= { ftpMountPoints 1 }

    ftpMountTable OBJECT-TYPE
        SYNTAX SEQUENCE OF FtpMountEntry
        MAX-ACCESS not-accessible
        STATUS obsolete
        DESCRIPTION
            "A table containing the FTP mount point configuration for the
             system.

             The number of entries in this table is given by the value of 
             ftpMountNumber.

            This object is obsolete."
        ::= { ftpMountPoints 2 }

    ftpMountEntry OBJECT-TYPE
        SYNTAX FtpMountEntry
        MAX-ACCESS not-accessible
        STATUS obsolete
        DESCRIPTION
            "FTP mount point information.

            This object is obsolete."
        INDEX { ftpMountName }
        ::= { ftpMountTable 1 }

    FtpMountEntry ::=
        SEQUENCE {
            ftpMountName
                DisplayString,
            ftpMountNumberUsers
                Unsigned32,
            ftpMountWWN
                DisplayString,
            ftpMountLUN
                Unsigned32,
            ftpMountPartitionID
                Unsigned32
        }

    ftpMountName OBJECT-TYPE
        SYNTAX DisplayString
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "The name of the FTP mount point.  Each FTP mount point
             has a unique name.

            This object is obsolete."
        ::= { ftpMountEntry 1 }

    ftpMountNumberUsers OBJECT-TYPE
        SYNTAX Unsigned32
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "The number of users of the FTP mount point.

            This object is obsolete."
        ::= { ftpMountEntry 2 }

    ftpMountWWN OBJECT-TYPE
        SYNTAX DisplayString
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "The world wide name (WWN) of the system drive the 
             FTP mount point is associated with.

            This object is obsolete."
        ::= { ftpMountEntry 3 }

    ftpMountLUN OBJECT-TYPE
        SYNTAX Unsigned32
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "The logical unit number (LUN) of the system drive
             the FTP mount point is associated with.

            This object is obsolete."
        ::= { ftpMountEntry 4 }

    ftpMountPartitionID OBJECT-TYPE
        SYNTAX Unsigned32
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "The partition ID of the volume the FTP mount point
             is associated with.

            This object is obsolete."
        ::= { ftpMountEntry 5 }

--------------------------------------------------------------------------------
--
-- File Protocol / FTP / FTP Users
--
--------------------------------------------------------------------------------

    ftpUserNumber OBJECT-TYPE
        SYNTAX Integer32
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "The number of FTP users which are configured for the system.

            This object is obsolete."
        ::= { ftpUsers 1 }

    ftpUserTable OBJECT-TYPE
        SYNTAX SEQUENCE OF FtpUserEntry
        MAX-ACCESS not-accessible
        STATUS obsolete
        DESCRIPTION
            "A table containing the FTP user configuration for the
             system.

             The number of entries in this table is given by the value of
             ftpUserNumber.

            This object is obsolete."
        ::= { ftpUsers 2 }

    ftpUserEntry OBJECT-TYPE
        SYNTAX FtpUserEntry
        MAX-ACCESS not-accessible
        STATUS obsolete
        DESCRIPTION
            "FTP user information.

            This object is obsolete."
        INDEX { ftpUserName }
        ::= { ftpUserTable 1 }

    FtpUserEntry ::=
        SEQUENCE {
            ftpUserName
                DisplayString,
            ftpUserMountPointExists
                INTEGER,
            ftpUserMountPoint
                DisplayString,
            ftpUserMountInitDirectory
                DisplayString
        }

    ftpUserName OBJECT-TYPE
        SYNTAX DisplayString
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "The name of the FTP user.

            This object is obsolete."
        ::= { ftpUserEntry 1 }

    ftpUserMountPointExists OBJECT-TYPE
        SYNTAX INTEGER {
            exists(1),
            doesnotExist(2)
        }
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "Indicates whether the FTP mount point is configured in
             the ftpMountTable.

            This object is obsolete."
        ::= { ftpUserEntry 2 }

    ftpUserMountPoint OBJECT-TYPE
        SYNTAX DisplayString
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "The name of the FTP users mount point.

            This object is obsolete."
        ::= { ftpUserEntry 3 }

    ftpUserMountInitDirectory OBJECT-TYPE
        SYNTAX DisplayString
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "The FTP users initial directory.

            This object is obsolete."
        ::= { ftpUserEntry 4 }

--------------------------------------------------------------------------------
--
-- File Protocol / FTP / FTP Audit Logging
--
--------------------------------------------------------------------------------

    ftpAuditLogging OBJECT-TYPE
        SYNTAX INTEGER {
            enabled(1),
            disabled(2)
        }
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "Indicates whether FTP Audit logging is enabled or not.

            This object is obsolete."
        ::= { ftpLogging 1 }

    ftpAuditLogVolumeWWN OBJECT-TYPE
        SYNTAX DisplayString
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "The world wide name (WWN) of the system drive the FTP 
             server is to write the audit log to.

            This object is obsolete."
        ::= { ftpLogging 2 }

    ftpAuditLogVolumeLUN OBJECT-TYPE
        SYNTAX Unsigned32
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "The logical unit number (LUN) of the system drive the
             FTP server is to write the audit log to.

            This object is obsolete."
        ::= { ftpLogging 3 }

    ftpAuditLogVolumePartitionID OBJECT-TYPE
        SYNTAX Unsigned32
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "The partition ID of the volume the FTP server is to
             write the audit log to.

            This object is obsolete."
        ::= { ftpLogging 4 }

    ftpAuditLogDirectory OBJECT-TYPE
        SYNTAX DisplayString
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "The directory the FTP server is to write the audit log to.

            This object is obsolete."
        ::= { ftpLogging 5 }

    ftpAuditLogRecordsPerFile OBJECT-TYPE
        SYNTAX Unsigned32
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "The number of records in each audit log file.

            This object is obsolete."
        ::= { ftpLogging 6 }

    ftpAuditMaximumLogFiles OBJECT-TYPE
        SYNTAX Unsigned32
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "The maximum number of audit log files.

            This object is obsolete."
        ::= { ftpLogging 7 }

