    nisEnabled OBJECT-TYPE
        SYNTAX INTEGER {
            enabled(1),
            disabled(2),
            unknown(3)
        }
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "Indicates whether NIS is enabled on this system or not."
        ::= { nis 1 }

    nisDomain OBJECT-TYPE
        SYNTAX DisplayString
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The NIS domain name."
        ::= { nis 2 }

    nisCurrentMaster OBJECT-TYPE
        SYNTAX DisplayString
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The current NIS master host address."
        ::= { nis 3 }

    nisServerBroadcastEnabled OBJECT-TYPE
        SYNTAX INTEGER {
            enabled(1),
            disabled(2),
            unknown(3)
        }
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "Indicates whether NIS should broadcast for NIS servers 
             on the local network instead of using the configured list
             of servers."
        ::= { nis 4 }

    nisVerificationEnabled OBJECT-TYPE
        SYNTAX INTEGER {
            enabled(1),
            disabled(2),
            unknown(3)
        }
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "Indicates whether NIS verification is enabled or not."
        ::= { nis 5 }

    nisTimeout OBJECT-TYPE
        SYNTAX Unsigned32
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "NIS timeout value in milliseconds."
        ::= { nis 6 }

    nisRebindInterval OBJECT-TYPE
        SYNTAX Unsigned32
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "NIS rebind in minutes."
        ::= { nis 7 }

    nisUserGroupTimeout OBJECT-TYPE
        SYNTAX Unsigned32
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "NIS user/group/netgroup timeout value in minutes."
        ::= { nis 8 }

    nisServerNumber OBJECT-TYPE
        SYNTAX Integer32
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The number of NIS servers which have been configured.

            A value of -1 indicates this information could not
            be determined."
        ::= { nis 9 }

    nisServerTable OBJECT-TYPE
        SYNTAX SEQUENCE OF NisServerEntry
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION
            "A table containing information about each NIS server.

             The number of entries in this table is given by the value of 
             nisServerNumber."
        ::= { nis 10 }

    nisServerEntry OBJECT-TYPE
        SYNTAX NisServerEntry
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION
            "Information about each configured NIS server."
        INDEX { nisServerIndex }
        ::= { nisServerTable 1 }

    NisServerEntry ::=
        SEQUENCE {
            nisServerIndex
                Integer32,
            nisServerIpAddress
                IpAddress,
            nisServerPriority
                Integer32
        }

    nisServerIndex OBJECT-TYPE
        SYNTAX Integer32 (1..50)
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "A unique value for each configured NIS server.

             Its value ranges between 1 and the value of nisServerNumber."
        ::= { nisServerEntry 1 }

    nisServerIpAddress OBJECT-TYPE
        SYNTAX IpAddress
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The IP address of the NIS server."
        ::= { nisServerEntry 2 }

    nisServerPriority OBJECT-TYPE
        SYNTAX Integer32
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The priority assigned to the NIS server.

            A value of -1 indicates this information could not
            be determined."
        ::= { nisServerEntry 3 }
    
--------------------------------------------------------------------------------
--
-- File Protocol / Security
--
--------------------------------------------------------------------------------

    securityMode OBJECT-TYPE
        SYNTAX INTEGER {
            mixed(1),
            unix(2),
            nosecurity(3),
            unknown(4)
        }
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "Indicates the configured security model.

             mixed(1)      - Windows or UNIX environments.
             unix(2)       - primarily UNIX environments.
             nosecurity(3) - all requests granted."
        ::= { security 1 }

    securityDomain OBJECT-TYPE
        SYNTAX DisplayString
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The servers configured domain name."
        ::= { security 2 }

--------------------------------------------------------------------------------
--
-- File Protocol / CIFS / Shares
--
--------------------------------------------------------------------------------

    shareNumber OBJECT-TYPE
        SYNTAX Integer32
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The number of CIFS shares which are configured for the system."
        ::= { shares 1 }

    shareTable OBJECT-TYPE
        SYNTAX SEQUENCE OF ShareEntry
        MAX-ACCESS not-accessible
        STATUS obsolete
        DESCRIPTION
            "A table containing the CIFS share configuration for the
             system.

             The number of entries in this table is given by the value of
             shareNumber."
        ::= { shares 2 }

    shareEntry OBJECT-TYPE
        SYNTAX ShareEntry
        MAX-ACCESS not-accessible
        STATUS obsolete
        DESCRIPTION
            "CIFS share information."
        INDEX { shareName }
        ::= { shareTable 1 }

    ShareEntry ::=
        SEQUENCE {
            shareName
                DisplayString,
            sharePath
                DisplayString,
            shareComment
                DisplayString,
            shareUsers
                Unsigned32,
            shareWWN
                DisplayString,
            shareLUN
                Unsigned32,
            sharePartitionID
                Unsigned32,
            shareMaxUsers
                Integer32
        }

    shareName OBJECT-TYPE
        SYNTAX DisplayString
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "The name of the CIFS share.  Each share has a unique name."
        ::= { shareEntry 1 }

    sharePath OBJECT-TYPE
        SYNTAX DisplayString
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "The path to the CIFS share."
        ::= { shareEntry 2 }

    shareComment OBJECT-TYPE
        SYNTAX DisplayString
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "An optional comment assigned by the creator of the 
             CIFS share to assist in identifying it.
             NOTE: This will be a zero length string if the creator has
                   not assigned a comment to it."
        ::= { shareEntry 3 }

    shareUsers OBJECT-TYPE
        SYNTAX Unsigned32
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "The number of users currently accessing the CIFS share."
        ::= { shareEntry 4 }

    shareWWN OBJECT-TYPE
        SYNTAX DisplayString
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "The world wide name (WWN) of the system drive the CIFS
             share is associated with.
             
             This will identify the File System
             on which the volume associated with the share is hosted."
        ::= { shareEntry 5 }

    shareLUN OBJECT-TYPE
        SYNTAX Unsigned32
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "The logical unit number (LUN) of the system drive the
             CIFS share is associated with.

             This now has no meaning and is set to zero."
        ::= { shareEntry 6 }

    sharePartitionID OBJECT-TYPE
        SYNTAX Unsigned32
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "The partition ID of the volume the CIFS share is associated
             with.

             This now has no meaning and is set to zero."
        ::= { shareEntry 7 }

    shareMaxUsers OBJECT-TYPE
        SYNTAX Integer32
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "The maximum number of users which are permitted access to 
             the CIFS share simultaneously.
             NOTE: this value is set to -1 if the number of maximum
                   number of simultaneous users is unlimited."
        ::= { shareEntry 8 }

    cifsShareTable OBJECT-TYPE
        SYNTAX SEQUENCE OF CifsShareEntry
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION
            "A table containing the CIFS share configuration for the
             system.

             The number of entries in this table is given by the value of
             shareNumber."
        ::= { shares 3 }

    cifsShareEntry OBJECT-TYPE
        SYNTAX CifsShareEntry
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION
            "CIFS share information."
        INDEX { cifsShareEvsId, cifsShareName }
        ::= { cifsShareTable 1 }

    CifsShareEntry ::=
        SEQUENCE {
            cifsShareEvsId
                Integer32,
            cifsShareName
                DisplayString,
            cifsSharePath
                DisplayString,
            cifsShareComment
                DisplayString,
            cifsShareUsers
                Unsigned32,
            cifsShareMaxUsers
                Integer32,
            cifsShareSpanId
                Integer32
        }

    cifsShareEvsId OBJECT-TYPE
        SYNTAX Integer32 (1..255)
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The ID of the Enterprise Virtual Server to which this
            share belongs."
        ::= { cifsShareEntry 1 }

    cifsShareName OBJECT-TYPE
        SYNTAX DisplayString
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The name of the CIFS share.  Each share has a name unique 
            to the EVS to which it belongs."
        ::= { cifsShareEntry 2 }

    cifsSharePath OBJECT-TYPE
        SYNTAX DisplayString
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The path to the CIFS share."
        ::= { cifsShareEntry 3 }

    cifsShareComment OBJECT-TYPE
        SYNTAX DisplayString
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "An optional comment assigned by the creator of the
             CIFS share to assist in identifying it.
             NOTE: This will be a zero length string if the creator has
                   not assigned a comment to it."
        ::= { cifsShareEntry 4 }

    cifsShareUsers OBJECT-TYPE
        SYNTAX Unsigned32
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The number of users currently accessing the CIFS share."
        ::= { cifsShareEntry 5 }

    cifsShareMaxUsers OBJECT-TYPE
        SYNTAX Integer32
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The maximum number of users which are permitted access to
             the CIFS share simultaneously.
             NOTE: this value is set to -1 if the maximum
                   number of simultaneous users is unlimited."
        ::= { cifsShareEntry 6 }

    cifsShareSpanId OBJECT-TYPE
        SYNTAX Integer32
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The ID of the volume the CIFS share is associated
             with."
        ::= { cifsShareEntry 7 }

--------------------------------------------------------------------------------
--
-- File Protocol / CIFS / Share Access
--
--------------------------------------------------------------------------------

    shareAccessNumber OBJECT-TYPE
        SYNTAX Integer32
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The number of CIFS share access entries which are 
             configured for the system."
        ::= { shareAccess 1 }

    shareAccessTable OBJECT-TYPE
        SYNTAX SEQUENCE OF ShareAccessEntry
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION
            "A table containing the CIFS share access configuration 
             for the system.

             The number of entries in this table is given by the value of 
             shareAccessNumber."
        ::= { shareAccess 2 }

    shareAccessEntry OBJECT-TYPE
        SYNTAX ShareAccessEntry
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION
            "CIFS share access information."
        INDEX { shareAccessIndex, shareAccessShareName }
        ::= { shareAccessTable 1 }

    ShareAccessEntry ::=
        SEQUENCE {
            shareAccessIndex
                Integer32,
            shareAccessShareName
                DisplayString,
            shareAccessName
                DisplayString,
            shareAccessPerms
                INTEGER
        }

    shareAccessIndex OBJECT-TYPE
        SYNTAX Integer32 (1..200)
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "An index for each access entry which exists for a
             CIFS share.

             For Si7000 and Si8000 families of SiServers, this 
             value ranges between 1 and the number of access
             entries which exist for the share.
             
             The value is the ID of the EVS of the CIFS share for the 
             access configuration."
        ::= { shareAccessEntry 1 }

    shareAccessShareName OBJECT-TYPE
        SYNTAX DisplayString
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The name of the CIFS share that the access configuration
             entry exists for."
        ::= { shareAccessEntry 2 }

    shareAccessName OBJECT-TYPE
        SYNTAX DisplayString
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The name of the user or group which has been granted access."
        ::= { shareAccessEntry 3 }

    shareAccessPerms OBJECT-TYPE
        SYNTAX INTEGER {
            noAccess(1),
            read(2),
            change(3),
            fullControl(4)
        }
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The access permissions the user or group has been granted to 
             the CIFS share."
        ::= { shareAccessEntry 4 }

