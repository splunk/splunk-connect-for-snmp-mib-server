--------------------------------------------------------------------------------
--
-- System / Server / Clustering
--
--------------------------------------------------------------------------------

    clusterName OBJECT-TYPE
        SYNTAX DisplayString
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The cluster management name."
        ::= { clustering 1 }

    clusterUuid OBJECT-TYPE
        SYNTAX DisplayString
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "A unique identification number for the cluster."
        ::= { clustering 2 }

    clusterConfig OBJECT-TYPE
        SYNTAX INTEGER {
            singleNode(1),
            activeStandby(2),
            activeActive(3),
            unknown(4)
        }
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The configuration of the cluster:
             1 - single standalone node
             2 - active / standby pair
             3 - active / active cluster
             4 - the configuration could not be determined."
        ::= { clustering 3 }

    clusterQuorumDeviceName OBJECT-TYPE
        SYNTAX DisplayString
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The quorum device management name."
        ::= { clustering 4 }

    clusterQuorumDeviceIpAddr OBJECT-TYPE
        SYNTAX IpAddress
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The quorum device IP address, or 0.0.0.0 if no quorum device
             is present."
        ::= { clustering 5 }

    clusterQuorumDeviceOwnedByPNode OBJECT-TYPE
        SYNTAX Integer32
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The identification number of the physical node which
             owns this quorum device. A value of -1 indicates that
             this cannot be determined."
        ::= { clustering 6 }

    clusterQuorumDeviceStatus OBJECT-TYPE
        SYNTAX INTEGER {
            unknown(0),
            unconfigured(1),
            offLine(2),
            owned(3),
            configured(4),
            granted(5),
            clusterNodeNotUp(6),
            misconfigured(7)
        }
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The status of the quorum device:
             0 - unknown
             1 - unconfigured
             2 - off line
             3 - owned
             4 - configured
             5 - granted
             6 - cluster node being queried is not up
             7 - quorum device does not have the correct
                 UUID for the cluster"
        ::= { clustering 7 }

    clusterPNodeNumber OBJECT-TYPE
        SYNTAX Integer32
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The number of physical nodes in the cluster. A value of -1
            indicates that this cannot be determined."
        ::= { clustering 8 }

    clusterPNodeTable OBJECT-TYPE
        SYNTAX SEQUENCE OF ClusterPNodeEntry
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION
            "Information about each physical node in the cluster.

             The number of entries in this table is given by
             clusterPNodeNumber."
        ::= { clustering 9 }

    clusterPNodeEntry OBJECT-TYPE
        SYNTAX ClusterPNodeEntry
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION
            "Information about each physical node."
        INDEX { clusterPNodeId }
        ::= { clusterPNodeTable 1 }

    ClusterPNodeEntry ::=
        SEQUENCE {
            clusterPNodeId
                Integer32,
            clusterPNodeName
                DisplayString,
            clusterPNodeIpAddr
                IpAddress,
            clusterPNodeStatus
                INTEGER
        }

    clusterPNodeId OBJECT-TYPE
        SYNTAX Integer32 (0..255)
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The identification number of the physical node. A value of -1
            indicates that this cannot be determined."
        ::= { clusterPNodeEntry 1 }

    clusterPNodeName OBJECT-TYPE
        SYNTAX DisplayString
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The management name of the physical node."
        ::= { clusterPNodeEntry 2 }

    clusterPNodeIpAddr OBJECT-TYPE
        SYNTAX IpAddress
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The IP address of the physical node. A value of 0.0.0.0 indicates
            that this cannot be determined."
        ::= { clusterPNodeEntry 3 }

    clusterPNodeStatus OBJECT-TYPE
        SYNTAX INTEGER {
            unknown(1),
            up(2),
            notUp(3),
            onLine(4),
            dead(5),
            dormant(6)
        }
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The status of the physical node:
             1 - unknown
             2 - up
             3 - not up
             4 - on line
             5 - dead
             6 - dormant"
        ::= { clusterPNodeEntry 4 }

    clusterVNodeNumber OBJECT-TYPE
        SYNTAX Integer32
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The number of virtual nodes in the cluster. A value of -1
            indicates that this cannot be determined."
        ::= { clustering 10 }

    clusterVNodeTable OBJECT-TYPE
        SYNTAX SEQUENCE OF ClusterVNodeEntry
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION
            "Information about each virtual node in the cluster.

             The number of entries in this table is given by
             clusterVNodeNumber"
        ::= { clustering 11 }

    clusterVNodeEntry OBJECT-TYPE
        SYNTAX ClusterVNodeEntry
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION
            "Information about each virtual node in the cluster."
        INDEX { clusterVNodeId }
        ::= { clusterVNodeTable 1 }

    ClusterVNodeEntry ::=
        SEQUENCE {
            clusterVNodeId
                Integer32,
            clusterVNodeName
                DisplayString,
            clusterVNodeIpAddr
                IpAddress,
            clusterVNodeStatus
                INTEGER,
            clusterVNodeAdmin
                INTEGER,
            clusterVNodeHostedBy
                Integer32
        }

    clusterVNodeId OBJECT-TYPE
        SYNTAX Integer32 (0..255)
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The identification number of the virtual node. A value of -1
            indicates that this cannot be determined."
        ::= { clusterVNodeEntry 1 }

    clusterVNodeName OBJECT-TYPE
        SYNTAX DisplayString
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The management name of the virtual node."
        ::= { clusterVNodeEntry 2 }

    clusterVNodeIpAddr OBJECT-TYPE
        SYNTAX IpAddress
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The IP address of the virtual node. A value of 0.0.0.0 indicates
            that this cannot be determined."
        ::= { clusterVNodeEntry 3 }

    clusterVNodeStatus OBJECT-TYPE
        SYNTAX INTEGER {
            unknown(1),
            onLine(2),
            offLine(3)
        }
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The status of the virtual node:
             1 - unknown
             2 - on line
             3 - off line"
        ::= { clusterVNodeEntry 4 }

    clusterVNodeAdmin OBJECT-TYPE
        SYNTAX INTEGER {
            false(0),
            true(1),
            unknown(2)
        }
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "Whether the virtual node is the administrative virtual node:
             0 - a service node (i.e. not the administrative virtual node)
             1 - the administrative virtual node
             2 - cannot be determined"
        ::= { clusterVNodeEntry 5 }

    clusterVNodeHostedBy OBJECT-TYPE
        SYNTAX Integer32
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The identification number of the physical node hosting this
            virtual node. A value of -1 indicates that this cannot be 
            determined."
        ::= { clusterVNodeEntry 6 }

--------------------------------------------------------------------------------
--
-- System / Server / Serial Numbers
--
--------------------------------------------------------------------------------


    serialNumberTable OBJECT-TYPE
        SYNTAX SEQUENCE OF SerialNumberEntry
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION
            "Serial numbers for server components."
        ::= { serialNumbers 1 }

    serialNumberEntry OBJECT-TYPE
        SYNTAX SerialNumberEntry
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION
            "Serial number for a component."
        INDEX { componentType, subComponentType, clusterNode }
        ::= { serialNumberTable 1 }

    SerialNumberEntry ::=
        SEQUENCE {
            componentType
                INTEGER,
            subComponentType
                INTEGER,
            clusterNode
                Integer32,
            serialNumber
                DisplayString
        }

    componentType OBJECT-TYPE
        SYNTAX INTEGER {
            chassis(1),
            blade(2),
            psu(3)
        }
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The type of component:
             1 - a chassis
             2 - a blade, such as NIM2
             3 - a power supply unit"
        ::= { serialNumberEntry 1 }

    subComponentType OBJECT-TYPE
        SYNTAX INTEGER {
            unknown(0),
            nim1(1),
            nim2(2),
            nim3(3),
            fsa17455(4),
            fsa1(5),
            fsa2(6),
            fsx1(7),
            fsb1(8),
            fsb2(9),
            sim1(10),
            sim2(11),
            chassis(12),
            psu1(13),
            psu2(14),
            fsb3(15),
            sim3(16),
            mcp1(17),
            mmb1(18),
            mfb1(19)
        }
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The sub-type of component."
        ::= { serialNumberEntry 2 }

    clusterNode OBJECT-TYPE
        SYNTAX Integer32 (0..255)
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The identification number of the cluster node hosting this 
             component. A value of -1 indicates that this cannot be 
             determined."
        ::= { serialNumberEntry 3 }

    serialNumber OBJECT-TYPE
        SYNTAX DisplayString
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The serial number of the component."
        ::= { serialNumberEntry 4 }



--------------------------------------------------------------------------------
--
-- System / Storage / Racks
--
--------------------------------------------------------------------------------

    rackNumber OBJECT-TYPE
        SYNTAX Integer32
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "The number of racks (regardless of their current state)
             which are present in this system.

            This object is now obsolete."
        ::= { racks 1 }

    rackTable OBJECT-TYPE
        SYNTAX SEQUENCE OF RackEntry
        MAX-ACCESS not-accessible
        STATUS obsolete
        DESCRIPTION
            "A table containing information about each rack in
             this system.
             
             The number of entries in this table is given by the value of 
             rackNumber.

             This table is now obsolete."
        ::= { racks 2 }

    rackEntry OBJECT-TYPE
        SYNTAX RackEntry
        MAX-ACCESS not-accessible
        STATUS obsolete
        DESCRIPTION
            "Status information about each rack in the system.

             This table is now obsolete."
        INDEX { rackIndex }
        ::= { rackTable 1 }

    RackEntry ::=
        SEQUENCE {
            rackIndex
                Integer32,
            rackStatus
                INTEGER,
            rackNumberOfEnclosures
                Unsigned32,
            rackNumberOfPhysicalDrives
                Unsigned32
        }

    rackIndex OBJECT-TYPE
        SYNTAX Integer32 (1..10)
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "A unique value for each rack present in this system.

             Its value ranges between 1 and the value of rackNumber.

             This object is now obsolete."
        ::= { rackEntry 1 }

    rackStatus OBJECT-TYPE
        SYNTAX INTEGER {
            ok(1),
            storageFailed(2),
            loopFailed(3),
            otherFailure(4)
        }
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "Indicates the status of the rack.

             ok(1)            - the RAID monitoring software is
                                communicating with the rack.
             storageFailed(2) - the RAID controller associated with
                                this rack has failed.
             loopFailed(3)    - the fibre channel loop has failed.
             otherFailure(4)  - the system is unable to communicate with the
                                rack, it may be booting.

             This object is now obsolete."
        ::= { rackEntry 2 }

    rackNumberOfEnclosures OBJECT-TYPE
        SYNTAX Unsigned32
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "The number of storage enclosures which are normally accessible
             by this rack.
             NOTE: If rackStatus is not ok, the value of this variable
                   will be 0.

             This object is now obsolete."
        ::= { rackEntry 3 }

    rackNumberOfPhysicalDrives OBJECT-TYPE
        SYNTAX Unsigned32
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "The number of physical drives which are normally accessible
             by this rack.
             NOTE: If rackStatus is not ok, the value of this variable 
                   will be 0.

             This object is now obsolete."
        ::= { rackEntry 4 }

--------------------------------------------------------------------------------
--
-- System / Storage / RAID / Controllers
--
--------------------------------------------------------------------------------

    raidControllerTable OBJECT-TYPE
        SYNTAX SEQUENCE OF RaidControllerEntry
        MAX-ACCESS not-accessible
        STATUS obsolete
        DESCRIPTION
            "A table containing general information about RAID controller(s)
             present in each rack within the system.

             A rack may contain dual controllers for redundancy purposes.
             This (if present) will be indicated by the secondaryStatus field.

             The number of entries in the table is given by the value of
             rackNumber provided that the system can communicate with the rack.

             This table is now obsolete."
        ::= { controllers 1 }

    raidControllerEntry OBJECT-TYPE
        SYNTAX RaidControllerEntry
        MAX-ACCESS not-accessible
        STATUS obsolete
        DESCRIPTION
            "General information about the RAID controller(s) in a rack.

             This table is now obsolete."
        INDEX { raidControllerRackIndex }
        ::= { raidControllerTable 1 }

    RaidControllerEntry ::=
        SEQUENCE {
            raidControllerRackIndex
                Integer32,
            raidControllerWWN
                DisplayString,
            raidControllerPrimaryStatus
                INTEGER,
            raidControllerSecondaryStatus
                INTEGER,
            raidControllerManufacturer
                DisplayString,
            raidControllerModel
                DisplayString,
            raidControllerCacheSize
                Unsigned32
        }

    raidControllerRackIndex OBJECT-TYPE
        SYNTAX Integer32 (1..10)
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "The rack in which the RAID controller(s) are located.
             The controller(s) identified by a particular value
             of this index is located within the rack identified by the
             same value of rackIndex.

             This object is now obsolete."
        ::= { raidControllerEntry 1 }

    raidControllerWWN OBJECT-TYPE
        SYNTAX DisplayString
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "The world wide name identifying the RAID controller(s).
             NOTE: If the world wide name cannot be obtained this value will
                   be a zero length string.

             This object is now obsolete."
        ::= { raidControllerEntry 2 }

    raidControllerPrimaryStatus OBJECT-TYPE
        SYNTAX INTEGER {
            online(1),
            failed(2),
            notPresent(3),
            unknown(4),
            inserted(5),
            offline(6)
        }
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "The status of primary RAID controller.

             This object is now obsolete."
        ::= { raidControllerEntry 3 }

    raidControllerSecondaryStatus OBJECT-TYPE
        SYNTAX INTEGER {
            online(1),
            failed(2),
            notPresent(3),
            unknown(4)
        }
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "The status of secondary RAID controller.

             NOTE: A status of notPresent(3) indicates that there is only
                   one RAID controller within this rack.

             This object is now obsolete."
        ::= { raidControllerEntry 4 }

    raidControllerManufacturer OBJECT-TYPE
        SYNTAX DisplayString
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "The manufacturer of the RAID controller(s).
             NOTE: If the manufacturer cannot be obtained this value will 
                   be a zero length string.

             This object is now obsolete."
        ::= { raidControllerEntry 5 }

    raidControllerModel OBJECT-TYPE
        SYNTAX DisplayString
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "The model of the RAID controller(s).
             NOTE: If the model cannot be obtained this value will 
                   be a zero length string.

             This object is now obsolete."
        ::= { raidControllerEntry 6 }

    raidControllerCacheSize OBJECT-TYPE
        SYNTAX Unsigned32
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "The size of the RAID controller(s) cache in bytes.
             NOTE: If the cache size cannot be obtained this value will
                   be a 0.

             This object is now obsolete."
        ::= { raidControllerEntry 7 }

    raidControllerConfigTable OBJECT-TYPE
        SYNTAX SEQUENCE OF RaidControllerConfigEntry
        MAX-ACCESS not-accessible
        STATUS obsolete
        DESCRIPTION
            "A table containing configuration information about
             the RAID controller present in each rack within 
             the system.  This information is the same for a
             RAID controller which has redundancy.

             The number of entries in the table is given by the value of
             rackNumber provided that the system can communicate with the rack.

             This object is now obsolete."
        ::= { controllers 2 }

    raidControllerConfigEntry OBJECT-TYPE
        SYNTAX RaidControllerConfigEntry
        MAX-ACCESS not-accessible
        STATUS obsolete
        DESCRIPTION
            "Configuration information about the RAID controller in a rack.

             This object is now obsolete."
        INDEX { raidControllerConfigRackIndex }
        ::= { raidControllerConfigTable 1 }

    RaidControllerConfigEntry ::=
        SEQUENCE {
            raidControllerConfigRackIndex
                Integer32,
            raidControllerConfigDRAMSize
                Unsigned32,
            raidControllerConfigFlashSize
                Unsigned32,
            raidControllerConfigNVRSize
                Unsigned32,
            raidControllerConfigClockSpeed
                Unsigned32,
            raidControllerConfigMemAccess
                Unsigned32,
            raidControllerConfigMemCycle
                Unsigned32,
            raidControllerConfigPhysicalSectorSize
                Unsigned32,
            raidControllerConfigLogicalSectorSize
                Unsigned32,
            raidControllerConfigMaxSystemDrives
                Unsigned32,
            raidControllerConfigMaxDrivesPerSystemDrive
                Unsigned32,
            raidControllerConfigMaxSpanPerVirtualDrive
                Unsigned32,
            raidControllerConfigFirmwareVersion
                DisplayString,
            raidControllerConfigFirmwareType
                Unsigned32,
            raidControllerConfigFirmwareMajor
                Unsigned32,
            raidControllerConfigFirmwareMinor
                Unsigned32,
            raidControllerConfigMaximumCommands
                Unsigned32,
            raidControllerConfigFirmwareBuild
                Unsigned32,
            raidControllerConfigRebuildRate
                Unsigned32,
            raidControllerConfigRebuildUnit
                Unsigned32,
            raidControllerConfigRebuildOn
                Unsigned32,
            raidControllerConfigRebuildOff
                Unsigned32
        }

    raidControllerConfigRackIndex OBJECT-TYPE
        SYNTAX Integer32 (1..10)
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "The rack in which the RAID controller is located.
             The controller identified by a particular value 
             of this index is located within the rack identified by the 
             same value of rackIndex.

             This object is now obsolete."
        ::= { raidControllerConfigEntry 1 }

    raidControllerConfigDRAMSize OBJECT-TYPE
        SYNTAX Unsigned32
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
