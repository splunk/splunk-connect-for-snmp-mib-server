     fcBadCRCErrors OBJECT-TYPE
        SYNTAX Counter32
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "The number of framing errors due to bad cyclic redundancy
             codes, at a fibre channel interface.

            This object is now obsolete."
        ::= { fcStatsEntry 12 }

    fcProtocolErrors OBJECT-TYPE
        SYNTAX Counter32
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "The number of fibre channel protocol errors at a fibre
            channel interface.

            This object is now obsolete."
        ::= { fcStatsEntry 13 }

    fcStatisticsTable OBJECT-TYPE
        SYNTAX SEQUENCE OF FcStatisticsEntry
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION
            "A table of statistics for the fibre channel interfaces."
        ::= { fcStats 25 }

    fcStatisticsEntry OBJECT-TYPE
        SYNTAX FcStatisticsEntry
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION
            "A set of statistics for a fibre channel interface. The
            interface is indexed by fcInterfaceIndex."
        INDEX { fcStatsClusterNode, fcStatsInterfaceIndex }
        ::= { fcStatisticsTable 1 }



    FcStatisticsEntry ::=
        SEQUENCE {
            fcStatsClusterNode
                Integer32,
            fcStatsInterfaceIndex
                Integer32,
            fcStatsInterfaceEnabled
                INTEGER,
            fcStatsInterfaceStatus
                INTEGER,
            fcStatsInterfaceLinkSpeed
                Integer32,
            fcStatsInterfaceLinkType
                INTEGER,
            fcStatsInstantaneousInRate
                Counter32,
            fcStatsInstantaneousOutRate
                Counter32,
            fcStatsPeakInRate
                Counter32,
            fcStatsPeakOutRate
                Counter32,
            fcStatsTotalRxBytes
                Counter64,
            fcStatsTotalTxBytes
                Counter64,
            fcStatsSignalLossErrors
                Counter32,
            fcStatsBadRXCharErrors
                Counter32,
            fcStatsLossSyncErrors
                Counter32,
            fcStatsLinkFailErrors
                Counter32,
            fcStatsRXEOFErrors
                Counter32,
            fcStatsDiscardedFrameErrors
                Counter32,
            fcStatsBadCRCErrors
                Counter32,
            fcStatsProtocolErrors
                Counter32
        }

    fcStatsClusterNode OBJECT-TYPE
        SYNTAX Integer32 (0..128)
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The Cluster Node to which this row applies."
        ::= { fcStatisticsEntry 1 }

    fcStatsInterfaceIndex OBJECT-TYPE
        SYNTAX Integer32 (0..128)
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The index of the fibre channel interface to which
            the statistics in this row apply."
        ::= { fcStatisticsEntry 2 }

    fcStatsInterfaceEnabled OBJECT-TYPE
        SYNTAX INTEGER {
            unknown(0),
            enabled(1),
            disabled(2)
        }
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "Whether the fibre interface is enabled or not.

             unknown(0)  - the information could not be obtained.
             enabled(1)  - the interface is enabled.
             disabled(2) - the interface is disabled."
        ::= { fcStatisticsEntry 3 }

    fcStatsInterfaceStatus OBJECT-TYPE
        SYNTAX INTEGER {
            unknown(0),
            up(1),
            isolated(2),
            down(3)
        }
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The status of the fibre interface.

             unknown(0)  - the information could not be obtained.
             up(1)  - the interface is up.
             isolated(2) - the interface is isolated.
             down(3) - the interface is down."
        ::= { fcStatisticsEntry 4 }

    fcStatsInterfaceLinkSpeed OBJECT-TYPE
        SYNTAX Integer32 (0..128)
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The speed of the fibre channel interface in Gbps."
        ::= { fcStatisticsEntry 5 }

    fcStatsInterfaceLinkType OBJECT-TYPE
        SYNTAX INTEGER {
            unknown(0),
            n(1),
            nl(2)
        }
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The link-type of the fibre interface.

             unknown(0)  - the information could not be obtained.
             n(1)        - fabric direct attach.
             nl(2)       - arbitrated loop operation."
        ::= { fcStatisticsEntry 6 }

    fcStatsInstantaneousInRate OBJECT-TYPE
        SYNTAX Counter32
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The number of bytes received by a fibre channel
             interface in the last second."
        ::= { fcStatisticsEntry 7 }

    fcStatsInstantaneousOutRate OBJECT-TYPE
        SYNTAX Counter32
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
