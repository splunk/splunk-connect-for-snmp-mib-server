--------------------------------------------------------------------------------
--
-- Management / Hardware Flow Control
--
--------------------------------------------------------------------------------

    hwFlowNumber OBJECT-TYPE
        SYNTAX Integer32
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "The number of boards which permit hardware flow control.

            This object is now obsolete, as Hardware Flow is only
            for use by developers."
        ::= { hwFlowControl 1 }

    hwFlowTable OBJECT-TYPE
        SYNTAX SEQUENCE OF HwFlowEntry
        MAX-ACCESS not-accessible
        STATUS obsolete
        DESCRIPTION
            "A table containing the hardware flow control settings
             for the boards within this system.

             The number of entries in this table is given by the value of
             hwFlowNumber.

            This object is now obsolete, as Hardware Flow is only
            for use by developers."
        ::= { hwFlowControl 2 }

    hwFlowEntry OBJECT-TYPE
        SYNTAX HwFlowEntry
        MAX-ACCESS not-accessible
        STATUS obsolete
        DESCRIPTION
            "Hardware flow control setting information.

            This object is now obsolete, as Hardware Flow is only
            for use by developers."
        INDEX { hwFlowIndex }
        ::= { hwFlowTable 1 }

    HwFlowEntry ::=
        SEQUENCE {
            hwFlowIndex
                INTEGER,
            hwFlowDebug
                INTEGER,
            hwFlowConsole
                INTEGER
        }

    hwFlowIndex OBJECT-TYPE
        SYNTAX INTEGER {
            tcp(1),
            fsb(2),
            fcp(3)
        }
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "An identifier for each board.

             tcp(1) - the tcp board.
             fsb(2) - the filesystem board.
             fcp(3) - the fibre channel board.

            This object is now obsolete, as Hardware Flow is only
            for use by developers."
        ::= { hwFlowEntry 1 }

    hwFlowDebug OBJECT-TYPE
        SYNTAX INTEGER {
            enabled(1),
            disabled(2)
        }
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "Indicates whether hardware flow control is enabled on the
             debug port (port number 0) on this board.

            This object is now obsolete, as Hardware Flow is only
            for use by developers."
        ::= { hwFlowEntry 2 }

    hwFlowConsole OBJECT-TYPE
        SYNTAX INTEGER {
            enabled(1),
            disabled(2)
        }
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "Indicates whether hardware flow control is enabled on
             the console port (port number 1) on this board.

            This object is now obsolete, as Hardware Flow is only
            for use by developers."
        ::= { hwFlowEntry 3 }

--------------------------------------------------------------------------------
--
-- performance / utilization
--
--------------------------------------------------------------------------------

    cpuUtilizationNumber OBJECT-TYPE
        SYNTAX Integer32
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The number of CPUs which may be present on this
             system."
        ::= { utilization 1 }

    cpuUtilizationTable OBJECT-TYPE
        SYNTAX SEQUENCE OF CpuUtilizationEntry
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION
            "A table of usage information relevant to each CPU
             which may be present on this system.

             The number of entries in this table is given by the value
             of cpuUtilizationNumber."
        ::= { utilization 2 }

    cpuUtilizationEntry OBJECT-TYPE
        SYNTAX CpuUtilizationEntry
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION
            "Usage information for a CPU which may be
             present on this system."
        INDEX { cpuUtilizationCnIndex, cpuIndex }
        ::= { cpuUtilizationTable 1 }

    CpuUtilizationEntry ::=
        SEQUENCE {
            cpuUtilizationCnIndex
                Unsigned32,
            cpuIndex
                Unsigned32,
            cpuUtilization
                Unsigned32
        }

    cpuUtilizationCnIndex OBJECT-TYPE
        SYNTAX Unsigned32 (1..255)
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The cluster node for this value."
        ::= { cpuUtilizationEntry 1 }

    cpuIndex OBJECT-TYPE
        SYNTAX Unsigned32 
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "CPU core index."
        ::= { cpuUtilizationEntry 2 }

    cpuUtilization OBJECT-TYPE
        SYNTAX Unsigned32
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "Current percentage usage of this CPU."
        ::= { cpuUtilizationEntry 3 }
        
    fpgaUtilizationNumber OBJECT-TYPE
        SYNTAX Integer32
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The number of Field Programmable Gate Arrays (regardless of their
             current state) which may be present on this system."
        ::= { utilization 3 }

    fpgaUtilizationTable OBJECT-TYPE
        SYNTAX SEQUENCE OF FpgaUtilizationEntry
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION
            "A table of usage information relevant to each Field Programmable
             Gate Array which may be present on this system.

             The number of entries in this table is given by the value of
             fpgaUtilizationNumber." 

        ::= { utilization 4 }

    fpgaUtilizationEntry OBJECT-TYPE
        SYNTAX FpgaUtilizationEntry
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION
            "Usage information for a Field Programmable Gate Array which
             may be present on this system."
        INDEX { fpgaUtilizationCnIndex, fpgaUtilizationFpgaIndex }
        ::= { fpgaUtilizationTable 1 }

    FpgaUtilizationEntry ::=
        SEQUENCE {
            fpgaUtilizationCnIndex
                Unsigned32,
            fpgaUtilizationFpgaIndex
                Unsigned32,
            fpgaUtilizationFpgaName
                DisplayString,
            fpgaUtilization
                Unsigned32
        }

    fpgaUtilizationCnIndex OBJECT-TYPE
        SYNTAX Unsigned32 (1..255)
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The cluster node for this value."
        ::= { fpgaUtilizationEntry 1 }

    fpgaUtilizationFpgaIndex OBJECT-TYPE
        SYNTAX Unsigned32
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The index of the Field Programmable Gate Array for this value."
        ::= { fpgaUtilizationEntry 2 }

    fpgaUtilizationFpgaName OBJECT-TYPE
        SYNTAX DisplayString
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The name of the Field Programmable Gate Array for this value."
        ::= { fpgaUtilizationEntry 3 }

    fpgaUtilization OBJECT-TYPE
        SYNTAX Unsigned32
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "Current percentage usage averaged over the last second of
             this Field Programmable Gate Array."
        ::= { fpgaUtilizationEntry 4 }

--------------------------------------------------------------------------------
--
-- performance / systemDriveStats
--
--------------------------------------------------------------------------------
        
    systemDriveStatsNumber OBJECT-TYPE
        SYNTAX Integer32
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The number of system drives for which statistics are available.
            
            If enhanced statistics gathering is not enabled, this
            value will be zero. See the command
            enhanced-perf-stats."
        ::= { systemDriveStats 1 }

    systemDriveStatsTable OBJECT-TYPE
        SYNTAX SEQUENCE OF SystemDriveStatsEntry
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION
            "A table of system drive statistics entries.
            
            If enhanced statistics gathering is not enabled, this
            table will be empty. See the command
            enhanced-perf-stats."
        ::= { systemDriveStats 2 }

    systemDriveStatsEntry OBJECT-TYPE
        SYNTAX SystemDriveStatsEntry
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION
            "System drive statistical information."
        INDEX { systemDriveStatsCnIndex, systemDriveStatsSdId }
        ::= { systemDriveStatsTable 1 }

    SystemDriveStatsEntry ::=
        SEQUENCE {
            systemDriveStatsCnIndex
                Unsigned32,
            systemDriveStatsSdId
                Unsigned32,
            cumNonZeroQueuedReadTime
                Counter64,
            cumNonZeroQueuedWriteTime
                Counter64,
            readCount
                Counter64,
            singleBufferWriteCount
                Counter64,  
            stripeWriteCount
                Counter64,         
            readCumLatency
               Counter64,
            oneWriteCumLatency
               Counter64,
            stripeWriteCumLatency
               Counter64
        }

    systemDriveStatsCnIndex OBJECT-TYPE
        SYNTAX Unsigned32 (1..255)
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The cluster node for this value."
        ::= { systemDriveStatsEntry 1 }

    systemDriveStatsSdId OBJECT-TYPE
        SYNTAX Unsigned32 (1..2147483647)
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The system drive identifier."
        ::= { systemDriveStatsEntry 2 }

    cumNonZeroQueuedReadTime OBJECT-TYPE
        SYNTAX Counter64
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The cumulative time that queued reads have taken since
             this statistic was last reset in microseconds."
        ::= { systemDriveStatsEntry 3 }

    cumNonZeroQueuedWriteTime OBJECT-TYPE
        SYNTAX Counter64
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The cumulative time that queued writes have taken since
             this statistic was last reset in microseconds."
        ::= { systemDriveStatsEntry 4 }

    readCount OBJECT-TYPE
        SYNTAX Counter64
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The total number of reads for this system drive."
        ::= { systemDriveStatsEntry 5 }

    singleBufferWriteCount OBJECT-TYPE
        SYNTAX Counter64
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The total number of single buffer writes for this system drive."
        ::= { systemDriveStatsEntry 6 }

    stripeWriteCount OBJECT-TYPE
        SYNTAX Counter64
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The total number of stripe writes for this system drive."
        ::= { systemDriveStatsEntry 7 }

    readCumLatency OBJECT-TYPE
        SYNTAX Counter64
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The cumulative latency for read operations in microseconds."
        ::= { systemDriveStatsEntry 8 }

    oneWriteCumLatency OBJECT-TYPE
        SYNTAX Counter64
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The cumulative latency for single buffer write operations 
             in microseconds."
        ::= { systemDriveStatsEntry 9 }

    stripeWriteCumLatency OBJECT-TYPE
        SYNTAX Counter64
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The cumulative latency for stripe write operations 
             in microseconds."
        ::= { systemDriveStatsEntry 10 }

--------------------------------------------------------------------------------
--
-- performance / spanStats
--
--------------------------------------------------------------------------------
        
    spanStatsNumber OBJECT-TYPE
        SYNTAX Integer32
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The number of spans for which statistics are available."
        ::= { spanStats 1 }

    spanStatsTable OBJECT-TYPE
        SYNTAX SEQUENCE OF SpanStatsEntry
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION
            "A table of span statistics entries."
        ::= { spanStats 2 }

    spanStatsEntry OBJECT-TYPE
        SYNTAX SpanStatsEntry
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION
            "Span statistical information."
        INDEX { spanStatsSpanId }
        ::= { spanStatsTable 1 }

    SpanStatsEntry ::=
        SEQUENCE {
            spanStatsSpanId
                Integer32,
            spanLabel
                DisplayString,
            spanCapacityTotalUpper
                Unsigned32,
            spanCapacityTotalLower
                Unsigned32,
            spanCapacityUsedUpper
                Unsigned32,
            spanCapacityUsedLower
                Unsigned32
        }

    spanStatsSpanId OBJECT-TYPE
        SYNTAX Integer32 (1..2147483647)
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The unique identifier for this span, generated from the first
             system drive in the span."
        ::= { spanStatsEntry 1 }

    spanLabel OBJECT-TYPE
        SYNTAX DisplayString
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The label for a span."
        ::= { spanStatsEntry 2 }

    spanCapacityTotalUpper OBJECT-TYPE
        SYNTAX Unsigned32
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The high order 32-bits representing the span's capacity in bytes.
            (see also spanCapacityTotalLower)"
        ::= { spanStatsEntry 3 }

    spanCapacityTotalLower OBJECT-TYPE
        SYNTAX Unsigned32
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The low order 32-bits representing the span's capacity in bytes.
            (see also spanCapacityTotalUpper)"
        ::= { spanStatsEntry 4 }

    spanCapacityUsedUpper OBJECT-TYPE
        SYNTAX Unsigned32
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The high order 32-bits representing the span's used space in bytes.
            (see also spanCapacityUsedLower)"
        ::= { spanStatsEntry 5 }

    spanCapacityUsedLower OBJECT-TYPE
        SYNTAX Unsigned32
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The low order 32-bits representing the span's used space in bytes.
            (see also spanCapacityUsedUpper)"
        ::= { spanStatsEntry 6 }

