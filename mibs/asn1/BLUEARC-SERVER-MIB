--
-- System / Storage / Snapshot Rules
--
--------------------------------------------------------------------------------

    snapshotRuleNumber OBJECT-TYPE
        SYNTAX Integer32
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The number of snapshot rules which are configured for
             this system."
        ::= { snapshot 1 }

    snapshotRuleTable OBJECT-TYPE
        SYNTAX SEQUENCE OF SnapshotRuleEntry
        MAX-ACCESS not-accessible
        STATUS obsolete
        DESCRIPTION
            "A table containing information about each snapshot rule.

             The number of entries in this table is given by the value of
             snapshotRuleNumber.

             This object is now obsolete."
        ::= { snapshot 2 }

    snapshotRuleEntry OBJECT-TYPE
        SYNTAX SnapshotRuleEntry
        MAX-ACCESS not-accessible
        STATUS obsolete
        DESCRIPTION
            "Information about a snapshot rule.

             This object is now obsolete."
        INDEX { snapshotRuleName }
        ::= { snapshotRuleTable 1 }

    SnapshotRuleEntry ::=
        SEQUENCE {
            snapshotRuleName
                DisplayString,
            snapshotRuleWWN
                DisplayString,
            snapshotRuleLUN
                Unsigned32,
            snapshotRulePartitionID
                Unsigned32,
            snapshotRuleQueueSize
                Integer32,
            snapshotRuleVolumeLabel
                DisplayString
        }

    snapshotRuleName OBJECT-TYPE
        SYNTAX DisplayString
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "The name of the snapshot rule.  Each snapshot rule has a unique
             name.

             This object is now obsolete."
        ::= { snapshotRuleEntry 1 }

    snapshotRuleWWN OBJECT-TYPE
        SYNTAX DisplayString
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "The world wide name (WWN) of the system drive the snapshot rule
             is associated with.

             This object is now obsolete."
        ::= { snapshotRuleEntry 2 }

    snapshotRuleLUN OBJECT-TYPE
        SYNTAX Unsigned32
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "The logical unit number (LUN) of the system drive the
             snapshot rule is associated with.

             This object is now obsolete."
        ::= { snapshotRuleEntry 3 }

    snapshotRulePartitionID OBJECT-TYPE
        SYNTAX Unsigned32
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "The partition ID of the volume the snapshot rule is associated
             with.

             This object is now obsolete."
        ::= { snapshotRuleEntry 4 }

    snapshotRuleQueueSize OBJECT-TYPE
        SYNTAX Integer32
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "The number of snapshots to keep before the system deletes the
             oldest snapshot.

             This object is now obsolete."
        ::= { snapshotRuleEntry 5 }

    snapshotRuleVolumeLabel OBJECT-TYPE
        SYNTAX DisplayString
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "The label of the volume to which the snapshot rule relates.

             This object is now obsolete."
        ::= { snapshotRuleEntry 6 }

--------------------------------------------------------------------------------
--
-- System / Storage / Snapshot Schedules
--
--------------------------------------------------------------------------------

    snapshotScheduleNumber OBJECT-TYPE
        SYNTAX Integer32
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The number of snapshot schedules which are configured for
             this system."
        ::= { snapshot 3 }

    snapshotScheduleTable OBJECT-TYPE
        SYNTAX SEQUENCE OF SnapshotScheduleEntry
        MAX-ACCESS not-accessible
        STATUS obsolete
        DESCRIPTION
            "A table containing information about each snapshot schedule.

             The number of entries in this table is given by the value of
             snapshotScheduleNumber.

             This object is now obsolete."
        ::= { snapshot 4 }

    snapshotScheduleEntry OBJECT-TYPE
        SYNTAX SnapshotScheduleEntry
        MAX-ACCESS not-accessible
        STATUS obsolete
        DESCRIPTION
            "Information about a snapshot schedule.

             This object is now obsolete."
        INDEX { snapshotScheduleRuleName, snapshotScheduleIndex }
        ::= { snapshotScheduleTable 1 }

    SnapshotScheduleEntry ::=
        SEQUENCE {
            snapshotScheduleRuleName
                DisplayString,
            snapshotScheduleIndex
                Integer32,
            snapshotScheduleDateTimeSpec
                DisplayString
        }

    snapshotScheduleRuleName OBJECT-TYPE
        SYNTAX DisplayString
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "The name of the parent snapshot rule that this schedule
             is associated with.

             This object is now obsolete."
        ::= { snapshotScheduleEntry 1 }

    snapshotScheduleIndex OBJECT-TYPE
        SYNTAX Integer32 (1..100)
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "An identifier for this snapshot schedule entry.
             This identifier ranges from 1 to the number of snapshot
             schedules configured for the parent snapshot rule.

             This object is now obsolete."
        ::= { snapshotScheduleEntry 2 }

    snapshotScheduleDateTimeSpec OBJECT-TYPE
        SYNTAX DisplayString
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "The date and time specification for when this snapshot
             schedule will be run in cron format.

             This object is now obsolete."
        ::= { snapshotScheduleEntry 3 }

