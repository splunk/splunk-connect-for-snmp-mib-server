    puStatsNumber OBJECT-TYPE
        SYNTAX Integer32
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "The number of power units from which statistics
             can be gathered.  Statistics are only available for
             power units that the system is able to communicate with.

             This object is obsolete. Power units must be indexed by
             IP address."
        ::= { powerUnits 5 }

    puStatsTable OBJECT-TYPE
        SYNTAX SEQUENCE OF PuStatsEntry
        MAX-ACCESS not-accessible
        STATUS obsolete
        DESCRIPTION
            "A table containing statistics for each power unit the system
             is communicating with.

             The number of entries in this table is given by the value of
             puStatsNumber.

             This object is obsolete. Power units must be indexed by
             IP address."
        ::= { powerUnits 6 }

    puStatsEntry OBJECT-TYPE
        SYNTAX PuStatsEntry
        MAX-ACCESS not-accessible
        STATUS obsolete
        DESCRIPTION
            "Statistics for each power unit the system is communicating
             with.

             This object is obsolete. Power units must be indexed by
             IP address."
        INDEX { puStatsIndex }
        ::= { puStatsTable 1 }

    PuStatsEntry ::=
        SEQUENCE {
            puStatsIndex
                Integer32,
            puStatsBatteryCharge
                INTEGER,
            puStatsLoad
                INTEGER,
            puStatsFrequency
                Integer32,
            puStatsLineVoltage
                Integer32,
            puStatsOutputVoltage
                Integer32,
            puStatsBatteryVoltage
                Integer32,
            puStatsTemperatureC
                Integer32,
            puStatsTemperatureF
                Integer32,
            puStatsLineMininumVoltage
                Integer32,
            puStatsLineMaximumVoltage
                Integer32,
            puStatsRemainingRunTime
                Integer32
        }

    puStatsIndex OBJECT-TYPE
        SYNTAX Integer32 (1..2)
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "The power unit from which the statistics have been gathered.
             The power unit identified by a particular value of this
             index is the same power unit as identified by the same value
             of puConfigurationIndex.

             This object is obsolete. Power units must be indexed by
             IP address."
        ::= { puStatsEntry 1 }

    puStatsBatteryCharge OBJECT-TYPE
        SYNTAX INTEGER (0..100)
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "The percentage by which the battery is charged.

             This object is obsolete. Power units must be indexed by
             IP address."
        ::= { puStatsEntry 2 }

    puStatsLoad OBJECT-TYPE
        SYNTAX INTEGER (0..100)
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "The percentage of the rated load in Watts.

             This object is obsolete. Power units must be indexed by
             IP address."
        ::= { puStatsEntry 3 }

    puStatsFrequency OBJECT-TYPE
        SYNTAX Integer32
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "The frequency of the AC mains in Hertz.

             This object is obsolete. Power units must be indexed by
             IP address."
        ::= { puStatsEntry 4 }

    puStatsLineVoltage OBJECT-TYPE
        SYNTAX Integer32
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "The AC mains voltage entering the power unit in Volts.

             This object is obsolete. Power units must be indexed by
             IP address."
        ::= { puStatsEntry 5 }

    puStatsOutputVoltage OBJECT-TYPE
        SYNTAX Integer32
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "The output voltage of the power unit in Volts.

             This object is obsolete. Power units must be indexed by
             IP address."
        ::= { puStatsEntry 6 }

    puStatsBatteryVoltage OBJECT-TYPE
        SYNTAX Integer32
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "The battery voltage in Volts.

             This object is obsolete. Power units must be indexed by
             IP address."
        ::= { puStatsEntry 7 }

    puStatsTemperatureC OBJECT-TYPE
        SYNTAX Integer32
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "The temperature inside the power unit in degrees Celsius.

             This object is obsolete. Power units must be indexed by
             IP address."
        ::= { puStatsEntry 8 }

    puStatsTemperatureF OBJECT-TYPE
        SYNTAX Integer32
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "The temperature inside the power unit in degrees Fahrenheit.

             This object is obsolete. Power units must be indexed by
             IP address."
        ::= { puStatsEntry 9 }

    puStatsLineMininumVoltage OBJECT-TYPE
        SYNTAX Integer32
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "The minimum line voltage in Volts.

             This object is obsolete. Power units must be indexed by
             IP address."
        ::= { puStatsEntry 10 }

    puStatsLineMaximumVoltage OBJECT-TYPE
        SYNTAX Integer32
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "The maximum line voltage in Volts.

             This object is obsolete. Power units must be indexed by
             IP address."
        ::= { puStatsEntry 11 }

    puStatsRemainingRunTime OBJECT-TYPE
        SYNTAX Integer32
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "The remaining runtime of the battery based on the
             obsolete load in minutes.

             This object is obsolete. Power units must be indexed by
             IP address."
        ::= { puStatsEntry 12 }

    puStatisticsCount OBJECT-TYPE
        SYNTAX Integer32
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The number of power units from which statistics
             can be gathered.  Statistics are only available for
             power units that the system is able to communicate with."
        ::= { powerUnits 11 }

    puStatisticsTable OBJECT-TYPE
        SYNTAX SEQUENCE OF PuStatisticsEntry
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION
            "A table containing statistics for each power unit the system
             is communicating with.

             The number of entries in this table is given by the value of
             puStatisticsCount."
        ::= { powerUnits 12 }

    puStatisticsEntry OBJECT-TYPE
        SYNTAX PuStatisticsEntry
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION
            "Statistics for each power unit the system is communicating
             with."
        INDEX { puStatisticsIndex }
        ::= { puStatisticsTable 1 }

    PuStatisticsEntry ::=
        SEQUENCE {
            puStatisticsIndex
                Integer32,
            puStatisticsBatteryCharge
                INTEGER,
            puStatisticsLoad
                INTEGER,
            puStatisticsFrequency
                Integer32,
            puStatisticsLineVoltage
                Integer32,
            puStatisticsOutputVoltage
                Integer32,
            puStatisticsBatteryVoltage
                Integer32,
            puStatisticsTemperatureC
                Integer32,
            puStatisticsTemperatureF
                Integer32,
            puStatisticsLineMininumVoltage
                Integer32,
            puStatisticsLineMaximumVoltage
                Integer32,
            puStatisticsRemainingRunTime
                Integer32
        }

    puStatisticsIndex OBJECT-TYPE
        SYNTAX Integer32 (1..2)
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The power unit from which the statistics have been gathered.
             The power unit identified by a particular value of this
             index is the same power unit as identified by the same value
             of puConfigurationIndex."
        ::= { puStatisticsEntry 1 }

    puStatisticsBatteryCharge OBJECT-TYPE
        SYNTAX INTEGER (0..100)
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The percentage by which the battery is charged."
        ::= { puStatisticsEntry 2 }

    puStatisticsLoad OBJECT-TYPE
        SYNTAX INTEGER (0..100)
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The percentage of the rated load in Watts."
        ::= { puStatisticsEntry 3 }

    puStatisticsFrequency OBJECT-TYPE
        SYNTAX Integer32
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The frequency of the AC mains in Hertz."
        ::= { puStatisticsEntry 4 }

    puStatisticsLineVoltage OBJECT-TYPE
        SYNTAX Integer32
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The AC mains voltage entering the power unit in Volts."
        ::= { puStatisticsEntry 5 }

    puStatisticsOutputVoltage OBJECT-TYPE
        SYNTAX Integer32
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The output voltage of the power unit in Volts."
        ::= { puStatisticsEntry 6 }

    puStatisticsBatteryVoltage OBJECT-TYPE
        SYNTAX Integer32
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The battery voltage in Volts."
        ::= { puStatisticsEntry 7 }

    puStatisticsTemperatureC OBJECT-TYPE
        SYNTAX Integer32
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The temperature inside the power unit in degrees Celsius."
        ::= { puStatisticsEntry 8 }

    puStatisticsTemperatureF OBJECT-TYPE
