    eventLogNumber OBJECT-TYPE
        SYNTAX Integer32
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "The total number of events currently logged.

            This object is now obsolete."
        ::= { eventlog 1 }

    eventLogTable OBJECT-TYPE
        SYNTAX SEQUENCE OF EventLogEntry
        MAX-ACCESS not-accessible
        STATUS obsolete
        DESCRIPTION
            "A table containing a list of all Events logged
             for the system.

             The number of entries in this table is given by the value of 
             eventLogNumber.

            This object is now obsolete."
        ::= { eventlog 2 }

    eventLogEntry OBJECT-TYPE
        SYNTAX EventLogEntry
        MAX-ACCESS not-accessible
        STATUS obsolete
        DESCRIPTION
            "Event log information.

            This object is now obsolete."
        INDEX { eventLogTimeStamp, eventLogHandle }
        ::= { eventLogTable 1 }

    EventLogEntry ::=
        SEQUENCE {
            eventLogTimeStamp
                DisplayString,
            eventLogHandle
                Integer32,
            eventLogText
                DisplayString
        }

    eventLogTimeStamp OBJECT-TYPE
        SYNTAX DisplayString
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "The timestamp of the event in UTC format.

            This object is now obsolete."
        ::= { eventLogEntry 1 }

    eventLogHandle OBJECT-TYPE
        SYNTAX Integer32 (1..65535)
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "A unique handle for the event.

            This object is now obsolete."
        ::= { eventLogEntry 2 }

    eventLogText OBJECT-TYPE
        SYNTAX DisplayString
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "The text describing the event.

            This object is now obsolete."
        ::= { eventLogEntry 3 }

