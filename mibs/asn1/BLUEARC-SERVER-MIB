    protocolXferStatsNumber OBJECT-TYPE
        SYNTAX Integer32
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The number of network protocols for which statistics
             are available.
            
            If enhanced statistics gathering is not enabled, this
            value will be zero. See the command
            enhanced-perf-stats."
        ::= { protocolStats 7 }

    protocolXferStatsTable OBJECT-TYPE
        SYNTAX SEQUENCE OF ProtocolXferStatsEntry
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION
            "Table of protocol transfer total statistics entries.
            
            If enhanced statistics gathering is not enabled, this
            table will be empty. See the command
            enhanced-perf-stats."
        ::= { protocolStats 8 }

    protocolXferStatsEntry OBJECT-TYPE
        SYNTAX ProtocolXferStatsEntry
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION
            "Read and Write transfer total for each of the following network
             protocols: CIFS, NFS."
        INDEX { protocolXferStatsFsId, protocolXferStatsFlavor }
        ::= { protocolXferStatsTable 1 }

    ProtocolXferStatsEntry ::=
        SEQUENCE {
            protocolXferStatsFsId
                Integer32,
            protocolXferStatsFlavor
                INTEGER,
            protocolXferStatsFsLabel
                DisplayString,
            protocolXferStatsBytesRead
                Counter64,
            protocolXferStatsBytesWritten
                Counter64
        }

    protocolXferStatsFsId OBJECT-TYPE
        SYNTAX Integer32 (1..2147483647)
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The device Id for this file system."
        ::= { protocolXferStatsEntry 1 }

    protocolXferStatsFlavor OBJECT-TYPE
        SYNTAX INTEGER {
            NFS(0),
            CIFS(1)
        }
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "Enumerated identity of the network protocols."
        ::= { protocolXferStatsEntry 2 }

    protocolXferStatsFsLabel OBJECT-TYPE
        SYNTAX DisplayString
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The file system label."
        ::= { protocolXferStatsEntry 3 }

    protocolXferStatsBytesRead OBJECT-TYPE
        SYNTAX Counter64
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The number of bytes read."
        ::= { protocolXferStatsEntry 4 }

    protocolXferStatsBytesWritten OBJECT-TYPE
        SYNTAX Counter64
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The number of bytes written."
        ::= { protocolXferStatsEntry 5 }
        
--------------------------------------------------------------------------------
--
-- performance / Cluster Stats
--
--------------------------------------------------------------------------------

    intraClusterPortErrorNumber OBJECT-TYPE
        SYNTAX Integer32
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The number of cluster network errors
            in the table."
        ::= { clusterStats 1 }

    intraClusterPortErrorTable OBJECT-TYPE
        SYNTAX SEQUENCE OF IntraClusterPortErrorEntry
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION
            "A table of network errors between nodes in
            a cluster."
        ::= { clusterStats 2 }

    intraClusterPortErrorEntry OBJECT-TYPE
        SYNTAX IntraClusterPortErrorEntry
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION
            "Network errors between nodes in
            a cluster."
        INDEX { intraClusterPortErrsCnId }
        ::= { intraClusterPortErrorTable 1 }

    IntraClusterPortErrorEntry ::=
        SEQUENCE {
            intraClusterPortErrsCnId
                Unsigned32,
            mirroringRetransmits
                Counter64,
            cnsRetransmits
                Counter64,
            totalRetransmits
                Counter64
        }

    intraClusterPortErrsCnId OBJECT-TYPE
        SYNTAX Unsigned32 (1..255)
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The ID of a cluster node for
            which intra-cluster errors are
            recorded."
        ::= { intraClusterPortErrorEntry 1 }

    mirroringRetransmits OBJECT-TYPE
        SYNTAX Counter64
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The number of retransmits of NVRAM
            mirroring packets. If this value is
            not available for the server series,
            it will be reported as zero."
        ::= { intraClusterPortErrorEntry 2 }

    cnsRetransmits OBJECT-TYPE
        SYNTAX Counter64
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The number of retransmits of redirected
            CNS packets. If this value is
            not available for the server series,
            it will be reported as zero."
        ::= { intraClusterPortErrorEntry 3 }

    totalRetransmits OBJECT-TYPE
        SYNTAX Counter64
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The total number of retransmits for the
            cluster node."
        ::= { intraClusterPortErrorEntry 4 }

    clusterNodeStatsNumber OBJECT-TYPE
        SYNTAX Integer32
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "Number of units that may be running a thread managing a network
            socket."
        ::= { clusterStats 3 }

    clusterNodeStatsTable OBJECT-TYPE
        SYNTAX SEQUENCE OF ClusterNodeStatsEntry
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION
            "A table of the number of threads managing a network socket
             that are running.

             The number of entries in this table is given by the value
             of clusterNodeStatsNumber." 

        ::= { clusterStats 4 }

    clusterNodeStatsEntry OBJECT-TYPE
        SYNTAX ClusterNodeStatsEntry
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION
            "Count information of threads managing a network socket that
             are running."  
        INDEX { clusterNodeStatsCnId }
        ::= { clusterNodeStatsTable 1 }

    ClusterNodeStatsEntry ::=
        SEQUENCE {
            clusterNodeStatsCnId
                Unsigned32,
            runningBossockFibers
                Unsigned32
        }

    clusterNodeStatsCnId OBJECT-TYPE
        SYNTAX Unsigned32 (1..255)
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The cluster node for this value."
        ::= { clusterNodeStatsEntry 1 }

    runningBossockFibers OBJECT-TYPE
        SYNTAX Unsigned32
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "Number of threads managing a network socket that
             are running on this cluster node."
        ::= { clusterNodeStatsEntry 2 }


--------------------------------------------------------------------------------
--
-- Trap Notifications
--
-- The agent may generate the following traps :
--
-- LinkUp, ColdStart
-- Authentication Failure (if configured)
--
--------------------------------------------------------------------------------

END


