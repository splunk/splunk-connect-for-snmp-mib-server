            enabled(1),
            disabled(2),
            unknown(3)
        }
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "Indicates whether or not NDMP is enabled at boot time."
        ::= { ndmpStatus 2 }

--------------------------------------------------------------------------------
--
-- Backup / NDMP Devices
--
--------------------------------------------------------------------------------

    autoChangerNumber OBJECT-TYPE
        SYNTAX Integer32
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The number of auto changers NDMP has detected."
        ::= { ndmpDevices 1 }

    autoChangerTable OBJECT-TYPE
        SYNTAX SEQUENCE OF AutoChangerEntry
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION
            "A table containing a list of auto changers NDMP has
             detected.

             The number of entries in this table is given by the value of
             autoChangerNumber."
        ::= { ndmpDevices 2 }

    autoChangerEntry OBJECT-TYPE
        SYNTAX AutoChangerEntry
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION
            "Auto changer information."
        INDEX { autoChangerIndex }
        ::= { autoChangerTable 1 }

    AutoChangerEntry ::=
        SEQUENCE {
            autoChangerIndex
                Integer32,
            autoChangerDeviceName
                DisplayString,
            autoChangerSerialNumber
                DisplayString,
            autoChangerEVS
                DisplayString
        }

    autoChangerIndex OBJECT-TYPE
        SYNTAX Integer32 (1..10)
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "A unique value for each auto changer detected.

             Its value ranges between 1 and the value of
             autoChangerNumber."
        ::= { autoChangerEntry 1 }

    autoChangerDeviceName OBJECT-TYPE
        SYNTAX DisplayString
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The device name of the auto changer."
        ::= { autoChangerEntry 2 }

    autoChangerSerialNumber OBJECT-TYPE
        SYNTAX DisplayString
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The serial number of the auto changer."
        ::= { autoChangerEntry 3 }

    autoChangerEVS OBJECT-TYPE
        SYNTAX DisplayString
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The Enterprise Virtual Server to which this
            auto changer is assigned."
        ::= { autoChangerEntry 4 }

    tapeDriveNumber OBJECT-TYPE
        SYNTAX Integer32
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The number of tape drives that NDMP has detected."
        ::= { ndmpDevices 3 }

    tapeDriveTable OBJECT-TYPE
        SYNTAX SEQUENCE OF TapeDriveEntry
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION
            "A table containing a list of all tape drives NDMP has detected.

             The number of entries in this table is given by the value of 
             tapeDriveNumber."
        ::= { ndmpDevices 4 }

    tapeDriveEntry OBJECT-TYPE
        SYNTAX TapeDriveEntry
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION
            "Tape drive information."
        INDEX { tapeDriveIndex }
        ::= { tapeDriveTable 1 }

    TapeDriveEntry ::=
        SEQUENCE {
            tapeDriveIndex
                Integer32,
            tapeDriveAutoChangerIndex
                Integer32,
            tapeDriveDeviceName
                DisplayString,
            tapeDriveSerialNumber
                DisplayString,
            tapeDriveLocation
                DisplayString,
            tapeDriveEVS
                DisplayString
        }

    tapeDriveIndex OBJECT-TYPE
        SYNTAX Integer32 (1..10)
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "A unique value for each tape drive.

             Its value ranges between 1 and the value of 
             tapeDriveNumber."
        ::= { tapeDriveEntry 1 }

    tapeDriveAutoChangerIndex OBJECT-TYPE
        SYNTAX Integer32
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The index of the auto changer in the autoChangerTable
             to which this tape drive is connected or -1 if it is
             not associated with an auto changer."
        ::= { tapeDriveEntry 2 }

    tapeDriveDeviceName OBJECT-TYPE
        SYNTAX DisplayString
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The device name of the tape drive."
        ::= { tapeDriveEntry 3 }

    tapeDriveSerialNumber OBJECT-TYPE
        SYNTAX DisplayString
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The serial number of the tape drive."
        ::= { tapeDriveEntry 4 }

    tapeDriveLocation OBJECT-TYPE
        SYNTAX DisplayString
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The location of the tape drive."
        ::= { tapeDriveEntry 5 }

    tapeDriveEVS OBJECT-TYPE
        SYNTAX DisplayString
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The Enterprise Virtual Server to which this
            tape drive is assigned."
        ::= { tapeDriveEntry 6 }

--------------------------------------------------------------------------------
--
-- Backup / NDMP Snapshot Options
--
--------------------------------------------------------------------------------

    ndmpAutoSnapCreateEnabled OBJECT-TYPE
        SYNTAX INTEGER {
            enabled(1),
            disabled(2),
            unknown(3)
        }
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "Determines whether snapshots are automatically created when 
             NDMP performs a backup.

             enabled(1)  - indicates that NDMP will create a snapshot (if one 
                           does not already exist) when a backup is to be
                           performed.
             disabled(2) - indicates that NDMP will not automatically
                           create a snapshot when a backup is to be performed.
                           It will take a backup of the live file system
                           if no snapshot exists.
             unknown(3)  - indicates that snapshot is disabled on this system
                           and the value is therefore unknown."
        ::= { ndmpSnapshotOptions 1 }

    ndmpAutoSnapDeleteMode OBJECT-TYPE
        SYNTAX INTEGER {
            immediately(1),
            whenObsolete(2),
            unknown(3)
        }
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "Indicates whether NDMP will remove a snapshot automatically
             when the backup is complete.
             immediately(1)  - indicates that the snapshot will be deleted
                               immediately after use.
             whenObsolete(2) - indicates that the snapshot will be deleted
                               when it becomes obsolete.
             unknown(3)      - indicates that snapshot is disabled on this 
                               system and the value is therefore unknown."
        ::= { ndmpSnapshotOptions 2 }

    ndmpAutoSnapMaxRetention OBJECT-TYPE
        SYNTAX Unsigned32
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The maximum retention time of snapshot (in days) which has been
             automatically created by NDMP as a result of performing
             a backup.

             NOTE: this value is set to 0 if snapshot is disabled on this
                   system."
        ::= { ndmpSnapshotOptions 3 }


--------------------------------------------------------------------------------
--
-- Management / System Users
--
--------------------------------------------------------------------------------

    systemUserNumber OBJECT-TYPE
        SYNTAX Integer32
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The number of users which are configured to access the system
             through web and telnet via password authentication.
             NOTE: this does not include users which have higher level access
             than supervisor."
        ::= { systemUsers 1 }

    systemUserTable OBJECT-TYPE
        SYNTAX SEQUENCE OF SystemUserEntry
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION
            "A table containing a list of all users configured to access the
             system through web and telnet via password authentication.
             NOTE: this does not include users which have higher level access
             than supervisor.

             The number of entries in this table is given by the value of 
             systemUserNumber."
        ::= { systemUsers 2 }

    systemUserEntry OBJECT-TYPE
        SYNTAX SystemUserEntry
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION
            "System user information."
        INDEX { systemUserName }
        ::= { systemUserTable 1 }

    SystemUserEntry ::=
        SEQUENCE {
            systemUserName
                DisplayString,
            systemUserAccessLevel
                DisplayString
        }

    systemUserName OBJECT-TYPE
        SYNTAX DisplayString
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The login name of the system user."
        ::= { systemUserEntry 1 }

    systemUserAccessLevel OBJECT-TYPE
        SYNTAX DisplayString
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The access level granted to the system user."
        ::= { systemUserEntry 2 }

--------------------------------------------------------------------------------
--
-- Management / License Information
--
--------------------------------------------------------------------------------

    licenseKeyNumber OBJECT-TYPE
        SYNTAX Integer32
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The number of license keys for this server."
        ::= { license 1 }

    licenseKeyTable OBJECT-TYPE
        SYNTAX SEQUENCE OF LicenseKeyEntry
        MAX-ACCESS not-accessible
        STATUS obsolete
        DESCRIPTION
            "A table containing a list of license keys.

             The number of entries in this table is given by the value
             of licenseKeyNumber."
        ::= { license 2 }

    licenseKeyEntry OBJECT-TYPE
        SYNTAX LicenseKeyEntry
        MAX-ACCESS not-accessible
        STATUS obsolete
        DESCRIPTION
            "License key information."
        INDEX { licenseKeyIndex }
        ::= { licenseKeyTable 1 }

    LicenseKeyEntry ::=
        SEQUENCE {
            licenseKeyIndex
                Integer32,
            licenseKeyString
                DisplayString,
            licenseKeyValid
                INTEGER,
            licenseCIFSService
                INTEGER,
            licenseNFSService
                INTEGER,
            licenseFTPService
                INTEGER,
            licenseHTTPService
                INTEGER,
            licenseFailoverService
                INTEGER,
            licenseRAIDService
                INTEGER
        }

    licenseKeyIndex OBJECT-TYPE
        SYNTAX Integer32 (1..30)
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "A unique value for each license key.

             Its value ranges between 1 and the value of
             licenseKeyNumber."
        ::= { licenseKeyEntry 1 }

    licenseKeyString OBJECT-TYPE
        SYNTAX DisplayString
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "The license key string."
        ::= { licenseKeyEntry 2 }

    licenseKeyValid OBJECT-TYPE
        SYNTAX INTEGER {
            valid(1),
            invalid(2),
            unknown(3)
        }
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "Indicates whether the license key is valid on this
             server or not."
        ::= { licenseKeyEntry 3 }

    licenseCIFSService OBJECT-TYPE
        SYNTAX INTEGER {
            enabled(1),
            disabled(2),
            unknown(3)
        }
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "Indicates whether this license key enables the
             use of the CIFS service or not."
        ::= { licenseKeyEntry 4 }

    licenseNFSService OBJECT-TYPE
        SYNTAX INTEGER {
            enabled(1),
            disabled(2),
            unknown(3)
        }
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "Indicates whether this license key enables the use
             of the NFS service or not."
        ::= { licenseKeyEntry 5 }

    licenseFTPService OBJECT-TYPE
        SYNTAX INTEGER {
            enabled(1),
            disabled(2),
            unknown(3)
        }
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "Indicates whether this license key enables the use of 
             the FTP service or not."
        ::= { licenseKeyEntry 6 }

    licenseHTTPService OBJECT-TYPE
        SYNTAX INTEGER {
            enabled(1),
            disabled(2),
            unknown(3)
        }
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "Indicates whether this license key enables the use of 
             the HTTP service or not."
        ::= { licenseKeyEntry 7 }

    licenseFailoverService OBJECT-TYPE
        SYNTAX INTEGER {
            enabled(1),
            disabled(2),
            unknown(3)
        }
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "Indicates whether this license key enables the use
             of the Failover service or not.

             This object is obsolete: failover has been
             superceded by clustering."
        ::= { licenseKeyEntry 8 }

    licenseRAIDService OBJECT-TYPE
        SYNTAX INTEGER {
            single(1),
            multiple(2),
            unknown(3)
        }
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "Indicates whether this license key enables the 
             use of more than one RAID controller in the system
             or not."
        ::= { licenseKeyEntry 9 }

    licenseTable OBJECT-TYPE
        SYNTAX SEQUENCE OF LicenseEntry
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION
            "A table containing a list of valid license keys and the service(s)
             they license."
        ::= { license 3 }

    licenseEntry OBJECT-TYPE
        SYNTAX LicenseEntry
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION
            "License information."
        INDEX { licenseIndex }
        ::= { licenseTable 1 }

    LicenseEntry ::=
        SEQUENCE {
            licenseIndex
                Integer32,
            licenseKey
                DisplayString,
            licenseService
                DisplayString
        }

    licenseIndex OBJECT-TYPE
        SYNTAX Integer32 (1..1024)
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "A unique value for each licensed service."
        ::= { licenseEntry 1 }

    licenseKey OBJECT-TYPE
        SYNTAX DisplayString
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The license key."
        ::= { licenseEntry 2 }

    licenseService OBJECT-TYPE
        SYNTAX DisplayString
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "A service licensed by the given license."
        ::= { licenseEntry 3 }

--------------------------------------------------------------------------------
--
-- Management / Access / Web
--
--------------------------------------------------------------------------------

    webAccessEnabled OBJECT-TYPE
        SYNTAX INTEGER {
            enabled(1),
            disabled(2),
            unknown(3)
        }
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "Indicates whether access to the web (GUI) interface is
             permitted or not.

             The setting disabled(2) indicates access to the
             web interface will not be allowed regardless of any
             other configuration settings for this interface."
        ::= { web 1 }

    webAccessRestricted OBJECT-TYPE
        SYNTAX INTEGER {
            restricted(1),
            unrestricted(2),
            unknown(3)
        }
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "Indicates whether access to the web interface is
             restricted to the set of hostnames or IP addresses
             configured in webAccessRestrictedTable or
             unrestricted (such that any client may access the
             web interface).

             NOTE : this setting controls the use of the web access
                    restricted table in that there may be entries
                    in this table, but access to the interface will
                    be allowed from any host if this variable is
                    set to unrestricted(2).
                    
            This object is now obsolete."
        ::= { web 2 }

    webAccessRestrictedNumber OBJECT-TYPE
        SYNTAX Integer32
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "The number of restricted web access entries
             configured.
                    
            This object is now obsolete."
        ::= { web 3 }

    webAccessRestrictedTable OBJECT-TYPE
        SYNTAX SEQUENCE OF WebAccessRestrictedEntry
        MAX-ACCESS not-accessible
        STATUS obsolete
        DESCRIPTION
            "A table containing the hostnames or IP addresses of clients
             which have restricted access to the web interface.

             The number of entries in this table is given by the value of
             webAccessRestrictedNumber.
                    
            This object is now obsolete."
        ::= { web 4 }

    webAccessRestrictedEntry OBJECT-TYPE
        SYNTAX WebAccessRestrictedEntry
        MAX-ACCESS not-accessible
        STATUS obsolete
        DESCRIPTION
            "Restricted web access information.
                    
            This object is now obsolete."
        INDEX { webAccessRestrictedHost }
        ::= { webAccessRestrictedTable 1 }

    WebAccessRestrictedEntry ::=
        SEQUENCE {
            webAccessRestrictedHost
                DisplayString
        }

    webAccessRestrictedHost OBJECT-TYPE
        SYNTAX DisplayString
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "The hostname or IP address of a client which has
             restricted access to the web interface.
                    
            This object is now obsolete."
        ::= { webAccessRestrictedEntry 1 }

    webAccessPortNumber OBJECT-TYPE
        SYNTAX Integer32
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "The port number through which access to the web interface
             is available (subject to any security restrictions).
                    
            This object is now obsolete."
        ::= { web 5 }

    webAccessMaxConnections OBJECT-TYPE
        SYNTAX Integer32
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "The maximum number of simultaneous connections which are
             allowed access to the web interface.
                    
            This object is now obsolete."
        ::= { web 6 }

