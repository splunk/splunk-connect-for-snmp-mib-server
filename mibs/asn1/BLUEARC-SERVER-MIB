        SYNTAX INTEGER {
            never(1),
            immediately(2),
            unknown(3)
        }
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The frequency at which alerts within the critical category
             may be sent to the mail recipient(s)."
        ::= { mailAlertConfig 2 }

    smtpSevFreq OBJECT-TYPE
        SYNTAX INTEGER {
            never(1),
            immediately(2),
            daily(3),
            unknown(4)
        }
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The frequency at which alerts within the severe category
             may be sent to the mail recipient(s)."
        ::= { mailAlertConfig 3 }

    smtpWarnFreq OBJECT-TYPE
        SYNTAX INTEGER {
            never(1),
            immediately(2),
            daily(3),
            unknown(4)
        }
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The frequency at which alerts within the warning category
             may be sent to the mail recipient(s)."
        ::= { mailAlertConfig 4 }

    smtpInfoFreq OBJECT-TYPE
        SYNTAX INTEGER {
            never(1),
            immediately(2),
            daily(3),
            unknown(4)
        }
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The frequency at which alerts within the information category
             may be sent to the mail recipient(s)."
        ::= { mailAlertConfig 5 }

    smtpRecipNumber OBJECT-TYPE
        SYNTAX Integer32
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The number of email recipients configured to receive alerts."
        ::= { mailAlertConfig 6 }

    smtpRecipTable OBJECT-TYPE
        SYNTAX SEQUENCE OF SmtpRecipEntry
        MAX-ACCESS not-accessible
        STATUS obsolete
        DESCRIPTION
            "A table containing a list of all configured mail alert
             recipients for this system.

             The number of entries in this table is given by the value of
             smtpRecipNumber.

             This object is obsolete.  Superceded by smptRecipientTable."
        ::= { mailAlertConfig 7 }

    smtpRecipEntry OBJECT-TYPE
        SYNTAX SmtpRecipEntry
        MAX-ACCESS not-accessible
        STATUS obsolete
        DESCRIPTION
            "Mail alert recipient information.

             This object is obsolete.  Superceded by smtpRecipientEntry."
        INDEX { smtpRecipIndex }
        ::= { smtpRecipTable 1 }

    SmtpRecipEntry ::=
        SEQUENCE {
            smtpRecipIndex
                Integer32,
            smtpRecipName
                DisplayString
        }

    smtpRecipIndex OBJECT-TYPE
        SYNTAX Integer32 (1..20)
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "A unique value for each configured mail recipient.

             Its value ranges between 1 and the value of
             smtpRecipNumber.

             This table entry is obsolete.  Superceded by smtpRecipientIndex."
        ::= { smtpRecipEntry 1 }

    smtpRecipName OBJECT-TYPE
        SYNTAX DisplayString
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "The email address of the recipient.

             This table entry is obsolete.  Superceded by smtpRecipientName."
        ::= { smtpRecipEntry 2 }

    smtpDiagUUencEnabled OBJECT-TYPE
        SYNTAX INTEGER {
            enabled(1),
            disabled(2),
            unknown(3)
        }
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "Indicates whether diagnostic emails are UU encoded
             or not."
        ::= { mailAlertConfig 8 }

    smtpUndisclosedRecipNumber OBJECT-TYPE
        SYNTAX Integer32
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "The number of email recipients configured to receive
             undisclosed alerts.
             
             This object is obsolete.  Undisclosed recipients are now grouped
             with disclosed recipients."
        ::= { mailAlertConfig 9 }

    smtpUndisclosedRecipTable OBJECT-TYPE
        SYNTAX SEQUENCE OF SmtpUndisclosedRecipEntry
        MAX-ACCESS not-accessible
        STATUS obsolete
        DESCRIPTION
            "A table containing a list of all configured mail alert 
             recipients which are to receive undisclosed alerts
             for this system.

             The number of entries in this table is given by the value of 
             smtpUndisclosedRecipNumber.
             
             This object is obsolete.  Undisclosed recipients are now grouped
             with disclosed recipients."
        ::= { mailAlertConfig 10 }

    smtpUndisclosedRecipEntry OBJECT-TYPE
        SYNTAX SmtpUndisclosedRecipEntry
        MAX-ACCESS not-accessible
        STATUS obsolete
        DESCRIPTION
            "Undisclosed mail alert recipient information.
            
             This table is obsolete.  Undisclosed recipients are now
             grouped with disclosed recipients."
        INDEX { smtpUndisclosedRecipIndex }
        ::= { smtpUndisclosedRecipTable 1 }

    SmtpUndisclosedRecipEntry ::=
        SEQUENCE {
            smtpUndisclosedRecipIndex
                Integer32,
            smtpUndisclosedRecipName
                DisplayString
        }

    smtpUndisclosedRecipIndex OBJECT-TYPE
        SYNTAX Integer32 (1..20)
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "A unique value for each configured mail recipient
             which is to receive undisclosed alert information.

             Its value ranges between 1 and the value of 
             smtpUndisclosedRecipNumber.
             
             This table entry is obsolete.  Undisclosed recipients are now
             grouped with disclosed recipients."
        ::= { smtpUndisclosedRecipEntry 1 }

    smtpUndisclosedRecipName OBJECT-TYPE
        SYNTAX DisplayString
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "The email address of the recipient.
            
             This object is obsolete.  Undisclosed recipients are now
             grouped with disclosed recipients."
        ::= { smtpUndisclosedRecipEntry 2 }

    smtpRecipientTable OBJECT-TYPE
        SYNTAX SEQUENCE OF SmtpRecipientEntry
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION
            "A table containing a list of all configured mail alert
             recipients for this system, whether to disclose
             details in emails sent to the recipients and whether
             to send empty summary emails when due.

             The number of entries in this table is given by the value of
             smtpRecipNumber."
        ::= { mailAlertConfig 11 }

    smtpRecipientEntry OBJECT-TYPE
        SYNTAX SmtpRecipientEntry
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION
            "Mail alert recipient information."
        INDEX { smtpRecipientIndex }
        ::= { smtpRecipientTable 1 }

    SmtpRecipientEntry ::=
        SEQUENCE {
            smtpRecipientIndex
                Integer32,
            smtpRecipientName
                DisplayString,
            smtpRecipientDisclose
                DisplayString,
            smtpRecipientEmpty
                DisplayString
        }

    smtpRecipientIndex OBJECT-TYPE
        SYNTAX Integer32 (1..20)
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "A unique value for each configured mail recipient."
        ::= { smtpRecipientEntry 1 }

    smtpRecipientName OBJECT-TYPE
        SYNTAX DisplayString
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The email address of the recipient."
        ::= { smtpRecipientEntry 2 }

    smtpRecipientDisclose OBJECT-TYPE
        SYNTAX DisplayString
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "Disclose details in emails to this recipient."
        ::= { smtpRecipientEntry 3 }

    smtpRecipientEmpty OBJECT-TYPE
        SYNTAX DisplayString
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "If there have been no events when summary emails are due 
            then send empty emails."
        ::= { smtpRecipientEntry 4 }


