    NfsUserEntry ::=
        SEQUENCE {
            nfsUserName
                DisplayString,
            nfsUserID
                Unsigned32
        }

    nfsUserName OBJECT-TYPE
        SYNTAX DisplayString
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "The name of the UNIX user."
        ::= { nfsUserEntry 1 }

