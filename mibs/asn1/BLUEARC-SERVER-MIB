--------------------------------------------------------------------------------
--
-- System / Server/ Environment
--
--------------------------------------------------------------------------------

    sensorNumber OBJECT-TYPE
        SYNTAX Integer32
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "The number of sensors (regardless of their current state)
             which are present within silicon server unit."
        ::= { environment 1 }

    sensorTable OBJECT-TYPE
        SYNTAX SEQUENCE OF SensorEntry
        MAX-ACCESS not-accessible
        STATUS obsolete
        DESCRIPTION
            "A table containing information gathered by sensors located
             within the silicon server unit.  Each sensor takes a 
             measurement of both fan speed and temperature.

             The number of entries in this table is given by the value of
             sensorNumber."
        ::= { environment 2 }

    sensorEntry OBJECT-TYPE
        SYNTAX SensorEntry
        MAX-ACCESS not-accessible
        STATUS obsolete
        DESCRIPTION
            "Information collected from each sensor located within
             the silicon server unit."
        INDEX { sensorIndex }
        ::= { sensorTable 1 }

    SensorEntry ::=
        SEQUENCE {
            sensorIndex
                Integer32,
            sensorSpeedStatus
                INTEGER,
            sensorSpeedReading
                Unsigned32,
            sensorTempStatus
                INTEGER,
            sensorTempCReading
                Integer32,
            sensorTempFReading
                Integer32
        }

    sensorIndex OBJECT-TYPE
        SYNTAX Integer32 (1..3)
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "A unique value for each sensor within the silicon server unit.

             Its value ranges between 1 and the value of sensorNumber."
        ::= { sensorEntry 1 }

    sensorSpeedStatus OBJECT-TYPE
        SYNTAX INTEGER {
            ok(1),
            warning(2),
            severe(3),
            failed(4)
        }
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "Indicates whether the speed measurement is within the
             expected range.

             ok(1)      - the reading is within the expected range.
             warning(2) - the reading is outside of the expected range,
                          but not severe.
             severe(3)  - the sensor unit should be replaced.
             failed(4)  - the sensor unit has failed.  The measured
                          speed is invalid.
             unknown(5) - the speed status could not be determined."
        ::= { sensorEntry 2 }

    sensorSpeedReading OBJECT-TYPE
        SYNTAX Unsigned32
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "A measurement of the fan speed taken by the sensor in rpm."
        ::= { sensorEntry 3 }

    sensorTempStatus OBJECT-TYPE
        SYNTAX INTEGER {
            ok(1),
            warning(2),
            severe(3),
            failed(4)
        }
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "Indicates whether the temperature measurement is within the
             expected range.

             ok(1)      - the reading is within the expected range.
             warning(2) - the reading is outside of the expected range,
                          but not severe.
             severe(3)  - the sensor unit should be replaced.
             failed(4)  - the sensor unit has failed.  The measured
                          temperature is invalid."
        ::= { sensorEntry 4 }

    sensorTempCReading OBJECT-TYPE
        SYNTAX Integer32
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "A measurement of the temperature taken by the sensor in degrees
             Celsius."
        ::= { sensorEntry 5 }

    sensorTempFReading OBJECT-TYPE
        SYNTAX Integer32
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "A measurement of the temperature taken by the sensor in degrees
             Fahrenheit."
        ::= { sensorEntry 6 }


    psuOneStatus OBJECT-TYPE
        SYNTAX INTEGER {
            ok(1),
            failed(2)
        }
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "The status of power supply unit 1 within the silicon server."
        ::= { environment 3 }

    psuTwoStatus OBJECT-TYPE
        SYNTAX INTEGER {
            ok(1),
            failed(2)
        }
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "The status of power supply unit 2 within the silicon server."
        ::= { environment 4 }

    opsPerSecond OBJECT-TYPE
        SYNTAX Unsigned32
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The number of operations per second.
            
            This value includes all hardware-accelerated access to file
            systems on a cluster-wide basis. It may not correspond with
            the values obtained from opsPerSecAverage in the fsStatsTable."
        ::= { environment 5 }

    fileSystemLoadClient OBJECT-TYPE
        SYNTAX Integer32
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The percentage of maximum load on file system resources
            due to client usage, averaged over the last 10 seconds.
            This represents the load on FSA."
        ::= { environment 6 }

    fileSystemLoadSystem OBJECT-TYPE
        SYNTAX Integer32
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The percentage of maximum load on file system resources
            due to system usage, averaged over the last 10 seconds.
            This represents the load on FSB."
        ::= { environment 7 }

    temperatureSensorNumber OBJECT-TYPE
        SYNTAX Integer32
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The number of temperature sensors (regardless of their
            current state) present in all silicon server units
            in the cluster."
        ::= { environment 8 }

    temperatureSensorTable OBJECT-TYPE
        SYNTAX SEQUENCE OF TemperatureSensorEntry
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION
            "A table containing information gathered by temperature
            sensors located in all silicon server units in the 
            cluster.

            The number of entries in this table is given by the value of
            temperatureSensorNumber."
        ::= { environment 9 }

    temperatureSensorEntry OBJECT-TYPE
        SYNTAX TemperatureSensorEntry
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION
            "Information collected from each temperature sensor located
            in the silicon server unit."
        INDEX { temperatureSensorClusterNode, temperatureSensorIndex }
        ::= { temperatureSensorTable 1 }

    TemperatureSensorEntry ::=
        SEQUENCE {
            temperatureSensorClusterNode
                Integer32,
            temperatureSensorIndex
                Integer32,
            temperatureSensorStatus
                INTEGER,
            temperatureSensorCReading
                Integer32,
            temperatureSensorFReading
                Integer32
        }

    temperatureSensorClusterNode OBJECT-TYPE
        SYNTAX Integer32 (1..255)
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The ID of the Cluster Node in which the fan
            is located."
        ::= { temperatureSensorEntry 1 }

    temperatureSensorIndex OBJECT-TYPE
        SYNTAX Integer32 (1..255)
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "A unique value for each temperature sensor on a
            cluster node."
        ::= { temperatureSensorEntry 2 }

    temperatureSensorStatus OBJECT-TYPE
        SYNTAX INTEGER {
            ok(1),
            tempWarning(2),
            tempSevere(3),
            tempSensorFailed(4),
            tempSensorWarning(5),
            unknown(6)
        }
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "Indicates whether the temperature measurement is within the
             expected range.

             ok(1)                  - the reading is within the expected range.
             tempWarning(2)         - the reading is outside of the expected
                                      range, but not severe.
             tempSevere(3)          - The reading is severely outside the
                                      expected range.
             tempSensoreFailed(4)   - the sensor unit has failed. The measured
                                      temperature is invalid
             tempSensorWarning(5)   - the sensor unit is in a warning condition.
             unknown(6)             - the status of the sensor could not be
                                      determined."
        ::= { temperatureSensorEntry 3 }

    temperatureSensorCReading OBJECT-TYPE
        SYNTAX Integer32
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "A measurement of the temperature taken by the sensor in degrees
             Celsius."
        ::= { temperatureSensorEntry 4 }

    temperatureSensorFReading OBJECT-TYPE
        SYNTAX Integer32
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "A measurement of the temperature taken by the sensor in degrees
             Fahrenheit."
        ::= { temperatureSensorEntry 5 }

    fanNumber OBJECT-TYPE
        SYNTAX Integer32
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The number of fans (regardless of their current state) present
            in all silicon server units in the cluster."
        ::= { environment 10 }

    fanTable OBJECT-TYPE
        SYNTAX SEQUENCE OF FanEntry
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION
            "A table containing information about the fans located in all 
            silicon server units in the cluster."
        ::= { environment 11 }

    fanEntry OBJECT-TYPE
        SYNTAX FanEntry
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION
            "Information about each fan located in all silicon server 
            units in the cluster."
        INDEX { fanClusterNode, fanIndex }
        ::= { fanTable 1 }

    FanEntry ::=
        SEQUENCE {
            fanClusterNode
                Integer32,
            fanIndex
                Integer32,
            fanFittedStatus
                INTEGER,
            fanSpeedStatus
                INTEGER,
            fanSpeed
                Integer32
        }

    fanClusterNode OBJECT-TYPE
        SYNTAX Integer32 (1..255)
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The ID of the Cluster Node in which the fan
            is located."
        ::= { fanEntry 1 }

    fanIndex OBJECT-TYPE
        SYNTAX Integer32 (1..255)
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "A unique value for each fan on a
            cluster node."
        ::= { fanEntry 2 }

    fanFittedStatus OBJECT-TYPE
        SYNTAX INTEGER {
            ok(1),
            okIdWrong(2),
            notFitted(3),
            unknown(4)
        }
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "Indicates the 'fitted' status of the fan.

             ok(1)                  - the fan is fitted correctly.
             okIdWrong(2)           - the fan is fitted correctly, but the
                                      ID is wrong.
             notFitted(3)           - the fan is not fitted.
             unknown(4)             - the status could not be determined."
        ::= { fanEntry 3 }

    fanSpeedStatus OBJECT-TYPE
        SYNTAX INTEGER {
            ok(1),
            warning(2),
            severe(3),
            unknown(4)
        }
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "Indicates the 'speed' status of the fan.

             ok(1)                  - the fan speed is ok.
             warning(2)             - the fan speed is at a warning level.
             severe(3)              - the fan speed is at a severe level.
             unknown(4)             - the fan speed status could not be
                                      determined."
        ::= { fanEntry 4 }

    fanSpeed OBJECT-TYPE
        SYNTAX Integer32
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The speed of the fan."
        ::= { fanEntry 5 }

    psuNumber OBJECT-TYPE
        SYNTAX Integer32
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The number of psus (regardless of their current state) present
            in all silicon server units in the cluster."
        ::= { environment 12 }

    psuTable OBJECT-TYPE
        SYNTAX SEQUENCE OF PsuEntry
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION
            "A table containing information about the psus located in the
            silicon server unit."
        ::= { environment 13 }

    psuEntry OBJECT-TYPE
        SYNTAX PsuEntry
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION
            "Information about each psu."
        INDEX { psuClusterNode, psuIndex }
        ::= { psuTable 1 }

    PsuEntry ::=
        SEQUENCE {
            psuClusterNode
                Integer32,
            psuIndex
                Integer32,
            psuStatus
                INTEGER
        }

    psuClusterNode OBJECT-TYPE
        SYNTAX Integer32 (1..255)
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The ID of the Cluster Node in which the psu
            is located."
        ::= { psuEntry 1 }

    psuIndex OBJECT-TYPE
        SYNTAX Integer32 (1..255)
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "A unique value for each psu on a
            cluster node."
        ::= { psuEntry 2 }

    psuStatus OBJECT-TYPE
        SYNTAX INTEGER {
            ok(1),
            failed(2),
            notFitted(3),
            unknown(4)
        }
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "Indicates the status of the psu.

             ok(1)                  - the psu is ok.
             failed(2)              - the psu has failed.
             notFitted(3)           - the psu is not fitted.
             unknown(4)             - the status could not be determined."
        ::= { psuEntry 3 }



--------------------------------------------------------------------------------
--
-- System / Server/ Locale
--
--------------------------------------------------------------------------------

    serverDate OBJECT-TYPE
        SYNTAX DisplayString
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The local date reported by the server in the form
             YYYY-MM-DD

             Where YYYY is the year.
                   MM   is the number of the month (01-12).
                   DD   is the day of the month."
        ::= { locale 1 }

    serverTime OBJECT-TYPE
        SYNTAX DisplayString
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The local time reported by the server in the form
             HH:MM:SS (UTC+/-hh:mm)

             Where HH is the hour (using 24 hour clock).
                   MM is the minute.
                   SS is the second.
                   
                   The remainder of the date indicates the number
                   of hours (hh) and minutes (mm) east of UTC
                   the local time has been adjusted by.

             NOTE: a negative value for the UTC offset indicates
                   a time zone which is west of UTC."
        ::= { locale 2 }

    utcOffset OBJECT-TYPE
        SYNTAX Integer32
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The local time zone offset setting for the server 
             expressed as a number of minutes.
             NOTE: a negative value indicates a time zone west 
                   of UTC."
        ::= { locale 3 }

    daylightSavings OBJECT-TYPE
        SYNTAX INTEGER {
            daylightSavings(1),
            noDaylightSavings(2),
            unknown(3)
        }
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "Indicates whether daylight savings are in effect."
        ::= { locale 4 }

    ntpServerNumber OBJECT-TYPE
        SYNTAX Integer32
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The number of NTP servers which have been configured
             to assist in synchronization of this silicon servers
             time."
        ::= { locale 5 }

    ntpServerTable OBJECT-TYPE
        SYNTAX SEQUENCE OF NtpServerEntry
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION
            "A table containing information about each NTP server.

             The number of entries in this table is given by the value of
             ntpServerNumber."
        ::= { locale 6 }

    ntpServerEntry OBJECT-TYPE
        SYNTAX NtpServerEntry
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION
            "Information about each configured NTP server."
        INDEX { ntpServerHost }
        ::= { ntpServerTable 1 }

    NtpServerEntry ::=
        SEQUENCE {
            ntpServerHost
                DisplayString
        }

    ntpServerHost OBJECT-TYPE
        SYNTAX DisplayString
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The hostname or IP address of the NTP server."
        ::= { ntpServerEntry 1 }

