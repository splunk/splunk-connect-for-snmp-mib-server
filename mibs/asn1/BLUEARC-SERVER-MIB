            snapshotRulesEVS
                Unsigned32,
            snapshotRulesName
                DisplayString,
            snapshotRulesQueueSize
                Integer32,
            snapshotRulesVolumeLabel
                DisplayString
        }

    snapshotRulesEVS OBJECT-TYPE
        SYNTAX Unsigned32
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The ID of the EVS on which this rule is defined."
        ::= { snapshotRulesEntry 1 }

    snapshotRulesName OBJECT-TYPE
        SYNTAX DisplayString
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The name of the snapshot rule.  Each snapshot rule has a unique
             name."
        ::= { snapshotRulesEntry 2 }

    snapshotRulesQueueSize OBJECT-TYPE
        SYNTAX Integer32
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The number of snapshots to keep before the system deletes the
             oldest snapshot."
        ::= { snapshotRulesEntry 3 }

    snapshotRulesVolumeLabel OBJECT-TYPE
        SYNTAX DisplayString
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The label of the volume to which the snapshot rule relates."
        ::= { snapshotRulesEntry 4 }

