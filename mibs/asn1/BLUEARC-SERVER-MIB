    enclosureFanNumber OBJECT-TYPE
        SYNTAX Integer32
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "The total number of fans which are present in all of the 
             enclosures in all racks within the system.

             This object is now obsolete."
        ::= { enclosures 1 }

    enclosureFanTable OBJECT-TYPE
        SYNTAX SEQUENCE OF EnclosureFanEntry
        MAX-ACCESS not-accessible
        STATUS obsolete
        DESCRIPTION
            "A table containing status information for each fan located
             within each enclosure (of each rack) within this system.

             The number of entries in this table is given by the value of 
             enclosureFanNumber.

             This object is now obsolete."
        ::= { enclosures 2 }

    enclosureFanEntry OBJECT-TYPE
        SYNTAX EnclosureFanEntry
        MAX-ACCESS not-accessible
        STATUS obsolete
        DESCRIPTION
            "Fan status information.

             This object is now obsolete."
        INDEX { enclosureFanRackIndex, enclosureFanEnclosureIndex, enclosureFanFanIndex }
        ::= { enclosureFanTable 1 }

    EnclosureFanEntry ::=
        SEQUENCE {
            enclosureFanRackIndex
                Integer32,
            enclosureFanEnclosureIndex
                Unsigned32,
            enclosureFanFanIndex
                Unsigned32,
            enclosureFanStatus
                INTEGER,
            enclosureFanSpeed
                INTEGER
        }

    enclosureFanRackIndex OBJECT-TYPE
        SYNTAX Integer32 (1..10)
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "The rack in which the fan is located.
             The fan identified by a particular value of this index is
             located within the rack identified by the same value of 
             rackIndex.

             This object is now obsolete."
        ::= { enclosureFanEntry 1 }

    enclosureFanEnclosureIndex OBJECT-TYPE
        SYNTAX Unsigned32
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "The enclosure in which the fan is located.
             Enclosure numbers start at 1.

             This object is now obsolete."
        ::= { enclosureFanEntry 2 }

    enclosureFanFanIndex OBJECT-TYPE
        SYNTAX Unsigned32
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "An identifier for the fan within the enclosure.
             Fan numbers start at 1.

             This object is now obsolete."
        ::= { enclosureFanEntry 3 }

    enclosureFanStatus OBJECT-TYPE
        SYNTAX INTEGER {
            ok(1),
            failed(2),
            unknown(3)
        }
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "The status of the fan sensor.

             This object is now obsolete."
        ::= { enclosureFanEntry 4 }

    enclosureFanSpeed OBJECT-TYPE
        SYNTAX INTEGER {
            fast(1),
            slow(2),
            stoppedOrUnknown(3)
        }
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "An indication of the speed at which the fan is 
             running.

             This object is now obsolete."
        ::= { enclosureFanEntry 5 }

    enclosureTemperatureNumber OBJECT-TYPE
        SYNTAX Integer32
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "The total number of temperature sensors which are 
             present in all of the enclosures in all racks within 
             the system.

             This object is now obsolete."
        ::= { enclosures 3 }

    enclosureTemperatureTable OBJECT-TYPE
        SYNTAX SEQUENCE OF EnclosureTemperatureEntry
        MAX-ACCESS not-accessible
        STATUS obsolete
        DESCRIPTION
            "A table containing temperature readings for each 
             sensor located within each enclosure (of each rack)
             within this system.

             The number of entries in this table is given by the value of 
             enclosureTemperatureNumber.

             This object is now obsolete."
        ::= { enclosures 4 }

    enclosureTemperatureEntry OBJECT-TYPE
        SYNTAX EnclosureTemperatureEntry
        MAX-ACCESS not-accessible
        STATUS obsolete
        DESCRIPTION
            "Information obtained from a temperature sensor located
             within an enclosure.

             This object is now obsolete."
        INDEX { enclosureTemperatureRackIndex, enclosureTemperatureEnclosureIndex, enclosureTemperatureTempIndex }
        ::= { enclosureTemperatureTable 1 }

    EnclosureTemperatureEntry ::=
        SEQUENCE {
            enclosureTemperatureRackIndex
                Integer32,
            enclosureTemperatureEnclosureIndex
                Integer32,
            enclosureTemperatureTempIndex
                Integer32,
            enclosureTemperatureStatus
                INTEGER,
            enclosureTemperatureOverTemp
                INTEGER,
            enclosureTemperatureTempC
                Integer32,
            enclosureTemperatureTempF
                Integer32,
            enclosureTemperatureRange
                INTEGER
        }

    enclosureTemperatureRackIndex OBJECT-TYPE
        SYNTAX Integer32 (1..10)
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "The rack in which the temperature sensor is located.
             The sensor identified by a particular value of this index is
             located within the rack identified by the same value of
             rackIndex.

             This object is now obsolete."
        ::= { enclosureTemperatureEntry 1 }

    enclosureTemperatureEnclosureIndex OBJECT-TYPE
        SYNTAX Integer32 (1..8)
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "The enclosure in which the temperature sensor is located.
             Enclosure numbers start at 1.

             This object is now obsolete."
        ::= { enclosureTemperatureEntry 2 }

    enclosureTemperatureTempIndex OBJECT-TYPE
        SYNTAX Integer32 (1..4)
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "An identifier for the temperature sensor within
             the enclosure.
             Temperature sensor numbers start at 1.

             This object is now obsolete."
        ::= { enclosureTemperatureEntry 3 }

    enclosureTemperatureStatus OBJECT-TYPE
        SYNTAX INTEGER {
            ok(1),
            failed(2),
            unknown(3)
        }
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "The status of the temperature sensor.

             This object is now obsolete."
        ::= { enclosureTemperatureEntry 4 }

    enclosureTemperatureOverTemp OBJECT-TYPE
        SYNTAX INTEGER {
            overTemp(1),
            noOverTemp(2),
            unknown(3)
        }
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "A flag indicating whether an over temperature has
             been detected by the sensor.
             
             overTemp(1)   - an over temperature has been detected.
             noOverTemp(2) - an over temperature has NOT been detected.

             This object is now obsolete."
        ::= { enclosureTemperatureEntry 5 }

    enclosureTemperatureTempC OBJECT-TYPE
        SYNTAX Integer32
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "The temperature measured by the sensor in degrees Celsius.

            A value of -1 indicates the temperature could not be
            determined.

             This object is now obsolete."
        ::= { enclosureTemperatureEntry 6 }

    enclosureTemperatureTempF OBJECT-TYPE
        SYNTAX Integer32
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "The temperature measured by the sensor in degrees Fahrenheit.

            A value of -1 indicates the temperature could not be
            determined.

             This object is now obsolete."
        ::= { enclosureTemperatureEntry 7 }

    enclosureTemperatureRange OBJECT-TYPE
        SYNTAX INTEGER {
            normal(1),
            warm(2),
            hot(3),
            unknown(4)
        }
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "Status indicator of the temperature with regard to the
             operating temperatures expected within the enclosure.

             This object is now obsolete."
        ::= { enclosureTemperatureEntry 8 }

    enclosurePSUNumber OBJECT-TYPE
        SYNTAX Integer32
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "The total number of power supplies which are present in 
             all of the enclosures in all racks within the system.

             This object is now obsolete."
        ::= { enclosures 5 }

    enclosurePSUTable OBJECT-TYPE
        SYNTAX SEQUENCE OF EnclosurePSUEntry
