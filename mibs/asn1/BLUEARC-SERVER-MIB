    httpMaximumLogFiles OBJECT-TYPE
        SYNTAX Unsigned32
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "The maximum number of log files the HTTP server will
             generate during logging.

             This object is now obsolete as no HTTP server is
             present."
        ::= { httpConfig 19 }

    httpMimeMapNumber OBJECT-TYPE
        SYNTAX Integer32
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "The number of mime map entries configured for the HTTP server.

             This object is now obsolete as no HTTP server is
             present."
        ::= { httpConfig 20 }

    httpMimeMapTable OBJECT-TYPE
        SYNTAX SEQUENCE OF HttpMimeMapEntry
        MAX-ACCESS not-accessible
        STATUS obsolete
        DESCRIPTION
            "A table containing the mime map configuration for the
             HTTP server in the system.

             The number of entries in this table is given by the value of 
             httpMimeMapNumber.

             This object is now obsolete as no HTTP server is
             present."
        ::= { httpConfig 21 }

    httpMimeMapEntry OBJECT-TYPE
        SYNTAX HttpMimeMapEntry
        MAX-ACCESS not-accessible
        STATUS obsolete
        DESCRIPTION
            "Mime map information.

             This object is now obsolete as no HTTP server is
             present."
        INDEX { httpMimeMapIndex }
        ::= { httpMimeMapTable 1 }

    HttpMimeMapEntry ::=
        SEQUENCE {
            httpMimeMapIndex
                Integer32,
            httpMimeMapExtension
                DisplayString,
            httpMimeMapType
                DisplayString
        }

    httpMimeMapIndex OBJECT-TYPE
        SYNTAX Integer32 (1..150)
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "A unique value for each mime map configured for the
             HTTP server in this system.

             Its value ranges between 1 and the value of httpMimeMapNumber.

             This object is now obsolete as no HTTP server is
             present."
        ::= { httpMimeMapEntry 1 }

    httpMimeMapExtension OBJECT-TYPE
        SYNTAX DisplayString
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "The extension on the file.

             This object is now obsolete as no HTTP server is
             present."
        ::= { httpMimeMapEntry 2 }

    httpMimeMapType OBJECT-TYPE
        SYNTAX DisplayString
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "The corresponding type the extension maps to.

             This object is now obsolete as no HTTP server is
             present."
        ::= { httpMimeMapEntry 3 }

--------------------------------------------------------------------------------
--
-- File Protocol / HTTP / HTTP Statistics
--
--------------------------------------------------------------------------------

    httpConnsAccepted OBJECT-TYPE
        SYNTAX Counter64
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "The number of TCP/IP connections that clients have 
             made to the server.  Clients running HTTP/1.1 can make 
             multiple requests per connection, and so this number will
             typically be lower that the total number of requests.

             This object is now obsolete as no HTTP server is
             present."
        ::= { httpStats 1 }

    httpConnsRefused OBJECT-TYPE
        SYNTAX Counter64
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "The total number of connections that were rejected 
             with a '503 Service Unavailable' message.

             This object is now obsolete as no HTTP server is
             present."
        ::= { httpStats 2 }

    httpBytesXferOut OBJECT-TYPE
        SYNTAX Counter64
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "The number of bytes sent by the server.  
             This includes output HTTP headers.

             This object is now obsolete as no HTTP server is
             present."
        ::= { httpStats 3 }

    httpBytesXferIn OBJECT-TYPE
        SYNTAX Counter64
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "The number of bytes received by the server.

             This object is now obsolete as no HTTP server is
             present."
        ::= { httpStats 4 }

    httpGetRequests OBJECT-TYPE
        SYNTAX Counter64
