    udpInShortPkts OBJECT-TYPE
        SYNTAX Counter64
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "The total number of short UDP packets received.

             This object is obsolete. It now forms part of the table
             udpStatisticsTable."
        ::= { udpStats 1 }

    udpInInvChksumPkts OBJECT-TYPE
        SYNTAX Counter64
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "The total number of UDP packets received with invalid
             checksums.

             This object is obsolete. It now forms part of the table
             udpStatisticsTable."
        ::= { udpStats 2 }
        
    udpStatisticsTable OBJECT-TYPE
        SYNTAX SEQUENCE OF UdpStatisticsEntry
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION
            "A table of UDP statistics for ethernet interfaces.

             The number of entries in this table is given by the value of
             ifNumber."
        ::= { udpStats 3 }

    udpStatisticsEntry OBJECT-TYPE
        SYNTAX UdpStatisticsEntry
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION
            "UDP statistics for ethernet interfaces."
        INDEX { ifIndex }
        ::= { udpStatisticsTable 1 }

    UdpStatisticsEntry ::=
        SEQUENCE {
            udpRxShortPkts
                Counter64,
            udpRxInvChksumPkts
                Counter64
        }

    udpRxShortPkts OBJECT-TYPE
        SYNTAX Counter64
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The total number of short UDP packets received."
        ::= { udpStatisticsEntry 1 }

    udpRxInvChksumPkts OBJECT-TYPE
        SYNTAX Counter64
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The total number of UDP packets received with invalid
             checksums."
        ::= { udpStatisticsEntry 2 }

