--------------------------------------------------------------------------------
--
-- System / Storage / NVRAM volume statistics
--
--------------------------------------------------------------------------------

    nvramFsStatsNumber OBJECT-TYPE
        SYNTAX Unsigned32 (1..2147483647)
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The number of volumes with NVRAM statistics."
        ::= { nvramStats 1 }

    nvramFsStatsTable OBJECT-TYPE
        SYNTAX SEQUENCE OF NvramFsStatsEntry
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION
            "A table of NVRAM data for a given file system."
        ::= { nvramStats 2 }

    nvramFsStatsEntry OBJECT-TYPE
        SYNTAX NvramFsStatsEntry
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION
            "Information about NVRAM on a given file system. The
            file system must be formatted, mounted, and NVRAM must
            be supported."
        INDEX { fsId }
        ::= { nvramFsStatsTable 1 }

    NvramFsStatsEntry ::=
        SEQUENCE {
            fsId
                Unsigned32,
            nvramFsStatsCurrentUsage
                Unsigned32,
            nvramFsStatsCheckpoints
                Unsigned32,
            nvramFsStatsActivityCheckpoints
                Unsigned32,
            nvramFsStatsWaitedAllocs
                Unsigned32,
            nvramFsStatsWaitingAllocs
                Unsigned32
        }

    fsId OBJECT-TYPE
        SYNTAX Unsigned32
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The id of the Span hosting the volume with which the
            NVRAM is associated."
        ::= { nvramFsStatsEntry 1 }

    nvramFsStatsCurrentUsage OBJECT-TYPE
        SYNTAX Unsigned32
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The amount of NVRAM currently in use for this volume."
        ::= { nvramFsStatsEntry 2 }

   nvramFsStatsCheckpoints OBJECT-TYPE
        SYNTAX Unsigned32
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The total number of checkpoints which have occurred for the
            NVRAM assigned to this volume."
        ::= { nvramFsStatsEntry 3 }

   nvramFsStatsActivityCheckpoints OBJECT-TYPE
        SYNTAX Unsigned32
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The number of checkpoints which have occurred for the NVRAM
            assigned to this volume as a result of file system activity
            (i.e. not timed checkpoints."
        ::= { nvramFsStatsEntry 4 }

   nvramFsStatsWaitedAllocs OBJECT-TYPE
        SYNTAX Unsigned32
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The number of allocations for this file system for which
             a wait has occurred."
        ::= { nvramFsStatsEntry 5 }

   nvramFsStatsWaitingAllocs OBJECT-TYPE
        SYNTAX Unsigned32
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The number of allocations for this file system for which 
             a wait is currently occurring."
        ::= { nvramFsStatsEntry 6 } 

--------------------------------------------------------------------------------
--
-- System / Storage / NVRAM pool statistics
--
--------------------------------------------------------------------------------
        
   nvramPoolStatsTable OBJECT-TYPE
        SYNTAX SEQUENCE OF NvramPoolStatsEntry
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION
            "A table of NVRAM data for a given Cluster Node."
        ::= { nvramStats 6 }

    nvramPoolStatsEntry OBJECT-TYPE
        SYNTAX NvramPoolStatsEntry
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION
            "Information about NVRAM on a given Cluster Node."
        INDEX { clusterNodeId }
        ::= { nvramPoolStatsTable 1 }

    NvramPoolStatsEntry ::=
        SEQUENCE {
            clusterNodeId
                Unsigned32,
            nvramPoolStatsSize
                Unsigned32,
            nvramPoolStatsMaximumUsed
                Unsigned32,
            nvramPoolStatsTotalCurrentUsage
                Unsigned32,
            nvramPoolStatsWaitedAllocs
                Unsigned32,
            nvramPoolStatsWaitingAllocs
                Unsigned32
        }

    clusterNodeId OBJECT-TYPE
        SYNTAX Unsigned32 (0..255)
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The ID of the cluster node for which these pool
            statistics are reported."
        ::= { nvramPoolStatsEntry 1 }

    nvramPoolStatsSize OBJECT-TYPE
        SYNTAX Unsigned32 (1..2147483647)
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The total size of nvram. If no formatted or mounted file system
            is found, or if nvram is not supported, this value will be reported
            as zero."
        ::= { nvramPoolStatsEntry 2 }

    nvramPoolStatsMaximumUsed OBJECT-TYPE
        SYNTAX Unsigned32 (1..2147483647)
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The maximum amount of nvram used. If no formatted or mounted 
            file system is found, or if nvram is not supported, this value 
            will be reported as zero."
        ::= { nvramPoolStatsEntry 3 }

    nvramPoolStatsTotalCurrentUsage OBJECT-TYPE
        SYNTAX Unsigned32 (1..2147483647)
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The amount of nvram currently in use. If no formatted or mounted
            file system is found, or if nvram is not supported, this value 
            will be reported as zero."
        ::= { nvramPoolStatsEntry 4 }

    nvramPoolStatsWaitedAllocs OBJECT-TYPE
        SYNTAX Unsigned32
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The number of allocations, for the entire NVRAM pool, for which 
            a wait has occurred."
        ::= { nvramPoolStatsEntry 5 }

    nvramPoolStatsWaitingAllocs OBJECT-TYPE
        SYNTAX Unsigned32 (1..2147483647)
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The number of allocations, for the entire NVRAM pool, for which 
            a wait is occurring."
        ::= { nvramPoolStatsEntry 6 }

