--------------------------------------------------------------------------------

    nfsUserMappingNumber OBJECT-TYPE
        SYNTAX Integer32
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "The number of NFS to NT user mappings which are configured 
             for the system."
        ::= { nfsUserMapping 1 }

    nfsUserMappingTable OBJECT-TYPE
        SYNTAX SEQUENCE OF NfsUserMappingEntry
        MAX-ACCESS not-accessible
        STATUS obsolete
        DESCRIPTION
            "A table containing the NFS to NT user mapping configuration
             for the system.

             The number of entries in this table is given by the value of 
             nfsUserMappingNumber."
        ::= { nfsUserMapping 2 }

    nfsUserMappingEntry OBJECT-TYPE
        SYNTAX NfsUserMappingEntry
        MAX-ACCESS not-accessible
        STATUS obsolete
        DESCRIPTION
            "NFS to NT user mapping information."
        INDEX { nfsUserMappingUnixUserName }
        ::= { nfsUserMappingTable 1 }

    NfsUserMappingEntry ::=
        SEQUENCE {
            nfsUserMappingUnixUserName
                DisplayString,
            nfsUserMappingUnixUserIDValid
                INTEGER,
            nfsUserMappingUnixUserID
                Unsigned32,
            nfsUserMappingNtUserName
                DisplayString,
            nfsUserMappingNtUserDomainName
                DisplayString
        }

    nfsUserMappingUnixUserName OBJECT-TYPE
        SYNTAX DisplayString
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "The name of the UNIX user for which the mapping exists."
        ::= { nfsUserMappingEntry 1 }

    nfsUserMappingUnixUserIDValid OBJECT-TYPE
        SYNTAX INTEGER {
            valid(1),
            invalid(2),
            unknown(3)
        }
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "Indicates whether the UNIX user ID is configured user in
             the nfsUserTable."
        ::= { nfsUserMappingEntry 2 }

    nfsUserMappingUnixUserID OBJECT-TYPE
        SYNTAX Unsigned32
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "The UNIX user ID."
        ::= { nfsUserMappingEntry 3 }

    nfsUserMappingNtUserName OBJECT-TYPE
        SYNTAX DisplayString
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "The name of the NT user for which the mapping exists."
        ::= { nfsUserMappingEntry 4 }

    nfsUserMappingNtUserDomainName OBJECT-TYPE
        SYNTAX DisplayString
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "The NT domain name for which the mapping exists."
        ::= { nfsUserMappingEntry 5 }

