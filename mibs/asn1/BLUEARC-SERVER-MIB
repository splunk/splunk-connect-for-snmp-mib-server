    commit3 OBJECT-TYPE
        SYNTAX Counter32
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "The total number of 'Commit' calls received for NFS version 3 
             since power up.

             This object is now obsolete, and is replaced with a table
             of values indexed by Cluster Node and name."
        ::= { nfsVersion3 22 }

    nfsV3StatsTable OBJECT-TYPE
        SYNTAX SEQUENCE OF NfsV3StatsEntry
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION
            "The statistics for the NFS version 3 service.

            The number of entries in this table is given by the
            value of clusterPNodeNumber."
        ::= { nfsVersion3 23 }

    nfsV3StatsEntry OBJECT-TYPE
        SYNTAX NfsV3StatsEntry
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION
            "The statistics for the NFS version 3 service."
        INDEX { nfsV3StatsClusterNode }
        ::= { nfsV3StatsTable 1 }

    NfsV3StatsEntry ::=
        SEQUENCE {
            nfsV3StatsClusterNode
                Unsigned32,
            nfsV3nullCalls
                Counter32,
            nfsV3getAttrCalls
                Counter32,
            nfsV3setAttrCalls
                Counter32,
            nfsV3lookupCalls
                Counter32,
            nfsV3accessCalls
                Counter32,
            nfsV3readLink
                Counter32,
            nfsV3read
                Counter32,
            nfsV3write
                Counter32,
            nfsV3create
                Counter32,
            nfsV3mkdir
                Counter32,
            nfsV3symLink
                Counter32,
            nfsV3mkNod
                Counter32,
            nfsV3remove
                Counter32,
            nfsV3rmDir
                Counter32,
            nfsV3rename
                Counter32,
            nfsV3link       
                Counter32,
            nfsV3readDir
                Counter32,
            nfsV3readDirPlus
                Counter32,
            nfsV3fsStat
                Counter32,
            nfsV3fsInfo
                Counter32,
            nfsV3pathConf
                Counter32,
            nfsV3commit
                Counter32
        }

    nfsV3StatsClusterNode OBJECT-TYPE
        SYNTAX Unsigned32
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The Cluster Node for this value."
        ::= { nfsV3StatsEntry 1 }

    nfsV3nullCalls OBJECT-TYPE
        SYNTAX Counter32
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The total number of 'Null' calls received for NFS version 3 
             since power up."
