    cifsStatsTable OBJECT-TYPE
        SYNTAX SEQUENCE OF CifsStatsEntry
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION
            "The statistics for the CIFS service.

            The number of entries in this table is given by the
            value of cifsStatsClusterNode."
        ::= { cifsStats 39 }

    cifsStatsEntry OBJECT-TYPE
        SYNTAX CifsStatsEntry
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION
            "The statistics for the CIFS service."
        INDEX { cifsStatsClusterNode }
        ::= { cifsStatsTable 1 }

    CifsStatsEntry ::=
        SEQUENCE {
            cifsStatsClusterNode
                Unsigned32,
            clients
                Unsigned32,
            mkdirCalls
                Unsigned32,
            rmdirCalls
                Unsigned32,
            openCalls
                Unsigned32,
            createCalls
                Unsigned32,
            closeCalls
                Unsigned32,
            flushCalls
                Unsigned32,
            unlinkCalls
                Unsigned32,
            renameCalls
                Unsigned32,
            getatrCalls
                Unsigned32,
            setatrCalls
                Unsigned32,
            readCalls
                Unsigned32,
            writeCalls
                Unsigned32,
            mknewCalls
                Unsigned32,
            chkpthCalls
                Unsigned32,
            lseekCalls
                Unsigned32,
            readBrawCalls
                Unsigned32,
            writeBrawCalls
                Unsigned32,
            lockingXCalls
                Unsigned32,
            transCalls
                Unsigned32,
            echoCalls
                Unsigned32,
            writeCloseCalls
                Unsigned32,
            openXCalls
                Unsigned32,
            readXCalls
                Unsigned32,
            writeXCalls
                Unsigned32,
            trans2Calls
                Unsigned32,
            findCloseCalls
                Unsigned32,
            tdisCalls
                Unsigned32,
            negProtCalls
                Unsigned32,
            sessSetupXCalls
                Unsigned32,
            ulogoffXCalls
                Unsigned32,
            tconXCalls
                Unsigned32,
            dskattrCalls
                Unsigned32,
            searchCalls
                Unsigned32,
            ntTransCalls    
                Unsigned32,
            ntTranssCalls
                Unsigned32,
            ntCreateXCalls
                Unsigned32,
            ntCancelCalls
                Unsigned32
        }

    cifsStatsClusterNode OBJECT-TYPE
        SYNTAX Unsigned32
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The Cluster Node for this value."
        ::= { cifsStatsEntry 1 }

