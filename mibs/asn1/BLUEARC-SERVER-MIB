--------------------------------------------------------------------------------
--
-- File Protocol / NFS / Statistics / NFS Version 2
--
--------------------------------------------------------------------------------

    null2Calls OBJECT-TYPE
        SYNTAX Counter32
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "The total number of 'Null' calls received for NFS version 2 
             since power up.

             This object is now obsolete, and is replaced with a table
             of values indexed by Cluster Node and name."
        ::= { nfsVersion2 1 }

    getAttr2Calls OBJECT-TYPE
        SYNTAX Counter32
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "The total number of 'GetAttr' calls received for NFS version 2 
             since power up.

             This object is now obsolete, and is replaced with a table
             of values indexed by Cluster Node and name."
        ::= { nfsVersion2 2 }

    setAttr2Calls OBJECT-TYPE
        SYNTAX Counter32
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "The total number of 'SetAttr' calls received for NFS version 2 
             since power up.

             This object is now obsolete, and is replaced with a table
             of values indexed by Cluster Node and name."
        ::= { nfsVersion2 3 }

    rootCalls OBJECT-TYPE
        SYNTAX Counter32
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "The total number of 'Root' calls received since power up
             NOTE: this variable is not used/implemented.

             This object is now obsolete, and is replaced with a table
             of values indexed by Cluster Node and name."
        ::= { nfsVersion2 4 }

    lookup2Calls OBJECT-TYPE
        SYNTAX Counter32
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "The total number of 'Lookup' calls received for NFS version 2 
             since power up.

             This object is now obsolete, and is replaced with a table
             of values indexed by Cluster Node and name."
        ::= { nfsVersion2 5 }

    readLink2 OBJECT-TYPE
        SYNTAX Counter32
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "The total number of 'ReadLink' calls received for NFS version 2 
             since power up.

             This object is now obsolete, and is replaced with a table
             of values indexed by Cluster Node and name."
        ::= { nfsVersion2 6 }

    read2 OBJECT-TYPE
        SYNTAX Counter32
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "The total number of 'Read' calls received for NFS version 2 
             since power up.

             This object is now obsolete, and is replaced with a table
             of values indexed by Cluster Node and name."
        ::= { nfsVersion2 7 }

    writeCache OBJECT-TYPE
        SYNTAX Counter32
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "The total number of 'Write Cache' calls received since power up.
             NOTE : this variable is not used/implemented.

             This object is now obsolete, and is replaced with a table
             of values indexed by Cluster Node and name."
        ::= { nfsVersion2 8 }

    write2 OBJECT-TYPE
        SYNTAX Counter32
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "The total number of 'Write' calls received for NFS version 2 
             since power up.

             This object is now obsolete, and is replaced with a table
             of values indexed by Cluster Node and name."
        ::= { nfsVersion2 9 }

    create2 OBJECT-TYPE
        SYNTAX Counter32
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "The total number of 'Create' calls received for NFS version 2 
             since power up.

             This object is now obsolete, and is replaced with a table
             of values indexed by Cluster Node and name."
        ::= { nfsVersion2 10 }

    remove2 OBJECT-TYPE
        SYNTAX Counter32
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "The total number of 'Remove' calls received for NFS version 2 
             since power up.

             This object is now obsolete, and is replaced with a table
             of values indexed by Cluster Node and name."
        ::= { nfsVersion2 11 }

    rename2 OBJECT-TYPE
        SYNTAX Counter32
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "The total number of 'Rename' calls received for NFS version 2 
             since power up.

             This object is now obsolete, and is replaced with a table
             of values indexed by Cluster Node and name."
        ::= { nfsVersion2 12 }

    link2 OBJECT-TYPE
        SYNTAX Counter32
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "The total number of 'Link' calls received for NFS version 2 
             since power up.

             This object is now obsolete, and is replaced with a table
             of values indexed by Cluster Node and name."
        ::= { nfsVersion2 13 }

    symLink2 OBJECT-TYPE
        SYNTAX Counter32
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "The total number of 'Sym Link' calls received for NFS version 2 
             since power up."
        ::= { nfsVersion2 14 }

    mkDir2 OBJECT-TYPE
        SYNTAX Counter32
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "The total number of 'MkDir' calls received for NFS version 2 
             since power up.

             This object is now obsolete, and is replaced with a table
             of values indexed by Cluster Node and name."
        ::= { nfsVersion2 15 }

    rmDir2 OBJECT-TYPE
        SYNTAX Counter32
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "The total number of 'RmDir' calls received for NFS version 2 
             since power up.

             This object is now obsolete, and is replaced with a table
             of values indexed by Cluster Node and name."
        ::= { nfsVersion2 16 }

    readDir2 OBJECT-TYPE
        SYNTAX Counter32
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "The total number of 'ReadDir' calls received for NFS version 2 
             since power up.

             This object is now obsolete, and is replaced with a table
             of values indexed by Cluster Node and name."
        ::= { nfsVersion2 17 }

    statFS2 OBJECT-TYPE
        SYNTAX Counter32
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "The total number of 'StatFS' calls received for NFS version 2 
             since power up.

             This object is now obsolete, and is replaced with a table
             of values indexed by Cluster Node and name."
        ::= { nfsVersion2 18 }

    nfsV2StatsTable OBJECT-TYPE
        SYNTAX SEQUENCE OF NfsV2StatsEntry
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION
            "The statistics for the NFS version 2 service.

            The number of entries in this table is given by the
            value of clusterPNodeNumber."
        ::= { nfsVersion2 19 }

    nfsV2StatsEntry OBJECT-TYPE
        SYNTAX NfsV2StatsEntry
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION
            "The statistics for the NFS version 2 service."
        INDEX { nfsV2StatsClusterNode }
        ::= { nfsV2StatsTable 1 }

    NfsV2StatsEntry ::=
        SEQUENCE {
            nfsV2StatsClusterNode
                Unsigned32,
            nfsV2nullCalls
                Counter32,
            nfsV2getAttrCalls
                Counter32,
            nfsV2setAttrCalls
                Counter32,
            nfsV2rootCalls
                Counter32,
            nfsV2lookupCalls
                Counter32,
            nfsV2readLink
                Counter32,
            nfsV2read
                Counter32,
            nfsV2writeCache
                Counter32,
            nfsV2write
                Counter32,
            nfsV2create
                Counter32,
            nfsV2remove
                Counter32,
            nfsV2rename
                Counter32,
            nfsV2link
                Counter32,
            nfsV2symLink
                Counter32,
            nfsV2mkDir
                Counter32,
            nfsV2rmDir
                Counter32,
            nfsV2readDir
                Counter32,
            nfsV2statFS
                Counter32
        }

    nfsV2StatsClusterNode OBJECT-TYPE
        SYNTAX Unsigned32
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The Cluster Node for this value."
        ::= { nfsV2StatsEntry 1 }

    nfsV2nullCalls OBJECT-TYPE
        SYNTAX Counter32
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The total number of 'Null' calls received for NFS version 2 
             since power up."
        ::= { nfsV2StatsEntry 2 }

    nfsV2getAttrCalls OBJECT-TYPE
        SYNTAX Counter32
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The total number of 'GetAttr' calls received for NFS version 2 
             since power up."
        ::= { nfsV2StatsEntry 3 }

    nfsV2setAttrCalls OBJECT-TYPE
        SYNTAX Counter32
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The total number of 'SetAttr' calls received for NFS version 2 
             since power up."
        ::= { nfsV2StatsEntry 4 }

    nfsV2rootCalls OBJECT-TYPE
        SYNTAX Counter32
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The total number of 'Root' calls received since power up
             NOTE: this variable is not used/implemented."
        ::= { nfsV2StatsEntry 5 }

    nfsV2lookupCalls OBJECT-TYPE
        SYNTAX Counter32
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The total number of 'Lookup' calls received for NFS version 2 
             since power up."
        ::= { nfsV2StatsEntry 6 }

    nfsV2readLink OBJECT-TYPE
        SYNTAX Counter32
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The total number of 'ReadLink' calls received for NFS version 2 
             since power up."
        ::= { nfsV2StatsEntry 7 }

    nfsV2read OBJECT-TYPE
        SYNTAX Counter32
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The total number of 'Read' calls received for NFS version 2 
             since power up."
        ::= { nfsV2StatsEntry 8 }

    nfsV2writeCache OBJECT-TYPE
        SYNTAX Counter32
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The total number of 'Write Cache' calls received since power up.
             NOTE : this variable is not used/implemented."
        ::= { nfsV2StatsEntry 9 }

    nfsV2write OBJECT-TYPE
        SYNTAX Counter32
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The total number of 'Write' calls received for NFS version 2 
             since power up."
        ::= { nfsV2StatsEntry 10 }

    nfsV2create OBJECT-TYPE
        SYNTAX Counter32
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The total number of 'Create' calls received for NFS version 2 
             since power up."
        ::= { nfsV2StatsEntry 11 }

    nfsV2remove OBJECT-TYPE
        SYNTAX Counter32
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The total number of 'Remove' calls received for NFS version 2 
             since power up."
        ::= { nfsV2StatsEntry 12 }

    nfsV2rename OBJECT-TYPE
        SYNTAX Counter32
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The total number of 'Rename' calls received for NFS version 2 
             since power up."
        ::= { nfsV2StatsEntry 13 }

    nfsV2link OBJECT-TYPE
        SYNTAX Counter32
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The total number of 'Link' calls received for NFS version 2 
             since power up."
        ::= { nfsV2StatsEntry 14 }

    nfsV2symLink OBJECT-TYPE
        SYNTAX Counter32
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The total number of 'Sym Link' calls received for NFS version 2 
             since power up."
        ::= { nfsV2StatsEntry 15 }

    nfsV2mkDir OBJECT-TYPE
        SYNTAX Counter32
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The total number of 'MkDir' calls received for NFS version 2 
             since power up."
        ::= { nfsV2StatsEntry 16 }

    nfsV2rmDir OBJECT-TYPE
        SYNTAX Counter32
        MAX-ACCESS read-only
        STATUS current
