    failoverConfigStatus OBJECT-TYPE
        SYNTAX INTEGER {
            configured(1),
            notConfigured(2)
        }
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "Indicates whether failover is configured for the silicon server.

            This object is obsolete.
            Failover has been replaced by clustering."
        ::= { failover 1 }

    failoverPrimaryName OBJECT-TYPE
        SYNTAX DisplayString
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "The primary management name or a zero length string if
             failover is not configured for this system.

            This object is obsolete.
            Failover has been replaced by clustering."
        ::= { failover 2 }

    failoverSystemName OBJECT-TYPE
        SYNTAX DisplayString
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
