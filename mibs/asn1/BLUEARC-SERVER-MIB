--------------------------------------------------------------------------------
--
-- File Protocol / ISCSI / ISCSI Configuration
--
--------------------------------------------------------------------------------

    iScsiServiceEnabled OBJECT-TYPE
        SYNTAX INTEGER {
            enabled(1),
            disabled(2),
            unknown(3)
        }
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "Indicates whether the iSCSI service is enabled or not.
            
            enabled(1)  The service is enabled
            disabled(2) The service is not enabled
            unknown(3)  The service status cannot be determined."
        ::= { iScsiConfiguration 1 }

    iScsiParameterTable OBJECT-TYPE
        SYNTAX SEQUENCE OF IScsiParameterEntry
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION
            "A table containing a list of iSCSI parameters."
        ::= { iScsiConfiguration 2 }

    iScsiParameterEntry OBJECT-TYPE
        SYNTAX IScsiParameterEntry
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION
            "iSCSI parameter information."
        INDEX { iScsiParameterEVS, iScsiParameterName }
        ::= { iScsiParameterTable 1 }

    IScsiParameterEntry ::=
        SEQUENCE {
            iScsiParameterEVS
                Unsigned32,
            iScsiParameterName
                DisplayString,
            iScsiParameterIsBoolean
                INTEGER,
            iScsiParameterValue
                Unsigned32
        }

    iScsiParameterEVS OBJECT-TYPE
        SYNTAX Unsigned32 (1..255)
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The EVS for which the Logical Unit parameters
            are configured."
        ::= { iScsiParameterEntry 1 }

    iScsiParameterName OBJECT-TYPE
        SYNTAX DisplayString
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "A unique name for the iSCSI parameter."
        ::= { iScsiParameterEntry 2 }

    iScsiParameterIsBoolean OBJECT-TYPE
        SYNTAX INTEGER {
            boolean(1),
            notBoolean(2)
        }
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "Whether the parameter is a boolean value.

            If the parameter is boolean, it will have either
            the value 0 (false) or 1 (true)."
        ::= { iScsiParameterEntry 3 }

    iScsiParameterValue OBJECT-TYPE
        SYNTAX Unsigned32
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The value for the parameter.

            If the parameter is boolean, it will have either
            the value 0 (false) or 1 (true)."
        ::= { iScsiParameterEntry 4 }

    iScsiTargetNumber OBJECT-TYPE
        SYNTAX Integer32
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The number of iSCSI targets."
        ::= { iScsiConfiguration 3 }

    iScsiTargetTable OBJECT-TYPE
        SYNTAX SEQUENCE OF IScsiTargetEntry
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION
            "A table containing a list of iSCSI targets.

             The number of entries in this table is given by the value of
             iScsiTargetNumber."
        ::= { iScsiConfiguration 4 }

    iScsiTargetEntry OBJECT-TYPE
        SYNTAX IScsiTargetEntry
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION
            "iSCSI target information."
        INDEX { iScsiGloballyUniqueName }
        ::= { iScsiTargetTable 1 }

    IScsiTargetEntry ::=
        SEQUENCE {
            iScsiGloballyUniqueName
                DisplayString,
            iScsiTargetName
                DisplayString,
            iScsiTargetComment
                DisplayString,
            iScsiTargetLogicalUnitNumber
                Unsigned32,
            iScsiTargetAuthEnabled
                INTEGER
        }

    iScsiGloballyUniqueName OBJECT-TYPE
        SYNTAX DisplayString
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "A unique name for the iSCSI target."
        ::= { iScsiTargetEntry 1 }

    iScsiTargetName OBJECT-TYPE
        SYNTAX DisplayString
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The name of the iSCSI target."
        ::= { iScsiTargetEntry 2 }

    iScsiTargetComment OBJECT-TYPE
        SYNTAX DisplayString
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The comment for the iSCSI target."
        ::= { iScsiTargetEntry 3 }

    iScsiTargetLogicalUnitNumber OBJECT-TYPE
        SYNTAX Unsigned32
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The number of Logical Units assigned
            to the iSCSI target."
        ::= { iScsiTargetEntry 4 }

    iScsiTargetAuthEnabled OBJECT-TYPE
        SYNTAX INTEGER {
            enabled(1),
            disabled(2)
        }
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "Whether authentication is enabled for this
            target."
        ::= { iScsiTargetEntry 5 }

    iScsiLogicalUnitNumber OBJECT-TYPE
        SYNTAX Integer32
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The number of iSCSI logical units."
        ::= { iScsiConfiguration 5 }


    iScsiLogicalUnitTable OBJECT-TYPE
        SYNTAX SEQUENCE OF IScsiLogicalUnitEntry
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION
            "A table containing a list of iSCSI Logical Units.

             The number of entries in this table is given by the value of
             iScsiTargetLogicalUnitNumber in iScsiTargetTable."
        ::= { iScsiConfiguration 6 }

    iScsiLogicalUnitEntry OBJECT-TYPE
        SYNTAX IScsiLogicalUnitEntry
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION
            "iSCSI Logical Unit information.

            Indexed by EVS and Logical Unit name."
        INDEX { iScsiLUEvs, iScsiLUName }
        ::= { iScsiLogicalUnitTable 1 }

    IScsiLogicalUnitEntry ::=
        SEQUENCE {
            iScsiLUEvs
                Unsigned32,
            iScsiLUName
                DisplayString,
            iScsiLUStatus
                INTEGER,
            iScsiLUComment
                DisplayString,
            iScsiLUDeviceId
                Unsigned32,
            iScsiLUPath
                DisplayString,
            iScsiLUInitialized
                INTEGER,
            iScsiLUSize
                Counter64
        }

    iScsiLUEvs OBJECT-TYPE
        SYNTAX Unsigned32 (1..255)
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The EVS to which the Logical Unit is assigned."
        ::= { iScsiLogicalUnitEntry 1 }

    iScsiLUName OBJECT-TYPE
        SYNTAX DisplayString
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The name of the Logical Unit."
        ::= { iScsiLogicalUnitEntry 2 }

    iScsiLUStatus OBJECT-TYPE
        SYNTAX INTEGER {
            unknown(1)
        }
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The status of the Logical Unit

            unknown(1) - the status cannot be determined."
        ::= { iScsiLogicalUnitEntry 3 }

    iScsiLUComment OBJECT-TYPE
        SYNTAX DisplayString
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The comment relating to the Logical Unit."
        ::= { iScsiLogicalUnitEntry 4 }

    iScsiLUDeviceId OBJECT-TYPE
        SYNTAX Unsigned32 (1..4294967295)
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The ID of the File System to which the Logical Unit
            is assigned."
        ::= { iScsiLogicalUnitEntry 5 }

    iScsiLUPath OBJECT-TYPE
        SYNTAX DisplayString
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The path on the File System relating to the 
            Logical Unit."
        ::= { iScsiLogicalUnitEntry 6 }

    iScsiLUInitialized OBJECT-TYPE
        SYNTAX INTEGER {
            initialized(1),
            uninitialized(2),
            unknown(3)
        }
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "Whether the Logical Unit is initialized.

            initialized(1) - the Logical Unit is initialized
            uninitialized(2) - the Logical unit is not initialized
            unknown(3) - the initialization status cannot be determined."
        ::= { iScsiLogicalUnitEntry 7 }

    iScsiLUSize OBJECT-TYPE
        SYNTAX Counter64
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The size, in bytes, of the Logical Unit."
        ::= { iScsiLogicalUnitEntry 8 }

    iScsiTargetLogicalUnitTable OBJECT-TYPE
        SYNTAX SEQUENCE OF IScsiTargetLogicalUnitEntry
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION
            "A table containing a list of iSCSI Logical Units
            assigned to iSCSI targets.

             The number of entries in this table is given by the value of
             iScsiTargetLogicalUnitNumber in iScsiTargetTable."
        ::= { iScsiConfiguration 7 }

    iScsiTargetLogicalUnitEntry OBJECT-TYPE
        SYNTAX IScsiTargetLogicalUnitEntry
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION
            "Information about iSCSI Logical Units
            assigned to iScsi targets.

            Indexed by target name and Logical Unit name."
        INDEX { iScsiTargetLUTargetName, iScsiTargetLULogicalUnitName }
        ::= { iScsiTargetLogicalUnitTable 1 }

    IScsiTargetLogicalUnitEntry ::=
        SEQUENCE {
            iScsiTargetLUTargetName
                DisplayString,
            iScsiTargetLULogicalUnitName
                DisplayString,
            iScsiTargetLun
                Integer32
        }

    iScsiTargetLUTargetName OBJECT-TYPE
        SYNTAX DisplayString
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The globally unique name of the target."
        ::= { iScsiTargetLogicalUnitEntry 1 }

    iScsiTargetLULogicalUnitName OBJECT-TYPE
        SYNTAX DisplayString
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The name of the Logical Unit assigned to the
            target."
        ::= { iScsiTargetLogicalUnitEntry 2 }

    iScsiTargetLun OBJECT-TYPE
        SYNTAX Integer32
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The Logical Unit Number allocated to the
            LUN when it was assigned to the target.
            This is an arbitrary number which must be unique
            on the target."
        ::= { iScsiTargetLogicalUnitEntry 3 }

    iSNSTable OBJECT-TYPE
        SYNTAX SEQUENCE OF ISNSEntry
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION
            "A table containing details of iSNS servers."

        ::= { iScsiConfiguration 8 }

    iSNSEntry OBJECT-TYPE
        SYNTAX ISNSEntry
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION
            "Information about iSNS servers."
        INDEX { iSNSIpAddress }
        ::= { iSNSTable 1 }

    ISNSEntry ::=
        SEQUENCE {
            iSNSIpAddress
                IpAddress,
            iSNSPort
                Unsigned32
        }

    iSNSIpAddress OBJECT-TYPE
        SYNTAX IpAddress
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The IP address of the iSNS server."
        ::= { iSNSEntry 1 }

    iSNSPort OBJECT-TYPE
        SYNTAX Unsigned32
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The port on which to connect to the iSNS
            server."
        ::= { iSNSEntry 2 }

    iScsiStatisticsTable OBJECT-TYPE
        SYNTAX SEQUENCE OF IScsiStatisticsEntry
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION
            "A table containing a list of iSCSI statistics."
        ::= { iScsiStatistics 1 }

    iScsiStatisticsEntry OBJECT-TYPE
        SYNTAX IScsiStatisticsEntry
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION
            "Information about iSCSI statistics.

            Indexed by Cluster Node id and metric name."
        INDEX { iScsiStatisticsNodeId, iScsiStatisticsName }
        ::= { iScsiStatisticsTable 1 }

    IScsiStatisticsEntry ::=
        SEQUENCE {
            iScsiStatisticsNodeId
                Unsigned32,
            iScsiStatisticsName
                DisplayString,
            iScsiStatisticsValue
                Counter64
        }

    iScsiStatisticsNodeId OBJECT-TYPE
        SYNTAX Unsigned32
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The Cluster Node for this value."
        ::= { iScsiStatisticsEntry 1 }

    iScsiStatisticsName OBJECT-TYPE
        SYNTAX DisplayString
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The name of the value being reported."
        ::= { iScsiStatisticsEntry 2 }

    iScsiStatisticsValue OBJECT-TYPE
        SYNTAX Counter64
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The statistics value reported."
        ::= { iScsiStatisticsEntry 3 }


