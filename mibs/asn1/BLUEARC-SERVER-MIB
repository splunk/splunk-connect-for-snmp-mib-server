--
-- performance / cacheStats
--
--------------------------------------------------------------------------------
        
    metaDataCacheStatsNumber OBJECT-TYPE
        SYNTAX Integer32
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The number of metadata caches for which statistics are available."
        ::= { cacheStats 1 }

    metaDataCacheStatsTable OBJECT-TYPE
        SYNTAX SEQUENCE OF MetaDataCacheStatsEntry
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION
            "Table of metadata cache statistics entries"
        ::= { cacheStats 2 }

    metaDataCacheStatsEntry OBJECT-TYPE
        SYNTAX MetaDataCacheStatsEntry
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION
            "Metadata cache statistical information.
             Metadata cache statistics are provided for the following
             vlsi chipset: wfile, wdir, wtree, obj, fsa."
        INDEX { metaDataCacheStatsFsId, metaDataCache }
        ::= { metaDataCacheStatsTable 1 }

    MetaDataCacheStatsEntry ::=
        SEQUENCE {
            metaDataCacheStatsFsId
                Integer32,
            metaDataCache
                INTEGER,
            metaDataCacheStatsFsLabel
                DisplayString,
            metaDataCacheStatsHits
                Counter32,
            metaDataCacheStatsMisses
                Counter32
        }

    metaDataCacheStatsFsId OBJECT-TYPE
        SYNTAX Integer32 (1..2147483647)
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The unique device Id for this file system."
        ::= { metaDataCacheStatsEntry 1 }

    metaDataCache OBJECT-TYPE
        SYNTAX INTEGER {
            wfile(1),
            wdir(2),
            wtree(3),
            objRoot(4),
            objLeaf(5),
            objIndirectionObject(6),
            fsa(7)
        }
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "Enumerated identity of the chip to which these stats pertain."
        ::= { metaDataCacheStatsEntry 2 }
        
    metaDataCacheStatsFsLabel OBJECT-TYPE
        SYNTAX DisplayString
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The file system label."
        ::= { metaDataCacheStatsEntry 3 }

    metaDataCacheStatsHits OBJECT-TYPE
        SYNTAX Counter32
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The number of cache hits experienced with this metadata cache."
        ::= { metaDataCacheStatsEntry 4 }

    metaDataCacheStatsMisses OBJECT-TYPE
        SYNTAX Counter32
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The number of cache misses experienced with this metadata cache."
        ::= { metaDataCacheStatsEntry 5 }
        
    sectorCacheStatsNumber OBJECT-TYPE
        SYNTAX Integer32
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The number of sector caches for which statistics are available."
        ::= { cacheStats 3 }

    sectorCacheStatsTable OBJECT-TYPE
        SYNTAX SEQUENCE OF SectorCacheStatsEntry
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION
            "Table of sector cache statistics entries"
        ::= { cacheStats 4 }

    sectorCacheStatsEntry OBJECT-TYPE
        SYNTAX SectorCacheStatsEntry
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION
            "Sector cache statistical information. Sector cache statistics are
             provided for the following operations: read, read-ahead, write."
        INDEX { sectorCacheStatsCnIndex, sectorCacheType }
        ::= { sectorCacheStatsTable 1 }

    SectorCacheStatsEntry ::=
        SEQUENCE {
            sectorCacheStatsCnIndex
                Unsigned32,
            sectorCacheType
                INTEGER,
            sectorCacheStatsHitsPSI
                Counter32,
            sectorCacheStatsHitsSSI
                Counter32,
            sectorCacheStatsHitsTotal
                Counter32,
            sectorCacheStatsMissesPSI
                Counter32,
            sectorCacheStatsMissesSSI
                Counter32,
            sectorCacheStatsMissesTotal
                Counter32
        }

    sectorCacheStatsCnIndex OBJECT-TYPE
        SYNTAX Unsigned32 (1..255)
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The cluster node for these statistics."
        ::= { sectorCacheStatsEntry 1 }

    sectorCacheType OBJECT-TYPE
        SYNTAX INTEGER {
            read(1),
            readAhead(2),
            write(3)
        }
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "Enumerated identity of the operation to which these stats pertain."
        ::= { sectorCacheStatsEntry 2 }

    sectorCacheStatsHitsPSI OBJECT-TYPE
        SYNTAX Counter32
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The number of cache hits experienced with this sector cache."
        ::= { sectorCacheStatsEntry 3 }

    sectorCacheStatsHitsSSI OBJECT-TYPE
        SYNTAX Counter32
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The number of cache hits experienced with this sector cache."
        ::= { sectorCacheStatsEntry 4 }

    sectorCacheStatsHitsTotal OBJECT-TYPE
        SYNTAX Counter32
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The number of cache hits experienced with this sector cache."
        ::= { sectorCacheStatsEntry 5 }

    sectorCacheStatsMissesPSI OBJECT-TYPE
        SYNTAX Counter32
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The number of cache misses experienced with this sector cache."
        ::= { sectorCacheStatsEntry 6 }

    sectorCacheStatsMissesSSI OBJECT-TYPE
        SYNTAX Counter32
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The number of cache misses experienced with this sector cache."
        ::= { sectorCacheStatsEntry 7 }

    sectorCacheStatsMissesTotal OBJECT-TYPE
        SYNTAX Counter32
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The number of cache misses experienced with this sector cache."
        ::= { sectorCacheStatsEntry 8 }


--------------------------------------------------------------------------------
--
-- performance / protocolStats (opcodes)
--
--------------------------------------------------------------------------------

    protocolStatsNumber OBJECT-TYPE
        SYNTAX Integer32
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The number of protocol statistical entries in the table.
            
            If enhanced statistics gathering is not enabled, this
            value will be zero. See the command
            enhanced-perf-stats."
        ::= { protocolStats 1 }
        
    protocolStatsTable OBJECT-TYPE
        SYNTAX SEQUENCE OF ProtocolStatsEntry
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION
            "Table of network protocol statistics entries. 
            
            If enhanced statistics gathering is not enabled, this
            table will be empty. See the command
            enhanced-perf-stats."
        ::= { protocolStats 2 }

    protocolStatsEntry OBJECT-TYPE
        SYNTAX ProtocolStatsEntry
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION
            "Network protocol statistical information."
        INDEX { protStatsFsId, protStatsFlavor, protStatsOpCode }
        ::= { protocolStatsTable 1 }

    ProtocolStatsEntry ::=
        SEQUENCE {
            protStatsFsId
                Integer32,
            protStatsFlavor
                INTEGER,
            protStatsOpCode
                Unsigned32,
            protStatsFsLabel
                DisplayString,
            protStatsOpCodeName
                DisplayString,
            protOpCount
                Counter64,
            protCumulativeLatency
                Counter64
        }

    protStatsFsId OBJECT-TYPE
        SYNTAX Integer32 (1..2147483647)
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The device ID for this file system."
        ::= { protocolStatsEntry 1 }

    protStatsFlavor OBJECT-TYPE
        SYNTAX INTEGER {
            NFS(0),
            CIFS(1)
        }
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The type of network protocol these statistics pertain to
             one of CIFS, NFS."
        ::= { protocolStatsEntry 2 }

    protStatsOpCode OBJECT-TYPE
        SYNTAX Unsigned32
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The opcode for the protocol operation in question."
        ::= { protocolStatsEntry 3 }

    protStatsFsLabel OBJECT-TYPE
        SYNTAX DisplayString
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The file system label."
        ::= { protocolStatsEntry 4 }

    protStatsOpCodeName OBJECT-TYPE
        SYNTAX DisplayString
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The name of the op-code."
        ::= { protocolStatsEntry 5 }

    protOpCount OBJECT-TYPE
        SYNTAX Counter64
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The total number of operations for this type of opcode."
        ::= { protocolStatsEntry 6 }

    protCumulativeLatency OBJECT-TYPE
        SYNTAX Counter64
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The cumulative latency experienced by operations for this opcode."
        ::= { protocolStatsEntry 7 }
   
