                Counter32,
            fcPeakOutRate
                Counter32,
            fcSignalLossErrors
                Counter32,
            fcBadRXCharErrors
                Counter32,
            fcLossSyncErrors
                Counter32,
            fcLinkFailErrors
                Counter32,
            fcRXEOFErrors
                Counter32,
            fcDiscardedFrameErrors
                Counter32,
            fcBadCRCErrors
                Counter32,
            fcProtocolErrors
                Counter32
        }

    fcInterfaceIndex OBJECT-TYPE
        SYNTAX Integer32 (0..128)
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "The index of the fibre channel interface to which
            the statistics in this row apply.

            This object is now obsolete."
        ::= { fcStatsEntry 1 }

