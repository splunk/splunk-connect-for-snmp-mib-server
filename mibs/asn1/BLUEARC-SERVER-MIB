    ethTotalBytes OBJECT-TYPE
        SYNTAX Counter64
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "The total number of bytes successfully transmitted and received
            through the network interface.

             This object is obsolete. It now forms part of the table
             ethernetStatisticsTable."
        ::= { etherStats 20 }

    ethernetStatisticsTable OBJECT-TYPE
        SYNTAX SEQUENCE OF EthernetStatisticsEntry
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION
            "A table of statistics for ethernet interfaces.

             The number of entries in this table is given by the value of
             ifNumber."
        ::= { etherStats 21 }

    ethernetStatisticsEntry OBJECT-TYPE
        SYNTAX EthernetStatisticsEntry
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION
            "Statistics for ethernet interfaces."
        INDEX { ifIndex }
        ::= { ethernetStatisticsTable 1 }

    EthernetStatisticsEntry ::=
        SEQUENCE {
            ethernetInstInOctetRate
                Counter32,
            ethernetInstOutOctetRate
                Counter32,
            ethernetPkInOctetRate
                Counter32,
            ethernetPkOutOctetRate
                Counter32,
            ethernetInFIFODrops
                Counter64,
            ethernetCRCErrs
                Counter64,
            ethernetOutFIFOUflows
                Counter64,
            ethernetOutOneCollision
                Counter64,
            ethernetOutTwoCollision
                Counter64,
            ethernetOutFifteenCollision
                Counter64,
            ethernetInEvFIFOPktDrop
                Counter64,
            ethernetEvFIFOMaxEvents
                Counter64,
            ethernetOutPackets
                Counter64,
            ethernetInPackets
                Counter64,
            ethernetTotalPackets
                Counter64,
            ethernetOutBytes
                Counter64,
            ethernetInBytes
                Counter64,
            ethernetTotalBytes
                Counter64,
            pausedOffTime
                Counter64
        }

     ethernetInstInOctetRate OBJECT-TYPE
        SYNTAX Counter32
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The instantaneous receive rate in bytes per second for
             data received through the network interface."
