--------------------------------------------------------------------------------
--
-- System / Storage / Automount
--
--------------------------------------------------------------------------------

    automountNumber OBJECT-TYPE
        SYNTAX Integer32
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "The number of automounts configured.

            This object is obsolete.
            Volumes are no longer automounted."
        ::= { automount 1 }

    automountTable OBJECT-TYPE
        SYNTAX SEQUENCE OF AutomountEntry
        MAX-ACCESS not-accessible
        STATUS obsolete
        DESCRIPTION
            "A table containing the automount configuration.

             The number of entries in this table is given by the value of 
             automountNumber.

            This object is obsolete.
            Volumes are no longer automounted."
        ::= { automount 2 }

    automountEntry OBJECT-TYPE
        SYNTAX AutomountEntry
        MAX-ACCESS not-accessible
        STATUS obsolete
        DESCRIPTION
            "An automount configuration entry.

            This object is obsolete.
            Volumes are no longer automounted."
        INDEX { automountIndex, automountPartitionIndex }
        ::= { automountTable 1 }

    AutomountEntry ::=
        SEQUENCE {
            automountIndex
                Integer32,
            automountPartitionIndex
                Unsigned32,
            automountWWN
                DisplayString,
            automountLUN
                Unsigned32
        }

    automountIndex OBJECT-TYPE
        SYNTAX Integer32 (1..100)
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "An identifier for the automount.

             Its value ranges between 1 and the value of automountNumber.

            This object is obsolete.
            Volumes are no longer automounted."
        ::= { automountEntry 1 }

    automountPartitionIndex OBJECT-TYPE
        SYNTAX Unsigned32
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "The partition number of the disk to automount.

            This object is obsolete.
            Volumes are no longer automounted."
        ::= { automountEntry 2 }

    automountWWN OBJECT-TYPE
        SYNTAX DisplayString
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "The world wide name (WWN) of the disk to automount.

            This object is obsolete.
            Volumes are no longer automounted."
        ::= { automountEntry 3 }

    automountLUN OBJECT-TYPE
        SYNTAX Unsigned32
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "The logical unit number (LUN) of the disk to automount.

            This object is obsolete.
            Volumes are no longer automounted."
        ::= { automountEntry 4 }

--------------------------------------------------------------------------------
--
-- System / Storage / System Drives
--
--------------------------------------------------------------------------------

    sysDriveNumber OBJECT-TYPE
        SYNTAX Integer32
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The number of accessible system drives.
             NOTE: A system drive is a single logical drive which is
                   created and managed by a RAID controller.  It comprises of
                   a number of physical drives, each of which contribute to
                   the total storage capability of the system drive."
        ::= { systemDrives 1 }

    sysDriveTable OBJECT-TYPE
        SYNTAX SEQUENCE OF SysDriveEntry
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION
            "A table containing information about each accessible
             system drive.

             The number of entries in this table is given by the value of
             sysDriveNumber."
        ::= { systemDrives 2 }

    sysDriveEntry OBJECT-TYPE
        SYNTAX SysDriveEntry
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION
            "Information about a system drive."
        INDEX { sysDriveIndex }
        ::= { sysDriveTable 1 }

    SysDriveEntry ::=
        SEQUENCE {
            sysDriveIndex
                Integer32,
            sysDriveWWN
                DisplayString,
            sysDriveLUN
                Unsigned32,
            sysDriveStatus
                INTEGER,
            sysDriveCapacity
                Counter64,
            sysDriveRaidLevel
                INTEGER,
            sysDriveCacheMode
                INTEGER
        }

    sysDriveIndex OBJECT-TYPE
        SYNTAX Integer32 (1..25)
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "An identifier for the system drive (system drive id).

             Its value ranges between 1 and the value of sysDriveNumber."
        ::= { sysDriveEntry 1 }

    sysDriveWWN OBJECT-TYPE
        SYNTAX DisplayString
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The world wide name (WWN) of the system drive."
        ::= { sysDriveEntry 2 }

    sysDriveLUN OBJECT-TYPE
        SYNTAX Unsigned32
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The logical unit number (LUN) of the system drive.
             NOTE: upto 8 system drives can exist under a single
                   RAID controller.

             LUNs range from 0 to 7."
        ::= { sysDriveEntry 3 }

    sysDriveStatus OBJECT-TYPE
        SYNTAX INTEGER {
            online(1),
            corrupt(2),
            failed(3),
            notPresent(4),
            disconnected(5),
            offline(6),
            initializing(7),
            formatting(8),
            unknown(9)
        }
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The status of the system drive.

             online(1)          - the system drive is accessible through
                                  the fibre channel interface.
             corrupt(2)         - the system drives master boot record
                                  is corrupt.
             failed(3)          - the system drive has failed and is no
                                  longer accessible through the fibre
                                  channel interface.
             notPresent(4)      - device no longer present.
             disconnected(5)    - device is present but not accessible
                                  by the controller
             offline(6)         - all ports are offline  
             initializing(7)    - not accessible until initialization phase has 
                                  completed
             formatting(8)      - device is formatting
             unknown(9)         - unknown device status

             This object will only have the value unknown(9)."

        ::= { sysDriveEntry 4 }

    sysDriveCapacity OBJECT-TYPE
        SYNTAX Counter64
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The capacity of the system drive in bytes."
        ::= { sysDriveEntry 5 }

    sysDriveRaidLevel OBJECT-TYPE
        SYNTAX INTEGER {
            raid0(1),
            raid1(2),
            raid3(3),
            raid5(4),
            raid10(5),
            jBOD(6),
            raid30(7),
            raid50(8),
            unknown(9)
        }
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The configured RAID level of the system drive.
             NOTE : jBOD(6) - means just a bunch of disks."
        ::= { sysDriveEntry 6 }

    sysDriveCacheMode OBJECT-TYPE
        SYNTAX INTEGER {
            writeThrough(1),
            writeBack(2),
            unknown(3)
        }
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The caching mode employed by the RAID controller for
             this system drive.

             This object will only have the value unknown(3)."
        ::= { sysDriveEntry 7 }

--------------------------------------------------------------------------------
--
-- System / Storage / Volumes
--
--------------------------------------------------------------------------------

    volumeNumber OBJECT-TYPE
        SYNTAX Integer32
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The number of volumes which are known to the system."
        ::= { volumes 1 }

    volumeTable OBJECT-TYPE
        SYNTAX SEQUENCE OF VolumeEntry
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION
            "A table containing information about each volume
             known to the system.

             The number of entries in this table is given by the value of
             volumeNumber."
        ::= { volumes 2 }

    volumeEntry OBJECT-TYPE
        SYNTAX VolumeEntry
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION
            "Information about a volume."
        INDEX { volumeSysDriveIndex, volumePartitionID }
        ::= { volumeTable 1 }

    VolumeEntry ::=
        SEQUENCE {
            volumeSysDriveIndex
                Integer32,
            volumePartitionID
                Unsigned32,
            volumeLabel
                DisplayString,
            volumeStatus
                INTEGER,
            volumeCapacity
                Counter64,
            volumeFreeCapacity
                Counter64,
            volumeEnterpriseVirtualServer
                DisplayString
        }

    volumeSysDriveIndex OBJECT-TYPE
        SYNTAX Integer32 (1..2147483647)
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The system drive on which the volume is stored.
             The system drive identified by a particular value of this index is
             located on the system drive identified by the same value of
             sysDriveIndex."
        ::= { volumeEntry 1 }

    volumePartitionID OBJECT-TYPE
        SYNTAX Unsigned32
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The partition ID of the volume.
             NOTE: this is the offset from the start of the system drive
                   at which the volume starts."
        ::= { volumeEntry 2 }

    volumeLabel OBJECT-TYPE
        SYNTAX DisplayString
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The label assigned to the volume by a user for
             identification purposes.
             NOTE: If a label has not been assigned to the volume, this
                   value will be a zero length string."
        ::= { volumeEntry 3 }

    volumeStatus OBJECT-TYPE
        SYNTAX INTEGER {
            unformatted(1),
            mounted(2),
            formatted(3),
            needsChecking(4)
        }
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The status of the volume.

             unformatted(1)   - the volume does NOT contain a filesystem.
             mounted(2)       - the volume contains a file system and may
                                be being accessed by users.
             formatted(3)     - the volume contains a filesystem, but is
                                not accessible by users as it is not
                                mounted.
             needsChecking(4) - the filesystem has some errors and cannot
                                be mounted.  Its must be checked and fixed
                                before it can be accessed by users."
        ::= { volumeEntry 4 }

    volumeCapacity OBJECT-TYPE
        SYNTAX Counter64
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The volumes capacity in bytes."
        ::= { volumeEntry 5 }

    volumeFreeCapacity OBJECT-TYPE
        SYNTAX Counter64
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The remaining free volume capacity in bytes.
             NOTE: This value is 0 if the volume is not mounted."
        ::= { volumeEntry 6 }

    volumeEnterpriseVirtualServer OBJECT-TYPE
        SYNTAX DisplayString
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The name of the Enterprise Virtual Server to which this
            volume is assigned."
        ::= { volumeEntry 7 }

    fsStatsTable OBJECT-TYPE
        SYNTAX SEQUENCE OF FsStatsEntry
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION
            "A table containing statistics about each FileSystem
             known to the system.

             The number of entries in this table is given by the value of
             volumeNumber."
        ::= { volumes 3 }

    fsStatsEntry OBJECT-TYPE
        SYNTAX FsStatsEntry
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION
            "Statistics on a FileSystem."
        INDEX { fsPermId }
        ::= { fsStatsTable 1 }

    FsStatsEntry ::=
        SEQUENCE {
            fsPermId
                Counter64,
            fsLabel
                DisplayString,
            opsPerSecAverage
                Unsigned32
        }

    fsPermId OBJECT-TYPE
        SYNTAX Counter64
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The permanent ID of the FileSystem for which these statistics
             are reported."
        ::= { fsStatsEntry 1 }

    fsLabel OBJECT-TYPE
        SYNTAX DisplayString
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The label of the FileSystem for which these statistics
             are reported."
        ::= { fsStatsEntry 2 }

    opsPerSecAverage OBJECT-TYPE
        SYNTAX Unsigned32
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The average file system accesses per second for the FileSystem. 
             These accesses relate to network protocols (such as CIFS and NFS)
             as well as commands issued from the CLI, backup and various
             background processes. 
            
             This average does not include hardware-accelerated accesses, and 
             so the values obtained may differ from opsPerSecond, a cluster-
             wide value that includes hardware-accelerated accesses.

             If the FileSystem is not mounted, or is unavailable for some other
             reason, this value is zero."
        ::= { fsStatsEntry 3 }


--------------------------------------------------------------------------------
--
-- System / Storage / Fibre Channel Statistics
--
--------------------------------------------------------------------------------

    fcRequests OBJECT-TYPE
        SYNTAX Counter64
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The total number of requests issued through the fibre channel
             interface."
        ::= { fcStats 1 }

    fcResponses OBJECT-TYPE
        SYNTAX Counter64
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The total number of responses received through the fibre channel
             interface."
        ::= { fcStats 2 }

    fcReadReqs OBJECT-TYPE
        SYNTAX Counter64
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The total number of disk read requests issued through the fibre
             channel interface."
        ::= { fcStats 3 }

    fcWriteReqs OBJECT-TYPE
        SYNTAX Counter64
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The total number of disk write requests issued through the
             fibre channel interface."
        ::= { fcStats 4 }

    fcReadResps OBJECT-TYPE
        SYNTAX Counter64
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The total number of disk read responses received through the
             fibre channel interface."
        ::= { fcStats 5 }

    fcWriteResps OBJECT-TYPE
        SYNTAX Counter64
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The total number of disk write responses received through the
             fibre channel interface."
        ::= { fcStats 6 }

    fcInstInRate OBJECT-TYPE
        SYNTAX Counter32
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "The instantaneous receive rate in bytes per second for data
             received through the fibre channel interface.

             This object is obsolete. It forms a column in fcStatsTable."
        ::= { fcStats 7 }

    fcInstOutRate OBJECT-TYPE
        SYNTAX Counter32
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "The instantaneous transmission rate in bytes per second for
             data transmitted through the fibre channel interface.

             This object is obsolete. It forms a column in fcStatsTable."
        ::= { fcStats 8 }

    fcPkInRate OBJECT-TYPE
        SYNTAX Counter32
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "The peak receive rate in bytes per second for data received
             through the fibre channel interface.

             This object is obsolete. It forms a column in fcStatsTable."
        ::= { fcStats 9 }

    fcPkOutRate OBJECT-TYPE
        SYNTAX Counter32
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "The peak transmission rate in bytes per second for
             data transmitted through the fibre channel interface.

             This object is obsolete. It forms a column in fcStatsTable."
        ::= { fcStats 10 }

    fcCacheHits OBJECT-TYPE
        SYNTAX Counter64
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The total number of disk read requests which were fulfilled
             through reading the sector cache (ie the requests did NOT
             require access to the disk)."
        ::= { fcStats 11 }

    fcCacheMisses OBJECT-TYPE
        SYNTAX Counter64
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The total number disk read requests which required access
             to the disk (ie the requests could NOT be fulfilled
             through reading the sector cache)."
        ::= { fcStats 12 }

    fcLossSignalErrs OBJECT-TYPE
        SYNTAX Counter32
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "The total number of loss of signal errors.

             This object is obsolete. It forms a column in fcStatsTable."
        ::= { fcStats 13 }

    fcBadRXCharErrs OBJECT-TYPE
        SYNTAX Counter32
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "The total number of bad receive character errors.

             This object is obsolete. It forms a column in fcStatsTable."
        ::= { fcStats 14 }

    fcLossSyncErrs OBJECT-TYPE
        SYNTAX Counter32
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "The total number of loss of synchronisation errors.

             This object is obsolete. It forms a column in fcStatsTable."
        ::= { fcStats 15 }

    fcLinkFailErrs OBJECT-TYPE
        SYNTAX Counter32
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "The total number of link fail errors.

             This object is obsolete. It forms a column in fcStatsTable."
        ::= { fcStats 16 }

    fcRXEOFErrs OBJECT-TYPE
        SYNTAX Counter32
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "The total number of messages received from the frame sender
             indicating that the frame should be aborted because a problem
             occurred during transmission.

             This object is obsolete. It forms a column in fcStatsTable."
        ::= { fcStats 17 }

    fcDiscardedFrameErrs OBJECT-TYPE
        SYNTAX Counter32
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "The total number of discarded frame errors.

             This object is obsolete. It forms a column in fcStatsTable."
        ::= { fcStats 18 }

    fcBadCRCErrs OBJECT-TYPE
        SYNTAX Counter32
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "The total number of framing errors due to bad cyclic redundancy
             codes.

             This object is obsolete. It forms a column in fcStatsTable."
        ::= { fcStats 19 }

    fcProtErrs OBJECT-TYPE
        SYNTAX Counter32
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "The total number of fibre channel protocol errors.

             This object is obsolete. It forms a column in fcStatsTable."
        ::= { fcStats 20 }

    fcIOStatusResubs OBJECT-TYPE
        SYNTAX Counter64
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The number of I/O requests that were resubmitted because they
             failed the first time."
        ::= { fcStats 21 }

    fcIOStatusFails OBJECT-TYPE
        SYNTAX Counter64
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The number of I/O requests that failed after resubmission.
             NOTE: This indicates a fatal error condition with the fibre
                   channel interface hardware."
        ::= { fcStats 22 }

    fibreChannelInterfaceNumber OBJECT-TYPE
        SYNTAX Unsigned32 (1 .. 32)              --32 interfaces?
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The number of fibre channel interfaces."
        ::= { fcStats 23 }

    fcStatsTable OBJECT-TYPE
        SYNTAX SEQUENCE OF FcStatsEntry
        MAX-ACCESS not-accessible
        STATUS obsolete
        DESCRIPTION
            "A table of statistics for the fibre channel interfaces.

            This object is now obsolete."
        ::= { fcStats 24 }

    fcStatsEntry OBJECT-TYPE
        SYNTAX FcStatsEntry
        MAX-ACCESS not-accessible
        STATUS obsolete
        DESCRIPTION
            "A set of statistics for a fibre channel interface. The
            interface is indexed by fcInterfaceIndex.

            This object is now obsolete."
        INDEX { fcInterfaceIndex }
        ::= { fcStatsTable 1 }

    FcStatsEntry ::=
        SEQUENCE {
            fcInterfaceIndex
                Integer32,
            fcInstantaneousInRate
                Counter32,
            fcInstantaneousOutRate
                Counter32,
