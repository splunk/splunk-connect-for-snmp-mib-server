        acosDhcpClientIndex           
                INTEGER,
        acosDhcpClientIpAddress
                IpAddress,
        acosDhcpClientMacAddress
                OCTET STRING,
        acosDhcpClientName
                DisplayString,
        acosDhcpClientLeaseTime
                INTEGER,
        acosDhcpClientSubnetMask
                IpAddress,
        acosDhcpClientGateway
                IpAddress,
        acosDhcpClientDnsServer
                IpAddress
                                        }

