        SYNTAX AcosDhcpClientEntry
        ACCESS not-accessible
        STATUS mandatory
    --% GROUP Dhcp
    --% ALIAS "Entry In DHCP Server Client Table"
        DESCRIPTION
        "An entry in the DHCP server special handling client table."
        INDEX { acosDhcpClientIndex
              }
        ::= { acosDhcpClientTable 1 }

