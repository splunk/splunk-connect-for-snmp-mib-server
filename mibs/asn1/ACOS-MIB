		::= { acosWANSerialStatsEntry 4 }

acosWANRxMissedFrames  OBJECT-TYPE
		SYNTAX INTEGER
		ACCESS read-only
		STATUS mandatory
    --% GROUP WAN
	--% ALIAS "WAN RX Frames Missed"
		DESCRIPTION
		"The total number of frames discarded on this port because the 
		receive buffer descriptor ring was empty."
		::= { acosWANSerialStatsEntry 5 }

acosWANRxErrorFrames  OBJECT-TYPE
		SYNTAX INTEGER
		ACCESS read-only
		STATUS mandatory
    --% GROUP WAN
	--% ALIAS "WAN RX Error Frames"
		DESCRIPTION
