--   1.3.6.1.4.1.231.7.1.2.2.1.12.1.2.10
		sheShdslVccTpTable OBJECT-TYPE
			SYNTAX SEQUENCE OF SheShdslVccTpEntry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"The VccTp-Table."
			::= { sheShdslMIBObjects 10 }

		
--   1.3.6.1.4.1.231.7.1.2.2.1.12.1.2.10.1
		sheShdslVccTpEntry OBJECT-TYPE
			SYNTAX SheShdslVccTpEntry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"This table contains the VccTp-Configurations. There will be one
				entry for every shdsl-interface available per default. This default
				entry should never be deleted."
			INDEX { sheShdslVccTpIndex }
			::= { sheShdslVccTpTable 1 }

		
		SheShdslVccTpEntry ::=
			SEQUENCE { 
				sheShdslVccTpIndex
					Integer32,
				sheShdslVccTpSubLayerIndex
					InterfaceIndex,
				sheShdslVccTpNumber
					Integer32,
				sheShdslVccTpVpi
					Integer32,
				sheShdslVccTpVci
					Integer32,
				sheShdslVccTpOamLevel
					SheAtmOamLevelType,
				sheShdslVccTpLoopTest
					SheAtmOamLoopTestType,
				sheShdslVccTpLoopTestResult
					SheAtmOamLoopTestResultType,
				sheShdslVccTpLoopLocId
					OCTET STRING,
				sheShdslVccTpAlarmSeverityIndex
					Integer32
			 }

--   1.3.6.1.4.1.231.7.1.2.2.1.12.1.2.10.1.1
		sheShdslVccTpIndex OBJECT-TYPE
			SYNTAX Integer32 (1..65565)
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"Index for that table. The index will follow this rule:
				VccTpIndex = (VccTpSubLayerIndex << 4) + VccTpNumber"
			::= { sheShdslVccTpEntry 1 }

		
--   1.3.6.1.4.1.231.7.1.2.2.1.12.1.2.10.1.2
