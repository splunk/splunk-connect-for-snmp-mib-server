--   1.3.6.1.4.1.231.7.1.2.2.1.12.1.2.3
		sheShdslLineParamsTable OBJECT-TYPE
			SYNTAX SEQUENCE OF SheShdslLineParamsEntry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"This MO is proprietary extension of RFC 3276 MIB and contains the proprietary line parameters.
				The table will only be supported for interfaces of iftype sdsl (96)!"
			::= { sheShdslMIBObjects 3 }

		
--   1.3.6.1.4.1.231.7.1.2.2.1.12.1.2.3.1
		sheShdslLineParamsEntry OBJECT-TYPE
			SYNTAX SheShdslLineParamsEntry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"An entry (conceptual row) in the dsShdslLineParamsTable.
				The Table is indexed by ifIndex.
				The table will only be supported for interfaces of iftype sdsl (96)!"
			INDEX { ifIndex }
			::= { sheShdslLineParamsTable 1 }

		
		SheShdslLineParamsEntry ::=
			SEQUENCE { 
				sheShdslAction
					SheSdslActionType,
				sheShdslMode
					SheShdslModeType,
				sheShdslPowerScale
					SheShdslPowerScaleType,
				sheShdslEncodeCoeffA
					SheShdslEncodeCoeffAType,
				sheShdslEncodeCoeffB
					SheShdslEncodeCoeffBType,
				sheShdslTxEOCBufferLen
					Unsigned32,
				sheShdslRxEOCBufferLen
					Unsigned32,
				sheShdslNTR
					SheShdslNTRType,
				sheShdslRxUpstreamFrameSync
					Unsigned32,
				sheShdslRxDownstreamFrameSync
					Unsigned32,
				sheShdslRxUpstreamStuffBits
					Unsigned32,
				sheShdslRxDownstreamStuffBits
					Unsigned32,
				sheShdslInitiate
					SheShdslInitiateType,
				sheShdslFramerRxClockMode
					SheShdslFramerRxClockModeType,
				sheShdslFramerRxPllMode
					SheShdslEnableModeType,
				sheShdslSerialAtmCiuBufferSize
					SheShdslSerialAtmCiuBufferSizeType,
				sheShdslTxFramerPulseDelay
					Unsigned32,
				sheShdslRxFramerPulseDelay
					Unsigned32,
				sheShdslMultiFrameMode
					SheShdslEnableModeType,
				sheShdslEnable4or6MbpsBitrate
					SheShdslEnableModeType,
				sheShdslTomDataWord1
					Unsigned32,
				sheShdslTomDataWord2
					Unsigned32,
				sheShdslTomDataWord3
					Unsigned32,
				sheShdslTomDataWord4
					Unsigned32,
