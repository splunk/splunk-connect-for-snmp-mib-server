-- Node definitions
--
	
-- 1.3.6.1.4.1.231.7.1.2.2.1.12.1
		sheShdslLineMIB OBJECT IDENTIFIER ::= { sheShdslMib 1 }

		
--   1.3.6.1.4.1.231.7.1.2.2.1.12.1.2
		sheShdslMIBObjects OBJECT IDENTIFIER ::= { sheShdslLineMIB 2 }

		
--   1.3.6.1.4.1.231.7.1.2.2.1.12.1.2.1
		sheShdslCapabilityGroup OBJECT IDENTIFIER ::= { sheShdslMIBObjects 1 }

		
--   1.3.6.1.4.1.231.7.1.2.2.1.12.1.2.1.1
		sheShdslCapabilityLineTxCap OBJECT-TYPE
			SYNTAX SheHdsl2ShdslTransmissionModeType
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"This bitmap specifies the transmission modes that the STU-C is capable of supporting. Right now, support for Annex A (q9921PotsNonOverlapped(2) and q9921PotsOverlapped(3)) is present.  This value depends on the DSL PHY firmware present on Columbia MxU."
			::= { sheShdslCapabilityGroup 1 }

		
--   1.3.6.1.4.1.231.7.1.2.2.1.12.1.2.2
		sheShdslLineStatusTable OBJECT-TYPE
			SYNTAX SEQUENCE OF SheShdslLineStatusEntry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"This MO is proprietary extension of RFC 3276 MIB and contains the proprietary line status.
				The table will only be supported for interfaces of iftype sdsl (96)!"
			::= { sheShdslMIBObjects 2 }

		
--   1.3.6.1.4.1.231.7.1.2.2.1.12.1.2.2.1
		sheShdslLineStatusEntry OBJECT-TYPE
			SYNTAX SheShdslLineStatusEntry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"An entry (conceptual row) in the dsShdslLineStatusTable.
				The Table is indexed by ifIndex.
				The table will only be supported for interfaces of iftype sdsl (96)!"
			INDEX { ifIndex }
			::= { sheShdslLineStatusTable 1 }

		
		SheShdslLineStatusEntry ::=
			SEQUENCE { 
				sheShdslOpState
					SheShdslOpStateType,
				sheShdslStartProgress
					SheShdslStartProgressType,
				sheShdslFwRelease
					OCTET STRING,
				sheShdslLineSwap
					SheShdslLineSwapType,
				sheShdslRmtCountryCode
					OCTET STRING,
				sheShdslRmtEncoderA
					Integer32,
				sheShdslRmtEncoderB
					Integer32,
				sheShdslRmtProviderCode
					OCTET STRING,
				sheShdslLocDetect
					Unsigned32,
				sheShdslTxPower
					Unsigned32,
				sheShdslFramerSync
					SheShdslFramerSyncType,
				sheShdslRmtTomData
					Unsigned32,
				sheShdslDriftAlarm
					Unsigned32,
				sheShdslReceiverGain
					Unsigned32,
				sheShdslBertError
					SheShdslBertErrorType,
				sheShdslFramerOHAndDefects
					OCTET STRING,
				sheShdslRmtFwVersion
					Unsigned32,
				sheShdslUtopiaCellDelineation
					SheShdslUtopiaCellDelineationType,
				sheShdslUtopiaRxCellCnt
					Unsigned32,
				sheShdslUtopiaCellDropCnt
					Unsigned32,
				sheShdslUtopiaRxHecErrorCnt
					Unsigned32,
				sheShdslUtopiaTxCellCnt
					Unsigned32,
				sheShdslRmtNsfCusId
					Unsigned32,
				sheShdslRmtNsfCusData
					OCTET STRING,
				sheShdslLocalHandshake
					OCTET STRING,
				sheShdslRemoteHandshake
					OCTET STRING,
				sheShdslActualHandshake
					OCTET STRING,
				sheShdslRmtTxPower
					Unsigned32,
				sheShdslRmtPowerBackoff
					SheShdslRmtPowerBackoffType,
				sheShdslAutoRetrainCount
					Unsigned32,
				sheShdslEocState
					SheShdslEocStateType,
				sheShdslFramerOneSecondCnt
					OCTET STRING,
				sheShdslNtrFault
					SheShdslNtrFaultType,
				sheShdslCpeMasterCore
					SheShdslCpeMasterCoreType,
				sheShdslParametricTestResult
					SheShdslParametricTestResultType,
				sheShdslParametricTestArray
					OCTET STRING,
				sheShdslStartUpInfo
					SheShdslStartUpInfoType,
				sheShdslBisModeLocalHandshake
					OCTET STRING,
				sheShdslBisModeRemoteHandshake
					OCTET STRING,
				sheShdslBisModeActualHandshake
					OCTET STRING
			 }

--  for shdsl.bis(SGagMOShdslLineStatusTableTbl.sheShdslStartUpInfo)
-- 1.3.6.1.4.1.231.7.1.2.2.1.12.1.2.2.1.1
		sheShdslOpState OBJECT-TYPE
			SYNTAX SheShdslOpStateType
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"This object identifies the high level operational state for the STU."
			::= { sheShdslLineStatusEntry 1 }

		
--   1.3.6.1.4.1.231.7.1.2.2.1.12.1.2.2.1.2
		sheShdslStartProgress OBJECT-TYPE
			SYNTAX SheShdslStartProgressType
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"This object identifies the current detailed operational state of the STU."
			::= { sheShdslLineStatusEntry 2 }

		
--   1.3.6.1.4.1.231.7.1.2.2.1.12.1.2.2.1.3
		sheShdslFwRelease OBJECT-TYPE
			SYNTAX OCTET STRING (SIZE (0 | 1))
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"Transceiver firmware release number."
			::= { sheShdslLineStatusEntry 3 }

		
--   1.3.6.1.4.1.231.7.1.2.2.1.12.1.2.2.1.4
		sheShdslLineSwap OBJECT-TYPE
			SYNTAX SheShdslLineSwapType
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"This object indicates if the physical lines are swapped, i.e., logical channel A is connected to physical channel B. This applies to 4-wire operation only."
			::= { sheShdslLineStatusEntry 4 }

		
--   1.3.6.1.4.1.231.7.1.2.2.1.12.1.2.2.1.5
		sheShdslRmtCountryCode OBJECT-TYPE
			SYNTAX OCTET STRING (SIZE (0..1))
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"This object provides the country code word, as defined in ITU-T G.991.2, for the STU at the other end of the loop. GlobespanVirata sets this to USA."
			::= { sheShdslLineStatusEntry 5 }

		
--   1.3.6.1.4.1.231.7.1.2.2.1.12.1.2.2.1.6
		sheShdslRmtEncoderA OBJECT-TYPE
			SYNTAX Integer32
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"This object identifies the 21-bit value corresponding to encoder coefficient A, as defined in ITU-T G.991.2, for the STU at the other end of the loop."
			::= { sheShdslLineStatusEntry 6 }

		
--   1.3.6.1.4.1.231.7.1.2.2.1.12.1.2.2.1.7
		sheShdslRmtEncoderB OBJECT-TYPE
			SYNTAX Integer32
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"This object identifies the 21-bit value corresponding to encoder coefficient B, as defined in ITU-T G.991.2, for the STU at the other end of the loop."
			::= { sheShdslLineStatusEntry 7 }

		
--   1.3.6.1.4.1.231.7.1.2.2.1.12.1.2.2.1.8
		sheShdslRmtProviderCode OBJECT-TYPE
			SYNTAX OCTET STRING (SIZE (0..1))
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"This object identifies the provider code word, as defined in ITU-T G.991.2, for the STU at the other end of the loop."
			::= { sheShdslLineStatusEntry 8 }

		
--   1.3.6.1.4.1.231.7.1.2.2.1.12.1.2.2.1.9
		sheShdslLocDetect OBJECT-TYPE
			SYNTAX Unsigned32
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"This object is used to determine if carrier has been lost."
			::= { sheShdslLineStatusEntry 9 }

		
--   1.3.6.1.4.1.231.7.1.2.2.1.12.1.2.2.1.10
		sheShdslTxPower OBJECT-TYPE
			SYNTAX Unsigned32
			UNITS "tenths of dBm"
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"This object identifies the local STU transmit power in tenths of a dBm."
			::= { sheShdslLineStatusEntry 10 }

		
--   1.3.6.1.4.1.231.7.1.2.2.1.12.1.2.2.1.11
		sheShdslFramerSync OBJECT-TYPE
			SYNTAX SheShdslFramerSyncType
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"This object returns information regarding the framer synchronization status."
			::= { sheShdslLineStatusEntry 11 }

		
--   1.3.6.1.4.1.231.7.1.2.2.1.12.1.2.2.1.12
		sheShdslRmtTomData OBJECT-TYPE
			SYNTAX Unsigned32
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"This object provides vendor-provided data, as defined in ITU-T G.991.2, for the STU at the other end of the loop."
			::= { sheShdslLineStatusEntry 12 }

		
--   1.3.6.1.4.1.231.7.1.2.2.1.12.1.2.2.1.13
		sheShdslDriftAlarm OBJECT-TYPE
			SYNTAX Unsigned32
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"This object identifies if the receive clock is in or out of range."
			::= { sheShdslLineStatusEntry 13 }

		
--   1.3.6.1.4.1.231.7.1.2.2.1.12.1.2.2.1.14
		sheShdslReceiverGain OBJECT-TYPE
			SYNTAX Unsigned32
			UNITS "dB"
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"This object provides the total receiver gain in dB."
			::= { sheShdslLineStatusEntry 14 }

		
--   1.3.6.1.4.1.231.7.1.2.2.1.12.1.2.2.1.15
		sheShdslBertError OBJECT-TYPE
			SYNTAX SheShdslBertErrorType
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"This object provides the count of bit errors since the last time the object was read, as well as the type of synchronization."
			::= { sheShdslLineStatusEntry 15 }

		
--   1.3.6.1.4.1.231.7.1.2.2.1.12.1.2.2.1.16
		sheShdslFramerOHAndDefects OBJECT-TYPE
			SYNTAX OCTET STRING (SIZE (0..10))
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"This object returns overhead data. The four least significant bits contain the overhead data in the following format: bit 0 is losd, bit 1 is sega, bit 2 is ps, and bit 3 is segd."
			::= { sheShdslLineStatusEntry 16 }

		
--   1.3.6.1.4.1.231.7.1.2.2.1.12.1.2.2.1.17
		sheShdslRmtFwVersion OBJECT-TYPE
			SYNTAX Unsigned32
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"This object provides the transceiver firmware release number of the STU at the other end of the loop."
			::= { sheShdslLineStatusEntry 17 }

		
--   1.3.6.1.4.1.231.7.1.2.2.1.12.1.2.2.1.18
		sheShdslUtopiaCellDelineation OBJECT-TYPE
			SYNTAX SheShdslUtopiaCellDelineationType
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"This object indicates whether cell delineation has been found."
			::= { sheShdslLineStatusEntry 18 }

		
--   1.3.6.1.4.1.231.7.1.2.2.1.12.1.2.2.1.19
		sheShdslUtopiaRxCellCnt OBJECT-TYPE
			SYNTAX Unsigned32 (0..65535)
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"This object indicates the number of UTOPIA cells received since the last time the object has been called. The maximum value is 0xFFFF."
			::= { sheShdslLineStatusEntry 19 }

		
--   1.3.6.1.4.1.231.7.1.2.2.1.12.1.2.2.1.20
		sheShdslUtopiaCellDropCnt OBJECT-TYPE
			SYNTAX Unsigned32 (0..255)
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"This object indicates the number of UTOPIA cells dropped since the last time the object has been called. The maximum value is 0xFF."
			::= { sheShdslLineStatusEntry 20 }

		
--   1.3.6.1.4.1.231.7.1.2.2.1.12.1.2.2.1.21
		sheShdslUtopiaRxHecErrorCnt OBJECT-TYPE
			SYNTAX Unsigned32 (0..255)
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"This object indicates the number of UTOPIA cells with HEC errors since the last time the object has been called. The maximum value is 0xFF."
			::= { sheShdslLineStatusEntry 21 }

		
--   1.3.6.1.4.1.231.7.1.2.2.1.12.1.2.2.1.22
		sheShdslUtopiaTxCellCnt OBJECT-TYPE
			SYNTAX Unsigned32 (0..65535)
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"This object indicates the number of UTOPIA cells transmitted since the last time the object has been called. The maximum value is 0xFFFF."
			::= { sheShdslLineStatusEntry 22 }

		
--   1.3.6.1.4.1.231.7.1.2.2.1.12.1.2.2.1.23
		sheShdslRmtNsfCusId OBJECT-TYPE
			SYNTAX Unsigned32
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"This object returns the customer identification that was sent by the STU at the other end of the loop."
			::= { sheShdslLineStatusEntry 23 }

		
--   1.3.6.1.4.1.231.7.1.2.2.1.12.1.2.2.1.24
		sheShdslRmtNsfCusData OBJECT-TYPE
			SYNTAX OCTET STRING (SIZE (0..4))
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"This object returns non-standard format customer data that was sent by the STU at the other end of the loop."
			::= { sheShdslLineStatusEntry 24 }

		
--   1.3.6.1.4.1.231.7.1.2.2.1.12.1.2.2.1.25
		sheShdslLocalHandshake OBJECT-TYPE
			SYNTAX OCTET STRING (SIZE (0..52))
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"This object provides a way to see what capabilities are supported by the local STU. A total of 26 handshake parameters are supported."
			::= { sheShdslLineStatusEntry 25 }

		
--   1.3.6.1.4.1.231.7.1.2.2.1.12.1.2.2.1.26
		sheShdslRemoteHandshake OBJECT-TYPE
			SYNTAX OCTET STRING (SIZE (0..52))
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"This object provides a way to see what capabilities are supported by the STU at the other end of the loop. A total of 26 handshake parameters are supported."
			::= { sheShdslLineStatusEntry 26 }

		
--   1.3.6.1.4.1.231.7.1.2.2.1.12.1.2.2.1.27
		sheShdslActualHandshake OBJECT-TYPE
			SYNTAX OCTET STRING (SIZE (0..52))
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"This object provides the results of capabilities exchanged during handshake. A total of 26 handshake parameters are supported."
			::= { sheShdslLineStatusEntry 27 }

		
--   1.3.6.1.4.1.231.7.1.2.2.1.12.1.2.2.1.28
		sheShdslRmtTxPower OBJECT-TYPE
			SYNTAX Unsigned32
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"This object provides the transmit power of the STU at the other end of the loop."
			::= { sheShdslLineStatusEntry 28 }

		
--   1.3.6.1.4.1.231.7.1.2.2.1.12.1.2.2.1.29
		sheShdslRmtPowerBackoff OBJECT-TYPE
			SYNTAX SheShdslRmtPowerBackoffType
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"This object indicates whether power backoff is enabled or disabled at the STU at the other end of the loop."
			::= { sheShdslLineStatusEntry 29 }

		
--   1.3.6.1.4.1.231.7.1.2.2.1.12.1.2.2.1.30
		sheShdslAutoRetrainCount OBJECT-TYPE
			SYNTAX Unsigned32
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"This object indicates the number of automatic retrains. This counter is only reset when a startup is initiated."
			::= { sheShdslLineStatusEntry 30 }

		
--   1.3.6.1.4.1.231.7.1.2.2.1.12.1.2.2.1.31
		sheShdslEocState OBJECT-TYPE
			SYNTAX SheShdslEocStateType
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"This object provides status information about the eoc stage."
			::= { sheShdslLineStatusEntry 31 }

		
--   1.3.6.1.4.1.231.7.1.2.2.1.12.1.2.2.1.32
		sheShdslFramerOneSecondCnt OBJECT-TYPE
			SYNTAX OCTET STRING (SIZE (0..6))
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"This object provides CRC, SEGA, and LOSW defect one second error counts, and should be called every second."
			::= { sheShdslLineStatusEntry 32 }

		
--   1.3.6.1.4.1.231.7.1.2.2.1.12.1.2.2.1.33
		sheShdslNtrFault OBJECT-TYPE
			SYNTAX SheShdslNtrFaultType
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"This object identifies the Network Timing Recovery Fault."
			::= { sheShdslLineStatusEntry 33 }

		
--   1.3.6.1.4.1.231.7.1.2.2.1.12.1.2.2.1.34
		sheShdslCpeMasterCore OBJECT-TYPE
			SYNTAX SheShdslCpeMasterCoreType
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"The CPE master Core."
			::= { sheShdslLineStatusEntry 34 }

		
--   1.3.6.1.4.1.231.7.1.2.2.1.12.1.2.2.1.35
		sheShdslParametricTestResult OBJECT-TYPE
			SYNTAX SheShdslParametricTestResultType
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
