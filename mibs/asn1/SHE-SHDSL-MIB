--   1.3.6.1.4.1.231.7.1.2.2.1.12.1.2.7.3
		shehdsl2ShdslSpanStatusTable OBJECT-TYPE
			SYNTAX SEQUENCE OF Shehdsl2ShdslSpanStatusEntry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"This table provides overall status information of
				HDSL2/SHDSL spans.  This table contains live data from
				equipment.  As such, it is NOT persistent."
			::= { shehdsl2ShdslMibObjects 3 }

		
--   1.3.6.1.4.1.231.7.1.2.2.1.12.1.2.7.3.1
		shehdsl2ShdslSpanStatusEntry OBJECT-TYPE
			SYNTAX Shehdsl2ShdslSpanStatusEntry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"An entry in the hdsl2ShdslSpanStatusTable.  Each entry
				represents the complete span in a single HDSL2/SHDSL line.
				It is indexed by the ifIndex of the associated HDSL2/SHDSL
				line."
			INDEX { ifIndex }
			::= { shehdsl2ShdslSpanStatusTable 1 }

		
		Shehdsl2ShdslSpanStatusEntry ::=
			SEQUENCE { 
				shehdsl2ShdslStatusNumAvailRepeaters
					Unsigned32,
				shehdsl2ShdslStatusMaxAttainableLineRate
					Unsigned32,
				shehdsl2ShdslStatusActualLineRate
					Unsigned32,
				shehdsl2ShdslStatusTransmissionModeCurrent
					SheHdsl2ShdslTransmissionModeType,
				sheShdslStatusMaxAttainablePayloadRate
					Unsigned32,
				sheShdslStatusActualPayloadRate
					Unsigned32,
				sheShdslSpanStatusPAMModulation
					SheShdslPAMModeType
			 }

--   1.3.6.1.4.1.231.7.1.2.2.1.12.1.2.7.3.1.1
		shehdsl2ShdslStatusNumAvailRepeaters OBJECT-TYPE
			SYNTAX Unsigned32 (0..8)
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"Contains the actual number of repeaters/regenerators
				discovered in this HDSL2/SHDSL span."
			::= { shehdsl2ShdslSpanStatusEntry 1 }

		
--   for shdsl.bis(4624000 -> 11408000)
-- 1.3.6.1.4.1.231.7.1.2.2.1.12.1.2.7.3.1.2
		shehdsl2ShdslStatusMaxAttainableLineRate OBJECT-TYPE
			SYNTAX Unsigned32 (0..11408000)
			UNITS "bps"
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"Contains the maximum attainable line rate in this HDSL2/SHDSL
				span. This object provides the maximum rate the line is
				capable of achieving. This is based upon measurements made
				during line probing. This rate includes payload (user data)
				and any applicable framing overhead."
			::= { shehdsl2ShdslSpanStatusEntry 2 }

		
--  for shdsl.bis(4624000 -> 11408000) 
-- 1.3.6.1.4.1.231.7.1.2.2.1.12.1.2.7.3.1.3
		shehdsl2ShdslStatusActualLineRate OBJECT-TYPE
			SYNTAX Unsigned32 (0..11408000)
			UNITS "bps"
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"Contains the actual line rate in this HDSL2/SHDSL span.  This
				should equal ifSpeed. This rate includes payload (user data)
				and any applicable framing overhead."
			::= { shehdsl2ShdslSpanStatusEntry 3 }

		
--   1.3.6.1.4.1.231.7.1.2.2.1.12.1.2.7.3.1.4
		shehdsl2ShdslStatusTransmissionModeCurrent OBJECT-TYPE
			SYNTAX SheHdsl2ShdslTransmissionModeType
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"Contains the current Power Spectral Density (PSD) regional
				setting of the HDSL2/SHDSL span.
				
				Bit   Meaning      Description
				1     region 1     Indicates ITU-T G.991.2 Annex A/F.
				2     region 2     Indicates ITU-T G.991.2 Annex B/G."
			::= { shehdsl2ShdslSpanStatusEntry 4 }

		
--   for shdsl.bis(SGagMOShdslSpanStatusTableTbl.StatusMaxAttainablePayloadRate)
-- 1.3.6.1.4.1.231.7.1.2.2.1.12.1.2.7.3.1.5
		sheShdslStatusMaxAttainablePayloadRate OBJECT-TYPE
			SYNTAX Unsigned32 (0..11408000)
			UNITS "bps"
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"This parameter contains the maximum attainable payload rate in this SHDSL span. This is based upon measurements made during line probing without including any framing overhead."
			::= { shehdsl2ShdslSpanStatusEntry 5 }

		
--   for shdsl.bis(SGagMOShdslSpanStatusTableTbl.StatusActualPayloadRate)
-- 1.3.6.1.4.1.231.7.1.2.2.1.12.1.2.7.3.1.6
		sheShdslStatusActualPayloadRate OBJECT-TYPE
			SYNTAX Unsigned32 (0..11408000)
			UNITS "bps"
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"This parameter contains the actual payload rate in this SHDSL span. This is based upon measurements made during line probing without including any framing overhead."
			::= { shehdsl2ShdslSpanStatusEntry 6 }

		
--   1.3.6.1.4.1.231.7.1.2.2.1.12.1.2.7.14.1.17
		sheShdslSpanStatusPAMModulation OBJECT-TYPE
			SYNTAX SheShdslPAMModeType
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"The parameter show the selected PAM constellation is to be used.
				 - SELECT_16_PAM   - Use 16 PAM only
				 - SELECT_32_PAM   - Use 32 PAM only
				Select 16Pam limits the data rates from 192 kbps to 3840 kbps
				Select 32Pam limits the data rates from 768 kbps to 5696 kbps
				
				This is propietary extension for interfaces of type shdsl (169) only.
				"
			::= { shehdsl2ShdslSpanStatusEntry 7 }

		
--   1.3.6.1.4.1.231.7.1.2.2.1.12.1.2.7.4
		shehdsl2ShdslInventoryTable OBJECT-TYPE
			SYNTAX SEQUENCE OF Shehdsl2ShdslInventoryEntry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"This table supports retrieval of unit inventory information
				available via the EOC from units in a HDSL2/SHDSL line.
				
				Entries in this table are dynamically created during the
				line discovery process.  The life cycle for these entries
				is as follows:
				
				   - xtu discovers a device, either a far-end xtu or an xru
				   - an inventory table entry is created for the device
				   - the line goes down for whatever reason
				   - inventory table entries for unreachable devices are
				     destroyed.
				
				As these entries are created/destroyed dynamically, they
				are NOT persistent."
			::= { shehdsl2ShdslMibObjects 4 }

		
--   1.3.6.1.4.1.231.7.1.2.2.1.12.1.2.7.4.1
		shehdsl2ShdslInventoryEntry OBJECT-TYPE
			SYNTAX Shehdsl2ShdslInventoryEntry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"An entry in the hdsl2ShdslInventoryTable.  Each entry
				represents inventory information for a single unit in a
				HDSL2/SHDSL line.  It is indexed by the ifIndex of the
				HDSL2/SHDSL line and the Hdsl2ShdslUnitId of the
				associated unit."
			INDEX { ifIndex, shehdsl2ShdslInvIndex }
			::= { shehdsl2ShdslInventoryTable 1 }

		
		Shehdsl2ShdslInventoryEntry ::=
			SEQUENCE { 
				shehdsl2ShdslInvIndex
					SheHdsl2ShdslUnitId,
				shehdsl2ShdslInvVendorID
					OCTET STRING,
				shehdsl2ShdslInvVendorModelNumber
					OCTET STRING,
				shehdsl2ShdslInvVendorSerialNumber
					OCTET STRING,
				shehdsl2ShdslInvVendorEOCSoftwareVersion
					Integer32,
				shehdsl2ShdslInvStandardVersion
					Integer32,
				shehdsl2ShdslInvVendorListNumber
					OCTET STRING,
				shehdsl2ShdslInvVendorIssueNumber
					OCTET STRING,
				shehdsl2ShdslInvVendorSoftwareVersion
					OCTET STRING,
				shehdsl2ShdslInvEquipmentCode
					OCTET STRING,
				shehdsl2ShdslInvVendorOther
					OCTET STRING,
				shehdsl2ShdslInvTransmissionModeCapability
					SheHdsl2ShdslTransmissionModeType
			 }

