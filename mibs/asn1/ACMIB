--
tasTstLineId OBJECT-TYPE
                SYNTAX Integer32
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "Test ID to be started for line testing."
                ::= { toneAnnceControl 43 }

tasTstLineIntfIndex OBJECT-TYPE
                SYNTAX IntfIndex
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "Interface index in SSPPCCC format of 
                        CE card on which this test is to be initiated."
                ::= { toneAnnceControl 44 }

tasTstLineAction  OBJECT-TYPE
                SYNTAX INTEGER {
                                tstStart(1),
                                tstStop(2),
                                tstAbort(3)
                               }
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "Test action to be performed."
                ::= { toneAnnceControl 45 }

tasMemIntfIndex       OBJECT-TYPE
                SYNTAX IntfIndex
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "Interface index in SSPPCCC format of TASM card on which
                             the memory  is to be initialised."
                ::= { toneAnnceControl 46 }

tasMemAction  OBJECT-TYPE
                SYNTAX INTEGER {
                                re-initialize(1)
                               }
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "Test action to be performed."
                ::= { toneAnnceControl 47 } 

-- OAM segment flows

toneAnnceIpAddress OBJECT-TYPE
                SYNTAX IpAddress
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "IP Address for announcement ftp server."
                ::= { toneAnnceControl 48 }
 
toneAnnceAccountName OBJECT-TYPE
                SYNTAX DisplayString (SIZE(20))
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "ftp account name for access to announcement 
                        ftp server."
                ::= { toneAnnceControl 49 }
 
toneAnnceAccountPassword OBJECT-TYPE
                SYNTAX DisplayString (SIZE(20))
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "ftp account password for access to announcement ftp
                             server."
                ::= { toneAnnceControl 50 }
 
toneAnnceSourceFile OBJECT-TYPE
                SYNTAX DisplayString (SIZE(79))
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "announcement file with complete path on the ftp 
                        server "
                ::= { toneAnnceControl 51 }

toneAnnceDestinationFile OBJECT-TYPE
                SYNTAX DisplayString (SIZE(12))
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "The announcementSourceFile will be renamed with
                        MS DOS file format and placed in the /scsi/announce 
                        directory on the disk."
                ::= { toneAnnceControl 52 }
 
toneAnnceFtpStatus OBJECT-TYPE
                SYNTAX INTEGER {
                                noActivity(1),
                                working(2),
                                doneSuccessfully(3),
                                doneWithError(4),
                                aborted(5)
                               } 
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "Current status of the ftp announcement download.
                             Status of the software announcement.  noActivity 
                             is the initial start state from boot-up.  working
                             is set while ftp'ing the announcement.  
                             The final state is one of doneSuccessfully, 
                             doneWithError, or aborted, all self-explanatory.  
                             It will stay in this state until re-boot or 
                             starting an announcement.  If the state is 
                             doneWithError then announcementSwErrorStatus will 
                             be set to reflect the type of error."
                ::= { toneAnnceControl 53 }

toneAnnceFtpErrorStatus OBJECT-TYPE
                SYNTAX INTEGER {
                                none(1),
                                userAbort(2),
                                invalidIpAddress(3),
                                invalidAccountName(4),
                                invalidAccountPassword(5),
                                invalidSourceFile(6),
                                invalidDstFileNameFormat(7),
                                dstFileNameAlreadyExist(8),
                                tooManyAnnouncementNow(9),
                                diskSpaceUsageExceed(10),
                                unableToOpenFile(11),
                                unableToMakeFtpConnection(12),
                                unableToWriteFile(13),
                                unableToCompleteFtp(14),
                                unableToSpawnTask(15),
                                unableToOpenScsiAnncDir(16),
                                statFails(17)
                               } 
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "The result of the finish of the ftp software
                             announcement download.
                             Set to a value 3-21 if announcementSwCopyStatus is
                             doneWithError.  Set to none if 
                             announcementFtpCopyStatus
                             is doneSuccessfully.  Set to userAbort if
                             announcementSwCopyStatus is aborted."
                DEFVAL { none }
                ::= { toneAnnceControl 54 }

toneAnnceFtpAction    OBJECT-TYPE
                SYNTAX INTEGER {
                                start(1),
                                abort(2)
                               } 
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "start ftp announcement files.  When 
                        announcementFtpCopyStatus is working, the start
                        request will be ignored.  Abort will have effect
                        only when announcementFtpCopystatus is working."
                ::= { toneAnnceControl 55 }



-- ATM Trunking IWF Group
 
atmTrunkingIWFBaseGroup   OBJECT IDENTIFIER ::= { atmTrunkingMIBObjects 1 }
 
atmTrunkingIWFIdentifier OBJECT-TYPE
        SYNTAX Integer32
        MAX-ACCESS read-write
        STATUS current
        DESCRIPTION "IWF ID for the local endpoint"
        DEFVAL { 0 }
        ::= { atmTrunkingIWFBaseGroup 1 }
 
atmTrunkingIWFLocalAddress OBJECT-TYPE
        SYNTAX OCTET STRING (SIZE(20))
        MAX-ACCESS read-write
        STATUS current
        DESCRIPTION "IWF ATM address for the local endpoint"
        ::= { atmTrunkingIWFBaseGroup 2 }
 
atmTrunkingIWFMaxRemoteIWF OBJECT-TYPE
        SYNTAX Integer32
        MAX-ACCESS read-write
        STATUS current
        DESCRIPTION "Maximum remote IWF that can connect to this endpoint"
        DEFVAL { 100 }
        ::= { atmTrunkingIWFBaseGroup 3 }
 
atmTrunkingIWFAdminStatus OBJECT-TYPE
        SYNTAX INTEGER { up(1), down(2), delete(3) }
        MAX-ACCESS read-write
        STATUS current
        DESCRIPTION "Indicates whether the Local IWF is active or not"
        ::= { atmTrunkingIWFBaseGroup 4 }
 
 
 
atmTrunkingRemoteIWFTable OBJECT-TYPE
        SYNTAX SEQUENCE OF AtmTrunkingRemoteIWFEntry
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION "Remote IWF table"
        ::= { atmTrunkingMIBObjects 2 }
 
atmTrunkingRemoteIWFEntry OBJECT-TYPE
        SYNTAX AtmTrunkingRemoteIWFEntry
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION "Entry in atmTrunkingRemoteIWFTable"
        INDEX { atmTrunkingRemoteIWFIdentifier }
        ::= { atmTrunkingRemoteIWFTable 1 }
 
AtmTrunkingRemoteIWFEntry ::=
        SEQUENCE {
                atmTrunkingRemoteIWFIdentifier Integer32,
                atmTrunkingRemoteIWFAddress OCTET STRING,
                atmTrunkingRemoteIWFTrnkMode INTEGER,
                atmTrunkingRemoteIWFAdminStatus INTEGER,
                atmTrunkingRemoteIWFRowStatus RowStatus
                }
 
atmTrunkingRemoteIWFIdentifier OBJECT-TYPE
        SYNTAX Integer32
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION "IWF ID for the remote IWF"
        ::= { atmTrunkingRemoteIWFEntry 1 }
 
atmTrunkingRemoteIWFAddress OBJECT-TYPE
        SYNTAX OCTET STRING (SIZE(20))
        MAX-ACCESS read-create
        STATUS current
        DESCRIPTION "IWF ATM address for the remote IWF"
        ::= { atmTrunkingRemoteIWFEntry 2 }

atmTrunkingRemoteIWFTrnkMode OBJECT-TYPE
        SYNTAX INTEGER { switched(1), non-switched(2) }
        MAX-ACCESS read-create
        STATUS current
        DESCRIPTION "Maximum remote IWF that can connect to this endpoint"
        DEFVAL { switched }
        ::= { atmTrunkingRemoteIWFEntry 3 }
 
atmTrunkingRemoteIWFAdminStatus OBJECT-TYPE
        SYNTAX INTEGER { up(1), down(2) }
        MAX-ACCESS read-create
        STATUS current
        DESCRIPTION "Indicates whether the Local IWF is active or not"
        ::= { atmTrunkingRemoteIWFEntry 4 }
 
atmTrunkingRemoteIWFRowStatus OBJECT-TYPE
        SYNTAX RowStatus
        MAX-ACCESS read-create
        STATUS current
        DESCRIPTION "Maximum remote IWF that can connect to this endpoint"
        ::= { atmTrunkingRemoteIWFEntry 5 }
 
 
 
atmTrunkingSigVccTable OBJECT-TYPE
        SYNTAX SEQUENCE OF AtmTrunkingSigVccEntry
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION "Table of iwf signaling vcc configuration entities"
        ::= { atmTrunkingMIBObjects 3 }
 
atmTrunkingSigVccEntry OBJECT-TYPE
        SYNTAX AtmTrunkingSigVccEntry
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION "Entry in atmTrunkingSigVccTable"
        INDEX { atmTrunkingSigVccRmtIWFIdentifier,
                atmTrunkingSigVccIdentifier }
        ::= { atmTrunkingSigVccTable 1 }
 
 
AtmTrunkingSigVccEntry ::=
        SEQUENCE {
                atmTrunkingSigVccRmtIWFIdentifier Integer32,
                atmTrunkingSigVccIdentifier    INTEGER,
                atmTrunkingSigVccOutgoingIfIndex IntfIndex,
                atmTrunkingSigVccOutgoingVpi   INTEGER,
                atmTrunkingSigVccOutgoingVci   INTEGER,
                atmTrunkingSigVccOwner         INTEGER,
                atmTrunkingSigVccInactiveTimer Integer32,
                atmTrunkingSigVccType          INTEGER,
                atmTrunkingSigVccSignaling     INTEGER,
                atmTrunkingSigVccFwPcr01       Integer32,
                atmTrunkingSigVccBwPcr01       Integer32,
                atmTrunkingSigVccFwScr01       Integer32,
                atmTrunkingSigVccBwScr01       Integer32,
                atmTrunkingSigVccFwMbs01       Integer32,
                atmTrunkingSigVccBwMbs01       Integer32,
                atmTrunkingSigVccMaxAal2Vcc    Integer32,
                atmTrunkingSigVccActiveAal2Vcc Integer32,
                atmTrunkingSigVccOperStatus    Integer32,
                atmTrunkingSigVccRowStatus     RowStatus,
                atmTrunkingSigVccViB           INTEGER,
                atmTrunkingSigVccVUNIB         INTEGER,
                atmTrunkingSigVccRetryTimer    Integer32,
                atmTrunkingSigVccRetryLimit    Integer32,
                atmTrunkingSigVccRestart       INTEGER,
                atmTrunkingSigVccPriority      INTEGER,
                atmTrunkingSigVccRetryCounter  Integer32
                }
 
