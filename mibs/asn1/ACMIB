-- IP over ATM Group

ipAtmArpSrvConnType OBJECT-TYPE
                SYNTAX INTEGER  {
                                pvc(1),
                                svc(2),
                                none(3)
                                }
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "type of Arp Server connection"
                ::= { ipAtmGroup 1 }

ipAtmIpAddr        OBJECT-TYPE
                SYNTAX IpAddress
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "Local IP Address" 
                ::= { ipAtmGroup 2 }
                
ipAtmIpMask        OBJECT-TYPE
                SYNTAX IpAddress
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "Local IP Mask" 
                ::= { ipAtmGroup 3 }


atmArpTable        OBJECT-TYPE
                SYNTAX SEQUENCE OF AtmArpEntry
                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION "ARP Table"
                ::= { ipAtmGroup 4 }

atmArpEntry        OBJECT-TYPE
                SYNTAX AtmArpEntry
                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION "ARP Table entry"
                INDEX { atmArpIpAddress }
                ::= { atmArpTable 1 }

AtmArpEntry        ::= SEQUENCE {
                              atmArpIpAddress                IpAddress,
                        atmArpAtmAddress        OCTET STRING,
                        atmArpAddrType                INTEGER,
                        atmArpEntryState        INTEGER,
                        atmArpEntryRowStatus        RowStatus
                        }

atmArpIpAddress        OBJECT-TYPE
                SYNTAX IpAddress
                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION "IP address of the IP host described in APR table
                             entry."
                ::= { atmArpEntry 1 }

atmArpAtmAddress OBJECT-TYPE
                SYNTAX OCTET STRING (SIZE(20))
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "ATM address of the IP host."
                ::= { atmArpEntry 2 }

atmArpAddrType        OBJECT-TYPE
                SYNTAX INTEGER {
                                nsap(1),
                                e164(2),
                                e164nsap(3)
                               }
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "Type of ATM address value of atmArp address."
                DEFVAL { nsap }
                ::= { atmArpEntry 3 }

atmArpEntryState OBJECT-TYPE
                SYNTAX INTEGER {
                                valid(1),
                                invalid(2),
                                unconnected(3),
                                permanent(4),
                                pendingAtmAddr(5),
                                pendingIpAddr(6),
                                pendingConnect(7)
                               }
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "State of this entry."
                DEFVAL { valid }
                ::= { atmArpEntry 4 }

atmArpEntryRowStatus OBJECT-TYPE
                SYNTAX RowStatus
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "RowStatus of this entry."
                ::= { atmArpEntry 5 }


appIpRouteTable OBJECT-TYPE
              SYNTAX  SEQUENCE OF AppIpRouteEntry
              MAX-ACCESS  not-accessible
              STATUS  current
              DESCRIPTION "Application IP Routing table."
              ::= { ipAtmGroup  5 }

appIpRouteEntry OBJECT-TYPE
              SYNTAX  AppIpRouteEntry
              MAX-ACCESS  not-accessible
              STATUS  current
              DESCRIPTION "A route to a particular destination."
              INDEX   { appIpRouteDest, appIpRouteMask, appIpRouteMetric1 }
              ::= { appIpRouteTable 1 }

AppIpRouteEntry ::=
              SEQUENCE {
                  appIpRouteDest
                      IpAddress,
                  appIpRouteMask
                      IpAddress,
                  appIpRouteMetric1
                      INTEGER,
                  appIpRouteNextHop
                      IpAddress,
                  appIpRouteState
                     INTEGER
              }

appIpRouteDest OBJECT-TYPE
              SYNTAX  IpAddress
              MAX-ACCESS  not-accessible
              STATUS  current
              DESCRIPTION
                      "The destination IP address of this route.  An
                      entry with a value of 0.0.0.0 is not valid
                      route. "
              ::= { appIpRouteEntry 1 }

appIpRouteMask OBJECT-TYPE
              SYNTAX  IpAddress
              MAX-ACCESS not-accessible
              STATUS  current
              DESCRIPTION
                      "Mask for Ip route, usefull only for SENS stack 
                       VxWorks"
              ::= { appIpRouteEntry 2 }


appIpRouteMetric1 OBJECT-TYPE
              SYNTAX  INTEGER (1..255)
              MAX-ACCESS not-accessible
              STATUS  current
              DESCRIPTION
                      "The primary routing metric for this route.  The
                      semantics of this metric are determined by the
                      routing-protocol specified in the routes
                      appIpRouteProto value.  If this metric is not used,
                      its value should be set to 1."
              ::= { appIpRouteEntry 3 }

appIpRouteNextHop OBJECT-TYPE
              SYNTAX  IpAddress
              MAX-ACCESS  read-write
              STATUS  current
              DESCRIPTION
                      "The IP address of the next hop of this route.
                      (In the case of a route bound to an interface
                      which is realized via a broadcast media, the value
                      of this field is the agent's IP address on that
                      interface.)"
              ::= { appIpRouteEntry 4 }

appIpRouteState OBJECT-TYPE
                SYNTAX INTEGER {
                                invalid(0),
                                inuse(1),
                                notinuse(2),
                                error(3)
                               }
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "Current state of route"
                ::= { appIpRouteEntry  5 }

-- Iisp Routing Group iispRouting

iispCbrRoutTable OBJECT-TYPE
                SYNTAX SEQUENCE OF IispCbrRoutEntry
                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION "IISP CBR Routing Table"
                ::= { iispRouting 1 }

iispCbrRoutEntry OBJECT-TYPE
                SYNTAX IispCbrRoutEntry
                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION "IISP CBR Table Entry"
                INDEX { iispCbrRoutLength,
                        iispCbrRoutNsapAddr }
                ::= { iispCbrRoutTable 1 }

IispCbrRoutEntry ::= SEQUENCE {
                              iispCbrRoutLength             Integer32,
                              iispCbrRoutNsapAddr        OCTET STRING,
                              iispCbrRoutIfIndex        IntfIndex,
                              iispCbrRoutRowStatus        RowStatus
                              }

iispCbrRoutLength OBJECT-TYPE
               SYNTAX Integer32
               MAX-ACCESS not-accessible
               STATUS current
               DESCRIPTION "length in bits"
               ::= { iispCbrRoutEntry 1 }

iispCbrRoutNsapAddr OBJECT-TYPE
               SYNTAX OCTET STRING (SIZE(1..20))
               MAX-ACCESS not-accessible
               STATUS current
               DESCRIPTION "nsap address"
               ::= { iispCbrRoutEntry 2 }

iispCbrRoutIfIndex OBJECT-TYPE
               SYNTAX IntfIndex
               MAX-ACCESS read-write
               STATUS current
               DESCRIPTION "Interface index - SSPPCCC"
               ::= { iispCbrRoutEntry 3 }

iispCbrRoutRowStatus OBJECT-TYPE
              SYNTAX RowStatus
              MAX-ACCESS read-write
              STATUS current
              DESCRIPTION "row status"
              ::= { iispCbrRoutEntry 4 }


iispVbrRoutTable OBJECT-TYPE
             SYNTAX SEQUENCE OF IispVbrRoutEntry
             MAX-ACCESS not-accessible
             STATUS current
             DESCRIPTION "IISP CBR Routing Table"
             ::= { iispRouting 2 }

