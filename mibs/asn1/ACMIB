-- connectionEvents

-- Request Fail Notify

atmPvcVccReqFailNotify  NOTIFICATION-TYPE
     OBJECTS {  atmPvcVccIfA, atmPvcVccVpiA, atmPvcVccVciA,
                atmPvcVccIfB, atmPvcVccVpiB, atmPvcVccVciB,
                 pvcFailureReasonCode} 
     STATUS  current
     DESCRIPTION "Response : The PVC VCC connection request between two atm 
                  interfaces failed."
        ::= { connectionEvents 1 }

atmPvcVpcReqFailNotify  NOTIFICATION-TYPE
     OBJECTS {  atmPvcVpcIfA,  atmPvcVpcVpiA, 
                atmPvcVpcIfB, atmPvcVpcVpiB,
                 pvcFailureReasonCode} 
     STATUS  current
     DESCRIPTION "Response : The PVC VPC connection request between two atm 
                  interfaces failed."
        ::= { connectionEvents 2 }

ipAtmAppPvcVccReqFailNotify  NOTIFICATION-TYPE
     OBJECTS { ipAtmAppPvcVccDestAddrA, ipAtmAppPvcVccSubnetMaskA, 
               ipAtmAppPvcVccIfB, ipAtmAppPvcVccVpiB,
               ipAtmAppPvcVccVciB, pvcFailureReasonCode }
     STATUS current
     DESCRIPTION "Response : The PVC VCC connection request between in-band 
                  management and an atm interface has failed."
        ::= { connectionEvents 3 }

cirEmAtmPvcVccReqFailNotify  NOTIFICATION-TYPE
     OBJECTS { cirEmAtmPvcVccIfA, cirEmAtmPvcVccIfB, 
               cirEmAtmPvcVccVpiB, cirEmAtmPvcVccVciB,
               pvcFailureReasonCode }
     STATUS current
     DESCRIPTION "Response : The PVC VCC connection request between a circuit 
                  emulation interface and an atm interface has failed."
        ::= { connectionEvents 4 }

vbrAtmPvcVccReqFailNotify NOTIFICATION-TYPE
     OBJECTS { vbrAtmPvcVccIfA,  vbrAtmPvcVccIfB,
               vbrAtmPvcVccVpiB, vbrAtmPvcVccVciB, 
