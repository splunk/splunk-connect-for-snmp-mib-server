     OBJECTS { aal2TrunkConfigIf, aal2TrunkConfigVpi,
               aal2TrunkConfigVci }
     STATUS current
     DESCRIPTION "Response : The PVC VCC AAL2 Trunk has been deleted"
        ::= { connectionEvents 74 }

aal2TrunkConfigReqFailNotify  NOTIFICATION-TYPE
     OBJECTS { aal2TrunkConfigIf, aal2TrunkConfigVpi,
               aal2TrunkConfigVci, pvcFailureReasonCode }
     STATUS current
     DESCRIPTION "Response : The PVC VCC aal2 trunk could not be setup"
        ::= { connectionEvents 75 }

gr303Aal2PvcConfigReqFailNotify  NOTIFICATION-TYPE
     OBJECTS { gr303Aal2PvcVccGr303IgId,
                             gr303Aal2PvcVccGr303CallRef,
                             gr303Aal2PvcVccIfB,
                             gr303Aal2PvcVccVpiB,
                             gr303Aal2PvcVccVciB,
                             gr303Aal2PvcVccAal2Cid
               }
     STATUS current
     DESCRIPTION "Response : The GR303 PVC VCC on aal2 trunk could not be setup"
        ::= { connectionEvents 76 }

gr303Aal2PvcSetUpNotify  NOTIFICATION-TYPE
     OBJECTS { gr303Aal2PvcVccGr303IgId,
                             gr303Aal2PvcVccGr303CallRef,
                             gr303Aal2PvcVccIfB,
                             gr303Aal2PvcVccVpiB,
                             gr303Aal2PvcVccVciB,
                             gr303Aal2PvcVccAal2Cid
               }
     STATUS current
     DESCRIPTION "Response : Notification that a monitor connection between Gr303 call ref and
                  ATM AAL2 CID has been made successfully."
        ::= { connectionEvents 77 }

gr303Aal2PvcTearDownNotify  NOTIFICATION-TYPE
     OBJECTS { gr303Aal2PvcVccGr303IgId,
                             gr303Aal2PvcVccGr303CallRef,
                             gr303Aal2PvcVccIfB,
                             gr303Aal2PvcVccVpiB,
                             gr303Aal2PvcVccVciB,
                             gr303Aal2PvcVccAal2Cid
               }
     STATUS current
     DESCRIPTION "Response : Notification that a monitor connection between Gr303 call ref and
                  ATM AAL2 CID has been deleted"
        ::= { connectionEvents 78 }

dhpvcChangeNotify  NOTIFICATION-TYPE
     OBJECTS {
                interfaceIndex,
                atmVpi,
                atmVci,
                pvcFailureReasonCode
             }
     STATUS current
     DESCRIPTION "This will indicate the changes in a dual home pvc. 
                  This will have 2 indications. One is when there is a switchover
                  which indicate the last state and the new one (eg Prim To Bkup)
                  Secondly there will also be an indication when there is a
                  state change which will indicate the new state only 
                  (eg Primary is Active and in No Fail Condition and Backup
                   is Passive and in Fail Condition). In case of state change
                  the traffic path is not changed but it indicates  the
                  state of the passive path. "
        ::= { connectionEvents 79 }

