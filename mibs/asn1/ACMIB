                             0:  all are clean
                             1:  console.def
                             2:  ecd.def
                             4:  ssid.def
                             8:  cnctn.db
                            16:  iisp.cbr
                            32:  iisp.vbr
                            64:  spvc.db
               128:  vipr.cfg
               256:  fwc.def
               512:  vi.db
              1024:  iproute.db
              2048:  atmvuni.db"
                ::= { sysControl 21 }

