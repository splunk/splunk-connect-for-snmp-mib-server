                                   localworkingDoNotRevert(17),
                                   localworkingManualSw(24),
                                   localworkingSD(26),
                                localworkingSF(28),
                                localworkingForced(30),
                                remoteprotectDoNotRevert(33),
                                remoteprotectManualSw(40),
                                remoteprotectSD(42),
                                remoteprotectSF(44),
                                remoteprotectForced(46),
                                remoteprotectLockOut(47),
                                   noRequest(48),
