                ::= { viprVpnStatsEntry 25 }

viprVpnStatsIcmpInTimeStampsReps OBJECT-TYPE
                SYNTAX Integer32
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "received time stamp replies"
                ::= { viprVpnStatsEntry 26 }


--  Interface level statistics that will be based on per CID.

viprStatsTable OBJECT-TYPE
                SYNTAX SEQUENCE OF ViprStatsEntry
                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION " The table which holds viprStatsEntry data."
                ::= { intfStatistics 10 }
                        
viprStatsEntry OBJECT-TYPE
                SYNTAX ViprStatsEntry
                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION " A table entry for VPN related statistics 
                              information."
                INDEX { viprSlot, viprRouterId,  viprIntfId, viprSubChnlId, 
                        viprSubChnlIntfIndex, viprSubChnlVpiDlci, 
                        viprSubChnlVci, viprSubChnlTrafficType }
                ::= { viprStatsTable 1 }

ViprStatsEntry ::= SEQUENCE {
                viprStatsReset                  INTEGER,
                viprStatsPacketsRxHi            Integer32,
                viprStatsPacketsRxLo            Integer32,
                viprStatsPacketsTxHi            Integer32,
                viprStatsPacketsTxLo            Integer32,
                viprStatsMismatchCrcHi          Integer32,
                viprStatsMismatchCrcLo          Integer32,
                viprStatsInsufficientPcktsHi    Integer32,
                viprStatsInsufficientPcktsLo    Integer32,
                viprStatsExceedAal5CellsHi      Integer32,
                viprStatsExceedAal5CellsLo      Integer32,
                viprStatsPrototypeErrorsHi      Integer32,
                viprStatsPrototypeErrorsLo      Integer32,
                viprStatsArpProtoRxHi           Integer32,
                viprStatsArpProtoRxLo           Integer32,
                viprStatsIPInHdrErrorsHi        Integer32,
                viprStatsIPInHdrErrorsLo        Integer32,
                viprStatsIPInAddrErrorsHi       Integer32,
                viprStatsIPInAddrErrorsLo       Integer32
                 }

