                SYNTAX Integer32
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "Number of null cells sent out  on the channel -
                             Low Word"
                ::= { atmIfStatsEntry 7 }

atmIfStatsValidCellsOutHi OBJECT-TYPE
                SYNTAX Integer32
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "Number of valid cells sent out  on the channel -
                             High Word"
                ::= { atmIfStatsEntry 8 }

atmIfStatsValidCellsOutLo OBJECT-TYPE
                SYNTAX Integer32
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "Number of valid cells sent out  on the channel -
                             Low Word"
                ::= { atmIfStatsEntry 9 }

atmIfStatsTimer OBJECT-TYPE
                SYNTAX Integer32 (1..899)
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "Time, in seconds, since last reset."
                ::= { atmIfStatsEntry 10 }

