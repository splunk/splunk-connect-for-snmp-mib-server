virtualIntfStatsTable OBJECT-TYPE
        SYNTAX SEQUENCE OF VirtualIntfStatsEntry
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION "Table of Virtual Interfaces Configurations"
        ::= { intfStatistics 11 }

virtualIntfStatsEntry OBJECT-TYPE
        SYNTAX VirtualIntfStatsEntry
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION "Index is the Interface and the Virutal Interface"
        INDEX { virtualIntfStatsIf, virtualIntfStatsVi }
        ::= { virtualIntfStatsTable 1 }

VirtualIntfStatsEntry ::= SEQUENCE {
          virtualIntfStatsIf   IntfIndex,
        virtualIntfStatsVi   INTEGER,
        virtualIntfStatsEgrCellCount  Integer32,
        virtualIntfStatsTimer   Integer32,
        virtualIntfStatsReset   INTEGER
        }

virtualIntfStatsIf OBJECT-TYPE
          SYNTAX IntfIndex
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION "Interface index for side A of an ATM-to-ATM PVC VCC
                connection.  Number value is interpreted as an
                interface of the form, SSPPCCC, where SS is the 
                card slot, PP is the port number, and CCC is the 
                channel number."
        ::= { virtualIntfStatsEntry 1 }

virtualIntfStatsVi OBJECT-TYPE
          SYNTAX INTEGER (0..1024)
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION "VI value. Def Val is 1 because 0 cannot be 
                configured"
        ::= { virtualIntfStatsEntry 2 }

virtualIntfStatsEgrCellCount OBJECT-TYPE
          SYNTAX Integer32
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION " Egress Cell Count on VI "
        ::= { virtualIntfStatsEntry 3 }

virtualIntfStatsTimer OBJECT-TYPE
          SYNTAX Integer32 (1..899)
          MAX-ACCESS read-only
        STATUS current
        DESCRIPTION "Time, in seconds, since last reset."
        ::= { virtualIntfStatsEntry 4 }

virtualIntfStatsReset OBJECT-TYPE
          SYNTAX INTEGER {
                        reset(1)
                        }
          MAX-ACCESS read-write
          STATUS current
          DESCRIPTION "stats reset field."
        ::= { virtualIntfStatsEntry 5 }


-- FW download group, two tables

cardFwVerTable OBJECT-TYPE
                   SYNTAX SEQUENCE OF CardFwVerEntry
                   MAX-ACCESS not-accessible
                   STATUS current
                   DESCRIPTION "Table of card firware version status"
                   ::= {  fwDriverSup 1 }
 
cardFwVerEntry OBJECT-TYPE
                   SYNTAX CardFwVerEntry
                   MAX-ACCESS not-accessible
                   STATUS current
                  DESCRIPTION "Row definition for card firmware version status"
                   INDEX { cardFwVerSlot }
                   ::= { cardFwVerTable 1 }
 
CardFwVerEntry ::= SEQUENCE {
                   cardFwVerSlot            INTEGER,
                   cardFwVerType            Integer32,
                   cardFwVerCurrent            OCTET STRING,
                   cardFwVerCurrentIsDefault            Integer32,
                   cardFwVerNext            OCTET STRING,
                   cardFwVerNextIsDefault            INTEGER,
                   cardFwDnldStatus            INTEGER,
                   cardFwRetries            Integer32
   }

cardFwVerSlot  OBJECT-TYPE
                   SYNTAX INTEGER (1..24)
                MAX-ACCESS not-accessible
                   STATUS current
                   DESCRIPTION "card slot number"
                   ::= { cardFwVerEntry 1 }

cardFwVerType  OBJECT-TYPE
                   SYNTAX Integer32
                   MAX-ACCESS read-write
                   STATUS current
                   DESCRIPTION "Card type"
                   ::= { cardFwVerEntry 2 }

cardFwVerCurrent  OBJECT-TYPE
                   SYNTAX OCTET STRING (SIZE(13))
                   MAX-ACCESS read-only
                   STATUS current
                   DESCRIPTION "Card firmware version currently loaded on 
                                firmware"
                   ::= { cardFwVerEntry 3 }

cardFwVerCurrentIsDefault  OBJECT-TYPE
                   SYNTAX Integer32
                   MAX-ACCESS read-only
                   STATUS current
                   DESCRIPTION "curent driver is default"
                   ::= { cardFwVerEntry 4 }

cardFwVerNext  OBJECT-TYPE
                   SYNTAX OCTET STRING (SIZE(13))
                   MAX-ACCESS read-write
                   STATUS current
                   DESCRIPTION "Card firmware version that user wants to 
                             upgrade to.  The values are limited those 
                             supported by the release."
                   ::= { cardFwVerEntry 5 }

cardFwVerNextIsDefault  OBJECT-TYPE
                   SYNTAX INTEGER (0..1)
                   MAX-ACCESS read-write
                   STATUS current
                   DESCRIPTION "next driver is default"
                   ::= { cardFwVerEntry 6 }

cardFwDnldStatus  OBJECT-TYPE
                   SYNTAX INTEGER  {
                              failed(1),
                              retry(2),
                              progress(3),
                              done(4),
            wrongCardType(5),
            wrongCardSType(6),
            drvUnavailable(7),
            drvInvHdrChksum(8),
            drvInvRecChksum(9),
            reqRepTimeout(10),
            cmplRepTimeout(11),
            mapCorrupted(12),
            mapUnavailable(13),
            mapInvChksum(14),
            mapNameImprop(15),
            cfgCorrupted(16),
            cfgUnavailable(17),
            cfgInvChksum(18),
            removeCardBeforeFinish(19),
            ioUnsupported(20)
      		}
   		MAX-ACCESS read-only
   		STATUS current
   		DESCRIPTION "firmware download procedure status"
   		DEFVAL { done }
   		::= { cardFwVerEntry 7 }

cardFwRetries  OBJECT-TYPE
                   SYNTAX Integer32
                   MAX-ACCESS read-only
                   STATUS current
                   DESCRIPTION "Number of times firmware download 
                             procedure failed consequently."
                   ::= { cardFwVerEntry 8 }

fwDriverTable OBJECT-TYPE
                   SYNTAX SEQUENCE OF FwDriverEntry
                   MAX-ACCESS not-accessible
                   STATUS current
                   DESCRIPTION "Table of available firmware drivers"
                   ::= {  fwDriverSup 2 }
 
fwDriverEntry OBJECT-TYPE
                SYNTAX FwDriverEntry
                   MAX-ACCESS not-accessible
                   STATUS current
                   DESCRIPTION "Row definition for available firmware drivers"
                   INDEX { fwSlotId, fwDriverType, fwIsDefault, fwDriverName }
                   ::= { fwDriverTable 1 }
 
FwDriverEntry ::= SEQUENCE {
                fwSlotId                Integer32,
                fwDriverType            Integer32,
                            fwIsDefault                    Integer32,
                fwDriverName            OCTET STRING,
                fwDriverFrn             Integer32,
                fwDriverSize            Integer32,
                fwDriverChecksum        Integer32
                }
 
fwSlotId OBJECT-TYPE
                SYNTAX Integer32
                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION "driver slot id"
                ::= { fwDriverEntry 1 }

fwDriverType OBJECT-TYPE
                SYNTAX Integer32
                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION "firmware driver type - definition 
                             consistent with cardType."
                ::= { fwDriverEntry 2 }

fwIsDefault OBJECT-TYPE
                SYNTAX Integer32
                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION "firmware driver wether default or not"
                ::= { fwDriverEntry 3 }

fwDriverName OBJECT-TYPE
                SYNTAX OCTET STRING (SIZE(13))
                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION "firmware driver file name"
                ::= { fwDriverEntry 4 }
 
fwDriverFrn  OBJECT-TYPE
                SYNTAX Integer32
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "Firmware Release Number of firmware driver"
                ::= { fwDriverEntry 5 }

fwDriverSize  OBJECT-TYPE
                SYNTAX Integer32
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "file size of firmware driver"
                ::= { fwDriverEntry 6 }

fwDriverChecksum  OBJECT-TYPE
                SYNTAX Integer32
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "checksum of firmware driver file"
                ::= { fwDriverEntry 7 }
