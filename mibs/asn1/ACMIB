            SYNTAX IispVbrRoutEntry
            MAX-ACCESS not-accessible
            STATUS current
            DESCRIPTION "IISP CBR Table Entry"
            INDEX { iispVbrRoutLength, iispVbrRoutNsapAddr }
            ::= { iispVbrRoutTable 1 }

IispVbrRoutEntry::= SEQUENCE {
                              iispVbrRoutLength             Integer32,
                              iispVbrRoutNsapAddr        OCTET STRING,
                              iispVbrRoutIfIndex        IntfIndex,
                              iispVbrRoutRowStatus        RowStatus
                             }

iispVbrRoutLength OBJECT-TYPE
                SYNTAX Integer32
                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION "length in bits"
                ::= { iispVbrRoutEntry 1 }

iispVbrRoutNsapAddr OBJECT-TYPE
                SYNTAX OCTET STRING (SIZE(1..20))
                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION "nsap address"
                ::= { iispVbrRoutEntry 2 }

iispVbrRoutIfIndex OBJECT-TYPE
                SYNTAX IntfIndex
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "Interface index - SSPPCCC"
                ::= { iispVbrRoutEntry 3 }

iispVbrRoutRowStatus OBJECT-TYPE
                SYNTAX RowStatus
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "row status"
                ::= { iispVbrRoutEntry 4 }



-- the following two tables are almost same as two iisp mib 
-- table above. They only diff at the number of the INDEX keys

iispCbrRouteTable  OBJECT-TYPE
                SYNTAX SEQUENCE OF IispCbrRouteEntry
                  MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION "IISP CBR Routing Table"
                ::= { iispRouting 3 }

iispCbrRouteEntry  OBJECT-TYPE
                SYNTAX IispCbrRouteEntry
                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION "IISP CBR Route Entry"
                INDEX { iispCbrRouteLength,
                        iispCbrRouteNsapAddr, iispCbrRouteIfIndex

                        ,iispCbrRouteIfVuniId 

                        }
                  ::= { iispCbrRouteTable 1 }

IispCbrRouteEntry ::= SEQUENCE {
                iispCbrRouteLength        Integer32,
                iispCbrRouteNsapAddr      OCTET STRING,
                iispCbrRouteIfIndex        IntfIndex,
                iispCbrRouteRowStatus  RowStatus

        ,iispCbrRouteIfVuniId    INTEGER,



                iispCbrRouteDest        Integer32,
                iispCbrRouteType        Integer32
                }

iispCbrRouteLength   OBJECT-TYPE
                SYNTAX Integer32
                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION "length in bits"
                ::= { iispCbrRouteEntry 1 }

iispCbrRouteNsapAddr  OBJECT-TYPE
                SYNTAX OCTET STRING (SIZE(1..20))
                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION "nsap address"
                ::= { iispCbrRouteEntry 2 }

iispCbrRouteIfIndex  OBJECT-TYPE
                SYNTAX IntfIndex
                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION "Interface index - SSPPCCC"
                ::= { iispCbrRouteEntry 3 }

iispCbrRouteRowStatus OBJECT-TYPE
                SYNTAX RowStatus
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "row status"
                ::= { iispCbrRouteEntry 4 }


iispCbrRouteIfVuniId OBJECT-TYPE
                SYNTAX INTEGER (0..255)
                MAX-ACCESS not-accessible
                STATUS current
                  DESCRIPTION "Virtual UNI interface VUNI Id"
        DEFVAL { 0 }
                ::= { iispCbrRouteEntry 5 }


-- The following 2 objects are defined according to the definition in the
-- header file atmRoute.h under atmRoute/inc. For backward compatibility and
-- minimal code changes, the values have been retained in dito
 
iispCbrRouteType OBJECT-TYPE
                SYNTAX INTEGER {
                        drmRouteTypeAny (0),
                        drmRouteTypeAtm (1),
                        drmRouteTypeIisp (2),
                        drmRouteTypeIlmiUser (3),
                        drmRouteTypeIsdn (11),
                        drmRouteTypeCe (21),
                        drmRouteTypeSs7 (31),
                        drmRouteTypeFr (41),
                        drmRouteTypeCasTrunk (51),
                        drmRouteTypeEndPtAtm (132),
                        drmRouteTypeEndPtCe (133),
                        drmRouteTypeEndPtTe (134),
                        drmRouteTypeEndPtIsdn (135),
                        drmRouteTypeEndPtIp (136),
                        drmRouteTypeEndPtFr (137)
                }
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "Type of Route, Categorisation of an Endpoint
                             Route or an intermediate Route"
                DEFVAL { 0 }

                ::= { iispCbrRouteEntry 6 }



 
iispCbrRouteDest OBJECT-TYPE
                SYNTAX INTEGER {
                        drmRouteDestAny (0),
                        drmRouteDestAtm (1),
                        drmRouteDestIsdn (2),
                        drmRouteDestCe (3),
                        drmRouteDestSs7 (4),
                        drmRouteDestFr (5),
                        drmRouteDestAalIwf (6),
                        drmRouteDestEndPtAtm (15),
                        drmRouteDestEndPtCe (16),
                        drmRouteDestEndPtTe (17),
                        drmRouteDestEndPtIsdn (18),
                        drmRouteTypeEndPtIp (19),
                        drmRouteTypeEndPtFr (20),
                        drmRouteTypeEndPtCas (21)
                }
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "Type of Destination, Categorisation of an
                             Endpoint destination or an intermediate
                             destination"
                DEFVAL { 0 }

                ::= { iispCbrRouteEntry 7 }




iispVbrRouteTable  OBJECT-TYPE
                SYNTAX SEQUENCE OF IispVbrRouteEntry
                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION "IISP CBR Routing Table"
                ::= { iispRouting 4 }

iispVbrRouteEntry  OBJECT-TYPE
                SYNTAX IispVbrRouteEntry
                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION "IISP CBR Table Entry"
                INDEX { iispVbrRouteLength, iispVbrRouteNsapAddr,
                        iispVbrRouteIfIndex

                        , iispVbrRouteIfVuniId

                        }
                  ::= { iispVbrRouteTable 1 }

IispVbrRouteEntry ::= SEQUENCE {
                iispVbrRouteLength       Integer32,
                iispVbrRouteNsapAddr     OCTET STRING,
                iispVbrRouteIfIndex        IntfIndex,
                iispVbrRouteRowStatus     RowStatus

        ,iispVbrRouteIfVuniId    INTEGER,



                iispVbrRouteDest        Integer32,
                iispVbrRouteType        Integer32
                }

iispVbrRouteLength  OBJECT-TYPE
                SYNTAX Integer32
                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION "length in bits"
                ::= { iispVbrRouteEntry 1 }

iispVbrRouteNsapAddr  OBJECT-TYPE
                SYNTAX OCTET STRING (SIZE(1..20))
                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION "nsap address"
                ::= { iispVbrRouteEntry 2 }

iispVbrRouteIfIndex OBJECT-TYPE
                SYNTAX IntfIndex
                MAX-ACCESS not-accessible
                STATUS current
                  DESCRIPTION "In most cases, it is the I/O Interface index - 
                             SSPPCCC. However, in case of ATM Trunking's 
                             Narrowband routing and Broadband routing, this
                             is either the CAS trunk group ID, or the remote 
                             IWF ID, which are 1 to 3 digits decimal numbers"
                ::= { iispVbrRouteEntry 3 }

iispVbrRouteRowStatus OBJECT-TYPE
                SYNTAX RowStatus
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "row status"
