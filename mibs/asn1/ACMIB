        ::= { cirAtmSpvcVccEntry 49 }


cirAtmSpvcVccSarTypeB OBJECT-TYPE
        SYNTAX INTEGER  {
                aal1(1),
                aal2(2)
                }
        MAX-ACCESS read-write
        STATUS current
        DESCRIPTION "This field will only be used when selecting voice
                 compression, in other cases it will be set to
                 aal1."
        DEFVAL { aal1 }
        ::= { cirAtmSpvcVccEntry 50 }

