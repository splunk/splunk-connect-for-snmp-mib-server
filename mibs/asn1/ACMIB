cirAtmSpvcVccFlow OBJECT-TYPE
        SYNTAX INTEGER  {
                 duplex(1),
                 simplexA2B(2),
                 simplexB2A(3),
                 pointToMultipointA2B(4),
                 pointToMultipointB2A(5)
                }
        MAX-ACCESS read-write
        STATUS current
        DESCRIPTION "Flow Type, pointToMultipointA2B and 
         pointToMultipointB2A are for future support."
        DEFVAL { duplex }
        ::= { cirAtmSpvcVccEntry 15 }

cirAtmSpvcVccServiceType OBJECT-TYPE
        SYNTAX INTEGER {
                cbr-2(2),
                cbr-3(3),
                cbr-4(4)
           }
        MAX-ACCESS read-write
        STATUS current
        DESCRIPTION "service type"
        ::= { cirAtmSpvcVccEntry 16 }

cirAtmSpvcVccConnType    OBJECT-TYPE
                SYNTAX INTEGER{
                                activeSvc(1),
                                passiveSvc(2)
                                }
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "The type of connection: activeSvc - attempt calls,
                whenever not established;
                             passiveSvc - accepts calls"
                ::= { cirAtmSpvcVccEntry 17 }

cirAtmSpvcVccOperStatus OBJECT-TYPE
                SYNTAX INTEGER {
                                other(1),
                                establishmentInProgress(2),
                                connected(3),
                                retriesExhausted(4),
                                noAddressSupplied(5),
                                lowerLayerDown(6),
                                waitingForCall(7),
                                waitingForRetryTimer(8)
                                }
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "other - none of the types specified below;
                             establishmentInProgress - connection is not 
                operational, but call attempts are ongoing;
                             connected - connection is currently operational;
                             retriesExhausted - retry limit has been reached 
                and call attempts have ceased;
                             noAddressSupplied - no remote address has been 
                configured, so no call attempts are initiated;
                             lowerLayerDown - SPVC handler is not operational."
                ::= { cirAtmSpvcVccEntry 18 }

cirAtmSpvcVccRowStatus  OBJECT-TYPE
                SYNTAX     RowStatus
                MAX-ACCESS read-write
                STATUS     current
                DESCRIPTION "Used to create and delete a soft PVCC.  When this 
         object is set to 'active' an attempt is made to 
         set up the soft PVCC.  When this object has the 
         value 'active' and is set to another value, any 
         set-up or connection in-progess is released."
                ::= { cirAtmSpvcVccEntry 19 }

cirAtmSpvcVccViB      OBJECT-TYPE
        SYNTAX INTEGER (0..1024)
        MAX-ACCESS read-write
        STATUS current
        DESCRIPTION "VI to associate the Egress part of 
                the B2A connection ."
        DEFVAL { 0 } 
        ::= { cirAtmSpvcVccEntry 20 }


cirAtmSpvcVccRetryInterval OBJECT-TYPE
        SYNTAX INTEGER (0..3600)
        UNITS      "seconds"
        MAX-ACCESS read-write
        STATUS current
        DESCRIPTION "The time to wait before attempting to establish 
         the SVC after previous failed call attempt. 
         0 indicates  retries will be attempted immediately."
        DEFVAL { 10 }
        ::= { cirAtmSpvcVccEntry 21 }

cirAtmSpvcVccRetryLimit OBJECT-TYPE
        SYNTAX INTEGER (0..65535)
        MAX-ACCESS read-write
        STATUS current
        DESCRIPTION "A maximum limit on how many retries. 
         0 indicates no limit."
        DEFVAL { 0 }
        ::= { cirAtmSpvcVccEntry 22 }

cirAtmSpvcVccRestart OBJECT-TYPE
        SYNTAX INTEGER {
                restart(1),
                noop(2)
                }
        MAX-ACCESS read-write
        STATUS current
        DESCRIPTION "restart, the active SVC is released if necessary 
         and a new setup procedure is begun; or noop"
        ::= { cirAtmSpvcVccEntry 23 }

cirAtmSpvcVccRetryFailure OBJECT-TYPE
        SYNTAX Integer32 
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION "Indicates how many attempts to establish the 
         connection have failed.  It is reset whenever a 
         connection is successfully established or the SVC 
         is restarted."
        ::= { cirAtmSpvcVccEntry 24 }

cirAtmSpvcVccReleaseCause OBJECT-TYPE
        SYNTAX INTEGER(1..127) 
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION "CAUSE field value of the last RELEASE message 
         received for this SVC."
        ::= { cirAtmSpvcVccEntry 25 }

cirAtmSpvcVccReleaseDiag OBJECT-TYPE
        SYNTAX OCTET STRING (SIZE(0..8)) 
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION "DIAGNOSTICS field value of the last RELEASE 
         message received for this SVC."
        ::= { cirAtmSpvcVccEntry 26 }


cirAtmSpvcVccRelease OBJECT-TYPE
        SYNTAX INTEGER {
                release(1),
                noop(2)
                }
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION "Release the connnection established on the endpoint
         and if the endpoint is Active, do not retry."
        ::= { cirAtmSpvcVccEntry 27 }


