vbrAtmSpvcTable OBJECT-TYPE
        SYNTAX SEQUENCE OF VbrAtmSpvcEntry
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION "Table of SPVC atm virtual channel connections"
        ::= { connectionConfig 35 }

vbrAtmSpvcEntry OBJECT-TYPE
        SYNTAX VbrAtmSpvcEntry
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION "Row definition of atm SPVC VCC connections"
        INDEX { spvcAddrIfA, vbrAtmSpvcRemoteVbrPortAddr }
        ::= { vbrAtmSpvcTable 1 }

VbrAtmSpvcEntry ::= SEQUENCE {
        vbrAtmSpvcRemoteVbrPortAddr              OCTET STRING,
        vbrAtmSpvcIfB                            IntfIndex,
        vbrAtmSpvcVpiB                           INTEGER,
        vbrAtmSpvcVciB                           INTEGER,
        vbrAtmSpvcServiceType                    INTEGER,
        vbrAtmSpvcPeakCellRateA2B                Integer32,
        vbrAtmSpvcSusCellRateA2B                 Integer32,
        vbrAtmSpvcMaxBurstSizeA2B                Integer32,
        vbrAtmSpvcPeakCellRateB2A                Integer32,
        vbrAtmSpvcSusCellRateB2A                 Integer32,
        vbrAtmSpvcMaxBurstSizeB2A                Integer32,
        vbrAtmSpvcFlow                           INTEGER,
        vbrAtmSpvcConnType                       INTEGER,
        vbrAtmSpvcOperStatus                     INTEGER,
        vbrAtmSpvcRowStatus                      RowStatus,
        vbrAtmSpvcViB                            INTEGER,
        vbrAtmSpvcRetryInterval                  INTEGER,
        vbrAtmSpvcRetryLimit                     INTEGER,
        vbrAtmSpvcRestart                        INTEGER,
        vbrAtmSpvcRetryFailure                   Integer32,
        vbrAtmSpvcReleaseCause                   INTEGER,
        vbrAtmSpvcReleaseDiag                    OCTET STRING,
        vbrAtmSpvcRelease                        INTEGER,
        vbrAtmSpvcStatsCellsEncodedHiA           Integer32,
        vbrAtmSpvcStatsCellsEncodedLoA           Integer32,
        vbrAtmSpvcStatsCellsDecodedHiA           Integer32,
        vbrAtmSpvcStatsCellsDecodedLoA           Integer32,
        vbrAtmSpvcStatsInCellCountHiB            Integer32,
        vbrAtmSpvcStatsInCellCountLoB            Integer32,
        vbrAtmSpvcStatsOutCellCountHiB           Integer32,
        vbrAtmSpvcStatsOutCellCountLoB           Integer32,
        vbrAtmSpvcStatsTimer                     Integer32,
        vbrAtmSpvcStatsOdometerCellsEncodedHiA   Integer32,
        vbrAtmSpvcStatsOdometerCellsEncodedLoA   Integer32,
        vbrAtmSpvcStatsOdometerCellsDecodedHiA   Integer32,
        vbrAtmSpvcStatsOdometerCellsDecodedLoA   Integer32,
        vbrAtmSpvcStatsInOdometerCellCountHiB    Integer32,
        vbrAtmSpvcStatsInOdometerCellCountLoB    Integer32,
        vbrAtmSpvcStatsOutOdometerCellCountHiB   Integer32,
        vbrAtmSpvcStatsOutOdometerCellCountLoB   Integer32,
        vbrAtmSpvcStatsOdometerTimer             Integer32,
        vbrAtmSpvcStatsOdometerReset             INTEGER,
        vbrAtmSpvcStatsCellsDroppedB             Integer32,
        vbrAtmSpvcStatsCellsTaggedB              Integer32

        ,vbrAtmSpvcVUNIB                         INTEGER,

        vbrAtmSpvcTrafficShapingA2B              INTEGER,
        vbrAtmSpvcPriority                    INTEGER
        }

vbrAtmSpvcRemoteVbrPortAddr OBJECT-TYPE
        SYNTAX OCTET STRING (SIZE(1..20))
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION "remote VBR port nsap address implemening IWF, not 
                             meaningful if passive type of connection"
        ::= { vbrAtmSpvcEntry 1 }

vbrAtmSpvcIfB OBJECT-TYPE
        SYNTAX IntfIndex
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION "Interface index for side B, the atm side, of a 
         variable bit rate-atm SPVC VCC connection.  Number 
         value is interpreted as an interface of the form, 
         SSPPCCC, where SS is the card slot, PP is the port 
         number, and CCC is the channel number."
        ::= { vbrAtmSpvcEntry 2 }

