        SYNTAX Integer32
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION "Maximum burst size - side B, in cells, 
                 this will reflect the frame relay parameters in
                 atm terms."
        DEFVAL { 1 }
        ::= { frAtmSpvcVccEntry 22 }

frAtmSpvcVccInterWorkingFunc OBJECT-TYPE
        SYNTAX INTEGER {

                frf8-Transl(1),
                frf8-Pass(2),
                frf5(3)
                   }
        MAX-ACCESS read-write
        STATUS current
        DESCRIPTION "InterWorking function."
        DEFVAL { frf8-Transl }
        ::= { frAtmSpvcVccEntry 23 }

frAtmSpvcVccConnType    OBJECT-TYPE
        SYNTAX INTEGER{
                activeSvc(1),
                passiveSvc(2)
                }
        MAX-ACCESS read-write
        STATUS current
        DESCRIPTION "The type of connection: activeSvc - attempt calls,
                 whenever not established; passiveSvc - accepts calls"
        ::= { frAtmSpvcVccEntry 24 }

frAtmSpvcVccOperStatus OBJECT-TYPE
        SYNTAX INTEGER {
                other(1),
                establishmentInProgress(2),
                connected(3),
                retriesExhausted(4),
                noAddressSupplied(5),
                lowerLayerDown(6),
                waitingForCall(7),
                waitingForRetryTimer(8)
                }
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION "other - none of the types specified below;
                 establishmentInProgress - connection is not
                        operational, but call attempts are ongoing;
                 connected - connection is currently operational;
                 retriesExhausted - retry limit has been reached
                        and call attempts have ceased;
                 noAddressSupplied - no remote address has been
                        configured, so no call attempts are initiated;
                 lowerLayerDown - SPVC handler is not operational."
        ::= { frAtmSpvcVccEntry 25 }

frAtmSpvcVccRowStatus OBJECT-TYPE
        SYNTAX RowStatus
        MAX-ACCESS read-write
        STATUS current
        DESCRIPTION "row status"
        ::= { frAtmSpvcVccEntry 26 }

frAtmSpvcVccRemoteFrPortAddr OBJECT-TYPE
        SYNTAX OCTET STRING (SIZE(1..20))
        MAX-ACCESS read-write
        STATUS current
        DESCRIPTION "The ATM address supporting the corresponding far
                 end FR IWF process.  If no address is supplied, 
                 no attempts to establish the active SVC are 
                 initiated."
        ::= { frAtmSpvcVccEntry 27 }
 
frAtmSpvcVccRemoteSelectType OBJECT-TYPE
        SYNTAX INTEGER  {
                required(1),
                any(2)
                }
        MAX-ACCESS read-write
        STATUS current
        DESCRIPTION "Indicates whether the remote DLCI/VPI/VCI values are to
                 be used at the destination.  If 'any' is used, the
                 destination switch will choose the DLCI/VPI/VCI values.
                 In such a case, once the SPVC is connected, the
                 value of this object will change to 'required',
                 such that the same DLCI/VPI/VCI values will continue to
                 be used even if the connection is torn down and
                 re-established.  If the value 'required' is
                 specified, the values:  frAtmSpvcVccRemoteDlci or
                 frAtmSpvcVccRemoteVpi and frAtmSpvcVccRemoteVci, must 
                 be supplied and are used at the destination."
        ::= { frAtmSpvcVccEntry 28 }
 
frAtmSpvcVccRemoteDlci OBJECT-TYPE
        SYNTAX INTEGER (0..4095)
        MAX-ACCESS read-write
        STATUS current
        DESCRIPTION "DLCI value to be used at the destination of an
                 ATM SPVC VCC connection.  Valid range is a number
                 between 0 and 4095.  If frAtmSpvcVccRemoteSelectType
                 is configured as 'any', then this value is not
                 relevant."
        ::= { frAtmSpvcVccEntry 29 }
 
frAtmSpvcVccRemoteVpi OBJECT-TYPE
        SYNTAX INTEGER (0..4095)
        MAX-ACCESS read-write
        STATUS current
        DESCRIPTION "VPI value to be used at the destination of an
                 ATM SPVC VCC connection.  Valid range is a number
                 between 0 and 4095.  If frAtmSpvcVccRemoteSelectType
                 is configured as 'any', then this value is not
                 relevant."
        ::= { frAtmSpvcVccEntry 30 }
 
frAtmSpvcVccRemoteVci OBJECT-TYPE
        SYNTAX INTEGER (0..65535)
        MAX-ACCESS read-write
        STATUS current
        DESCRIPTION "VCI value to be used at the destination of an ATM
                 SPVC VCC connection.  Valid range is a number
                 between 0 and 65535.  If frAtmSpvcVccRemoteSelectType
                 is configured as 'any', then this value is not
                 relevant."
        ::= { frAtmSpvcVccEntry 31 }
 

frAtmSpvcVccRetryInterval OBJECT-TYPE
        SYNTAX INTEGER (0..3600)
        UNITS      "seconds"
        MAX-ACCESS read-write
        STATUS current
        DESCRIPTION "The time to wait before attempting to establish
                 the SVC after previous failed call attempt. 
                 0 indicates retry will be attempted immediately."
        DEFVAL { 10 }    
        ::= { frAtmSpvcVccEntry 32 }
     
frAtmSpvcVccRetryLimit OBJECT-TYPE
        SYNTAX INTEGER (0..65535) 
        MAX-ACCESS read-write
        STATUS current
        DESCRIPTION "A maximum limit on how many retries.     
                 0 indicates no limit."
        DEFVAL { 0 }  
        ::= { frAtmSpvcVccEntry 33 }

frAtmSpvcVccRestart OBJECT-TYPE
        SYNTAX INTEGER {
                restart(1),
                noop(2)
                }
        MAX-ACCESS read-write
        STATUS current
        DESCRIPTION "restart, the active SVC is released if necessary
                 and a new setup procedure is begun; or noop"
        ::= { frAtmSpvcVccEntry 34 }

frAtmSpvcVccRetryFailure OBJECT-TYPE
        SYNTAX Integer32
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION "Indicates how many attempts to establish the
                 connection have failed.  It is reset whenever a
                 connection is successfully established or the SVC
                 is restarted."
        ::= { frAtmSpvcVccEntry 35 }

frAtmSpvcVccReleaseCause OBJECT-TYPE
        SYNTAX INTEGER(1..127)
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION "CAUSE field value of the last RELEASE message
                 received for this SVC."
        ::= { frAtmSpvcVccEntry 36 }

frAtmSpvcVccReleaseDiag OBJECT-TYPE
        SYNTAX OCTET STRING (SIZE(0..8))
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION "DIAGNOSTICS field value of the last RELEASE
                 message received for this SVC."
        ::= { frAtmSpvcVccEntry 37 }

frAtmSpvcVccRelease OBJECT-TYPE
        SYNTAX INTEGER {
                release(1),
                noop(2)
                }
        MAX-ACCESS read-write
        STATUS current
        DESCRIPTION "Release the connnection established on the endpoint
                 and if the endpoint is Active, do not retry."
        ::= { frAtmSpvcVccEntry 38 }

frAtmSpvcVccStatsCellsEncodedHiA OBJECT-TYPE
        SYNTAX Integer32
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION "Cells encoded on side A- (High order portion) "
        ::= { frAtmSpvcVccEntry 39 }

frAtmSpvcVccStatsCellsEncodedLoA OBJECT-TYPE
        SYNTAX Integer32
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION "Cells encoded on side A- (Low order portion)"
        ::= { frAtmSpvcVccEntry 40 }

frAtmSpvcVccStatsCellsDecodedHiA OBJECT-TYPE
        SYNTAX Integer32
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION "Cells decoded on side A- (High order portion)"
        ::= { frAtmSpvcVccEntry 41 }

frAtmSpvcVccStatsCellsDecodedLoA OBJECT-TYPE
        SYNTAX Integer32
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION "Cells decoded on side A- (Low order portion)"
        ::= { frAtmSpvcVccEntry 42 }

frAtmSpvcVccStatsInCellCountHiB OBJECT-TYPE
        SYNTAX Integer32
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION "Valid cells received (Input Billing Count)"
        ::= { frAtmSpvcVccEntry 43 }

frAtmSpvcVccStatsInCellCountLoB OBJECT-TYPE
        SYNTAX Integer32
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION "Valid cells received (Input Billing Count)"
        ::= { frAtmSpvcVccEntry 44 }

frAtmSpvcVccStatsOutCellCountHiB OBJECT-TYPE
        SYNTAX Integer32
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION "Valid cells transmitted (Output Billing Count)"
