atmPvcVpcTable        OBJECT-TYPE
                SYNTAX SEQUENCE OF AtmPvcVpcEntry
                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION "Table of PVC atm virtual path connections"
                ::= { connectionConfig 2 }

atmPvcVpcEntry        OBJECT-TYPE
                SYNTAX AtmPvcVpcEntry
                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION "Row definition of atm connections"
                INDEX { atmPvcVpcIfA, atmPvcVpcVpiA, 
                        atmPvcVpcIfB, atmPvcVpcVpiB }
                ::= { atmPvcVpcTable 1 }

AtmPvcVpcEntry        ::= SEQUENCE {
                atmPvcVpcIfA                        IntfIndex,
                atmPvcVpcVpiA                        INTEGER,
                atmPvcVpcIfB                        IntfIndex,
                atmPvcVpcVpiB                        INTEGER,
                atmPvcVpcServiceTypeA2B                INTEGER,
                atmPvcVpcSarTypeA2B                INTEGER,
                atmPvcVpcPeakCellRateA2B        Integer32,
                atmPvcVpcSusCellRateA2B                Integer32,
                atmPvcVpcMaxBurstSizeA2B        Integer32,
                atmPvcVpcServiceTypeB2A                INTEGER,
                atmPvcVpcSarTypeB2A                INTEGER,
                atmPvcVpcPeakCellRateB2A        Integer32,
                atmPvcVpcSusCellRateB2A                Integer32,
                atmPvcVpcMaxBurstSizeB2A        Integer32,
                atmPvcVpcType                        INTEGER,
                atmPvcVpcFlow                        INTEGER,
                atmPvcVpcRowStatus                RowStatus,
                atmPvcVpcConnectionStatus        INTEGER,
                atmPvcVpcOamStatus                INTEGER,

                atmPvcVpcDualHomed                INTEGER,

                atmPvcVpcViA                    INTEGER,
                atmPvcVpcViB                    INTEGER,
                atmPvcVpcCnfrmTypeA2B           INTEGER,
                atmPvcVpcCnfrmTypeB2A           INTEGER,

                atmPvcVpcVUNIA                  INTEGER,
                atmPvcVpcVUNIB                  INTEGER,

                atmPvcVpcOamStatusB             INTEGER,
                atmPvcVpcOAMConnEndPtStatusA    INTEGER,
                atmPvcVpcOAMConnEndPtStatusB    INTEGER,
                atmPvcVpcOAMConnSegEndPtStatusA INTEGER,
                atmPvcVpcOAMConnSegEndPtStatusB INTEGER,
                atmPvcVpcConnFailureCauseA        AtmConnFailureCause,
                atmPvcVpcConnFailureCauseB        AtmConnFailureCause,
                atmPvcVpcTrafficShapingA2B      INTEGER,
		atmPvcVpcTrafficShapingB2A      INTEGER
                }

atmPvcVpcIfA        OBJECT-TYPE
                SYNTAX IntfIndex
                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION "Interface index for side A of an ATM-to-ATM PVC VPC
                             connection.  Number value is interpreted as an
                             interface of the form, SSPPCCC, where SS is the 
                             card slot, PP is the port number, and CCC is the 
                             channel number."
                ::= { atmPvcVpcEntry 1 }

atmPvcVpcVpiA        OBJECT-TYPE
                SYNTAX INTEGER (0..4095)
                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION "VPI value for side A of an ATM-to-ATM PVC VPC 
                             connection.  Valid range is a number between 0 and 
                             4095."
                ::= { atmPvcVpcEntry 2 }

atmPvcVpcIfB        OBJECT-TYPE
                SYNTAX IntfIndex
                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION "Interface index for side B of an ATM-to-ATM PVC VPC
                             connection.  Number value is interpreted as an
                             interface of the form, SSPPCCC, where SS is the 
                             card slot, PP is the port number, and CCC is the 
                             channel number."
                ::= { atmPvcVpcEntry 3 }

atmPvcVpcVpiB        OBJECT-TYPE
                SYNTAX INTEGER (0..4095)
                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION "VPI value for side B of an ATM-to-ATM PVC VPC 
                             connection.  Valid range is a number between 0 and 
                             4095."
                ::= { atmPvcVpcEntry 4 }

