                SYNTAX INTEGER {
                                tenToMinus4(1),
                                tenToMinus5(2)
                                }
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "Bits error Rate threshold to declare signal failure"
                DEFVAL { tenToMinus4 }
                ::= { oc3APSPairConfigEntry 6 }

oc3APSPairWorkingSDBERThreshold OBJECT-TYPE
                SYNTAX INTEGER {
                                tenToMinus5(1),
                                tenToMinus6(2),
                                tenToMinus7(3),
                                tenToMinus8(4)
                                }
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "Bits error Rate threshold to declare signal degrade"
                DEFVAL { tenToMinus5 }
                ::= { oc3APSPairConfigEntry 5 }

oc3APSPairProtectionSDBERThreshold OBJECT-TYPE
                SYNTAX INTEGER {
                                tenToMinus5(1),
                                tenToMinus6(2),
                                tenToMinus7(3),
                                tenToMinus8(4)
                                }
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "Bits error Rate threshold to declare signal degrade" 
                DEFVAL { tenToMinus5 }
                ::= { oc3APSPairConfigEntry 7 }

 
oc3APSPairWTRPeriod OBJECT-TYPE
                SYNTAX INTEGER (5..12)
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION " Duration for Wait To Restore Period between 5
                             to 12 minutes. Ref: BellCore GR-253-Core,
                             Issue 3, page 200, R5-56."
                DEFVAL { 5 }
                ::= { oc3APSPairConfigEntry 8 }

oc3APSWorkingSlot OBJECT-TYPE
                SYNTAX Integer32
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "Working slot of APS pair."
                ::= { oc3APSPairConfigEntry 9 }

oc3APSProtectionSlot OBJECT-TYPE
                SYNTAX Integer32
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "Protection slot of APS pair."
                ::= { oc3APSPairConfigEntry 10 }

oc3APSActiveSlot OBJECT-TYPE
                SYNTAX Integer32
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "Active slot of APS pair."
                ::= { oc3APSPairConfigEntry 11 }

oc3APSPairStatus OBJECT-TYPE
                SYNTAX INTEGER {
                                noFailure(1),
                                modeMismatch(2),
                                swByteFailure(3),
                                chMismatch(4)
                                }
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "noFailure   : The protection line is not in use.
                            modeMismatch : one end is configured unidirectional 
                                           and the other is bidirectional (both 
                                           the ends should be configured in the same mode).
                                           Also when one end is configured for 1:1 architechture 
                                           and the other end indicates it is configured for 
                                           1:n architechture.(Bidirectional mode only)
                            swByteFailure: protection switching byte failure - defect declared 
                                           when invalid / persistantly unacceptable codes are 
                                           received in the K1 byte. failure declared if defect 
                                           persists for an extended period.
                                           Invalid code is when the incoming K1 byte contains 
                                           an unused code or a code irrelevant for the specific 
                                           switching operation.(Only in bi-directional mode)
                            chMismatch   : channel mismatch is declared when the channel numbers 
                                           in the transmitted K1 byte and the received K2 byte
                                           do not match for 50 ms.Channel mismatch is monitored 
                                           only in bidirectional mode."
                                                          
                DEFVAL { noFailure }
                ::= { oc3APSPairConfigEntry 12 }


oc3APSPairUserCommand OBJECT-TYPE
                SYNTAX INTEGER {
                                noAction(1),
                                manualSwitchToWorkingPort(2),
                                manualSwitchToProtectionPort(3),
                                forcedSwitchToWorkingPort(4),
                                forcedSwitchToProtectionPort(5),
                                lockoutProtection(6),
                                clear(7)
                                }
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "APS Pair User Command"
                DEFVAL { noAction }
                ::= { oc3APSPairConfigEntry 13 }


