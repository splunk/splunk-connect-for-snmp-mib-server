                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION "An entry in the in-band management to atm DHPVC 
                             table.  There is one entry in the table 
                             corresponding to each active row in the 
                             ipAtmAppPvcVccTable for which the 
                             ipAtmAppPvcVccDualHomed object takes the value 
                             'yes'."
                INDEX { ipAtmAppPvcVccDestAddrA, ipAtmAppPvcVccSubnetMaskA,
                        ipAtmAppPvcVccIfB, ipAtmAppPvcVccVpiB, 
                        ipAtmAppPvcVccVciB }
                ::= { ipAtmBkAppPvcVccTable 1 }

IpAtmBkAppPvcVccEntry ::= SEQUENCE {
                ipAtmBkAppPvcVccIfB                IntfIndex,
                ipAtmBkAppPvcVccVpiB                INTEGER,
                ipAtmBkAppPvcVccVciB                INTEGER,
                ipAtmBkAppPvcVccActivePvc        INTEGER,
                ipAtmBkAppPvcVccAutoSwitch        INTEGER,
                ipAtmBkAppPvcVccRowStatus        RowStatus,
                ipAtmBkAppPvcVccViB             INTEGER,

                ipAtmBkAppPvcVccVUNIB           INTEGER,

                ipAtmBkAppPvcVccConnFailureCauseB  AtmConnFailureCause
                }

ipAtmBkAppPvcVccIfB OBJECT-TYPE
                SYNTAX IntfIndex
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "Interface index for side B, the atm side, of a 
                             backup in-band management-atm DHPVC VCC connection.
                             Number value is interpreted as an interface of the
                             form, SSPPCCC, where SS is the card slot, PP is 
                             the port number, and CCC is the channel number."
                ::= { ipAtmBkAppPvcVccEntry 1 }

ipAtmBkAppPvcVccVpiB OBJECT-TYPE
                SYNTAX INTEGER (0..4095)
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "VPI value for side B, the atm side, of a backup 
                             in-band management-atm DHPVC VCC connection.  Valid
                             range is a number between 0 and 4095."
                ::= { ipAtmBkAppPvcVccEntry 2 }

ipAtmBkAppPvcVccVciB OBJECT-TYPE
                SYNTAX INTEGER (0..65535)
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "VCI value for side B, the atm side, of a backup
                             in-band management-atm DHPVC VCC connection.  Valid
                             range is a number between 0 and 65535."
                ::= { ipAtmBkAppPvcVccEntry 3 }

ipAtmBkAppPvcVccActivePvc OBJECT-TYPE
                SYNTAX INTEGER {
                                primary(1),
                                backup(2),
                                none(3)
                               }
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "Current active PVC."
                DEFVAL { none }
                ::= { ipAtmBkAppPvcVccEntry 4 }

ipAtmBkAppPvcVccAutoSwitch OBJECT-TYPE
                SYNTAX INTEGER  {
                                yes(1),
                                no(2)
                                }
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "Auto switch back to primary link when primary link
                             is recovered."
                DEFVAL { yes }
                ::= { ipAtmBkAppPvcVccEntry 5 }

ipAtmBkAppPvcVccRowStatus OBJECT-TYPE
                SYNTAX RowStatus
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "row status"
                ::= { ipAtmBkAppPvcVccEntry 6 }


ipAtmBkAppPvcVccViB OBJECT-TYPE
                SYNTAX INTEGER (0..1024)
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "VI to associate the Egress part of the
                                A2B connection ."
                DEFVAL { 0 }
                ::= { ipAtmBkAppPvcVccEntry 7 }


ipAtmBkAppPvcVccVUNIB OBJECT-TYPE
                SYNTAX INTEGER (0..255)
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "VUNI to associate the Egress part of the
                                A2B connection ."
                DEFVAL { 0 }
                ::= { ipAtmBkAppPvcVccEntry 8 }


ipAtmBkAppPvcVccConnFailureCauseB    OBJECT-TYPE
                SYNTAX AtmConnFailureCause
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "connection status reason code"
                 ::= { ipAtmBkAppPvcVccEntry 9 }



 

spvcAddrTable OBJECT-TYPE
                SYNTAX SEQUENCE OF SpvcAddrEntry
                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION "Table of SPVC network interface addresses."
                ::= { connectionConfig 31 }

spvcAddrEntry OBJECT-TYPE
                SYNTAX SpvcAddrEntry
                MAX-ACCESS  not-accessible
                STATUS current
                DESCRIPTION "Row definition of SPVC address table."
                INDEX { spvcAddrIfA }
                ::= { spvcAddrTable 1 }

SpvcAddrEntry ::= SEQUENCE {
                        spvcAddrIfA                            IntfIndex,
                        spvcAddrLocalPortAddr   OCTET STRING,
                        spvcAddrRowStatus                RowStatus
                }

spvcAddrIfA        OBJECT-TYPE
                SYNTAX IntfIndex
                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION "Interface index for side A of an SPVC 
                             connection.  Number value is interpreted as an 
                             interface of the form, SSPPCCC, where SS is the 
                             card slot, PP is the port number, and CCC is the 
                             channel number."
                ::= { spvcAddrEntry 1 }

spvcAddrLocalPortAddr OBJECT-TYPE
                SYNTAX OCTET STRING (SIZE(1..20))
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "Local port NSAP address."
                ::= { spvcAddrEntry 2 }

spvcAddrRowStatus OBJECT-TYPE
                SYNTAX RowStatus
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "This is used to configure a NSAP address on the
                             local port.  Only when the address is configured
                             can SPVC connections be established."
                ::= { spvcAddrEntry 3 }





 


frAtmSpvcVccTable OBJECT-TYPE
        SYNTAX SEQUENCE OF FrAtmSpvcVccEntry
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION "Table of frame relay to ATM IWF connections"
        ::= { connectionConfig 32 }

frAtmSpvcVccEntry OBJECT-TYPE
        SYNTAX FrAtmSpvcVccEntry
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION "Row definition of frame relay to ATM IWF 
              connections "
        INDEX { spvcAddrIfA, frAtmSpvcVccDlciA}
        ::= { frAtmSpvcVccTable 1 }

FrAtmSpvcVccEntry ::= SEQUENCE 
        {
        frAtmSpvcVccDlciA                           Integer32,
        frAtmSpvcVccIfB                             IntfIndex,
        frAtmSpvcVccVpiB                            INTEGER,
        frAtmSpvcVccVciB                            INTEGER,
        frAtmSpvcVccMaxFrameSizeIn                  Integer32,
        frAtmSpvcVccBcIn                            Integer32,
        frAtmSpvcVccBeIn                            Integer32,
        frAtmSpvcVccCirIn                           Integer32,
        frAtmSpvcVccMaxFrameSizeOut                 Integer32,
        frAtmSpvcVccBcOut                           Integer32,
        frAtmSpvcVccBeOut                           Integer32,
        frAtmSpvcVccCirOut                          Integer32,
        frAtmSpvcVccServiceType                     INTEGER,
        frAtmSpvcVccFlow                            INTEGER,
        frAtmSpvcVccTimeLenIn                       INTEGER,
        frAtmSpvcVccTimeLenOut                      INTEGER,
        frAtmSpvcVccPeakCellRateA2B                 Integer32,
        frAtmSpvcVccSusCellRateA2B                  Integer32,
        frAtmSpvcVccMaxBurstSizeA2B                 Integer32,
        frAtmSpvcVccPeakCellRateB2A                 Integer32,
        frAtmSpvcVccSusCellRateB2A                  Integer32,
        frAtmSpvcVccMaxBurstSizeB2A                 Integer32,
        frAtmSpvcVccInterWorkingFunc                INTEGER,
        frAtmSpvcVccConnType                        INTEGER,
        frAtmSpvcVccOperStatus                      INTEGER,
        frAtmSpvcVccRowStatus                       RowStatus,
        frAtmSpvcVccRemoteFrPortAddr                OCTET STRING,
        frAtmSpvcVccRemoteSelectType                INTEGER,
        frAtmSpvcVccRemoteDlci                      INTEGER,
        frAtmSpvcVccRemoteVpi                       INTEGER,
        frAtmSpvcVccRemoteVci                       INTEGER,
        frAtmSpvcVccRetryInterval                   INTEGER,
        frAtmSpvcVccRetryLimit                      INTEGER,
        frAtmSpvcVccRestart                         INTEGER,
        frAtmSpvcVccRetryFailure                    Integer32,
        frAtmSpvcVccReleaseCause                    INTEGER,    
        frAtmSpvcVccReleaseDiag                     OCTET STRING,
        frAtmSpvcVccRelease                         INTEGER,
        frAtmSpvcVccStatsCellsEncodedHiA            Integer32,
        frAtmSpvcVccStatsCellsEncodedLoA            Integer32,
        frAtmSpvcVccStatsCellsDecodedHiA            Integer32,
        frAtmSpvcVccStatsCellsDecodedLoA            Integer32,
        frAtmSpvcVccStatsInCellCountHiB             Integer32,
        frAtmSpvcVccStatsInCellCountLoB             Integer32,
        frAtmSpvcVccStatsOutCellCountHiB            Integer32,
        frAtmSpvcVccStatsOutCellCountLoB            Integer32,
        frAtmSpvcVccStatsTimer                      Integer32,
        frAtmSpvcVccStatsFramesDroppedA             Integer32,
        frAtmSpvcVccStatsOdometerCellsEncodedHiA    Integer32,
        frAtmSpvcVccStatsOdometerCellsEncodedLoA    Integer32,
        frAtmSpvcVccStatsOdometerCellsDecodedHiA    Integer32,
        frAtmSpvcVccStatsOdometerCellsDecodedLoA    Integer32,
        frAtmSpvcVccStatsInOdometerCellCountHiB     Integer32,
        frAtmSpvcVccStatsInOdometerCellCountLoB     Integer32,
        frAtmSpvcVccStatsOutOdometerCellCountHiB    Integer32,
        frAtmSpvcVccStatsOutOdometerCellCountLoB    Integer32,
        frAtmSpvcVccStatsOdometerTimer              Integer32,
        frAtmSpvcVccStatsOdometerReset              INTEGER,
        frAtmSpvcVccStatsAal5ErrFramesA             Integer32,
        frAtmSpvcVccStatsFramesTaggedA              Integer32,
        frAtmSpvcVccStatsCellsDroppedB              Integer32,
        frAtmSpvcVccStatsCellsTaggedB               Integer32

        ,frAtmSpvcVccVUNIB                          INTEGER


        ,frAtmSpvcVccTrafficShapingA2B              INTEGER
        ,frAtmSpvcVccPriority                       INTEGER
        }

frAtmSpvcVccDlciA OBJECT-TYPE
        SYNTAX INTEGER (0..1023)
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION "DLCI value for side A, the frame relay side, 
                 of a frame relay-atm SPVC VCC connection.  Valid
                 range is a number between 0 and 1023."
        ::= { frAtmSpvcVccEntry 1 }

frAtmSpvcVccIfB OBJECT-TYPE
        SYNTAX IntfIndex
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION "Interface index for side B, the atm side, of a 
                 frame relay-atm SPVC VCC connection.  Number value 
                 is interpreted as an interface of the form, 
                 SSPPCCC, where SS is the card slot, PP is the port 
                 number, and CCC is the channel number."
        ::= { frAtmSpvcVccEntry 2 }

frAtmSpvcVccVpiB OBJECT-TYPE
        SYNTAX INTEGER (0..4095)
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION "VPI value for side B, the atm side, of a frame
                 relay-atm SPVC VCC connection.  Valid range is a 
                 number between 0 and 4095."
        ::= { frAtmSpvcVccEntry 3 }

frAtmSpvcVccVciB OBJECT-TYPE
        SYNTAX INTEGER (0..65535)
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION "VCI value for side B, the atm side, of a frame
                 relay-atm SPVC VCC connection.  Valid range is a 
                 number between 0 and 65535."
        ::= { frAtmSpvcVccEntry 4 }

frAtmSpvcVccMaxFrameSizeIn OBJECT-TYPE
        SYNTAX Integer32
        MAX-ACCESS read-write
        STATUS current
        DESCRIPTION "The value of this object is the size in octets of
                 the largest frame relay information field for this
                 SPVC in the ingress direction (out of the frame 
                 relay UNI)."
        DEFVAL { 4096 }
        ::= { frAtmSpvcVccEntry 5 }

frAtmSpvcVccBcIn OBJECT-TYPE
        SYNTAX Integer32
        MAX-ACCESS read-write
        STATUS current
        DESCRIPTION "The value of this object is equal to the
                 committed burst size (Bc) parameter (measured in
                 bits) for this SPVC in the ingress
                 direction (into the frame relay UNI)."
        DEFVAL { 0 }
        ::= { frAtmSpvcVccEntry 6 }

frAtmSpvcVccBeIn OBJECT-TYPE
        SYNTAX Integer32
        MAX-ACCESS read-write
        STATUS current
        DESCRIPTION "The value of this object is equal to the excess
                 burst size (Be) parameter (measured in bits) for
                 this SPVC in the ingress direction (into
                 the frame relay UNI)."
        DEFVAL { 45000000 }
        ::= { frAtmSpvcVccEntry 7 }

frAtmSpvcVccCirIn OBJECT-TYPE
        SYNTAX Integer32
        MAX-ACCESS read-write
        STATUS current
        DESCRIPTION "The value of this object is equal to the
                 committed information rate (CIR) parameter
                 (measured in bits per second) for this SPVC
                 in the ingress direction (into the frame
                 relay UNI)."
        DEFVAL { 0 }
        ::= { frAtmSpvcVccEntry 8 }

frAtmSpvcVccMaxFrameSizeOut OBJECT-TYPE
        SYNTAX Integer32
        MAX-ACCESS read-write
        STATUS current
        DESCRIPTION "The value of this object is the size in octets of
                 the largest frame relay information field for this
                 SPVC in the egress direction (out of the frame 
                 relay UNI)."
        DEFVAL { 4096 }
        ::= { frAtmSpvcVccEntry 9 }

frAtmSpvcVccBcOut OBJECT-TYPE
        SYNTAX Integer32
        MAX-ACCESS read-write
        STATUS current
        DESCRIPTION "The value of this object is equal to the
                 committed burst size (Bc) parameter (measured in
                 bits) for this SPVC in the egress
                 direction (into the frame relay UNI)."
        DEFVAL { 0 }
        ::= { frAtmSpvcVccEntry 10 }

frAtmSpvcVccBeOut OBJECT-TYPE
        SYNTAX Integer32
        MAX-ACCESS read-write
        STATUS current
        DESCRIPTION "The value of this object is equal to the excess
                 burst size (Be) parameter (measured in bits) for
                 this SPVC in the egress direction (into
                 the frame relay UNI)"
        DEFVAL { 45000000 }
        ::= { frAtmSpvcVccEntry 11 }

frAtmSpvcVccCirOut OBJECT-TYPE
        SYNTAX Integer32
        MAX-ACCESS read-write
        STATUS current
        DESCRIPTION "The value of this object is equal to the
                 committed information rate (CIR) parameter
                 (measured in bits per second) for this SPVC
                 in the egress direction (into the frame
                 relay UNI)."
        DEFVAL { 0 }
        ::= { frAtmSpvcVccEntry 12 }

frAtmSpvcVccServiceType OBJECT-TYPE
        SYNTAX INTEGER {
                ubr(1),
                vbr-nrt2(2),
                vbr-nrt1(3),
                vbr-rt2(4),
                vbr-rt1(5),
                vbr-express(6)
                   }
        MAX-ACCESS read-write
        STATUS current
        DESCRIPTION "service type, Frame relay traffic is carried
                 by VBR type of service only."
        DEFVAL { ubr }
        ::= { frAtmSpvcVccEntry 13 }

frAtmSpvcVccFlow OBJECT-TYPE
