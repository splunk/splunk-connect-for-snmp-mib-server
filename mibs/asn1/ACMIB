     DESCRIPTION "Response : Notification that a SPVC connection between CE and
		  ATM endpoints has been deleted.
          Return values - A-side interfaceIndex
                          Remote NSAP address
                          B-side interfaceIndex
                          B-side vpi/vci"
	::= { connectionEvents 70 }

vbrAtmSpvcSetUpNotify  NOTIFICATION-TYPE
     OBJECTS { spvcAddrIfA, vbrAtmSpvcRemoteVbrPortAddr,
                vbrAtmSpvcIfB, vbrAtmSpvcVpiB, 
               vbrAtmSpvcVciB }
     STATUS current
     DESCRIPTION "Response :  Notification that a SPVC connection between TE 
		  and ATM endpoints has been made successfully.
          Return values - A-side interfaceIndex
                          Remote NSAP address
                          B-side interfaceIndex
                          B-side vpi/vci"
	::= { connectionEvents 71 }

vbrAtmSpvcTearDownNotify  NOTIFICATION-TYPE
     OBJECTS { spvcAddrIfA, vbrAtmSpvcRemoteVbrPortAddr,
               vbrAtmSpvcIfB, vbrAtmSpvcVpiB, 
               vbrAtmSpvcVciB }
     STATUS current
     DESCRIPTION "Response : Notification that a SPVC connection between TE and
		  ATM endpoints has been deleted.
          Return values - A-side interfaceIndex
                          Remote NSAP address
                          B-side interfaceIndex
                          B-side vpi/vci"
	::= { connectionEvents 72 }




aal2TrunkConfigSetupNotify  NOTIFICATION-TYPE
     OBJECTS { aal2TrunkConfigIf, aal2TrunkConfigVpi,
               aal2TrunkConfigVci }
     STATUS current
     DESCRIPTION "Response : The PVC VCC connection for aal2 trunk"
        ::= { connectionEvents 73 }

