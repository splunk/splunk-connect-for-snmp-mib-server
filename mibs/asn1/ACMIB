                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "Coding rule translation type."
                DEFVAL { none }
                ::= { gr303Aal2PvcVccEntry 13 }


gr303Aal2PvcVccConnectionStatus OBJECT-TYPE
                SYNTAX INTEGER {
                                active(1),
                                inactive(2)
                               }
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "Indicates the current connections status.
                             Active:  All interfaces through which the 
                                      connection is going are in service.
                             Inactive:  One of the end interfaces of the 
                                        connection is not in service.

                             The rest of the states apply to connections going
                             through resource interfaces like DSP2 and FEC.  These
                             interfaces are dynamically allocated.

                             If an end interface and the resource interfaces 
                             are out of service, the status will be inactive 
                             because the end interfaces take priority."
                DEFVAL { inactive }
                ::= { gr303Aal2PvcVccEntry 14 }

gr303Aal2PvcVccAltLineType OBJECT-TYPE
                SYNTAX INTEGER {
                                alt-LoopReverseBattery(1),
                alt-groundStart(9),
                                alt-loopStart (13),
                                alt-winkStart (29)
                               }
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "Analog Line Termination Line Type on the subscriber side. 
        The ABCD bit codes for diffterent signal types depend on 
        the analog line type. The analog type can be overwritten by an EOC 
        message from the IDT"
                DEFVAL { alt-loopStart }
                ::= { gr303Aal2PvcVccEntry 15 }


