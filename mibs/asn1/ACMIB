                SYNTAX INTEGER   {
                        outOfService (1),
                        inService (2),
                        delete (3)

                    }
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "ISDN interface admin status"
                ::= { isdnIntfConfigEntry 3 }

