                SYNTAX TaxiChannelConfigEntry
                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION "Row definition for common physical
                             port attributes"
                INDEX { taxiChannelId }
                ::= { taxiChannelConfigTable 1 }

