                SYNTAX INTEGER  {
                                 disabled(1),
                                 enabled(2)
                                }
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "silence detection off/on"
                DEFVAL { disabled }
                ::= { gr303Aal2PvcVccEntry 10 }

gr303Aal2PvcVccEchoCancellation OBJECT-TYPE
                SYNTAX INTEGER  {
                                 none(1),
                                 g165-nearEnd(2),
                                 g168(3)
                                }
                MAX-ACCESS read-write
                STATUS current
