                SYNTAX IpAddress
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "remote manager IP Address, where TRAP will be
                              sent to."
                 ::= { sysConfig 11 }

siteRemoteMgrAddr5 OBJECT-TYPE
                SYNTAX IpAddress
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "remote manager IP Address, where TRAP will be
                             sent to."
                  ::= { sysConfig 12 }

siteMasterNodeAtmAddr1 OBJECT-TYPE
                SYNTAX OCTET STRING (SIZE(20))
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "Master node atm address, where TRAP will be
                             sent to."
                ::= { sysConfig 13 }

siteMasterNodeAtmAddr2 OBJECT-TYPE
                SYNTAX OCTET STRING (SIZE(20))
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "Master node atm address, where TRAP will be
                             sent to."
                ::= { sysConfig 14 }

siteEnhanceFeatures OBJECT-TYPE
                SYNTAX INTEGER (1..255)
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "enhancement features capability -
                              number consists of a sum to represent
                              the following:
                                 1: securedAtm
                                 2: isdn
                                 4: frameRelay "
                ::= { sysConfig 15 }

siteTrapDialOutFeature  OBJECT-TYPE
                SYNTAX INTEGER {
                                disabled(1),
                                dialTerminal(2),
                                dialBeeper(3)
                                }
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "Enables the dialing out to a remote terminal
                             or beeper for trap notification."
                ::= { sysConfig 16 }

siteTrapDialOutPhoneNum OBJECT-TYPE
               SYNTAX DisplayString (SIZE(20))
               MAX-ACCESS read-write
               STATUS current
               DESCRIPTION "Phone number of the terminal to dial when
                             trap notifications are phoned out"
                ::= { sysConfig 17 }

siteTrapDialOutBeeperData OBJECT-TYPE
                SYNTAX DisplayString (SIZE(20))
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "Data (phone digits) to send to beeper if the
                             dialBeeper option is selected."
                ::= { sysConfig 18 } 

siteDateAndTime OBJECT-TYPE
                SYNTAX DateAndTime
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "Current date and time: 11 octets
                             octet#  contents            range
                             ------  --------            -----
                               1-2   year                0..65536
                                3    month               1..12
                                4    day                 1..31
                                5    hour                0..23
                                6    minutes             0..59
                                7    seconds      0..60(60=leap second)
                                8    deci-seconds         0..9
                                9    direction from UTC  '+'/'-'
                               10    hours from UTC      0..11
                               11    minutes from UTC    0..59"
                ::= { sysConfig 19 }

siteSoftwareVersion OBJECT-TYPE
                SYNTAX DisplayString (SIZE(15))
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "Constant value representing the version number
                             of the current software."
                ::= { sysConfig 20 }


