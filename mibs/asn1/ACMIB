atmAtmSpvcVpcTable OBJECT-TYPE
        SYNTAX SEQUENCE OF AtmAtmSpvcVpcEntry
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION "Table of SPVC ATM virtual path connections."
        ::= { connectionConfig 38 }

atmAtmSpvcVpcEntry OBJECT-TYPE
        SYNTAX AtmAtmSpvcVpcEntry
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION "Row definition of ATM SPVC VPC connections."
        INDEX { spvcAddrIfA, atmAtmSpvcVpcVpiA, atmAtmSpvcVpcRemoteAtmPortAddr }
        ::= { atmAtmSpvcVpcTable 1 }

AtmAtmSpvcVpcEntry ::= SEQUENCE {
        atmAtmSpvcVpcVpiA                          INTEGER,
        atmAtmSpvcVpcRemoteAtmPortAddr             OCTET STRING,
        atmAtmSpvcVpcIfB                           IntfIndex,
        atmAtmSpvcVpcVpiB                          INTEGER,
        atmAtmSpvcVpcServiceTypeA2B                INTEGER,
        atmAtmSpvcVpcSarTypeA2B                    INTEGER,
        atmAtmSpvcVpcPeakCellRateA2B               Integer32,
        atmAtmSpvcVpcSusCellRateA2B                Integer32,
        atmAtmSpvcVpcMaxBurstSizeA2B               Integer32,
        atmAtmSpvcVpcFrwdErrCorrectionA2B          INTEGER,
        atmAtmSpvcVpcServiceTypeB2A                INTEGER,
        atmAtmSpvcVpcSarTypeB2A                    INTEGER,
        atmAtmSpvcVpcPeakCellRateB2A               Integer32,
        atmAtmSpvcVpcSusCellRateB2A                Integer32,
        atmAtmSpvcVpcMaxBurstSizeB2A               Integer32,
        atmAtmSpvcVpcFrwdErrCorrectionB2A          INTEGER,
        atmAtmSpvcVpcFlow                          INTEGER,
        atmAtmSpvcVpcType                          INTEGER,
        atmAtmSpvcVpcConnType                      INTEGER,
        atmAtmSpvcVpcLeafReference                 INTEGER,
        atmAtmSpvcVpcOperStatus                    INTEGER,
        atmAtmSpvcVpcRowStatus                     RowStatus,
        atmAtmSpvcVpcRemoteSelectType              INTEGER,
        atmAtmSpvcVpcRemoteVpi                     INTEGER,
        atmAtmSpvcVpcRetryInterval                 INTEGER,
        atmAtmSpvcVpcRetryLimit                    INTEGER,
        atmAtmSpvcVpcRestart                       INTEGER,
        atmAtmSpvcVpcRetryFailure                  Integer32,
        atmAtmSpvcVpcReleaseCause                  INTEGER,
        atmAtmSpvcVpcReleaseDiag                   OCTET STRING,
        atmAtmSpvcVpcRelease                       INTEGER,
        atmAtmSpvcVpcStatsInCellCountHiA           Integer32,
        atmAtmSpvcVpcStatsInCellCountLoA           Integer32,
        atmAtmSpvcVpcStatsOutCellCountHiA          Integer32,
        atmAtmSpvcVpcStatsOutCellCountLoA          Integer32,
        atmAtmSpvcVpcStatsInCellCountHiB           Integer32,
        atmAtmSpvcVpcStatsInCellCountLoB           Integer32,
        atmAtmSpvcVpcStatsOutCellCountHiB          Integer32,
        atmAtmSpvcVpcStatsOutCellCountLoB          Integer32,
        atmAtmSpvcVpcStatsTimer                    Integer32,
        atmAtmSpvcVpcStatsFrwdErrCrrctnRateA2B     INTEGER,
        atmAtmSpvcVpcStatsFrwdErrCrrctnRateB2A     INTEGER,
        atmAtmSpvcVpcStatsInOdometerCellCountHiA   Integer32,
        atmAtmSpvcVpcStatsInOdometerCellCountLoA   Integer32,
        atmAtmSpvcVpcStatsOutOdometerCellCountHiA  Integer32,
        atmAtmSpvcVpcStatsOutOdometerCellCountLoA  Integer32,
        atmAtmSpvcVpcStatsInOdometerCellCountHiB   Integer32,
        atmAtmSpvcVpcStatsInOdometerCellCountLoB   Integer32,
        atmAtmSpvcVpcStatsOutOdometerCellCountHiB  Integer32,
        atmAtmSpvcVpcStatsOutOdometerCellCountLoB  Integer32,
        atmAtmSpvcVpcStatsOdometerTimer            Integer32,
        atmAtmSpvcVpcStatsOdometerReset            INTEGER,
        atmAtmSpvcVpcViA                           INTEGER,
        atmAtmSpvcVpcViB                           INTEGER,
        atmAtmSpvcVpcStatsCellsDroppedA            Integer32,
        atmAtmSpvcVpcStatsCellsTaggedA             Integer32,
        atmAtmSpvcVpcStatsCellsDroppedB            Integer32,
        atmAtmSpvcVpcStatsCellsTaggedB             Integer32

        ,atmAtmSpvcVpcVUNIA                        INTEGER,
        atmAtmSpvcVpcVUNIB                         INTEGER,

        atmAtmSpvcVpcTrafficShapingA2B             INTEGER,
        atmAtmSpvcVpcTrafficShapingB2A             INTEGER,
        atmAtmSpvcVpcPriority                    INTEGER
        }

atmAtmSpvcVpcVpiA    OBJECT-TYPE
        SYNTAX INTEGER (0..4095)
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION "VPI value for side A of an ATM SPVC VPC connection.
                 Valid range is a number between 0 and 4095."
