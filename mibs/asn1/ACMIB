        SYNTAX INTEGER {
                none(1),
                eighthRate(2),
                quarterRate(3),
                halfRate(4),
                autoHiQ(5),
                auto(6),
                autoLoQ(7)
                   }
        MAX-ACCESS read-write
        STATUS current
        DESCRIPTION "Forward error correction scheme, ATM trunk side."
        DEFVAL { none }
        ::= { atmAtmSpvcVccEntry 18 }

atmAtmSpvcVccFlow    OBJECT-TYPE
        SYNTAX INTEGER    {
                duplex(1),
                simplexA2B(2),
                pointToMultipointA2B(3),
                simplexB2A(4),
                pointToMultipointB2A(5)
                }
        MAX-ACCESS read-write
        STATUS current
        DESCRIPTION "Type of flow, pointToMultipointA2B is for future
                 support."
        DEFVAL { duplex }
        ::= { atmAtmSpvcVccEntry 19 }

atmAtmSpvcVccType    OBJECT-TYPE
        SYNTAX INTEGER  {
                nonsecured(1),
                secured(2)
                }
        MAX-ACCESS read-write
        STATUS current
        DESCRIPTION "Type of connection."
        DEFVAL { nonsecured }
        ::= { atmAtmSpvcVccEntry 20 }

atmAtmSpvcVccConnType OBJECT-TYPE
        SYNTAX INTEGER  {
                activeSvc(1),
                passiveSvc(2)
                }
        MAX-ACCESS read-write
        STATUS current
        DESCRIPTION "The type of connection:
                 activeSvc - attempt calls, whenever not 
                   established.
                 passiveSvc - accept calls."
        ::= { atmAtmSpvcVccEntry 21 }

atmAtmSpvcVccLeafReference OBJECT-TYPE
        SYNTAX INTEGER (1..65535)
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION "An arbitrary integer which serves to distinguish
                 the mulitple leaves attached to a root of a 
                 point-to-multipoint SPVC.  If point-to-point SPVC, 
                 the value should be 1."
        ::= { atmAtmSpvcVccEntry 22 }

atmAtmSpvcVccOperStatus OBJECT-TYPE
        SYNTAX INTEGER  {
                other(1),
                establishmentInProgress(2),
                connected(3),
                retriesExhausted(4),
                noAddressSupplied(5),
                lowerLayerDown(6),
                waitingForCall(7),
                waitingForRetryTimer(8)
                }
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION "The operation status of the connection:
                 other - none of the types specified below.
                 establishmentInProgress - connection is not 
                operational, but call attempts are ongoing.
                 connected - connection is currently operational.
                 retriesExhausted - retry limit has been reached 
                and call attempts have ceased.
                 noAddressSupplied - no remote address has been 
                configured, so no call attempts are initiated.
                 lowerLayerDown - SPVC handler is not operational."
        ::= { atmAtmSpvcVccEntry 23 }

atmAtmSpvcVccRowStatus OBJECT-TYPE
        SYNTAX RowStatus
        MAX-ACCESS read-write
        STATUS current
        DESCRIPTION "Used to create and delete a soft PVC.  When this
                 object is set to 'active', an attempt is made to 
                 set up the soft PVC.  When this object has the 
                 value 'active' and is set to another value, any 
                 setup or connection in-progress is released."
        ::= { atmAtmSpvcVccEntry 24 }


atmAtmSpvcVccRemoteSelectType OBJECT-TYPE
        SYNTAX INTEGER  {
                required(1),
                any(2)
                }
        MAX-ACCESS read-write
        STATUS current
        DESCRIPTION "Indicates whether the remote VPI/VCI values are to 
                 be used at the destination.  If 'any' is used, the 
                 destination switch will choose the VPI/VCI values. 
                 In such a case, once the SPVC is connected, the 
                 value of this object will change to 'required', 
                 such that the same VPI/VCI values will continue to 
                 be used even if the connection is torn down and 
                 re-established.  If the value 'required' is
                 specified, the values:  atmAtmSpvcVccRemoteVpi and 
                 atmAtmSpvcVccRemoteVci, must be supplied and are used
                 at the destination."
        ::= { atmAtmSpvcVccEntry 25 }

atmAtmSpvcVccRemoteVpi OBJECT-TYPE
        SYNTAX INTEGER (0..4095)
        MAX-ACCESS read-write
        STATUS current
        DESCRIPTION "VPI value to be used at the destination of an 
                 ATM SPVC VCC connection.  Valid range is a number 
                 between 0 and 4095.  If atmAtmSpvcVccRemoteSelectType 
                 is configured as 'any', then this value is not 
                 relevant."
        ::= { atmAtmSpvcVccEntry 26 }

atmAtmSpvcVccRemoteVci OBJECT-TYPE
        SYNTAX INTEGER (0..65535)
        MAX-ACCESS read-write
        STATUS current
        DESCRIPTION "VCI value to be used at the destination of an ATM 
                 SPVC VCC connection.  Valid range is a number 
                 between 0 and 65535.  If atmAtmSpvcVccRemoteSelectType
                 is configured as 'any', then this value is not 
                 relevant."
        ::= { atmAtmSpvcVccEntry 27 }

atmAtmSpvcVccRetryInterval OBJECT-TYPE
        SYNTAX INTEGER (0..3600)
        MAX-ACCESS read-write
        STATUS current
        DESCRIPTION "The time to wait before attempting to establish 
                 the SVC after a previous failed call attempt.  A 
                 value of 0 indicates retries will be attempted immediately."
        ::= { atmAtmSpvcVccEntry 28 }

atmAtmSpvcVccRetryLimit OBJECT-TYPE
        SYNTAX INTEGER (0..65535)
        MAX-ACCESS read-write
        STATUS current
        DESCRIPTION "A maximum limit on how many retries should be 
                 attempted.  A value of 0 indicates no limit."
        ::= { atmAtmSpvcVccEntry 29 }

atmAtmSpvcVccRestart OBJECT-TYPE
        SYNTAX INTEGER  {
                restart(1),
                noop(2)
                }
        MAX-ACCESS read-write
        STATUS current
        DESCRIPTION "The active SVC is released, if necessary, and a 
                 new setup procedure is begun if 'restart', 
                 otherwise, noop."
        ::= { atmAtmSpvcVccEntry 30 }

atmAtmSpvcVccRetryFailure OBJECT-TYPE
        SYNTAX Integer32
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION "Indicates how many attempts to establish the 
                 connection have failed.  It it reset whenever a 
                 connection is successfully established or the SVC 
                 is restarted."
        ::= { atmAtmSpvcVccEntry 31 }

atmAtmSpvcVccReleaseCause OBJECT-TYPE
        SYNTAX INTEGER (1..127)
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION "CAUSE field value of the last RELEASE message 
                 received for this SVC."
        ::= { atmAtmSpvcVccEntry 32 }

atmAtmSpvcVccReleaseDiag OBJECT-TYPE
        SYNTAX OCTET STRING (SIZE(0..8))
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION "DIAGNOSTICS field value of the last RELEASE 
                 message received for this SVC."
        ::= { atmAtmSpvcVccEntry 33 }


atmAtmSpvcVccRelease OBJECT-TYPE
        SYNTAX INTEGER {
                release(1),
                noop(2)
                }
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION "Release the connnection established on the endpoint
                 and if the endpoint is Active, do not retry."
        ::= { atmAtmSpvcVccEntry 34 }


atmAtmSpvcVccStatsInCellCountHiA OBJECT-TYPE
        SYNTAX Integer32
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION "Valid Cells received on A - (High order portion)"
        ::= { atmAtmSpvcVccEntry 35 }

atmAtmSpvcVccStatsInCellCountLoA OBJECT-TYPE
        SYNTAX Integer32
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION "Valid Cells received on A - (Low order portion) "
        ::= { atmAtmSpvcVccEntry 36 }

atmAtmSpvcVccStatsOutCellCountHiA OBJECT-TYPE
        SYNTAX Integer32
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION "Valid cells transmitted on A -(High order portion)"
        ::= { atmAtmSpvcVccEntry 37 }

atmAtmSpvcVccStatsOutCellCountLoA OBJECT-TYPE
        SYNTAX Integer32
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION "Valid cells transmitted on A -(Low order portion)"
        ::= { atmAtmSpvcVccEntry 38 }

atmAtmSpvcVccStatsInCellCountHiB OBJECT-TYPE
        SYNTAX Integer32
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION "Valid Cells received on B - (High order portion)"
        ::= { atmAtmSpvcVccEntry 39 }

atmAtmSpvcVccStatsInCellCountLoB OBJECT-TYPE
        SYNTAX Integer32
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION "Valid Cells received on B - (Low order portion) "
        ::= { atmAtmSpvcVccEntry 40 }

atmAtmSpvcVccStatsOutCellCountHiB OBJECT-TYPE
        SYNTAX Integer32
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION "Valid cells transmitted on B -(High order portion)"
        ::= { atmAtmSpvcVccEntry 41 }

atmAtmSpvcVccStatsOutCellCountLoB OBJECT-TYPE
        SYNTAX Integer32
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION "Valid cells transmitted on B -(Low order portion)"
        ::= { atmAtmSpvcVccEntry 42 }

atmAtmSpvcVccStatsTimer OBJECT-TYPE
        SYNTAX Integer32
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION "Number of seconds stats have been collecting"
        ::= { atmAtmSpvcVccEntry 43 }

atmAtmSpvcVccStatsFrwdErrCrrctnRateA2B OBJECT-TYPE
        SYNTAX INTEGER {
                none(1),
                eighthRate(2),
                quarterRate(3),
                halfRate(4)
                   }
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION "Displays the Forward error correction rate being 
                 used by the FEC module in the A2B direction.  This
                 field is dynamic when Auto rates are selected.  In
                 other cases, it will display the rate set by the 
                 user."
        DEFVAL { none }
        ::= { atmAtmSpvcVccEntry 44 }

atmAtmSpvcVccStatsFrwdErrCrrctnRateB2A OBJECT-TYPE
        SYNTAX INTEGER {
                none(1),
                eighthRate(2),
                quarterRate(3),
                halfRate(4)
                   }
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION "Displays the Forward error correction rate being 
                 used by the FEC module in the B2A direction.  This
                 field is dynamic when Auto rates are selected.  In
                 other cases, it will display the rate set by the 
                 user."
