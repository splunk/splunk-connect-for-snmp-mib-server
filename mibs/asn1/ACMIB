                SYNTAX INTEGER  {
                                taxi100(1),
                                taxi140(2)
                                }
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION " taxi type"
                DEFVAL { taxi100 }
                ::= { taxiPortConfigEntry 2 }

