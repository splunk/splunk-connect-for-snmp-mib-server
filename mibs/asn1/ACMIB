                SYNTAX Integer32
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "Number of valid cells received on the channel -
                             low Word"
                ::= { atmIfStatsEntry 3 }

atmIfStatsErroredCellsIn OBJECT-TYPE
                SYNTAX Integer32
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "Number of errored cells received on the channel"
                ::= { atmIfStatsEntry 4 }

atmIfStatsMismatchedCellsIn OBJECT-TYPE
                SYNTAX Integer32
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "Number of Vpi/Vci translation errors on input
                             cells."
                ::= { atmIfStatsEntry 5 }

atmIfStatsNullCellsOutHi OBJECT-TYPE
                SYNTAX Integer32
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "Number of null cells sent out  on the channel -
                             High Word"
                ::= { atmIfStatsEntry 6 }

