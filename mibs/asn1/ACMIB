-- moduleEvents

cardInsertionNotify  NOTIFICATION-TYPE
             OBJECTS { cardSlot, cardType, cardProtectionStatus, cardOperStatus}
             STATUS  current
        DESCRIPTION "Info : The indicated slot has had a card inserted into it."
             ::= { moduleEvents 1 }

cardRemovedOrFailedNotify  NOTIFICATION-TYPE
             OBJECTS { cardSlot }
             STATUS  current
             DESCRIPTION "Major : The indicated slot has changed state."
             ::= { moduleEvents 2 }

lineStatusChangedNotify    NOTIFICATION-TYPE
             OBJECTS { portId, lineStatus }
             STATUS  current
             DESCRIPTION "Major : The indicated port has had a change in the line
                     status."
             ::= { moduleEvents 3 }

firmwareDownloadSucceededNotify NOTIFICATION-TYPE
             OBJECTS { fwReleaseSlot }
             STATUS  current
             DESCRIPTION "Info : The indicated slot has successfully completed a 
                     firmware download." 
             ::= { moduleEvents 4 }

firmwareDownloadFailedNotify NOTIFICATION-TYPE
             OBJECTS { fwReleaseSlot, firmwareDownloadReasonCode }
             STATUS  current
             DESCRIPTION "Minor : The indicated slot had the indicated failure
                     during a firmware download."
             ::= { moduleEvents 5 }

moduleRebootNotify NOTIFICATION-TYPE
             OBJECTS { cardSlot, cardType }
             STATUS  current
             DESCRIPTION "Major : This trap indicates that a cardType in cardSlot 
                     becomes busy and the CPU reboots it.  This is different 
                     from when an I/O card is actually removed."
             ::= { moduleEvents 6 }

oc3APSStateChangeNotify NOTIFICATION-TYPE
        OBJECTS { 
                  oc3APSPairPortIndex, oc3APSReasonCode, 
                  oc3APSK1K2Rx, oc3APSK1K2Tx,oc3APSSelectorState, 
                  oc3APSWorkingLineSignalStatus,
                  oc3APSProtectionLineSignalStatus
                }
        STATUS current
        DESCRIPTION "Notification of a change in the APS state machine."
        ::= { moduleEvents 7 }

oc3APSSwitchoverNotify NOTIFICATION-TYPE
        OBJECTS { 
                  oc3APSPairPortIndex, oc3APSReasonCode, 
                  oc3APSK1K2Rx, oc3APSK1K2Tx,oc3APSSelectorState,
                  oc3APSWorkingLineSignalStatus,
                  oc3APSProtectionLineSignalStatus
                }
        STATUS current
        DESCRIPTION "Notification of a switchover in an APS pair."
        ::= { moduleEvents 8 }

portModifyFailNotify NOTIFICATION-TYPE
        OBJECTS {portId, portFailureReasonCode}
        STATUS current
        DESCRIPTION "Info : Notification that an attempt to modify an 
                     port parameters has failed."
        ::= { moduleEvents 9 }

