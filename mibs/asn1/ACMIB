ethernetPortConfigEntry OBJECT-TYPE
                SYNTAX EthernetPortConfigEntry
                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION "Row definition for common physical
                             port attributes"
                INDEX { ethernetPortId }
                ::= { ethernetPortConfigTable 1 }
 
EthernetPortConfigEntry ::= SEQUENCE {
                ethernetPortId               Integer32,
                ethernetLinkType        INTEGER,
                ethernetPhysicalAddr        OCTET STRING,
                ethernetLineStatus        INTEGER,
                ethernetDuplexMode        INTEGER
                }
 
ethernetPortId        OBJECT-TYPE
                SYNTAX Integer32
                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION "interface index - SSPP"
                ::= { ethernetPortConfigEntry 1 }
 
ethernetLinkType OBJECT-TYPE
                SYNTAX INTEGER  {
                                l-10BaseT(1),
                                l-100BaseT(2),
                                auto(3)
                                }
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "The type of port can either be set to 10baseT or
                             100baseT for the first port on the ethernet card.
                             The other ports can only be set to 10baseT.  The
                             Auto mode will detect what LAN speed the port is
                             connected to."
                DEFVAL { l-10BaseT }
                ::= { ethernetPortConfigEntry 2 }
 
ethernetPhysicalAddr OBJECT-TYPE
                SYNTAX OCTET STRING (SIZE(6))
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "The MAC address used by this bridge when it must
                             be referred to in a unique fashion.  It is 
                             recommended that this be the numerically smallest
                             MAC address of all ports that belong to this
                             bridge.  However it is only required to be unique.
                             When concatenated with dot1dSprPriority a unique
                             Bridgeidentifier is formed which is used in the
                             Spanning Tree Protocol."
                REFERENCE "IEEE 802.1D-1990: Sections 6.4.1.1.3 and 3.12.5"
                ::= { ethernetPortConfigEntry 3 }

ethernetLineStatus OBJECT-TYPE
                SYNTAX INTEGER (1..31)
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "ethernet line status:
                        1: Down
                        2: 10baseT-FullDuplex
                        4: 10baseT-HalfDuplex
                        8: 100baseT-FullDuplex
                       16: 100baseT-HalfDuplex  "
                DEFVAL { 1 }
                ::= { ethernetPortConfigEntry 4 }
 
ethernetDuplexMode OBJECT-TYPE
                SYNTAX INTEGER  {
                                full(1),
                                half(2),
                                auto(3)
                                }
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "Set the port to either be in full or half duplex.
                             You can also set the port to auto detect the mode
                             it is connected to."
                DEFVAL { full }
                ::= { ethernetPortConfigEntry 5 }


