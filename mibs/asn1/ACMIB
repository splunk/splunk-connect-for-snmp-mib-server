imaGrpChannelConfigTable  OBJECT-TYPE
                SYNTAX SEQUENCE OF ImaGrpChannelConfigEntry
                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION "Table used for the IMA virtual channel (group)
                             configuration on cards that have physical 
                             interfaces configured as atmIma. Represents three
                             channels that can be configured as IMA groups on
                             port 7 of DS1 and E1 cards."
                ::= { channelConfig 14 }

imaGrpChannelConfigEntry OBJECT-TYPE
                SYNTAX ImaGrpChannelConfigEntry
                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION "Row definition for common physical
                             port attributes"
                INDEX { imaGrpChannelId }
                ::= { imaGrpChannelConfigTable 1 }

ImaGrpChannelConfigEntry ::= SEQUENCE {
                imaGrpChannelId                    IntfIndex,
                imaGrpChannelIfType                INTEGER,
                imaGrpChannelPortMap               Integer32,
                imaGrpChannelNeState               ImaGrpChannelEndState,
                imaGrpChannelFeState               ImaGrpChannelEndState,
                imaGrpChannelFailureStatus         INTEGER,
                imaGrpChannelSymmetry              INTEGER,
                imaGrpChannelMinNumTxLinks         INTEGER,
                imaGrpChannelMinNumRxLinks         INTEGER,
                imaGrpChannelNeTxClkMode           INTEGER,
                imaGrpChannelFeTxClkMode           INTEGER,
                imaGrpChannelTxImaId               INTEGER,
                imaGrpChannelRxImaId               INTEGER,
                imaGrpChannelTxFrameLength         INTEGER,
                imaGrpChannelRxFrameLength         INTEGER,
                imaGrpChannelDiffLinkDelayMax      Integer32,
                imaGrpChannelLeastDelayLink        IntfIndex,
                imaGrpChannelDiffDelayMaxObs       Integer32,
                imaGrpChannelAlphaValue            INTEGER,
                imaGrpChannelBetaValue             INTEGER,
                imaGrpChannelGammaValue            INTEGER,
                imaGrpChannelStatsNeNumFailures    Integer32,
                imaGrpChannelStatsFeNumFailures    Integer32,
                imaGrpChannelStatsNumTxActLnks     Integer32,
                imaGrpChannelStatsNumRxActLnks     Integer32,
                imaGrpChannelStatsTimer            Integer32,
                imaGrpChannelCompatMode                   INTEGER
                }

imaGrpChannelId OBJECT-TYPE
                SYNTAX IntfIndex
                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION "interface index - SSPPCCC"
                ::= { imaGrpChannelConfigEntry 1 }

imaGrpChannelIfType OBJECT-TYPE
                SYNTAX INTEGER  {
                                unconfigured(1),
                                atmUni3-0(2),
                                atmUni3-1(3),
                                iispUser(4),
                                iispNetwork(5),
                                atmPnni1-0(6),
                                atmUni4-0(7)
                                }
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "interface type "
                DEFVAL { unconfigured }
                ::= { imaGrpChannelConfigEntry 2 }

imaGrpChannelPortMap OBJECT-TYPE
                SYNTAX Integer32
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "A bitmap that identifies the physical IMA
                             interfaces in this group. The least significant 
                             bit represents port 1."
                ::= { imaGrpChannelConfigEntry 3 }

imaGrpChannelNeState OBJECT-TYPE
                SYNTAX ImaGrpChannelEndState 
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "The current operational state of the near-end IMA 
                             Group State Machine."
                ::= {imaGrpChannelConfigEntry 4 }

imaGrpChannelFeState OBJECT-TYPE
                SYNTAX ImaGrpChannelEndState 
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "The current operational state of the far-end IMA 
                             Group State Machine."
                ::= { imaGrpChannelConfigEntry 5 }

imaGrpChannelFailureStatus OBJECT-TYPE
                SYNTAX INTEGER { 
                        noFailure(1), 
                        startUpNe(2),
                        startUpFe(3), 
                        invalidMValueNe(4),
                        invalidMValueFe(5),
                        failedAssymetricNe(6),
                        failedAssymetricFe(7),
                        insuffLinksNe(8),
                        insuffLinksFe(9),
                        blockedNe(10),
                        blockedFe(11),
                        otherFailure(12)
                      } 
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "The current failure status of the IMA group
                             (the reason why the GTSM is in the down state).
                        noFailure(1)          no failure
                        startUpNe(2)          near-end is in the state of start-up
                        startUpFe(3)          far-end  is in the state of start-up
                        invalidMValueNe(4)    near-end has invalid frame length
                        invalidMValueFe(5)    far-end  has invalid frame length
                        failedAssymetricNe(6) near-end failed due to incompatable
                                              symmetry
                        failedAssymetricFe(7) far-end  failed due to incompatable
                                              symmetry
                        insuffLinksNe(8)      Near-end reported less than the min
                                              number of Tx links are active or
                                              less than the min number
                                              of Rx links are active
                        insuffLinksFe(9)      Far-end reported less than the min
                                              number of Rx links are active or less
                                              than thenmin number of Tx links are
                                              active
                        blockedNe(10)         Near-end is administratively
                                              out-of-service
                        blockedFe(11)         Far-end  is administratively
                                              out-of-service
                        otherFailure(12)      other failures"
                ::= { imaGrpChannelConfigEntry 6 }   

imaGrpChannelSymmetry OBJECT-TYPE
                SYNTAX INTEGER { 
                        symOperation(1),
                        asymOperation(2),
                        asymConfiguration(3)
                      }
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "Symmetry of the IMA group.
                        Symmetric configuration: all ports of IMA group are
                        configured as full-duplex.  With symmetric configuration,
                        operation is symmetric if no links failes; is asymmetric
                        if a link fails so that the active forward and backward
                        links between far-end and near-end are asymmetric.  If
                        the configuration is asymmetric, the operation can never
                        be symmetric."
                DEFVAL { symOperation }
                ::= {imaGrpChannelConfigEntry 7 }

imaGrpChannelMinNumTxLinks OBJECT-TYPE
                SYNTAX INTEGER (1..32)
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "Minimum number of transmit links required to be 
                             Active for the IMA group to be in the Up state."
                ::= { imaGrpChannelConfigEntry 8 }

imaGrpChannelMinNumRxLinks OBJECT-TYPE
                SYNTAX INTEGER (1..32)
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "Minimum number of receive links required to be 
                             Active for the IMA group to be in the Up state."
                ::= { imaGrpChannelConfigEntry 9 }

imaGrpChannelNeTxClkMode OBJECT-TYPE
                SYNTAX INTEGER {
                        ctc(1),
                        itc(2)
                      }
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "Transmit clocking mode used by the near-end IMA 
                  group. There are two possible modes: the Common Transmit 
                  Clock (CTC) and the Independent Transmit Clock (ITC). The 
                  CTC mode corresponds to the case when the transmit clock of 
                  all IMA links are derived from the same source. The ITC 
                  configuration corresponds to the case where there is at 
                  least one IMA link whose transmit clock is derived from a 
                  source different than at least another link transmit clock. 
                  The underlying clock configuration must be set up by the 
                  user to ensure operation in the selected mode."
                DEFVAL { ctc }
                ::= { imaGrpChannelConfigEntry 10 }

imaGrpChannelFeTxClkMode OBJECT-TYPE
                SYNTAX INTEGER {
                        ctc(1),
                        itc(2)
                      }
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION
                  "Transmit clocking mode used by the far-end IMA group.
                  There are two possible modes: the Common Transmit 
                  Clock (CTC) and the Independent Transmit Clock (ITC). 
                  The CTC mode corresponds to the case when the transmit clock 
                  of all IMA links are derived from the same source. The ITC 
                  configuration corresponds to the case where there is at least
                  one IMA link whose transmit clock is derived from a source 
                  different than at least another link transmit clock."
                ::= { imaGrpChannelConfigEntry 11 }

imaGrpChannelTxImaId OBJECT-TYPE
                SYNTAX INTEGER (0..255)
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "The IMA ID currently in use by the near-end IMA 
                            function."
                ::= {imaGrpChannelConfigEntry 12 }

imaGrpChannelRxImaId OBJECT-TYPE
                SYNTAX INTEGER (0..255)
                MAX-ACCESS  read-only
                STATUS current
                DESCRIPTION "The IMA ID currently in use by the far-end IMA 
                             function."
                ::= { imaGrpChannelConfigEntry 13 }

imaGrpChannelTxFrameLength OBJECT-TYPE
                SYNTAX INTEGER {
                        m32(32),
                        m64(64),
                        m128(128),
                        m256(256)
                      }
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION
                 "The frame length to be used by the IMA group in the transmit 
                  direction. Can only be set when the IMA group is startup."
                DEFVAL { m128 }
                ::= { imaGrpChannelConfigEntry 14 }

imaGrpChannelRxFrameLength OBJECT-TYPE
                SYNTAX INTEGER {
                        m32(32),
                        m64(64),
                        m128(128),
                        m256(256)
                      }
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "Value of IMA frame length as received from 
                             remote IMA function."
                ::= { imaGrpChannelConfigEntry 15 }

imaGrpChannelDiffLinkDelayMax OBJECT-TYPE
                SYNTAX INTEGER (1..200)
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION
                "The maximum number of milliseconds of delay differential among
                 the links that will be tolerated on this interface."
                DEFVAL { 25 }
                ::= { imaGrpChannelConfigEntry 16 }

imaGrpChannelLeastDelayLink OBJECT-TYPE
                SYNTAX IntfIndex
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION
                 "The Interface Index of the link configured in the IMA 
                  group which has the smallest link propagation delay. This 
                  value has meaning only if at least 1 link has been configured
                  in the IMA group."
                ::= { imaGrpChannelConfigEntry 17 }
      
imaGrpChannelDiffDelayMaxObs OBJECT-TYPE
                SYNTAX Integer32
                MAX-ACCESS read-only 
                STATUS current
                DESCRIPTION
                 "The latest maximum differential delay observed (in 
                  milliseconds), i.e. between the links having the least and 
                  most link propagation delay, among the receive links that 
                  are currently configured in the IMA group."
                ::= { imaGrpChannelConfigEntry 18 }

imaGrpChannelAlphaValue OBJECT-TYPE
                SYNTAX INTEGER (1..2)
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION
                 "This indicates the 'alpha' value used to specify the 
                  number of consecutive invalid ICP cells to be detected 
                  before moving to the IMA HUNT state."
                DEFVAL { 2 }
                ::= { imaGrpChannelConfigEntry 19 }

imaGrpChannelBetaValue OBJECT-TYPE
                SYNTAX INTEGER (1..5)
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION
                 "This indicates the 'beta' value used to specify the 
                  number of consecutive errored ICP cells to be detected 
                  before moving to the IMA HUNT state."
                DEFVAL { 2 }
                ::= { imaGrpChannelConfigEntry 20 }

imaGrpChannelGammaValue OBJECT-TYPE
                SYNTAX INTEGER (1..5)
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION
                 "This indicates the 'gamma' value used to specify the 
                  number of consecutive valid ICP cells to be detected before 
                  moving to the IMA SYNC state from the PRESYNC state."
                DEFVAL { 1 }
                ::= { imaGrpChannelConfigEntry 21 }

imaGrpChannelStatsNeNumFailures OBJECT-TYPE
                SYNTAX Integer32
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION
                 "The number of times a near-end group failure (Config-Abort,
                  Insufficient-Links) has been reported."
                ::= { imaGrpChannelConfigEntry 22 }

imaGrpChannelStatsFeNumFailures OBJECT-TYPE
                SYNTAX Integer32
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION
                 "The number of times a far-end group failure (Config-Abort-FE,
                  Insufficient-Links-FE, Blocked-FE) has been reported."
                ::= { imaGrpChannelConfigEntry 23 }

imaGrpChannelStatsNumTxActLnks OBJECT-TYPE
                SYNTAX Integer32
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION
                 "The number of links which are configured to transmit and are
                  currently Active in this IMA group."
                ::= { imaGrpChannelConfigEntry 24 }
 
imaGrpChannelStatsNumRxActLnks OBJECT-TYPE
                SYNTAX Integer32
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION
                 "The number of links which are configured to receive and are
                  currently Active in this IMA group."
                ::= { imaGrpChannelConfigEntry 25 }

imaGrpChannelStatsTimer OBJECT-TYPE
                SYNTAX Integer32
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION " Time, in seconds, since last reset."
                ::= { imaGrpChannelConfigEntry 26 }

imaGrpChannelCompatMode OBJECT-TYPE
                SYNTAX INTEGER {
                        no(1),
                        yes(2)
                }
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION 
                 "This is used to indicate interoperability mode to the I/O
                 card in terms of reporting the link status at the correct
                 location in the IMA Control Protocol (ICP) cell"
                DEFVAL {no}
                ::= { imaGrpChannelConfigEntry 27 }

--
-- Channel Configuration for the new card
--

tasmChannelConfigTable OBJECT-TYPE
                SYNTAX SEQUENCE OF TasmChannelConfigEntry
                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION "Table of common physical Channel attributes"
                ::= { channelConfig 15 }

tasmChannelConfigEntry OBJECT-TYPE
                SYNTAX TasmChannelConfigEntry
                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION "Row definition for common physical
                             channel attributes"
                INDEX { tasmChannelId }
                ::= { tasmChannelConfigTable 1 }

TasmChannelConfigEntry ::= SEQUENCE {
                tasmChannelId                   IntfIndex,
                tasmChannelIfType               INTEGER,
                tasmChannelAlarmType            INTEGER,
                tasmChannelModeConfig                INTEGER
                }

tasmChannelId       OBJECT-TYPE
                SYNTAX IntfIndex
                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION "Interface index - SSPPCC"
                ::= { tasmChannelConfigEntry 1 }

tasmChannelIfType OBJECT-TYPE
                SYNTAX INTEGER  {
                                unconfigured(1),
                                toneAnnce(2)
                                }
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "Channel Interface Type"
                DEFVAL { unconfigured }
                ::= { tasmChannelConfigEntry 2 }

tasmChannelAlarmType OBJECT-TYPE
                SYNTAX INTEGER  {
                                noAlarm(1),
                                tasmFailure(2)
                                }
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "This field is a read only failure which
                        gives the status of a DSP in the TASM card.When
                        the status is tasmFailure the Operational Status
                        at the interface level will go OOS"
                DEFVAL { noAlarm }
                ::= { tasmChannelConfigEntry 3 }

tasmChannelModeConfig OBJECT-TYPE
                SYNTAX INTEGER {
                                casMode(1),
                                tasMode(2)
                                     }
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "This field is used to configure the mdoe in which
                        a TASM DSP will be used asd a resource. The modes are
                        explained as follows:
                        casMode: DSP resource is used for DTMF/MF
                                tone detection/generation and running the
                                Type 100, 105, and 108 tests.
                        tasMode: DSP resource is used for playing/compressing
                                announcement."
                DEFVAL { casMode }
                ::= {tasmChannelConfigEntry 4 }


-- The Interface Configuration Group

atmUniConfigTable        OBJECT-TYPE
                SYNTAX SEQUENCE OF AtmUniConfigEntry
                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION "Table of atm user to network interface (UNI)"
                ::= { intfConfig 1 }

atmUniConfigEntry        OBJECT-TYPE
                SYNTAX AtmUniConfigEntry
                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION "Row definition of atm UNI"
                INDEX { atmUniIfIndex } 
                ::= { atmUniConfigTable 1 }

AtmUniConfigEntry        ::= SEQUENCE {
                atmUniIfIndex                        IntfIndex,
                atmUniAddrType                        INTEGER,
                atmUniSvcMinVpi                        INTEGER,
                atmUniSvcMaxVpi                        INTEGER,
                atmUniSvcMinVci                        INTEGER,
                atmUniSvcMaxVci                        INTEGER,
                atmUniAdminStatus                INTEGER,
                atmUniOperStatus                INTEGER,
                atmUniOverSubscription                INTEGER,
                atmUniNwPrefix1                        OCTET STRING,
                atmUniT303                        Integer32,
                atmUniT308                        Integer32,
                atmUniT309                        Integer32,
                atmUniT310                        Integer32,
                atmUniT313                        Integer32,
                atmUniT316                        Integer32,
                atmUniT317                        Integer32,
                atmUniT322                        Integer32,
                atmUniT398                        Integer32,
                atmUniT399                        Integer32,
                atmUniMyNeighborIpAddress        IpAddress,
                atmUniMyNeighborIfName                DisplayString,
                atmUniVpiSig                        INTEGER,
                atmUniVciSig                        INTEGER,
                atmUniVpiIlmi                        INTEGER,
                atmUniVciIlmi                        INTEGER,
                atmUniAtmSignalingStatus        INTEGER,



                atmUniMyIfName                        DisplayString,
                atmUniIlmiProtocolStatus        INTEGER,
                atmUniIlmiOperStatus                INTEGER,
                atmUniIlmiConfigProc                INTEGER,
                atmUniIlmiConnectProc                INTEGER,
                atmUniIlmiAttachPtProc                INTEGER,
                atmUniIlmiSecureLinkProc        INTEGER,
                atmUniIlmiAddrRegProc                INTEGER,
                atmUniIlmiTimerS                Integer32,
                atmUniIlmiTimerT                Integer32,
                atmUniIlmiRetryK                Integer32,
                atmUniIntfType                        INTEGER,
                atmUniUserAddress                OCTET STRING,
                atmUniViSuppFlag                INTEGER,
                atmUniUPCSupp                         INTEGER,
                atmUniUbrLoadBalSupp                 INTEGER,
                atmUniAvailBwIgr                   Integer32,
                atmUniAvailBwEgr                  Integer32,

        atmUniVUNISuppFlag     INTEGER,
        atmUniUserAddrOrgScope  INTEGER,

                atmUniSscopRxWndSz              INTEGER,
                atmUniConnTrafficShapingFlag    INTEGER
                }

