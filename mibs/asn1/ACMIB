ipAtmAppPvcVccTable OBJECT-TYPE
                SYNTAX SEQUENCE OF IpAtmAppPvcVccEntry
                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION "Table about in-band management atm connection
                             entries."
                ::= { connectionConfig 4 }

ipAtmAppPvcVccEntry OBJECT-TYPE
                SYNTAX IpAtmAppPvcVccEntry
                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION "Row definition of in-band management
                             atm connection attribution."
                INDEX { ipAtmAppPvcVccDestAddrA, ipAtmAppPvcVccSubnetMaskA,
                        ipAtmAppPvcVccIfB, ipAtmAppPvcVccVpiB, 
                        ipAtmAppPvcVccVciB }
                ::= { ipAtmAppPvcVccTable 1 }

IpAtmAppPvcVccEntry ::= SEQUENCE {
                ipAtmAppPvcVccDestAddrA          IpAddress,
                ipAtmAppPvcVccSubnetMaskA        IpAddress,
                ipAtmAppPvcVccIfB                Integer32,
                ipAtmAppPvcVccVpiB               INTEGER,
                ipAtmAppPvcVccVciB               INTEGER,
                ipAtmAppPvcVccServiceTypeA2B     INTEGER,
                ipAtmAppPvcVccServiceTypeB2A     INTEGER,
                ipAtmAppPvcVccPeakCellRateB2A    Integer32,
                ipAtmAppPvcVccSusCellRateB2A     Integer32,
                ipAtmAppPvcVccMaxBurstSizeB2A    Integer32,
                ipAtmAppPvcVccType               INTEGER,
                ipAtmAppPvcVccRowStatus          RowStatus,
                ipAtmAppPvcVccConnectionStatus   INTEGER,

                ipAtmAppPvcVccDualHomed          INTEGER,

                ipAtmAppPvcVccViB                INTEGER,
                ipAtmAppPvcVccCnfrmTypeB2A       INTEGER,


                ipAtmAppPvcVccVUNIB              INTEGER,

                ipAtmAppPvcVccMfsB2A             INTEGER,
                ipAtmAppPvcVccConnFailureCauseB  AtmConnFailureCause,
                ipAtmAppPvcVccTrafficShapingA2B  INTEGER,
                ipAtmAppPvcVccOamStatus          INTEGER
                }

ipAtmAppPvcVccDestAddrA OBJECT-TYPE
                SYNTAX IpAddress
                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION "Destination IP address of an in-band management-
                             atm connection.  Value is and IP address."
                ::= { ipAtmAppPvcVccEntry 1 }

ipAtmAppPvcVccSubnetMaskA OBJECT-TYPE
                SYNTAX IpAddress
                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION "The subnet mask of the destination IP address.  
                             Value is an IP address."
                ::= { ipAtmAppPvcVccEntry 2 }

ipAtmAppPvcVccIfB OBJECT-TYPE
                SYNTAX Integer32
                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION "Interface index for side B, the atm side, of an 
                             in-band management-atm connection.  Number value 
                             is interpreted as an interface of the form, 
                             SSPPCCC, where SS is the card slot, PP is the port
                              number, and CCC is the channel number."
                ::= { ipAtmAppPvcVccEntry 3 }

ipAtmAppPvcVccVpiB OBJECT-TYPE
                SYNTAX INTEGER (0..4095)
                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION "VPI value for side B, the atm side, of an 
                             in-band management-atm connection.  Valid range
                             is a number between 0 and 4095."
                ::= { ipAtmAppPvcVccEntry 4 }

ipAtmAppPvcVccVciB OBJECT-TYPE
                SYNTAX INTEGER (0..65535)
                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION "VCI value for side B, the atm side, of an 
                             in-band management-atm connection.  Valid range
                             is a number between 0 and 65535."
                ::= { ipAtmAppPvcVccEntry 5 }

ipAtmAppPvcVccServiceTypeA2B OBJECT-TYPE
                SYNTAX INTEGER  {
                                ubr(1),
                                vbr-express(2)
                                }
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "Service type from side A to B"
                DEFVAL { ubr }
                ::= { ipAtmAppPvcVccEntry 6 }

