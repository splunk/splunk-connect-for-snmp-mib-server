                SYNTAX INTEGER {
                                disabled(1),
                                enabled(2)
                                }
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "refers to enabling/disabling cell scrambling on
                             ATM interface."
                DEFVAL { disabled }
                ::= { ds1ChannelConfigEntry 8 }

msChannelConfigTable OBJECT-TYPE
                SYNTAX SEQUENCE OF MsChannelConfigEntry
                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION "Channel configuration table for multi-serial 
                             ports."
                ::= { channelConfig 4 }

