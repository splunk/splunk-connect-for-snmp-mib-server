                STATUS current
                DESCRIPTION "Traffic Shaping Support in the B2A direction."
                DEFVAL { disabled }
                ::= { atmSpvcVccEntry 27 }

atmSpvcVccActiveSvcTable OBJECT-TYPE
                SYNTAX SEQUENCE OF AtmSpvcVccActiveSvcEntry
                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION "The table used to manage active SVCs established 
                             across ATM networks between ATM entities."
                ::= { connectionConfig 18 }

atmSpvcVccActiveSvcEntry OBJECT-TYPE
                SYNTAX AtmSpvcVccActiveSvcEntry
                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION "An entry in the ATM active SVC table.  There is one
                             entry in the table corresponding to each active row
                             in the atm SPVC VCC Table for which the 
                             atmSpvcVccConnType takes the value 'activeSvc'."
                INDEX { atmSpvcVccIfA, atmSpvcVccVpiA, atmSpvcVccVciA }
                ::= { atmSpvcVccActiveSvcTable 1 }

AtmSpvcVccActiveSvcEntry ::= SEQUENCE {
                atmSpvcVccRemoteAtmPortAddr        OCTET STRING,
                atmSpvcVccRemoteSelectType        INTEGER,
                atmSpvcVccRemoteVpi                INTEGER,
                atmSpvcVccRemoteVci                INTEGER,
                atmSpvcVccRetryInterval                INTEGER,
                atmSpvcVccRetryLimit                INTEGER,
                atmSpvcVccRestart                INTEGER,
                atmSpvcVccRetryFailure                Integer32,
                atmSpvcVccReleaseCause                INTEGER,
                atmSpvcVccReleaseDiag                OCTET STRING
                }

atmSpvcVccRemoteAtmPortAddr OBJECT-TYPE
                SYNTAX OCTET STRING (SIZE(1..20))
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "Remote ATM port NSAP address, not meaningful if 
                             passive type of connection."
                ::= { atmSpvcVccActiveSvcEntry 1 }

atmSpvcVccRemoteSelectType OBJECT-TYPE
                SYNTAX INTEGER  {
                                required(1),
                                any(2)
                                }
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "Indicates whether the remote VPI/VCI values are to 
                             be used at the destination.  If 'any' is used, the 
                             destination switch will choose the VPI/VCI values. 
                             In such a case, once the SPVC is connected, the 
                             value of this object will change to 'required', 
                             such that the same VPI/VCI values will continue to 
                             be used even if the connection is torn down and 
                             re-established.  If the value 'required' is
                             specified, the values:  atmSpvcVccRemoteVpi and 
                             atmSpvcVccRemoteVci, must be supplied and are used
                             at the destination."
                ::= { atmSpvcVccActiveSvcEntry 2 }

atmSpvcVccRemoteVpi OBJECT-TYPE
                SYNTAX INTEGER (0..4095)
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "VPI value to be used at the destination of an 
                             ATM SPVC VCC connection.  Valid range is a number 
                             between 0 and 4095.  If atmSpvcVccRemoteSelectType 
                             is configured as 'any', then this value is not 
                             relevant."
                ::= { atmSpvcVccActiveSvcEntry 3 }

atmSpvcVccRemoteVci OBJECT-TYPE
                SYNTAX INTEGER (0..65535)
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "VCI value to be used at the destination of an ATM 
                             SPVC VCC connection.  Valid range is a number 
                             between 0 and 65535.  If atmSpvcVccRemoteSelectType
                             is configured as 'any', then this value is not 
                             relevant."
                ::= { atmSpvcVccActiveSvcEntry 4 }

atmSpvcVccRetryInterval OBJECT-TYPE
                SYNTAX INTEGER (0..3600)
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "The time to wait before attempting to establish 
                             the SVC after a previous failed call attempt.  A 
                             value of 0 indicates retry will be attempted immediately."
                ::= { atmSpvcVccActiveSvcEntry 5 }

atmSpvcVccRetryLimit OBJECT-TYPE
                SYNTAX INTEGER (0..65535)
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "A maximum limit on how many retries should be 
                             attempted.  A value of 0 indicates no limit."
                ::= { atmSpvcVccActiveSvcEntry 6 }

atmSpvcVccRestart OBJECT-TYPE
                SYNTAX INTEGER  {
                                restart(1),
                                noop(2)
                                }
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "The active SVC is released, if necessary, and a 
                             new setup procedure is begun if 'restart', 
                             otherwise, noop."
                ::= { atmSpvcVccActiveSvcEntry 7 }

atmSpvcVccRetryFailure OBJECT-TYPE
                SYNTAX Integer32
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "Indicates how many attempts to establish the 
                             connection have failed.  It it reset whenever a 
                             connection is successfully established or the SVC 
                             is restarted."
                ::= { atmSpvcVccActiveSvcEntry 8 }

atmSpvcVccReleaseCause OBJECT-TYPE
                SYNTAX INTEGER (1..127)
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "CAUSE field value of the last RELEASE message 
                             received for this SVC."
                ::= { atmSpvcVccActiveSvcEntry 9 }

atmSpvcVccReleaseDiag OBJECT-TYPE
                SYNTAX OCTET STRING (SIZE(0..8))
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "DIAGNOSTICS field value of the last RELEASE 
                             message received for this SVC."
                ::= { atmSpvcVccActiveSvcEntry 10 }


cirEmAtmSpvcVccTable OBJECT-TYPE
                SYNTAX SEQUENCE OF CirEmAtmSpvcVccEntry
                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION "Table of Circuit Emulation to ATM Soft PVCs."
                ::= { connectionConfig 19 }

cirEmAtmSpvcVccEntry OBJECT-TYPE
                SYNTAX CirEmAtmSpvcVccEntry
                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION "Row definition of circuit emulation to ATM SPVC
                              connections."
                INDEX { cirEmAtmSpvcVccIfA }
                ::= { cirEmAtmSpvcVccTable 1 }

CirEmAtmSpvcVccEntry ::= SEQUENCE {
                        cirEmAtmSpvcVccIfA                         IntfIndex,
                        cirEmAtmSpvcVccIfB                        IntfIndex,
                        cirEmAtmSpvcVccVpiB                          INTEGER,
                        cirEmAtmSpvcVccVciB                          INTEGER,
                        cirEmAtmSpvcVccVoiceCompressionA2B        INTEGER,
                        cirEmAtmSpvcVccSilenceDetectionA2B        INTEGER,
                        cirEmAtmSpvcVccEchoCancellationA2B        INTEGER,
                        cirEmAtmSpvcVccCngToneDetectionA2B        INTEGER,
                        cirEmAtmSpvcVccCodingTranslationA2B        INTEGER,
                        cirEmAtmSpvcVccVoiceCompressionB2A        INTEGER,
                        cirEmAtmSpvcVccSilenceDetectionB2A        INTEGER,
                        cirEmAtmSpvcVccEchoCancellationB2A        INTEGER,
                        cirEmAtmSpvcVccCngToneDetectionB2A        INTEGER,
                        cirEmAtmSpvcVccCodingTranslationB2A        INTEGER,
                        cirEmAtmSpvcVccFlow                        INTEGER,
                        cirEmAtmSpvcVccServiceType                INTEGER,
                        cirEmAtmSpvcVccConnType                        INTEGER,
                        cirEmAtmSpvcVccCePortAddr                OCTET STRING,
                        cirEmAtmSpvcVccOperStatus                INTEGER,
                        cirEmAtmSpvcVccRowStatus                RowStatus,
                        cirEmAtmSpvcVccViB                        INTEGER
                        }

cirEmAtmSpvcVccIfA OBJECT-TYPE
                SYNTAX IntfIndex
                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION "Interface index for side A, the circuit emulation
                             side, of a circuit emulation-atm SPVC VCC 
                             connection.  Number value is interpreted as an 
                             interface of the form, SSPPCCC, where SS is the 
                             card slot, PP is the port number, and CCC is the 
                             channel number."
                ::= { cirEmAtmSpvcVccEntry 1 }

cirEmAtmSpvcVccIfB OBJECT-TYPE
                SYNTAX IntfIndex
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "Interface index for side B, the atm side, of a 
                             circuit emulation-atm SPVC VCC connection.  Number 
                             value is interpreted as an interface of the form, 
                             SSPPCCC, where SS is the card slot, PP is the port                              number, and CCC is the channel number."
                ::= { cirEmAtmSpvcVccEntry 2 }

cirEmAtmSpvcVccVpiB OBJECT-TYPE
                SYNTAX INTEGER (0..4095)
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "VPI value for side B, the atm side,  of a
                             circuit emulation-atm SPVC VCC connection.  Valid 
                             range is a number between 0 and 4095."
                ::= { cirEmAtmSpvcVccEntry 3 }

cirEmAtmSpvcVccVciB OBJECT-TYPE
                SYNTAX INTEGER (0..65535)
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "VCI value for side B, the atm side,  of a
                             circuit emulation-atm SPVC VCC connection.  Valid 
                             range is a number between 0 and 65535."
                ::= { cirEmAtmSpvcVccEntry 4 }

cirEmAtmSpvcVccVoiceCompressionA2B OBJECT-TYPE
                SYNTAX INTEGER  {
                                none(1),
                                g726-16k(2),
                                g726-24k(3),
                                g726-32k(4),
                                g726-40k(5)
                                }
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "Voice Compression Algorithm"
                DEFVAL { none }
                ::= { cirEmAtmSpvcVccEntry 5 }

