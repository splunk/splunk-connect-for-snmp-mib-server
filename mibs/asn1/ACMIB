frAtmPvcVccConnectionStatus OBJECT-TYPE
                SYNTAX INTEGER {
                                active(1),
                                inactive(2),
                                inactiveA2B-B2A(3),
                                inactiveA2B(4),
                                inactiveB2A(5)
                               }
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "Indicates the current connections status.
                             Active:  All interfaces through which the 
                                      connection is going are in service.
                             Inactive:  One of the end interfaces of the 
                                        connection is not in service.

                             The rest of the states apply to connections going
                             through resource interfaces like DSP2 and FEC.  These
                             interfaces are dynamically allocated.

                             InactiveA2B-B2A:  Both the A2B and B2A resource
                                               interfaces are not in service.
                             InactiveA2B:  The A2B resource interface is not in 
                                           service.
                             InactiveB2A:  The B2A resource interface is not in 
                                           service.

                             If an end interface and the resource interfaces 
                             are out of service, the status will be inactive 
                             because the end interfaces take priority."
                DEFVAL { inactive }
                ::= { frAtmPvcVccEntry 27 }


frAtmPvcVccDualHomed OBJECT-TYPE
                SYNTAX INTEGER {
                                no(1),
                                yes(2)
                               }
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "Does this PVC need a backup Dual homing PVC."
                DEFVAL { no }
                ::= { frAtmPvcVccEntry 28 }


frAtmPvcVccOamStatus OBJECT-TYPE
                SYNTAX INTEGER {
                                unsupp(1),
                                end-Pt(2),
                                end-Seg-Pt(3)
                               }
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "Indicates the OAM monitoring status of side A.
                             The 2 possible states are:
                             Unsupp:  No monitoring enabled.
                             End-Pt:  A side is only an end point, therefore
                                it will respond to AIS/RDI."
                DEFVAL { unsupp }
                ::= { frAtmPvcVccEntry 29 }

frAtmPvcVccViB  OBJECT-TYPE
                SYNTAX INTEGER (0..1024)
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "VI to associate the Egress part of the
                        A2B connection ."
                DEFVAL { 0 }
                ::= { frAtmPvcVccEntry 30 }

frAtmPvcVccCnfrmTypeB2A    OBJECT-TYPE
                SYNTAX INTEGER {
                                    best-effort(1),
                                one-bucket-notag-0plus1(2),
                                two-bucket-notag-0plus1-0plus1(3),
                                two-bucket-notag-0plus1-0(4),
                                two-bucket-tag-0plus1-0(5),
                                best-effort-tag(6)
                               }
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "The following is the description of this field
                        This will be used in conjunction with the
                        service type field to get the correct
                        traffic behaviour. In the conformances the 
                        the 0 or 0plus1 refers to the clp bit in the
                        cells. If 0plus1 is the bucket reference then
                        the clp bit is ignored. If 0 is specified for 
                        the bucket then check only clp=0 cells.
                        1: Best Effort will try to send all cellsin a 
                        best effort way. This will follow the 
                        Aqueman Algorithm
                        2: one-bucket-notag-0plus1 will use 1 bucket
                        i.e is the PCR(0+1) bucket and drop all
                        cells in excess.
                        3: two-bucket-notag-0plus1-0plus1 uses 2 buckets.
                        The 2 buckets are pcr(0+1) and scr(0+1).
                        All cells conforming to both buckets will be
                        passed through. The rest will be dropped.
                        4: two-bucket-notag-0plus1-0 uses 2 buckets.
                        The 2 buckets are pcr(0+1) and scr(0) or 
                        pcr(0). Cells which conform to both buckets
                        are passed through. The rest are dropped.
                        5: two-bucket-tag-0plus1-0 uses 2 buckets.
                        The 2 buckets are pcr(0+1) and scr(0) or 
                        pcr(0). Cells which do not conform to the first
                        bucket are dropped. Cells which do not 
                        conform to the 2nd bucket are tagged.
                        6: best-effort-tag. Tag all cells and then
                        do a best effort.  "
                DEFVAL { best-effort }
                ::= { frAtmPvcVccEntry 31 }


frAtmPvcVccVUNIB  OBJECT-TYPE
                SYNTAX INTEGER (0..255)
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "VUNI to associate the Egress part of the
                        A2B connection ."
                DEFVAL { 0 }
                ::= { frAtmPvcVccEntry 32 }


frAtmPvcVccOAMConnEndPtStatus    OBJECT-TYPE
                 SYNTAX INTEGER {
                                ete-active(1),
                                ete-ais-state(2),
                                ete-rdi-state(3),
                                ete-ais-rdi-state(4)
                                }
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "Connection endpoint status. Valid only 
                        at connection endpoint."
                ::= { frAtmPvcVccEntry 33 }
                
frAtmPvcVccOAMSegEndPtStatus    OBJECT-TYPE
                SYNTAX INTEGER {
                                seg-active(1), 
                                seg-ais-state(2),
                                seg-rdi-state(3),
                                seg-ais-rdi-state(4)
                                }
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "Segment endpoint status. Valid only at
                        segment/connection endpoint."
                 ::= { frAtmPvcVccEntry 34 }

frAtmPvcVccMfsB2A OBJECT-TYPE
                SYNTAX INTEGER (1..65535)
                MAX-ACCESS read-write 
                STATUS current
                DESCRIPTION "Maximum Frame Size for GFR traffic" 
                DEFVAL { 1 }
                ::= { frAtmPvcVccEntry 35 }

frAtmPvcVccConnFailureCauseA    OBJECT-TYPE
                SYNTAX FrConnFailureCause
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION " connestion status reason code"
                ::= { frAtmPvcVccEntry 36 }

frAtmPvcVccConnFailureCauseB    OBJECT-TYPE
                SYNTAX AtmConnFailureCause
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "connection status reason code"
                ::= { frAtmPvcVccEntry 37 }

frAtmPvcVccTrafficShapingA2B     OBJECT-TYPE
                SYNTAX INTEGER {
                                disabled(1),
                                enabled(2)
                               }
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "Traffic Shaping Support in the A2B direction."
                DEFVAL { disabled }
                ::= { frAtmPvcVccEntry 38 }



 --     In    ____________________________________ Out
 --    ______| SWITCH                            |__________
 --     Out  |                                   |  In
 --          |___________________________________|
 --  Frame Relay UNI A                           Frame Relay UNI B
 --                                   

 -- The Frame Relay PVC Table

frFrPvcTable         OBJECT-TYPE
                    SYNTAX SEQUENCE OF FrFrPvcEntry
                    MAX-ACCESS not-accessible
                    STATUS current
                    DESCRIPTION "Table of frame relay to frame relay connections"
                    ::= { connectionConfig 8 }

frFrPvcEntry         OBJECT-TYPE
                SYNTAX FrFrPvcEntry
                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION "Row definition of frame relay to frame relay PVC
                             connections  "
                INDEX { frFrPvcIfA, frFrPvcDlciA,
                        frFrPvcIfB, frFrPvcDlciB }
                ::= { frFrPvcTable 1 }

FrFrPvcEntry ::= SEQUENCE {
                           frFrPvcIfA                        IntfIndex,
                           frFrPvcDlciA                  Integer32,
                           frFrPvcIfB                        IntfIndex,
                           frFrPvcDlciB                        Integer32,
                           frFrPvcMaxFrameSizeA2B         Integer32,
                           frFrPvcBcA2B                   Integer32,
                           frFrPvcBeA2B                        Integer32,
                           frFrPvcCirA2B                  Integer32,
                           frFrPvcMaxFrameSizeB2A         Integer32,
                           frFrPvcBcB2A                  Integer32,
                           frFrPvcBeB2A                  Integer32,
                           frFrPvcCirB2A                 Integer32,
                           frFrPvcServiceType                 INTEGER,
                           frFrPvcFlow                         INTEGER,
                           frFrPvcRowStatus                RowStatus,
                           frFrPvcTimeLenA2B                 INTEGER,
                           frFrPvcInterWorkingFuncA2B         INTEGER,
                           frFrPvcTimeLenB2A                 INTEGER,
                           frFrPvcInterWorkingFuncB2A         INTEGER,
                           frFrPvcPeakCellRateA2B        Integer32,
                           frFrPvcSusCellRateA2B        Integer32,
                           frFrPvcMaxBurstSizeA2B        Integer32,
                           frFrPvcPeakCellRateB2A        Integer32,
                           frFrPvcSusCellRateB2A        Integer32,
                           frFrPvcMaxBurstSizeB2A        Integer32,
                           frFrPvcConnectionStatus      INTEGER,
                           frFrPvcConnFailureCauseA     FrConnFailureCause,
                           frFrPvcConnFailureCauseB     FrConnFailureCause
                          }

frFrPvcIfA        OBJECT-TYPE
                SYNTAX IntfIndex
                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION "Interface index for side A of a frame relay-frame
                             relay PVC connection.  Number value is interpreted 
                             as an interface of the form, SSPPCCC, where SS is 
                             the card slot, PP is the port number, and CCC is 
                             the channel number."
                ::= { frFrPvcEntry 1 }

frFrPvcDlciA OBJECT-TYPE
                SYNTAX Integer32
                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION "DLCI value for side A of a frame relay-frame relay
                             PVC connection.  Valid range is a number between
                             0 and 1023."
                ::= { frFrPvcEntry 2 }

frFrPvcIfB OBJECT-TYPE
                SYNTAX IntfIndex
                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION "Interface index for side B of a frame relay-frame
                             relay PVC connection.  Number value is interpreted 
                             as an interface of the form, SSPPCCC, where SS is 
                             the card slot, PP is the port number, and CCC is 
                             the channel number."
                ::= { frFrPvcEntry 3 }

frFrPvcDlciB OBJECT-TYPE
                SYNTAX Integer32
                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION "DLCI value for side B of a frame relay-frame relay
                             PVC connection.  Valid range is a number between
                             0 and 1023."
                ::= { frFrPvcEntry 4 }

frFrPvcMaxFrameSizeA2B OBJECT-TYPE
                SYNTAX Integer32
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "The value of this object is the size in octets of
                                  the largest frame relay information field for this
                                    PVC in the A to B direction."
                DEFVAL { 4096 }
                ::= { frFrPvcEntry 5 }

frFrPvcBcA2B OBJECT-TYPE
                SYNTAX Integer32
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "The value of this object is equal to the
                                  committed burst size (Bc) parameter (measured in
                                  bits) for this PVC in the A to B direction."
                DEFVAL { 0 }
                ::= { frFrPvcEntry 6 }

frFrPvcBeA2B OBJECT-TYPE
                SYNTAX Integer32
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "The value of this object is equal to the excess
                                  burst size (Be) parameter (measured in bits) for
                             this PVC in the A to B direction."
                DEFVAL { 45000000 }
                ::= { frFrPvcEntry 7 }

frFrPvcCirA2B OBJECT-TYPE
                SYNTAX Integer32
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "The value of this object is equal to the
                                  committed information rate (CIR) parameter
                                  (measured in bits per second) for this PVC
                             in the A to B direction."
                DEFVAL { 0 }
                ::= { frFrPvcEntry 8 }

frFrPvcMaxFrameSizeB2A OBJECT-TYPE
                SYNTAX Integer32
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "The value of this object is the size in octets of
                                  the largest frame relay information field for this
                                    PVC in the B to A direction."
                DEFVAL { 4096 }
                ::= { frFrPvcEntry 9 }

frFrPvcBcB2A OBJECT-TYPE
                SYNTAX Integer32
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "The value of this object is equal to the
                                  committed burst size (Bc) parameter (measured in
                                  bits) for this PVC in the B to A direction."
                DEFVAL { 0 }
                ::= { frFrPvcEntry 10 }

frFrPvcBeB2A OBJECT-TYPE
                SYNTAX Integer32
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "The value of this object is equal to the excess
                                  burst size (Be) parameter (measured in bits) for
                             this PVC in the B to A direction."
                DEFVAL { 45000000 }
                ::= { frFrPvcEntry 11 }

frFrPvcCirB2A OBJECT-TYPE
                SYNTAX Integer32
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "The value of this object is equal to the
                                  committed information rate (CIR) parameter
                                  (measured in bits per second) for this PVC
                             in the B to A direction."
                DEFVAL { 0 }
                ::= { frFrPvcEntry 12 }

frFrPvcServiceType OBJECT-TYPE
                SYNTAX INTEGER {
                                ubr(1),
                                vbr-nrt2(2),
                                vbr-nrt1(3),
                                vbr-rt2(4),
                                vbr-rt1(5),
                                vbr-express(6)
                               }
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "service type, Frame relay traffic is carried
                             by VBR type of service only."
                DEFVAL { ubr }
                ::= { frFrPvcEntry 13 }

frFrPvcFlow        OBJECT-TYPE
                SYNTAX INTEGER {
                                duplex(1),
                                simplexA2B(2)
                               }
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION " Flow Type"
                DEFVAL { duplex }
                ::= { frFrPvcEntry 14 }

frFrPvcRowStatus OBJECT-TYPE
                SYNTAX RowStatus
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "row status"
                ::= { frFrPvcEntry 15 }

frFrPvcTimeLenA2B OBJECT-TYPE
                SYNTAX INTEGER (64..4000)
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "The time interval over which CIR and the burst
                             sizes (Bc and Be) are measured in the A2B 
                             direction.  The unit is milliseconds."
                DEFVAL { 1000 }
                ::= { frFrPvcEntry 16 }

frFrPvcInterWorkingFuncA2B OBJECT-TYPE
                SYNTAX INTEGER {
                                frf8-Transl(1),
