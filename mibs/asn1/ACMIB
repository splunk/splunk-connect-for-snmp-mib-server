-- GR303 IG interface table (table of grouped interfaces)

gr303IgIntfConfigTable OBJECT-TYPE
                SYNTAX SEQUENCE OF Gr303IgIntfConfigEntry
                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION "Table of interface attributes in an GR303IG"
                ::= { gr303IgConfig 2 }

gr303IgIntfConfigEntry OBJECT-TYPE
                SYNTAX Gr303IgIntfConfigEntry
                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION "Row definition for GR303 Access Arrangement Interface configuration"
                INDEX { gr303IntfIgId , gr303IgIntfId}
                ::= { gr303IgIntfConfigTable 1 }

Gr303IgIntfConfigEntry::= SEQUENCE {
                gr303IntfIgId        INTEGER,
                gr303IgIntfId        INTEGER,
                gr303IgIntfPhysId INTEGER,
                gr303IgIntfNetId INTEGER,
                gr303IgIntfRowStatus RowStatus
           }
 
gr303IntfIgId OBJECT-TYPE
                SYNTAX INTEGER (1..84)
                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION "This is used as an index into GR303Ig table which
                recognizes GR303 Interface Group. This is used as a first 
                index into this interface table."
                ::= { gr303IgIntfConfigEntry 1 }

gr303IgIntfId OBJECT-TYPE
                SYNTAX INTEGER (1..28)
                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION "This is used as an index into IG interface table 
                which recognizes GR303 Ig interfaces. This is used as a 
                second index into this table"
                ::= { gr303IgIntfConfigEntry 2 }

gr303IgIntfPhysId   OBJECT-TYPE
                SYNTAX Integer32
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "Interface's Physical  ID (SSPPCCC)"
                ::= { gr303IgIntfConfigEntry 3 }


gr303IgIntfNetId OBJECT-TYPE
                SYNTAX Integer32
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "gr303Ig Interface (DS1) Net Id assigned by the network"
                ::= { gr303IgIntfConfigEntry 4 }

gr303IgIntfRowStatus OBJECT-TYPE
                SYNTAX RowStatus
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "GR303IG interface entry row status - row status can not be made
                             active until all the values are set"
                ::= { gr303IgIntfConfigEntry 5 }


-- GR303 LAPD Table

