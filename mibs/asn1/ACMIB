                ds1ChannelCellMapping                 INTEGER,
                ds1ChannelInputCodingRule         INTEGER,
                ds1ChannelSecuFeature                 INTEGER,
                ds1ChannelSignaling                INTEGER,
                ds1ChannelCellScramble                INTEGER
                }

