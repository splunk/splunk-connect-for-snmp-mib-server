cirAtmSpvcVccTable OBJECT-TYPE
        SYNTAX SEQUENCE OF CirAtmSpvcVccEntry
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION "Table of Circuit Emulation to ATM Soft PVCs."
        ::= { connectionConfig 34 }

cirAtmSpvcVccEntry OBJECT-TYPE
        SYNTAX CirAtmSpvcVccEntry
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION "Row definition of circuit emulation to ATM SPVC
          connections."
        INDEX { spvcAddrIfA, cirAtmSpvcVccRemoteCePortAddr }
        ::= { cirAtmSpvcVccTable 1 }

CirAtmSpvcVccEntry ::= SEQUENCE {
        cirAtmSpvcVccRemoteCePortAddr                 OCTET STRING,
        cirAtmSpvcVccIfB                              IntfIndex,
        cirAtmSpvcVccVpiB                             INTEGER,
        cirAtmSpvcVccVciB                             INTEGER,
        cirAtmSpvcVccVoiceCompressionA2B              INTEGER,
        cirAtmSpvcVccSilenceDetectionA2B              INTEGER,
        cirAtmSpvcVccEchoCancellationA2B              INTEGER,
        cirAtmSpvcVccCngToneDetectionA2B              INTEGER,
        cirAtmSpvcVccCodingTranslationA2B             INTEGER,
        cirAtmSpvcVccVoiceCompressionB2A              INTEGER,
        cirAtmSpvcVccSilenceDetectionB2A              INTEGER,
        cirAtmSpvcVccEchoCancellationB2A              INTEGER,
        cirAtmSpvcVccCngToneDetectionB2A              INTEGER,
        cirAtmSpvcVccCodingTranslationB2A             INTEGER,
        cirAtmSpvcVccFlow                             INTEGER,
        cirAtmSpvcVccServiceType                      INTEGER,
        cirAtmSpvcVccConnType                         INTEGER,
        cirAtmSpvcVccOperStatus                       INTEGER,
        cirAtmSpvcVccRowStatus                        RowStatus,
        cirAtmSpvcVccViB                              INTEGER,
        cirAtmSpvcVccRetryInterval                    INTEGER,
        cirAtmSpvcVccRetryLimit                       INTEGER,
        cirAtmSpvcVccRestart                          INTEGER,
        cirAtmSpvcVccRetryFailure                     Integer32,
        cirAtmSpvcVccReleaseCause                     INTEGER,
        cirAtmSpvcVccReleaseDiag                      OCTET STRING,
        cirAtmSpvcVccRelease                          INTEGER,
        cirAtmSpvcVccStatsCellsEncodedHiA             Integer32,
        cirAtmSpvcVccStatsCellsEncodedLoA             Integer32,
        cirAtmSpvcVccStatsCellsDecodedHiA             Integer32,
        cirAtmSpvcVccStatsCellsDecodedLoA             Integer32,
        cirAtmSpvcVccStatsInCellCountHiB              Integer32,
        cirAtmSpvcVccStatsInCellCountLoB              Integer32,
        cirAtmSpvcVccStatsOutCellCountHiB             Integer32,
        cirAtmSpvcVccStatsOutCellCountLoB             Integer32,
        cirAtmSpvcVccStatsTimer                       Integer32,
        cirAtmSpvcVccStatsOdometerCellsEncodedHiA     Integer32,
        cirAtmSpvcVccStatsOdometerCellsEncodedLoA     Integer32,
        cirAtmSpvcVccStatsOdometerCellsDecodedHiA     Integer32,
        cirAtmSpvcVccStatsOdometerCellsDecodedLoA     Integer32,
        cirAtmSpvcVccStatsInOdometerCellCountHiB      Integer32,
        cirAtmSpvcVccStatsInOdometerCellCountLoB      Integer32,
        cirAtmSpvcVccStatsOutOdometerCellCountHiB     Integer32,
        cirAtmSpvcVccStatsOutOdometerCellCountLoB     Integer32,
        cirAtmSpvcVccStatsOdometerTimer               Integer32,
        cirAtmSpvcVccStatsOdometerReset               INTEGER,
        cirAtmSpvcVccStatsCellsDroppedB               Integer32,
        cirAtmSpvcVccStatsCellsTaggedB                Integer32,

        cirAtmSpvcVccVUNIB                              INTEGER,

        cirAtmSpvcVccSarTypeB                        Integer32,
        cirAtmSpvcVccPriority                        INTEGER
        }

cirAtmSpvcVccRemoteCePortAddr    OBJECT-TYPE
        SYNTAX OCTET STRING (SIZE(1..20))
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION "The ATM address supporting the corresponding far 
         end CES IWF process.  If no address is supplied, 
         no attempts to establish the active SVC are 
         initiated."
        ::= { cirAtmSpvcVccEntry 1 }

cirAtmSpvcVccIfB OBJECT-TYPE
        SYNTAX IntfIndex
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION "Interface index for side B, the atm side, of a 
         circuit emulation-atm SPVC VCC connection.  Number 
         value is interpreted as an interface of the form, 
         SSPPCCC, where SS is the card slot, PP is the port                  number, and CCC is the channel number."
        ::= { cirAtmSpvcVccEntry 2 }

cirAtmSpvcVccVpiB OBJECT-TYPE
        SYNTAX INTEGER (0..4095)
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION "VPI value for side B, the atm side,  of a
         circuit emulation-atm SPVC VCC connection.  Valid 
         range is a number between 0 and 4095."
        ::= { cirAtmSpvcVccEntry 3 }

cirAtmSpvcVccVciB OBJECT-TYPE
        SYNTAX INTEGER (0..65535)
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION "VCI value for side B, the atm side,  of a
         circuit emulation-atm SPVC VCC connection.  Valid 
         range is a number between 0 and 65535."
        ::= { cirAtmSpvcVccEntry 4 }

cirAtmSpvcVccVoiceCompressionA2B OBJECT-TYPE
        SYNTAX INTEGER  {
                none(1),
                g726-16k(2),
                g726-24k(3),
                g726-32k(4),
                g726-40k(5),
                                g729a-8k(6)
                }
        MAX-ACCESS read-write
        STATUS current
        DESCRIPTION "Voice Compression Algorithm"
        DEFVAL { none }
        ::= { cirAtmSpvcVccEntry 5 }

cirAtmSpvcVccSilenceDetectionA2B OBJECT-TYPE
        SYNTAX INTEGER  {
                 disabled(1),
                 enabled(2)
                }
        MAX-ACCESS read-write
        STATUS current
        DESCRIPTION "silence detection off/on"
        DEFVAL { disabled }
        ::= { cirAtmSpvcVccEntry 6 }

cirAtmSpvcVccEchoCancellationA2B OBJECT-TYPE
        SYNTAX INTEGER  {
                 none(1),
                 g165-nearEnd(2),
                 g168(3)
                }
        MAX-ACCESS read-write
        STATUS current
        DESCRIPTION "Echo Cancellation Algorithm"
        DEFVAL { none }
        ::= { cirAtmSpvcVccEntry 7 }

cirAtmSpvcVccCngToneDetectionA2B OBJECT-TYPE
        SYNTAX INTEGER  {
                 disabled(1),
                 bypass(2),
                 g726-32k(3)
                }
        MAX-ACCESS read-write
        STATUS current
        DESCRIPTION "Calling Tone Detection Algorithm,
         one of the enum value, g726-32k(3)
         is obsoleted on 01Mar99 such that
         user would not be able to configure
         CE-ATM"
        DEFVAL { disabled }
        ::= { cirAtmSpvcVccEntry 8 }

cirAtmSpvcVccCodingTranslationA2B OBJECT-TYPE
        SYNTAX INTEGER  {
                                 none(1),
                                 muLaw2muLaw(2),
                                 aLaw2aLaw(3),
                                 muLaw2aLaw(4),
                                 aLaw2muLaw(5)
                }
        MAX-ACCESS read-write
        STATUS current
        DESCRIPTION "Coding rule translation type."
        DEFVAL { none }
        ::= { cirAtmSpvcVccEntry 9 }

