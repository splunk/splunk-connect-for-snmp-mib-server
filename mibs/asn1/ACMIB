                SYNTAX INTEGER (0..4095)
                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION "VPI value for Atm side , the atm side, 
                        of a atm trunk connection.  
                        Valid range is a number between 0 and 4095."
                ::= { aal2TrunkConfigEntry 2 }

aal2TrunkConfigVci OBJECT-TYPE
                SYNTAX INTEGER (0..65535)
                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION "VCI value for the atm side, of a variable
                        bit rate-atm PVC VCC connection.  
                        Valid range is a number between 0 and 65535."
                ::= { aal2TrunkConfigEntry 3 }

aal2TrunkConfigServiceType OBJECT-TYPE
                SYNTAX INTEGER {
                                cbr-1(1),
                                cbr-2(2),
                                cbr-3(3),
                                cbr-4(4),
                                ubr(5),
                                vbr-nrt2(6),
                                vbr-nrt1(7),
                                vbr-rt2(8),
                                vbr-rt1(9),
                                vbr-express(10)
                                }
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "Service/Queue types supported for Aal2 Trunk 
                             connections.When using Vbr/Ubr type service
                             type oversubscription will have to be taken into 
                             account for which nature of traffic going over the
                             Trunk should be known before hand "
                DEFVAL { cbr-1 }
                ::= { aal2TrunkConfigEntry 4 }

aal2TrunkConfigPeakCellRateIgr        OBJECT-TYPE
                SYNTAX Integer32
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "peak cell rate - atm side inp, in cell per 
                        second"
                DEFVAL { 1 }
                ::= { aal2TrunkConfigEntry 5 }
 
