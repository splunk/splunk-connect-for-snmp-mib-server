--  SubChannel level statistics that will be based on per CID.

viprSubChnlStatsTable OBJECT-TYPE
                SYNTAX SEQUENCE OF ViprSubChnlStatsEntry
                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION " The table which holds 
                              viprSubChnlStatsEntry data."
                ::= { connectionStatistics 17 }
                        
viprSubChnlStatsEntry OBJECT-TYPE
                SYNTAX ViprSubChnlStatsEntry
                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION " A table entry that contains IP interface 
                              sub channel statistics."
                INDEX { viprSlot, viprRouterId,  viprIntfId, viprSubChnlId, 
                        viprSubChnlIntfIndex, viprSubChnlVpiDlci, 
                        viprSubChnlVci, viprSubChnlTrafficType }
                ::= { viprSubChnlStatsTable 1 }

ViprSubChnlStatsEntry ::= SEQUENCE {
                viprSubChnlStatsInCellCountHiA          Integer32,
                viprSubChnlStatsInCellCountLoA          Integer32,
                     viprSubChnlStatsOutCellCountHiA         Integer32,
                     viprSubChnlStatsOutCellCountLoA         Integer32,
                viprSubChnlStatsInCellCountHiB          Integer32,
                viprSubChnlStatsInCellCountLoB          Integer32,
                     viprSubChnlStatsOutCellCountHiB         Integer32,
                     viprSubChnlStatsOutCellCountLoB         Integer32,
                viprSubChnlStatsTimer                        Integer32,
                viprSubChnlStatsInOdometerCellCountHiA  Integer32,
                viprSubChnlStatsInOdometerCellCountLoA  Integer32,
                     viprSubChnlStatsOutOdometerCellCountHiA Integer32,
                     viprSubChnlStatsOutOdometerCellCountLoA Integer32,
                viprSubChnlStatsInOdometerCellCountHiB  Integer32,
                viprSubChnlStatsInOdometerCellCountLoB  Integer32,
                     viprSubChnlStatsOutOdometerCellCountHiB Integer32,
                     viprSubChnlStatsOutOdometerCellCountLoB Integer32,
                viprSubChnlStatsOdometerTimer                Integer32,
                viprSubChnlStatsOdometerReset                 INTEGER,
                viprSubChnlStatsCellsDroppedB                 Integer32,
                viprSubChnlStatsCellsTaggedB                 Integer32
                }

