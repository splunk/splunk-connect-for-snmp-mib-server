                SYNTAX MsChannelConfigEntry
                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION "Row definition for multi-serial channel config. "
                INDEX { msChannelId }
                ::= { msChannelConfigTable 1 }

MsChannelConfigEntry ::= SEQUENCE
                                 {
                                  msChannelId                        IntfIndex,
                                  msChannelIfType                INTEGER,
                                  msChannelCellBearingMode        INTEGER,
                                  msChannelCellBearingAddrMode  INTEGER,
                                  msChannelCellBearingPayloadFec INTEGER,
                                  msChannelSecuFeature                INTEGER
                                 }

msChannelId        OBJECT-TYPE
                SYNTAX IntfIndex
                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION "interface index - SSPPCCC"
                ::= { msChannelConfigEntry 1 }

msChannelIfType OBJECT-TYPE
                SYNTAX INTEGER {
                                unconfigured(1),
                                atmUni3-0(2),
                                atmUni3-1(3),

                                iispUser(4),
                                iispNetwork(5),

                                circuitEmulation(6),
                                terminalEmulation(7),
                                frameRelayUni(8),
                                hdlcPassThrough(9),
                                frameRelayNni(10),

                                atmPnni1-0(11),

                                atmUni4-0(12)
                               }
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "interface type for multi-serial port"
                DEFVAL { unconfigured }
                ::= { msChannelConfigEntry 2 }

msChannelCellBearingMode OBJECT-TYPE
                SYNTAX INTEGER {
                                lanet(1),
                                ccittDirectMapping(2),
                                none(3)
                               }
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "The cell delineation mode is either LANET or
                                CCITT based"
                DEFVAL { lanet }
                ::= { msChannelConfigEntry 3 }

msChannelCellBearingAddrMode OBJECT-TYPE
                SYNTAX INTEGER {
                                standard(1),
                                noiseTolerant(2)
                               }
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "standard - ATM Forum UNI 3.0/3.1
                             noiseTolerant - A proprietary noise tolerant
                             address mapping scheme"
                DEFVAL { standard }
                ::= { msChannelConfigEntry 4 }

msChannelCellBearingPayloadFec OBJECT-TYPE
                SYNTAX INTEGER {
                                dynamic(1),
                                threeEighthsRate(2),
                                oneQuarterRate(3),
                                oneEighthRate(4)
                               }
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "payload error correction scheme"
                DEFVAL { dynamic }
                ::= { msChannelConfigEntry 5 }

msChannelSecuFeature OBJECT-TYPE
                SYNTAX INTEGER {
                                unsecurable(1),
                                secured(2),
                                securable(3)
                                }
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "enhancement feature"
                DEFVAL { unsecurable }
                ::= { msChannelConfigEntry 6 }

