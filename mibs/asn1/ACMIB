aal2TrunkConfigTable  OBJECT-TYPE
                SYNTAX SEQUENCE OF Aal2TrunkConfigEntry
                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION "Table of terminal emulation to ATM network 
                        IWF connections "
                ::= { connectionConfig 36 }



aal2TrunkConfigEntry  OBJECT-TYPE
                SYNTAX Aal2TrunkConfigEntry
                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION "Row definition of terminal emulation to ATM 
                network IWF connections"
                INDEX {  aal2TrunkConfigIf,
                        aal2TrunkConfigVpi, aal2TrunkConfigVci }
                ::= { aal2TrunkConfigTable 1 }

Aal2TrunkConfigEntry ::= SEQUENCE {
                aal2TrunkConfigIf                IntfIndex,
                aal2TrunkConfigVpi               INTEGER,
                aal2TrunkConfigVci               INTEGER,
                aal2TrunkConfigServiceType       INTEGER,
                aal2TrunkConfigPeakCellRateIgr   Integer32,
                aal2TrunkConfigSusCellRateIgr    Integer32,
                aal2TrunkConfigMaxBurstSizeIgr   Integer32,
                aal2TrunkConfigPeakCellRateEgr   Integer32,
                aal2TrunkConfigSusCellRateEgr    Integer32,
                aal2TrunkConfigMaxBurstSizeEgr   Integer32,
                aal2TrunkConfigFlow              INTEGER,
                aal2TrunkConfigRowStatus         RowStatus,
                aal2TrunkConfigConnectionStatus  INTEGER,
                aal2TrunkConfigViB               INTEGER,
                aal2TrunkConfigCnfrmTypeB2A      INTEGER,

                aal2TrunkConfigVUNIB             INTEGER,

                aal2TrunkAvailBwIgr              INTEGER,
                aal2TrunkAvailBwEgr              INTEGER,
                aal2TrunkGR303                   INTEGER,
                aal2TrunkConfigOamStatus         INTEGER,
                aal2TrunkConnFailureCauseB       AtmConnFailureCause,
                aal2TrunkCuTimer                 INTEGER
                }

aal2TrunkConfigIf OBJECT-TYPE
                SYNTAX IntfIndex
                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION "Interface Id of the Atm Side of the Aal2 Trunk."
                ::= { aal2TrunkConfigEntry 1 }

