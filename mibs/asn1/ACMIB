                SYNTAX INTEGER {
                                eprom(1),
                                current(2),
                                next(3)
                               }
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "Update the fwReleaseCurrentVersion and the
                             fwReleaseNextVersion variables by remapping
                             the firmware files from the CPU disk drive."
                DEFVAL { eprom }
                ::= { fwReleaseEntry 7 }


