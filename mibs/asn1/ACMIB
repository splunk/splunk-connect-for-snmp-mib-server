                SYNTAX IntfIndex
                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION "interface index - SSPPCCC"
                ::= { ds1ChannelConfigEntry 1 }

ds1ChannelIfType OBJECT-TYPE
                SYNTAX INTEGER  {
                                unconfigured(1),
                                atmUni3-0(2),
                                atmUni3-1(3),

                                iispUser(4),
                                iispNetwork(5),

                                circuitEmulation(6),
                                frameRelayUni(7),
                                hdlcPassThrough(8),

                                pri-isdn-network(9),

                                dbCirEm(10),
                                frameRelayNni(11),

                                pri-isdn-user(12),

                                atmIma(13),

                                atmPnni1-0(14),

                                casTrunkLine(15),
                                atmUni4-0(16),
                gr303 (17)
                                }
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "UNI or NNI type for ATM ,Frame Relay, or 
                             ISDN service interface type for channel"
                DEFVAL { unconfigured }
                ::= { ds1ChannelConfigEntry 2 }

