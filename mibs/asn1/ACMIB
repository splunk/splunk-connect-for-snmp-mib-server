                STATUS current
                DESCRIPTION "Calling Tone Detection Algorithm"
                DEFVAL { disabled }
                ::= { gr303Aal2PvcVccEntry 12 }

gr303Aal2PvcVccCodingTranslation OBJECT-TYPE
                SYNTAX INTEGER  {
                                 none(1),
                                 muLaw2muLaw(2),
                                 aLaw2aLaw(3),
                                 muLaw2aLaw(4),
                                 aLaw2muLaw(5)
