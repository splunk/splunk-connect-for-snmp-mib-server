                SYNTAX Integer32
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "received mask reply"
                ::= { viprVpnStatsEntry 19 }

viprVpnStatsIcmpInEchoReps OBJECT-TYPE
                SYNTAX Integer32
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "received echo replies"
                ::= { viprVpnStatsEntry 20 }

viprVpnStatsIcmpInDestUnreachs OBJECT-TYPE
                SYNTAX Integer32
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "received destination unreacheable message"
                ::= { viprVpnStatsEntry 21 }

