        SYNTAX INTEGER  {
                nsap(1),
                e164(2),
                private(3)
                }
        MAX-ACCESS read-write
        STATUS current
        DESCRIPTION "atm address type"
        DEFVAL { nsap }
        ::= { atmVnnConfigEntry 2 }

