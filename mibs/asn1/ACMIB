                SYNTAX IntfIndex
                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION "Interface index for side B, the atm side, of a 
                             circuit emulation-atm PVC VCC connection.  Number 
                             value is interpreted as an interface of the form, 
                             SSPPCCC, where SS is the card slot, PP is the port
                 number, and CCC is the channel number."
                ::= { gr303Aal2PvcVccEntry 3 }

gr303Aal2PvcVccVpiB OBJECT-TYPE
                SYNTAX INTEGER (0..4095)
                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION "VPI value for side B, the atm side,  of a
                             circuit emulation-atm PVC VCC connection.  Valid 
                             range is a number between 0 and 4095."
                ::= { gr303Aal2PvcVccEntry 4 }

gr303Aal2PvcVccVciB OBJECT-TYPE
                SYNTAX INTEGER (0..65535)
                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION "VCI value for side B, the atm side,  of a
                             circuit emulation-atm PVC VCC connection.  Valid 
                             range is a number between 0 and 65535."
                ::= { gr303Aal2PvcVccEntry 5 }

gr303Aal2PvcVccAal2Cid OBJECT-TYPE
                SYNTAX INTEGER (0..255)
                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION "Aal2 Cid Used to pack ce chan in an
                        Atm Trunk. This value will be unique
                        per Atm Trunk. This field will be applicable
                        when Aal type is chosen to be Standard Aal2."
                DEFVAL { 0 }
                ::= { gr303Aal2PvcVccEntry 6 }

gr303Aal2PvcVccRowStatus OBJECT-TYPE
                SYNTAX RowStatus
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "row status"
                ::= { gr303Aal2PvcVccEntry 7 }

gr303Aal2PvcVccVoiceEncodingProfile OBJECT-TYPE
                SYNTAX INTEGER  {
                                itut-profile-1(1),
                                itut-profile-3(3),
                                af-profile-6(6),
                                itut-profile-7(7),
                                af-profile-7(8),
                                af-profile-8(9),
                                af-profile-9(10),
                                af-profile-10(11),
                                af-profile-12(12),
                                custom(256),
                                default (257)
                                }
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "Profile for use by voice/voiceband data.
                     Profile-3 and Profile-7 are ITU-T predefined profiles
                     which specify the voice compression and silence removal
                     algorithms. If custom profile is chosen all DSP specific 
                     parameters have to be specified by the user. If default
                     profile is chosen all parameters on the  AAL2
                     trunk, containing this CID, will be used." 
                DEFVAL { itut-profile-1 }
                ::= { gr303Aal2PvcVccEntry 8 }

gr303Aal2PvcVccVoiceCompression OBJECT-TYPE
                SYNTAX INTEGER  {
                                none(1),
                                g726-16k(2),
                                g726-24k(3),
                                g726-32k(4),
                                g726-40k(5),
                                g729a-8k(6)
                                }
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "Voice Compression Algorithm"
                DEFVAL { none }
                ::= { gr303Aal2PvcVccEntry 9 }

