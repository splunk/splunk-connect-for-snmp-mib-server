atmPvcVpcStatsEntry OBJECT-TYPE
                SYNTAX AtmPvcVpcStatsEntry
                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION "Row definition for the connection statistics
                             table"
                INDEX { atmPvcVpcStatsIfA, atmPvcVpcStatsVpiA,
                        atmPvcVpcStatsIfB, atmPvcVpcStatsVpiB }
                ::= { atmPvcVpcStatsTable 1}

AtmPvcVpcStatsEntry ::= SEQUENCE {
                atmPvcVpcStatsIfA                         IntfIndex,
                atmPvcVpcStatsVpiA                         INTEGER,
                atmPvcVpcStatsIfB                         IntfIndex,
                atmPvcVpcStatsVpiB                         INTEGER,
                atmPvcVpcStatsInCellCountHiA                Integer32,
                atmPvcVpcStatsInCellCountLoA                Integer32,
                atmPvcVpcStatsOutCellCountHiA                 Integer32,
                atmPvcVpcStatsOutCellCountLoA                 Integer32,
                atmPvcVpcStatsInCellCountHiB                Integer32,
                atmPvcVpcStatsInCellCountLoB                Integer32,
                atmPvcVpcStatsOutCellCountHiB                 Integer32,
                atmPvcVpcStatsOutCellCountLoB                 Integer32,
                atmPvcVpcStatsTimer                        Integer32,
                atmPvcVpcStatsInOdometerCellCountHiA        Integer32,
                atmPvcVpcStatsInOdometerCellCountLoA        Integer32,
                atmPvcVpcStatsOutOdometerCellCountHiA         Integer32,
                atmPvcVpcStatsOutOdometerCellCountLoA         Integer32,
                atmPvcVpcStatsInOdometerCellCountHiB        Integer32,
                atmPvcVpcStatsInOdometerCellCountLoB        Integer32,
                atmPvcVpcStatsOutOdometerCellCountHiB         Integer32,
                atmPvcVpcStatsOutOdometerCellCountLoB         Integer32,
                atmPvcVpcStatsOdometerTimer                Integer32,
                atmPvcVpcStatsOdometerReset                INTEGER,
                atmPvcVpcStatsOamAisTxA                        Integer32,
                atmPvcVpcStatsOamAisRxA                        Integer32,
                atmPvcVpcStatsOamRdiTxA                        Integer32,
                atmPvcVpcStatsOamRdiRxA                        Integer32,
                atmPvcVpcStatsOamAisTxB                        Integer32,
                atmPvcVpcStatsOamAisRxB                        Integer32,
                atmPvcVpcStatsOamRdiTxB                        Integer32,
                atmPvcVpcStatsOamRdiRxB                        Integer32,
                atmPvcVpcStatsCellsDroppedA             Integer32,
                atmPvcVpcStatsCellsTaggedA              Integer32,
                atmPvcVpcStatsCellsDroppedB             Integer32,
                atmPvcVpcStatsCellsTaggedB                   Integer32
                }

