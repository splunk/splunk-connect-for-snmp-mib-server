gr303Aal2PvcVccTable OBJECT-TYPE
                SYNTAX SEQUENCE OF Gr303Aal2PvcVccEntry
                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION "Table of Gr303 to AAL2 Cid monitor VCCs"

                ::= { connectionConfig 37 }

gr303Aal2PvcVccEntry OBJECT-TYPE
                SYNTAX Gr303Aal2PvcVccEntry
                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION "Row definition of circuit emulation to atm
                              connections."
                INDEX { gr303Aal2PvcVccGr303IgId,  gr303Aal2PvcVccGr303CallRef,
                gr303Aal2PvcVccIfB, gr303Aal2PvcVccVpiB, 
                gr303Aal2PvcVccVciB, gr303Aal2PvcVccAal2Cid}
                ::= { gr303Aal2PvcVccTable 1 }

Gr303Aal2PvcVccEntry ::= SEQUENCE {
                             gr303Aal2PvcVccGr303IgId                 INTEGER,
                             gr303Aal2PvcVccGr303CallRef        INTEGER,
                             gr303Aal2PvcVccIfB                        IntfIndex,
                             gr303Aal2PvcVccVpiB                  INTEGER,
                             gr303Aal2PvcVccVciB                  INTEGER,
                             gr303Aal2PvcVccAal2Cid                INTEGER,
                             gr303Aal2PvcVccRowStatus                RowStatus,
                 gr303Aal2PvcVccVoiceEncodingProfile INTEGER,
                 gr303Aal2PvcVccVoiceCompression INTEGER,
                 gr303Aal2PvcVccSilenceDetection INTEGER,
                 gr303Aal2PvcVccEchoCancellation INTEGER,
                 gr303Aal2PvcVccCngToneDetection INTEGER,
                 gr303Aal2PvcVccCodingTranslation INTEGER,
                             gr303Aal2PvcVccConnectionStatus        INTEGER,
                 gr303Aal2PvcVccAltLineType         INTEGER
                             }

