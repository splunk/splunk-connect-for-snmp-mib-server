-- Virtual UNI ILMI statistics table

vuniIlmiStatsTable        OBJECT-TYPE
                SYNTAX SEQUENCE OF VuniIlmiStatsEntry
                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION "Table of statistics for an ILMI enabled interface."
                ::= { intfStatistics 12 }

vuniIlmiStatsEntry        OBJECT-TYPE
                SYNTAX VuniIlmiStatsEntry
                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION "Row definition for the ILMI statistics table."
                INDEX { vuniIlmiStatsId, vuniIlmiVUNIId }
                ::= { vuniIlmiStatsTable 1 }

VuniIlmiStatsEntry        ::= SEQUENCE {
                vuniIlmiStatsId                        IntfIndex,
                vuniIlmiVUNIId        INTEGER,
                vuniIlmiStatsTimer                        Integer32,
                vuniIlmiStatsLinkResets                Integer32,
                vuniIlmiStatsAttachPtChanged        Integer32,
                vuniIlmiStatsAttachPtVerified        Integer32,
                vuniIlmiStatsSecureLnkProcInit        Integer32,
                vuniIlmiStatsColdStartRx                Integer32,
                vuniIlmiStatsColdStartTx                Integer32,
                vuniIlmiStatsPduRx                        Integer32,
                vuniIlmiStatsPduTx                        Integer32,
                vuniIlmiStatsReqTimeouts                Integer32,
                vuniIlmiStatsPacketDisc                Integer32,
        vuniResetIlmiStatistics     INTEGER
                }

vuniIlmiStatsId        OBJECT-TYPE
                SYNTAX IntfIndex
                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION "Interface index (SSPPCCC) for the ILMI interface."
                ::= { vuniIlmiStatsEntry 1 }

vuniIlmiVUNIId        OBJECT-TYPE
                SYNTAX INTEGER (1..255)
                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION "Virtual UNI number, second index to access the ILMI on
                     virtual UNI interface."
                DEFVAL { 1 }
                ::= { vuniIlmiStatsEntry 2 }

vuniIlmiStatsTimer        OBJECT-TYPE
                SYNTAX Integer32
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "Time, in seconds, since last statistics reset."
                ::= { vuniIlmiStatsEntry 3 }

vuniIlmiStatsLinkResets OBJECT-TYPE
                SYNTAX Integer32
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "Times connectivity has toggled between down and 
                             up."
                ::= { vuniIlmiStatsEntry 4 }

vuniIlmiStatsAttachPtChanged OBJECT-TYPE
                SYNTAX Integer32
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "Times Attachment Point change was detected."
                ::= { vuniIlmiStatsEntry 5 }

vuniIlmiStatsAttachPtVerified OBJECT-TYPE
                SYNTAX Integer32
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "Times Attachment Point was verified successfully."
                ::= { vuniIlmiStatsEntry 6 }

vuniIlmiStatsSecureLnkProcInit OBJECT-TYPE
                SYNTAX Integer32
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "Times Secure Link Procedure was initiated."
                ::= { vuniIlmiStatsEntry 7 }

vuniIlmiStatsColdStartRx OBJECT-TYPE
                SYNTAX Integer32
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "Times Cold Start trap was received."
                ::= { vuniIlmiStatsEntry 8 }

vuniIlmiStatsColdStartTx OBJECT-TYPE
                SYNTAX Integer32
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "Times Cold Start trap was transmitted."
                ::= { vuniIlmiStatsEntry 9 }

vuniIlmiStatsPduRx OBJECT-TYPE
                SYNTAX Integer32
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "vuniIlmi pdu received"
                ::= { vuniIlmiStatsEntry 10 }

vuniIlmiStatsPduTx OBJECT-TYPE
                SYNTAX Integer32
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "vuniIlmi pdu transmitted"
                ::= { vuniIlmiStatsEntry 11 }

vuniIlmiStatsReqTimeouts OBJECT-TYPE
                SYNTAX Integer32
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "Times getRequest of any type timed out."
                ::= { vuniIlmiStatsEntry 12 }

vuniIlmiStatsPacketDisc OBJECT-TYPE
                SYNTAX Integer32
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "Times a received packet was discarded."
                ::= { vuniIlmiStatsEntry 13 }

vuniResetIlmiStatistics OBJECT-TYPE
                SYNTAX INTEGER
        {
          noAction (1),
          reset    (2)
        }
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "Reset the ATM ILMI statistics counters for 
                             the given channel Id (SSPPCCC) and vuni Id"
        DEFVAL { noAction }
                ::= { vuniIlmiStatsEntry 14}



-- Connection Statistics Group connectionStatistics 

