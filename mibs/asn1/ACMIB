                SYNTAX INTEGER  { 
                                none(1),
                                aal5(2),                                        
                                aal2Std(3)
                                }
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "This field will be used to connect hdlc
                             to atm directly when sar type is none or aal5 or
                             go through dsp2 first when sar type is aal2Std."
                DEFVAL { aal5 }
