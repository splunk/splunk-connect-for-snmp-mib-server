                DEFVAL { baud300 }
                ::= { msPortConfigEntry 3 }

msProtocol        OBJECT-TYPE
                SYNTAX INTEGER {
                                rs232(1),
                                rs449(2),
                                rs530(3),
                                v35(4),
                                x21(5),
                                kg(6),
                                rs530A(7)
                               }
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "protocol type for serial port"
                DEFVAL { rs449 }
                ::= { msPortConfigEntry 4 }

msEquipmentType      OBJECT-TYPE
                SYNTAX INTEGER {
                                dte(1),
                                dce(2)
                               }
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "Equipment type"
                DEFVAL { dte }
                ::= { msPortConfigEntry 5 }

msRs232XmitMode OBJECT-TYPE
                SYNTAX INTEGER {
                                synchronous(1),
                                asynchronous(2)
                               }
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "transmit mode for rs232 DTE/DCE serial port"
                DEFVAL { synchronous }
                ::= { msPortConfigEntry 6 }

msRs232Parity        OBJECT-TYPE
                SYNTAX INTEGER {
                                none(1),
                                odd(2),
                                even(3)
                               }
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "parity for rs232 DTE/DCE serial port"
                DEFVAL { none }
                ::= { msPortConfigEntry 7 }

msRs232DataBits OBJECT-TYPE
                SYNTAX INTEGER {
                                eight(1),
                                seven(2),
                                six(3)
                               }
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "data bits for rs232 DTE/DCE serial port"
                DEFVAL { eight }
                ::= { msPortConfigEntry 8 }

msRs232StopBits OBJECT-TYPE
                SYNTAX INTEGER {
                                one(1),
                                two(2)
                               }
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "stop bits for rs232 DTE/DCE serial port"
                DEFVAL { one }
                ::= { msPortConfigEntry 9 }

msTxDataClkSrcForDte OBJECT-TYPE
                SYNTAX INTEGER {
                                localTiming(1),
                                txRxTiming(2),
                                txTiming(3)
                               }
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "Transmit data clock source for DTE,
                             externalTx in is not valid for rs232.
                             This field is ignored if the local side is DCE."
                DEFVAL { localTiming }
                ::= { msPortConfigEntry 10 }

msTxDataClkSrcForDce OBJECT-TYPE
                SYNTAX INTEGER {
                                localTiming(1),
                                loopTiming(2)
                               }
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "Transmit data clock source for DCE
                             localTiming: a local clock source is used. (e.g., from 
                             backplane (stratum)
                             loopTiming: from remote
                             This field is ignored if the local side is DTE"
                DEFVAL { localTiming }
                ::= { msPortConfigEntry 11 }

msRxDataClkSrcForDce OBJECT-TYPE
                SYNTAX INTEGER {
                                localTiming(1),
                                externalTiming(2)
                               }
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "Receive data clock source for DCE
                             localTiming: a local clock source is used. (e.g., from
                             backplane (stratum)
                             loopTiming: from remote
                             This field is ignored if the local side is DTE"
                DEFVAL { localTiming }
                ::= { msPortConfigEntry 12 }


msRxClkPolarity OBJECT-TYPE
                SYNTAX INTEGER {
                                standard(1),
                                inverted (2)
                               }
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "Whether or not to reverse the receive clock"
                DEFVAL { standard }
                ::= { msPortConfigEntry 13 }

msTxClkPolarity OBJECT-TYPE
                SYNTAX INTEGER {
                                standard(1),
                                inverted (2)
                               }
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "Whether or not to reverse the transmit clock"
                DEFVAL { standard }
                ::= { msPortConfigEntry 14 }

msKgResync        OBJECT-TYPE
                SYNTAX INTEGER {
                                enabled(1),
                                disabled(2)
                               }
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "Turn KG resync on/off"
                DEFVAL { enabled }
                ::= { msPortConfigEntry 15 }

msHandshake        OBJECT-TYPE
                SYNTAX INTEGER {
                                observed(1),
                                ignored(2)
                               }
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION " Multiserial Module handshake:
                              observed : used with AAL1 to monitor
                                        RTS/CTS exchange."
                DEFVAL { ignored }
                ::= { msPortConfigEntry 16 }

msSpeedAdaption OBJECT-TYPE
                SYNTAX INTEGER {
                                enabled(1),
                                disabled(2)
                               }
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "The state of allowing speed adaption"
                DEFVAL { enabled }
                ::= { msPortConfigEntry 17 }

msBitStuffing OBJECT-TYPE
                SYNTAX INTEGER {
                                disabled(1),
                                enabled(2)
                                }
                MAX-ACCESS read-write
                STATUS current
                 DESCRIPTION "When msBitStuffing is enabled, 
                        CAC throughput is (1 + 1/7) user throughtput.
                  Otherwise the CAC throughput is the same as the user throughput. 
                  msBitStuffing can only be enabled for circuit emulation interfaces.
                        " 
                DEFVAL { disabled }
                ::= { msPortConfigEntry 18 }

msLineStatus  OBJECT-TYPE
                SYNTAX INTEGER (1..2)
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "line status on the port:
                        The msLineStatus is a bit map representation.
                        It can represent multiple failures (alarms) 
                        if supported by the card.  
                        
                        NoAlarm should be set if and only if 
                                no other flag is set.

                        The various bit positions are:
                        1: NoAlarm
                        2: Loss of Signal  "
                DEFVAL { 1 }
                ::= { msPortConfigEntry 19 }


msRxDataPolarity OBJECT-TYPE
                SYNTAX INTEGER {
                                standard(1),
                                inverted (2)
                                }
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "Whether or not to reverse the receive data"
                DEFVAL { standard }
                ::= { msPortConfigEntry 20}

msTxDataPolarity OBJECT-TYPE
                SYNTAX INTEGER {
                                standard(1),
                                inverted (2)
                                }
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "Whether or not to reverse the transmit data"
                DEFVAL { standard }
                ::= { msPortConfigEntry 21 }

msLoopbackConfig OBJECT-TYPE
                SYNTAX INTEGER  {
                                noLoop(1),
                                localLoop(2),
                                lineLoop(3)
                                }
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "This variable represents the loopback 
                               configuration of the Serial port."
                DEFVAL { noLoop }
                ::= { msPortConfigEntry 22 }

msLOSDetection OBJECT-TYPE
                SYNTAX INTEGER {
                          disabled(1),
                        enabled(2)        
                                }
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "State of detection of LOS by IO card. 
         LOS detection is always enabled when msBitStuffing is enabled. 
               When the interface is not circuit emulation, the IO card does not report any LOS."
                DEFVAL { disabled }
                ::= { msPortConfigEntry 23 } 

             

ds1PortConfigTable  OBJECT-TYPE
                SYNTAX SEQUENCE OF Ds1PortConfigEntry
                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION "Table of ds1 specific port attributes"
                ::= { equipConfig 5 }

ds1PortConfigEntry  OBJECT-TYPE
                SYNTAX Ds1PortConfigEntry
                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION "Row definition for ds1 specific
                             port attributes"
                INDEX { ds1PortId }
                ::= { ds1PortConfigTable 1 }

Ds1PortConfigEntry  ::= SEQUENCE {
                ds1PortId               Integer32,
                ds1Channelization       INTEGER,
                ds1LineType             INTEGER,
                ds1LineCoding           INTEGER,
                ds1LoopbackConfig       INTEGER,
                ds1TransmitClockSource  INTEGER,
                ds1LineBuildOut         INTEGER,
                ds1LineStatus           INTEGER,

                ds1EnhanceFeatureSet1   INTEGER,

                ds1DataTransferType     INTEGER,
                ds1OamInterworkingType  INTEGER,

                ds1ConnGatewayFeature   INTEGER,

                ds1InLineCode           INTEGER
                }
 
ds1PortId       OBJECT-TYPE
                SYNTAX Integer32 
                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION "interface index - slot/port: SSPP"
                ::= { ds1PortConfigEntry 1 }
 
ds1Channelization        OBJECT-TYPE
                SYNTAX INTEGER  {
                                 disabled(1),
                                 enabled(2)
                                }
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "ds1 interface type"
                DEFVAL { disabled }
                ::= { ds1PortConfigEntry 2 }
 
ds1LineType     OBJECT-TYPE
                SYNTAX INTEGER  {
                                esf(1),
                                d4(2)
                                }
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "ds1 interface type"
                DEFVAL { esf }
                ::= { ds1PortConfigEntry 3 }
 
ds1LineCoding   OBJECT-TYPE
                SYNTAX INTEGER  {
                                b8zs(1),
                                ami(2)
                                }
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "ds1 interface type"
                DEFVAL { b8zs }
                ::= { ds1PortConfigEntry 4 }
   
ds1LoopbackConfig       OBJECT-TYPE
                SYNTAX INTEGER  {
                                noLoop(1),
                                localLoop(2),
                                lineLoop(3),
                                payloadLoop(4)
                                }
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "This variable represents the loopback
                               configuration of the ds1 interface"
                DEFVAL { noLoop }
                ::= { ds1PortConfigEntry 5 }
 
ds1TransmitClockSource  OBJECT-TYPE
                SYNTAX INTEGER  {
                                loopTiming(1),
                                localTiming(2),
                                adaptive(3)
                                }
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "ds1 timing source"
                DEFVAL { localTiming }
                ::= { ds1PortConfigEntry 6 }

ds1LineBuildOut   OBJECT-TYPE
                SYNTAX INTEGER  {
                                upTo133Feet(1),
                                upTo266Feet(2),
                                upTo399Feet(3),
                                upTo533Feet(4),
                                upTo655Feet(5),
                                negative7-5Db(6),
                                negative15Db(7),
                                negative22-5Db(8)
                                }
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "ds1 line build out"
                ::= { ds1PortConfigEntry 7 }

ds1LineStatus  OBJECT-TYPE
                SYNTAX INTEGER (1..16383)
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "This variable indicates the Line Status of the
                             interface. It contains loopback, failure, re-
                             ceived 'alarm' and transmitted 'alarm' infor-
                             mation.

                                  The dsx1LineStatus is a bit map represented  
                             as a sum, therefore, it can represent multiple 
                             failures (alarms) and a LoopbackState 
                             simultaneously.

                                  ds1NoAlarm should be set if and only if no other 
                             flag is set.

                                  If the ds1LoopbackState bit is set, the loopbak
                             in effect can be determined from the 
                             ds1LoopbackConfig object.

                     The various bit positions are:
                  1:       dsx1NoAlarm:          No Alarm Present
                  2:       dsx1RcvFarEndLOF:     Far end LOF (a.k.a., Yellow 
                                               Alarm)
                  4:       dsx1XmtFarEndLOF:     Near end sending LOF Indication
                  8:       dsx1RcvAIS:           Far end sending AIS
                 16:      dsx1XmtAIS:           Near end sending AIS
                 32:      dsx1LossOfFrame:      Near end LOF (a.k.a., Red Alarm)
                 64:      dsx1LossOfSignal:     Near end Loss Of Signal
                128:     dsx1LoopbackState:    Near end is looped
                256:     dsx1T16AIS:           E1 TS16 AIS
                512:     dsx1RcvFarEndLOMF:    Far End Sending TS16 LOMF
                       1024:    dsx1XmtFarEndLOMF:    Near End Sending TS16 LOMF
                       2048:    dsx1RcvTestCode:      Near End detects a test code
                       4096:    dsx1OtherFailure:     any line status not defined 
                                               here
                       8192:    dsx1RmtLoopback:      Far end loopback"
                DEFVAL { 1 }
                ::= { ds1PortConfigEntry 8 }





--        Object added for NEC
ds1EnhanceFeatureSet1 OBJECT-TYPE
                SYNTAX INTEGER  {
                                disabled(1),
                                enabled(2)
                                }
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "To enable/disable a proprietary ab signaling
                             scheme, it applies to all DS0s in the port.  No
                             individual DS0 signaling can be configured under
                             this scheme."
                DEFVAL { disabled }
                ::= { ds1PortConfigEntry 10 }


ds1DataTransferType OBJECT-TYPE
                SYNTAX INTEGER  {
                                structured(1),
                                unstructured(2)
                                }
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "DS1 data transfer type is only applicable in 
                             Circuit Emulation where unstructured CE packets 
                             entire T1 frame including the F-bit into the AAL1
                             for data tranfer, whereas, the structured CE 
                             packets the entire T1 frame except the F-bit."
                DEFVAL { structured }
                ::= { ds1PortConfigEntry 11 }
 

ds1OamInterworkingType OBJECT-TYPE
                SYNTAX INTEGER {
                        trunkCond(1),
                        unframed1s(2)
                        }
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "Specifies whether the interworking is done 
                             by trunk conditioning or unframed1s"
                DEFVAL {trunkCond}
                ::= {ds1PortConfigEntry 12 }


ds1ConnGatewayFeature OBJECT-TYPE
                SYNTAX INTEGER  {
                                disabled(1),
                                enabled(2)
                                }
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "To enable/disable connection gateway
                                (Sapphire) API."
                 DEFVAL { disabled }
                ::= { ds1PortConfigEntry 14 }


ds1InLineCode    OBJECT-TYPE
                SYNTAX INTEGER  {
                                aNSI(1),
                                ds1-ANI(2)
                                }
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "To select and support the inlinecode option
                                in either ANSI or DS1_ANI
                                (ANI: Access Network Interface)"
                DEFVAL { aNSI }
                ::= { ds1PortConfigEntry 15 }

oc3PortConfigTable OBJECT-TYPE
                SYNTAX SEQUENCE OF Oc3PortConfigEntry
                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION "Table of common physical Port attributes"
                ::= { equipConfig 6 }
 
oc3PortConfigEntry OBJECT-TYPE
                SYNTAX Oc3PortConfigEntry
                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION "Row definition for common physical
                             port attributes"
                INDEX { oc3PortId }
                ::= { oc3PortConfigTable 1 }
 
Oc3PortConfigEntry ::= SEQUENCE {
                oc3PortId               Integer32,
                oc3LoopbackConfig        INTEGER,
                oc3TransmitClockSource        INTEGER,
                oc3LineStatus                INTEGER,
                oc3OutputTransferRate        INTEGER,
                oc3APSConfiguration             INTEGER
                }
 
