                SYNTAX IpAddress
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "router ATM IP Address."
                ::= { routerConfigEntry 4 }
 
routerIpNetworkMask OBJECT-TYPE
                SYNTAX IpAddress
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION " An IP network mask for an IP interface 
                              within a given router (VPN)"
                ::= { routerConfigEntry 5 }
 
routerNumber        OBJECT-TYPE
                SYNTAX INTEGER (0..999)
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION " A scalar number which identifies 
                              a given router (VPN)"
                DEFVAL { 0 }
                ::= { routerConfigEntry 6 }

