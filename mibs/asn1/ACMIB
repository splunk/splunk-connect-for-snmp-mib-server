h248MaxNumberOfContexts OBJECT-TYPE 
                SYNTAX Integer32 
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "The value of the property gives the 
                        maximum number of contexts that can exist at
                        anytime.  The NULL context is not included 
                        in this number."
                DEFVAL { 1000 }
                ::= {  h248SysControl 1 }

h248MaxTerminationsPerContext OBJECT-TYPE 
                SYNTAX Integer32
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "The max number of terminations allowed
                        in a context."
                     DEFVAL { 2 }
                ::= { h248SysControl 2 }

h248NormalMGExecutionTime OBJECT-TYPE 
                SYNTAX Integer32
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "Settable by the MGC to indicate the 
                        interval within which the MGC expects
                        a response to any transaction from the MG
                        (exclusive of the network delay). Value
                        represents milliseconds."
                DEFVAL { 1000 }
                ::= { h248SysControl 3 }

h248NormalMGCExecutionTime OBJECT-TYPE 
                SYNTAX Integer32
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "Settable by the MGC to indicate the
                        interval within which the MG expects
                        a response to any transaction from the MG
                        (exclusive of the network delay). Value
                        represents milliseconds."
                     DEFVAL { 1000 }
                ::= { h248SysControl 4 }

h248MGProvisionalResponseTimerValue OBJECT-TYPE 
                SYNTAX Integer32
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "Indicates the time within which the MGC should expect
                        a Pending Response from the MG if a Transaction can not be
                        completed. Initially set to h248NormalMGExecutionTime,
			plus network delay, but maybe lowered. Value represents
			milliseconds."
                     DEFVAL { 1000 }
                ::= { h248SysControl 5 }

h248MGCProvisionalResponseTimerValue OBJECT-TYPE 
                SYNTAX Integer32
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "Indicates the time within which the MG should expect
                        a Pending Response from the MGC if a Transaction can not be
                        completed. Initially set to h248NormalMGCExecutionTime,
			plus network delay, but maybe lowered. Value represents
			milliseconds."
                     DEFVAL { 1000 }
                ::= { h248SysControl 6 }

h248MGCIpAddress1 OBJECT-TYPE 
                SYNTAX IpAddress
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "The IP address of the 1st MGC."
                ::= { h248SysControl 7 }

h248MGCIpAddress2 OBJECT-TYPE 
                SYNTAX IpAddress
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "The IP address of the 2nd MGC."
                ::= { h248SysControl 8 }



-- Protection Group protectionGroup

protectionGroupTable OBJECT-TYPE
                SYNTAX SEQUENCE OF ProtectionGroupEntry
                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION "Table of interface protection group
                           attributes"
                 ::= { protectionGroup 1 }

protectionGroupEntry OBJECT-TYPE
                 SYNTAX ProtectionGroupEntry
                 MAX-ACCESS not-accessible
                 STATUS current
                 DESCRIPTION "Defines one interface protection group"
                 INDEX { pgtGroupId }
                 ::= { protectionGroupTable 1 }

ProtectionGroupEntry ::= SEQUENCE {
                 pgtGroupId         Integer32,
                 pgtSwitchoverMethod      INTEGER,
                       pgtSwitchbackMethod      INTEGER,
                 pgtStandbySelection      INTEGER,
                pgtRowStatus             RowStatus
                        }

pgtGroupId    OBJECT-TYPE
                SYNTAX Integer32
                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION "a user selected logical number representing
                            a protection  group uniquely within a PSAX"
                ::= { protectionGroupEntry 1 }

pgtSwitchoverMethod OBJECT-TYPE
                SYNTAX INTEGER  {
                           auto(1),
                           manual(2)
                              }
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION " switchover method to switch traffic from
                failed primary interface to available standby interface. In
                auto method, PSAX automatically switches the traffic
                without user   intervention. In manual method, a failed
                primary does not switch   traffic and gives control to user
                to do so."
                DEFVAL { auto }
                ::= { protectionGroupEntry 2 }

pgtSwitchbackMethod OBJECT-TYPE
                    SYNTAX INTEGER  {
                           auto(1),
                           manual(2)
                                 }
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION " switchback method to switch traffic
                from a standby to its corresponding recovered primary
                interface. In auto method, PSAX automatically
                switches the traffic without user intervention. In
                manual method, standby does not switch traffic and
                gives control to user to do so."
                DEFVAL { auto }
                ::= { protectionGroupEntry 3 }

pgtStandbySelection OBJECT-TYPE
                SYNTAX INTEGER  {
                          sequential(1),
                          latest-used(2),
                          oldest-used(3),
                          random(4),
                          explicit(5)
                      }
                 MAX-ACCESS read-write
                 STATUS current
                 DESCRIPTION "Standby selection method determines the
                 order of selection of    standby when primary interface
                 fails. The options are:
                 sequential -- internally select next higher available
                 standby to the last one,
                 latest_used -- used last one freed,
                 oldest_used -- used to longest idle one,  
                                  random \255 randomly select one,
                 explicit -- selected by user."
                   DEFVAL { sequential }
                   ::= { protectionGroupEntry 4 }

pgtRowStatus    OBJECT-TYPE
                SYNTAX RowStatus
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "row status"
                ::= { protectionGroupEntry  5 }

interfaceProtectionTable OBJECT-TYPE
                 SYNTAX SEQUENCE OF InterfaceProtectionEntry
                 MAX-ACCESS not-accessible
                 STATUS current
                 DESCRIPTION "Table of interface protection  attributes"
                 ::= { protectionGroup 2 }

interfaceProtectionEntry OBJECT-TYPE
                SYNTAX InterfaceProtectionEntry
                MAX-ACCESS not-accessible
                STATUS current
                       DESCRIPTION "Defines attributes for interface protection"
                      INDEX  { iptGroupId ,iptLogicalIntfId }
                ::= { interfaceProtectionTable 1 }

InterfaceProtectionEntry ::= SEQUENCE {
                iptGroupId                Integer32,
                iptLogicalIntfId        Integer32,
                iptPhysicalIntfId        Integer32,
                iptOperState                INTEGER,
                iptIntfType                INTEGER,
                iptOperatingIntf        Integer32,
                iptProtCommand                INTEGER,
                iptSelectedStandby          Integer32,
                iptRowStatus                RowStatus          
                  }

iptGroupId       OBJECT-TYPE
                 SYNTAX Integer32
                 MAX-ACCESS not-accessible
                 STATUS current
                 DESCRIPTION "a user selected logical number representing
                              a protection group uniquely within a PSAX."
                 ::= { interfaceProtectionEntry 1 }

iptLogicalIntfId        OBJECT-TYPE
                        SYNTAX  Integer32
                        MAX-ACCESS not-accessible
                        STATUS current
                        DESCRIPTION " Logical interface number assigned by user"
                        ::= { interfaceProtectionEntry 2 }

iptPhysicalIntfId        OBJECT-TYPE
                 SYNTAX  Integer32
                 MAX-ACCESS read-write
                 STATUS current
                 DESCRIPTION "physical interface in SSPPCCC format"
                 DEFVAL { 0 }
                 ::= { interfaceProtectionEntry 3 }

iptIntfType        OBJECT-TYPE
                 SYNTAX INTEGER  {
                          primary(1),
                          secondary(2)
                                    }
                 MAX-ACCESS read-write
                 STATUS current
                 DESCRIPTION "interface primary or secondary."
                 DEFVAL { primary }
                 ::= { interfaceProtectionEntry 4 }

iptOperState        OBJECT-TYPE
                SYNTAX INTEGER  {
                          inactive(1),
                          active(2)
                            }
                 MAX-ACCESS read-only
                 STATUS current
                 DESCRIPTION "indicates operational state of the interface."
                 DEFVAL { inactive }
                 ::= { interfaceProtectionEntry 5 }

iptOperatingIntf        OBJECT-TYPE
                SYNTAX Integer32
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "protection operational state of the interface.
                Indicates which logical interface is carrying traffic originally
                configured  for this interface."
                ::= { interfaceProtectionEntry 6 }

iptProtCommand        OBJECT-TYPE
                 SYNTAX INTEGER  {
                        none(1),
                        switchover(2),
                         switchback(3)
                      }
                 MAX-ACCESS read-write
                 STATUS current
                 DESCRIPTION "protection operational state of the interface.
                 Indicates    which logical interface is carrying traffic
                 originally configured  for this interface."
                 DEFVAL{ none }
                 ::= { interfaceProtectionEntry 7 }

iptSelectedStandby        OBJECT-TYPE
                 SYNTAX Integer32
                 MAX-ACCESS read-write
                 STATUS current
                 DESCRIPTION "Standby interface logical number on which to
                 switch traffic. Used only in case of manual switchover
                 with explicit standby  selection."
                 DEFVAL { 0 }
                  ::= { interfaceProtectionEntry 8 }


iptRowStatus    OBJECT-TYPE
                SYNTAX RowStatus
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "row status"
                ::= { interfaceProtectionEntry  9 }


-- End of protection Group 




-- Traps Group sysTraps 

-- systemEvents

systemColdStartNotify NOTIFICATION-TYPE
        STATUS        current
        DESCRIPTION "Info : System has successfully completed initialization
                     from a complete power down stage and is ready for 
                     operation."
        ::= { systemEvents 1 }

systemWarmStartNotify NOTIFICATION-TYPE
        STATUS current
        DESCRIPTION "Info : System has successfully completed initialization 
                     from a user or other system level interrupt or restart and 
                     is ready for operation."
        ::= { systemEvents 2 }

ecdBootFailureNotify NOTIFICATION-TYPE
        STATUS current
        DESCRIPTION "Critical : System failed in the Boot (startup) procedures.
                     Please call Support immediately to resolve the problem."
        ::= { systemEvents 3 }

referenceClockFailNotify NOTIFICATION-TYPE
        STATUS current
        DESCRIPTION "Critical : The reference clock used for the operation of 
                     the bus-based backplane has failed.  Please call Support 
                     immediately to resolve the problem."
        ::= { systemEvents 4 }

referenceClockClearedNotify NOTIFICATION-TYPE
        STATUS current
        DESCRIPTION "Info : The error in the backplane reference clock has 
                     been corrected."
        ::= { systemEvents 5 }

compositeClockFailNotify NOTIFICATION-TYPE
        STATUS current
        DESCRIPTION "Critical : The composite clock has failed.  Please call 
                     Support immediately to resolve the problem."
        ::= { systemEvents 6 }

compositeClockClearedNotify NOTIFICATION-TYPE
        STATUS current
        DESCRIPTION "Info : The error in the composite clock has been 
                     corrected."
        ::= { systemEvents 7 }

stratumModeChangeNotify NOTIFICATION-TYPE
             OBJECTS { stratumMode }
             STATUS current
        DESCRIPTION "Info : The current operational status of the Stratum card
                     has changed."
        ::= { systemEvents 8 }

powerSupplyStatusNotify NOTIFICATION-TYPE
             OBJECTS { powerSupplyReasonCode }
             STATUS current
        DESCRIPTION "Minor : Indicates the status of the power supply."
        ::= { systemEvents 9 }

softwareDownloadStatusNotify NOTIFICATION-TYPE
        OBJECTS { upgradeSwCopyStatus, upgradeSwErrorStatus }
        STATUS current
        DESCRIPTION  "Info : Indicates the completion status of the ftp 
                      download of a software upgrade."
        ::= { systemEvents 10 }


--        Notifications added for NEC
muxReadyConfirmReceivedNotify NOTIFICATION-TYPE
        STATUS current
        DESCRIPTION "Info : Indicates that the MUX (AC 120) has received a
                     MUX_READY_CONF message in response to the MUX_READY_IND
                     message when the AC 120 is configured as the TCP client
                     for the AC 120 to Signalling Gateway connection."
        ::= { systemEvents 11 }
 
muxReadyConfirmNotReceivedNotify NOTIFICATION-TYPE
        STATUS current
        DESCRIPTION "Major : If AC 120 is configured as TCP client, this trap
                     is sent if the MUX_READY_CONF message is not received
                     after sending the MUX_READY_IND message three times.
                     This indicates unexpected behavior on the AC 120 to
                     Signalling Gateway link."
        ::= { systemEvents 12 }
 
oneWayMessageWhileInTwoWayStateNotify NOTIFICATION-TYPE
        STATUS current
        DESCRIPTION "Major : This trap indicates that a one way message is
                     received from the Signalling Gateway (SG) when the AC 120
                     is in the two way state of the keep-alive protocol on the
                     TCP link between the AC 120 and the SG."
        ::= { systemEvents 13 }
 
inactivityTimerExpiredNotify NOTIFICATION-TYPE
        STATUS current
        DESCRIPTION "Major : Sent by the AC 120 when the inactivity timer of
                     the keep-alive protocoal expires, i.e. when the AC 120
                     does not see a message from the Signalling Gateway on the
                     TCP link during the time period specified by the
                     inactivity timer value setting."
        ::= { systemEvents 14 }
 
keepAliveTimerExpiredInLlDownStateNotify NOTIFICATION-TYPE
        STATUS current
        DESCRIPTION "Major : Sent by the AC 120 when the keep-alive timer
                     expires in the LLDOWN state of the keep-alive protocol,
                     i.e. when the AC 120 does not see a keep-alive message
                     from the Signalling Gateway on the TCP link during the
                     time period specified by the keep-alive timer setting."
        ::= { systemEvents 15 }
 
keepAliveTimerExpiredInOneWayStateNotify NOTIFICATION-TYPE
        STATUS current
        DESCRIPTION "Major : Sent by the AC 120 when the keep-alive timer
                     expires in the ONE-WAY state of the keep-alive protocol,
                     i.e. when the AC 120 does not see a keep-alive message
                     from the Signalling Gateway on the TCP link during the
                     time period specified by the keep-alive timer setting."
        ::= { systemEvents 16 }


completeClockFailedNotify NOTIFICATION-TYPE
        STATUS current
        DESCRIPTION "Critical : The stratum cards have either been removed or 
                     have failed leading to no clock being provided."
        ::= { systemEvents 17 }

completeClockRecoveredNotify NOTIFICATION-TYPE
        STATUS current
