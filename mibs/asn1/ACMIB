                            routerIfIndex                     Integer32,
                            routerAdminStatus           INTEGER,
                            routerOperStatus            INTEGER,
                            routerIpAddress                IpAddress,
                            routerIpNetworkMask         IpAddress,
                            routerNumber                INTEGER,
                            routerEthernetIpAddress        IpAddress
                            }

