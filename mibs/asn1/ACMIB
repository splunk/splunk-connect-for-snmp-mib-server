virtualIntfConfigTable        OBJECT-TYPE
                SYNTAX SEQUENCE OF VirtualIntfConfigEntry
                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION "Table of Virtual Interfaces Configurations"
                ::= { intfConfig 18 }

virtualIntfConfigEntry        OBJECT-TYPE
                SYNTAX VirtualIntfConfigEntry
                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION "Index is the Interface and the Virutal Interface"
                INDEX { virtualIntfConfigIf, virtualIntfConfigVi }
                ::= { virtualIntfConfigTable 1 }

VirtualIntfConfigEntry        ::= SEQUENCE {
                virtualIntfConfigIf                      IntfIndex,
                virtualIntfConfigVi                      INTEGER,
                virtualIntfConfigCellRate                Integer32,
                virtualIntfConfigOverSubs                Integer32,
                virtualIntfConfigRowStatus               RowStatus,
                virtualIntfConfigUPCSupp                 INTEGER,
                virtualIntfAvailBwEgr                    Integer32
                }

virtualIntfConfigIf        OBJECT-TYPE
                SYNTAX IntfIndex
                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION "Interface index for side A of an ATM-to-ATM PVC VCC
                             connection.  Number value is interpreted as an
                             interface of the form, SSPPCCC, where SS is the 
                             card slot, PP is the port number, and CCC is the 
                             channel number."
                ::= { virtualIntfConfigEntry 1 }

virtualIntfConfigVi        OBJECT-TYPE
                SYNTAX INTEGER (0..1024)
                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION "VI value. Def Val is 1 because 0 cannot be 
                             configured"
                DEFVAL { 1 }
                ::= { virtualIntfConfigEntry 2 }

virtualIntfConfigCellRate        OBJECT-TYPE
                SYNTAX Integer32
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION " Bandwidth assigned for the VI. 
                              The unit is cells per sec.
                              Total Bandwidth of all VI's 
                              cannot exceed bandwidth of the 
                              main interface. CAC will be supported per VI."
                ::= { virtualIntfConfigEntry 3 }

virtualIntfConfigOverSubs       OBJECT-TYPE
                SYNTAX Integer32
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION " Over Subscription rate for VI "
                ::= { virtualIntfConfigEntry 4 }

virtualIntfConfigRowStatus        OBJECT-TYPE
                SYNTAX RowStatus
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "row status"
                ::= { virtualIntfConfigEntry 5 }

virtualIntfConfigUPCSupp OBJECT-TYPE
                SYNTAX INTEGER  {
                                disabled(1),
                                enabled(2)
                                }
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "virtual interface upc supp off/on
                        If this is changed then all the connections
                        on this interface will be replayed."
                DEFVAL { disabled }
                ::= { virtualIntfConfigEntry 6 }

virtualIntfAvailBwEgr   OBJECT-TYPE
                SYNTAX Integer32
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION " Avail Bw for that VI in cells per sec in the
                              Egress Direction or the direction away
                              from back plane."
                ::= { virtualIntfConfigEntry 7 }





-- Virtual UNI configuration table 

virtualUNIConfigTable        OBJECT-TYPE
                SYNTAX SEQUENCE OF VirtualUNIConfigEntry
                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION "Table of Virtual UNI Interfaces Configurations"
                ::= { intfConfig 19 }

virtualUNIConfigEntry        OBJECT-TYPE
                SYNTAX VirtualUNIConfigEntry
                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION "Index is the Interface and the Virutal UNI  Interface"
                INDEX { virtualUNIIfIndex, virtualUNIVUNIId }
                ::= { virtualUNIConfigTable 1 }

VirtualUNIConfigEntry        ::= SEQUENCE {
                virtualUNIIfIndex           IntfIndex,
                virtualUNIVUNIId            INTEGER,
                virtualUNIVISupport         INTEGER,
                virtualUNIMinVpi            INTEGER,
                virtualUNIMaxVpi            INTEGER,
                virtualUNIConfigRowStatus   RowStatus,
                virtualUNIAddrType          INTEGER,
                virtualUNISvcMinVpi         INTEGER,
                virtualUNISvcMaxVpi         INTEGER,
                virtualUNISvcMinVci         INTEGER,
                virtualUNISvcMaxVci         INTEGER,
                virtualUNIAdminStatus       INTEGER,
                virtualUNIOperStatus        INTEGER,
                virtualUNINwPrefix1         OCTET STRING,
                virtualUNIT303              Integer32,
                virtualUNIT308                        Integer32,
                virtualUNIT309                        Integer32,
                virtualUNIT310                        Integer32,
                virtualUNIT313                        Integer32,
                virtualUNIT316                        Integer32,
                virtualUNIT317                        Integer32,
                virtualUNIT322                        Integer32,
                virtualUNIT398                        Integer32,
                virtualUNIT399                        Integer32,
                virtualUNIMyNeighborIpAddress        IpAddress,
                virtualUNIMyNeighborIfName                DisplayString,
                virtualUNIVpiSig                        INTEGER,
                virtualUNIVciSig                        INTEGER,
                virtualUNISignalingAdminStatus   INTEGER,
                virtualUNISignalingOperStatus    INTEGER,
                virtualUNIVpiIlmi                        INTEGER,
                virtualUNIVciIlmi                        INTEGER,
                virtualUNIMyIfName                        DisplayString,
                virtualUNIIlmiProtocolStatus        INTEGER,
                virtualUNIIlmiOperStatus                INTEGER,
                virtualUNIIlmiConfigProc                INTEGER,
                virtualUNIIlmiConnectProc                INTEGER,
                virtualUNIIlmiAttachPtProc                INTEGER,
                virtualUNIIlmiSecureLinkProc        INTEGER,
                virtualUNIIlmiAddrRegProc                INTEGER,
                virtualUNIIlmiTimerS                Integer32,
                virtualUNIIlmiTimerT                Integer32,
                virtualUNIIlmiRetryK                Integer32,
                virtualUNIIntfType                        INTEGER,
                virtualUNIUserAddress                OCTET STRING,
                virtualUNIUserAddrOrgScope        INTEGER ,
                virtualUNISscopRxWndSz          INTEGER
                }

virtualUNIIfIndex        OBJECT-TYPE
                SYNTAX IntfIndex
                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION "This is the first  index to the Virtual UNI 
                Interface table. Interface index will be of the form SSPPCCC, 
                where SS is the card slot, PP is the port number, and CCC 
                is the channel number."
                ::= { virtualUNIConfigEntry 1 }

virtualUNIVUNIId        OBJECT-TYPE
                SYNTAX INTEGER (1..255)
                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION "Virtual UNI number, second index to access the virtual
                     UNI interface."
                DEFVAL { 1 }
                ::= { virtualUNIConfigEntry 2 }

virtualUNIVISupport        OBJECT-TYPE
                SYNTAX INTEGER  {
                                enabled(1),
                                disabled(2)
                                }
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "Virtual Interface (Traffic Shaping) is enabled or
        disabled on this VUNI. A Virtual Interface should already be
        configured befaore the traffic shaping can be enabled on this VUNI. If
        VISupport is disabled then all the connections on the VUNI can only be
        configured as UBR connections if this VUNI interface is on a VI enabled 
        physical interface"
                DEFVAL { disabled }
                ::= { virtualUNIConfigEntry 3 }

