atmSvcVccTable        OBJECT-TYPE
                SYNTAX SEQUENCE OF AtmSvcVccEntry
                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION "Table of atm connections"
                ::= { connectionConfig 3 }

atmSvcVccEntry        OBJECT-TYPE
                SYNTAX AtmSvcVccEntry
                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION "Row definition of atm connections"
                INDEX { atmSvcVccIfA, atmSvcVccVpiA, 
                        atmSvcVccVciA, atmSvcVccIfB, 
                        atmSvcVccVpiB, atmSvcVccVciB }
                ::= { atmSvcVccTable 1 }

AtmSvcVccEntry        ::= SEQUENCE {
                atmSvcVccIfA                        IntfIndex,
                atmSvcVccVpiA                       INTEGER,
                atmSvcVccVciA                       INTEGER,
                atmSvcVccIfB                        IntfIndex,
                atmSvcVccVpiB                       INTEGER,
                atmSvcVccVciB                       INTEGER,
                atmSvcVccServiceTypeA2B             INTEGER,
                atmSvcVccSarTypeA2B                 INTEGER,
                atmSvcVccPeakCellRateA2B            Integer32,
                atmSvcVccSusCellRateA2B             Integer32,
                atmSvcVccMaxBurstSizeA2B            Integer32,
                atmSvcVccServiceTypeB2A             INTEGER,
                atmSvcVccSarTypeB2A                 INTEGER,
                atmSvcVccPeakCellRateB2A            Integer32,
                atmSvcVccSusCellRateB2A             Integer32,
                atmSvcVccMaxBurstSizeB2A            Integer32,
                atmSvcVccFlow                       INTEGER,
                atmSvcVccType                       INTEGER,
                atmSvcVccConnectionStatus           INTEGER,
                atmSvcVccViA                        INTEGER,
                atmSvcVccViB                        INTEGER,
                atmSvcVccCnfrmTypeA2B               INTEGER,
                atmSvcVccCnfrmTypeB2A               INTEGER

                ,atmSvcVccVUNIA                     INTEGER,
                 atmSvcVccVUNIB                     INTEGER

                ,atmSvcVccTrafficShapingA2B         INTEGER,
                atmSvcVccTrafficShapingB2A          INTEGER

                }

