                SYNTAX INTEGER {
                                stratumA(1),
                                stratumB(2)
                                }
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "Current status of the backup stratum card."
                ::= { stratumConfig 12 }



-- Cell Test specific objects



cellTestIfB OBJECT-TYPE
                SYNTAX IntfIndex
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "Interface index used for testing cell transfer.
                             Number value is interpreted as an interface of the 
                             form, SSPPCCC, where SS is the card slot, PP is 
                             the port number, and CCC is the channel number."
                ::= { cellTest 1 }

cellTestVpiB OBJECT-TYPE
                SYNTAX INTEGER (0..4095)
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "VPI value for testing cell transfer to an I/O card.
                             Valid range is a number between 0 and 4095.  If
                             the I/O card interface is not ATM, then this will 
                             always be zero."
                ::= { cellTest 2 }

cellTestVcidB OBJECT-TYPE
                SYNTAX INTEGER (0..65535)
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "Virtual Check Id for testing cell transfer to an
                             I/O card.  Valid range is a number between 0 and
                             65535.  If the I/O card interface is frame relay,
                             this field acts as DLCI and the valid range is a
                             number between 0 and 1023.  If the I/O card 
                             interface is circuit emulation, then this will
                             always be 0."
              ::= { cellTest 3 }

cellTestServiceType OBJECT-TYPE
               SYNTAX INTEGER  {
                                ubr(1),
                                vbr-nrt2(2),
                                vbr-nrt1(3),
                                vbr-rt2(4),
                                vbr-rt1(5),
                                vbr-express(6),
                                cbr4(7),
                                cbr3(8),
                                cbr2(9),
                                cbr1(10)
                               }
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "Service types for testing cell transfer"
                DEFVAL { ubr }
                ::= { cellTest 4 }

cellTestConnType OBJECT-TYPE
                SYNTAX INTEGER {
                                simplexTx(1),
                                simplexRx(2),
                                duplex(3)
                               }
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "Connection types for testing cell transfer"
                DEFVAL { simplexTx }
                ::= { cellTest 5 }

cellTestProtocol OBJECT-TYPE
                SYNTAX INTEGER {
                                atm(1),
                                frame-relay (2),




                                hdlc(4)
                               }
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "Protocol (interface type) on the I/O"
                DEFVAL { atm }
                ::= { cellTest 6 }

cellTestPayLoadTx OBJECT-TYPE
                SYNTAX OCTET STRING (SIZE(88))
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "PayLoad to be transmitted on I/O interface"
                ::= { cellTest 7 }

cellTestPayLoadRx OBJECT-TYPE
                SYNTAX OCTET STRING (SIZE(88))
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "Last PayLoad received  on I/O interface"
                ::= { cellTest 8 }

cellTestAction OBJECT-TYPE
               SYNTAX INTEGER
                       {
                         send(1),
                         send-10(2),
                         setupconn(3),
                         stop (4),
                          clearTxStats(5),
                         clearRxStats(6)
                       }
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "PayLoad received  on I/O interface"
                DEFVAL { stop }
                ::= { cellTest 9 }

cellTestRxPduCount OBJECT-TYPE
                SYNTAX Integer32
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "Valid cells received (Input Billing Count)"
                ::= { cellTest 10 }

cellTestTxPduCount OBJECT-TYPE
                SYNTAX Integer32
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "Valid cells transmitted (Output Billing Count)"
                ::= { cellTest 11 }

cellTestMismatchPduCount OBJECT-TYPE
                SYNTAX Integer32
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "Number of times Rx payload mismatched with Tx
                             payload."
                ::= { cellTest 12 }

cellTestStatus        OBJECT-TYPE
                SYNTAX INTEGER
                        {
                         running(1),
                         not-running(2)
                        }
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "Indicates whether a cellTest test is in progress
                             or not."
                DEFVAL { not-running }
                ::= { cellTest 13 }


cellTestVUNIB OBJECT-TYPE
                SYNTAX INTEGER (0..255)
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "VUNI id for testing cell transfer to an I/O card.
                     This field is valid only for ATM interface."
                ::= { cellTest 14 }


