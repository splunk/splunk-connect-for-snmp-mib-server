                atmTrunkingBrVccBwAtmScr01                   Integer32,
                atmTrunkingBrVccFwAtmMbs01                   Integer32,
                atmTrunkingBrVccBwAtmMbs01                   Integer32,
                atmTrunkingBrVccConnType                     INTEGER,
