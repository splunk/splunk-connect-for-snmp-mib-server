-- Vipr Sub Channel Configuration Group
 
 
viprSubChnlConfigTable  OBJECT-TYPE
                SYNTAX SEQUENCE OF ViprSubChnlConfigEntry
                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION "Table of Vipr Sub Channel Config Information"
                ::= { viprConfig 3 }
 
viprSubChnlConfigEntry  OBJECT-TYPE
                SYNTAX ViprSubChnlConfigEntry
                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION " definition for VIPR sub channel attributes"
                INDEX { viprSlot, viprRouterId, viprIntfId, viprSubChnlId }
                ::= { viprSubChnlConfigTable 1 }
 
ViprSubChnlConfigEntry  ::= SEQUENCE {
                viprSubChnlId                      Integer32,
                viprSubChnlIntfIndex               IntfIndex,
                viprSubChnlTrafficType             INTEGER,
                viprSubChnlLlcEncapType            INTEGER,
                viprSubChnlMacEncapType            INTEGER,
                viprSubChnlTosFeature              INTEGER,
                viprSubChnlTosValue                INTEGER,
                viprSubChnlTosMask                 INTEGER,
                viprSubChnlVpiDlci                 INTEGER,
                viprSubChnlVci                     INTEGER,
                viprSubChnlSarType                 INTEGER,
                viprSubChnlServiceType             INTEGER,
                viprSubChnlFwdErrCorrection        INTEGER,
                viprSubChnlInterworkingType        INTEGER,
                viprSubChnlFrameSize               Integer32,
                viprSubChnlComBurstSize            Integer32,
                viprSubChnlExcessBurstSize         Integer32,
                viprSubChnlCommitedInfoRate        Integer32,
                viprSubChnlTimeInterval            INTEGER,
                viprSubChnlAtmPeakCellRate         Integer32,
                viprSubChnlAtmSustainedCellRate    Integer32,
                viprSubChnlAtmMaxBurstSize         Integer32,
                viprSubChnlFrPeakCellRate          Integer32,
                viprSubChnlFrSustainedCellRate     Integer32,
                viprSubChnlFrMaxBurstSize          Integer32,
                viprSubChnlFlowType                INTEGER,
                viprSubChnlAdminStatus             INTEGER,
                viprSubChnlOperStatus              INTEGER,
                viprSubChnlInitTmr                 Integer32,
                viprSubChnlDisabWarnTmr            Integer32,
                viprSubChnlShutDwnWarnTmr          Integer32,
                viprSubChnlDisableWarning          INTEGER,
                viprSubChnlDisableReason           INTEGER,
                viprSubChnlRowStatus               RowStatus,
                viprSubChnlImIpAddr                IpAddress,
                viprSubChnlImMask                  IpAddress,
                viprSubChnlViB                     INTEGER

                ,viprSubChnlVUNIB                  INTEGER

                ,viprSubChnlTrafficShaping         INTEGER,
                viprSubChnlAtmOamStatus            INTEGER
                }
 
 
viprSubChnlId   OBJECT-TYPE
                SYNTAX Integer32(1..3)
                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION "A unique Id for the sub channel in the interface"
                ::= { viprSubChnlConfigEntry 1 }
 
 
viprSubChnlIntfIndex   OBJECT-TYPE
                SYNTAX IntfIndex
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "The Slot Port Channel to which the IP Packet to be
                             forwarded when the TOS specified in the incoming
                             IP packet is matched to the TosValue, TosMask pair"
                ::= { viprSubChnlConfigEntry 2 }
 
viprSubChnlTrafficType   OBJECT-TYPE
                SYNTAX INTEGER{
                       atm(1),
                       fR(2),
                       bridge(3),
                       routing(4),
                       inband(5)
                       }
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "The type of interface configured on the SSPPCCC,
                             viprSubChnlIntfIndex"
                DEFVAL { atm }
                ::= { viprSubChnlConfigEntry 3 }
 
viprSubChnlLlcEncapType    OBJECT-TYPE
                SYNTAX INTEGER{
                       vc(1),
                       llcSnap(2)
                       }
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "The encapsulation technique used in making IP packet
                             to ATM cell"
                DEFVAL { none }
                ::= { viprSubChnlConfigEntry 4 }
 
viprSubChnlMacEncapType    OBJECT-TYPE
                SYNTAX INTEGER{
                       macHdr(1),
                       noMacHdr(2)
                       }
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "The encapsulation technique used in making IP packet
                             to ATM cell"
                DEFVAL { none }
                ::= { viprSubChnlConfigEntry 5 }
 
viprSubChnlTosFeature    OBJECT-TYPE
                SYNTAX INTEGER{
                       disabled(1),
                       enabled(2)
                       }
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "Allow the user to set the TOS values or not"
                DEFVAL { 1 }
                ::= { viprSubChnlConfigEntry 6 }
 

viprSubChnlTosValue   OBJECT-TYPE
                SYNTAX INTEGER(0..255)
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "The type of service specified in IP packet masked
                             with viprSubChnlTosMask has to be equal to this
                             value in order to have the IP packets getting
                             forwarded in this sub channel"
                DEFVAL { 255 }
                ::= { viprSubChnlConfigEntry 7 }
 
viprSubChnlTosMask   OBJECT-TYPE
                SYNTAX INTEGER(0..255)
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "The Type of Service mask"
                DEFVAL { 255 }
                ::= { viprSubChnlConfigEntry 8 }
 
