bridgeStatsTable OBJECT-TYPE
                SYNTAX SEQUENCE OF BridgeStatsEntry
                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION "Table of statistics for a bridge interface."
                ::= { intfStatistics 6 }

bridgeStatsEntry OBJECT-TYPE
                SYNTAX BridgeStatsEntry
                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION "Row definition for the bridge interface statistics
                             table"
                INDEX { bridgeStatsId }
                ::= { bridgeStatsTable 1 }

BridgeStatsEntry ::= SEQUENCE {
                bridgeStatsId                                        Integer32,
                bridgeStatsTimer                                Integer32,
                bridgeStatsBasePortDelayExceededDiscardsHi        Integer32,
                bridgeStatsBasePortDelayExceededDiscardsLo        Integer32,
                bridgeStatsBasePortMtuExceededDiscardsHi        Integer32,
                bridgeStatsBasePortMtuExceededDiscardsLo        Integer32,
                bridgeStatsTpPortInFramesHi                        Integer32,
                bridgeStatsTpPortInFramesLo                        Integer32,
                bridgeStatsTpPortOutFramesHi                        Integer32,
                bridgeStatsTpPortOutFramesLo                        Integer32,
                bridgeStatsTpPortInDiscardsHi                        Integer32,
                bridgeStatsTpPortInDiscardsLo                        Integer32
                }

bridgeStatsId OBJECT-TYPE
                SYNTAX Integer32
                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION "Interface index (SSPPCCC) to the bridge"
                ::= { bridgeStatsEntry 1 }

bridgeStatsTimer OBJECT-TYPE
                SYNTAX Integer32
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "Time, in seconds, since last reset."
                ::= { bridgeStatsEntry 2 }



bridgeStatsBasePortDelayExceededDiscardsHi OBJECT-TYPE
                SYNTAX Integer32
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "The number of frames discarded by this port due
                             to excessive transit delay through the bridge.  It
                             is incremented by both transparent and source
                             route bridges. High-order 32 bits."
                REFERENCE "IEEE 802.1D-1990: Section 6.6.1.1.3"
                ::= { bridgeStatsEntry 8 }

bridgeStatsBasePortDelayExceededDiscardsLo OBJECT-TYPE
                SYNTAX Integer32
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "The number of frames discarded by this port due
                             to excessive transit delay through the bridge.  It
                             is incremented by both transparent and source
                             route bridges.  Low-order 32 bits."
                REFERENCE "IEEE 802.1D-1990: Section 6.6.1.1.3"
                ::= { bridgeStatsEntry 9 }

bridgeStatsBasePortMtuExceededDiscardsHi OBJECT-TYPE
                SYNTAX Integer32
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "The number of frames discarded by this port due
                             to an excessive size.  It is incremented by both
                             transparent and source route bridges.  High-order
                             32 bits."
                REFERENCE "IEEE 802.1D-1990: Section 6.6.1.1.3"
                ::= { bridgeStatsEntry 10 }

bridgeStatsBasePortMtuExceededDiscardsLo OBJECT-TYPE
                SYNTAX Integer32
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "The number of frames discarded by this port due
                             to an excessive size.  It is incremented by both
                             transparent and source route bridges.  Low-order
                             32 bits."
                REFERENCE "IEEE 802.1D-1990: Section 6.6.1.1.3"
                ::= { bridgeStatsEntry 11 }

bridgeStatsTpPortInFramesHi OBJECT-TYPE
                SYNTAX Integer32
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "The number of frames that have been received by
                             this port from its segment.  Note that a frame
                             received on the interface corresponding to this
                             port is only counted by this object if and only
                             if it is for a protocol being processed by the
                             local bridging function, including bridge 
                             management frames.  High-order 32 bits."
                REFERENCE "IEEE 802.1D-1990: Section 6.6.1.1.3"
                ::= { bridgeStatsEntry 12 }

bridgeStatsTpPortInFramesLo OBJECT-TYPE
                SYNTAX Integer32
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "The number of frames that have been received by
                             this port from its segment.  Note that a frame
                             received on the interface corresponding to this
                             port is only counted by this object if and only
                             if it is for a protocol being processed by the
                             local bridging function, including bridge 
                             management frames.  Low-order 32 bits."
                REFERENCE "IEEE 802.1D-1990: Section 6.6.1.1.3"
                ::= { bridgeStatsEntry 13 }

bridgeStatsTpPortOutFramesHi OBJECT-TYPE
                SYNTAX Integer32
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "The number of frames that have been transmitted
                             by this port to its segment.  Note that a frame
                             transmitted on the interface corresponding to this
                             port is only counted by this object if and only if
                             it is for a protocol being processed by the local
                             bridging function, including bridge management
                             frames.  High-order 32 bits."
                REFERENCE "IEEE 802.1D-1990: Section 6.6.1.1.3"
                ::= { bridgeStatsEntry 14 }

bridgeStatsTpPortOutFramesLo OBJECT-TYPE
                SYNTAX Integer32
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "The number of frames that have been transmitted
                             by this port to its segment.  Note that a frame
                             transmitted on the interface corresponding to this
                             port is only counted by this object if and only if
                             it is for a protocol being processed by the local
                             bridging function, including bridge management
                             frames.  Low-order 32 bits."
                REFERENCE "IEEE 802.1D-1990: Section 6.6.1.1.3"
                ::= { bridgeStatsEntry 15 }

bridgeStatsTpPortInDiscardsHi OBJECT-TYPE
                SYNTAX Integer32
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "Count of valid frames received which were
                             discarded (i.e., filtered) by the Forwarding
                             Process.  High-order 32 bits."
                REFERENCE "IEEE 802.1D-1990: Section 6.6.1.1.3"
                ::= { bridgeStatsEntry 16 }

bridgeStatsTpPortInDiscardsLo OBJECT-TYPE
                SYNTAX Integer32
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "Count of valid frames received which were
                             discarded (i.e., filtered) by the Forwarding
                             Process.  Low-order 32 bits."
                REFERENCE "IEEE 802.1D-1990: Section 6.6.1.1.3"
                ::= { bridgeStatsEntry 17 }


