-- H.248 Permanent Termination ID Table
h248TermIdTable OBJECT-TYPE 
                SYNTAX SEQUENCE OF H248TermIdEntry 
                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION "Table of H.248 Terminations"
                ::= {  h248Config 1 }

h248TermIdEntry OBJECT-TYPE 
                SYNTAX H248TermIdEntry
                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION "Row definition for H.248 
                    Termination ID table"
                INDEX { h248TermSlotId, 
                        h248TermPhyPortId,
                        h248TermLogPortId,
                        h248TermChanId ,
                        h248TermVpi,
                        h248TermVci
                        }
                ::= { h248TermIdTable 1 }

H248TermIdEntry ::= SEQUENCE {
                        h248TermSlotId                 INTEGER,
                        h248TermPhyPortId          INTEGER,
                        h248TermLogPortId        INTEGER,
                        h248TermChanId                INTEGER,
                        h248TermVpi                INTEGER,
                        h248TermVci                INTEGER,
                        h248TermId                OCTET STRING,
                        h248TermAal2Cid                INTEGER,
                        h248TermActivation      INTEGER,
                        h248TermRowStatus        RowStatus
                        }

h248TermSlotId OBJECT-TYPE
                SYNTAX INTEGER ( 1..99 )
                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION "Slot # for termination"
                ::= { h248TermIdEntry 1 }

h248TermPhyPortId OBJECT-TYPE
                SYNTAX INTEGER ( 0..999 )
                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION "Physical Port # for termination"
                ::= { h248TermIdEntry 2 }

h248TermLogPortId OBJECT-TYPE
                SYNTAX INTEGER ( 0..999 )
                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION "Logical Port # for termination"
                ::= { h248TermIdEntry 3 }

h248TermChanId OBJECT-TYPE
                SYNTAX INTEGER ( 0..999 )
                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION "Chan # for termination"
                ::= { h248TermIdEntry 4 }

h248TermVpi OBJECT-TYPE
                SYNTAX INTEGER ( 0..4096 )
                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION "VPI # for termination. Value 0 is
                        reserved to indicate N/A, e.g., TDM
                        termination. Thus, VPI = h248TermVpi - 1,
                        for h248TermVpi > 0."
                ::= { h248TermIdEntry 5 }

h248TermVci OBJECT-TYPE
                SYNTAX INTEGER ( 0..65536 )
                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION "VCI # for termination. Value 0 is
                        reserved to indicate N/A, e.g., TDM
                        termination. Thus, VCI = h248TermVci - 1,
                        for h248TermVci > 0."
                ::= { h248TermIdEntry 6 }

h248TermId OBJECT-TYPE
                SYNTAX OCTET STRING (SIZE(32))
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "Termination ID is provisioned
                        by user and used for identifying termination
                        between MGC and MG"
                ::= { h248TermIdEntry 7 }

h248TermAal2Cid OBJECT-TYPE
                SYNTAX INTEGER (0..255)
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "AAL2 CID"
                ::= { h248TermIdEntry 8 }

h248TermActivation OBJECT-TYPE
                SYNTAX INTEGER {
                        disable(1),
                        enable(2)
                        }
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "Activation for termination ID"
                ::= { h248TermIdEntry 9 }

h248TermRowStatus OBJECT-TYPE
                SYNTAX RowStatus
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "h248TermIdEntry row status"
                ::= { h248TermIdEntry 10 }

