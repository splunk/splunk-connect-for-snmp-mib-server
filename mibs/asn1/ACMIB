                SYNTAX Oc3APSPairConfigEntry
                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION "Row definition for common physical
                             port attributes"
                INDEX { oc3APSPairId }
                ::= { oc3APSPairConfigTable 1 }
 
Oc3APSPairConfigEntry ::= SEQUENCE {
                oc3APSPairId                        Integer32,
                oc3APSPairMode                        INTEGER,
                oc3APSPairOption                INTEGER,
                oc3APSPairWorkingSFBERThreshold        INTEGER,
                oc3APSPairProtectionSFBERThreshold        INTEGER,
                oc3APSPairWorkingSDBERThreshold        INTEGER,
                oc3APSPairProtectionSDBERThreshold        INTEGER,
                oc3APSPairWTRPeriod                INTEGER,
                oc3APSWorkingSlot                Integer32,
                oc3APSProtectionSlot                Integer32,
                oc3APSActiveSlot                Integer32,
                oc3APSPairStatus                INTEGER,
                oc3APSPairUserCommand                INTEGER,
                oc3APSPairCurrentState          INTEGER,
                oc3APSPairWorkingLineSignalStatus         INTEGER,
                oc3APSPairProtectionLineSignalStatus        INTEGER
                }
 
oc3APSPairId       OBJECT-TYPE
                SYNTAX Integer32
                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION "APS Pair Port index - SSPP"
                ::= { oc3APSPairConfigEntry 1 }
 
oc3APSPairMode OBJECT-TYPE
                SYNTAX INTEGER  {
                                unidirectional(1),
                                bidirectional(2)
                                }
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "Unidirectional or Bidirectional mode:
                             Uni - Each pair operates independently
                                   i.e. a switch over in one pair does not
                                   effect the remote pair.
                             Bi - both remote and local pairs have to
                                   switch together."
                DEFVAL { unidirectional }
                ::= { oc3APSPairConfigEntry 2 }
 
oc3APSPairOption OBJECT-TYPE
                SYNTAX INTEGER {
                                nonrevertive(1),
                                revertive(2)
                                }
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION " Nonrevertive - After a switch to Prot port,
                             even if the working comes up there is no
                             switch to the working port back.
                             Revertive - traffic is switched back to
                             work port after it comes back."
                DEFVAL { nonrevertive }
                ::= { oc3APSPairConfigEntry 3 }

oc3APSPairWorkingSFBERThreshold OBJECT-TYPE
                SYNTAX INTEGER {
                                tenToMinus4(1),
                                tenToMinus5(2)
                                }
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "Bits error Rate threshold to declare signal failure"
                DEFVAL { tenToMinus4 }
                ::= { oc3APSPairConfigEntry 4 }


