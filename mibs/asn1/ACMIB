                DESCRIPTION "row status"
                ::= { viprSubChnlConfigEntry 34 }

viprSubChnlImIpAddr     OBJECT-TYPE
                SYNTAX IpAddress
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "inband management IpAddr"
                ::= { viprSubChnlConfigEntry 35 }

viprSubChnlImMask      OBJECT-TYPE
                SYNTAX IpAddress
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "inband management Mask"
                ::= { viprSubChnlConfigEntry 36 }

viprSubChnlViB   OBJECT-TYPE
                SYNTAX INTEGER (0..4096)
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "The viB value of the connection"
                ::= { viprSubChnlConfigEntry 37 }


viprSubChnlVUNIB   OBJECT-TYPE
                SYNTAX INTEGER (0..255)
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "The VUNIB value of the connection"
                ::= { viprSubChnlConfigEntry 38 }

 
viprSubChnlTrafficShaping       OBJECT-TYPE
                SYNTAX INTEGER {
                                disabled(1),
                                enabled(2)
                               }
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "Traffic Shaping Support in the A2B direction 
                             for outgoing ATM connections."
                DEFVAL { disabled }
                ::= { viprSubChnlConfigEntry 39 }

viprSubChnlAtmOamStatus OBJECT-TYPE
                SYNTAX INTEGER {
                                unsupp(1),
                                end-Pt(2),
                                end-Seg-Pt(3)
                               }
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "Indicates the OAM monitoring status.
                             The 3 possible states are:
                             Unsupp:  No monitoring enabled.
                             End-Pt:  A side is only an end point, therefore
                                it will respond to AIS/RDI.
                             End-Seg-Pt: Connection endPoint that is also
                             a segment endPoint."
                DEFVAL { unsupp }
                ::= { viprSubChnlConfigEntry 40 }



-- Vipr Route Table
 
 
viprRouteConfigTable  OBJECT-TYPE
                SYNTAX SEQUENCE OF ViprRouteConfigEntry
                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION "Table of Vipr static route Config Information"
                ::= { viprConfig 4 }
 
viprRouteConfigEntry  OBJECT-TYPE
                SYNTAX ViprRouteConfigEntry
                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION "Definition for Vipr static route attributes"
                INDEX { viprSlot, viprRouterId, viprRouteId }
                ::= { viprRouteConfigTable 1 }
 
ViprRouteConfigEntry  ::= SEQUENCE {
                viprRouteId                      Integer32,
                viprRouteIPAddress               IpAddress,
                viprRouteIPNetMask               IpAddress,
                viprRouteDestAddress             IpAddress,
                viprRouteGatewayAddress          IpAddress,
                viprRouteServiceType             INTEGER,
                viprRouteMetric                  Integer32,
                viprRouteAdminStatus             INTEGER,
                viprRouteOperStatus              INTEGER,
                viprRouteInitTmr                 Integer32,
                viprRouteDisabWarnTmr            Integer32,
                viprRouteShutDwnWarnTmr          Integer32,
                viprRouteDisableWarning          INTEGER,
                viprRouteDisableReason           INTEGER,
                viprRouteRowStatus               RowStatus,
                viprRouteDestMask                IpAddress
                }
 
 
 
viprRouteId     OBJECT-TYPE
                SYNTAX Integer32(1..1024)
                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION "The unique Id to the routing table entry"
                ::= { viprRouteConfigEntry 1 }
 
viprRouteIPAddress   OBJECT-TYPE
                SYNTAX IpAddress
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "The IP address of the host/network in the route
                             entry"
                ::= { viprRouteConfigEntry 2 }
 
viprRouteIPNetMask   OBJECT-TYPE
                SYNTAX IpAddress
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "The IP Subnet Mask for the route entry to identify
                             whether the route entry is a host or network"
                ::= { viprRouteConfigEntry 3 }
 
viprRouteDestAddress   OBJECT-TYPE
                SYNTAX IpAddress
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "The IP address of the host/network in the route
                             entry"
                ::= { viprRouteConfigEntry 4 }

viprRouteGatewayAddress   OBJECT-TYPE
                SYNTAX IpAddress
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "The IP address of the Gateway"
                ::= { viprRouteConfigEntry 5 }
 
viprRouteServiceType OBJECT-TYPE
                SYNTAX INTEGER {
                       gold(1),
                       silver(2),
                       bronze(3)
                }
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "The Service types that are supported"
                DEFVAL { gold }
                ::= { viprRouteConfigEntry 6 }
 
viprRouteMetric OBJECT-TYPE
                SYNTAX Integer32
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "A scalar value for user-supplied static 
                             route metric values. "
                ::= { viprRouteConfigEntry 7 }
 
 
viprRouteAdminStatus       OBJECT-TYPE
                SYNTAX INTEGER  {
                       outOfService(1),
                       inService(2)
                       }
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "This variable represents the various X.731 states
                             the Route state machine can take Administratively"
                DEFVAL { outOfService }
                ::= { viprRouteConfigEntry 8 }

viprRouteOperStatus       OBJECT-TYPE
                SYNTAX INTEGER  {
                       outOfService(1),
                       inService(2),
                       unlocked-Init(3),
                       unlocked-Disable(4),
                       locked-Shutting(5)
                       }
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "This variable represents the various X.731 states
                             the Route state machine can take Operationally"
                DEFVAL { outOfService }
                ::= { viprRouteConfigEntry 9 }
 
viprRouteInitTmr OBJECT-TYPE
                SYNTAX Integer32
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "This variable describes the waiting period in
                             the locked-Init state before moving onto
                             locked-Diable state."
                DEFVAL { 192 }
                ::= { viprRouteConfigEntry 10 }
 
viprRouteDisabWarnTmr OBJECT-TYPE
                SYNTAX Integer32
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "This variable describes the periodicity in which
                             the error messages are to be flashed when the
                             system is in unlocked-Disable state."
                DEFVAL { 18000 }
                ::= { viprRouteConfigEntry 11 }
 
viprRouteShutDwnWarnTmr OBJECT-TYPE
                SYNTAX Integer32
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "This variable describes the periodicity in which
                             the error messages are to be flashed when the
                             system is in locked-Shutting state."
                DEFVAL { 18000 }
                ::= { viprRouteConfigEntry 12 }
 
 
viprRouteDisableWarning OBJECT-TYPE
                SYNTAX INTEGER {
                       enabled(1),
                       disabled(2)
                       }
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "This variable when disabled will mask all the
                             Disable warning messages."
                DEFVAL { enabled }
                ::= { viprRouteConfigEntry 13 }
 
viprRouteDisableReason OBJECT-TYPE
                SYNTAX INTEGER {
                       none(1),
                       intfDisabled(2),
                       routDisabled(3)
                       }
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "This variable describes the reason why the
                             Route in the unlocked-Disabled state."
                DEFVAL { none }
                ::= { viprRouteConfigEntry 14 }
 
viprRouteRowStatus      OBJECT-TYPE
                SYNTAX RowStatus
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "row status"
                ::= { viprRouteConfigEntry 15 }
 
 
viprRouteDestMask    OBJECT-TYPE
                SYNTAX IpAddress
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "The IP Mask of the host/network in the route
                             entry"
                ::= { viprRouteConfigEntry 16 }
 


-- Vipr Routing Table
 


viprRoutingConfigTable  OBJECT-TYPE
                SYNTAX SEQUENCE OF ViprRoutingConfigEntry
                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION "Table of Vipr routes Config Information"
                ::= { viprConfig 5 }
 
viprRoutingConfigEntry  OBJECT-TYPE
                SYNTAX ViprRoutingConfigEntry
                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION "Definition for Vipr route attributes"
                INDEX { viprSlot, viprRouterId, viprRoutingCommand, viprRoutingIndex }
                ::= { viprRoutingConfigTable 1 }
 
ViprRoutingConfigEntry  ::= SEQUENCE {
                viprRoutingCommand                 INTEGER,
                viprRoutingIPAddress               IpAddress,
                viprRoutingIPNetMask               IpAddress,
                viprRoutingDestAddress             IpAddress,
                viprRoutingGatewayAddress          IpAddress,
                viprRoutingServiceType             INTEGER,
                viprRoutingType                    INTEGER,
                viprRoutingMetric                  Integer32,
                viprRoutingDestMask                IpAddress,
                viprRoutingAge                     Integer32,
                viprRoutingIndex                   Integer32
                }
 
viprRoutingCommand   OBJECT-TYPE
                SYNTAX INTEGER {
                       start(1),
                       more(2),
                       abort(3),
                       retrieve(4)
                }
                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION "The Routing table request command
                             Set to start to initiate the caching of first 15 entries
                             Set to more to initiate the caching of next 15 entries
                             Set to abort to reset the counter in I/O card
                             Set to retrieve in order to get the entries"
                ::= { viprRoutingConfigEntry 1 }
 
viprRoutingIPAddress   OBJECT-TYPE
                SYNTAX IpAddress
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "The IP address of the host/network in the route
                             entry"
                ::= { viprRoutingConfigEntry 2 }
 
viprRoutingIPNetMask   OBJECT-TYPE
                SYNTAX IpAddress
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "The IP Subnet Mask for the route entry to identify
                             whether the route entry is a host or network"
                ::= { viprRoutingConfigEntry 3 }
 
viprRoutingDestAddress   OBJECT-TYPE
                SYNTAX IpAddress
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "The IP address of the host/network in the route
                             entry"
                ::= { viprRoutingConfigEntry 4 }
 
viprRoutingGatewayAddress   OBJECT-TYPE
                SYNTAX IpAddress
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "The IP address of the Gateway"
                ::= { viprRoutingConfigEntry 5 }
 
viprRoutingServiceType OBJECT-TYPE
                SYNTAX INTEGER {
                       gold(1),
                       silver(2),
                       bronze(3)
                }
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "The Service types that are supported"
                DEFVAL { gold }
                ::= { viprRoutingConfigEntry 6 }
 
viprRoutingType    OBJECT-TYPE
                SYNTAX INTEGER  {
                       stat(1),
                       rip(2),
                       ospf(3),
                       intf(4)
                       }
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "Route types supported by the RS Card."
                DEFVAL { intf }
                ::= { viprRoutingConfigEntry 7 }

viprRoutingMetric OBJECT-TYPE
                SYNTAX Integer32
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "The Path cost"
                ::= { viprRoutingConfigEntry 8 }
 
viprRoutingDestMask   OBJECT-TYPE
                SYNTAX IpAddress
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "The IP address Mask of the host/network in the route
                             entry"
                ::= { viprRoutingConfigEntry 9 }

viprRoutingAge OBJECT-TYPE
                SYNTAX Integer32
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "Age of the routing entry"
                ::= { viprRoutingConfigEntry 10 }
 
viprRoutingIndex  OBJECT-TYPE
                SYNTAX Integer32
                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION "Index to the routing entry"
                ::= { viprRoutingConfigEntry 11 }

-- Vipr ARP Table
 


 
viprArpConfigTable  OBJECT-TYPE
                SYNTAX SEQUENCE OF ViprArpConfigEntry
                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION "Table of Vipr Arp Table Information"
                ::= { viprConfig 6 }
 
viprArpConfigEntry  OBJECT-TYPE
                SYNTAX ViprArpConfigEntry
                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION "Definition for Vipr Arp Table attributes"
                INDEX { viprSlot, viprRouterId, viprArpCommand , viprArpIndex}
                ::= { viprArpConfigTable 1 }
 
ViprArpConfigEntry  ::= SEQUENCE {
                viprArpCommand                 INTEGER,
                viprArpIPAddress               IpAddress,
                viprArpHwAddress               OCTET STRING,
                viprArpEntryAge                Integer32,
                viprArpInterfaceIPAddress      IpAddress,
                viprArpInterfaceIPMask         IpAddress,
                viprArpInterfaceServiceType    INTEGER,
                viprArpIndex                   Integer32
                }
 
viprArpCommand   OBJECT-TYPE
                SYNTAX INTEGER {
                       start(1),
                       more(2),
                       abort(3),
                       retrieve(4)
                }
                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION "The ARP table request command
                             Set to start to initiate the caching of first 15 entries
                             Set to more to initiate the caching of next 15 entries
                             Set to abort to reset the counter in I/O card
                             Set to retrieve in order to get the entries"
                ::= { viprArpConfigEntry 1 }
 
viprArpIPAddress   OBJECT-TYPE
                SYNTAX IpAddress
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "The IP address of the host/network"
                ::= { viprArpConfigEntry 2 }
 
viprArpHwAddress   OBJECT-TYPE
                SYNTAX OCTET STRING (SIZE(6))
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "The hardware address of the host/network"
                ::= { viprArpConfigEntry 3 }

viprArpEntryAge   OBJECT-TYPE
                SYNTAX Integer32
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "Age of cache entry."
                ::= { viprArpConfigEntry 4 }

viprArpInterfaceIPAddress   OBJECT-TYPE
                SYNTAX IpAddress
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "The Interface IP address through which it is learned"
                ::= { viprArpConfigEntry 5 }

viprArpInterfaceIPMask   OBJECT-TYPE
                SYNTAX IpAddress
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "The Interface IP address Mask"
                ::= { viprArpConfigEntry 6 }

viprArpInterfaceServiceType   OBJECT-TYPE
                SYNTAX INTEGER {
                       gold(1),
                       silver(2),
                       bronze(3)
                }
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "The Service type of the interface through which the
                             ARP table is learned"
                DEFVAL { gold }
                ::= { viprArpConfigEntry 7 }

viprArpIndex   OBJECT-TYPE
                SYNTAX Integer32 
                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION "The Index to the Arp table"
                ::= { viprArpConfigEntry 8 }


-- Vipr PING Table
 
 
viprPingConfigTable  OBJECT-TYPE
                SYNTAX SEQUENCE OF ViprPingConfigEntry
                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION "Table of Vipr Ping Table Information"
                ::= { viprConfig 7 }
 
viprPingConfigEntry  OBJECT-TYPE
                SYNTAX ViprPingConfigEntry
                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION "Definition for Vipr Ping Table attributes"
                INDEX { viprSlot, viprRouterId, ipAddr }
                ::= { viprPingConfigTable 1 }
 
ViprPingConfigEntry  ::= SEQUENCE {
                ipAddr                    IpAddress,
                pingStatus                INTEGER
                }
 
 
ipAddr   OBJECT-TYPE
                SYNTAX IpAddress
                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION "The IP address of the host/network to be echoed"
                ::= { viprPingConfigEntry 1 }

pingStatus   OBJECT-TYPE
                SYNTAX INTEGER {
                       alive(1),
                       not-Alive(2)
                }
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "The response value - Alive/Not Alive"
                ::= { viprPingConfigEntry 2 }

-- Vipr Router RIP Update Interval per Slot

viprRipConfigTable      OBJECT-TYPE
                SYNTAX SEQUENCE OF ViprRipConfigEntry
                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION "Table of Vipr Rip Update Interval for each card"
                ::= { viprConfig 8 }

viprRipConfigEntry      OBJECT-TYPE
                SYNTAX ViprRipConfigEntry
                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION "Definition of Rip Update attributes"
                INDEX { viprSlot }
                ::= { viprRipConfigTable 1 }

ViprRipConfigEntry  ::= SEQUENCE {
                viprRipUpdateInt        INTEGER
                }

viprRipUpdateInt        OBJECT-TYPE
                SYNTAX INTEGER  {
                        lowUpdateInt(8),
                        mediumUpdateInt(16),
                        normalUpdateInt(30)
                        }
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "This variable represents the Rip Update Interval
                             used by the RIP protocol to transmit packets.
                             The unit is in seconds. The default is 30 seconds
                             and user can change it to 8 or 16 seconds for a
                             route server card. The value will apply to all
                             VPN's in a route server card. But different
                             route server cards in different slots can have
                             separate values for Rip Update Interval"
                DEFVAL { normalUpdateInt }
                ::= { viprRipConfigEntry 1 }

--  VPN level statistics will be on the port level statistics. 

viprVpnStatsTable OBJECT-TYPE
                SYNTAX SEQUENCE OF ViprVpnStatsEntry
                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION "Statistics collected per vpn based"
                ::= { equipStatistics 9 }

viprVpnStatsEntry OBJECT-TYPE
                SYNTAX ViprVpnStatsEntry
                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION "Statistics collected per vpn based"
                INDEX { viprSlot, viprRouterId }
                ::= { viprVpnStatsTable 1 }

ViprVpnStatsEntry ::= SEQUENCE {
               viprVpnStatsTimer                     Integer32,
               viprVpnStatsIcmpOutEchoRPs            Integer32,
               viprVpnStatsIcmpOutDestURs            Integer32,
               viprVpnStatsIcmpOutSrcQ               Integer32,
               viprVpnStatsIcmpOutRedirects          Integer32,
               viprVpnStatsIcmpOutEchoRQs            Integer32,
               viprVpnStatsIcmpOutTimeXs             Integer32,
               viprVpnStatsIcmpOutParamps            Integer32,
               viprVpnStatsIcmpOutTimeRQs            Integer32,
               viprVpnStatsIcmpOutTimeRPs            Integer32,
               viprVpnStatsIcmpOutInfoRQs            Integer32,
               viprVpnStatsIcmpOutInfoRPs            Integer32,
               viprVpnStatsIcmpOutMaskRQs            Integer32,
               viprVpnStatsIcmpOutMaskRPs            Integer32,
               viprVpnStatsIcmpOutErrors             Integer32,
               viprVpnStatsIcmpInErrors              Integer32,
               viprVpnStatsIcmpInEchoRQs             Integer32,
               viprVpnStatsIcmpInAddrMasks           Integer32,
               viprVpnStatsIcmpInAddrMaskReps        Integer32,
               viprVpnStatsIcmpInEchoReps            Integer32,
               viprVpnStatsIcmpInDestUnreachs        Integer32,
               viprVpnStatsIcmpInSrcQuenchs          Integer32,
               viprVpnStatsIcmpInTimeExcds           Integer32,
               viprVpnStatsIcmpInParamProbs          Integer32,
               viprVpnStatsIcmpInTimeStamps          Integer32,
               viprVpnStatsIcmpInTimeStampsReps      Integer32
               }

viprVpnStatsTimer OBJECT-TYPE
                SYNTAX Integer32
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION " A timeout value in which the RS Module 
                             must reply to statistics requests."
                ::= { viprVpnStatsEntry 1 }

