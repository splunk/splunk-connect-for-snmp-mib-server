        OBJECTS { activeStratum }
        STATUS  current
        DESCRIPTION "Info : The secondary stratum card has become the 
                        primary primary card."
        ::= { moduleEvents 10 }


imaGroupStatusChangeNotify NOTIFICATION-TYPE
    OBJECTS {
              interfaceIndex,imaGrpChannelNeState, imaGrpChannelFeState,
              imaGrpChannelFailureStatus, imaGrpChannelSymmetry
             }
