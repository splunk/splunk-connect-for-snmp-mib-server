casConfig      OBJECT IDENTIFIER ::=   { intfConfig 20 }

casConfigTable  OBJECT-TYPE
                  SYNTAX SEQUENCE OF CasConfigEntry 
                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION "Table for CAS trunk/line configuration."
                ::= { casConfig 1 }

casConfigEntry  OBJECT-TYPE
                SYNTAX CasConfigEntry
                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION "Row definition for CAS trunk/line configuration."
                INDEX { casIfIndex } 
                ::= { casConfigTable 1 }

CasConfigEntry ::= SEQUENCE {
                            casIfIndex          IntfIndex,
                            casAdminStatus            INTEGER,
                            casOperStatus              INTEGER,
                            casProtocolType           INTEGER,
                            casDirection          INTEGER,
                            casAddressingType         INTEGER,
                            casBitPatternType         INTEGER,
                            casGroupId          INTEGER
                            }

casIfIndex OBJECT-TYPE
                  SYNTAX IntfIndex
                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION "The channel ID (slot/port/channel) in SSPPCCC 
                        format associated with the CAS trunk/line."
                  ::= { casConfigEntry 1 }

casAdminStatus  OBJECT-TYPE
                  SYNTAX INTEGER {
                                outOfService(1),
                                inService(2),
                                delete(3)
                                }
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "The administrative action:
                        outOfService-take the CAS trunk/line out of service, 
                        inService-take the CAS trunk/line in service,
                         delete-remove/deconfigure the CAS trunk/line."
                  DEFVAL { outOfService }
                ::= { casConfigEntry 2 }

casOperStatus OBJECT-TYPE
                SYNTAX INTEGER {
                                outOfService(1),
                                inService(2)
                                }
                  MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "The operational status of the CAS trunk/line:
                        outOfService-the CAS trunk/line is out of service,
                        inService-the CAS trunk/line is in service."
                DEFVAL { outOfService }
                  ::= { casConfigEntry 3 }

casProtocolType OBJECT-TYPE
                  SYNTAX INTEGER {
                                wink-start(1),
                                loop-start-network(2),
                                loop-start-user(3),
                                ground-start-network(4),
                                ground-start-user(5),
                                itu-t-r2(6)
                                }
                  MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "The type of CAS protocol to be used. 
                        The protocol must match the one used on the 
                        other end of the trunk/line."
                DEFVAL { wink-start }
                  ::= { casConfigEntry 4 }

casDirection  OBJECT-TYPE
                SYNTAX INTEGER {
                                both(1), 
                                incoming(2),
                                outgoing(3)
                                }
                  MAX-ACCESS read-write
                  STATUS current
                  DESCRIPTION "The direction of traffic flow on this CAS
                        trunk/line."
                DEFVAL { both }
                  ::= { casConfigEntry 5 }

casAddressingType OBJECT-TYPE
                SYNTAX INTEGER {
                                dtmf(1), 
                                r1-mf(2),
                                r2-mf(3) 
                                }
                  MAX-ACCESS read-write
                  STATUS current
                  DESCRIPTION "The addressing type used for the CAS trunk/line.
                                dtmf  -- dual tone multifrequency
                                r1-mf -- R1 multifrequency,
                                r2-mf -- R2 multifrequency.
                            Type of 're-mf' is valid only when casProtocolType 
                            is 'wink start', while 'r2-mf' is valid only when
                            casProtocolType is 'itu-t r2' ."
                  DEFVAL { dtmf }
                  ::= { casConfigEntry 6 }

casBitPatternType OBJECT-TYPE
                  SYNTAX INTEGER {
                                ab(1),
                                abcd(2)
                                }
                  MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "The type of bit pattern used for supervisory 
                         signaling. The value is AB when SF framing is
                              used, ABCD when ESF framing is used for DS1.
                        In case of E1, the value is always ABCD."
                  ::= { casConfigEntry 7 }

casGroupId OBJECT-TYPE
                  SYNTAX INTEGER (0..255)
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "The identifier to associate a group of CAS
                        trunks/lines which have same reachability.
                        When SG is disabled, it must be specified with a
                        non-zero value. When SG is enabled, the value should
                        be set to zero which indicate no routing performed
                        based on the value."
                DEFVAL { 0 }
                ::= { casConfigEntry 8 }


--
-- tone announcement server configuration table
--
toneAnnceConfigTable OBJECT-TYPE
                SYNTAX SEQUENCE OF ToneAnnceConfigEntry
                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION "Table for the Tone Announcement Interface"
                ::= { intfConfig 21 }

toneAnnceConfigEntry OBJECT-TYPE
                SYNTAX ToneAnnceConfigEntry
                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION "Row definition for Tone Announcement interface"
                INDEX { toneAnnceIfIndex }
                ::= { toneAnnceConfigTable 1 }


ToneAnnceConfigEntry ::= SEQUENCE {
                toneAnnceIfIndex       IntfIndex,
                toneAnnceAdminStatus   INTEGER,
                toneAnnceOperStatus    INTEGER
                }

toneAnnceIfIndex     OBJECT-TYPE
                SYNTAX IntfIndex
                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION "interface index (slot/port/channel) SSPPCCC"
                ::= { toneAnnceConfigEntry 1 }

toneAnnceAdminStatus OBJECT-TYPE
                SYNTAX INTEGER {
                                outOfService(1),
                                inService(2),
                                delete(3)
                                }
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "The administrative action:
                        'outOfService' means take the interface out of service
                        'inService'    means take the interface in service
                        'delete'       remove/deconfigure the interface."
                DEFVAL { outOfService }
                ::= { toneAnnceConfigEntry 2 }

toneAnnceOperStatus  OBJECT-TYPE
                SYNTAX INTEGER {
                                outOfService(1),
                                inService(2)
                               }
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "The operational status of the interface:
                        'outOfService' means the interface is out of service 
                        'inService'    means the interface is in service"
                DEFVAL { outOfService }
                ::= { toneAnnceConfigEntry 3 }





-- GR303 Interface table 
gr303IntfConfig             OBJECT IDENTIFIER ::=   { intfConfig 22 }

gr303IntfConfigTable OBJECT-TYPE
                SYNTAX SEQUENCE OF Gr303IntfConfigEntry
                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION "Table of GR303 interface configuration attributes"
                ::= { gr303IntfConfig 1 }

gr303IntfConfigEntry OBJECT-TYPE
                SYNTAX Gr303IntfConfigEntry
                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION "Row definition for GR303  interface configuration
                                            table"
                INDEX { gr303IntfId }
                ::= { gr303IntfConfigTable 1 }

Gr303IntfConfigEntry ::= SEQUENCE {
                gr303IntfId                Integer32,
                gr303TMCChannelId  Integer32,      
                gr303EOCChannelId  Integer32,      
                gr303IntfAdminStatus INTEGER,
                gr303IntfOperStatus INTEGER
        }

gr303IntfId        OBJECT-TYPE
                SYNTAX Integer32
                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION "Interface ID (SSPPCCC)"
                ::= { gr303IntfConfigEntry 1 }

gr303TMCChannelId        OBJECT-TYPE
                SYNTAX Integer32 (0..24)
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "TMC-Channel Number. If TMC Channel is set to 0
                             there will be no  TMC channel on the
                             corresponding DS1. Generally the TMC channel
                             should be specified on the one of the DS1's in a
                             GR303 interface group and should be on DS0 no.
                             24"
                DEFVAL { 0 }
                ::= { gr303IntfConfigEntry 2 }

gr303EOCChannelId        OBJECT-TYPE
                SYNTAX Integer32 (0..24)
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "EOC-Channel Number. If EOC Channel is set to 0
                             there will be no  EOC channel on the
                             corresponding DS1. Generally the TMC channel
                             should be specified on the one of the DS1's in a
                             GR303 interface group and should be on DS0 no.
                             12"
                DEFVAL { 0 }
                ::= { gr303IntfConfigEntry 3 }
gr303IntfAdminStatus        OBJECT-TYPE
                SYNTAX INTEGER   {
                        outOfService (1),
                        inService (2),
                        delete (3)

                    }
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "GR303 interface admin status"
                ::= { gr303IntfConfigEntry 4 }

gr303IntfOperStatus        OBJECT-TYPE
                SYNTAX INTEGER   {
                                    outOfService (1),
                        inService    (2)
                    }
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "GR303 interface operational status"
                ::= { gr303IntfConfigEntry 5 }


