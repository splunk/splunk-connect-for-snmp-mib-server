        SYNTAX INTEGER  {
                priority-1(1),
                priority-2(2),
                priority-3(3),
                priority-4(4)
                }
        MAX-ACCESS read-write
        STATUS current
        DESCRIPTION "Priority of TrunkingSigVcc - priority-1 being highest priority ."
        DEFVAL { priority-1 }
        ::= { atmTrunkingSigVccEntry 25 }


atmTrunkingSigVccRetryCounter OBJECT-TYPE
        SYNTAX Integer32
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION "Indicates how many attempts to establish the
                 connection have failed.  It is reset whenever a
                 connection is successfully established or the SigVcc SPVC
                 is restarted."
        ::= { atmTrunkingSigVccEntry 26 }

 
