                SYNTAX IpAddress
                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION "Entry IP address."
                ::= { arp1Entry 3 }

arp1PhysicalAddr OBJECT-TYPE
                SYNTAX OCTET STRING (SIZE(6))
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "Entry physical address."
                ::= { arp1Entry 4 }

arp1EntryAge        OBJECT-TYPE
                SYNTAX Integer32
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "Age of cache entry."
                DEFVAL { 0 }
                ::= { arp1Entry 5 }


