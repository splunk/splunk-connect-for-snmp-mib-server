                        vbrAtmSpvcVccConnType           INTEGER,
                        vbrAtmSpvcVccVbrPortAddr        OCTET STRING,
                        vbrAtmSpvcVccOperStatus         INTEGER,
                        vbrAtmSpvcVccRowStatus          RowStatus,
                        vbrAtmSpvcVccViB                INTEGER,
                        vbrAtmSpvcVccTrafficShapingA2B  INTEGER
                                }

