                SYNTAX OCTET STRING (SIZE(1..20))
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "local Vbr port nsap address"
                ::= { vbrAtmSpvcVccEntry 14 }

vbrAtmSpvcVccOperStatus OBJECT-TYPE
                SYNTAX INTEGER {
                                other(1),
                                establishmentInProgress(2),
                                connected(3),
                                retriesExhausted(4),
                                noAddressSupplied(5),
                                lowerLayerDown(6)
                                } 
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "other - none of the types specified below;
                             establishmentInProgress - connection is not 
                                operational, but call attempts are ongoing;
                             connected - connection is currently operational;
                             retriesExhausted - retry limit has been reached 
                                and call attempts have ceased;
                             noAddressSupplied - no remote address has been 
                                configured, so no call attempts are initiated;
                             lowerLayerDown - SPVC handler is not operational."
                ::= { vbrAtmSpvcVccEntry 15 }

vbrAtmSpvcVccRowStatus OBJECT-TYPE
                SYNTAX RowStatus
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "Used to create and delete a soft PVCC.  When this 
                             object is set to 'active' an attempt is made to 
                             set up the soft PVCC.  When this object has the 
                             value 'active' and is set to another value, any 
                             set-up or connection in-progess is released."
                ::= { vbrAtmSpvcVccEntry 16 }

vbrAtmSpvcVccViB OBJECT-TYPE
                SYNTAX INTEGER (0..1024)
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "VI to associate the Egress part of the
                        B2A connection ."
                DEFVAL { 0 }
                ::= { vbrAtmSpvcVccEntry 17 }

vbrAtmSpvcVccTrafficShapingA2B     OBJECT-TYPE
                SYNTAX INTEGER {
                                disabled(1),
                                enabled(2)
                               }
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "Traffic Shaping Support in the A2B direction."
                DEFVAL { disabled }
                ::= { vbrAtmSpvcVccEntry 18 }


vbrAtmSpvcVccActiveSvcTable OBJECT-TYPE
                SYNTAX SEQUENCE OF VbrAtmSpvcVccActiveSvcEntry
                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION "The table used to manage active SVCs established 
                             across ATM networks between ATM entities."
                ::= { connectionConfig 22 }

vbrAtmSpvcVccActiveSvcEntry OBJECT-TYPE
                SYNTAX VbrAtmSpvcVccActiveSvcEntry
                MAX-ACCESS  not-accessible
                STATUS current
                DESCRIPTION "An entry in the Terminal Emulation ATM active SVC
                             table. There is one entry in the table 
                             corresponding to each active row in the 
                             vbrAtmSpvcVccTable for which the 
                             vbrAtmSpvcVccConnType
                             takes the value 'activeSvc'."
                INDEX { vbrAtmSpvcVccIfA }
                ::= { vbrAtmSpvcVccActiveSvcTable 1 }

VbrAtmSpvcVccActiveSvcEntry ::= SEQUENCE {
                        vbrAtmSpvcVccRemoteVbrPortAddr        OCTET STRING,
                        vbrAtmSpvcVccRetryInterval        INTEGER,
                        vbrAtmSpvcVccRetryLimit                INTEGER,
                        vbrAtmSpvcVccRestart                INTEGER,
                        vbrAtmSpvcVccRetryFailure        Integer32,
                        vbrAtmSpvcVccReleaseCause        INTEGER,
                        vbrAtmSpvcVccReleaseDiag        OCTET STRING
                }

vbrAtmSpvcVccRemoteVbrPortAddr OBJECT-TYPE
                SYNTAX OCTET STRING (SIZE(1..20))
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "remote VBR port nsap address implemening IWF, not 
                             meaningful if passive type of connection"
                ::= { vbrAtmSpvcVccActiveSvcEntry 1 }

vbrAtmSpvcVccRetryInterval OBJECT-TYPE
                SYNTAX INTEGER (0..3600)
                UNITS "seconds"
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "the time to wait before attempting to establish 
                             the SVC after previous failed call attempt. 
                             0 indicates retry will be attempted immediately."
                DEFVAL { 10 }
                ::= { vbrAtmSpvcVccActiveSvcEntry 2 }

vbrAtmSpvcVccRetryLimit        OBJECT-TYPE
                SYNTAX INTEGER (0..65535)
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "a maximum limit on how many retries. 
                             0 indicates no limit."
                DEFVAL { 0 }
                ::= { vbrAtmSpvcVccActiveSvcEntry 3 }

vbrAtmSpvcVccRestart OBJECT-TYPE
                SYNTAX INTEGER {
                                restart(1),
                                noop(2)
                                }
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "restart, the active SVC is released if necessary 
                             and a new setup procedure is begun; or noop"
                ::= { vbrAtmSpvcVccActiveSvcEntry 4 }

vbrAtmSpvcVccRetryFailure OBJECT-TYPE
                SYNTAX Integer32 
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "indicates how many attempts to establish the 
                             connection have failed.  It is reset whenever a 
                             connection is successfully established or the SVC 
                             is restarted."
                ::= { vbrAtmSpvcVccActiveSvcEntry 5 }

vbrAtmSpvcVccReleaseCause OBJECT-TYPE
                SYNTAX INTEGER(1..127) 
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "CAUSE field value of the last RELEASE message 
                             received for this SVC"
                ::= { vbrAtmSpvcVccActiveSvcEntry 6 }

vbrAtmSpvcVccReleaseDiag OBJECT-TYPE
                SYNTAX OCTET STRING (SIZE(0..8)) 
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "DIAGNOSTICS field value of the last RELEASE 
                             message received for this SVC"
                ::= { vbrAtmSpvcVccActiveSvcEntry 7 }


atmSpvcVccRlsTable OBJECT-TYPE
                SYNTAX SEQUENCE OF AtmSpvcVccRlsEntry
