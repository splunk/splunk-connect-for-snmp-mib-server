-- CPU Version: V07.01.C00





ACMIB DEFINITIONS ::= BEGIN
 
               IMPORTS
                  MODULE-IDENTITY, OBJECT-TYPE, OBJECT-IDENTITY, 
                  NOTIFICATION-TYPE, Counter32, Integer32, IpAddress, 
                  TimeTicks,  enterprises
                      FROM SNMPv2-SMI
                  TEXTUAL-CONVENTION, DisplayString,
                  TimeStamp, RowStatus, DateAndTime
                      FROM SNMPv2-TC
                  MODULE-COMPLIANCE, OBJECT-GROUP
                      FROM SNMPv2-CONF
                  ifIndex, mib-2
                      FROM RFC1213-MIB;
 
lucent    OBJECT IDENTIFIER ::= { enterprises 1751 }
 
mibs      OBJECT IDENTIFIER ::= { lucent 2 }
 
acMIB           MODULE-IDENTITY
                    LAST-UPDATED "000204081230Z"
                    ORGANIZATION "Lucent Technologies, Inc."
                    CONTACT-INFO
                      "
                      "

                    DESCRIPTION
                     "Lucent Access Concentrator, Release 6.2"
                    ::= { mibs 18 }

          IntfIndex ::= TEXTUAL-CONVENTION
              STATUS    current
              DESCRIPTION "A INTEGER "
              SYNTAX Integer32

-- For new Connection status data type

        ConnFailureCause ::= TEXTUAL-CONVENTION
        STATUS  current
        DESCRIPTION     "General connection failure cause"
        SYNTAX  INTEGER {
                           none(1),
                           physical(2)
                        }


        FrConnFailureCause ::= TEXTUAL-CONVENTION
        STATUS  current
        DESCRIPTION "Frame Relay connection failure cause"
        SYNTAX  INTEGER {
                            none(1),
                            physical(2),
                            lmi(3),
                            dlci(4)
                        }

AtmConnFailureCause ::= TEXTUAL-CONVENTION
        STATUS  current
        DESCRIPTION  "ATM connection failure cause"
        SYNTAX  INTEGER {
                            none(1),
                            physical(2),
                            oamAis(3),
                            oamRdi(4)
                        }

-- End of adding new connection status data type

-- added imaGrpChannelEndState type

      ImaGrpChannelEndState ::= TEXTUAL-CONVENTION
        STATUS current
        DESCRIPTION "The current operational state of the Near-end or far-end IM
                     A Group State Machine."
        SYNTAX INTEGER  {
                            notConfigured(1),
                            startUp(2),
                            startUpAck(3),
                            cfgAbtUnsupportedM(4),
                            cfgAbtIncSymmetry(5),
                            cfgAbtOther(6),
                            insufficientLinks(7),
                            blocked(8),
                            operational(9)
                        }

          -- Groups in the Access Concentrator MIB

          sysConfig             OBJECT IDENTIFIER ::=   { acMIB 1 }

          equipConfig           OBJECT IDENTIFIER ::=   { acMIB 2 }

          channelConfig         OBJECT IDENTIFIER ::=   { acMIB 3 }

          intfConfig            OBJECT IDENTIFIER ::=   { acMIB 5 }

          connectionConfig      OBJECT IDENTIFIER ::=   { acMIB 6 }

          userServiceConfig     OBJECT IDENTIFIER ::=   { acMIB 7 }

          sysTraps              OBJECT IDENTIFIER ::=   { acMIB 8 }
          systemEvents          OBJECT IDENTIFIER ::=   { sysTraps 1 }
          moduleEvents          OBJECT IDENTIFIER ::=   { sysTraps 2 }
          interfaceEvents       OBJECT IDENTIFIER ::=   { sysTraps 3 }
          connectionEvents      OBJECT IDENTIFIER ::=   { sysTraps 4 }

          sysStatistics         OBJECT IDENTIFIER ::=   { acMIB 9 }

          equipStatistics       OBJECT IDENTIFIER ::=   { acMIB 10 }

          intfStatistics        OBJECT IDENTIFIER ::=   { acMIB 11 }
 
          connectionStatistics  OBJECT IDENTIFIER ::=   { acMIB 12 }
 
          ipAtmGroup            OBJECT IDENTIFIER ::=   { acMIB 13 }
 
          iispRouting           OBJECT IDENTIFIER ::=   { acMIB 14 }
 
          returnCode            OBJECT IDENTIFIER ::=   { acMIB 15 }
 
          sysControl            OBJECT IDENTIFIER ::=   { acMIB 16 }
 
          stratumConfig         OBJECT IDENTIFIER ::=   { acMIB 17 }
 
          cellTest              OBJECT IDENTIFIER ::=   { acMIB 18 }

          sysInfo               OBJECT IDENTIFIER ::=   { acMIB 19 }

          ethernetConfig        OBJECT IDENTIFIER ::=   { acMIB 20 }

          alarmConfig              OBJECT IDENTIFIER ::=   { acMIB 21 }

          sysTest               OBJECT IDENTIFIER ::=   { acMIB 22 }

          fwDriverSup           OBJECT IDENTIFIER ::=   { acMIB 23 }

          viprConfig            OBJECT IDENTIFIER ::=   { acMIB 24 }

          toneAnnceControl      OBJECT IDENTIFIER ::=   { acMIB 25 } 

          gr303Config           OBJECT IDENTIFIER ::=   { acMIB 26 }

          gr303Stats            OBJECT IDENTIFIER ::=   { acMIB 27 }

          atmTrunkingMIBObjects   OBJECT IDENTIFIER ::= { acMIB 28 }

          protectionGroup       OBJECT IDENTIFIER ::=   { acMIB 29 }

          h248Config            OBJECT IDENTIFIER ::=   { acMIB 30 }

          h248SysControl        OBJECT IDENTIFIER ::=   { acMIB 31 }

 
-- System Configuration Group sysConfig

siteName          OBJECT-TYPE
                  SYNTAX DisplayString (SIZE(20))
                  MAX-ACCESS read-write
                  STATUS current
                  DESCRIPTION "site name - ascii"
                  ::= { sysConfig 1 }

siteID            OBJECT-TYPE
                  SYNTAX OCTET STRING (SIZE(4))
                  MAX-ACCESS read-write
                  STATUS current
                  DESCRIPTION "site id - numerical"
                  ::= { sysConfig 2 }

siteEtherIpAddr   OBJECT-TYPE
                  SYNTAX IpAddress
                  MAX-ACCESS read-write
                  STATUS current
                  DESCRIPTION "ip address for ethernet access to the site
                               (switch)"
                  ::= { sysConfig 3 }

siteEtherIpMask OBJECT-TYPE
                SYNTAX IpAddress
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "ip mask for ethernet access to the site
                               (switch)"
                 ::= { sysConfig 4 }

siteGatewayAddr OBJECT-TYPE
                SYNTAX IpAddress
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "ip address for gateway access to the site
                              (switch)"
                 ::= { sysConfig 5 }

siteAtmAddrPrefix1 OBJECT-TYPE
                SYNTAX OCTET STRING (SIZE(13))
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "This is one of the two atm address prefix 
                             of this switch. "
                ::= { sysConfig 6 }

siteAtmAddrUser1 OBJECT-TYPE
                SYNTAX OCTET STRING (SIZE(7))
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "This is one of the two atm address user part 
                              of this switch."
                ::= { sysConfig 7 }

siteRemoteMgrAddr1 OBJECT-TYPE
                SYNTAX IpAddress
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "remote manager IP Address, where TRAP will be
                             sent to."
                ::= { sysConfig 8 }

