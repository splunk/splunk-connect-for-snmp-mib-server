        DESCRIPTION "ATM Trunking Signalling VCC SPVC Retry Limit. 
                     0 indicates unlimited "
        DEFVAL { 0 }
        ::= { atmTrunkingSigVccEntry 23}

atmTrunkingSigVccRestart OBJECT-TYPE
        SYNTAX INTEGER  {
                restart(1),
                noop(2)
                }
        MAX-ACCESS read-write
        STATUS current
        DESCRIPTION "The active ATM Trunking SPVC is released, if necessary, 
                 and a new setup procedure is begun if restart,
                 otherwise, noop."
        ::= { atmTrunkingSigVccEntry 24 }

