                             cirEmAtmPvcVccRowStatus             RowStatus,
                             cirEmAtmPvcVccCngToneDetectionA2B   INTEGER,
                             cirEmAtmPvcVccCodingTranslationA2B  INTEGER,
                             cirEmAtmPvcVccVoiceCompressionB2A   INTEGER,
                             cirEmAtmPvcVccSilenceDetectionB2A   INTEGER,
                             cirEmAtmPvcVccEchoCancellationB2A   INTEGER,
                             cirEmAtmPvcVccCngToneDetectionB2A   INTEGER,
                             cirEmAtmPvcVccCodingTranslationB2A  INTEGER,
                             cirEmAtmPvcVccSarTypeB              INTEGER,
                             cirEmAtmPvcVccConnectionStatus      INTEGER,

                             cirEmAtmPvcVccDualHomed             INTEGER,

                             cirEmAtmPvcVccOamStatus             INTEGER,
                             cirEmAtmPvcVccViB                   INTEGER,
