                SYNTAX INTEGER  {
                                 disabled(1),
                                 enabled(2)
                                }
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "Traffic Shaping for Conn was enabled
                             primarily because the core would lose
                             cells since we were non-conformant cells.
                             Therefore the hardware added traffic
                             shaping per connection. But condition is
                             VI should not be enabled if this feature
                             is enabled."
                DEFVAL { disabled }
                ::= { atmPnniConfigEntry 78 }


-- End of 1  MIB Table Entries



-- The 1  Interface Configuration Group

atmVnnConfigTable OBJECT-TYPE
        SYNTAX SEQUENCE OF AtmVnnConfigEntry
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION "Table of atm user to network interface (UNI)"
        ::= { intfConfig 17 }

atmVnnConfigEntry OBJECT-TYPE
        SYNTAX AtmVnnConfigEntry
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION "Row definition of atm UNI"
        INDEX { atmVnnIfIndex } 
        ::= { atmVnnConfigTable 1 }

AtmVnnConfigEntry ::= SEQUENCE {
        atmVnnIfIndex               IntfIndex,
        atmVnnAddrType              INTEGER,
        atmVnnSvcMinVpi             INTEGER,
        atmVnnSvcMaxVpi             INTEGER,
        atmVnnSvcMinVci             INTEGER,
        atmVnnSvcMaxVci             INTEGER,
        atmVnnAdminStatus           INTEGER,
        atmVnnOperStatus            INTEGER,
        atmVnnOverSubscription      INTEGER,
        atmVnnNwPrefix1             OCTET STRING,
        atmVnnT303                  Integer32,
        atmVnnT308                  Integer32,
        atmVnnT309                  Integer32,
        atmVnnT310                  Integer32,
        atmVnnT313                  Integer32,
        atmVnnT316                  Integer32,
        atmVnnT317                  Integer32,
        atmVnnT322                  Integer32,
        atmVnnT398                  Integer32,
        atmVnnT399                  Integer32,
        atmVnnMyNeighborIpAddress   IpAddress,
        atmVnnMyNeighborIfName      DisplayString,
        atmVnnVpiSig                INTEGER,
        atmVnnVciSig                INTEGER,
        atmVnnVpiIlmi               INTEGER,
        atmVnnVciIlmi               INTEGER,
        atmVnnAtmSignalingStatus    INTEGER,
        atmVnnMyIfName              DisplayString,
        atmVnnIlmiProtocolStatus    INTEGER,
        atmVnnIlmiOperStatus        INTEGER,
        atmVnnIlmiConfigProc        INTEGER,
        atmVnnIlmiConnectProc       INTEGER,
        atmVnnIlmiAttachPtProc      INTEGER,
        atmVnnIlmiSecureLinkProc    INTEGER,
        atmVnnIlmiAddrRegProc       INTEGER,
        atmVnnIlmiTimerS            Integer32,
        atmVnnIlmiTimerT            Integer32,
        atmVnnIlmiRetryK            Integer32,
        atmVnnAvailBwIgr            Integer32,
        atmVnnAvailBwEgr            Integer32,
        atmVnnUPCSupp                     INTEGER,
        atmVnnUbrLoadBalSupp        INTEGER,
        atmVnnSscopRxWndSz          INTEGER
        }

atmVnnIfIndex OBJECT-TYPE
        SYNTAX IntfIndex
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION "interface index (slot/port/channel) SSPPCCC"
        ::= { atmVnnConfigEntry 1 }

