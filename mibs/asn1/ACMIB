atmPvcVpcVUNIB        OBJECT-TYPE
                SYNTAX INTEGER (0..255)
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "VUNI Id to associate the Egress part of the 
                             A2B connection ."
                DEFVAL { 0 }
                ::= { atmPvcVpcEntry 28 }



atmPvcVpcOamStatusB OBJECT-TYPE
                SYNTAX INTEGER {
                                unsupp(1),
                                end-Pt(2),
                                seg-Pt(3),
                                end-Seg-Pt(4),
                                intermediate-Pt(5),
                                intra-Segment-Pt(6)
                                }
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "Indicates the Oam monitoring status of side B
                        of an internal connection. Refer to ITU I.610 and
                        Bellcore GR1248-CORE for definitions.
                        The 6 possible states are:
                        Unsupp:  No OAM cell monitoring or generation 
                                enabled. All OAM flows are transparently 
                                passed through.
                        End-Pt:  Connection endPoint, segment flows are 
                                discarded.  
                        Seg-Pt:  Segment endPoint.
                              End-Seg-Pt: Connection endPoint that is also a 
                                segment endPoint.
                        Intermediate-Pt: Connecting point, that is outside 
                                any segment.  Generates only ete flows.  
                        Intra-Segment-Pt: Connecting point with a segment.
                                Generates ete as well as segment flows."
                 DEFVAL { unsupp }
                 ::= { atmPvcVpcEntry 29 }

atmPvcVpcOAMConnEndPtStatusA    OBJECT-TYPE
                 SYNTAX INTEGER {
                                ete-active(1),
                                ete-ais-state(2),
                                ete-rdi-state(3),
                                ete-ais-rdi-state(4)
                                }
                 MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "Connection endpoint side A status. Valid only 
                        at connection endpoint."
                 ::= { atmPvcVpcEntry 30 }

atmPvcVpcOAMConnEndPtStatusB    OBJECT-TYPE
                 SYNTAX INTEGER {
                                ete-active(1),
                                ete-ais-state(2),
                                ete-rdi-state(3),
                                ete-ais-rdi-state(4)
                                }
                 MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "Connection endpoint side B status. Valid only at 
                        connection endpoint."
                ::= { atmPvcVpcEntry 31 }

atmPvcVpcOAMConnSegEndPtStatusA    OBJECT-TYPE
                 SYNTAX INTEGER {
                                seg-active(1),
                                seg-ais-state(2),
                                seg-rdi-state(3),
                                seg-ais-rdi-state(4)
                                }
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "Segment endpoint side A status. Valid only at 
                        segment/connection endpoints."
                ::= { atmPvcVpcEntry 32 }

atmPvcVpcOAMConnSegEndPtStatusB    OBJECT-TYPE
                 SYNTAX INTEGER {
                                seg-active(1),
                                seg-ais-state(2),
                                seg-rdi-state(3),
                                seg-ais-rdi-state(4)
                                }
                MAX-ACCESS read-only
                STATUS current
                 DESCRIPTION "Segment endpoint side B status. Valid only at 
