                atmTrunkingBrVccMaxFrameLength               INTEGER,
                atmTrunkingBrVccIdleChannSuprSupp            INTEGER,
                atmTrunkingBrVccSilenceDetSupp               INTEGER,
                atmTrunkingBrVccCompressionSpec              INTEGER,
                atmTrunkingBrVccAudioProfileSource           INTEGER,
                atmTrunkingBrVccAudioProfileId               INTEGER,
                atmTrunkingBrVccAvailBw                      INTEGER,
                atmTrunkingBrVccRowStatus                    RowStatus,
