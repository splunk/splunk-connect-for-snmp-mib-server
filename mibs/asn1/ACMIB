-- intfAtmPvcConnDisplayTable

intfAtmPvcConnDisplayTable         OBJECT-TYPE
                    SYNTAX SEQUENCE OF IntfAtmConnDisplayEntry
                    MAX-ACCESS not-accessible
                    STATUS current
                    DESCRIPTION "List of Pvc Connections on an Interface."
                    ::= { connectionConfig 39 }

intfAtmPvcConnDisplayEntry         OBJECT-TYPE
                SYNTAX IntfAtmConnDisplayEntry
                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION "Gives list of pvc going towards the backplane
                             on a particluar interface"
                             
                INDEX { intfAtmPvcConnDisplaySlot,intfAtmPvcConnDisplayPort,
                        intfAtmPvcConnDisplayChannel,intfAtmPvcConnDisplayVpi,
                        intfAtmPvcConnDisplayVci}
                ::= { intfAtmPvcConnDisplayTable 1 }

IntfAtmConnDisplayEntry ::= SEQUENCE {
                             intfAtmPvcConnDisplaySlot                Integer32,
                             intfAtmPvcConnDisplayPort                Integer32,
                             intfAtmPvcConnDisplayChannel                Integer32,
                           intfAtmPvcConnDisplayVpi                INTEGER,
                           intfAtmPvcConnDisplayVci                INTEGER,
                           intfAtmPvcConnDisplayFlowType         INTEGER
                          }

intfAtmPvcConnDisplaySlot        OBJECT-TYPE
                SYNTAX Integer32
                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION "Slot of the particular interface"
                ::= { intfAtmPvcConnDisplayEntry 1 }

intfAtmPvcConnDisplayPort        OBJECT-TYPE
                SYNTAX Integer32
                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION "Port of the particular interface"
                ::= { intfAtmPvcConnDisplayEntry 2 }

intfAtmPvcConnDisplayChannel        OBJECT-TYPE
                SYNTAX Integer32
                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION "Channel of the particular interface"
                ::= { intfAtmPvcConnDisplayEntry 3 }

intfAtmPvcConnDisplayVpi OBJECT-TYPE
                SYNTAX INTEGER (0..4095)
                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION "VPI value for connection on Intf
                             Valid range is a number between 0 and 4095."
                ::= { intfAtmPvcConnDisplayEntry 4 }

intfAtmPvcConnDisplayVci OBJECT-TYPE
                SYNTAX INTEGER (0..65535)
                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION "VCI value for connection on Intf 
                             Valid range is a number between 0 and 65535."
                ::= { intfAtmPvcConnDisplayEntry 5 }

intfAtmPvcConnDisplayFlowType OBJECT-TYPE
                SYNTAX INTEGER {
                                cnctnValid(1),
                                cnctnNull(2),
                                cnctnPtmpValid(3),
                                cnctnPtmpNull(4)
                               }
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "Flow type:Indicates Flow Towards the Back Plane
                             cnctnValid: Traffic flow towards BP is enabled.
                             cnctnNull: Traffic flow towards BP is disabled.
                             cnctnPtmpValid: Traffic flow towards BP is enabled
                                             and it is point to multipoint
                                             connection.
                             cnctnPtmpNull: Traffic flow towards BP is disabled
                                             and it is point to multipoint
                                             connection."
                ::= { intfAtmPvcConnDisplayEntry 6}

