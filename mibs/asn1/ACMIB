                SYNTAX INTEGER {
                                outOfService(1),
                                inService(2)
                               }
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "The operational status of the interface:
                             'outOfService' interface is out of service
                             'inService'    interface is in service"
                DEFVAL { outOfService }
                ::= { atmImaConfigEntry 9 }

atmPnniConfigTable  OBJECT-TYPE
                    SYNTAX SEQUENCE OF AtmPnniConfigEntry
                    MAX-ACCESS not-accessible
                    STATUS current
                    DESCRIPTION "Table of atm PNNI Configuration"
                    ::= { intfConfig 16 }

atmPnniConfigEntry  OBJECT-TYPE
                    SYNTAX AtmPnniConfigEntry
                    MAX-ACCESS not-accessible
                    STATUS current
                    DESCRIPTION "Row definition of PNNI Configuration"
                    INDEX { atmPnniIfIndex }
                    ::= { atmPnniConfigTable 1 }

AtmPnniConfigEntry      ::= SEQUENCE {
                        atmPnniIfIndex                            IntfIndex,
                        atmPnniSvcMinVpi                       INTEGER,
                        atmPnniSvcMaxVpi                       INTEGER,
                        atmPnniSvcMinVci                       INTEGER,
                        atmPnniSvcMaxVci                       INTEGER,
                        atmPnniAdminStatus                       INTEGER,
                        atmPnniOperStatus                       INTEGER,
                        atmPnniT303                               Integer32,
                        atmPnniT308                               Integer32,
                        atmPnniT309                               Integer32,
                        atmPnniT310                               Integer32,
                        atmPnniT313                               Integer32,
                        atmPnniT316                               Integer32,
                        atmPnniT317                               Integer32,
                        atmPnniT322                               Integer32,
                        atmPnniT397                               Integer32,
                        atmPnniT398                               Integer32,
                        atmPnniT399                               Integer32,
                        atmPnniCbrServCatSupp                       INTEGER,
                             atmPnniCbrGcacClpEqualZero               INTEGER,
                          atmPnniCbrMaxCellRate                        Integer32,
                        atmPnniCbrAvailCellRate                       Integer32,
                        atmPnniCbrCellXerDelay                       Integer32,
                        atmPnniCbrCellLossRatioZero               Integer32,
                        atmPnniCbrCellLossRatioZeroPlusOne     Integer32,
                        atmPnniCbrCellDelayVar                       Integer32,
                        atmPnniRtVbrServCatSupp                       INTEGER,
                        atmPnniRtVbrGcacClpEqualZero               INTEGER,
                        atmPnniRtVbrMaxCellRate                       Integer32,
                        atmPnniRtVbrAvailCellRate               Integer32,
                        atmPnniRtVbrCellXerDelay               Integer32,
                        atmPnniRtVbrCellLossRatioZero               Integer32,
                        atmPnniRtVbrCellLossRatioZeroPlusOne   Integer32,
                        atmPnniRtVbrCellDelayVar               Integer32,
                        atmPnniRtVbrGcacPresent                       INTEGER,
                        atmPnniRtVbrCellRateMargin               Integer32,
                        atmPnniRtVbrVarianceFactor               Integer32,
                        atmPnniNrtVbrServCatSupp               Integer32,
                        atmPnniNrtVbrGcacClpEqualZero               Integer32,
                        atmPnniNrtVbrMaxCellRate               Integer32,
                        atmPnniNrtVbrAvailCellRate               Integer32,
                        atmPnniNrtVbrCellXerDelay               Integer32,
                        atmPnniNrtVbrCellLossRatioZero               Integer32,
                        atmPnniNrtVbrCellLossRatioZeroPlusOne  Integer32,
                        atmPnniNrtVbrGcacPresent               INTEGER,
                        atmPnniNrtVbrCellRateMargin               Integer32,
                        atmPnniNrtVbrVarianceFactor               Integer32,
                        atmPnniAbrServCatSupp                       Integer32,
                        atmPnniAbrMaxCellRate                       Integer32,
                        atmPnniAbrAvailCellRate                       Integer32,
                        atmPnniUbrServCatSupp                       Integer32,
                        atmPnniUbrMaxCellRate                       Integer32,
                        atmPnniOverSubscription                INTEGER,
                        atmPnniSgnlEnabled                     INTEGER,
                        atmPnniVpiSig                               INTEGER,
                        atmPnniVciSig                                      INTEGER,
                        atmPnniAvailBwIgr                         Integer32,
                        atmPnniAvailBwEgr                         Integer32,
                        atmPnniUPCSupp                                INTEGER,
                        atmPnniUbrLoadBalSupp                        INTEGER,
                        atmPnniSscopRxWndSz                       INTEGER,
                        atmPnniRemoteSvcMinVpi                       INTEGER,
                        atmPnniRemoteSvcMaxVpi                       INTEGER,
                        atmPnniRemoteSvcMinVci                 INTEGER,
                        atmPnniRemoteSvcMaxVci                 INTEGER,
                        atmPnniVpiIlmi                               INTEGER,
                        atmPnniVciIlmi                               INTEGER,
                        atmPnniIlmiProtocolStatus              INTEGER,
                        atmPnniIlmiOperStatus                        INTEGER,
                        atmPnniIlmiConfigProc                       INTEGER,
                        atmPnniIlmiConnectProc                 INTEGER,
                        atmPnniIlmiAttachPtProc                INTEGER,
                        atmPnniIlmiSecureLinkProc               INTEGER,
                        atmPnniIlmiAddrRegProc                 INTEGER,
                        atmPnniIlmiTimerS                       Integer32,
                        atmPnniIlmiTimerT                       Integer32,
                        atmPnniIlmiRetryK                       Integer32,
                        atmPnniConnTrafficShapingFlag          INTEGER
                    }                        

