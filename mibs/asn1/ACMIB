                        ds3PortStatsId                        Integer32,
                        ds3PortStatsTimer                Integer32,
                        ds3PortStatsCurrentSEFSs        Integer32,
                        ds3PortStatsCurrentLCVs                Integer32,
                        ds3PortStatsCurrentLESs                Integer32
                        }

