atmImaConfigTable OBJECT-TYPE
                SYNTAX SEQUENCE OF AtmImaConfigEntry
                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION "Inverse Multiplexing interface table."
                ::= { intfConfig 15 }

atmImaConfigEntry OBJECT-TYPE
                SYNTAX AtmImaConfigEntry
                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION "Row definition for Inverse Multiplexing
                              attributes"
                INDEX { atmImaIntfIndex } 
                ::= { atmImaConfigTable 1 }

AtmImaConfigEntry ::= SEQUENCE {
                        atmImaIntfIndex        IntfIndex,
                        atmImaNeTxState        INTEGER,
                        atmImaNeRxState        INTEGER,
                        atmImaFeTxState        INTEGER,
                        atmImaFeRxState        INTEGER,
                        atmImaNeRxDefect       INTEGER,
                        atmImaFeRxDefect       INTEGER,
                        atmImaAdminStatus      INTEGER,
                        atmImaOperStatus       INTEGER
                        }

atmImaIntfIndex OBJECT-TYPE
                SYNTAX Integer32
                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION "interface index (slot/port/channel) SSPPCCC"
                ::= { atmImaConfigEntry 1 }

atmImaNeTxState OBJECT-TYPE
                SYNTAX INTEGER {
                        notInGroup(1), 
                        unusableNoReason(2), 
                        unusableFault(3),
                        unusableMisconnected(4),
                        unusableInhibited(5),
                        unusableFailed(6),
                        usable(7), 
                        active(8)
                        }
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "The near-end transmit state.
                 notInGroup            -- Interface is not assigned to group
                 unusableNoReason      -- Doesn't work; Don't know why.
                 unusableFailed        -- Check the Defect value 
                 usable                -- Interface is ready to go.
                 active                -- Interface is in use."
                DEFVAL { notInGroup }
                ::= { atmImaConfigEntry 2 }

atmImaNeRxState OBJECT-TYPE
                SYNTAX INTEGER {
                        notInGroup(1), 
                        unusableNoReason(2), 
                        unusableFault(3),
                        unusableMisconnected(4),
                        unusableInhibited(5),
                        unusableFailed(6),
                        usable(7), 
                        active(8)
                       }
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "The near-end receive state.
                 notInGroup            -- Interface is not assigned to group
                 unusableNoReason      -- Doesn't work; Don't know why.
                 unusableFailed        -- Check the Defect value 
                 usable                -- Interface is ready to go.
                 active                -- Interface is in use."
                DEFVAL { notInGroup }
                ::= { atmImaConfigEntry 3 }

atmImaFeTxState OBJECT-TYPE
                SYNTAX INTEGER {
                        notInGroup(1), 
                        unusableNoReason(2), 
                        unusableFault(3),
                        unusableMisconnected(4),
                        unusableInhibited(5),
                        unusableFailed(6),
                        usable(7), 
                        active(8)
                        }
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "The far-end transmit state.
                 notInGroup            -- Interface is not assigned to group
                 unusableNoReason      -- Doesn't work; Don't know why.
                 unusableFailed        -- Check the Defect value 
                 usable                -- Interface is ready to go.
                 active                -- Interface is in use."
                DEFVAL { notInGroup }
                ::= { atmImaConfigEntry 4 }

