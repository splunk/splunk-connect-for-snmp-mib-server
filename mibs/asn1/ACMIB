ethernetPortStatsTable OBJECT-TYPE
                SYNTAX SEQUENCE OF EthernetPortStatsEntry
                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION "Statisics for a collection of ethernet-like 
                             interfaces attached to a particular system."
                ::= { equipStatistics 7 }
 
ethernetPortStatsEntry OBJECT-TYPE
                SYNTAX EthernetPortStatsEntry
                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION "Statistics for a particular interface to an
                             ethernet-like medium."
                INDEX { ethernetPortStatsId }
                ::= { ethernetPortStatsTable 1 }
 
EthernetPortStatsEntry ::= SEQUENCE {
                ethernetPortStatsId                                  Integer32,
                ethernetPortStatsTimer                                Integer32,
                ethernetPortStatsAlignmentErrors                Integer32,
                ethernetPortStatsFCSErrors                        Integer32,
                ethernetPortStatsSingleCollisionFrames                Integer32,
                ethernetPortStatsMultipleCollisionFrames        Integer32,
                ethernetPortStatsSQETestErrors                        Integer32,
                ethernetPortStatsDeferredTransmissions                Integer32,
                ethernetPortStatsLateCollisions                        Integer32,
                ethernetPortStatsExcessiveCollisions                Integer32,
                ethernetPortStatsInternalMacTransmitErrors        Integer32,
                ethernetPortStatsCarrierSenseErrors                Integer32,
                ethernetPortStatsFrameTooLongs                        Integer32,
                ethernetPortStatsInternalMacReceiveErrors        Integer32
                }
 
ethernetPortStatsId  OBJECT-TYPE
                SYNTAX Integer32
                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION "An index value that uniquely identifies an 
                             interface to an ethernet-like medium.  The
                             interface identified by a particular value of
                             this index is the same interface as identified
                             by the same value of ethernetPortId."
                ::= { ethernetPortStatsEntry 1 }
 
ethernetPortStatsTimer OBJECT-TYPE
                SYNTAX Integer32
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "Time, in seconds, since last reset."
                DEFVAL { 0 }
                ::= { ethernetPortStatsEntry 2 }
 
ethernetPortStatsAlignmentErrors OBJECT-TYPE
                SYNTAX Integer32
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "A count of frames received on a particular
                             interface that are not an integral number of
                             octets in length and do not pass the FCS check.

                             The count represented by an instance of this
                             object is incremented when the alignmentError
                             status is returned by the MAC service to the
                             LLC (or other MAC user).  Received frames for 
                             which multiple error conditions obtain are,
                             according to the conventions of IEEE 802.3
                             Layer Management, counted exclusively according
                             to the error status presented to the LLC."
                REFERENCE "IEEE 802.3 Layer Management"
                DEFVAL { 0 }
                ::= { ethernetPortStatsEntry 3 }
 
ethernetPortStatsFCSErrors OBJECT-TYPE
                SYNTAX Integer32
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "A count of frames received on a particular
                             interface that are an integral number of
                             octets in length but do not pass the FCS check.

                             The count represented by an instance of this
                             object is incremented when the frameCheckError
                             status is returned by the MAC service to the
                             LLC (or other MAC user).  Received frames for 
                             which multiple error conditions obtain are,
                             according to the conventions of IEEE 802.3
                             Layer Management, counted exclusively according
                             to the error status presented to the LLC."
                REFERENCE "IEEE 802.3 Layer Management"
                DEFVAL { 0 }
                ::= { ethernetPortStatsEntry 4 }
 
ethernetPortStatsSingleCollisionFrames OBJECT-TYPE
                SYNTAX Integer32
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "A count of successfully transmitted frames on
                             a particular interface for which trnasmission
                             is inhibited by exactly one collision.

                             A frame that is counted by an instance of this
                             object is also counted by the corresponding
                             instance of either the ifOutUcastPkts,
                             IfOutMulticastPkts, or ifOutBroadcastPkts,
                             and is not counted by the corresponding
                             instance of the 
                             ethernetPortStatsMulitipleCollisionFrames object."
                REFERENCE "IEEE 802.3 Layer Management"
                DEFVAL { 0 }
                ::= { ethernetPortStatsEntry 5 }
 
ethernetPortStatsMultipleCollisionFrames OBJECT-TYPE
                SYNTAX Integer32
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "A count of successfully transmitted frames on
                             a particular interface for which trnasmission
                             is inhibited by more than one collision.

                             A frame that is counted by an instance of this
                             object is also counted by the corresponding
                             instance of either the ifOutUcastPkts,
                             IfOutMulticastPkts, or ifOutBroadcastPkts,
                             and is not counted by the corresponding
                             instance of the 
                             ethernetPortStatsSingleCollisionFrames object."
                REFERENCE "IEEE 802.3 Layer Management"
                DEFVAL { 0 }
                ::= { ethernetPortStatsEntry 6 }
 
ethernetPortStatsSQETestErrors OBJECT-TYPE
                SYNTAX Integer32
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "A count of times that the SQE TEST ERROR message
                             is generated by the PLS sublayer for a particular
                             interface.  The SQE TEST ERROR message is
                             defined in section 7.2.2.2.4 of ANSI/IEEE 
                             802.3-1985 and its generation is described
                             in section 7.2.4.6 of the same document."
                REFERENCE "ANSI/IEEE Std 802.3-1985 Carrier Sense Multiple
                           Access with Collision Detection Access Method
                           and Physical Layer Specifications" 
                DEFVAL { 0 }
                ::= { ethernetPortStatsEntry 7 }
 
ethernetPortStatsDeferredTransmissions OBJECT-TYPE
                SYNTAX Integer32
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "A count of frames for which the first transmission
                             attempt on a particluar interface is delayed
                             because the medium is busy.

                             the count represented by an instance of this
                             object does not include frames involved in 
                             collisions."
                REFERENCE "IEEE 802.3 Layer Management"
                DEFVAL { 0 }
                ::= { ethernetPortStatsEntry 8 }
 
ethernetPortStatsLateCollisions OBJECT-TYPE
                SYNTAX Integer32
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "The number of times that a collision is detected
                             on a particular interface later than 512 bit-times
                             into the transmission of a packet.

                             Five hundred and twelve bit-times corresponds to
                             51.2 microseconds on a 10 Mbit/s system.  A (late)
                             collision included in a count represented by an
                             instance of this object is also considered as a
                             (generic) collision for purposes of other 
                             collision-related statistics."
                REFERENCE "IEEE 802.3 Layer Management"
                DEFVAL { 0 }
                ::= { ethernetPortStatsEntry 9 }
 
ethernetPortStatsExcessiveCollisions OBJECT-TYPE
                SYNTAX Integer32
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "A count of frames for which transmission on a
                             particular interface fails due to excessive
                             collisions."
                REFERENCE "IEEE 802.3 Layer Management"
                DEFVAL { 0 }
                ::= { ethernetPortStatsEntry 10 }
 
ethernetPortStatsInternalMacTransmitErrors OBJECT-TYPE
                SYNTAX Integer32
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "A count of frames for which transmission on a
                             particular interface fails due to an internal
                             MAC sublayer transmit error.  A frame is only
                             counted by an instance of this object if it is
                             not counted by the corresponding instance of
                             either the ethernetPortStatsLateCollisions object,
                             the ethernetPortStatsExcessiveCollisions object,
                             or the ethernetPortStatsCarrierSenseErrors object.
                             The precise meaning of the count represented by
                             an instance of this object is implementation-
                             specific.  In particular, an instance of this
                             object may represent a count of transmission
                             errors on a particular interface that are not
                             otherwise counted."
                REFERENCE "IEEE 802.3 Layer Management"
                DEFVAL { 0 }
                ::= { ethernetPortStatsEntry 11 }
 
ethernetPortStatsCarrierSenseErrors OBJECT-TYPE
                SYNTAX Integer32
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "The number of times that the carrier sense 
                             condition was lost or never asserted when
                             attempting to transmit a frame on a particular
                             interface.

                             The count represented by an instance of this 
                             object is incremented at most once per 
                             transmission attempt, even if the carrier sense
                             condition fluctuates during a transmission
                             attempt."
                REFERENCE "IEEE 802.3 Layer Management"
                DEFVAL { 0 }
                ::= { ethernetPortStatsEntry 12 }
 
ethernetPortStatsFrameTooLongs OBJECT-TYPE
                SYNTAX Integer32
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "A count of frames received on a particular
                             interface that exceed the maximum permitted
                             frame size.

                             The count represented by an instance of this 
                             object is incremented when the frameTooLong status
                             is returned by the MAC service to the LLC (or 
                             other MAC user).  Received frames for which 
                             multiple error conditions obtain are, according
                             to the conventions of IEEE 802.3 Layer 
                             Management, counted exclusively according to the
                             error status presented to the LLC."
                REFERENCE "IEEE 802.3 Layer Management"
                DEFVAL { 0 }
                ::= { ethernetPortStatsEntry 13 }
 
ethernetPortStatsInternalMacReceiveErrors OBJECT-TYPE
                SYNTAX Integer32
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "A count of frames for which reception on a 
                             particular interface fails due to an internal
                             MAC sublayer receive error.  A frame is only 
                             counted by an instance of this object if it is not
                             counted by the corresponding instance of either
                             the ethernetPortStatsFrameTooLongs object, the
                             ethernetPortStatsAlignmentErrors object, or the
                             ethernetPortStatsFCSErrors object.

                             The precise meaning of the count represented by
                             an instance of this object is implementation-
                             specific.  In particular, an instance of this
                             object may represent a count of receive errors
                             on a particular interface that are not 
                             otherwise counted."
                REFERENCE "IEEE 802.3 Layer Management"
                DEFVAL { 0 }
                ::= { ethernetPortStatsEntry 14 }
 

-- Interface Statistics Group intfStatistics

atmIfStatsTable OBJECT-TYPE
                SYNTAX SEQUENCE OF AtmIfStatsEntry
                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION "Table of statistics for an atmUni interface"
                ::= { intfStatistics 1 }

atmIfStatsEntry OBJECT-TYPE
                SYNTAX AtmIfStatsEntry
                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION "Row definition for the atmUni statistics
                             table"
                INDEX { atmIfStatsId }
                ::= { atmIfStatsTable 1}

AtmIfStatsEntry ::= SEQUENCE {
                atmIfStatsId IntfIndex,
                atmIfStatsValidCellsInHi        Integer32,
                atmIfStatsValidCellsInLo        Integer32,
                atmIfStatsErroredCellsIn        Integer32,
                atmIfStatsMismatchedCellsIn        Integer32,
                atmIfStatsNullCellsOutHi        Integer32,
                atmIfStatsNullCellsOutLo        Integer32,
                atmIfStatsValidCellsOutHi        Integer32,
                atmIfStatsValidCellsOutLo        Integer32,
                atmIfStatsTimer                        Integer32
                }

atmIfStatsId OBJECT-TYPE
                SYNTAX IntfIndex
                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION "Interface index (SSPPCCC) to the atmUni 
                             statistics table"
                ::= { atmIfStatsEntry 1 }

