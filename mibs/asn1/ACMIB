                        trunkCond(1),
                        unframed1s(2)
                        }
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "Specifies whether the interworking is done 
                             by trunk conditioning or unframed1s"
                DEFVAL {trunkCond }
                ::= {e1PortConfigEntry 13 }


e1EnhanceFeatureSet1 OBJECT-TYPE
                SYNTAX INTEGER {
                        disabled(1),
                        enabled(2)
                        }
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "To enable/disable a proprietary ab signaling
                             scheme, it applies to all DS0s in the port.
                             No individual DS0 signaling can be 
                             configured under the scheme."
                             
                DEFVAL { disabled }
                ::= {e1PortConfigEntry 14 }

