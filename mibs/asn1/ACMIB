                SYNTAX VbrAtmPvcVccEntry
                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION "Row definition of terminal emulation to ATM 
                             network IWF connections"
                INDEX { vbrAtmPvcVccIfA, vbrAtmPvcVccIfB,
                        vbrAtmPvcVccVpiB, vbrAtmPvcVccVciB }
                ::= { vbrAtmPvcVccTable 1 }

VbrAtmPvcVccEntry ::= SEQUENCE {
                           vbrAtmPvcVccIfA                IntfIndex,
                           vbrAtmPvcVccIfB                IntfIndex,
                           vbrAtmPvcVccVpiB               INTEGER,
                           vbrAtmPvcVccVciB               INTEGER,
                           vbrAtmPvcVccServiceType        INTEGER,
                           vbrAtmPvcVccPeakCellRateA2B    Integer32,
                           vbrAtmPvcVccSusCellRateA2B     Integer32,
                           vbrAtmPvcVccMaxBurstSizeA2B    Integer32,
                           vbrAtmPvcVccPeakCellRateB2A    Integer32,
                           vbrAtmPvcVccSusCellRateB2A     Integer32,
                           vbrAtmPvcVccMaxBurstSizeB2A    Integer32,
                           vbrAtmPvcVccFlow               INTEGER,
                           vbrAtmPvcVccRowStatus          RowStatus,
                           vbrAtmPvcVccConnectionStatus   INTEGER,

                           vbrAtmPvcVccDualHomed          INTEGER,

                           vbrAtmPvcVccOamStatus          INTEGER,
                           vbrAtmPvcVccViB                INTEGER,
                           vbrAtmPvcVccCnfrmTypeB2A       INTEGER,

                           vbrAtmPvcVccVUNIB              INTEGER,

                           vbrAtmPvcVccOAMConnEndPtStatus INTEGER,
                           vbrAtmPvcVccOAMSegEndPtStatus  INTEGER,
                           vbrAtmPvcVccMfsB2A             INTEGER,
                           vbrAtmPvcVccConnFailureCauseA  ConnFailureCause,
                           vbrAtmPvcVccConnFailureCauseB  AtmConnFailureCause,
                           vbrAtmPvcVccSarTypeB           INTEGER,
                           vbrAtmPvcVccAal2Cid            INTEGER,
                           vbrAtmPvcVccTrafficShapingA2B  INTEGER
                          }

vbrAtmPvcVccIfA        OBJECT-TYPE
                SYNTAX IntfIndex
                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION "Interface index for side A, the variable bit rate
                             side, of a variable bit rate-atm PVC VCC 
                             connection.  Number value is interpreted as an 
                             interface of the form, SSPPCCC, where SS is the 
                             card slot, PP is the port number, and CCC is the 
                             channel number."
                ::= { vbrAtmPvcVccEntry 1 }

vbrAtmPvcVccIfB OBJECT-TYPE
                SYNTAX IntfIndex
                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION "Interface index for side B, the atm side, of a 
                             variable bit rate-atm PVC VCC connection.  Number 
                             value is interpreted as an interface of the form, 
                             SSPPCCC, where SS is the card slot, PP is the port 
                             number, and CCC is the channel number."
                ::= { vbrAtmPvcVccEntry 2 }

vbrAtmPvcVccVpiB OBJECT-TYPE
               SYNTAX INTEGER (0..4095)
               MAX-ACCESS not-accessible
               STATUS current
