                SYNTAX INTEGER (1..84)
                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION "GR303IG Number index which recognizes GR303
                Interface group"
                ::= { gr303LapdConfigEntry 1 }

gr303T200        OBJECT-TYPE
                SYNTAX INTEGER(100..350)
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "value of Lapd T200 timer in miliseconds in step
                size of 50 ms"
                DEFVAL { 150 }
                ::= { gr303LapdConfigEntry 2 }

gr303T203        OBJECT-TYPE
                SYNTAX INTEGER(10..300) 
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "value of Lapd T203 timer in seconds"
                DEFVAL { 30 }
                ::= { gr303LapdConfigEntry 3 }

gr303N200        OBJECT-TYPE
                SYNTAX INTEGER(1..10) 
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "Maximum number of retransmissions"
                DEFVAL { 3 }
                ::= { gr303LapdConfigEntry 4 }


gr303K           OBJECT-TYPE
                SYNTAX INTEGER(1..7) 
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "Maximum number of outstanding I frames -
                             Lapd - window size."
                DEFVAL { 7 }
                ::= { gr303LapdConfigEntry 5 }


