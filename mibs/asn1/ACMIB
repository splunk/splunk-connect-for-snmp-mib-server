        SYNTAX INTEGER {
                none(1),
                eighthRate(2),
                quarterRate(3),
                halfRate(4),
                autoHiQ(5),
                auto(6),
                autoLoQ(7)
                   }
        MAX-ACCESS read-write
        STATUS current
        DESCRIPTION "Forward error correction scheme, ATM port side."
        DEFVAL { none }
        ::= { atmAtmSpvcVccEntry 12 }

