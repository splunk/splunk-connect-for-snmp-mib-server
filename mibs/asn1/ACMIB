                SYNTAX Gr303LapdConfigEntry
                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION "Row definition for LAPD attributes in a GR303IG"
                INDEX { gr303LapdIgId }
                ::= { gr303LapdConfigTable 1 }

