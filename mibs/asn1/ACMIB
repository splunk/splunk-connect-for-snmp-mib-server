                SYNTAX INTEGER {
                                unsecurable(1),
                                secured(2),
                                securable(3)
                                }
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION " enhancement feature"
                DEFVAL { unsecurable }
                ::= { taxiChannelConfigEntry 3 }

