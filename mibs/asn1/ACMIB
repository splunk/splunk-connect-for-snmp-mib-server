tasmMaxAnnce OBJECT-TYPE
                SYNTAX INTEGER  (1..10000)
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "Specifies the maximum number of tones and
                             announcements that can be played simultaneously.
                             This will depend on the number of control blocks 
                             allocated for collecting digits and for line 
                             tests as the memory comes from the same pool. 
                             The tasmControlBlockResAllocUsage field will be 
                             updated to indicate this."
                DEFVAL { 1 }
                ::= { sysConfig 77 }

tasmMaxStdTones OBJECT-TYPE
                SYNTAX INTEGER  (1..10)
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "Specifies the maximum number of
                             tones that can be played simutaneously. This field
                             has been obsoleted for optimisation of resources."
                DEFVAL { 10 }
                ::= { sysConfig 78 }

tasmMemoryAction OBJECT-TYPE
                SYNTAX INTEGER {
                                re-initialize(1)
                                }
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "Specifies the action to be performed 
                        on the memory in the TASM card used for
                        storing announcements."
                DEFVAL { 1 }
                ::= { sysConfig 79 }

--
-- Announcement configuration for the management of announcement files in 
-- the system.
--

tasAnnceTable OBJECT-TYPE
                SYNTAX SEQUENCE OF TasAnnceEntry
                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION "Table of announcements for the Tone
                             Announcement server ."
                ::= { sysConfig 80 }

tasAnnceEntry OBJECT-TYPE
                SYNTAX TasAnnceEntry
                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION "Row definition for Tone Announcement server
                             table announcement entries."
                INDEX { tasAnnceId }
                ::= { tasAnnceTable 1 }

TasAnnceEntry ::= SEQUENCE {
                tasAnnceId                  INTEGER,
                tasAnnceType                INTEGER,
                tasAnnceCmprsType           INTEGER,
                tasAnnceValid               INTEGER,
                tasAnnceDescription         DisplayString
                }

tasAnnceId OBJECT-TYPE
                SYNTAX INTEGER (1..100)
                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION "Announcement ID."
                ::= { tasAnnceEntry 1 }

tasAnnceType OBJECT-TYPE
                SYNTAX INTEGER {
                                voice(1),
                                toneVoice(2),
                                userTones(3),
                                music(4),
                                musicVoice(5),
                                toneMusic(6)
                               }
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "The type of this announcement:
                             voice indicates it is a pure voice
                             announcement file.
                             toneVoice indicates it is a file with tones
                             at the beginning followed by voice.
                             userTones indicates it is a file with purely
                             user generated tones.
                             music indicates it is a file with pure music.
                             musicVoice indicates it is a file with music
                             at the beginning followed by voice.
                             toneMusic indicates it is a file with tones
                             at the beginning followed by music."
                ::= { tasAnnceEntry 2 }

tasAnnceCmprsType OBJECT-TYPE
                SYNTAX INTEGER {
                                uncompressed(1),
                                compress8k(2),
                                compress16k(3),
                                compress32k(4)
                               }
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "The type of compression used for
                             storing this announcement:
                             compress8k indicates 8k compression algorithm
                             should be used. compress16k indicates 16k 
                             compression algorithm should be used.
                             compress32k indicates 32k compression algorithm
                             should be used."
                ::= { tasAnnceEntry 3 }

tasAnnceValid OBJECT-TYPE
                SYNTAX INTEGER {
                                good(1),
                                olderVersion(2),
                                corrupted(3)
                               }
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "Indicates the condition of this announcement,
                             whether it is good, it is stored using an older
                             compression algorithm, or it is corrupted."
                ::= { tasAnnceEntry 4 }

tasAnnceDescription OBJECT-TYPE
                SYNTAX DisplayString (SIZE(255))
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "Description for this announcement."
                ::= { tasAnnceEntry 5 }

