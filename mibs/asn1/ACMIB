                SYNTAX RowStatus
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "row status"
                ::= { ipAtmAppPvcVccEntry 12 }

ipAtmAppPvcVccConnectionStatus OBJECT-TYPE
                SYNTAX INTEGER {
                                active(1),
                                inactive(2),
                                inactiveA2B-B2A(3),
                                inactiveA2B(4),
                                inactiveB2A(5)
                               }
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "Indicates the current connections status.
                             Active:  All interfaces through which the 
                                      connection is going are in service.
                             Inactive:  One of the end interfaces of the 
                                        connection is not in service.

                             The rest of the states apply to connections going
                             through resource interfaces like DSP2 and FEC.  These
                             interfaces are dynamically allocated.

                             InactiveA2B-B2A:  Both the A2B and B2A resource
                                               interfaces are not in service.
                             InactiveA2B:  The A2B resource interface is not in 
                                           service.
                             InactiveB2A:  The B2A resource interface is not in 
                                           service.

                             If an end interface and the resource interfaces 
                             are out of service, the status will be inactive 
                             because the end interfaces take priority."
                DEFVAL { inactive }
                ::= { ipAtmAppPvcVccEntry 13 }


ipAtmAppPvcVccDualHomed OBJECT-TYPE
                SYNTAX INTEGER {
                                no(1),
                                yes(2)
                               }
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "Does this PVC need a backup Dual homing PVC."
                DEFVAL { no }
                ::= { ipAtmAppPvcVccEntry 14 }


ipAtmAppPvcVccViB       OBJECT-TYPE
                SYNTAX INTEGER (0..1024)
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "VI to associate the Egress part of the
                        A2B connection ."
                DEFVAL { 0 }
                ::= { ipAtmAppPvcVccEntry 15 }

ipAtmAppPvcVccCnfrmTypeB2A    OBJECT-TYPE
                SYNTAX INTEGER {
                                    best-effort(1),
                                one-bucket-notag-0plus1(2),
                                two-bucket-notag-0plus1-0plus1(3),
                                two-bucket-notag-0plus1-0(4),
                                two-bucket-tag-0plus1-0(5),
                                best-effort-tag(6)
                               }
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "The following is the description of this field
                        This will be used in conjunction with the
                        service type field to get the correct
                        traffic behaviour. In the conformances the 
                        the 0 or 0plus1 refers to the clp bit in the
                        cells. If 0plus1 is the bucket reference then
                        the clp bit is ignored. If 0 is specified for 
                        the bucket then check only clp=0 cells.
                        1: Best Effort will try to send all cellsin a 
                        best effort way. This will follow the 
                        Aqueman Algorithm
                        2: one-bucket-notag-0plus1 will use 1 bucket
                        i.e is the PCR(0+1) bucket and drop all
                        cells in excess.
                        3: two-bucket-notag-0plus1-0plus1 uses 2 buckets.
                        The 2 buckets are pcr(0+1) and scr(0+1).
                        All cells conforming to both buckets will be
                        passed through. The rest will be dropped.
                        4: two-bucket-notag-0plus1-0 uses 2 buckets.
                        The 2 buckets are pcr(0+1) and scr(0) or 
                        pcr(0). Cells which conform to both buckets
                        are passed through. The rest are dropped.
                        5: two-bucket-tag-0plus1-0 uses 2 buckets.
                        The 2 buckets are pcr(0+1) and scr(0) or 
                        pcr(0). Cells which do not conform to the first
                        bucket are dropped. Cells which do not 
                        conform to the 2nd bucket are tagged.
                        6: best-effort-tag. Tag all cells and then
                        do a best effort.  "
                DEFVAL { best-effort }
                ::= { ipAtmAppPvcVccEntry 16 }



ipAtmAppPvcVccVUNIB       OBJECT-TYPE
                SYNTAX INTEGER (0..255)
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "VUNI to associate the Egress part of the
                        A2B connection ."
                DEFVAL { 0 }
                ::= { ipAtmAppPvcVccEntry 17 }


ipAtmAppPvcVccMfsB2A OBJECT-TYPE
                SYNTAX INTEGER (1..65535)
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "Maximum Frame Size for GFR traffic"
                DEFVAL { 1 }
                ::= { ipAtmAppPvcVccEntry 18 }

ipAtmAppPvcVccConnFailureCauseB    OBJECT-TYPE
                SYNTAX AtmConnFailureCause
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "connection status reason code"
                ::= { ipAtmAppPvcVccEntry 19 }

ipAtmAppPvcVccTrafficShapingA2B     OBJECT-TYPE
                SYNTAX INTEGER {
                                disabled(1),
                                enabled(2)
                               }
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "Traffic Shaping Support in the A2B direction."
                DEFVAL { disabled }
          ::= { ipAtmAppPvcVccEntry 20 }


ipAtmAppPvcVccOamStatus OBJECT-TYPE
                SYNTAX INTEGER {
                                unsupp(1),
                                end-Pt(2),
                                end-Seg-Pt(3)
                               }
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "Indicates the OAM monitoring status.
                             The 3 possible states are:
                             Unsupp:  No monitoring enabled.
                             End-Pt:  A side is only an end point, therefore
                                it will respond to AIS/RDI.
                             End-Seg-Pt: Connection endPoint that is also
                             a segment endPoint."
                DEFVAL { unsupp }
                ::= { ipAtmAppPvcVccEntry 21 }

ipAtmAppSvcVccTable OBJECT-TYPE
                SYNTAX SEQUENCE OF IpAtmAppSvcVccEntry
                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION "Table of SVC VCC connection entries"
                ::= { connectionConfig 5 }

ipAtmAppSvcVccEntry OBJECT-TYPE
                SYNTAX IpAtmAppSvcVccEntry
                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION "Row definition of SVC VCC connection
                             attribution."
                INDEX { ipAtmAppSvcVccDestAddrA, ipAtmAppSvcVccSubnetMaskA,
                        ipAtmAppSvcVccIfB, ipAtmAppSvcVccVpiB, 
                        ipAtmAppSvcVccVciB }
                ::= { ipAtmAppSvcVccTable 1 }

IpAtmAppSvcVccEntry ::= SEQUENCE {
                ipAtmAppSvcVccDestAddrA          IpAddress,
                ipAtmAppSvcVccSubnetMaskA        IpAddress,
                ipAtmAppSvcVccIfB                Integer32,
                ipAtmAppSvcVccVpiB               INTEGER,
                ipAtmAppSvcVccVciB               INTEGER,
                ipAtmAppSvcVccServiceTypeA2B     INTEGER,
                ipAtmAppSvcVccServiceTypeB2A     INTEGER,
                ipAtmAppSvcVccPeakCellRateB2A    Integer32,
                ipAtmAppSvcVccSusCellRateB2A     Integer32,
                ipAtmAppSvcVccMaxBurstSizeB2A    Integer32,
                ipAtmAppSvcVccType               INTEGER,
                ipAtmAppSvcVccConnectionStatus   INTEGER,
                ipAtmAppSvcVccViB                INTEGER,
                ipAtmAppSvcVccCnfrmTypeB2A       INTEGER

                ,ipAtmAppSvcVccVUNIB             INTEGER

                ,ipAtmAppSvcVccTrafficShapingA2B INTEGER
                }

ipAtmAppSvcVccDestAddrA OBJECT-TYPE
                SYNTAX IpAddress
                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION "Destination IP address,value is IP address."
                ::= { ipAtmAppSvcVccEntry 1 }

