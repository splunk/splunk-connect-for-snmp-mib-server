                SYNTAX INTEGER   {
                        outOfService (1),
                        inService    (2)
                    }
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "ISDN interface operational status"
                ::= { isdnIntfConfigEntry 4 }

isdnIntfSwitchType        OBJECT-TYPE
                SYNTAX INTEGER   {
                                    pri-ni2 (1),
                        pri-net5 (2),
                        pri-1tr6 (3),
                        pri-4ess (4),
                        pri-5ess (5),
                        pri-dass2  (6),
                        pri-dms100  (7),
                        pri-kdd  (8),
                        pri-ntt  (9),
                        pri-net5sw  (10),
                        pri-ts014  (11),
                        pri-vn  (12)
                    }
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "ISDN interface Switch Type"
                        DEFVAL { pri-ni2 }
                ::= { isdnIntfConfigEntry 5 }

-- LAPD configuration table

isdnL2ConfigTable OBJECT-TYPE
                SYNTAX SEQUENCE OF IsdnL2ConfigEntry
                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION "Table of Layer 2 (LAPD) attributes"
                ::= { isdnConfig 2 }

isdnL2ConfigEntry OBJECT-TYPE
                SYNTAX IsdnL2ConfigEntry
                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION "Row definition for ISDN LAPD parameters"
                INDEX { isdnL2IntfId }
                ::= { isdnL2ConfigTable 1 }

IsdnL2ConfigEntry ::= SEQUENCE {
                isdnL2IntfId                INTEGER,
                isdnT200                INTEGER,
                isdnT203                INTEGER,
                isdnN200                INTEGER,
                isdnN201                INTEGER,
                isdnK                        INTEGER,
                isdnL2OperStatus        INTEGER,
                isdnL2CurrentState        INTEGER
                }

isdnL2IntfId      OBJECT-TYPE
                SYNTAX Integer32
                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION "Interface index (SSPPCCC)"
                ::= { isdnL2ConfigEntry 1 }

isdnT200        OBJECT-TYPE
                SYNTAX INTEGER (1..100)
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "value of L2 T200 timer in 1/10 seconds"
                DEFVAL { 10 }
                ::= { isdnL2ConfigEntry 2 }

isdnT203        OBJECT-TYPE
                SYNTAX INTEGER (1..60)
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "value of L2 T203 timer in seconds"
                DEFVAL { 10 }
                ::= { isdnL2ConfigEntry 3 }

isdnN200        OBJECT-TYPE
                SYNTAX INTEGER (1..5)
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "Maximum number of retransmissions before
                             the LAPD Link is declared as down"
                DEFVAL { 3 }
                ::= { isdnL2ConfigEntry 4 }

isdnN201        OBJECT-TYPE
                SYNTAX INTEGER (1..300)
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "Maximum number of octets in an information 
                                field - Maximum value of this parameter
                                will be 300 bytes."
                DEFVAL { 260 }
                ::= { isdnL2ConfigEntry 5 }

isdnK           OBJECT-TYPE
                SYNTAX INTEGER (1..127) 
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "Maximum number of outstanding I frames -
                             L2 - window size."
                DEFVAL { 7 }
                ::= { isdnL2ConfigEntry 6 }

isdnL2OperStatus    OBJECT-TYPE
                    SYNTAX INTEGER  {
                           not-initiated (1) ,
                           up (2),
                           down (3)
                }
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "ISDN Layer 2 operational status  - 
                             not-initiated means that first SETUP message 
                             has not been transmitted and L2 estalishment 
                             procedure has not been initiated yet"
               ::= { isdnL2ConfigEntry 7}

isdnL2CurrentState    OBJECT-TYPE
                    SYNTAX INTEGER  {
                           tei-unassigned(1),
                           tei-assigned (4),
                           awaiting-establishment (5),
                           awaiting-release (6),
                           multiple-frame-established (7),
                           timer-recovery (8)
                }
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "ISDN Layer 2 protocol's current state" 
               ::= { isdnL2ConfigEntry 8}

