                SYNTAX IpAddress
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "The IP address of CS"
                ::= { sysConfig 21 }

csTcpServerPortNumber OBJECT-TYPE
                SYNTAX INTEGER ( 1025..65535 )
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "The Port Number used for communication
                with CS.
                NOTE: Port # 5000 and 6000 have been 
                              reservered for AC products.
                              DO NOT USED THEM !!! "
                DEFVAL { 1025 }
                ::= { sysConfig 22 }

