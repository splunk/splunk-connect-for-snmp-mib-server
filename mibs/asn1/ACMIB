                cirEmAtmSpvcVccStatsInCellCountHiB                Integer32,
                cirEmAtmSpvcVccStatsInCellCountLoB                Integer32,
                cirEmAtmSpvcVccStatsOutCellCountHiB                 Integer32,
                cirEmAtmSpvcVccStatsOutCellCountLoB                 Integer32,
                cirEmAtmSpvcVccStatsTimer                        Integer32,
                cirEmAtmSpvcVccStatsOdometerCellsEncodedHiA        Integer32,
                cirEmAtmSpvcVccStatsOdometerCellsEncodedLoA        Integer32,
                cirEmAtmSpvcVccStatsOdometerCellsDecodedHiA        Integer32,
                cirEmAtmSpvcVccStatsOdometerCellsDecodedLoA        Integer32,
                cirEmAtmSpvcVccStatsInOdometerCellCountHiB        Integer32,
                cirEmAtmSpvcVccStatsInOdometerCellCountLoB        Integer32,
