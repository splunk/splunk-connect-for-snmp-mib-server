-- sysTest 3

oamLoopbackTable OBJECT-TYPE
                SYNTAX SEQUENCE OF OamLoopbackEntry
                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION "Table of oam loopback functions"
                ::= { sysTest 3 }

oamLoopbackEntry OBJECT-TYPE
                SYNTAX OamLoopbackEntry
                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION "Row definition of oam loop back"
                INDEX { oamLpbkIfB, oamLpbkVpiB, oamLpbkVciB}
                ::= { oamLoopbackTable 1 }

OamLoopbackEntry ::= SEQUENCE {
                oamLpbkIfB   IntfIndex,
                oamLpbkVpiB  INTEGER,
                oamLpbkVciB  INTEGER,
                oamLoopbackType   INTEGER,
                oamLpbkLocationID OCTET STRING,
                oamLpbkAction     INTEGER,
                oamLpbkStatus     INTEGER,
                oamLpbkTxCount    Integer32,
                oamLpbkRxCount    Integer32,
                oamLpbkMismatchCount   Integer32,
                oamLpbkTimeout INTEGER
                }

oamLpbkIfB      OBJECT-TYPE
                SYNTAX IntfIndex
                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION "Interface index used for starting loopback
                        test.  Number value is interpreted as an interface 
                        of the form, SSPPCCC, where SS is the card slot, PP 
                        is the port number, and CCC is the channel number."
                ::= { oamLoopbackEntry 1 }

oamLpbkVpiB     OBJECT-TYPE
                SYNTAX INTEGER (0..4095)
                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION "VPI value under testing for oam loopback test.
                             Valid range is a number between 0 and 4095."
                ::= { oamLoopbackEntry 2 }

oamLpbkVciB     OBJECT-TYPE
                SYNTAX INTEGER (0..65535)
                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION "VCI value under testing for oam loopback test.
                             Valid range is a number between 0 and 65535."
                ::= {oamLoopbackEntry  3 }

oamLoopbackType OBJECT-TYPE
                SYNTAX INTEGER {
                                end-to-end(1),
                                segment-endpoint(2),
                                next-connection-point(3),
                                specific-location(4)
                                }
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "OAM Loopback type. Determines whether to
                        send ete or segment loopback cell, and the value
                        of location Id field in the cell. Only for
                        specific-location value, user specified locationId
                        field value taken into account."
                DEFVAL { end-to-end }
                ::= { oamLoopbackEntry 4 }

oamLpbkLocationID OBJECT-TYPE
                SYNTAX OCTET STRING (SIZE(16))
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "Location ID of the ATM Node where the 
loopback is
                             to occur."
                DEFVAL { 'FFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFF'H }
                ::= { oamLoopbackEntry  5 }

oamLpbkAction   OBJECT-TYPE
                SYNTAX INTEGER {
                                sendLoopbackOnce(1),
                                sendLoopbackContinuously(2),
                                stopTest(3),
                                resetStats(4)
                               }
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "Test action to be performed.
                             endLoopbackContinuously is not used. "
                DEFVAL { sendLoopbackOnce }
                ::= { oamLoopbackEntry  6 }

oamLpbkStatus   OBJECT-TYPE
                SYNTAX INTEGER  {
                                inactive(1),
                                waiting-for-response(2),
                                successful(3),
                                timedout(4)
                                }
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "Inidcates the current status of oam test."
                DEFVAL { inactive}
                ::= { oamLoopbackEntry  7}



oamLpbkTxCount  OBJECT-TYPE
                SYNTAX Integer32
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "Valid cells transmitted."
                ::= { oamLoopbackEntry  8 }

oamLpbkRxCount  OBJECT-TYPE
                SYNTAX Integer32
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "Valid cells received."
                ::= { oamLoopbackEntry  9 }

oamLpbkMismatchCount OBJECT-TYPE
                SYNTAX Integer32
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "Number of times Rx correlation tag is 
                        mismatched with the Tx correlation tag."
                ::= { oamLoopbackEntry  10 }

oamLpbkTimeout  OBJECT-TYPE
                SYNTAX Integer32
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "number of time out "
                ::= { oamLoopbackEntry  11 }


-- Vipr Router Configuration Group
 
viprRouterConfigTable  OBJECT-TYPE
                SYNTAX SEQUENCE OF ViprRouterConfigEntry
                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION "Table of Vipr Router Instance Information"
                ::= { viprConfig 1 }
 
viprRouterConfigEntry  OBJECT-TYPE
                SYNTAX ViprRouterConfigEntry
                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION " definition for VIPR Route instance attributes"
                INDEX { viprSlot, viprRouterId }
                ::= { viprRouterConfigTable 1 }
 
ViprRouterConfigEntry  ::= SEQUENCE {
                viprSlot                        Integer32,
                viprRouterId                    INTEGER,
                viprRouterName                  DisplayString,
                viprRouterNoOfInterfaces        INTEGER,
                viprRouterNoOfRoutes            INTEGER,
                viprRouterAdminStatus           INTEGER,
                viprRouterOperStatus            INTEGER,
                viprRouterInitTmr               Integer32,
                viprRouterDisabWarnTmr          Integer32,
                viprRouterShutDwnWarnTmr        Integer32,
                viprRouterDisableWarning        INTEGER,
                viprRouterDisableReason         INTEGER,
                viprRouterRowStatus             RowStatus
                }
 
viprSlot        OBJECT-TYPE
                SYNTAX Integer32
                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION "The slot number of the Virtual Router card (SS)"
                ::= { viprRouterConfigEntry 1 }
 
viprRouterId    OBJECT-TYPE
                SYNTAX INTEGER(1..6)
                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION "A unique Id for the Router Instance in a router card"
                ::= { viprRouterConfigEntry 2 }
 
viprRouterName  OBJECT-TYPE
                SYNTAX DisplayString (SIZE(32))
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "The ASCII name of the Router Instance"
                ::= { viprRouterConfigEntry 3 }
 
viprRouterNoOfInterfaces OBJECT-TYPE
                SYNTAX INTEGER (1..99)
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "This variable describes the no of interfaces
                             defined in the Router Instance"
                DEFVAL { 1 }
                ::= { viprRouterConfigEntry 4 }
 
viprRouterNoOfRoutes OBJECT-TYPE
                SYNTAX INTEGER (1..1024)
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "This variable describes the no of static routes
                             defined in the Router Instance"
                DEFVAL { 1 }
                ::= { viprRouterConfigEntry 5 }
 
viprRouterAdminStatus       OBJECT-TYPE
                SYNTAX INTEGER  {
                       outOfService(1),
                       inService(2)
                       }
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "This variable represents the various X.731 states
                             the Router state machine can take Administartively"
                DEFVAL { outOfService }
                ::= { viprRouterConfigEntry 6 }
 
viprRouterOperStatus       OBJECT-TYPE
                SYNTAX INTEGER  {
                       outOfService(1),
                       inService(2),
                       unlocked-Init(3),
                       unlocked-Disable(4),
                       locked-Shutting(5)
                       }
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "This variable represents the various X.731 states
                             the Router state machine can take operationally"
                DEFVAL { outOfService }
                ::= { viprRouterConfigEntry 7 }
 
viprRouterInitTmr OBJECT-TYPE
                SYNTAX Integer32
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "This variable describes the waiting period in
                             the locked-Init state before moving onto
                             locked-Diable state."
                DEFVAL { 192 }
                ::= { viprRouterConfigEntry 8 }
 
viprRouterDisabWarnTmr OBJECT-TYPE
                SYNTAX Integer32
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "This variable describes the periodicity in which
                             the error messages are to be flashed when the
                             system is in unlocked-Disable state."
                DEFVAL { 36000 }
                ::= { viprRouterConfigEntry 9 }
 
viprRouterShutDwnWarnTmr OBJECT-TYPE
                SYNTAX Integer32
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "This variable describes the periodicity in which
                             the error messages are to be flashed when the
                             system is in locked-Shutting state."
                DEFVAL { 36000 }
                ::= { viprRouterConfigEntry 10 }
 
 
viprRouterDisableWarning OBJECT-TYPE
                SYNTAX INTEGER {
                       enabled(1),
                       disabled(2)
                       }
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "This variable when disabled will mask all the
                             warning messages."
                DEFVAL { enabled }
                ::= { viprRouterConfigEntry 11 }
 
viprRouterDisableReason OBJECT-TYPE
                SYNTAX INTEGER {
                       none(1),
                       manual(2),
                       automatic(3)
                       }
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "This variable describes the reason why the Router
                             in the unlocked-Disabled state."
                DEFVAL { none }
                ::= { viprRouterConfigEntry 12 }

viprRouterRowStatus      OBJECT-TYPE
                SYNTAX RowStatus
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "row status"
                ::= { viprRouterConfigEntry 13 }

