                SYNTAX INTEGER  {
                                unconfigured(1),
                                atmUni3-0(2),
                                atmUni3-1(3),

                                iispUser(4),
                                iispNetwork(5),

                                atmUni4-0(6)
                                }
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION " UNI or NNI type"
                DEFVAL { unconfigured }
                ::= { taxiChannelConfigEntry 2 }

