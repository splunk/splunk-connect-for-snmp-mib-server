                DESCRIPTION "Time exceeded"
                ::= { viprVpnStatsEntry 7}

viprVpnStatsIcmpOutParamps OBJECT-TYPE
                SYNTAX Integer32
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "Parameter problem"
                ::= { viprVpnStatsEntry 8 }

viprVpnStatsIcmpOutTimeRQs OBJECT-TYPE
                SYNTAX Integer32
                MAX-ACCESS read-only
