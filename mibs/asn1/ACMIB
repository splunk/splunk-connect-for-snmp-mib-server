                SYNTAX Ds3PortStatsEntry
                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION "Row definition for the DS3 port statistics
                             table"
                INDEX { ds3PortStatsId }
                ::= { ds3PortStatsTable 1 }

