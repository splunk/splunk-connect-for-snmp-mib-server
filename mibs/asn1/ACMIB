 --     In    ____________________________________ Out
 --    ______| SWITCH                            |__________
 --     Out  |                                   |  In
 --          |___________________________________|
 --  Frame Relay UNI A                           ATM UNI/NNI B
 --                                   

 -- The Frame Relay to ATM Inter-working PVC Table

frAtmPvcVccTable OBJECT-TYPE
                    SYNTAX SEQUENCE OF FrAtmPvcVccEntry
                    MAX-ACCESS not-accessible
                    STATUS current
                    DESCRIPTION "Table of frame relay to ATM IWF connections"
                    ::= { connectionConfig 7 }

frAtmPvcVccEntry OBJECT-TYPE
                SYNTAX FrAtmPvcVccEntry
                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION "Row definition of frame relay to ATM IWF 
                              connections "
                INDEX { frAtmPvcVccIfA, frAtmPvcVccDlciA, frAtmPvcVccIfB,
                        frAtmPvcVccVpiB, frAtmPvcVccVciB }
                ::= { frAtmPvcVccTable 1 }

FrAtmPvcVccEntry ::= SEQUENCE
                             {
                              frAtmPvcVccIfA                    IntfIndex,
                              frAtmPvcVccDlciA                  Integer32,
                              frAtmPvcVccIfB                    IntfIndex,
                              frAtmPvcVccVpiB                   INTEGER,
                              frAtmPvcVccVciB                   INTEGER,
                              frAtmPvcVccMaxFrameSizeIn         Integer32,
                              frAtmPvcVccBcIn                   Integer32,
                              frAtmPvcVccBeIn                   Integer32,
                              frAtmPvcVccCirIn                  Integer32,
                              frAtmPvcVccMaxFrameSizeOut        Integer32,
                              frAtmPvcVccBcOut                  Integer32,
                              frAtmPvcVccBeOut                  Integer32,
                              frAtmPvcVccCirOut                 Integer32,
                              frAtmPvcVccServiceType            INTEGER,
                              frAtmPvcVccFlow                   INTEGER,
                              frAtmPvcVccRowStatus              RowStatus,
                              frAtmPvcVccTimeLenIn              INTEGER,
                              frAtmPvcVccInterWorkingFuncIn     INTEGER,
                              frAtmPvcVccTimeLenOut             INTEGER,
                              frAtmPvcVccInterWorkingFuncOut    INTEGER,
                              frAtmPvcVccPeakCellRateA2B        Integer32,
                              frAtmPvcVccSusCellRateA2B         Integer32,
                              frAtmPvcVccMaxBurstSizeA2B        Integer32,
                              frAtmPvcVccPeakCellRateB2A        Integer32,
                              frAtmPvcVccSusCellRateB2A         Integer32,
                              frAtmPvcVccMaxBurstSizeB2A        Integer32,
                              frAtmPvcVccConnectionStatus       INTEGER,

                              frAtmPvcVccDualHomed              INTEGER,

                              frAtmPvcVccOamStatus              INTEGER,
                              frAtmPvcVccViB                    INTEGER,
                              frAtmPvcVccCnfrmTypeB2A           INTEGER,

                              frAtmPvcVccVUNIB                  INTEGER,

                              frAtmPvcVccOAMConnEndPtStatus     INTEGER,
                              frAtmPvcVccOAMSegEndPtStatus      INTEGER,
                              frAtmPvcVccMfsB2A                 INTEGER,
                              frAtmPvcVccConnFailureCauseA      FrConnFailureCause,
                              frAtmPvcVccConnFailureCauseB      AtmConnFailureCause,
                              frAtmPvcVccTrafficShapingA2B      INTEGER
                             }

frAtmPvcVccIfA        OBJECT-TYPE
                SYNTAX IntfIndex
                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION "Interface index for side A, the frame relay side, 
                             of a frame relay-atm PVC VCC connection.  Number 
                             value is interpreted as an interface of the form, 
                             SSPPCCC, where SS is the card slot, PP is the port 
                             number, and CCC is the channel number."
                ::= { frAtmPvcVccEntry 1 }

frAtmPvcVccDlciA OBJECT-TYPE
                SYNTAX Integer32
                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION "DLCI value for side A, the frame relay side, 
                             of a frame relay-atm PVC VCC connection.  Valid
                             range is a number between 0 and 1023."
                ::= { frAtmPvcVccEntry 2 }

frAtmPvcVccIfB OBJECT-TYPE
                SYNTAX IntfIndex
                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION "Interface index for side B, the atm side, of a 
                             frame relay-atm PVC VCC connection.  Number value 
                             is interpreted as an interface of the form, 
                             SSPPCCC, where SS is the card slot, PP is the port 
                             number, and CCC is the channel number."
                ::= { frAtmPvcVccEntry 3 }

frAtmPvcVccVpiB OBJECT-TYPE
                SYNTAX INTEGER (0..4095)
                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION "VPI value for side B, the atm side, of a frame
                             relay-atm PVC VCC connection.  Valid range is a 
                             number between 0 and 4095."
                ::= { frAtmPvcVccEntry 4 }

frAtmPvcVccVciB OBJECT-TYPE
                SYNTAX INTEGER (0..65535)
                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION "VCI value for side B, the atm side, of a frame
                             relay-atm PVC VCC connection.  Valid range is a 
                             number between 0 and 65535."
                ::= { frAtmPvcVccEntry 5 }

