-- Stratum Configuration Group stratumConfig



-- Equipment Configuration Group
-- The switch supports the following loopback configurations for some of the
-- interface ports, e.g., ds3, ds1, e1, e3, and oc3.

--                noLoop mode

--                The interface is not in a loop back state and signal
--                flow is normal.

--                  __________________________
--                 [                          ]
--                 [  SWITCH                  ]  ___ signal flow
--                 [                          ] /
--         ==<=================<====================<==
-- I/O port ===>  ##                          ##  <===  I/O port
--         ==>=================>====================>==
--                 [                          ] \___ signal flow
--                 [                          ]
--                 [__________________________]


--                localLoop mode

--                The signal received from far end I/O port, that is supposed
--                to be transmitted out at this interface does go through the
--                device (maximum penetration) but is looped back out.

--                  __________________________
--                 [                          ]
--                 [  SWITCH                  ]
--                 [                          ]
--                 [ ====<==========================<==
-- I/O port ===>  ## |                        ##  <===  I/O port
--                 [ ====>==========================>==
--                 [                          ]
--                 [                          ]
--                 [__________________________]


--                lineLoop mode

--                The received signal at this interface does not go through
--                the device (minimum penetration) but is looped back out.

--                  __________________________
--                 [                          ]
--                 [  SWITCH                  ]
--                 [                          ]
--         ==>=======                         ]
-- I/O port ===>  ## |                        ##  <===  I/O port
--         ==<=======                         ]
--                 [                          ]
--                 [                          ]
--                 [__________________________]


--                payloadLoop mode

--                The received signal at this interface goes through the
--                device (maximum penetration) but is looped back out.

--                  __________________________
--                 [                          ]
--                 [  SWITCH                  ]
--                 [                          ]
--         ==>==========================>==== ]
-- I/O port ===>  ##                        | ##  <===  I/O port
--         ==<==========================<==== ]
--                 [                          ]
--                 [                          ]
--                 [__________________________]


cardTable       OBJECT-TYPE
                SYNTAX SEQUENCE OF CardEntry
                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION "Table of card configuration data"
                ::= { equipConfig 1 }
 
cardEntry       OBJECT-TYPE
                SYNTAX CardEntry
                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION "Row definition for ac120 card"
                INDEX { cardSlot }
                ::= { cardTable 1 }
 
CardEntry       ::= SEQUENCE {
                cardSlot                INTEGER,
                cardType                INTEGER,
                cardProtectionStatus    INTEGER,
                cardOperStatus          INTEGER,
                cardAdminStatus         INTEGER,
                cardAlarmStatus         INTEGER,
                cardCleiCode            DisplayString,
                cardRevision            Integer32,
                cardSerialNumber        DisplayString,
                cardOperatingMode       INTEGER,
                protectionConfigMode    INTEGER,
                protectionStatus        INTEGER,
                protectionSlot          INTEGER 
                }
 
cardSlot        OBJECT-TYPE
                SYNTAX INTEGER (1..24)
                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION "Physical slot location.  Valid range is a number
                             between 1 and 24."
                ::= { cardEntry 1 }
 
cardType        OBJECT-TYPE
                SYNTAX INTEGER  {
                                none(1),
                                taxi(2),
                                oC-3c(3),
                                dS3-ATM(4),
                                dS1-CE(5),
                                dS1-ATM(6),
                                e1-CE(7),
                                e3-ATM(8),
                                highSpeed(9),
                                mSerial(10),
                                dSP1(11),
                                tWStation(12),
                                tWOffice(13),
                                cPU(14),
                                stratum(15),
                                pwrSupply(16),
                                protectionCard(17),
                                e1-ATM(18),
                                ethernet(19),
                                enhDS1(20),
                                enhE1(21),
                                oC-3cMMAQ(22),
                                oC-3cMMTS(23),
                                oC-3cSMAQ(24),
                                oC-3cSMTS(25),
                                sTM-1SMAQ(26),
                                sTM-1SMTS(27),
                                sTM-1MMAQ(28),
                                sTM-1MMTS(29),
                                dS3-FR(30),
                                dSP2A(31),
                                dS1-IMA(32),
                                e1-IMA(33),
                                alarm(34),
                                dSP2B(35),
                                aps-OC-3cSM(36),
                                aps-OC-3cMM(37),
                                msp-STM-1SM(38),
                                msp-STM-1MM(39),
                                cH-DS3(40),
                                cH-STS1(41),
                                rT-S(42),
                                cC-Server(43),
                                hD-E1(44),
                                hD-DS1(45),
                                dS3-IMA(46),
                                dSP2C(47),
                                tasm(48),
                                mD-DS1(49),
                                quadSerial(50),
                                mD-DS1-IMA(51),
                                mD-E1-IMA(52),
                                hD-DS1-IMA(53),
                                hD-E1-IMA(54),
                                chDS3STS1-3P(55),
                                unstDS3E3-3P(56),
                                dS3E3-ATM-3P(57),
                                oC12c-STM4-MM(58),
                                oC12c-STM4-SM(59),
                                dSP2D(60),
                                ethernet-4P(61),
                                chDS3STS1-3P-Prot(62),
                                alarm-fan(63),
                                dS1-Nx64CAS(64)
                                }
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "Type of card in a physical slot.  A specific type 
                             of card is associated with each number.
                 none(1)                 no board present
                taxi(2)                 Taxi
                oC-3c(3)                OC3
                dS3-ATM(4)              DS3 ATM
                dS1-CE(5)               DS1 circuit emulation
                dS1-ATM(6)              DS1 ATM
                e1-CE(7)                E1 circuit emulation
                e3-ATM(8)               E3 ATM
                highSpeed(9)            High Speed Serial
                mSerial(10)             Multi Serial
                dSP1(11)                DSP
                tWStation(12)           Two Wire Station
                tWOffice(13)            Two Wire Office
                cPU(14)                 Central Processor Unit
                stratum(15)             Stratum
                pwrSupply(16)           Power Supply
                protectionCard(17)      protection card
                e1-ATM(18)              E1 ATM
                ethernet(19)            Ethernet
                enhDS1(20)              Enhanced DS1
                enhE1(21)               Enhanced E1
                oC-3cMMAQ(22)           OC3 Multimode Aqueman
                oC-3cMMTS(23)           OC3 Multimode Traffic Shaping
                oC-3cSMAQ(24)           OC3 Singlemode Aqueman
                oC-3cSMTS(25)           OC3 Singlemode Traffic Shaping
                sTM-1SMAQ(26)           STM1 Singlemode Aqueman
                sTM-1SMTS(27)           STM1 Singlemode Traffic Shaping
                sTM-1MMAQ(28)           STM1 Multimode Aqueman
                sTM-1MMTS(29)           STM1 Multimode Traffic Shaping
                dS3-FR(30)              DS3 Frame Relay
                dSP2A(31)               DSP2 card revision 2A
                dS1-IMA(32)             DS1 Inverse Multiplexing over
                                        ATM (IMA) I/O card
                e1-IMA(33)              E1 IMA
                alarm(34)               alarm card
                dSP2B(35)               DSP card revision 2B
                aps-OC-3cSM(36)         Automatic Protection Switching
                                        OC3 Single Mode
                aps-OC-3cMM(37)         Automatic Protection Switching
                                        OC3 Multi Mode
                msp-STM-1SM(38)         Multi Switch Protection STM1
                                        Singemode
                msp-STM-1MM(39)         Multi Switch Protection STM1
                                        Multimode
                cH-DS3(40)              Channelized DS3
                cH-STS1(41)             Channelized STS1
                rT-S(42)                Route Server
                cC-Server(43)           Continuity Check Server
                hD-E1(44)               High Density E1
                hD-DS1(45)              High Density DS1
                dS3-IMA(46)             DS3 IMA
                dSP2C(47)               DSP card revision 2C
                tasm(48)                Tone Announcement Service Module
                mD-DS1(49)              Medium Density DS1
                quadSerial(50)          Quad Serial
                mD-DS1-IMA(51)          Medium Density DS1 IMA
                mD-E1-IMA(52)           Medium Density E1 IMA
                hD-DS1-IMA(53)          High Density DS1 IMA
                hD-E1-IMA(54)           High Density E1 IMA
                chDS3STS1-3P(55)        3-port channelized DS3 STS1
                unstDS3E3-3P(56)        3-port unstructured DS3 E3
                dS3E3-ATM-3P(57)        3-port DS3 E3 ATM
                oC12c-STM4-MM(58)       OC12 STM4 Multimode
                oC12c-STM4-SM(59)       OC12 STM4 Singlemode
                dSP2D(60)               DSP card revision 2D
                ethernet-4P(61)         4-port Ethernet
                chDS3STS1-3P-Prot(62)   3-port channelized DS3 STS1
                                        protection
                alarm-fan(63)           alarm fan card
                dS1-Nx64CAS(64)         DS1 card supporting N x 64 CAS"
                DEFVAL { none }
                ::= { cardEntry 2 }
 
cardProtectionStatus    OBJECT-TYPE
                SYNTAX INTEGER  {
                                none(1),
                                protected(2),
                                wrongType(3)
                                }
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "Current protection status of the card: 
                        none if the card is un-protected; protected
                        if it is under the protection of another card; 
                        wrongType if the card types of protection card
                        and protected card are different."
                DEFVAL { none }
                ::= { cardEntry 3 }
 
cardOperStatus OBJECT-TYPE
                SYNTAX INTEGER  {
                                primary(1),
                                standby(2),
                                unknown(3)
                                }
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "Current operational status of the card.
                        For I/O card, it is primary if configured, 
                        it is unknown if unconfigured, or removed.
                        For CPU, it is primary if the CPU is master
                        CPU, or standby if the CPU is slave CPU."
                DEFVAL { unknown }
                ::= { cardEntry 4 }

cardAdminStatus OBJECT-TYPE
                SYNTAX INTEGER  {
                                switchProtection(1)
                                }
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "administrative status"
                ::= { cardEntry 5 }
 
cardAlarmStatus  OBJECT-TYPE
                SYNTAX INTEGER  {
                                noAlarm(1),
                                wrongCardType(2),
                                lineFailed(3),
                                cardRemoved(4),
                                referenceClockFailed(5),
                                compositeClockFailed(6),
                                overload(7),
                                plus5vFailed(8),
                                plus120vFailed(9),
                                minus48vFailed(10),
                                unknownAlarm(11),
                                completeClockFailed(12),
                                backplaneCircuitryFailed(13),
                                powerFailed(14),
                                inputPortClkFailed(15)
                                }
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "last alarm occurred"
                DEFVAL { noAlarm }
                ::= { cardEntry 6 }

cardCleiCode        OBJECT-TYPE
                SYNTAX DisplayString (SIZE(10))
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "CLEI code"
                ::= { cardEntry 7}

cardRevision        OBJECT-TYPE
                SYNTAX Integer32 
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "Card Revision number"
                ::= { cardEntry 8}

cardSerialNumber OBJECT-TYPE
                SYNTAX DisplayString (SIZE(12))
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "Serial Number"
                ::= { cardEntry 9}

cardOperatingMode        OBJECT-TYPE
                SYNTAX INTEGER  {
                                none(1),
                                dS3(2), 
                                e3(3),  
                                sTS1e(4),
                                oC12(5),
                                sTM4(6)
                                } 
                MAX-ACCESS read-write
                STATUS current 
                DESCRIPTION "Mode of operation for a card. This is 
                applicable
                to those card types which can be configured in a dual mode.
                e.g. unstDS3E3-3P can be operational as either a 3 port 
                DS3 or
                E3 card. All ports on the card will be of the same type
                depending on the card's operating mode."
        DEFVAL { none }
                ::= { cardEntry 10 }

protectionConfigMode          OBJECT-TYPE
                    SYNTAX INTEGER  {
                              unprotected(1),
                              protection(2),
                              working(3)
                              }
                    MAX-ACCESS read-write
                    STATUS current
                    DESCRIPTION "Protection Configuration Mode of a Card
                       unprotected: Card is not in protection or protected state
                       protection: Card is in protection state
                       working: Card is in protected/working state and this 
                               state is read only and cannot be set by the user"
                    DEFVAL { unprotected }
                    ::= { cardEntry 11 }

protectionStatus             OBJECT-TYPE
                    SYNTAX INTEGER  {
                              none(1),
                              inservice-Active(2),
                              inservice-Standby(3),
                              outOfService-Active(4),
                              outOfService-Standby(5)
                              }
                    MAX-ACCESS read-only
                    STATUS current
                    DESCRIPTION "Protection Status of a card. Inservice and 
                              OutofService indicates if the card has failed or 
                              still operational.
                              Active or Standby state indicates if it is 
                              carrying active traffic or not."
                    DEFVAL { none }
                    ::= { cardEntry 12 }

protectionSlot              OBJECT-TYPE 
                    SYNTAX INTEGER (0..15)
                    MAX-ACCESS read-only
                    STATUS current
                    DESCRIPTION "If Protection Config Mode is Working then 
                              this field gives the protection slot."
                    ::= { cardEntry 13 }

 
taxiPortConfigTable OBJECT-TYPE
                SYNTAX SEQUENCE OF TaxiPortConfigEntry
                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION "Table of common physical Port attributes"
                ::= { equipConfig 2 }

taxiPortConfigEntry OBJECT-TYPE
                SYNTAX TaxiPortConfigEntry
                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION "Row definition for common physical
                             port attributes"
                INDEX { taxiPortId }
                ::= { taxiPortConfigTable 1 }

TaxiPortConfigEntry ::= SEQUENCE {
                taxiPortId                Integer32,
                taxiPortType                INTEGER,
                taxiLineStatus                INTEGER,
                taxiOutputTransferRate        INTEGER
                }

taxiPortId      OBJECT-TYPE
                SYNTAX Integer32
                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION "interface index - SSPP"
                ::= { taxiPortConfigEntry 1 }

