vbrAtmSpvcVccStatsCellsDroppedB OBJECT-TYPE
                SYNTAX Integer32
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "Cells Dropped on the Incoming side of B"
                ::= { vbrAtmSpvcVccStatsEntry 24 }

vbrAtmSpvcVccStatsCellsTaggedB OBJECT-TYPE
                SYNTAX Integer32
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "Cells Tagged on the Incoming side of B"
                ::= { vbrAtmSpvcVccStatsEntry 25 }

atmSpvcVccStatsTable OBJECT-TYPE
                SYNTAX SEQUENCE OF AtmSpvcVccStatsEntry
                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION "Table of statistics for a ATM SPVC channel 
                             connection"
                ::= { connectionStatistics 16 }

atmSpvcVccStatsEntry OBJECT-TYPE
                SYNTAX AtmSpvcVccStatsEntry
                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION "Row definition for the ATM SPVC connection 
                             statistics table"
                INDEX { atmSpvcVccStatsIfA, atmSpvcVccStatsVpiA,
                        atmSpvcVccStatsVciA, atmSpvcVccStatsIfB,
                        atmSpvcVccStatsVpiB, atmSpvcVccStatsVciB }
                ::= { atmSpvcVccStatsTable 1 }

AtmSpvcVccStatsEntry ::= SEQUENCE {
                atmSpvcVccStatsIfA                         IntfIndex,
                atmSpvcVccStatsVpiA                         INTEGER,
                atmSpvcVccStatsVciA                        INTEGER,
                atmSpvcVccStatsIfB                         IntfIndex,
                atmSpvcVccStatsVpiB                         INTEGER,
                atmSpvcVccStatsVciB                        INTEGER,
                atmSpvcVccStatsInCellCountHiA                Integer32,
                atmSpvcVccStatsInCellCountLoA                Integer32,
                atmSpvcVccStatsOutCellCountHiA                 Integer32,
                atmSpvcVccStatsOutCellCountLoA                 Integer32,
                atmSpvcVccStatsInCellCountHiB                Integer32,
                atmSpvcVccStatsInCellCountLoB                Integer32,
                atmSpvcVccStatsOutCellCountHiB                 Integer32,
                atmSpvcVccStatsOutCellCountLoB                 Integer32,
                atmSpvcVccStatsTimer                        Integer32,
                atmSpvcVccStatsFrwdErrCrrctnRateA2B        INTEGER,
                atmSpvcVccStatsFrwdErrCrrctnRateB2A        INTEGER,
                atmSpvcVccStatsInOdometerCellCountHiA        Integer32,
                atmSpvcVccStatsInOdometerCellCountLoA        Integer32,
                atmSpvcVccStatsOutOdometerCellCountHiA         Integer32,
                atmSpvcVccStatsOutOdometerCellCountLoA         Integer32,
                atmSpvcVccStatsInOdometerCellCountHiB        Integer32,
                atmSpvcVccStatsInOdometerCellCountLoB        Integer32,
                atmSpvcVccStatsOutOdometerCellCountHiB         Integer32,
                atmSpvcVccStatsOutOdometerCellCountLoB         Integer32,
                atmSpvcVccStatsOdometerTimer                Integer32,
                atmSpvcVccStatsOdometerReset                 INTEGER,
                atmSpvcVccStatsCellsDroppedA                  Integer32,
                atmSpvcVccStatsCellsTaggedA                  Integer32,
                atmSpvcVccStatsCellsDroppedB                  Integer32,
                atmSpvcVccStatsCellsTaggedB                  Integer32
                }

atmSpvcVccStatsIfA OBJECT-TYPE
                SYNTAX IntfIndex
                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION "Interface index for side A of an ATM-to-ATM PVC VCC
                             connection.  Number value is interpreted as an
                             interface of the form, SSPPCCC, where SS is the 
                             card slot, PP is the port number, and CCC is the 
                             channel number."
                ::= { atmSpvcVccStatsEntry 1 }

atmSpvcVccStatsVpiA OBJECT-TYPE
                SYNTAX INTEGER (0..4095)
                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION "VPI value for side A of an ATM-to-ATM PVC VCC 
                             connection.  Valid range is a number between 0 and 
                             4095."
                ::= { atmSpvcVccStatsEntry 2 }

atmSpvcVccStatsVciA OBJECT-TYPE
                SYNTAX INTEGER (0..65535)
                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION "VCI value for side A of an ATM-to-ATM PVC VCC 
                             connection.  Valid range is a number between 0 and 
                             65535."
                ::= { atmSpvcVccStatsEntry 3 }

atmSpvcVccStatsIfB OBJECT-TYPE
                SYNTAX IntfIndex
                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION "Interface index for side B of an ATM-to-ATM PVC VCC
                             connection.  Number value is interpreted as an
                             interface of the form, SSPPCCC, where SS is the 
                             card slot, PP is the port number, and CCC is the 
                             channel number."
                ::= { atmSpvcVccStatsEntry 4 }

atmSpvcVccStatsVpiB OBJECT-TYPE
                SYNTAX INTEGER (0..4095)
                MAX-ACCESS not-accessible
                STATUS current
