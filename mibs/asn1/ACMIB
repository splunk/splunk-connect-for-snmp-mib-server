                SYNTAX INTEGER {
                                equipment(1),
                                connections(2),
                                routing(3),
                                all(4)
                               }
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "save configuration "
                ::= { sysControl 1 }

upgradeSoftware OBJECT-TYPE
                SYNTAX INTEGER {
                                upgrade(1),
                                fallBack(2),
                                forceUpgrade(3),
                                forceFallBack(4),
                                synchronize(5),-- backup sw ver sync
                                abort(6),-- abort any ver control progress
                                forceSynchronize(7),--similar to forceUpgrade/forceFallBack
                                forceAbort(8) -- abort for forceUpgrade/forceFallBack
                                }
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "software load management"
                ::= { sysControl 2 }

