                twoWireSourceChannelId                         IntfIndex,
                twoWireSourceChannelIfType                 INTEGER,
                twoWireSourceChannelConnectionType        INTEGER,
                twoWireSourceChannelCompandingLaw         INTEGER,
                twoWireSourceChannelSecuFeature         INTEGER,
                twoWireSourceChannelVoiceCompression         INTEGER,
                twoWireSourceChannelEchoCancellation         INTEGER,
                twoWireSourceChannelSilenceSupression         INTEGER,
                twoWireSourceChannelEncapsulationAal         INTEGER,
                twoWireSourceChannelSubscriberService        INTEGER
                }
 
