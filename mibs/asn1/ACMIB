                SYNTAX Integer32
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "unrecognized ICMP type"
                ::= { viprVpnStatsEntry 15 }

viprVpnStatsIcmpInErrors OBJECT-TYPE
                SYNTAX Integer32
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "error in received icmp message"
                ::= { viprVpnStatsEntry 16 }

