                 ::= { sysInfo 26 }


primaryCpuHwType OBJECT-TYPE
                  SYNTAX INTEGER {
                                notKnown(1), 
                                version1(2),
                                version2(3)
                               }
                  MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "Indicates the hardware version type of the 
                                primary CPU.
                             version1: 150MHz Saturn13 board supporting 
                                        8MB-64MB Memory.
                             version2: 200MHz Saturn15 board supporting 
                                        64MB-256MB Memory."
                DEFVAL { notKnown }
                ::= { sysInfo 27 }

secondaryCpuHwType OBJECT-TYPE
                  SYNTAX INTEGER {
                                notKnown(1),
                                version1(2),
                                version2(3)
                                }
                  MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "Indicates the hardware version type of the 
                                secondary CPU.
                             version1: 150MHz Saturn13 board supporting 
                                8MB-64MB Memory.
                             version2: 200MHz Saturn15 board supporting 
                                64MB-256MB Memory."
                DEFVAL { notKnown }
                ::= { sysInfo 28 }

msgPoolViewEntry OBJECT-TYPE
                 SYNTAX MsgPoolViewEntry
                 MAX-ACCESS not-accessible
                 STATUS current
                 DESCRIPTION "Row definition of Msg Pool Table"
                 INDEX { msgPoolViewIndex }
                 ::= { msgPoolViewTable 1 }

MsgPoolViewEntry ::= SEQUENCE {
                         msgPoolViewIndex                Integer32,
                        msgPoolViewTskId                Integer32,
                        msgPoolViewTskName                DisplayString,
                        msgPoolViewMsgCmd                Integer32
                 }

msgPoolViewIndex OBJECT-TYPE
                 SYNTAX Integer32
                 MAX-ACCESS not-accessible
                 STATUS current
                 DESCRIPTION "It is the index of the MsgPool array for which
                              the information is requested. Information about
                              Msgs Which are not there in the free pool is
                              returned.A msg is said to be out of the free
                              pool by comparing the signature.
                              The limit being the max number of msgs allowed
                              on the system"
                 ::= { msgPoolViewEntry 1 }

msgPoolViewTskId OBJECT-TYPE
                 SYNTAX Integer32
                 MAX-ACCESS read-only
                 STATUS current
                 DESCRIPTION "Task Id of the Sender of the Particular Msg,
                              This will give the integer value, has to be
                              converted to hex for comparison"
                 ::= { msgPoolViewEntry 2}

msgPoolViewTskName OBJECT-TYPE
                   SYNTAX DisplayString (SIZE(20))
                   MAX-ACCESS read-only
                   STATUS current
                   DESCRIPTION "Task Name of the Sender of the Particular Msg"
                 ::= { msgPoolViewEntry 3}

msgPoolViewMsgCmd OBJECT-TYPE
                  SYNTAX Integer32
                  MAX-ACCESS read-only
                  STATUS current
                  DESCRIPTION "msgCmd being sent by the sending Task"
                 ::= { msgPoolViewEntry 4}


sysInfoDiskCheck OBJECT-TYPE
                  SYNTAX INTEGER {
                                  ok    (1),
                                  error (2)
                                 }
                          
                  MAX-ACCESS read-only
                  STATUS current
                  DESCRIPTION "Indicates the checking status of 
                               the disk in CPU card. "
                  ::= { sysInfo 29 }

switchAlarmStatus        OBJECT-TYPE
                    SYNTAX INTEGER {
                                notAvailable(1),
                                none(2),
                                minor(3),
                                major(4),
                                critical(5)
                                }
                    MAX-ACCESS read-only
                    STATUS current
                    DESCRIPTION "Current status of the alarm on the switch.
                    notAvailable - The alarm status could not be retreived.
                    none - No alarm is present.
                    minor, major, critical - An alarm of the enumerated type
                                             is present"
                    ::= { sysInfo 30 }


-- Ethernet cache configuration group

arp1Table        OBJECT-TYPE
                SYNTAX SEQUENCE OF Arp1Entry
                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION "Table of ARP cache data."
                ::= { ethernetConfig 1 }

arp1Entry        OBJECT-TYPE
                SYNTAX Arp1Entry
                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION "Row definition for ARP cache."
                INDEX { arp1CardSlot, arp1BridgeGroupNum, arp1IpAddress }
                ::= { arp1Table 1 }

Arp1Entry        ::= SEQUENCE {
                              arp1CardSlot                INTEGER,
                              arp1BridgeGroupNum        INTEGER,
                              arp1IpAddress                IpAddress,
                              arp1PhysicalAddr                OCTET STRING,
                              arp1EntryAge                Integer32
                             }

