-- GR303 Interface Group Table (GR303IG)

gr303IgConfig                 OBJECT IDENTIFIER ::=   { gr303Config 1}

gr303IgConfigTable OBJECT-TYPE
                SYNTAX SEQUENCE OF Gr303IgConfigEntry
                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION "Table of GR303 group configuration"
                ::= { gr303IgConfig 1 }

gr303IgConfigEntry OBJECT-TYPE
                SYNTAX Gr303IgConfigEntry
                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION "Row definition for GR303 group configuration"
                INDEX { gr303IgId }
                ::= { gr303IgConfigTable 1 }

Gr303IgConfigEntry::= SEQUENCE {
            gr303IgId                        INTEGER,
            gr303IgRowStatus            RowStatus,       
                gr303IgAdminStatus          INTEGER,
                gr303IgPrimaryTMCChanIntfId  IntfIndex,
                gr303IgPrimaryEOCChanIntfId  IntfIndex,
                gr303IgBackupTMCChanIntfId  IntfIndex,
                gr303IgBackupEOCChanIntfId  IntfIndex,
            gr303IgPrimaryTMCStatus      INTEGER,
            gr303IgPrimaryEOCStatus      INTEGER,
            gr303IgBackupTMCStatus      INTEGER,
            gr303IgBackupEOCStatus      INTEGER,
            gr303IgTMCProtSwitchProvState INTEGER,
            gr303IgEOCProtSwitchProvState INTEGER,
            gr303IgTMCProtSwitchCommand INTEGER,
            gr303IgEOCProtSwitchCommand INTEGER,
            gr303IgLocalDigitalSwitchType INTEGER,
            gr303IgLocalDigitalSwitchVersion INTEGER
           }
 
gr303IgId      OBJECT-TYPE
                SYNTAX INTEGER (1..84)
                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION "This is used as an index into GR303IG table which
                recognizes GR303 Interface Group"
                ::= { gr303IgConfigEntry 1 }

gr303IgRowStatus          OBJECT-TYPE
                             SYNTAX RowStatus
                             MAX-ACCESS read-write
                             STATUS current
                             DESCRIPTION "GR303IG config entry row status"
                             ::= { gr303IgConfigEntry 2 }

gr303IgAdminStatus      OBJECT-TYPE
                SYNTAX INTEGER  {
                        inService (1),
                            outOfService (2)
                }
                MAX-ACCESS read-write
                STATUS current
