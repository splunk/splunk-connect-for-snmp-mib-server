protectionGroupConfigTable   OBJECT-TYPE
                         SYNTAX SEQUENCE OF ProtectionGroupConfigEntry
                         MAX-ACCESS not-accessible
                         STATUS current
                         DESCRIPTION "This table gives the list of Slot 
                             Protection Groups in the System. Slot protection 
                             group in one in which there are n cards being
                             protected by 1 standby card."
                ::= { equipConfig 19 }

protectionGroupConfigEntry   OBJECT-TYPE
                         SYNTAX ProtectionGroupConfigEntry
                         MAX-ACCESS not-accessible
                         STATUS current
                         DESCRIPTION "Row Definition for the Protection Group 
                             Tables"
                         INDEX { protectionSlotId }
                ::= { protectionGroupConfigTable 1 }

ProtectionGroupConfigEntry ::= SEQUENCE {
               protectionSlotId          INTEGER,
               workingSlotBitmap         INTEGER,
               protectionCommand         INTEGER,
               protectionCommandSlotId   INTEGER,
               protectionGroupStatus     INTEGER,
               workingSlotId             INTEGER,
               workingFailedBitmap       INTEGER,
               protectionActiveCounter   Integer32,
               protectionOOSCounter      Integer32
                                }

protectionSlotId             OBJECT-TYPE
                         SYNTAX INTEGER (1..15)
                         MAX-ACCESS not-accessible
                         STATUS current
                         DESCRIPTION "Protection Group Index. Index is the 
                             slot id of the Slot Configured as Protection."
                ::= { protectionGroupConfigEntry 1 }

workingSlotBitmap            OBJECT-TYPE
                         SYNTAX INTEGER (0..65535)
                         MAX-ACCESS read-write
                         STATUS current
                         DESCRIPTION "Working Slot Bitmap which the protection 
                             slot will monitor"
                         DEFVAL { 0 }
                ::= { protectionGroupConfigEntry 2 }

protectionCommand            OBJECT-TYPE
                         SYNTAX INTEGER {
                                clear(1),
                                forcedSwitchOverToProtection(2)
                             }
                         MAX-ACCESS read-write
                         STATUS current
                         DESCRIPTION "List of commands which can be executed 
                             on a protection Group.
                             Clear : This command is used to clear any previous
                                     commands which may have been and get back 
                                     to normal nto1 operation
                             Forced SwitchOver To Protection : Switchover to 
                                     Protection card even if the working card 
                                     is in service active
                             These commands to used in conjunction with 
                             the working slot id."
                          DEFVAL { clear }
                ::= { protectionGroupConfigEntry 3 }


protectionCommandSlotId      OBJECT-TYPE
                         SYNTAX INTEGER (1..15)
                         MAX-ACCESS read-write
                         STATUS current
                         DESCRIPTION "This is the working Slot to which command
                             has to be applied"
                         DEFVAL { 0 }
                ::= { protectionGroupConfigEntry 4}

protectionGroupStatus         OBJECT-TYPE
                         SYNTAX INTEGER  {
                                none(1),
                                inservice-Active(2),
                                inservice-Standby(3),
                                outOfService-Active(4),
                                outOfService-Standby(5)
                               }
                         MAX-ACCESS read-only
                         STATUS current
                         DESCRIPTION "Protection Status of a Protection Card. 
                             Inservice and OutofService indicates if the card 
                             has failed or still operational. Active or Standby
                             state indicates if it is carrying active traffic
                             or not."
                          DEFVAL { none }
                ::= { protectionGroupConfigEntry 5 }

workingSlotId                 OBJECT-TYPE
                          SYNTAX INTEGER (0..15)
                          MAX-ACCESS read-only
                          STATUS current
                          DESCRIPTION "If protection group status is active, 
                              this field will give the slot which is being 
                              working. In case it is in standby mode this
                              field will show 0"
                          DEFVAL { 0 }
                ::= { protectionGroupConfigEntry 6 }

workingFailedBitmap           OBJECT-TYPE
                          SYNTAX INTEGER (0..65535)
                          MAX-ACCESS read-only
                          STATUS current
                          DESCRIPTION "Gives Bitmap of the Failed Working Slots.
                              Subset of the Working Slot Bitmap."
                           DEFVAL { 0 }
                ::= { protectionGroupConfigEntry 7 }

protectionActiveCounter       OBJECT-TYPE
                          SYNTAX Integer32
                          MAX-ACCESS read-only
                          STATUS current
                          DESCRIPTION "Give the Number of Times a Protection 
                              has gone to active."
                ::= { protectionGroupConfigEntry 8 }

protectionOOSCounter          OBJECT-TYPE
                          SYNTAX Integer32
                          MAX-ACCESS read-only
                          STATUS current
                          DESCRIPTION "Give the Number of Times a Protection 
                              Card has failed."
                ::= { protectionGroupConfigEntry 9 }


-- The Channel Configuration Group

taxiChannelConfigTable OBJECT-TYPE
                SYNTAX SEQUENCE OF TaxiChannelConfigEntry
                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION "Table of common physical Port attributes"
                ::= { channelConfig 1 }

