                msKgResync                INTEGER,
                msHandshake                INTEGER,
                msSpeedAdaption                INTEGER,
                msBitStuffing                INTEGER,
                msLineStatus             INTEGER,
                msRxDataPolarity        INTEGER,
                msTxDataPolarity          INTEGER,
                msLoopbackConfig        INTEGER,
                msLOSDetection          INTEGER
                }

