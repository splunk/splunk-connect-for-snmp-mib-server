                SYNTAX INTEGER  {
                                 disabled(1),
                                 enabled(2)
                                }
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "silence detection off/on"
                DEFVAL { disabled }
                ::= { cirEmAtmSpvcVccEntry 6 }

