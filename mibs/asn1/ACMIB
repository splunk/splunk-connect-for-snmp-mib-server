-- Alarm card configuration group

alarmStatusInputBitmap OBJECT-TYPE
                SYNTAX OCTET STRING (SIZE(2))
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "Remote status from external device.
                             bit = 0  closed,
                             bit = 1  open,
                
                             LSB 15 bits represent 15 external inputs.
                             MSB bit 15 is always 0."
                ::= { alarmConfig 1 }

alarmStatusOutputBitmap OBJECT-TYPE
                SYNTAX OCTET STRING (SIZE(1))
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "Control output for alarm card.
                             bit = 0  deactivated,
                             bit = 1  activated,
                
                             LSB 4 bits represent 4 external outputs.
                             Bit 0 through bit 3 is output 1 through 4
                             Control output 4 will be activated upon
                             power failure."
                DEFVAL { 0 }
                ::= { alarmConfig 2 }

alarmSeverityLevelBitmap OBJECT-TYPE
                SYNTAX OCTET STRING (SIZE(1))
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "Alarm severity level.
                             bit = 0  noAlarm,
                             bit = 1  Alarm present,
                
                             LSB 3 bits represent severity levels.
                             bit 0 = Minor Alarm,
                             bit 1 = Major Alarm,
                             bit 2 = Critical Alarm."
                DEFVAL { 0 }
                ::= { alarmConfig 3 }

alarmAcoStatusBitmap OBJECT-TYPE
                SYNTAX OCTET STRING (SIZE(1))
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "Audio alarm status.
                             bit = 0  No alarm cutoff,
                             bit = 1  Alarm cutoff,
                
                             LSB 3 bits represent audio alarms.
                             bit 0 = Local alarm cutoff,
                             bit 1 = remote alarm cutoff,
                             bit 2 = composite alarm cutoff status."
                ::= { alarmConfig 4 }


-- system test configuration group

oamTest                OBJECT IDENTIFIER ::= { sysTest 1 }

oamTestIfB        OBJECT-TYPE
                SYNTAX IntfIndex
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "Interface index used for starting loopback test.
                             Number value is interpreted as an interface of the 
                             form, SSPPCCC, where SS is the card slot, PP is 
                             the port number, and CCC is the channel number."
                ::= { oamTest 1 }

oamTestVpiB         OBJECT-TYPE
                SYNTAX INTEGER (0..4095)
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "VPI value under testing for oam loopback test.
                             Valid range is a number between 0 and 4095."
                ::= { oamTest 2 }

oamTestVciB         OBJECT-TYPE
                SYNTAX INTEGER (0..65535)
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "VCI value under testing for oam loopback test.
                             Valid range is a number between 0 and 65535."
                ::= { oamTest 3 }

oamTestStatus        OBJECT-TYPE
                SYNTAX INTEGER  {
                                not-running(1),
                                running(2),
                                successful(3),
                                failure(4),
                                timedout(5),
                                waiting-for-response(6)
                                }
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "Inidcates the current status of oam test."
                DEFVAL { not-runnin }
                ::= { oamTest 4 }

oamTestAction        OBJECT-TYPE
                SYNTAX INTEGER {
                                sendLoopback(1),
                                sendContinuously(2),
                                stopTest(3),
                                resetTxCount(4),
                                resetRxCount(5),
                                resetMismatchCount(6)
                               }
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "Test action to be performed."
                DEFVAL { sendLoopback }
                ::= { oamTest 5 }

oamTestTxCount        OBJECT-TYPE
                SYNTAX Integer32
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "Valid cells transmitted."
                ::= { oamTest 6 }

oamTestRxCount        OBJECT-TYPE
                SYNTAX Integer32
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "Valid cells received."
                ::= { oamTest 7 }

oamTestMismatchCount OBJECT-TYPE
                SYNTAX Integer32
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "Number of times Rx correlation tag is mismatched
                             with the Tx correlation tag."
                ::= { oamTest 8 }

oamTestLocationID OBJECT-TYPE
                SYNTAX OCTET STRING (SIZE(16))
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "Location ID of the ATM Node where the loopback is 
                             to occur."
                DEFVAL { 'FFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFF'H }
                ::= { oamTest 9 }

oamTestLoopbackType OBJECT-TYPE
                SYNTAX INTEGER {
                                end-to-end(1),
                                segment-endpoint(2),
                                next-connection-point(3),
                                specific-location(4)
                                }
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "OAM Loopback type. Determines whether to 
                        send ete or segment loopback cell, and the value
                        of location Id field in the cell. Only for
                        specific-location value, user specified locationId 
                        field value taken into account."
                DEFVAL { end-to-end }
                ::= { oamTest 10 }

-- sysTest 2 

oamActDeactTable OBJECT-TYPE
                SYNTAX SEQUENCE OF OamActDeactEntry
                  MAX-ACCESS not-accessible
                  STATUS current
                  DESCRIPTION "Table of oam activate and deactivate functions"
                  ::= { sysTest 2 }

oamActDeactEntry OBJECT-TYPE
                  SYNTAX OamActDeactEntry
                MAX-ACCESS not-accessible 
                STATUS current
                DESCRIPTION "Row definition of oam activate and deactivate"
                INDEX { oamActDeactIfB, oamActDeactVpiB, oamActDeactVciB}
                ::= { oamActDeactTable 1 }

OamActDeactEntry ::= SEQUENCE {
                  oamActDeactIfB   IntfIndex,
                  oamActDeactVpiB  INTEGER,
                  oamActDeactVciB  INTEGER,
                  oamActDeactFuncType  INTEGER,
                 oamActDeactDirFlow  INTEGER,
                  oamActDeactFlowType     INTEGER,
                  oamActDeactBlkSizeAB  INTEGER,
                  oamActDeactBlkSizeBA  INTEGER,
                  oamActDeactStsSrcPointEteCC  INTEGER,
                  oamActDeactStsSnkPointEteCC  INTEGER,
                  oamActDeactRsltPrevReq INTEGER,
                  oamActDeactRowStatus  RowStatus,
                oamActDeactStsSrcPointEtePM  INTEGER,
                oamActDeactStsSnkPointEtePM  INTEGER,
                oamActDeactStsSrcPointEtePMBR  INTEGER,
                oamActDeactStsSnkPointEtePMBR  INTEGER,
                oamActDeactBRBlockError  INTEGER,
                oamActDeactBRLostInsertedCell   INTEGER,
                oamActDeactBRMisInsertedCell INTEGER,
                oamActDeactStsSrcPointSegPM          INTEGER,
                    oamActDeactStsSnkPointSegPM          INTEGER,
                  oamActDeactStsSrcPointSegPMBR          INTEGER,
                    oamActDeactStsSnkPointSegPMBR          INTEGER,
                  oamActDeactStsSrcPointSegCC          INTEGER,
                    oamActDeactStsSnkPointSegCC          INTEGER
                }


oamActDeactIfB OBJECT-TYPE
                  SYNTAX IntfIndex
                  MAX-ACCESS not-accessible
                  STATUS current
                DESCRIPTION "Interface index for an  OAM CC connection.  
                        Number value is interpreted as an interface of the 
                        form, SSPPCCC, where SS is the card slot, PP 
                        is the port number, and CCC is the channel number."
                  ::= { oamActDeactEntry 1 }

oamActDeactVpiB OBJECT-TYPE
                  SYNTAX INTEGER (0..4095)
                  MAX-ACCESS not-accessible
                  STATUS current
                  DESCRIPTION "VPI value for an  oam CC connection.  
                            Valid range is a number between 0 and 4095."
                  ::= { oamActDeactEntry 2 }

oamActDeactVciB OBJECT-TYPE
                  SYNTAX INTEGER (0..65535)
                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION "VCI value for oam CC connection.  
                        Valid range is a number between 0 and 65535."
                  ::= { oamActDeactEntry 3 }

oamActDeactFuncType OBJECT-TYPE
                  SYNTAX INTEGER  {
                                    fpm-and-br(1),
                                    fpm-only(2),
                                continuity-check(3),
                                pmbr-and-cc(4),
                                pm-and-cc(5)
                                }
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "OAM Function type represents the OAM flow for
                        activation or deactivattion for this connection.
                        the possible function types activated in one connection 
                        are :
                        fpm-and-br : forward performance monitoring/backward 
                                     reporting
                        fpm-only : forward performance monitoring
                        continuity-check : continuity check
                        pmbr-and-cc : forward performance monitoring/backward 
                                      reporting and continuity check
                        pm-and-cc : forward performance monitoring and 
                                    continuity check"
                DEFVAL { continuity-check }
                ::= { oamActDeactEntry 4 }

oamActDeactDirFlow OBJECT-TYPE
                  SYNTAX INTEGER  {
                                    towards-far-end(1),
                                    towards-near-end(2),
                                    both-way(3)
                                    }
                  MAX-ACCESS read-write
                  STATUS current
                  DESCRIPTION "The direction in which cells will be 
                                transmitted."
                  DEFVAL { towards-near-end }
                  ::= { oamActDeactEntry 5 }

oamActDeactFlowType OBJECT-TYPE
                  SYNTAX INTEGER  {
                                end-to-end(1),
                                segment(2)
                                }
                  MAX-ACCESS read-write
                  STATUS current
                DESCRIPTION "Flow Type."
                DEFVAL { end-to-end }
                  ::= { oamActDeactEntry 6 }

oamActDeactBlkSizeAB OBJECT-TYPE
                  SYNTAX INTEGER  {
                                size128(1),
                                size256(2),
                                size512(3),
                                size1024(4),
                                size4096(5),
                                size8192(6),
                                size16384(7),
                                size32768(8),
                                size2048(9)
                                }
                  MAX-ACCESS read-write
                  STATUS current
                  DESCRIPTION "Block Size A-B represents the size of 
                        the block of user cells taken into consideration 
                        for performance monitoring in A-B direction."
                  DEFVAL { size1024 }
                  ::= { oamActDeactEntry 7 }

oamActDeactBlkSizeBA OBJECT-TYPE
                SYNTAX INTEGER  {
                                size128(1),
