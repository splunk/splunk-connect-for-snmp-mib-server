atmAtmSpvcVccTable OBJECT-TYPE
        SYNTAX SEQUENCE OF AtmAtmSpvcVccEntry
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION "Table of SPVC ATM virtual channel connections."
        ::= { connectionConfig 33 }

atmAtmSpvcVccEntry OBJECT-TYPE
        SYNTAX AtmAtmSpvcVccEntry
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION "Row definition of ATM SPVC VCC connections."
        INDEX { spvcAddrIfA, atmAtmSpvcVccVpiA, atmAtmSpvcVccVciA, atmAtmSpvcVccRemoteAtmPortAddr }
        ::= { atmAtmSpvcVccTable 1 }

AtmAtmSpvcVccEntry ::= SEQUENCE {
        atmAtmSpvcVccVpiA                          INTEGER,
        atmAtmSpvcVccVciA                          INTEGER,
        atmAtmSpvcVccRemoteAtmPortAddr             OCTET STRING,
        atmAtmSpvcVccIfB                           IntfIndex,
        atmAtmSpvcVccVpiB                          INTEGER,
        atmAtmSpvcVccVciB                          INTEGER,
        atmAtmSpvcVccServiceTypeA2B                INTEGER,
        atmAtmSpvcVccSarTypeA2B                    INTEGER,
        atmAtmSpvcVccPeakCellRateA2B               Integer32,
        atmAtmSpvcVccSusCellRateA2B                Integer32,
        atmAtmSpvcVccMaxBurstSizeA2B               Integer32,
        atmAtmSpvcVccFrwdErrCorrectionA2B          INTEGER,
        atmAtmSpvcVccServiceTypeB2A                INTEGER,
        atmAtmSpvcVccSarTypeB2A                    INTEGER,
        atmAtmSpvcVccPeakCellRateB2A               Integer32,
        atmAtmSpvcVccSusCellRateB2A                Integer32,
        atmAtmSpvcVccMaxBurstSizeB2A               Integer32,
        atmAtmSpvcVccFrwdErrCorrectionB2A          INTEGER,
        atmAtmSpvcVccFlow                          INTEGER,
        atmAtmSpvcVccType                          INTEGER,
        atmAtmSpvcVccConnType                      INTEGER,
        atmAtmSpvcVccLeafReference                 INTEGER,
        atmAtmSpvcVccOperStatus                    INTEGER,
        atmAtmSpvcVccRowStatus                     RowStatus,
        atmAtmSpvcVccRemoteSelectType              INTEGER,
        atmAtmSpvcVccRemoteVpi                     INTEGER,
        atmAtmSpvcVccRemoteVci                     INTEGER,
        atmAtmSpvcVccRetryInterval                 INTEGER,
        atmAtmSpvcVccRetryLimit                    INTEGER,
        atmAtmSpvcVccRestart                       INTEGER,
        atmAtmSpvcVccRetryFailure                  Integer32,
        atmAtmSpvcVccReleaseCause                  INTEGER,
        atmAtmSpvcVccReleaseDiag                   OCTET STRING,
        atmAtmSpvcVccRelease                       INTEGER,
        atmAtmSpvcVccStatsInCellCountHiA           Integer32,
        atmAtmSpvcVccStatsInCellCountLoA           Integer32,
        atmAtmSpvcVccStatsOutCellCountHiA          Integer32,
        atmAtmSpvcVccStatsOutCellCountLoA          Integer32,
        atmAtmSpvcVccStatsInCellCountHiB           Integer32,
        atmAtmSpvcVccStatsInCellCountLoB           Integer32,
        atmAtmSpvcVccStatsOutCellCountHiB          Integer32,
        atmAtmSpvcVccStatsOutCellCountLoB          Integer32,
        atmAtmSpvcVccStatsTimer                    Integer32,
        atmAtmSpvcVccStatsFrwdErrCrrctnRateA2B     INTEGER,
        atmAtmSpvcVccStatsFrwdErrCrrctnRateB2A     INTEGER,
        atmAtmSpvcVccStatsInOdometerCellCountHiA   Integer32,
        atmAtmSpvcVccStatsInOdometerCellCountLoA   Integer32,
        atmAtmSpvcVccStatsOutOdometerCellCountHiA  Integer32,
        atmAtmSpvcVccStatsOutOdometerCellCountLoA  Integer32,
        atmAtmSpvcVccStatsInOdometerCellCountHiB   Integer32,
        atmAtmSpvcVccStatsInOdometerCellCountLoB   Integer32,
        atmAtmSpvcVccStatsOutOdometerCellCountHiB  Integer32,
        atmAtmSpvcVccStatsOutOdometerCellCountLoB  Integer32,
        atmAtmSpvcVccStatsOdometerTimer            Integer32,
        atmAtmSpvcVccStatsOdometerReset            INTEGER,
        atmAtmSpvcVccViA                           INTEGER,
        atmAtmSpvcVccViB                           INTEGER,
        atmAtmSpvcVccStatsCellsDroppedA            Integer32,
        atmAtmSpvcVccStatsCellsTaggedA             Integer32,
        atmAtmSpvcVccStatsCellsDroppedB            Integer32,
        atmAtmSpvcVccStatsCellsTaggedB             Integer32

        ,atmAtmSpvcVccVUNIA                        INTEGER,
        atmAtmSpvcVccVUNIB                         INTEGER,

        atmAtmSpvcVccTrafficShapingA2B             INTEGER,
        atmAtmSpvcVccTrafficShapingB2A             INTEGER,
        atmAtmSpvcVccPriority                      INTEGER
        }

atmAtmSpvcVccVpiA    OBJECT-TYPE
        SYNTAX INTEGER (0..4095)
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION "VPI value for side A of an ATM SPVC VCC connection.
                 Valid range is a number between 0 and 4095."
        ::= { atmAtmSpvcVccEntry 1 }

atmAtmSpvcVccVciA    OBJECT-TYPE
        SYNTAX INTEGER (0..65535)
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION "VCI value for side A of an ATM SPVC VCC connection.
                 Valid range is a number between 0 and 65535."
        ::= { atmAtmSpvcVccEntry 2 }

atmAtmSpvcVccRemoteAtmPortAddr OBJECT-TYPE
        SYNTAX OCTET STRING (SIZE(1..20))
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION "Remote ATM port NSAP address, not meaningful if 
                 passive type of connection."
        ::= { atmAtmSpvcVccEntry 3 }

atmAtmSpvcVccIfB    OBJECT-TYPE
        SYNTAX IntfIndex
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION "Interface index for side B of an ATM SPVC VCC 
                 connection.  Number value is interpreted as an 
                 interface of the form, SSPPCCC, where SS is the 
                 card slot, PP is the port number, and CCC is the 
                 channel number."
        ::= { atmAtmSpvcVccEntry 4 }

atmAtmSpvcVccVpiB    OBJECT-TYPE
        SYNTAX INTEGER (0..4095)
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION "VPI value for side B of an ATM SPVC VCC connection.
                 Valid range is a number between 0 and 4095."
        ::= { atmAtmSpvcVccEntry 5 }

atmAtmSpvcVccVciB    OBJECT-TYPE
        SYNTAX INTEGER (0..65535)
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION "VCI value for side B of an ATM SPVC VCC connection.
                 Valid range is a number between 0 and 65535."
        ::= { atmAtmSpvcVccEntry 6 }

atmAtmSpvcVccServiceTypeA2B OBJECT-TYPE
        SYNTAX INTEGER  {
                ubr(1),
                vbr-nrt2(2),
                vbr-nrt1(3),
                vbr-rt2(4),
                vbr-rt1(5),
                vbr-express(6),
                cbr4(7),
                cbr3(8),
                cbr2(9)
                }
        MAX-ACCESS read-write
        STATUS current
        DESCRIPTION "Service type, ATM port."
        DEFVAL { ubr }
        ::= { atmAtmSpvcVccEntry 7 }

atmAtmSpvcVccSarTypeA2B OBJECT-TYPE
        SYNTAX INTEGER  {
                aal5(1),
                aal3-4(2),
                aal2(3),
                aal1(4),
                null(5)
                }
        MAX-ACCESS read-write
        STATUS current
        DESCRIPTION "SAR type used in ATM adaption, ATM port."
        DEFVAL { null }
        ::= { atmAtmSpvcVccEntry 8 }

