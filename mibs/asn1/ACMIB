stratumSyncSource OBJECT-TYPE
                SYNTAX INTEGER {
                                lineTiming(1),
                                compositeClock(2),
                                freerun(3),
                                t1BITS(4),
                                e1ETSI(5)
                               }
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "Clock source for the stratum card - 
                                lineTiming: must have slot and port
                                specified in stratumPrimaryLineId.
                                compositeClock: local clock must
                                be available. "
                DEFVAL { lineTiming }
                ::= { stratumConfig 1 }

stratumPrimaryLineId OBJECT-TYPE
                SYNTAX Integer32
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "Format SSPP (slot and port #'s) to be used
                                     when lineTiming is specified in 
                                stratumSyncSource."
                ::= { stratumConfig 2 }

stratumSecondaryLineId OBJECT-TYPE
                SYNTAX Integer32
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "Format SSPP (slot and port #'s) to be used if
                             the primary line has failed. Zeros imply that
                             the stratum should switch to holdover mode 
                             instead."
                ::= { stratumConfig 3 }

stratumLevel         OBJECT-TYPE
                SYNTAX INTEGER {
                                stratum3(1),
                                stratum4(2)
                               }
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "Enable or disable the switching over to stratum
                             4 when stratum 3 is not possible."
                DEFVAL { stratum3 }
                ::= { stratumConfig 4 }

stratumLineCommand OBJECT-TYPE
                SYNTAX INTEGER {
                                gotoPrimaryLine(1),
                                gotoSecondaryLine(2)
                               }
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "Forces the stratum card to use one of the two
                             specified lines as its clock source when the 
                             stratumSyncSource is lineTiming."
                ::= { stratumConfig 5 }

stratumLineTimingStatus OBJECT-TYPE
                SYNTAX INTEGER {
                                primaryLine(1),
                                secondaryLine(2),
                                none(3)
                               }
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "Represents the source of the current clock."
                ::= { stratumConfig 6 }

stratumMode        OBJECT-TYPE
                SYNTAX INTEGER {
                                synchronized3(1),
                                synchronized4(2),
                                holdover(3),
                                freerun(4),
                                cardRemoved(5)
                                }
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "Current status of the primary stratum card.
                synchronized3(1) stratum is synchronized with accuracy
                                 4.6 x 10e-6
                synchronized4(2) stratum is synchronized with accuracy
                                 3.2 x 10e-5
                holdover(3)      an operating condition of a clock that has
                                 lost its controlling input and is using
                                 stored data acquired while in normal
                                 operation, to control its output
                freerun(4)       an operating condition of the clock whose
                                 output signals are internally controlled
                cardRemoved(5)   stratum removed
                "
                ::= { stratumConfig 7 }

stratumBackupMode OBJECT-TYPE
                SYNTAX INTEGER {
                                synchronized3(1),
                                synchronized4(2),
                                holdover(3),
                                freerun(4),
                                cardRemoved(5)
                                }
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "Current status of the backup stratum card. "
                ::= { stratumConfig 8 }


stratumRefFailAction OBJECT-TYPE
                SYNTAX INTEGER {
                                holdover(1),
                                freerun(2)
                               }
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "Defines whether the stratum should switch to
                             freerun or holdover on reference clock failure."
                DEFVAL { holdover }
                ::= { stratumConfig 9 }

stratumPortStatus OBJECT-TYPE
                SYNTAX INTEGER {
                                unconfigured(1),
                                noalarm(2),
                                portAIS(3),
                                portLOF(4),
                                portLOS(5),
                                portFOS(6)
                                }
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "Current status of the primary stratum card.
                             portFOS indicates port frequency offset. "
                ::= { stratumConfig 10 }

stratumBkupPortStatus OBJECT-TYPE
                SYNTAX INTEGER {
                                unconfigured(1),
                                noalarm(2),
                                portAIS(3),
                                portLOF(4),
                                portLOS(5),
                                portFOS(6)
                                }
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "Current status of the backup stratum card.
                             portFOS indicates port frequency offset. "
                ::= { stratumConfig 11 }

