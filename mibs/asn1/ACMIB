                atmTrunkingBrVccViB                          INTEGER,
                atmTrunkingBrVccVUNIB                        INTEGER,
                atmTrunkingBrVccServiceType                  INTEGER,
                atmTrunkingBrVccSarType                      INTEGER,
                atmTrunkingBrVccOwner                        INTEGER,
                atmTrunkingBrVccOperStatus                   INTEGER,
                atmTrunkingBrVccFwAtmPcr01                   Integer32,
                atmTrunkingBrVccBwAtmPcr01                   Integer32,
