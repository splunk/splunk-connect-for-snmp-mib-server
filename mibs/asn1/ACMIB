                SYNTAX INTEGER{
                                activeSvc(1),
                                passiveSvc(2)
                                }
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "The type of connection: activeSvc - attempt calls,
                                whenever not established;
                             passiveSvc - accepts calls"
                ::= { cirEmAtmSpvcVccEntry 17 }

cirEmAtmSpvcVccCePortAddr OBJECT-TYPE
                SYNTAX OCTET STRING (SIZE(1..20))
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "local CES port nsap address"
                ::= { cirEmAtmSpvcVccEntry 18 }

cirEmAtmSpvcVccOperStatus OBJECT-TYPE
                SYNTAX INTEGER {
                                other(1),
                                establishmentInProgress(2),
                                connected(3),
                                retriesExhausted(4),
                                noAddressSupplied(5),
                                lowerLayerDown(6)
                                }
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "other - none of the types specified below;
                             establishmentInProgress - connection is not 
                                operational, but call attempts are ongoing;
                             connected - connection is currently operational;
                             retriesExhausted - retry limit has been reached 
                                and call attempts have ceased;
                             noAddressSupplied - no remote address has been 
                                configured, so no call attempts are initiated;
                             lowerLayerDown - SPVC handler is not operational."
                ::= { cirEmAtmSpvcVccEntry 19 }

cirEmAtmSpvcVccRowStatus  OBJECT-TYPE
                SYNTAX     RowStatus
                MAX-ACCESS read-write
                STATUS     current
                DESCRIPTION "Used to create and delete a soft PVCC.  When this 
                             object is set to 'active' an attempt is made to 
                             set up the soft PVCC.  When this object has the 
                             value 'active' and is set to another value, any 
                             set-up or connection in-progess is released."
                ::= { cirEmAtmSpvcVccEntry 20 }

cirEmAtmSpvcVccViB      OBJECT-TYPE
                SYNTAX INTEGER (0..1024)
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "VI to associate the Egress part of 
                                the B2A connection ."
                DEFVAL { 0 } 
                ::= { cirEmAtmSpvcVccEntry 21 }

cirEmAtmSpvcVccActiveSvcTable OBJECT-TYPE
                SYNTAX SEQUENCE OF CirEmAtmSpvcVccActiveSvcEntry
                MAX-ACCESS  not-accessible
                STATUS current
                DESCRIPTION "The table used to manage active SVCs established 
                             across ATM networks between CES entities."
                ::= { connectionConfig 20 }

cirEmAtmSpvcVccActiveSvcEntry OBJECT-TYPE
                SYNTAX CirEmAtmSpvcVccActiveSvcEntry
                MAX-ACCESS  not-accessible
                STATUS current
                DESCRIPTION "An entry in the CES active SVC table. There is 
                             one entry in the table corresponding to each 
                              active row in the cirEmAtmSpvcVccRowStatus for 
                             which the cirEmAtmSpvcVccConnType takes the value
                             'activeSvc'."
                INDEX { cirEmAtmSpvcVccIfA }
                ::= { cirEmAtmSpvcVccActiveSvcTable 1 }

CirEmAtmSpvcVccActiveSvcEntry ::= SEQUENCE {
                              cirEmAtmSpvcVccRemoteCePortAddr    OCTET STRING,
                              cirEmAtmSpvcVccRetryInterval       INTEGER,
                              cirEmAtmSpvcVccRetryLimit          INTEGER,
                              cirEmAtmSpvcVccRestart             INTEGER,
                              cirEmAtmSpvcVccRetryFailure        Integer32,
                              cirEmAtmSpvcVccReleaseCause        INTEGER,
                              cirEmAtmSpvcVccReleaseDiag         OCTET STRING
                              }

cirEmAtmSpvcVccRemoteCePortAddr        OBJECT-TYPE
                SYNTAX OCTET STRING (SIZE(1..20))
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "The ATM address supporting the corresponding far 
                             end CES IWF process.  If no address is supplied, 
                             no attempts to establish the active SVC are 
                             initiated."
                ::= { cirEmAtmSpvcVccActiveSvcEntry 1 }

cirEmAtmSpvcVccRetryInterval OBJECT-TYPE
                SYNTAX INTEGER (0..3600)
                UNITS      "seconds"
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "The time to wait before attempting to establish 
                             the SVC after previous failed call attempt. 
                             0 indicates retry will be attempted immediately."
                DEFVAL { 10 }
                ::= { cirEmAtmSpvcVccActiveSvcEntry 2 }

cirEmAtmSpvcVccRetryLimit OBJECT-TYPE
                SYNTAX INTEGER (0..65535)
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "A maximum limit on how many retries. 
                             0 indicates no limit."
                DEFVAL { 0 }
                ::= { cirEmAtmSpvcVccActiveSvcEntry 3 }

cirEmAtmSpvcVccRestart OBJECT-TYPE
                SYNTAX INTEGER {
                                restart(1),
                                noop(2)
                                }
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION "restart, the active SVC is released if necessary 
                             and a new setup procedure is begun; or noop"
                ::= { cirEmAtmSpvcVccActiveSvcEntry 4 }

cirEmAtmSpvcVccRetryFailure OBJECT-TYPE
                SYNTAX Integer32 
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "Indicates how many attempts to establish the 
                             connection have failed.  It is reset whenever a 
                             connection is successfully established or the SVC 
                             is restarted."
                ::= { cirEmAtmSpvcVccActiveSvcEntry 5 }

cirEmAtmSpvcVccReleaseCause OBJECT-TYPE
                SYNTAX INTEGER(1..127) 
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "CAUSE field value of the last RELEASE message 
                             received for this SVC."
                ::= { cirEmAtmSpvcVccActiveSvcEntry 6 }

cirEmAtmSpvcVccReleaseDiag OBJECT-TYPE
                SYNTAX OCTET STRING (SIZE(0..8)) 
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION "DIAGNOSTICS field value of the last RELEASE 
                             message received for this SVC."
                ::= { cirEmAtmSpvcVccActiveSvcEntry 7 }


vbrAtmSpvcVccTable OBJECT-TYPE
                SYNTAX SEQUENCE OF VbrAtmSpvcVccEntry
