                cirEmAtmSpvcVccStatsOutOdometerCellCountLoB         Integer32,
                cirEmAtmSpvcVccStatsOdometerTimer                Integer32,
                cirEmAtmSpvcVccStatsOdometerReset                INTEGER,
                cirEmAtmSpvcVccStatsCellsDroppedB                 Integer32,
                cirEmAtmSpvcVccStatsCellsTaggedB                  Integer32
                }

