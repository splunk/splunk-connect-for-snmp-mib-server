	 Valid [i]nterface = 1-2"
    ::= {uas7022FarEndUnavailableTimeRegEntry 4}

--
-- ********************************************************************
-- Alarm Object Identifier Definitions
-- ********************************************************************

uas7022NoResponseAlm	OBJECT IDENTIFIER ::= { uas7022Alarms 1  }
uas7022DiagRxErrAlm	OBJECT IDENTIFIER ::= { uas7022Alarms 2  }
uas7022PowerUpAlm	OBJECT IDENTIFIER ::= { uas7022Alarms 3  }
uas7022TimingLoss       OBJECT IDENTIFIER ::= { uas7022Alarms 4  }
uas7022LCV		OBJECT IDENTIFIER ::= { uas7022Alarms 5  }
uas7022LossOfSignal     OBJECT IDENTIFIER ::= { uas7022Alarms 6  }
uas7022LossOfFrame      OBJECT IDENTIFIER ::= { uas7022Alarms 7  }
uas7022AlarmIndSignal	OBJECT IDENTIFIER ::= { uas7022Alarms 8  }
uas7022NEES		OBJECT IDENTIFIER ::= { uas7022Alarms 9  }
uas7022NEBBE		OBJECT IDENTIFIER ::= { uas7022Alarms 10 }
uas7022NESES		OBJECT IDENTIFIER ::= { uas7022Alarms 11 }
uas7022NEUAS		OBJECT IDENTIFIER ::= { uas7022Alarms 12 }
uas7022FEES		OBJECT IDENTIFIER ::= { uas7022Alarms 13 }
uas7022FEBBE		OBJECT IDENTIFIER ::= { uas7022Alarms 14 }
uas7022FESES		OBJECT IDENTIFIER ::= { uas7022Alarms 15 }
uas7022FEUAS		OBJECT IDENTIFIER ::= { uas7022Alarms 16 }
uas7022RAI		OBJECT IDENTIFIER ::= { uas7022Alarms 17 }

-- ALARM                     Maskable      Valid interfaces         window/thres
-- ====================================================================================
-- uas7022NoResponseAlm		no		unit			no
-- uas7022DiagRxErrAlm		no		unit			no
-- uas7022PowerUpAlm		no		unit			no
-- uas7022TimingLoss		yes		NIUA/NIUB		no
-- uas7022LCV			yes		NIUA/NIUB		yes
-- uas7022LossofSignal		yes		NIUA/NIUB		no
-- uas7022LossOfFrame		yes		NIUA/NIUB		no
-- uas7022Ais			yes		NIUA/NIUB		no
-- uas7022NEES			yes		NIUA/NIUB		yes
-- uas7022NEBBE			yes		NIUA/NIUB		yes
-- uas7022NESES			yes		NIUA/NIUB		yes
-- uas7022NEUAS			yes		NIUA/NIUB		yes
-- uas7022FEES			yes		NIUA/NIUB		yes
-- uas7022FEBBE			yes		NIUA/NIUB		yes
-- uas7022FESES			yes		NIUA/NIUB		yes
-- uas7022FEUAS			yes		NIUA/NIUB		yes
-- uas7022RAI			yes		NIUA/NIUB		no
-- 

--
-- ********************************************************************
-- GDC UAS7022 Alarm Configuration
-- ********************************************************************

-- ********************************************************************
-- Near End Alarm Configuration
-- ********************************************************************
uas7022NearEndAlarmConfigTable OBJECT-TYPE
    SYNTAX SEQUENCE OF Uas7022NearEndAlarmConfigEntry
    ACCESS not-accessible
    STATUS mandatory
    DESCRIPTION
	"The uas7022NearEndAlarmConfigTable contains entries that configure 
	 alarm  reporting.  The structure of the table is such that alarm
	 configuration is supported on a unit and interface basis, and
	 then on an alarm type basis within the interface."
    ::= { uas7022AlarmConfig 1 }

uas7022NearEndAlarmConfigEntry OBJECT-TYPE
    SYNTAX Uas7022NearEndAlarmConfigEntry
    ACCESS not-accessible
    STATUS mandatory
    DESCRIPTION
	"An entry in the Near End Alarm Configuration table."
    INDEX { uas7022NearEndAlarmConfigIndex, uas7022NearEndAlarmConfigIdentifier }
    ::= { uas7022NearEndAlarmConfigTable 1 }

Uas7022NearEndAlarmConfigEntry ::=
    SEQUENCE {
	uas7022NearEndAlarmConfigIndex      SCinstance,
	uas7022NearEndAlarmConfigIdentifier OBJECT IDENTIFIER,
	uas7022NearEndAlarmCountWindow      INTEGER,
	uas7022NearEndAlarmCountThreshold   INTEGER
    }

uas7022NearEndAlarmConfigIndex OBJECT-TYPE
    SYNTAX SCinstance
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
        "Integer value which uniquely identifies the UAS7022 to which this
         entry is applicable. SCinstance is defined to be SLDi where:
         S (byte value) - physical shelf slot location (01-32)
         L (byte value) - line number (01)
         D (byte value) - drop number (00)
         i (byte value) - interface number (01-02)"
    ::= { uas7022NearEndAlarmConfigEntry 1 }

uas7022NearEndAlarmConfigIdentifier OBJECT-TYPE
    SYNTAX OBJECT IDENTIFIER
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
    "format: iso.org.dod.internet.private.enterprises.gdc.
             dsx1.uas7022.uas7022Alarms.ALM
     example:  1.3.6.1.4.1.498.6.13.6.ALM
     where ALM = 1 for uas7022NoResponse alarm, 2 for uas7022DiagRxErr alarm,
                 etc., as specified in the Alarm Definitions above."
    ::= { uas7022NearEndAlarmConfigEntry 2 }

uas7022NearEndAlarmCountWindow OBJECT-TYPE
    SYNTAX INTEGER{
		disabled(1),
		last1sec(2),
		last10sec(3),
		last30sec(4),
		last1min(5),
		last15min(6),
		last1hr(7),
		last24hr(8),
		infinite(9)
	   }
    ACCESS  read-write
    STATUS  mandatory
    DESCRIPTION
	"This variable sets/reads the alarm window.
 	 This window is used with the alarm threshold to determine
	 how long the alarm should be active before reporting.
         Default: disabled(1)"
    ::= { uas7022NearEndAlarmConfigEntry 3 }

uas7022NearEndAlarmCountThreshold OBJECT-TYPE
    SYNTAX INTEGER {
		thresGT1(1),
		thresGT3(2),
		thresGT10(3),
		thresGT100(4),
		thresGT1000(5),
		thresGT10000(6)
	   }
    ACCESS  read-write
    STATUS  mandatory
    DESCRIPTION
	"This function sets/reads the alarm threshold criteria.
 	 This threshold is used along with the the alarm window to 
	 determine the number of instances in a given time frame for
	 an alarm to occur before the alarm is considered active.
	 Default: thresGT1(1)"
    ::= { uas7022NearEndAlarmConfigEntry 4 }

-- ********************************************************************
-- Far End Alarm Configuration
-- ********************************************************************

uas7022FarEndAlarmConfigTable OBJECT-TYPE
    SYNTAX SEQUENCE OF Uas7022FarEndAlarmConfigEntry
    ACCESS not-accessible
    STATUS mandatory
    DESCRIPTION
	"The uas7022FarEndAlarmConfigTable contains entries that configure alarm
	 reporting.  The structure of the table is such that alarm
	 configuration is supported on a unit and interface basis, and
	 then on an alarm type basis within the interface."
    ::= { uas7022AlarmConfig 2 }

uas7022FarEndAlarmConfigEntry OBJECT-TYPE
    SYNTAX Uas7022FarEndAlarmConfigEntry
    ACCESS not-accessible
    STATUS mandatory
    DESCRIPTION
	 "An entry in the Far End Alarm Configuration table."
    INDEX { uas7022FarEndAlarmConfigIndex, uas7022FarEndAlarmConfigIdentifier }
    ::= { uas7022FarEndAlarmConfigTable 1 }

Uas7022FarEndAlarmConfigEntry ::=
    SEQUENCE {
	uas7022FarEndAlarmConfigIndex      SCinstance,
	uas7022FarEndAlarmConfigIdentifier OBJECT IDENTIFIER,
	uas7022FarEndAlarmCountWindow      INTEGER,
	uas7022FarEndAlarmCountThreshold   INTEGER
    }

uas7022FarEndAlarmConfigIndex OBJECT-TYPE
    SYNTAX SCinstance
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
        "Integer value which uniquely identifies the UAS7022 to which this
         entry is applicable. SCinstance is defined to be SLDi where:
         S (byte value) - physical shelf slot location (01-32)
         L (byte value) - line number (01)
         D (byte value) - drop number (00)
         i (byte value) - interface number (01-02)"
    ::= { uas7022FarEndAlarmConfigEntry 1 }

uas7022FarEndAlarmConfigIdentifier OBJECT-TYPE
    SYNTAX OBJECT IDENTIFIER
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
    "format: iso.org.dod.internet.private.enterprises.gdc.
             dsx1.uas7022.uas7022Alarms.ALM
     example:  1.3.6.1.4.1.498.6.13.6.ALM
     where ALM = 1 for uas7022NoResponse alarm, 2 for uas7022DiagRxErr alarm,
                 etc., as specified in the Alarm Definitions above."

    ::= { uas7022FarEndAlarmConfigEntry 2 }

uas7022FarEndAlarmCountWindow OBJECT-TYPE
    SYNTAX INTEGER {
		disabled(1),
		last1sec(2),
		last10sec(3),
		last30sec(4),
		last1min(5),
		last15min(6),
		last1hr(7),
		last24hr(8),
		infinite(9)
	   }
    ACCESS  read-write
    STATUS  mandatory
    DESCRIPTION
	"This variable sets/reads the alarm window.
	 This window is used with the alarm threshold to determine
	 how long the alarm should be active before reporting.
	 Default: disabled(1)"
    ::= { uas7022FarEndAlarmConfigEntry 3 }

uas7022FarEndAlarmCountThreshold OBJECT-TYPE
    SYNTAX INTEGER {
		thresGT1(1),
		thresGT3(2),
		thresGT10(3),
		thresGT100(4),
		thresGT1000(5),
		thresGT10000(6)
	   }
     ACCESS  read-write
     STATUS  mandatory
     DESCRIPTION
	 "This function sets/reads the alarm threshold criteria.
	  This threshold is used along with the the alarm window to 
	  determine the number of instances in a given time frame for
	  an alarm to occur before the alarm is considered active.
	  Default: thresGT1(1)"
     ::= { uas7022FarEndAlarmConfigEntry 4 }

--
-- ********************************************************************
-- GDC Local UAS7022 Alarm Configuration
-- ********************************************************************
-- Local Alarms are alarms that optionally activate the backplane
-- Major or Minor buses. These alarms changes are never reported in an
-- unsolicited message to the proxy agent hence, the reason for the alarm
-- card. These alarms are different from the previously defined alarms
-- in that the mask state has three possible values.

uas7022LocalAlarmConfigTable OBJECT-TYPE
    SYNTAX SEQUENCE OF Uas7022LocalAlarmConfigEntry
    ACCESS not-accessible
    STATUS mandatory
    DESCRIPTION
	"The uas7022LocalAlarmConfigTable contains entries 
	 that configure alarm reporting to the alarm card."
     ::= { uas7022AlarmConfig 3 }

uas7022LocalAlarmConfigEntry OBJECT-TYPE
    SYNTAX Uas7022LocalAlarmConfigEntry
    ACCESS not-accessible
    STATUS mandatory
    DESCRIPTION
	"An entry in the Local Alarm Configuration table."
    INDEX { uas7022LocalAlarmConfigIndex }
    ::= { uas7022LocalAlarmConfigTable 1 }

Uas7022LocalAlarmConfigEntry ::=
    SEQUENCE {
	uas7022LocalAlarmConfigIndex SCinstance,
	uas7022UASNE                 INTEGER,
	uas7022SESNE                 INTEGER,
	uas7022BBENE                 INTEGER,
	uas7022ESNE                  INTEGER,
	uas7022UASFE                 INTEGER,
	uas7022SESFE                 INTEGER,
	uas7022BBEFE                 INTEGER,
	uas7022ESFE                  INTEGER,
	uas7022LOS                   INTEGER,
	uas7022LOF                   INTEGER,
	uas7022AIS                   INTEGER,
	uas7022TmgLoss               INTEGER,
	uas7022LCVs                  INTEGER,
	uas7022NtwkRAI               INTEGER
    }

uas7022LocalAlarmConfigIndex OBJECT-TYPE
    SYNTAX SCinstance
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
        "Integer value which uniquely identifies the UAS7022 to which this
         entry is applicable. SCinstance is defined to be SLDi where:
         S (byte value) - physical shelf slot location (01-32)
         L (byte value) - line number (01)
         D (byte value) - drop number (00)
         i (byte value) - interface number (01-02)"
    ::= { uas7022LocalAlarmConfigEntry 1 }

uas7022UASNE OBJECT-TYPE
    SYNTAX INTEGER {
		disabled(1),
		enabledMajor(2),
		enabledMinor(3)
		}
    ACCESS  read-write
    STATUS  mandatory
    DESCRIPTION
	"Disables or enables the alarm on the Major buss or Minor
	 buss.
	 Valid [i]nterface = 1-2"
    ::= { uas7022LocalAlarmConfigEntry 2 }

