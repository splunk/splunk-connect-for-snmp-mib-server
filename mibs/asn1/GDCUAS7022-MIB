    SYNTAX  SEQUENCE OF Uas7022NearEndUnavailableTimeRegEntry
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
	"The uas7022 Near End Unavailable Time Register table."
    ::= { uas7022Performance 9 }

uas7022NearEndUnavailableTimeRegEntry OBJECT-TYPE
    SYNTAX  Uas7022NearEndUnavailableTimeRegEntry
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
	"An entry in the Near End Unavailable Time Register table."
    INDEX   { uas7022NearEndUnavailableTimeRegIndex, uas7022NearEndUnavailableTimeRegNumber }
    ::= { uas7022NearEndUnavailableTimeRegTable 1 }

Uas7022NearEndUnavailableTimeRegEntry ::=
    SEQUENCE {
	uas7022NearEndUnavailableTimeRegIndex  SCinstance,
	uas7022NearEndUnavailableTimeRegNumber INTEGER,
	uas7022NearEndUnavailableTimeRegStart  INTEGER,
	uas7022NearEndUnavailableTimeRegStop   INTEGER
    }

uas7022NearEndUnavailableTimeRegIndex OBJECT-TYPE
    SYNTAX  SCinstance
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
        "Integer value which uniquely identifies the UAS7022 to which this
         entry is applicable. SCinstance is defined to be SLDi where:
         S (byte value) - physical shelf slot location (01-32)
         L (byte value) - line number (01)
         D (byte value) - drop number (00)
         i (byte value) - interface number (01-02)"
    ::= { uas7022NearEndUnavailableTimeRegEntry 1 }

uas7022NearEndUnavailableTimeRegNumber OBJECT-TYPE
    SYNTAX  INTEGER (1..6)
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
	"A number between 1 and 6, where the number is the number of the 
	 NearEndUnavailable Time Register.
	 Valid [i]nterface = 1-2"
    ::= { uas7022NearEndUnavailableTimeRegEntry 2 }

uas7022NearEndUnavailableTimeRegStart OBJECT-TYPE
    SYNTAX  INTEGER (1..2147483647)
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
	"Start time of one of the NearEndUnavailable Time Registers.
	 Valid [i]nterface = 1-2"
    ::= {uas7022NearEndUnavailableTimeRegEntry 3}

uas7022NearEndUnavailableTimeRegStop OBJECT-TYPE
    SYNTAX  INTEGER (1..2147483647)
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
	"Stop time of one of the NearEndUnavailable Time Registers.
	 Valid [i]nterface = 1-2"
    ::= {uas7022NearEndUnavailableTimeRegEntry 4}

