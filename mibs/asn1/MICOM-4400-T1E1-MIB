--  "Parallel" Configuration Parameter ("nvm") Groups/Tables


--  the T1/E1 Line Configuration Table 
--  (Configuration Parameters)

--  This table provides the T1/E1 line configuration parameters
--  for identifying a T1/E1 line with its corresponding profile. (Configuration)

nvm4400t1e1LineCfgTable OBJECT-TYPE
	SYNTAX  SEQUENCE OF Nvm4400t1e1LineCfgEntry
	ACCESS  not-accessible
	STATUS  mandatory
	DESCRIPTION
	  "NAME = ; 
	   DESC = The T1/E1 Line Configuration table. \
		  (Configuration);
	   HELP = ;
	   CAPABILITIES = NET_CFG, VPN_CFG;"
	::= { t1e1-r2-configuration 6 }

nvm4400t1e1LineCfgEntry OBJECT-TYPE
	SYNTAX  Nvm4400t1e1LineCfgEntry
	ACCESS  not-accessible
	STATUS  mandatory
	DESCRIPTION
	  "NAME = ; 
	   DESC = An entry in the T1/E1 Line Configuration \
		  table. (Configuration);
	   HELP = ;
	   CAPABILITIES = ;"
	INDEX   { nvm4400t1e1LineCfgLineID }
	::= { nvm4400t1e1LineCfgTable 1 }

Nvm4400t1e1LineCfgEntry ::= SEQUENCE {
	nvm4400t1e1LineCfgLineID		INTEGER,
	nvm4400t1e1LineCfgProfileNum		INTEGER
	}

nvm4400t1e1LineCfgLineID OBJECT-TYPE
	SYNTAX	INTEGER {
		local (1),
		network (2)
		}
	ACCESS  read-only
	STATUS  mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = The T1/E1 line identification specifies which line \
		  is to be configured.  Used as the index to the \
		  T1/E1 Line Configuration table. (Configuration);
	   HELP = ;
	   CAPABILITIES = ;"
	::= { nvm4400t1e1LineCfgEntry 1 }

nvm4400t1e1LineCfgProfileNum OBJECT-TYPE
	SYNTAX	INTEGER (1..2)
	ACCESS  read-only
	STATUS  mandatory
	DESCRIPTION
	  "NAME = ; 
	   DESC = The value of this object indicates the profile \
		  to be used to configure the specified T1/E1 \
		  line. (Configuration);
	   HELP = 1: expected as default profile for local line. \
		  2: expected as default profile for network line.;
	   CAPABILITIES = ;"
	::= { nvm4400t1e1LineCfgEntry 2 }


--  the T1/E1 Profile Configuration Table 
--  (Configuration Parameters)

--  This table provides the T1/E1 profile configuration parameters.
--  This table is used to view a profile of T1/E1 line configuration
--  characteristics; of which, a subset may be used for a particular T1/E1
--  line (local or network) to define its configuration. (Configuration)

nvm4400t1e1ProfCfgTable OBJECT-TYPE
	SYNTAX  SEQUENCE OF Nvm4400t1e1ProfCfgEntry
	ACCESS  not-accessible
	STATUS  mandatory
	DESCRIPTION
	  "NAME = ; 
	   DESC = The T1/E1 Profile Configuration table. \
		  (Configuration);
	   HELP = ;
	   CAPABILITIES = NET_CFG, VPN_CFG;"
	::= { t1e1-r2-configuration 7 }

nvm4400t1e1ProfCfgEntry OBJECT-TYPE
	SYNTAX  Nvm4400t1e1ProfCfgEntry
	ACCESS  not-accessible
	STATUS  mandatory
	DESCRIPTION
	  "NAME = ; 
	   DESC = An entry in the T1/E1 Profile Configuration \
		  table. (Configuration);
	   HELP = ;
	   CAPABILITIES = ;"
	INDEX   { nvm4400t1e1ProfCfgProfileNum }
	::= { nvm4400t1e1ProfCfgTable 1 }

Nvm4400t1e1ProfCfgEntry ::= SEQUENCE {
	nvm4400t1e1ProfCfgProfileNum		INTEGER,
	nvm4400t1e1ProfCfgT1FrameFmt		INTEGER,
	nvm4400t1e1ProfCfgT1LineCode		INTEGER,
	nvm4400t1e1ProfCfgE1LineCode		INTEGER,
	nvm4400t1e1ProfCfgIdleCode		INTEGER,
	nvm4400t1e1ProfCfgT1LineBuildOut	INTEGER,
	nvm4400t1e1ProfCfgT1Loopback		INTEGER,
	nvm4400t1e1ProfCfgE1CRC4		INTEGER,
	nvm4400t1e1ProfCfgE1TS16Conn		INTEGER
	}

nvm4400t1e1ProfCfgProfileNum OBJECT-TYPE
	SYNTAX	INTEGER (1..2)
	ACCESS  read-only
	STATUS  mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = The value of this object indicates the profile \
		  number.  Used as an index to the T1/E1 \
		  Profile Configuration table. (Configuration);
	   HELP = ;
	   CAPABILITIES = ;"
	::= { nvm4400t1e1ProfCfgEntry 1 }

nvm4400t1e1ProfCfgT1FrameFmt OBJECT-TYPE
	SYNTAX	INTEGER {
		superFrame (1),
		extendedSuperFrame (2)
		}
	ACCESS  read-only
	STATUS  mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = The value of this object specifies the T1 \
		  frame format used in the line. (Configuration);
	   HELP = ;
	   CAPABILITIES = ;"
	DEFVAL { superFrame }
	::= { nvm4400t1e1ProfCfgEntry 2 }

nvm4400t1e1ProfCfgT1LineCode OBJECT-TYPE
	SYNTAX	INTEGER {
		aMI (1),
		b8ZS (2)
		}
	ACCESS  read-only
	STATUS  mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = The value of this object specifies the T1 \
		  coding format used in the line. (Configuration);
	   HELP = ;
	   CAPABILITIES = ;"
	DEFVAL { aMI }
	::= { nvm4400t1e1ProfCfgEntry 3 }

nvm4400t1e1ProfCfgE1LineCode OBJECT-TYPE
	SYNTAX	INTEGER {
		aMI (1),
		hDB3 (2)
		}
	ACCESS  read-only
	STATUS  mandatory
	DESCRIPTION
	  "NAME = ; 
	   DESC = The value of this object specifies the E1 \
		  coding format used in the line. (Configuration);
	   HELP = ;
	   CAPABILITIES = ;"
	DEFVAL { aMI }
	::= { nvm4400t1e1ProfCfgEntry 4 }

nvm4400t1e1ProfCfgIdleCode OBJECT-TYPE
	SYNTAX	INTEGER (0..'ff'h)
	ACCESS  read-only
	STATUS  mandatory
	DESCRIPTION
	  "NAME = ; 
	   DESC = The value of this object specifies the code \
		  to be filled in the line when idle. (Configuration);
	   HELP = ;
	   CAPABILITIES = ;"
	DEFVAL {'7f'h }
	::= { nvm4400t1e1ProfCfgEntry 5 }

nvm4400t1e1ProfCfgT1LineBuildOut OBJECT-TYPE
	SYNTAX	INTEGER {
		lessThan110Feet (1),
		lessThan220Feet (2),
		lessThan330Feet (3),
		lessThan440Feet (4),
		externalLBO (5),
		greaterThan665Feet (6),
		lessThan550Feet (7),
		lessThan660Feet (8)
		}
	ACCESS  read-only
	STATUS  mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = The value of this object specifies the cable \
		  length attached to a local T1 line. (Configuration);
	   HELP = ;
	   CAPABILITIES = ;"
	DEFVAL { lessThan110Feet }
	::= { nvm4400t1e1ProfCfgEntry 6 }

nvm4400t1e1ProfCfgT1Loopback OBJECT-TYPE
	SYNTAX	INTEGER {
		disable (1),
		enable (2)
		}
	ACCESS  read-only
	STATUS  mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = The value of this object specifies whether \
		  to enable or disable the T1 line loopback \
		  to be invoked. (Configuration);
	   HELP = 1: expected as default for a local T1 line profile. \
		  2: expected as default for a network T1 line profile.;
	   CAPABILITIES = ;"
	::= { nvm4400t1e1ProfCfgEntry 7 }

nvm4400t1e1ProfCfgE1CRC4 OBJECT-TYPE
	SYNTAX	INTEGER {
		disable (1),
		enable (2)
		}
	ACCESS  read-only
	STATUS  mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = The value of this object specifies whether \
		  to enable or disable a T1 line CRC4. (Configuration);
	   HELP = ;
	   CAPABILITIES = ;"
	DEFVAL { disable }
	::= { nvm4400t1e1ProfCfgEntry 8 }

nvm4400t1e1ProfCfgE1TS16Conn OBJECT-TYPE
	SYNTAX	INTEGER {
