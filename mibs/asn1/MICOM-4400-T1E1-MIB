-- *** STATUS groups/tables ***

--  Operational Parameter ("mcm") Groups/Tables (note: no parallel "nvm" objects
--  are defined for STATUS parameters.)


--  the T1/E1 Line Status Display Table 

--  This table provides the T1/E1 line status display parameters.

mcm4400t1e1LineStatusTable OBJECT-TYPE
	SYNTAX  SEQUENCE OF Mcm4400t1e1LineStatusEntry
	ACCESS  not-accessible
	STATUS  mandatory
	DESCRIPTION
	  "NAME = ; 
	   DESC = The T1/E1 Line Status Display table. \
	   HELP = ;
	   CAPABILITIES = NET_CFG, VPN_CFG;"
	::= { t1e1-r2-status 1 }

mcm4400t1e1LineStatusEntry OBJECT-TYPE
	SYNTAX  Mcm4400t1e1LineStatusEntry
	ACCESS  not-accessible
	STATUS  mandatory
	DESCRIPTION
	  "NAME = ; 
	   DESC = An entry in the T1/E1 Line Status Display \
		  table.;
	   HELP = ;
	   CAPABILITIES = ;"
	INDEX   { mcm4400t1e1LineStatusLineIndex }
	::= { mcm4400t1e1LineStatusTable 1 }

Mcm4400t1e1LineStatusEntry ::= SEQUENCE {
	mcm4400t1e1LineStatusLineIndex		INTEGER,
	mcm4400t1e1LineStatusInterface		INTEGER,
	mcm4400t1e1LineStatusImpedance		INTEGER,
	mcm4400t1e1LineStatusTestMode		INTEGER,
	mcm4400t1e1LineStatusTestingDS0Ch	INTEGER,
	mcm4400t1e1LineStatusAISAlarm		INTEGER,
	mcm4400t1e1LineStatusRedAlarm		INTEGER,
	mcm4400t1e1LineStatusYellowAlarm	INTEGER,
	mcm4400t1e1LineStatusTS16AIS		INTEGER,
	mcm4400t1e1LineStatusOOSMFAlarm		INTEGER,
	mcm4400t1e1LineStatusMFASAlarm		INTEGER,
	mcm4400t1e1LineStatusFASAlarm		INTEGER,
	mcm4400t1e1LineStatusOOCMFAlarm		INTEGER
	}

mcm4400t1e1LineStatusLineIndex OBJECT-TYPE
	SYNTAX	INTEGER {
		localLine (1),
		networkLine (2)
		}
	ACCESS  read-only
	STATUS  mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = The T1/E1 line index specifies which line is to \
		  be displayed.  Used as an index to the \
		  T1/E1 Line Status Display table.;
	   HELP = ;
	   CAPABILITIES = ;"
	::= { mcm4400t1e1LineStatusEntry 1 }

mcm4400t1e1LineStatusInterface OBJECT-TYPE
	SYNTAX	INTEGER {
		t1 (1),
		e1 (2)
		}
	ACCESS  read-only
	STATUS  mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = The value of this object indicates the interface \
		  type as either T1 or E1 for the \
		  T1/E1 Line Status Display table.;
	   HELP = ;
	   CAPABILITIES = ;"
	::= { mcm4400t1e1LineStatusEntry 2 }

mcm4400t1e1LineStatusImpedance OBJECT-TYPE
	SYNTAX	INTEGER {
		line120ohm (1),
		line75ohm (2) 
		}
	ACCESS  read-only
	STATUS  mandatory
	DESCRIPTION
	  "NAME = ; 
	   DESC = The value of this object indicates the impedance \
		  for the specified line.;
	   HELP = ;
	   CAPABILITIES = ;"
	::= { mcm4400t1e1LineStatusEntry 3 }

mcm4400t1e1LineStatusTestMode OBJECT-TYPE
	SYNTAX	INTEGER {
		none (1),
		inLineLoopback (2),
		inChannelLoopback (3),
		inChannelPatternTest (4) 
		}
	ACCESS  read-only
	STATUS  mandatory
	DESCRIPTION
	  "NAME = ; 
