	  "NAME = ; 
	   DESC = An entry in the T1/E1 DS0 Channel \
		  Connection Configuration table. (Configuration);
	   HELP = ;
	   CAPABILITIES = ;"
	INDEX   { nvm4400t1e1DS0ChConnLineID,
nvm4400t1e1DS0ChConnDS0Index }
	::= { nvm4400t1e1DS0ChConnTable 1 }

Nvm4400t1e1DS0ChConnEntry ::= SEQUENCE {
	nvm4400t1e1DS0ChConnLineID		INTEGER,
	nvm4400t1e1DS0ChConnDS0Index		INTEGER,
	nvm4400t1e1DS0ChConnState		INTEGER
	}

