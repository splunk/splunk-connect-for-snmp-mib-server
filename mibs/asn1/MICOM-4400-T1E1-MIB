mcm4400t1e1DS0ChTestCmdTable OBJECT-TYPE
	SYNTAX  SEQUENCE OF Mcm4400t1e1DS0ChTestCmdEntry
	ACCESS  not-accessible
	STATUS  mandatory
	DESCRIPTION
	  "NAME = ; 
	   DESC = The T1/E1 DS0 Channel Test Command \
		  table. \
	   HELP = ;
	   CAPABILITIES = NET_OPER, VPN_OPER ;"
	::= { t1e1-r2-control 1 }

mcm4400t1e1DS0ChTestCmdEntry OBJECT-TYPE
	SYNTAX  Mcm4400t1e1DS0ChTestCmdEntry
	ACCESS  not-accessible
	STATUS  mandatory
	DESCRIPTION
	  "NAME = ; 
	   DESC = An entry in the T1/E1 DS0 Channel \
		  Test Command table.;
	   HELP = ;
	   CAPABILITIES = ;"
	INDEX   { mcm4400t1e1DS0ChTestCmdLineID,
		mcm4400t1e1DS0ChTestCmdDS0Index }
	::= { mcm4400t1e1DS0ChTestCmdTable 1 }

Mcm4400t1e1DS0ChTestCmdEntry ::= SEQUENCE {
	mcm4400t1e1DS0ChTestCmdLineID		INTEGER,
	mcm4400t1e1DS0ChTestCmdDS0Index		INTEGER,
	mcm4400t1e1DS0ChTestCmdTstPattern	INTEGER
	}

mcm4400t1e1DS0ChTestCmdLineID OBJECT-TYPE
	SYNTAX	INTEGER {
		local (1),
		network (2)
		}
	ACCESS  read-only
	STATUS  mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = The T1/E1 line ID specifies which line is to \
		  be tested.  Used as an index to the \
		  T1/E1 DS0 Channel Test Command table.;
	   HELP = ;
	   CAPABILITIES = ;"
	::= { mcm4400t1e1DS0ChTestCmdEntry 1 }

mcm4400t1e1DS0ChTestCmdDS0Index OBJECT-TYPE
	SYNTAX	INTEGER (1..30)
	ACCESS  read-only
	STATUS  mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = The DS0 Index specifies which time slot is \
		  to be tested.  Used as an index to the \
		  T1/E1 DS0 Channel Test Command table.;
	   HELP = For T1, 24 is the maximum.  An error will be \
		  returned if a value greater than 24 is selected.;
	   CAPABILITIES = ;"
	::= { mcm4400t1e1DS0ChTestCmdEntry 2 }

mcm4400t1e1DS0ChTestCmdTstPattern OBJECT-TYPE
	SYNTAX	INTEGER {
		qrss (1),
		tp511 (2),
		loop (3),
		fixed-E1Only (4)
		}
	ACCESS  write-only
	STATUS  mandatory
	DESCRIPTION
	  "NAME = ; 
	   DESC = Setting this object will initiate various \
		  test pattern testing on the specified T1/E1 \
		  DS0 channel.;
	   HELP = For QRSS, TP511, and Fixed, the T1/E1 \
		  access module will generate and verify the \
		  selected test pattern. \
		  For Loop test, it simply loops the input \
		  data back to the same DS0 channel. \
		  In T1, if 4 is specified, an error will be \
		  returned.;
	   CAPABILITIES = ;"
	::= { mcm4400t1e1DS0ChTestCmdEntry 3 }


--  the T1/E1 Line Test Command Table 

--  This table provides the T1/E1 line test commands.

mcm4400t1e1LineTestCmdTable OBJECT-TYPE
	SYNTAX  SEQUENCE OF Mcm4400t1e1LineTestCmdEntry
	ACCESS  not-accessible
	STATUS  mandatory
	DESCRIPTION
	  "NAME = ; 
	   DESC = The T1/E1 Line Test Command \
		  table. \
	   HELP = ;
	   CAPABILITIES = NET_OPER, VPN_OPER ;"
	::= { t1e1-r2-control 2 }

mcm4400t1e1LineTestCmdEntry OBJECT-TYPE
	SYNTAX  Mcm4400t1e1LineTestCmdEntry
	ACCESS  not-accessible
	STATUS  mandatory
	DESCRIPTION
	  "NAME = ; 
	   DESC = An entry in the T1/E1 Line Test \
		  Command table.;
	   HELP = ;
	   CAPABILITIES = ;"
	INDEX   { mcm4400t1e1LineTestCmdLineID }
	::= { mcm4400t1e1LineTestCmdTable 1 }

Mcm4400t1e1LineTestCmdEntry ::= SEQUENCE {
	mcm4400t1e1LineTestCmdLineID		INTEGER,
	mcm4400t1e1LineTestCmdTstPattern	INTEGER
	}

mcm4400t1e1LineTestCmdLineID OBJECT-TYPE
	SYNTAX	INTEGER {
		local (1),
		network (2)
		}
	ACCESS  read-only
	STATUS  mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = The T1/E1 line ID specifies which line is to \
		  be tested.  Used as the index to the \
		  T1/E1 Line Test Command table.;
	   HELP = ;
	   CAPABILITIES = ;"
	::= { mcm4400t1e1LineTestCmdEntry 1 }

mcm4400t1e1LineTestCmdTstPattern OBJECT-TYPE
	SYNTAX	INTEGER {
		externalLineLoopback (1),
		externalDigitalLoopback (2),
		internalLineLoopback (3),
		internalDigitalLoopback (4)
		}
	ACCESS  write-only
	STATUS  mandatory
	DESCRIPTION
	  "NAME = ; 
	   DESC = Setting this object will initiate various \
		  tests on the specified T1/E1 line.;
	   HELP = Line Loopback: receiver or transmitter is \
		  connected to transmitter or receiver; respectively. \
		  Digital Loopback: data is stripped off the \
		  DS0 channel and reinserted in the same channel. \
		  External: data coming from the line. \
		  Internal: data going to the line.;
	   CAPABILITIES = ;"
	::= { mcm4400t1e1LineTestCmdEntry 2 }


--  the T1/E1 Data Port Test Command Table 

--  This table provides the T1/E1 data port test commands.

mcm4400t1e1DPTestCmdTable OBJECT-TYPE
	SYNTAX  SEQUENCE OF Mcm4400t1e1DPTestCmdEntry
	ACCESS  not-accessible
	STATUS  mandatory
	DESCRIPTION
	  "NAME = ; 
	   DESC = The T1/E1 Data Port Test Command \
		  table. \
	   HELP = ;
	   CAPABILITIES = NET_OPER, VPN_OPER ;"
	::= { t1e1-r2-control 3 }

mcm4400t1e1DPTestCmdEntry OBJECT-TYPE
	SYNTAX  Mcm4400t1e1DPTestCmdEntry
	ACCESS  not-accessible
	STATUS  mandatory
	DESCRIPTION
	  "NAME = ; 
	   DESC = An entry in the T1/E1 Data Port \
		  Test Command table.;
	   HELP = ;
	   CAPABILITIES = ;"
	INDEX   { mcm4400t1e1DPTestCmdPortNum }
	::= { mcm4400t1e1DPTestCmdTable 1 }

Mcm4400t1e1DPTestCmdEntry ::= SEQUENCE {
	mcm4400t1e1DPTestCmdPortNum		INTEGER,
	mcm4400t1e1DPTestCmdTstPattern		INTEGER
	}

mcm4400t1e1DPTestCmdPortNum OBJECT-TYPE
	SYNTAX	INTEGER {
		dataPort1 (1),
		dataPort2 (2)
		}
	ACCESS  read-only
	STATUS  mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = The T1/E1 Port Number specifies which data \
		  port is to be tested.  Used as the index to the \
		  T1/E1 Data Port Test Command table.;
	   HELP = ;
	   CAPABILITIES = ;"
	::= { mcm4400t1e1DPTestCmdEntry 1 }

mcm4400t1e1DPTestCmdTstPattern OBJECT-TYPE
	SYNTAX	INTEGER {
		externalLoopback (1),
		internalLoopback (2)
		}
	ACCESS  write-only
	STATUS  mandatory
	DESCRIPTION
	  "NAME = ; 
	   DESC = Setting this object will initiate various \
		  tests on the specified T1/E1 data port.;
	   HELP = External: loop back data coming from the \
		  device attached to the port. \
		  Internal: loop back data coming from the  \
		  DS0 channel.;
	   CAPABILITIES = ;"
	::= { mcm4400t1e1DPTestCmdEntry 2 }


--  the T1/E1 System Control Action Command Group 

--  This object provides the T1/E1 system control/action commands.

mcm4400t1e1SysControlActionCmd OBJECT-TYPE
	SYNTAX	INTEGER {
		testLED (1),
		resetT1E1 (2),
		clearCounter (3),
		download (4),
		stopChannelTest (5),
		stopLineTest (6),
		stopPortTest (7),
		stopLEDTest (8),
		stopAllTests (9)
		}
	ACCESS  write-only
	STATUS  mandatory
	DESCRIPTION
	  "NAME = ; 
	   DESC = Setting this object will perform various \
		  actions on the T1/E1 system. \
		  Note: Starting with software release 3.0, \
		  the enumeration clearCounter (3) is \
		  not supported. The enumeration is included \
		  here only to support backwards compatibility \
		  for software versions prior to release 3.0.; 
	   HELP = 1: initiate LED test. \
		  2: reset T1/E1 module. \
		  3: clear statistics counters. \
		  4: download code. \
		  5: stop DS0 channel test. \
		  6: stop line test. \
		  7: stop port test. \
		  8: stop LED test. \
		  9: stop all tests.;
	   CAPABILITIES = NET_OPER, VPN_OPER ;"
	::= { t1e1-r2-control 4 }


mcm4400T1NoCodeImage TRAP-TYPE
	ENTERPRISE	micom-2t1e1
	VARIABLES	{mcmSysAsciiTimeOfDay}
	DESCRIPTION
	  "NAME = ;
	   DESC = Indicates that the built image doesn't \
		  contain T1 code.\
		  Severity Level: MAJOR.;
	   HELP = The Image loaded does not contain T1 code.\
                  Load a new Image that includes T1 code, or \
                  consult with the System Admin. or Field \
                  Personnel.;"
	::= 1

mcm4400T1DownloadFail TRAP-TYPE
	ENTERPRISE	micom-2t1e1
	VARIABLES	{mcmSysAsciiTimeOfDay}
	DESCRIPTION
	  "NAME = ;
	   DESC = Indicates a failure occurred during the T1 \
		  code download.\
                  Severity Level: MAJOR;
	   HELP = Retry the download. Re-boot the access device and \
                  check if it downloads correctly. Also, verify \
                  the T1 code that is build with access device image \
                  is the right version. If problem persist, consult \
                  your system admin., or field personnel.;"
	::= 2

mcm4400T1OutOfOrder TRAP-TYPE
	ENTERPRISE	micom-2t1e1
	VARIABLES	{mcmSysAsciiTimeOfDay}
	DESCRIPTION
	  "NAME = ;
	   DESC = Indicates the T1 is not responding.\
		  Severity Level: CRITICAL.;
	   HELP = Management frame is out of order. Check for \
                  Framing error. If it does not recover, Please \
                  contact your Sys Admin. or Field personnel.;" 
	::= 3

mcm4400T1OutOfOrderRecovered TRAP-TYPE
	ENTERPRISE	micom-2t1e1
	VARIABLES	{mcmSysAsciiTimeOfDay}
	DESCRIPTION
	  "NAME = ;
	   DESC = Indicates recovery from the T1 out of order condition.\
                  Severity Level: INFORMATIVE.;
	   HELP = T1 is responding. Normal Status Indicator.;"
	::= 4

mcm4400T1FailToSyncUp TRAP-TYPE
	ENTERPRISE	micom-2t1e1
	VARIABLES	{mcmSysAsciiTimeOfDay}
	DESCRIPTION
	  "NAME = ;
	   DESC = Indicates the T1 is unable to sync up.\
                  Severity Level : CRITICAL.;
	   HELP = Check the physical connection locally, and \
                  to the remote device. Possible protocol error. \
                  If problem still persist, contact your \
                  system admin., or field personnel.;"
	::= 5

mcm4400T1FailToSyncUpRecovered TRAP-TYPE
	ENTERPRISE	micom-2t1e1
	VARIABLES	{mcmSysAsciiTimeOfDay}
	DESCRIPTION
	  "NAME = ;
	   DESC = Indicates recovery from the T1 failure \
		  to sync up condition.\
                  Severity Level: INFORMATIVE.;
	   HELP = T1 has recovered. Normal Status Indicator.;"
	::= 6

mcm4400T1AISAlarmSet TRAP-TYPE
	ENTERPRISE	micom-2t1e1
