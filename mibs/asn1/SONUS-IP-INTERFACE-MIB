sonusSrvrEthernetSwitchIpTable OBJECT-TYPE
    SYNTAX          SEQUENCE OF SonusSrvrEthernetSwitchIpEntry
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION     
            "The table of ethernet switch IP address for servers"
    ::= { sonusIpInterfaceMIBObjects 12 }

sonusSrvrEthernetSwitchIpEntry OBJECT-TYPE
    SYNTAX          SonusSrvrEthernetSwitchIpEntry
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION     
            "ethernet switch IP address for a server"
    INDEX           { sonusSrvrEthernetSwitchIpShelf, 
                      sonusSrvrEthernetSwitchIpSlot, 
                      sonusSrvrEthernetSwitchIpPort }
    ::= { sonusSrvrEthernetSwitchIpTable 1 }

SonusSrvrEthernetSwitchIpEntry ::=
    SEQUENCE {
        sonusSrvrEthernetSwitchIpShelf  Integer32,
        sonusSrvrEthernetSwitchIpSlot   Integer32,
        sonusSrvrEthernetSwitchIpIpAddress  IpAddress,
        sonusSrvrEthernetSwitchIpPort   Integer32
    }

sonusSrvrEthernetSwitchIpShelf OBJECT-TYPE
    SYNTAX          Integer32 (1..6)
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION     
            "The shelf index of the server"
    ::= { sonusSrvrEthernetSwitchIpEntry 1 }

sonusSrvrEthernetSwitchIpSlot OBJECT-TYPE
    SYNTAX          Integer32 (1..16)
    MAX-ACCESS      read-write
    STATUS          current
