    ::= { sonusSrvrEthernetSwitchIpEntry 4 }

sonusNrsSocketAccessAdmnTable OBJECT-TYPE
    SYNTAX          SEQUENCE OF SonusNrsSocketAccessAdmnEntry
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION     
            "The table of socket access definitions"
    ::= { sonusIpInterfaceMIBObjects 13 }

sonusNrsSocketAccessAdmnEntry OBJECT-TYPE
    SYNTAX          SonusNrsSocketAccessAdmnEntry
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION     
            "socket access definition for a server"
    INDEX           { sonusNrsSocketAccessProtocol, 
                      sonusNrsSocketAccessPort, 
                      sonusNrsSocketAccessIfIndex }
    ::= { sonusNrsSocketAccessAdmnTable 1 }

SonusNrsSocketAccessAdmnEntry ::=
    SEQUENCE {
        sonusNrsSocketAccessProtocol    INTEGER,
        sonusNrsSocketAccessPort        Integer32,
        sonusNrsSocketAccessIfIndex     Integer32,
        sonusNrsSocketAccessPermission  INTEGER,
        sonusNrsSocketAccessRowStatus   RowStatus
    }

sonusNrsSocketAccessProtocol OBJECT-TYPE
    SYNTAX          INTEGER { tcp(6), udp(17) }
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION     
            "The protocol definition"
    DEFVAL          { tcp }
    ::= { sonusNrsSocketAccessAdmnEntry 1 }

sonusNrsSocketAccessPort OBJECT-TYPE
    SYNTAX          Integer32 (1..65535)
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "The port number, value between 1-65535"
    ::= { sonusNrsSocketAccessAdmnEntry 2 }

sonusNrsSocketAccessIfIndex OBJECT-TYPE
    SYNTAX          Integer32 (1..65535)
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION     
            "A non-zero unsigned long ifindex assigned to the port where defines the access permition."
    ::= { sonusNrsSocketAccessAdmnEntry 3 }

sonusNrsSocketAccessPermission OBJECT-TYPE
    SYNTAX          INTEGER { deny(1), permit(2) }
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION     
            "The access permission"
    DEFVAL          { permit }
    ::= { sonusNrsSocketAccessAdmnEntry 4 }

sonusNrsSocketAccessRowStatus OBJECT-TYPE
    SYNTAX          RowStatus
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION     
            ""
    ::= { sonusNrsSocketAccessAdmnEntry 5 }

sonusNrsDebugIfTable OBJECT-TYPE
    SYNTAX          SEQUENCE OF SonusNrsDebugIfEntry
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION     
            "The collection of Debug Interface information for the
             specified instance."
    ::= { sonusIpInterfaceMIBObjects 99 }

sonusNrsDebugIfEntry OBJECT-TYPE
    SYNTAX          SonusNrsDebugIfEntry
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION     
            "The collection of Debug Interface information for the
             specified instance."
    INDEX           { sonusNrsDebugIfShelf, sonusNrsDebugIfSlot }
    ::= { sonusNrsDebugIfTable 1 }

SonusNrsDebugIfEntry ::=
    SEQUENCE {
        sonusNrsDebugIfIndex            Integer32,
        sonusNrsDebugIfShelf            Integer32,
        sonusNrsDebugIfSlot             Integer32,
        sonusNrsDebugIfPort             Integer32,
        sonusNrsDebugIfCircuit          Integer32,
        sonusNrsDebugIfType             IANAifType,
        sonusNrsDebugIfIpAddress        IpAddress,
        sonusNrsDebugIfIpMask           IpAddress,
        sonusNrsDebugIfMtu              Integer32,
        sonusNrsDebugIfGatewayAddress   IpAddress,
        sonusNrsDebugIfAdminStatus      INTEGER,
        sonusNrsDebugIfOperStatus       INTEGER
    }

sonusNrsDebugIfIndex OBJECT-TYPE
    SYNTAX          Integer32
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "The ifIndex assigned to this debug NIF when it was created.
             This will not change until the NIF is deleted."
    ::= { sonusNrsDebugIfEntry 1 }

sonusNrsDebugIfShelf OBJECT-TYPE
    SYNTAX          Integer32
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION     
            "A 32-bit integer identifying the shelf of the Debug
             Interface"
    ::= { sonusNrsDebugIfEntry 2 }

sonusNrsDebugIfSlot OBJECT-TYPE
    SYNTAX          Integer32
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION     
            "A 32-bit integer identifying the slot of the Debug
             Interface"
    ::= { sonusNrsDebugIfEntry 3 }

sonusNrsDebugIfPort OBJECT-TYPE
    SYNTAX          Integer32
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION     
            "A 32-bit integer identifying the port of the Debug
             Interface.  For current hardware this must be 1."
    ::= { sonusNrsDebugIfEntry 4 }

sonusNrsDebugIfCircuit OBJECT-TYPE
    SYNTAX          Integer32
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION     
            "A 32-bit integer identifying the circuit of the Debug
             Interface.  For current hardware this must be 1."
    ::= { sonusNrsDebugIfEntry 5 }

sonusNrsDebugIfType OBJECT-TYPE
    SYNTAX          IANAifType
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION     
            "The type of interface, distinguished according to the
             physical/link protocol(s) immediately 'below' the network
             layer in the protocol stack.
             
             Values are from RFC1213 for the ifType object.
             
             Current hardware supports only the ethernet-csmacd (6)
             type."
    ::= { sonusNrsDebugIfEntry 6 }

sonusNrsDebugIfIpAddress OBJECT-TYPE
    SYNTAX          IpAddress
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION     
            "The IP Address of the Debug Interface"
    ::= { sonusNrsDebugIfEntry 7 }

sonusNrsDebugIfIpMask OBJECT-TYPE
    SYNTAX          IpAddress
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION     
            "The IP subnet mask of this Debug Interface."
    ::= { sonusNrsDebugIfEntry 8 }

sonusNrsDebugIfMtu OBJECT-TYPE
    SYNTAX          Integer32
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION     
            "The size of the largest datagram that can be sent/received
             on the interface, specified in octets."
    ::= { sonusNrsDebugIfEntry 9 }

sonusNrsDebugIfGatewayAddress OBJECT-TYPE
    SYNTAX          IpAddress
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION     
            "The IP address of the default gateway for this Debug
             Interface."
    ::= { sonusNrsDebugIfEntry 10 }

sonusNrsDebugIfAdminStatus OBJECT-TYPE
    SYNTAX          INTEGER { disabled(1), enabled(2) }
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION     
            "The requested status of the interface."
    ::= { sonusNrsDebugIfEntry 11 }

sonusNrsDebugIfOperStatus OBJECT-TYPE
    SYNTAX          INTEGER { outOfService(1), inService(2), 
                        outOfServiceDisabled(3), outOfServiceLinkDown(4) }
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "The actual status of the debug interface.  A link will
             be InService if the sonusNrsDebugIfAdminStatus is enabled and
             the physical link is up.  It will be OutOfService if the admin
             status is enabled but the physical link is down.  It will be
             OutOfServiceDisabled if the physical link is up but the admin
             status is disabled.  It will be OutOfServiceLinkDown if the
             physical link is down and the admin status is disabled."
    ::= { sonusNrsDebugIfEntry 12 }

sonusIpInterfaceMIBNotifications OBJECT IDENTIFIER
    ::= { sonusIpInterfaceMIB 2 }

sonusIpInterfaceMIBNotificationsPrefix OBJECT IDENTIFIER
    ::= { sonusIpInterfaceMIBNotifications 0 }

sonusIpInterfaceMIBNotificationsObjects OBJECT IDENTIFIER
    ::= { sonusIpInterfaceMIBNotifications 1 }

sonusNrsNifInServiceNotification NOTIFICATION-TYPE
    OBJECTS         { sonusNifShelf, sonusNifSlot, sonusNifPort, 
                      sonusNifIndex, sonusNifName, sonusEventDescription, 
                      sonusEventClass, sonusEventLevel }
    STATUS          current
    DESCRIPTION     
            "This trap indicates that a NIF is available to support
             call processing."
    ::= { sonusIpInterfaceMIBNotificationsPrefix 1 }

sonusNrsNifOutOfServiceNotification NOTIFICATION-TYPE
    OBJECTS         { sonusNifShelf, sonusNifSlot, sonusNifPort, 
                      sonusNifIndex, sonusNifName, 
                      sonusNifOutOfServiceReason, sonusEventDescription, 
                      sonusEventClass, sonusEventLevel }
    STATUS          current
    DESCRIPTION     
            "This trap indicates that a NIF is no longer available to
             support call processing."
    ::= { sonusIpInterfaceMIBNotificationsPrefix 2 }

sonusNrsNifEnabledNotification NOTIFICATION-TYPE
    OBJECTS         { sonusNifShelf, sonusNifSlot, sonusNifPort, 
                      sonusNifIndex, sonusNifName, sonusEventDescription, 
                      sonusEventClass, sonusEventLevel }
    STATUS          current
    DESCRIPTION     
            "This trap indicates that a NIF's configuration is enabled."
    ::= { sonusIpInterfaceMIBNotificationsPrefix 3 }

sonusNrsNifDisabledNotification NOTIFICATION-TYPE
    OBJECTS         { sonusNifShelf, sonusNifSlot, sonusNifPort, 
                      sonusNifIndex, sonusNifName, sonusEventDescription, 
                      sonusEventClass, sonusEventLevel }
    STATUS          current
    DESCRIPTION     
            "This trap indicates that a NIF's configuration is disabled."
    ::= { sonusIpInterfaceMIBNotificationsPrefix 4 }

sonusNrsNifDeletedNotification NOTIFICATION-TYPE
    OBJECTS         { sonusNifShelf, sonusNifSlot, sonusNifPort, 
                      sonusNifIndex, sonusNifName, sonusEventDescription, 
                      sonusEventClass, sonusEventLevel }
    STATUS          current
    DESCRIPTION     
            "This trap indicates that a NIF's configuration is deleted."
    ::= { sonusIpInterfaceMIBNotificationsPrefix 5 }

sonusNrsNifHighWatermarkNotification NOTIFICATION-TYPE
    OBJECTS         { sonusNifShelf, sonusNifSlot, sonusNifPort, 
                      sonusNifIndex, sonusNifName, sonusEventDescription, 
                      sonusEventClass, sonusEventLevel }
    STATUS          current
    DESCRIPTION     
            "This trap indicates that a NIF's high watermark has been reached 
             the first time since the NIF's low watermark has beend reached."
    ::= { sonusIpInterfaceMIBNotificationsPrefix 6 }

sonusNrsNifLowWatermarkNotification NOTIFICATION-TYPE
    OBJECTS         { sonusNifShelf, sonusNifSlot, sonusNifPort, 
                      sonusNifIndex, sonusNifName, sonusEventDescription, 
                      sonusEventClass, sonusEventLevel }
    STATUS          current
    DESCRIPTION     
            "This trap indicates that a NIF's low watermark has been reached
             the first time since the NIF's high watermark has been reached."
    ::= { sonusIpInterfaceMIBNotificationsPrefix 7 }

sonusNrsNifBwHighDeviationNotification NOTIFICATION-TYPE
    OBJECTS         { sonusNifShelf, sonusNifSlot, sonusNifPort, 
                      sonusNifIndex, sonusNifName, sonusEventDescription, 
                      sonusEventClass, sonusEventLevel }
    STATUS          current
    DESCRIPTION     
            "This trap indicates that a NIF's actual bandwidth is greater than the 
             expected bandwidth plus deviation amount. This trap will be re-armed
             when the actual bandwdith dropped below the expected level. "
    ::= { sonusIpInterfaceMIBNotificationsPrefix 8 }

sonusNrsNifBwLowDeviationNotification NOTIFICATION-TYPE
    OBJECTS         { sonusNifShelf, sonusNifSlot, sonusNifPort, 
                      sonusNifIndex, sonusNifName, sonusEventDescription, 
                      sonusEventClass, sonusEventLevel }
    STATUS          current
    DESCRIPTION     
            "This trap indicates that a NIF's actual bandwidth is lower than the
             expected bandwidth minus 2 times the deviation amount.This trap will 
             be re-armed when the actual bandwdith reached the expected level."
    ::= { sonusIpInterfaceMIBNotificationsPrefix 9 }

sonusNrsLinkDownNotification NOTIFICATION-TYPE
    OBJECTS         { sonusShelfIndex, sonusSlotIndex, 
                      sonusEventDescription, sonusEventClass, 
                      sonusEventLevel }
    STATUS          current
    DESCRIPTION     
            "This trap indicates that a link down event has been detected on a server"
    ::= { sonusIpInterfaceMIBNotificationsPrefix 10 }

sonusNrsLinkUpNotification NOTIFICATION-TYPE
    OBJECTS         { sonusShelfIndex, sonusSlotIndex, 
                      sonusEventDescription, sonusEventClass, 
                      sonusEventLevel }
    STATUS          current
    DESCRIPTION     
            "This trap indicates that a link up event has event detected on a server"
    ::= { sonusIpInterfaceMIBNotificationsPrefix 11 }

sonusNrsNifCreatedNotification NOTIFICATION-TYPE
    OBJECTS         { sonusNifShelf, sonusNifSlot, sonusNifPort, 
                      sonusNifIndex, sonusNifName, sonusEventDescription, 
                      sonusEventClass, sonusEventLevel }
    STATUS          current
    DESCRIPTION     
            "This trap indicates that a NIF's configuration is created."
    ::= { sonusIpInterfaceMIBNotificationsPrefix 12 }

END -- end of module SONUS-IP-INTERFACE-MIB.
