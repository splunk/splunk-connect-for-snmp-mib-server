-- *****************************************************************************
-- Directories Config Group
-- *****************************************************************************


w2kConfigDirPollInterval OBJECT-TYPE
	SYNTAX INTEGER
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
		"&<caiW2kOs.227>The time (in seconds) between the polling of the directory information. Valid values are greater than or equal to 30."
	DEFVAL { 120 }
	::= { w2kConfigDirGroup 1 }


w2kConfigDirPollMethod OBJECT-TYPE
	SYNTAX INTEGER {
		disabled(1),
		poll-interval-only(2),
		poll-interval-and-query(3),
		query-only(4)
	}
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
		"&<caiW2kOs.228>If set to 'disabled', the agent no more retrieves the directory information (not by polling, nor by get request) and sets all statuses to ok. If set to 'poll-interval-only', the agent only retrieves the information by polling, but not by get request. If set to 'poll-interval-and-query', the agent retrieves the information in both cases (polling and get request). If set to 'query-only', the agent retrieves the information for get requests only."
	DEFVAL { 2 }
	::= { w2kConfigDirGroup 2 }


w2kConfigDirName OBJECT-TYPE
	SYNTAX DisplayString
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
		"&<caiW2kOs.229>Name of the watcher to be added/removed, monitoring a directory with this name. Important: This name is case insensitive."
	::= { w2kConfigDirGroup 3 }


w2kConfigDirDescription OBJECT-TYPE
	SYNTAX DisplayString
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
		"&<caiW2kOs.136>Any description for this watcher."
	::= { w2kConfigDirGroup 4 }


w2kConfigDirAggLag OBJECT-TYPE
	SYNTAX INTEGER
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
		"&<caiW2kOs.137>The number of consecutive poll intervals on which any status is not in the Ok or Up state, before the aggregate status changes. Valid values are greater than 0."
	DEFVAL { 1 }
	::= { w2kConfigDirGroup 5 }


w2kConfigDirExist OBJECT-TYPE
	SYNTAX INTEGER {
		should-exist(1),
		should-not-exist(2)
	}
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
		"&<caiW2kOs.232>Flag indicating whether to raise an alert if a directory is existent or if it is not-existent."
	DEFVAL { 1 }
	::= { w2kConfigDirGroup 6 }


