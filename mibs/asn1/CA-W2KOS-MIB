-- *****************************************************************************
-- Mounts Config Group
-- *****************************************************************************


w2kConfigMntPollInterval OBJECT-TYPE
	SYNTAX INTEGER
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
		"&<caiW2kOs.173>The time (in seconds) between the polling of the mount information. Valid values are greater than or equal to 30."
	DEFVAL { 120 }
	::= { w2kConfigMntGroup 1 }


w2kConfigMntPollMethod OBJECT-TYPE
	SYNTAX INTEGER {
		disabled(1),
		poll-interval-only(2),
		poll-interval-and-query(3),
		query-only(4)
	}
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
		"&<caiW2kOs.174>If set to 'disabled', the agent no more retrieves the mount information (not by polling, nor by get request) and sets all statuses to ok. If set to 'poll-interval-only', the agent only retrieves the information by polling, but not by get request. If set to 'poll-interval-and-query', the agent retrieves the information in both cases (polling and get request). If set to 'query-only', the agent retrieves the information for get requests only."
	DEFVAL { 2 }
	::= { w2kConfigMntGroup 2 }


w2kConfigMntAutoPollInterval OBJECT-TYPE
	SYNTAX INTEGER
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
		"&<caiW2kOs.175>The time (in minutes), when a new auto discovery process is started. Valid values are greater than or equal to 30 or set to -1. If set to -1, auto discovery of mounts will no more be made."
	DEFVAL { 60 }
	::= { w2kConfigMntGroup 3 }


w2kConfigMntName OBJECT-TYPE
	SYNTAX DisplayString
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
		"&<caiW2kOs.176>Name of the watcher to be added/removed, monitoring a mount with this name. If adding an auto discovery watcher, this name can be a wildcarded mount name filtering the mounts to be auto discovered (any backslash in the wildcarded name must be doubled). For each auto discovered mount a watcher will be created. Important: This name is case insensitive."
	::= { w2kConfigMntGroup 4 }


w2kConfigMntDescription OBJECT-TYPE
	SYNTAX DisplayString
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
		"&<caiW2kOs.136>Any description for this watcher."
	::= { w2kConfigMntGroup 5 }


w2kConfigMntAggLag OBJECT-TYPE
	SYNTAX INTEGER
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
		"&<caiW2kOs.137>The number of consecutive poll intervals on which any status is not in the Ok or Up state, before the aggregate status changes. Valid values are greater than 0."
	DEFVAL { 1 }
	::= { w2kConfigMntGroup 6 }


w2kConfigMntRelToMonitor OBJECT-TYPE
	SYNTAX INTEGER {
		do-not-monitor(1),
		down-warning(2),
		down-critical(3)
	}
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
		"&<caiW2kOs.179>Flag indicating whether a change of the logical volume, mounted by this mount, should be monitored and if so, the level of alert to raise. Note: A change will not occur if the related logical volume is lossed. In that case the mount itself becomes lossed."
	DEFVAL { 2 }
	::= { w2kConfigMntGroup 7 }


w2kConfigMntLossAction OBJECT-TYPE
	SYNTAX INTEGER {
		remove(1),
		up(2),
		down-warning(3),
		down-critical(4)
	}
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
		"&<caiW2kOs.180>Flag defining the severity if the loss of the mount is detected or whether to remove this watcher in that case."
	DEFVAL { 4 }
	::= { w2kConfigMntGroup 8 }


w2kConfigMntAutoTableWatcherName OBJECT-TYPE
	SYNTAX DisplayString
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
		"&<caiW2kOs.151>Name of the watcher to be added in the auto discovery table."
	::= { w2kConfigMntGroup 9 }


w2kConfigMntButton OBJECT-TYPE
	SYNTAX INTEGER {
		none(1),
		add(2),
		remove(3),
		add-auto(4),
		remove-auto(5)
	}
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
		"&<caiW2kOs.152>This attribute is used to add or remove a watcher with the current value of the watcher name attribute in this group, or to add or remove an auto discovery watcher with the current value of the auto table watcher name attribute in this group."
	::= { w2kConfigMntGroup 10 }


w2kConfigMntAutoTable OBJECT-TYPE
	SYNTAX SEQUENCE OF W2kConfigMntAutoRow
	ACCESS not-accessible
	STATUS mandatory
	DESCRIPTION
		"&<caiW2kOs.183>A table containing all auto discovery watchers for mounts."
	::= { w2kConfigMntGroup 11 }


w2kConfigMntAutoEntry OBJECT-TYPE
	SYNTAX W2kConfigMntAutoRow
	ACCESS not-accessible
	STATUS mandatory
	DESCRIPTION
		"&<caiW2kOs.154>The table entry."
	INDEX { w2kConfigMntAutoWatcherName }
	::= { w2kConfigMntAutoTable 1 }


W2kConfigMntAutoRow ::=
	SEQUENCE {
		w2kConfigMntAutoWatcherName DisplayString,
		w2kConfigMntAutoName DisplayString,
		w2kConfigMntAutoDescription DisplayString,
		w2kConfigMntAutoAggLag INTEGER,
		w2kConfigMntAutoRelToMonitor INTEGER,
		w2kConfigMntAutoLossAction INTEGER,
		w2kConfigMntAutoButton INTEGER
	}


