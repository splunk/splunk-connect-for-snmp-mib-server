-- *****************************************************************************
-- Registry Entries Status Group
-- *****************************************************************************


w2kStatusRegCount OBJECT-TYPE
	SYNTAX INTEGER
	ACCESS read-only
	STATUS mandatory
	DESCRIPTION
		"&<caiW2kOs.840>The number of entries in the w2kStatusRegTable."
	::= { w2kStatusRegGroup 1 }


w2kStatusRegTable OBJECT-TYPE
	SYNTAX SEQUENCE OF W2kStatusRegRow
	ACCESS not-accessible
	STATUS mandatory
	DESCRIPTION
		"&<caiW2kOs.841>A table containing information about all monitored registry entries."
	::= { w2kStatusRegGroup 2 }


w2kStatusRegEntry OBJECT-TYPE
	SYNTAX W2kStatusRegRow
	ACCESS not-accessible
	STATUS mandatory
	DESCRIPTION
		"&<caiW2kOs.154>The table entry."
	INDEX { w2kStatusRegName }
	::= { w2kStatusRegTable 1 }


W2kStatusRegRow ::=
	SEQUENCE {
		w2kStatusRegName DisplayString,
		w2kStatusRegDescription DisplayString,
		w2kStatusRegAggLagValue INTEGER,
		w2kStatusRegAggLag INTEGER,
		w2kStatusRegAggStatus INTEGER,
		w2kStatusRegExist INTEGER,
		w2kStatusRegExistMonitor INTEGER,
		w2kStatusRegExistStatus INTEGER,
		w2kStatusRegValueValue DisplayString,
		w2kStatusRegValueType INTEGER,
		w2kStatusRegValueRef DisplayString,
		w2kStatusRegValueCond INTEGER,
		w2kStatusRegValueMonitor INTEGER,
		w2kStatusRegValueStatus INTEGER,
		w2kStatusRegCallBackRef DisplayString,
		w2kStatusRegButton INTEGER
	}


w2kStatusRegName OBJECT-TYPE
	SYNTAX DisplayString
	ACCESS read-only
	STATUS mandatory
	DESCRIPTION
		"&<caiW2kOs.843>Name of the watcher, monitoring a registry entry with this name. The name is the complete registry path ending with the registry leaf name. If the leaf name contains a backslash, the leaf name must be quoted with double quotes. Important: This name is case insensitive."
	::= { w2kStatusRegEntry 1 }


w2kStatusRegDescription OBJECT-TYPE
	SYNTAX DisplayString
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
		"&<caiW2kOs.136>Any description for this watcher."
	::= { w2kStatusRegEntry 2 }


w2kStatusRegAggLagValue OBJECT-TYPE
	SYNTAX INTEGER
	ACCESS read-only
	STATUS mandatory
	DESCRIPTION
		"&<caiW2kOs.499>The number of consecutive poll intervals on which any status was not in the Ok or Up state."
	::= { w2kStatusRegEntry 3 }


w2kStatusRegAggLag OBJECT-TYPE
	SYNTAX INTEGER
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
		"&<caiW2kOs.137>The number of consecutive poll intervals on which any status is not in the Ok or Up state, before the aggregate status changes. Valid values are greater than 0."
	::= { w2kStatusRegEntry 4 }


w2kStatusRegAggStatus OBJECT-TYPE
	SYNTAX INTEGER {
		unknown(1),
		ok(2),
		warning(3),
		critical(4)
	}
	ACCESS read-only
	STATUS mandatory
	DESCRIPTION
		"&<caiW2kOs.847>The current status of the registry entry as a 'worst case' aggregate of all its statuses."
	::= { w2kStatusRegEntry 5 }


w2kStatusRegExist OBJECT-TYPE
	SYNTAX INTEGER {
		should-exist(1),
		should-not-exist(2)
	}
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
		"&<caiW2kOs.382>Flag indicating whether to raise an alert if a registry entry is existent or if it is not-existent."
	DEFVAL { 0 }
	::= { w2kStatusRegEntry 6 }


w2kStatusRegExistMonitor OBJECT-TYPE
	SYNTAX INTEGER {
		do-not-monitor(1),
		down-warning(2),
		down-critical(3)
	}
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
		"&<caiW2kOs.383>Flag indicating whether the existence of the registry entry should be monitored and if so, the level of alert to raise."
	DEFVAL { 0 }
	::= { w2kStatusRegEntry 7 }


w2kStatusRegExistStatus OBJECT-TYPE
	SYNTAX INTEGER {
		unknown(1),
		up(2),
		down-warning(3),
		down-critical(4)
	}
	ACCESS read-only
	STATUS mandatory
	DESCRIPTION
		"&<caiW2kOs.850>The current status of the registry entry existence."
	::= { w2kStatusRegEntry 8 }


w2kStatusRegValueValue OBJECT-TYPE
	SYNTAX DisplayString
	ACCESS read-only
	STATUS mandatory
	DESCRIPTION
		"&<caiW2kOs.851>The current value of the registry entry."
	::= { w2kStatusRegEntry 9 }


w2kStatusRegValueType OBJECT-TYPE
	SYNTAX INTEGER {
		string(1),
		binary(2),
		dword(3),
		unknown(4)
	}
	ACCESS read-only
	STATUS mandatory
	DESCRIPTION
		"&<caiW2kOs.852>The current type of the registry entry."
	::= { w2kStatusRegEntry 10 }


w2kStatusRegValueRef OBJECT-TYPE
	SYNTAX DisplayString
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
		"&<caiW2kOs.384>This attribute is used as the reference value the registry entry is compared with. If the condition is one of 'should-be-equal', 'should-not-be-equal', 'should-be-below' or 'should-be-above', this reference value must be set by the user. In case of 'should-not-change', by setting the status button attribute to 'reset-value', the reference value is set to the current value of the registry entry."
	::= { w2kStatusRegEntry 11 }


w2kStatusRegValueCond OBJECT-TYPE
	SYNTAX INTEGER {
		should-be-equal(1),
		should-not-be-equal(2),
		should-be-below(3),
		should-be-above(4),
		should-not-change(5)
	}
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
		"&<caiW2kOs.385>Flag indicating whether to raise an alert if a registry entry does not fullfill the given condition compared with the reference value attribute. 'should-be-below' and 'should-be-above' is only allowed for entries with type DWORD. For entries with type Binary 'should-not-change' is the only possible value."
	DEFVAL { 0 }
	::= { w2kStatusRegEntry 12 }


w2kStatusRegValueMonitor OBJECT-TYPE
	SYNTAX INTEGER {
		do-not-monitor(1),
		down-warning(2),
		down-critical(3)
	}
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
		"&<caiW2kOs.386>Flag indicating whether the value of the registry entry should be monitored and if so, the level of alert to raise."
	DEFVAL { 0 }
	::= { w2kStatusRegEntry 13 }


w2kStatusRegValueStatus OBJECT-TYPE
	SYNTAX INTEGER {
		unknown(1),
		up(2),
		down-warning(3),
		down-critical(4)
	}
	ACCESS read-only
	STATUS mandatory
	DESCRIPTION
		"&<caiW2kOs.856>The current status of the registry entry as determined by the thresholds. The status 'unknown' means that the current value could not be retrieved from the system."
	::= { w2kStatusRegEntry 14 }


w2kStatusRegCallBackRef OBJECT-TYPE
	SYNTAX DisplayString
	ACCESS read-only
	STATUS mandatory
	DESCRIPTION
		"&<caiW2kOs.443>The name of the Call-Back reference that should be invoked in case of a status change."
	::= { w2kStatusRegEntry 15 }


w2kStatusRegButton OBJECT-TYPE
	SYNTAX INTEGER {
		none(1),
		remove(2),
		reset-value(3)
	}
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
		"&<caiW2kOs.858>This attribute is used to remove this watcher or to reset the value reference to the current value of the registry entry. The 'reset-value' is only possible in case the condition is 'should-not-change'."
	::= { w2kStatusRegEntry 16 }




