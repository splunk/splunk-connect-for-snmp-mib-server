	DEFVAL { 0 }
	::= { w2kStatusSessEntry 21 }


w2kStatusSessCpuStatus OBJECT-TYPE
	SYNTAX INTEGER {
		unknown(1),
		ok(2),
		warning(3),
		critical(4)
	}
	ACCESS read-only
	STATUS mandatory
	DESCRIPTION
		"&<caiW2kOs.762>The current status of the session's CPU usage as determined by the thresholds. The status 'unknown' means that the current value could not be retrieved from the system."
	::= { w2kStatusSessEntry 22 }


w2kStatusSessCallBackRef OBJECT-TYPE
	SYNTAX DisplayString
	ACCESS read-only
	STATUS mandatory
	DESCRIPTION
		"&<caiW2kOs.443>The name of the Call-Back reference that should be invoked in case of a status change."
	::= { w2kStatusSessEntry 23 }


w2kStatusSessButton OBJECT-TYPE
	SYNTAX INTEGER {
		none(1),
		remove(2)
	}
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
		"&<caiW2kOs.530>This attribute is used to remove this watcher."
	::= { w2kStatusSessEntry 24 }




-- *****************************************************************************
-- Printer Status Group
-- *****************************************************************************


w2kStatusPrnCount OBJECT-TYPE
	SYNTAX INTEGER
	ACCESS read-only
	STATUS mandatory
	DESCRIPTION
		"&<caiW2kOs.765>The number of entries in the w2kStatusPrnTable."
	::= { w2kStatusPrnGroup 1 }


w2kStatusPrnTable OBJECT-TYPE
	SYNTAX SEQUENCE OF W2kStatusPrnRow
	ACCESS not-accessible
	STATUS mandatory
	DESCRIPTION
		"&<caiW2kOs.766>A table containing information about all monitored printers."
	::= { w2kStatusPrnGroup 2 }


w2kStatusPrnEntry OBJECT-TYPE
	SYNTAX W2kStatusPrnRow
	ACCESS not-accessible
	STATUS mandatory
	DESCRIPTION
		"&<caiW2kOs.154>The table entry."
	INDEX { w2kStatusPrnName }
	::= { w2kStatusPrnTable 1 }


W2kStatusPrnRow ::=
	SEQUENCE {
		w2kStatusPrnName DisplayString,
		w2kStatusPrnDescription DisplayString,
		w2kStatusPrnAutoWatcherName DisplayString,
		w2kStatusPrnAggLagValue INTEGER,
		w2kStatusPrnAggLag INTEGER,
		w2kStatusPrnAggStatus INTEGER,
		w2kStatusPrnEventDescr INTEGER,
		w2kStatusPrnEventMonitor INTEGER,
		w2kStatusPrnEventStatus INTEGER,
		w2kStatusPrnQueueValue INTEGER,
		w2kStatusPrnQueueWarn INTEGER,
		w2kStatusPrnQueueCrit INTEGER,
		w2kStatusPrnQueueMonitor INTEGER,
		w2kStatusPrnQueueStatus INTEGER,
		w2kStatusPrnLossAction INTEGER,
		w2kStatusPrnLossStatus INTEGER,
		w2kStatusPrnCallBackRef DisplayString,
		w2kStatusPrnButton INTEGER
	}


w2kStatusPrnName OBJECT-TYPE
	SYNTAX DisplayString
	ACCESS read-only
	STATUS mandatory
	DESCRIPTION
		"&<caiW2kOs.768>Name of the watcher, monitoring a local printer with this name. Important: This name is case insensitive."
	::= { w2kStatusPrnEntry 1 }


w2kStatusPrnDescription OBJECT-TYPE
	SYNTAX DisplayString
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
		"&<caiW2kOs.136>Any description for this watcher."
	::= { w2kStatusPrnEntry 2 }


