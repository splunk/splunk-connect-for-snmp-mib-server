-- *****************************************************************************
-- Jobs Config Group
-- *****************************************************************************


w2kConfigJobPollInterval OBJECT-TYPE
	SYNTAX INTEGER
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
		"&<caiW2kOs.306>The time (in seconds) between the polling of the job information. Valid values are greater than or equal to 30."
	DEFVAL { 120 }
	::= { w2kConfigJobGroup 1 }


w2kConfigJobPollMethod OBJECT-TYPE
	SYNTAX INTEGER {
		disabled(1),
		poll-interval-only(2),
		poll-interval-and-query(3),
		query-only(4)
	}
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
		"&<caiW2kOs.307>If set to 'disabled', the agent no more retrieves the job information (not by polling, nor by get request) and sets all statuses to ok. If set to 'poll-interval-only', the agent only retrieves the information by polling, but not by get request. If set to 'poll-interval-and-query', the agent retrieves the information in both cases (polling and get request). If set to 'query-only', the agent retrieves the information for get requests only."
	DEFVAL { 2 }
	::= { w2kConfigJobGroup 2 }


w2kConfigJobName OBJECT-TYPE
	SYNTAX DisplayString
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
		"&<caiW2kOs.308>Name of the watcher to be added/removed, monitoring a job with this name. Important: This name is case insensitive."
	::= { w2kConfigJobGroup 3 }


w2kConfigJobDescription OBJECT-TYPE
	SYNTAX DisplayString
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
		"&<caiW2kOs.136>Any description for this watcher."
	::= { w2kConfigJobGroup 4 }


w2kConfigJobAggLag OBJECT-TYPE
	SYNTAX INTEGER
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
		"&<caiW2kOs.137>The number of consecutive poll intervals on which any status is not in the Ok or Up state, before the aggregate status changes. Valid values are greater than 0."
	DEFVAL { 1 }
	::= { w2kConfigJobGroup 5 }


w2kConfigJobExist OBJECT-TYPE
	SYNTAX INTEGER {
		should-exist(1),
		should-not-exist(2)
	}
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
		"&<caiW2kOs.311>Flag indicating whether to raise an alert if a job is existent or if it is not-existent."
	DEFVAL { 1 }
	::= { w2kConfigJobGroup 6 }


w2kConfigJobExistMonitor OBJECT-TYPE
	SYNTAX INTEGER {
		do-not-monitor(1),
		down-warning(2),
		down-critical(3)
	}
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
		"&<caiW2kOs.312>Flag indicating whether the existence of the job should be monitored and if so, the level of alert to raise."
	DEFVAL { 3 }
	::= { w2kConfigJobGroup 7 }


w2kConfigJobProcessMin OBJECT-TYPE
	SYNTAX INTEGER
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
		"&<caiW2kOs.313>Minimum threshold for the number of active processes associated with the job. Valid values are greater than or equal to -1 and smaller than or equal to the maximum threshold. Setting to -1 disables the monitoring of the minimum."
	DEFVAL { 1 }
	::= { w2kConfigJobGroup 8 }


w2kConfigJobProcessMax OBJECT-TYPE
	SYNTAX INTEGER
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
		"&<caiW2kOs.314>Maximum threshold for the number of active processes associated with the job. Valid values are greater than or equal to -1 and greater than or equal to the minimum threshold. Setting to -1 disables the monitoring of the maximum."
	DEFVAL { 10 }
	::= { w2kConfigJobGroup 9 }


w2kConfigJobProcessMonitor OBJECT-TYPE
	SYNTAX INTEGER {
		do-not-monitor(1),
		down-warning(2),
		down-critical(3)
	}
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
		"&<caiW2kOs.315>Flag indicating whether the number of active processes associated with the job should be monitored and if so, the level of alert to raise."
	DEFVAL { 2 }
	::= { w2kConfigJobGroup 10 }


w2kConfigJobCpuWarn OBJECT-TYPE
	SYNTAX INTEGER
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
		"&<caiW2kOs.316>Warning threshold for the CPU usage of the job (as percentage value of 100). Valid values are 1 to 99 and smaller than the critical threshold."
	DEFVAL { 10 }
	::= { w2kConfigJobGroup 11 }


w2kConfigJobCpuCrit OBJECT-TYPE
	SYNTAX INTEGER
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
		"&<caiW2kOs.317>Critical threshold for the CPU usage of the job (as percentage value of 100). Valid values are 2 to 100 and greater than the warning threshold."
	DEFVAL { 20 }
	::= { w2kConfigJobGroup 12 }


w2kConfigJobCpuMonitor OBJECT-TYPE
	SYNTAX INTEGER {
		do-not-monitor(1),
		warning-only(2),
		critical-only(3),
		both(4)
	}
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
		"&<caiW2kOs.318>Flag indicating whether the CPU usage of the job should be monitored and if so, which threshold should be compared with."
	DEFVAL { 4 }
	::= { w2kConfigJobGroup 13 }


w2kConfigJobButton OBJECT-TYPE
	SYNTAX INTEGER {
		none(1),
		add(2),
		remove(3)
	}
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
		"&<caiW2kOs.226>This attribute is used to add or remove a watcher with the current value of the watcher name attribute in this group."
	::= { w2kConfigJobGroup 14 }




