w2kConfigSessPollInterval OBJECT-TYPE
	SYNTAX INTEGER
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
		"&<caiW2kOs.320>The time (in seconds) between the polling of the session information. Valid values are greater than or equal to 30."
	DEFVAL { 120 }
	::= { w2kConfigSessGroup 1 }


w2kConfigSessPollMethod OBJECT-TYPE
	SYNTAX INTEGER {
		disabled(1),
		poll-interval-only(2),
		poll-interval-and-query(3),
		query-only(4)
	}
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
		"&<caiW2kOs.321>If set to 'disabled', the agent no more retrieves the session information (not by polling, nor by get request) and sets all statuses to ok. If set to 'poll-interval-only', the agent only retrieves the information by polling, but not by get request. If set to 'poll-interval-and-query', the agent retrieves the information in both cases (polling and get request). If set to 'query-only', the agent retrieves the information for get requests only."
	DEFVAL { 2 }
	::= { w2kConfigSessGroup 2 }


w2kConfigSessClientName OBJECT-TYPE
	SYNTAX DisplayString
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
		"&<caiW2kOs.322>Client name of the watcher to be added/removed, monitoring a session with this client name. The client name is the name of the host, from where this session was initiated. An asterisk '*' can be used as client name or can be prepended/appended to it, in order to widen the scope of sessions this watcher monitors. Important: This name is case insensitive."
	::= { w2kConfigSessGroup 3 }


w2kConfigSessUserName OBJECT-TYPE
	SYNTAX DisplayString
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
		"&<caiW2kOs.323>User name of the watcher to be added/removed, monitoring a session with this user name. The user name specifies the user, who logged in and is in the form 'domainName\userName'. An asterisk '*' can be used as user name or can be prepended/appended to it, in order to widen the scope of sessions this watcher monitors. Important: This name is case insensitive."
	::= { w2kConfigSessGroup 4 }


w2kConfigSessDescription OBJECT-TYPE
	SYNTAX DisplayString
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
		"&<caiW2kOs.136>Any description for this watcher."
	::= { w2kConfigSessGroup 5 }


