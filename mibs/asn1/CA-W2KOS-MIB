	SYNTAX DisplayString
	ACCESS read-only
	STATUS mandatory
	DESCRIPTION
		"&<caiW2kOs.118>The node name of the system."
	::= { w2kConfigSysGroup 1 }


w2kConfigSysVersion OBJECT-TYPE
	SYNTAX DisplayString
	ACCESS read-only
	STATUS mandatory
	DESCRIPTION
		"&<caiW2kOs.119>The OS version (build, service pack, etc.) of the system."
	::= { w2kConfigSysGroup 2 }


w2kConfigSysBios OBJECT-TYPE
	SYNTAX DisplayString
	ACCESS read-only
	STATUS mandatory
	DESCRIPTION
		"&<caiW2kOs.120>The BIOS version (date, revision, etc.) of the system."
	::= { w2kConfigSysGroup 3 }


w2kConfigSysColdStartTime OBJECT-TYPE
	SYNTAX DisplayString
	ACCESS read-only
	STATUS mandatory
	DESCRIPTION
		"&<caiW2kOs.121>The date and time that the system was last started."
	::= { w2kConfigSysGroup 4 }




-- *****************************************************************************
-- Processor Config Group
-- *****************************************************************************


w2kConfigCpuPollInterval OBJECT-TYPE
	SYNTAX INTEGER
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
		"&<caiW2kOs.122>The time (in seconds) between the polling of the processor information. Valid values are greater than or equal to 30."
	DEFVAL { 120 }
	::= { w2kConfigCpuGroup 1 }


w2kConfigCpuPollMethod OBJECT-TYPE
	SYNTAX INTEGER {
		disabled(1),
		poll-interval-only(2),
		poll-interval-and-query(3),
		query-only(4)
	}
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
		"&<caiW2kOs.123>If set to 'disabled', the agent no more retrieves the processor information (not by polling, nor by get request) and sets all statuses to ok. If set to 'poll-interval-only', the agent only retrieves the information by polling, but not by get request. If set to 'poll-interval-and-query', the agent retrieves the information in both cases (polling and get request). If set to 'query-only', the agent retrieves the information for get requests only."
	DEFVAL { 2 }
	::= { w2kConfigCpuGroup 2 }


w2kConfigCpuLoadLag OBJECT-TYPE
	SYNTAX INTEGER
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
		"&<caiW2kOs.124>The number of consecutive poll intervals that the threshold has to be (b)reached before the status changes. Valid values are greater than 0."
	DEFVAL { 3 }
	::= { w2kConfigCpuGroup 3 }


w2kConfigCpuLoadWarn OBJECT-TYPE
	SYNTAX INTEGER
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
		"&<caiW2kOs.125>Warning threshold for the utilization of the processor (as percentage value of 100). Valid values are 1 to 99 and smaller than the critical threshold."
	DEFVAL { 70 }
	::= { w2kConfigCpuGroup 4 }


w2kConfigCpuLoadCrit OBJECT-TYPE
	SYNTAX INTEGER
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
		"&<caiW2kOs.126>Critical threshold for the utilization of the processor (as percentage value of 100). Valid values are 2 to 100 and greater than the warning threshold."
	DEFVAL { 90 }
	::= { w2kConfigCpuGroup 5 }


w2kConfigCpuLoadMonitor OBJECT-TYPE
	SYNTAX INTEGER {
		do-not-monitor(1),
		warning-only(2),
		critical-only(3),
		both(4)
	}
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
		"&<caiW2kOs.127>Flag indicating whether the utilization of the processor should be monitored and if so, which threshold should be compared with."
	DEFVAL { 4 }
	::= { w2kConfigCpuGroup 6 }


w2kConfigCpuLossAction OBJECT-TYPE
	SYNTAX INTEGER {
		remove(1),
		up(2),
		down-warning(3),
		down-critical(4)
	}
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
		"&<caiW2kOs.128>Flag defining the severity if the loss of the processor is detected or whether to remove this watcher in that case."
	DEFVAL { 1 }
	::= { w2kConfigCpuGroup 7 }




-- *****************************************************************************
-- Memory Config Group
-- *****************************************************************************


w2kConfigMemPollInterval OBJECT-TYPE
	SYNTAX INTEGER
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
		"&<caiW2kOs.129>The time (in seconds) between the polling of the memory information. Valid values are greater than or equal to 30."
	DEFVAL { 120 }
	::= { w2kConfigMemGroup 1 }


w2kConfigMemPollMethod OBJECT-TYPE
	SYNTAX INTEGER {
		disabled(1),
		poll-interval-only(2),
		poll-interval-and-query(3),
		query-only(4)
	}
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
		"&<caiW2kOs.130>If set to 'disabled', the agent no more retrieves the memory information (not by polling, nor by get request) and sets all statuses to ok. If set to 'poll-interval-only', the agent only retrieves the information by polling, but not by get request. If set to 'poll-interval-and-query', the agent retrieves the information in both cases (polling and get request). If set to 'query-only', the agent retrieves the information for get requests only."
	DEFVAL { 2 }
	::= { w2kConfigMemGroup 2 }




-- *****************************************************************************
-- Logical Volumes Config Group
-- *****************************************************************************


w2kConfigLVolPollInterval OBJECT-TYPE
	SYNTAX INTEGER
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
		"&<caiW2kOs.131>The time (in seconds) between the polling of the logical volume information. Valid values are greater than or equal to 30."
	DEFVAL { 120 }
	::= { w2kConfigLVolGroup 1 }


w2kConfigLVolPollMethod OBJECT-TYPE
	SYNTAX INTEGER {
		disabled(1),
		poll-interval-only(2),
		poll-interval-and-query(3),
		query-only(4)
	}
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
		"&<caiW2kOs.132>If set to 'disabled', the agent no more retrieves the logical volume information (not by polling, nor by get request) and sets all statuses to ok. If set to 'poll-interval-only', the agent only retrieves the information by polling, but not by get request. If set to 'poll-interval-and-query', the agent retrieves the information in both cases (polling and get request). If set to 'query-only', the agent retrieves the information for get requests only."
	DEFVAL { 2 }
	::= { w2kConfigLVolGroup 2 }


w2kConfigLVolAutoPollInterval OBJECT-TYPE
	SYNTAX INTEGER
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
		"&<caiW2kOs.133>The time (in minutes), when a new auto discovery process is started. Valid values are greater than or equal to 30 or set to -1. If set to -1, auto discovery of logical volumes will no more be made."
	DEFVAL { 60 }
	::= { w2kConfigLVolGroup 3 }


w2kConfigLVolFragPollInterval OBJECT-TYPE
	SYNTAX INTEGER
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
		"&<caiW2kOs.134>The time (in hours), when the fragmentation determination of the watched logical volumes is started again (triggered by the next polling or a get request). The agent will not determine the fragmentation every standard poll interval, because it is a long lasting task. Valid values are greater than or equal to 1 or set to -1. After a set on this attribute, the fragmentation determination will be started new by the next polling or by a get request. If set to -1, the fragmentation is no more polled."
	DEFVAL { 8 }
	::= { w2kConfigLVolGroup 4 }


w2kConfigLVolName OBJECT-TYPE
	SYNTAX DisplayString
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
		"&<caiW2kOs.135>Name of the watcher to be added/removed, monitoring a logical volume with this name. If adding an auto discovery watcher, this name can be a wildcarded logical volume name filtering the logical volumes to be auto discovered (any backslash in the wildcarded name must be doubled). For each auto discovered logical volume a watcher will be created. Important: This name is case insensitive."
	::= { w2kConfigLVolGroup 5 }


w2kConfigLVolDescription OBJECT-TYPE
	SYNTAX DisplayString
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
		"&<caiW2kOs.136>Any description for this watcher."
	::= { w2kConfigLVolGroup 6 }


w2kConfigLVolAggLag OBJECT-TYPE
	SYNTAX INTEGER
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
		"&<caiW2kOs.137>The number of consecutive poll intervals on which any status is not in the Ok or Up state, before the aggregate status changes. Valid values are greater than 0."
	DEFVAL { 1 }
	::= { w2kConfigLVolGroup 7 }


w2kConfigLVolSizeWarn OBJECT-TYPE
	SYNTAX DisplayString
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
		"&<caiW2kOs.138>Warning threshold in megabytes, either for the free (by appending a 'F') or for the used logical volume size. The used size can also be defined as a percentage value of the total logical volume size (by appending a '%'). Valid values are greater than 0 and smaller (in case of 'free' greater) than the critical threshold."
	DEFVAL { "70%" }
	::= { w2kConfigLVolGroup 8 }


w2kConfigLVolSizeCrit OBJECT-TYPE
	SYNTAX DisplayString
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
		"&<caiW2kOs.139>Critical threshold in megabytes, either for the free (by appending a 'F') or for the used logical volume size. The used size can also be defined as a percentage value of the total logical volume size (by appending a '%'). Valid values are greater than 1 and greater (in case of 'free' smaller) than the warning threshold."
	DEFVAL { "90%" }
	::= { w2kConfigLVolGroup 9 }


w2kConfigLVolSizeMonitor OBJECT-TYPE
	SYNTAX INTEGER {
		do-not-monitor(1),
		warning-only(2),
		critical-only(3),
		both(4)
	}
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
		"&<caiW2kOs.140>Flag indicating whether the used logical volume size should be monitored and if so, which threshold should be compared with."
	DEFVAL { 4 }
	::= { w2kConfigLVolGroup 10 }


w2kConfigLVolSizeDWarn OBJECT-TYPE
	SYNTAX DisplayString
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
		"&<caiW2kOs.141>Warning threshold, either for the growth (by appending a '+') or for the shrinkage (by appending a '-') or for the absolute delta in used logical volume size monitored in one poll interval. Growth, shrinkage and absolute delta can be given in megabytes or as a percentage value of the total logical volume size (by appending a '%'). Valid values are greater than 0 and smaller than the critical threshold."
	DEFVAL { "1%" }
	::= { w2kConfigLVolGroup 11 }


w2kConfigLVolSizeDCrit OBJECT-TYPE
	SYNTAX DisplayString
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
		"&<caiW2kOs.142>Critical threshold, either for the growth (by appending a '+') or for the shrinkage (by appending a '-') or for the absolute delta in used logical volume size monitored in one poll interval. Growth, shrinkage and absolute delta can be given in megabytes or as a percentage value of the total logical volume size (by appending a '%'). Valid values are greater than 1 and greater than the warning threshold."
	DEFVAL { "2%" }
	::= { w2kConfigLVolGroup 12 }


w2kConfigLVolSizeDMonitor OBJECT-TYPE
	SYNTAX INTEGER {
		do-not-monitor(1),
		warning-only(2),
		critical-only(3),
		both(4)
	}
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
		"&<caiW2kOs.143>Flag indicating whether the delta in used logical volume size should be monitored and if so, which threshold should be compared with."
	DEFVAL { 4 }
	::= { w2kConfigLVolGroup 13 }


w2kConfigLVolTPutWarn OBJECT-TYPE
	SYNTAX INTEGER
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
		"&<caiW2kOs.144>Warning threshold for the throughput rate (kilobytes per second). Valid values are greater than 0 and smaller than the critical threshold."
	DEFVAL { 400 }
	::= { w2kConfigLVolGroup 14 }


w2kConfigLVolTPutCrit OBJECT-TYPE
	SYNTAX INTEGER
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
		"&<caiW2kOs.145>Critical threshold for the throughput rate (kilobytes per second). Valid values are greater than 1 and greater than the warning threshold."
	DEFVAL { 800 }
	::= { w2kConfigLVolGroup 15 }


w2kConfigLVolTPutMonitor OBJECT-TYPE
	SYNTAX INTEGER {
		do-not-monitor(1),
		warning-only(2),
		critical-only(3),
		both(4)
	}
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
		"&<caiW2kOs.146>Flag indicating whether the throughput rate should be monitored and if so, which threshold should be compared with."
	DEFVAL { 4 }
	::= { w2kConfigLVolGroup 16 }


w2kConfigLVolFragWarn OBJECT-TYPE
	SYNTAX INTEGER
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
		"&<caiW2kOs.147>Warning threshold for the file fragmentation (as percentage value of 100). Valid values are 1 to 99 and smaller than the critical threshold."
	DEFVAL { 30 }
	::= { w2kConfigLVolGroup 17 }


w2kConfigLVolFragCrit OBJECT-TYPE
	SYNTAX INTEGER
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
		"&<caiW2kOs.148>Critical threshold for the file fragmentation (as percentage value of 100). Valid values are 2 to 100 and greater than the warning threshold."
	DEFVAL { 50 }
	::= { w2kConfigLVolGroup 18 }


w2kConfigLVolFragMonitor OBJECT-TYPE
	SYNTAX INTEGER {
		do-not-monitor(1),
		warning-only(2),
		critical-only(3),
		both(4)
	}
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
		"&<caiW2kOs.149>Flag indicating whether the file fragmentation should be monitored and if so, which threshold should be compared with."
	DEFVAL { 4 }
	::= { w2kConfigLVolGroup 19 }


w2kConfigLVolLossAction OBJECT-TYPE
	SYNTAX INTEGER {
		remove(1),
		up(2),
		down-warning(3),
		down-critical(4)
	}
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
		"&<caiW2kOs.150>Flag defining the severity if the loss of the logical volume is detected or whether to remove this watcher in that case."
	DEFVAL { 4 }
	::= { w2kConfigLVolGroup 20 }


w2kConfigLVolAutoTableWatcherName OBJECT-TYPE
	SYNTAX DisplayString
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
		"&<caiW2kOs.151>Name of the watcher to be added in the auto discovery table."
	::= { w2kConfigLVolGroup 21 }


w2kConfigLVolButton OBJECT-TYPE
	SYNTAX INTEGER {
		none(1),
		add(2),
		remove(3),
		add-auto(4),
		remove-auto(5)
	}
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
		"&<caiW2kOs.152>This attribute is used to add or remove a watcher with the current value of the watcher name attribute in this group, or to add or remove an auto discovery watcher with the current value of the auto table watcher name attribute in this group."
	::= { w2kConfigLVolGroup 22 }


w2kConfigLVolAutoTable OBJECT-TYPE
	SYNTAX SEQUENCE OF W2kConfigLVolAutoRow
	ACCESS not-accessible
	STATUS mandatory
	DESCRIPTION
		"&<caiW2kOs.153>A table containing all auto discovery watchers for logical volumes."
	::= { w2kConfigLVolGroup 23 }


w2kConfigLVolAutoEntry OBJECT-TYPE
	SYNTAX W2kConfigLVolAutoRow
	ACCESS not-accessible
	STATUS mandatory
	DESCRIPTION
		"&<caiW2kOs.154>The table entry."
	INDEX { w2kConfigLVolAutoWatcherName }
	::= { w2kConfigLVolAutoTable 1 }


W2kConfigLVolAutoRow ::=
	SEQUENCE {
		w2kConfigLVolAutoWatcherName DisplayString,
		w2kConfigLVolAutoName DisplayString,
		w2kConfigLVolAutoDescription DisplayString,
		w2kConfigLVolAutoAggLag INTEGER,
		w2kConfigLVolAutoSizeWarn DisplayString,
		w2kConfigLVolAutoSizeCrit DisplayString,
		w2kConfigLVolAutoSizeMonitor INTEGER,
		w2kConfigLVolAutoSizeDWarn DisplayString,
		w2kConfigLVolAutoSizeDCrit DisplayString,
		w2kConfigLVolAutoSizeDMonitor INTEGER,
		w2kConfigLVolAutoTPutWarn INTEGER,
		w2kConfigLVolAutoTPutCrit INTEGER,
		w2kConfigLVolAutoTPutMonitor INTEGER,
		w2kConfigLVolAutoFragWarn INTEGER,
		w2kConfigLVolAutoFragCrit INTEGER,
		w2kConfigLVolAutoFragMonitor INTEGER,
		w2kConfigLVolAutoLossAction INTEGER,
		w2kConfigLVolAutoButton INTEGER
	}


