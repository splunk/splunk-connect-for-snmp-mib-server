-- *****************************************************************************
-- Logical Volumes Status Group
-- *****************************************************************************


w2kStatusLVolCount OBJECT-TYPE
	SYNTAX INTEGER
	ACCESS read-only
	STATUS mandatory
	DESCRIPTION
		"&<caiW2kOs.491>The number of entries in the w2kStatusLVolTable."
	::= { w2kStatusLVolGroup 1 }


w2kStatusLVolTable OBJECT-TYPE
	SYNTAX SEQUENCE OF W2kStatusLVolRow
	ACCESS not-accessible
	STATUS mandatory
	DESCRIPTION
		"&<caiW2kOs.492>A table containing information about all monitored logical volumes."
	::= { w2kStatusLVolGroup 2 }


w2kStatusLVolEntry OBJECT-TYPE
	SYNTAX W2kStatusLVolRow
	ACCESS not-accessible
	STATUS mandatory
	DESCRIPTION
		"&<caiW2kOs.154>The table entry."
	INDEX { w2kStatusLVolName }
	::= { w2kStatusLVolTable 1 }


W2kStatusLVolRow ::=
	SEQUENCE {
		w2kStatusLVolName DisplayString,
		w2kStatusLVolDescription DisplayString,
		w2kStatusLVolMounts DisplayString,
		w2kStatusLVolInfo DisplayString,
		w2kStatusLVolAutoWatcherName DisplayString,
		w2kStatusLVolAggLagValue INTEGER,
		w2kStatusLVolAggLag INTEGER,
		w2kStatusLVolAggStatus INTEGER,
		w2kStatusLVolSizeTotal INTEGER,
		w2kStatusLVolSizeValue INTEGER,
		w2kStatusLVolSizeWarnValue INTEGER,
		w2kStatusLVolSizeCritValue INTEGER,
		w2kStatusLVolSizeWarn DisplayString,
		w2kStatusLVolSizeCrit DisplayString,
		w2kStatusLVolSizeMonitor INTEGER,
		w2kStatusLVolSizeStatus INTEGER,
		w2kStatusLVolSizeDValue INTEGER,
		w2kStatusLVolSizeDWarnValue DisplayString,
		w2kStatusLVolSizeDCritValue DisplayString,
		w2kStatusLVolSizeDWarn DisplayString,
		w2kStatusLVolSizeDCrit DisplayString,
		w2kStatusLVolSizeDMonitor INTEGER,
		w2kStatusLVolSizeDStatus INTEGER,
		w2kStatusLVolTPutValue INTEGER,
		w2kStatusLVolTPutWarn INTEGER,
		w2kStatusLVolTPutCrit INTEGER,
		w2kStatusLVolTPutMonitor INTEGER,
		w2kStatusLVolTPutStatus INTEGER,
		w2kStatusLVolFragValue INTEGER,
		w2kStatusLVolFragWarn INTEGER,
		w2kStatusLVolFragCrit INTEGER,
		w2kStatusLVolFragMonitor INTEGER,
		w2kStatusLVolFragStatus INTEGER,
		w2kStatusLVolLossAction INTEGER,
		w2kStatusLVolLossStatus INTEGER,
		w2kStatusLVolCallBackRef DisplayString,
		w2kStatusLVolButton INTEGER
	}


w2kStatusLVolName OBJECT-TYPE
	SYNTAX DisplayString
	ACCESS read-only
	STATUS mandatory
	DESCRIPTION
		"&<caiW2kOs.494>Name of the watcher, monitoring a logical volume with this name. Important: This name is case insensitive."
	::= { w2kStatusLVolEntry 1 }


w2kStatusLVolDescription OBJECT-TYPE
	SYNTAX DisplayString
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
		"&<caiW2kOs.136>Any description for this watcher."
	::= { w2kStatusLVolEntry 2 }


w2kStatusLVolMounts OBJECT-TYPE
	SYNTAX DisplayString
	ACCESS read-only
	STATUS mandatory
	DESCRIPTION
		"&<caiW2kOs.496>A list of the mount points, where this logical volume is mounted on."
	::= { w2kStatusLVolEntry 3 }


w2kStatusLVolInfo OBJECT-TYPE
	SYNTAX DisplayString
	ACCESS read-only
	STATUS mandatory
	DESCRIPTION
		"&<caiW2kOs.497>Information about the format (e.g. NTFS), the alternate name (e.g. used in PerfMon) and the label of the logical volume. The alternate name may be empty. The single values are delimited by a pipe sign."
	::= { w2kStatusLVolEntry 4 }


w2kStatusLVolAutoWatcherName OBJECT-TYPE
	SYNTAX DisplayString
