	SYNTAX DisplayString
	ACCESS read-only
	STATUS mandatory
	DESCRIPTION
		"&<caiW2kOs.498>Name of the auto discovery watcher, which created this watcher."
	::= { w2kStatusPrnEntry 3 }


w2kStatusPrnAggLagValue OBJECT-TYPE
	SYNTAX INTEGER
	ACCESS read-only
	STATUS mandatory
	DESCRIPTION
		"&<caiW2kOs.499>The number of consecutive poll intervals on which any status was not in the Ok or Up state."
	::= { w2kStatusPrnEntry 4 }


w2kStatusPrnAggLag OBJECT-TYPE
	SYNTAX INTEGER
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
		"&<caiW2kOs.137>The number of consecutive poll intervals on which any status is not in the Ok or Up state, before the aggregate status changes. Valid values are greater than 0."
	::= { w2kStatusPrnEntry 5 }


w2kStatusPrnAggStatus OBJECT-TYPE
	SYNTAX INTEGER {
		unknown(1),
		ok(2),
		warning(3),
		critical(4)
	}
	ACCESS read-only
	STATUS mandatory
	DESCRIPTION
		"&<caiW2kOs.773>The current status of the printer as a 'worst case' aggregate of all its statuses."
	::= { w2kStatusPrnEntry 6 }


w2kStatusPrnEventDescr OBJECT-TYPE
	SYNTAX INTEGER {
		unknown-no-print-jobs(1),
		printing(2),
		paper-error(3),
		toner-error(4),
		manual(5),
		offline(6),
		paused(7),
		software-error(8),
		general-error(9)
	}
	ACCESS read-only
	STATUS mandatory
	DESCRIPTION
		"&<caiW2kOs.774>The description of the last printer event. Note: Printer events can only be determined if there is a printing job in the printers queue."
	::= { w2kStatusPrnEntry 7 }


w2kStatusPrnEventMonitor OBJECT-TYPE
	SYNTAX INTEGER {
		do-not-monitor(1),
		down-warning(2),
		down-critical(3)
	}
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
		"&<caiW2kOs.342>Flag indicating whether the occurence of printer events should be monitored and if so, the level of alert to raise."
	DEFVAL { 0 }
	::= { w2kStatusPrnEntry 8 }


w2kStatusPrnEventStatus OBJECT-TYPE
	SYNTAX INTEGER {
		unknown(1),
		up(2),
		down-warning(3),
		down-critical(4)
	}
	ACCESS read-only
	STATUS mandatory
	DESCRIPTION
		"&<caiW2kOs.776>The current status of the printer event monitoring. Note: Printer events can only be determined if there is a printing job in the printers queue. The status 'unknown' means that the current value could not be retrieved from the system."
	::= { w2kStatusPrnEntry 9 }


w2kStatusPrnQueueValue OBJECT-TYPE
	SYNTAX INTEGER
	ACCESS read-only
	STATUS mandatory
	DESCRIPTION
		"&<caiW2kOs.777>The current value of jobs in the printer's queue."
	::= { w2kStatusPrnEntry 10 }


