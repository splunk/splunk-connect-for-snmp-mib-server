	SYNTAX INTEGER {
		none(1),
		remove(2),
		reset-relTo(3)
	}
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
		"&<caiW2kOs.547>This attribute is used to remove this watcher or to reset the 'related to' reference to the current value of the mount."
	::= { w2kStatusMntEntry 14 }




-- *****************************************************************************
-- Distributed File Systems Status Group
-- *****************************************************************************


w2kStatusDfsCount OBJECT-TYPE
	SYNTAX INTEGER
	ACCESS read-only
	STATUS mandatory
	DESCRIPTION
		"&<caiW2kOs.548>The number of entries in the w2kStatusDfsTable."
	::= { w2kStatusDfsGroup 1 }


w2kStatusDfsTable OBJECT-TYPE
	SYNTAX SEQUENCE OF W2kStatusDfsRow
	ACCESS not-accessible
	STATUS mandatory
	DESCRIPTION
		"&<caiW2kOs.549>A table containing information about all monitored distributed file system links."
	::= { w2kStatusDfsGroup 2 }


w2kStatusDfsEntry OBJECT-TYPE
	SYNTAX W2kStatusDfsRow
	ACCESS not-accessible
	STATUS mandatory
	DESCRIPTION
		"&<caiW2kOs.154>The table entry."
	INDEX { w2kStatusDfsName }
	::= { w2kStatusDfsTable 1 }


W2kStatusDfsRow ::=
	SEQUENCE {
		w2kStatusDfsName DisplayString,
		w2kStatusDfsDescription DisplayString,
		w2kStatusDfsIds DisplayString,
		w2kStatusDfsAutoWatcherName DisplayString,
		w2kStatusDfsAggLagValue INTEGER,
		w2kStatusDfsAggLag INTEGER,
		w2kStatusDfsAggStatus INTEGER,
		w2kStatusDfsReplicaTotal INTEGER,
		w2kStatusDfsReplicaValue INTEGER,
		w2kStatusDfsReplicaWarn INTEGER,
		w2kStatusDfsReplicaCrit INTEGER,
		w2kStatusDfsReplicaMonitor INTEGER,
		w2kStatusDfsReplicaStatus INTEGER,
		w2kStatusDfsLossAction INTEGER,
		w2kStatusDfsLossStatus INTEGER,
		w2kStatusDfsCallBackRef DisplayString,
		w2kStatusDfsButton INTEGER
	}


w2kStatusDfsName OBJECT-TYPE
	SYNTAX DisplayString
	ACCESS read-only
	STATUS mandatory
	DESCRIPTION
		"&<caiW2kOs.551>Name of the watcher, monitoring a distributed file system link with this name. The name consists of the Dfs root name followed by the Dfs link name. These two names are concatenated by a back-slash. The single Dfs root name (without '\DfsLinkName') can also be used as watcher name, to be able to monitor all Dfs links of a Dfs root. In that case no loss action will be triggered if no Dfs link exists. Important: This name is case insensitive."
	::= { w2kStatusDfsEntry 1 }


w2kStatusDfsDescription OBJECT-TYPE
	SYNTAX DisplayString
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
		"&<caiW2kOs.136>Any description for this watcher."
	::= { w2kStatusDfsEntry 2 }


w2kStatusDfsIds OBJECT-TYPE
	SYNTAX DisplayString
	ACCESS read-only
	STATUS mandatory
	DESCRIPTION
		"&<caiW2kOs.553>A pipe sign separated list of those replicas of the distributed file system link, that are unavailable."
	::= { w2kStatusDfsEntry 3 }


w2kStatusDfsAutoWatcherName OBJECT-TYPE
	SYNTAX DisplayString
	ACCESS read-only
	STATUS mandatory
	DESCRIPTION
		"&<caiW2kOs.498>Name of the auto discovery watcher, which created this watcher."
	::= { w2kStatusDfsEntry 4 }


w2kStatusDfsAggLagValue OBJECT-TYPE
	SYNTAX INTEGER
	ACCESS read-only
	STATUS mandatory
	DESCRIPTION
		"&<caiW2kOs.499>The number of consecutive poll intervals on which any status was not in the Ok or Up state."
	::= { w2kStatusDfsEntry 5 }


