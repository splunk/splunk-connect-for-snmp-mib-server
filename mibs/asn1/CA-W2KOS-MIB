-- *****************************************************************************


w2kConfigSrvcPollInterval OBJECT-TYPE
	SYNTAX INTEGER
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
		"&<caiW2kOs.283>The time (in seconds) between the polling of the service information. Valid values are greater than or equal to 30."
	DEFVAL { 120 }
	::= { w2kConfigSrvcGroup 1 }


w2kConfigSrvcPollMethod OBJECT-TYPE
	SYNTAX INTEGER {
		disabled(1),
		poll-interval-only(2),
		poll-interval-and-query(3),
		query-only(4)
	}
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
		"&<caiW2kOs.284>If set to 'disabled', the agent no more retrieves the service information (not by polling, nor by get request) and sets all statuses to ok. If set to 'poll-interval-only', the agent only retrieves the information by polling, but not by get request. If set to 'poll-interval-and-query', the agent retrieves the information in both cases (polling and get request). If set to 'query-only', the agent retrieves the information for get requests only."
	DEFVAL { 2 }
	::= { w2kConfigSrvcGroup 2 }


w2kConfigSrvcAutoPollInterval OBJECT-TYPE
	SYNTAX INTEGER
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
		"&<caiW2kOs.285>The time (in minutes), when a new auto discovery process is started. Valid values are greater than or equal to 30 or set to -1. If set to -1, auto discovery of services will no more be made."
	DEFVAL { 60 }
	::= { w2kConfigSrvcGroup 3 }


w2kConfigSrvcName OBJECT-TYPE
	SYNTAX DisplayString
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
		"&<caiW2kOs.286>Name of the watcher to be added/removed, monitoring a service with this name. If adding an auto discovery watcher, this name can be a wildcarded service name filtering the services to be auto discovered (any backslash in the wildcarded name must be doubled). For each auto discovered service a watcher will be created. Important: This name is case insensitive."
	::= { w2kConfigSrvcGroup 4 }


w2kConfigSrvcDescription OBJECT-TYPE
	SYNTAX DisplayString
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
		"&<caiW2kOs.136>Any description for this watcher."
	::= { w2kConfigSrvcGroup 5 }


w2kConfigSrvcAggLag OBJECT-TYPE
	SYNTAX INTEGER
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
		"&<caiW2kOs.137>The number of consecutive poll intervals on which any status is not in the Ok or Up state, before the aggregate status changes. Valid values are greater than 0."
	DEFVAL { 1 }
	::= { w2kConfigSrvcGroup 6 }


w2kConfigSrvcExist OBJECT-TYPE
	SYNTAX INTEGER {
		should-exist(1),
		should-not-exist(2)
	}
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
		"&<caiW2kOs.289>Flag indicating whether to raise an alert if a service is existent or if it is not-existent."
	DEFVAL { 1 }
	::= { w2kConfigSrvcGroup 7 }


w2kConfigSrvcExistMonitor OBJECT-TYPE
	SYNTAX INTEGER {
		do-not-monitor(1),
		down-warning(2),
		down-critical(3)
	}
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
		"&<caiW2kOs.290>Flag indicating whether the existence of the service should be monitored and if so, the level of alert to raise."
	DEFVAL { 3 }
	::= { w2kConfigSrvcGroup 8 }


w2kConfigSrvcActive OBJECT-TYPE
	SYNTAX INTEGER {
		should-be-active(1),
		should-not-be-active(2)
	}
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
		"&<caiW2kOs.291>Flag indicating whether to raise an alert if an existent service is active or if it is inactive."
	DEFVAL { 1 }
	::= { w2kConfigSrvcGroup 9 }


w2kConfigSrvcActiveMonitor OBJECT-TYPE
	SYNTAX INTEGER {
		do-not-monitor(1),
		down-warning(2),
		down-critical(3)
	}
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
		"&<caiW2kOs.292>Flag indicating whether the activity of the service should be monitored and if so, the level of alert to raise."
	DEFVAL { 2 }
	::= { w2kConfigSrvcGroup 10 }


w2kConfigSrvcAutoTableWatcherName OBJECT-TYPE
	SYNTAX DisplayString
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
		"&<caiW2kOs.151>Name of the watcher to be added in the auto discovery table."
	::= { w2kConfigSrvcGroup 11 }


w2kConfigSrvcButton OBJECT-TYPE
	SYNTAX INTEGER {
		none(1),
		add(2),
		remove(3),
		add-auto(4),
		remove-auto(5)
	}
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
		"&<caiW2kOs.152>This attribute is used to add or remove a watcher with the current value of the watcher name attribute in this group, or to add or remove an auto discovery watcher with the current value of the auto table watcher name attribute in this group."
	::= { w2kConfigSrvcGroup 12 }


w2kConfigSrvcAutoTable OBJECT-TYPE
	SYNTAX SEQUENCE OF W2kConfigSrvcAutoRow
	ACCESS not-accessible
	STATUS mandatory
	DESCRIPTION
		"&<caiW2kOs.295>A table containing all auto discovery watchers for services."
	::= { w2kConfigSrvcGroup 13 }


w2kConfigSrvcAutoEntry OBJECT-TYPE
	SYNTAX W2kConfigSrvcAutoRow
	ACCESS not-accessible
	STATUS mandatory
	DESCRIPTION
		"&<caiW2kOs.154>The table entry."
	INDEX { w2kConfigSrvcAutoWatcherName }
	::= { w2kConfigSrvcAutoTable 1 }


W2kConfigSrvcAutoRow ::=
	SEQUENCE {
		w2kConfigSrvcAutoWatcherName DisplayString,
		w2kConfigSrvcAutoName DisplayString,
		w2kConfigSrvcAutoDescription DisplayString,
		w2kConfigSrvcAutoAggLag INTEGER,
		w2kConfigSrvcAutoExist INTEGER,
		w2kConfigSrvcAutoExistMonitor INTEGER,
		w2kConfigSrvcAutoActive INTEGER,
		w2kConfigSrvcAutoActiveMonitor INTEGER,
		w2kConfigSrvcAutoButton INTEGER
	}


w2kConfigSrvcAutoWatcherName OBJECT-TYPE
	SYNTAX DisplayString
	ACCESS read-only
	STATUS mandatory
	DESCRIPTION
		"&<caiW2kOs.155>Name of the auto discovery watcher."
	::= { w2kConfigSrvcAutoEntry 1 }


w2kConfigSrvcAutoName OBJECT-TYPE
	SYNTAX DisplayString
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
		"&<caiW2kOs.298>Wildcarded service name filtering the services to be auto discovered (any backslash in the wildcarded name must be doubled). For each auto discovered service a watcher will be created. Important: This name is case insensitive."
	::= { w2kConfigSrvcAutoEntry 2 }


w2kConfigSrvcAutoDescription OBJECT-TYPE
	SYNTAX DisplayString
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
		"&<caiW2kOs.299>Default for auto discovered services."
	::= { w2kConfigSrvcAutoEntry 3 }


w2kConfigSrvcAutoAggLag OBJECT-TYPE
	SYNTAX INTEGER
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
		"&<caiW2kOs.299>Default for auto discovered services."
	::= { w2kConfigSrvcAutoEntry 4 }


