	SYNTAX INTEGER {
		none(1),
		remove(2)
	}
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
		"&<caiW2kOs.530>This attribute is used to remove this watcher."
	::= { w2kStatusDfsEntry 17 }




