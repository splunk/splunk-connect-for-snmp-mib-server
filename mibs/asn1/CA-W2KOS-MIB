-- *****************************************************************************
-- Processes Status Group
-- *****************************************************************************


w2kStatusProcCount OBJECT-TYPE
	SYNTAX INTEGER
	ACCESS read-only
	STATUS mandatory
	DESCRIPTION
		"&<caiW2kOs.656>The number of entries in the w2kStatusProcTable."
	::= { w2kStatusProcGroup 1 }


w2kStatusProcTable OBJECT-TYPE
	SYNTAX SEQUENCE OF W2kStatusProcRow
	ACCESS not-accessible
	STATUS mandatory
	DESCRIPTION
		"&<caiW2kOs.657>A table containing information about all monitored processes."
	::= { w2kStatusProcGroup 2 }


w2kStatusProcEntry OBJECT-TYPE
	SYNTAX W2kStatusProcRow
	ACCESS not-accessible
	STATUS mandatory
	DESCRIPTION
		"&<caiW2kOs.154>The table entry."
	INDEX { w2kStatusProcProcName, w2kStatusProcPathName, w2kStatusProcUserName }
	::= { w2kStatusProcTable 1 }


W2kStatusProcRow ::=
	SEQUENCE {
		w2kStatusProcProcName DisplayString,
		w2kStatusProcPathName DisplayString,
		w2kStatusProcUserName DisplayString,
		w2kStatusProcDescription DisplayString,
		w2kStatusProcIds DisplayString,
		w2kStatusProcAggLagValue INTEGER,
		w2kStatusProcAggLag INTEGER,
		w2kStatusProcAggStatus INTEGER,
		w2kStatusProcInstValue INTEGER,
		w2kStatusProcInstMin INTEGER,
		w2kStatusProcInstMax INTEGER,
		w2kStatusProcInstMonitor INTEGER,
		w2kStatusProcInstStatus INTEGER,
		w2kStatusProcChildMinValue INTEGER,
		w2kStatusProcChildMaxValue INTEGER,
		w2kStatusProcChildMin INTEGER,
		w2kStatusProcChildMax INTEGER,
		w2kStatusProcChildMonitor INTEGER,
		w2kStatusProcChildStatus INTEGER,
		w2kStatusProcThreadMinValue INTEGER,
		w2kStatusProcThreadMaxValue INTEGER,
		w2kStatusProcThreadMin INTEGER,
		w2kStatusProcThreadMax INTEGER,
		w2kStatusProcThreadMonitor INTEGER,
		w2kStatusProcThreadStatus INTEGER,
		w2kStatusProcMemoryValue INTEGER,
		w2kStatusProcMemoryWarn INTEGER,
		w2kStatusProcMemoryCrit INTEGER,
		w2kStatusProcMemoryMonitor INTEGER,
		w2kStatusProcMemoryStatus INTEGER,
		w2kStatusProcCpuValue INTEGER,
		w2kStatusProcCpuWarn INTEGER,
		w2kStatusProcCpuCrit INTEGER,
		w2kStatusProcCpuMonitor INTEGER,
		w2kStatusProcCpuStatus INTEGER,
		w2kStatusProcCallBackRef DisplayString,
		w2kStatusProcButton INTEGER
	}


w2kStatusProcProcName OBJECT-TYPE
	SYNTAX DisplayString
	ACCESS read-only
	STATUS mandatory
	DESCRIPTION
		"&<caiW2kOs.659>Process name of the watcher, monitoring a process with this process name. An asterisk '*' can be used as process name or can be prepended/appended to it, in order to widen the scope of processes this watcher monitors. Important: This name is case insensitive."
	::= { w2kStatusProcEntry 1 }


w2kStatusProcPathName OBJECT-TYPE
	SYNTAX DisplayString
	ACCESS read-only
	STATUS mandatory
	DESCRIPTION
		"&<caiW2kOs.660>Path name of the watcher, monitoring a process with this path name. An asterisk '*' can be used as path name or can be prepended/appended to it, in order to widen the scope of processes this watcher monitors. Important: This name is case insensitive."
	::= { w2kStatusProcEntry 2 }


w2kStatusProcUserName OBJECT-TYPE
	SYNTAX DisplayString
	ACCESS read-only
	STATUS mandatory
	DESCRIPTION
		"&<caiW2kOs.661>User name of the watcher, monitoring a process started by this user. An asterisk '*' can be used as user name or can be prepended/appended to it, in order to widen the scope of processes this watcher monitors. Important: This name is case insensitive."
	::= { w2kStatusProcEntry 3 }


w2kStatusProcDescription OBJECT-TYPE
	SYNTAX DisplayString
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
		"&<caiW2kOs.136>Any description for this watcher."
	::= { w2kStatusProcEntry 4 }


w2kStatusProcIds OBJECT-TYPE
	SYNTAX DisplayString
	ACCESS read-only
	STATUS mandatory
	DESCRIPTION
		"&<caiW2kOs.663>A pipe sign separated list of the ids of those processes, that (b)reach any threshold (except instance metric)."
	::= { w2kStatusProcEntry 5 }


w2kStatusProcAggLagValue OBJECT-TYPE
	SYNTAX INTEGER
	ACCESS read-only
	STATUS mandatory
	DESCRIPTION
		"&<caiW2kOs.499>The number of consecutive poll intervals on which any status was not in the Ok or Up state."
	::= { w2kStatusProcEntry 6 }


w2kStatusProcAggLag OBJECT-TYPE
	SYNTAX INTEGER
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
		"&<caiW2kOs.137>The number of consecutive poll intervals on which any status is not in the Ok or Up state, before the aggregate status changes. Valid values are greater than 0."
	::= { w2kStatusProcEntry 7 }


w2kStatusProcAggStatus OBJECT-TYPE
	SYNTAX INTEGER {
		unknown(1),
		ok(2),
		warning(3),
		critical(4)
	}
	ACCESS read-only
	STATUS mandatory
	DESCRIPTION
		"&<caiW2kOs.666>The current status of the process as a 'worst case' aggregate of all its statuses."
	::= { w2kStatusProcEntry 8 }


