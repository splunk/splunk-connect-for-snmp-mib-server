		w2kStatusQuotSizeWarn INTEGER,
		w2kStatusQuotSizeCrit INTEGER,
		w2kStatusQuotSizeMonitor INTEGER,
		w2kStatusQuotSizeStatus INTEGER,
		w2kStatusQuotCallBackRef DisplayString,
		w2kStatusQuotButton INTEGER
	}


