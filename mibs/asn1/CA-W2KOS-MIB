	SYNTAX DisplayString
	ACCESS read-only
	STATUS mandatory
	DESCRIPTION
		"&<caiW2kOs.155>Name of the auto discovery watcher."
	::= { w2kConfigMntAutoEntry 1 }


w2kConfigMntAutoName OBJECT-TYPE
	SYNTAX DisplayString
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
		"&<caiW2kOs.186>Wildcarded mount name filtering the mounts to be auto discovered (any backslash in the wildcarded name must be doubled). For each auto discovered mount a watcher will be created. Important: This name is case insensitive."
	::= { w2kConfigMntAutoEntry 2 }


w2kConfigMntAutoDescription OBJECT-TYPE
	SYNTAX DisplayString
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
		"&<caiW2kOs.187>Default for auto discovered mounts."
	::= { w2kConfigMntAutoEntry 3 }


w2kConfigMntAutoAggLag OBJECT-TYPE
	SYNTAX INTEGER
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
		"&<caiW2kOs.187>Default for auto discovered mounts."
	::= { w2kConfigMntAutoEntry 4 }


w2kConfigMntAutoRelToMonitor OBJECT-TYPE
	SYNTAX INTEGER {
		do-not-monitor(1),
		down-warning(2),
		down-critical(3)
	}
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
		"&<caiW2kOs.187>Default for auto discovered mounts."
	DEFVAL { 0 }
	::= { w2kConfigMntAutoEntry 5 }


w2kConfigMntAutoLossAction OBJECT-TYPE
	SYNTAX INTEGER {
		remove(1),
		up(2),
		down-warning(3),
		down-critical(4)
	}
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
		"&<caiW2kOs.187>Default for auto discovered mounts."
	DEFVAL { 0 }
	::= { w2kConfigMntAutoEntry 6 }


