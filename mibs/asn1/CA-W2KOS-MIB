	SYNTAX INTEGER
	ACCESS read-only
	STATUS mandatory
	DESCRIPTION
		"&<caiW2kOs.444>The number of entries in the w2kStatusCpuTable."
	::= { w2kStatusCpuGroup 9 }


w2kStatusCpuTable OBJECT-TYPE
	SYNTAX SEQUENCE OF W2kStatusCpuRow
	ACCESS not-accessible
	STATUS mandatory
	DESCRIPTION
		"&<caiW2kOs.445>A table containing information about all monitored processors."
	::= { w2kStatusCpuGroup 10 }


w2kStatusCpuEntry OBJECT-TYPE
	SYNTAX W2kStatusCpuRow
	ACCESS not-accessible
	STATUS mandatory
	DESCRIPTION
		"&<caiW2kOs.154>The table entry."
	INDEX { w2kStatusCpuName }
	::= { w2kStatusCpuTable 1 }


W2kStatusCpuRow ::=
	SEQUENCE {
		w2kStatusCpuName DisplayString,
		w2kStatusCpuType DisplayString,
		w2kStatusCpuAggStatus INTEGER,
		w2kStatusCpuLoadValue INTEGER,
		w2kStatusCpuLoadLagValue INTEGER,
		w2kStatusCpuLoadLag INTEGER,
		w2kStatusCpuLoadWarn INTEGER,
		w2kStatusCpuLoadCrit INTEGER,
		w2kStatusCpuLoadMonitor INTEGER,
		w2kStatusCpuLoadStatus INTEGER,
		w2kStatusCpuLossAction INTEGER,
		w2kStatusCpuLossStatus INTEGER,
		w2kStatusCpuCallBackRef DisplayString
	}


w2kStatusCpuName OBJECT-TYPE
	SYNTAX DisplayString
	ACCESS read-only
	STATUS mandatory
	DESCRIPTION
		"&<caiW2kOs.447>Name of the watcher, monitoring a processor with the processor identification number contained in this dummy name."
	::= { w2kStatusCpuEntry 1 }


w2kStatusCpuType OBJECT-TYPE
	SYNTAX DisplayString
	ACCESS read-only
	STATUS mandatory
	DESCRIPTION
		"&<caiW2kOs.448>The type of the processor."
	::= { w2kStatusCpuEntry 2 }


