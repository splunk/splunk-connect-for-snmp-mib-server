CA-W2KOS-MIB DEFINITIONS ::= BEGIN


IMPORTS
	TRAP-TYPE FROM RFC-1215
	enterprises   FROM RFC1155-SMI
	OBJECT-TYPE   FROM RFC1155-SMI
	DisplayString FROM SNMPv2-TC;


-- *****************************************************************************
-- Oids of caiW2kOs agent start with 1.3.6.1.4.1.791.2.10.2.43
-- *****************************************************************************
cai       OBJECT IDENTIFIER ::= { enterprises 791 }
caiSysMgr OBJECT IDENTIFIER ::= { cai 2 }
tng       OBJECT IDENTIFIER ::= { caiSysMgr 10 }
agents    OBJECT IDENTIFIER ::= { tng 2 }
caiW2kOs  OBJECT IDENTIFIER ::= { agents 43 }


-- *****************************************************************************
-- The four big groups
-- *****************************************************************************
w2kConfigGroup    OBJECT IDENTIFIER ::= { caiW2kOs 1 }
w2kStatusGroup    OBJECT IDENTIFIER ::= { caiW2kOs 2 }
w2kAvailableGroup OBJECT IDENTIFIER ::= { caiW2kOs 3 }
w2kHistoryGroup   OBJECT IDENTIFIER ::= { caiW2kOs 4 }


-- *****************************************************************************
-- Configuration Groups
-- *****************************************************************************
w2kConfigGeneralGroup OBJECT IDENTIFIER ::= { w2kConfigGroup 1 }
w2kConfigSysGroup     OBJECT IDENTIFIER ::= { w2kConfigGroup 2 }
w2kConfigCpuGroup     OBJECT IDENTIFIER ::= { w2kConfigGroup 3 }
w2kConfigMemGroup     OBJECT IDENTIFIER ::= { w2kConfigGroup 4 }
w2kConfigLVolGroup    OBJECT IDENTIFIER ::= { w2kConfigGroup 5 }
w2kConfigMntGroup     OBJECT IDENTIFIER ::= { w2kConfigGroup 6 }
w2kConfigDfsGroup     OBJECT IDENTIFIER ::= { w2kConfigGroup 7 }
w2kConfigQuotGroup    OBJECT IDENTIFIER ::= { w2kConfigGroup 8 }
w2kConfigDirGroup     OBJECT IDENTIFIER ::= { w2kConfigGroup 9 }
w2kConfigFileGroup    OBJECT IDENTIFIER ::= { w2kConfigGroup 10 }
w2kConfigProcGroup    OBJECT IDENTIFIER ::= { w2kConfigGroup 11 }
w2kConfigSrvcGroup    OBJECT IDENTIFIER ::= { w2kConfigGroup 12 }
w2kConfigJobGroup     OBJECT IDENTIFIER ::= { w2kConfigGroup 13 }
w2kConfigSessGroup    OBJECT IDENTIFIER ::= { w2kConfigGroup 14 }
w2kConfigPrnGroup     OBJECT IDENTIFIER ::= { w2kConfigGroup 15 }
w2kConfigNetGroup     OBJECT IDENTIFIER ::= { w2kConfigGroup 16 }
w2kConfigRegGroup     OBJECT IDENTIFIER ::= { w2kConfigGroup 17 }


-- *****************************************************************************
-- Status Groups
-- *****************************************************************************
w2kStatusGeneralGroup OBJECT IDENTIFIER ::= { w2kStatusGroup 1 }
w2kStatusCpuGroup     OBJECT IDENTIFIER ::= { w2kStatusGroup 2 }
w2kStatusMemGroup     OBJECT IDENTIFIER ::= { w2kStatusGroup 3 }
w2kStatusLVolGroup    OBJECT IDENTIFIER ::= { w2kStatusGroup 4 }
w2kStatusMntGroup     OBJECT IDENTIFIER ::= { w2kStatusGroup 5 }
w2kStatusDfsGroup     OBJECT IDENTIFIER ::= { w2kStatusGroup 6 }
w2kStatusQuotGroup    OBJECT IDENTIFIER ::= { w2kStatusGroup 7 }
w2kStatusDirGroup     OBJECT IDENTIFIER ::= { w2kStatusGroup 8 }
w2kStatusFileGroup    OBJECT IDENTIFIER ::= { w2kStatusGroup 9 }
w2kStatusProcGroup    OBJECT IDENTIFIER ::= { w2kStatusGroup 10 }
w2kStatusSrvcGroup    OBJECT IDENTIFIER ::= { w2kStatusGroup 11 }
w2kStatusJobGroup     OBJECT IDENTIFIER ::= { w2kStatusGroup 12 }
w2kStatusSessGroup    OBJECT IDENTIFIER ::= { w2kStatusGroup 13 }
w2kStatusPrnGroup     OBJECT IDENTIFIER ::= { w2kStatusGroup 14 }
w2kStatusNetGroup     OBJECT IDENTIFIER ::= { w2kStatusGroup 15 }
w2kStatusRegGroup     OBJECT IDENTIFIER ::= { w2kStatusGroup 16 }


-- *****************************************************************************
-- Availability Groups
-- *****************************************************************************
w2kAvailLVolGroup OBJECT IDENTIFIER ::= { w2kAvailableGroup 1 }
w2kAvailMntGroup  OBJECT IDENTIFIER ::= { w2kAvailableGroup 2 }
w2kAvailDfsGroup  OBJECT IDENTIFIER ::= { w2kAvailableGroup 3 }
w2kAvailQuotGroup OBJECT IDENTIFIER ::= { w2kAvailableGroup 4 }
w2kAvailProcGroup OBJECT IDENTIFIER ::= { w2kAvailableGroup 5 }
w2kAvailSrvcGroup OBJECT IDENTIFIER ::= { w2kAvailableGroup 6 }
w2kAvailJobGroup  OBJECT IDENTIFIER ::= { w2kAvailableGroup 7 }
w2kAvailSessGroup OBJECT IDENTIFIER ::= { w2kAvailableGroup 8 }
w2kAvailPrnGroup  OBJECT IDENTIFIER ::= { w2kAvailableGroup 9 }




-- *****************************************************************************
-- General Config Group
-- *****************************************************************************


w2kConfigGeneralAgentVersion OBJECT-TYPE
	SYNTAX DisplayString
	ACCESS read-only
	STATUS mandatory
	DESCRIPTION
		"&<caiW2kOs.100>The currently installed version of the agent."
	::= { w2kConfigGeneralGroup 1 }


w2kConfigGeneralColdStartTime OBJECT-TYPE
	SYNTAX DisplayString
	ACCESS read-only
	STATUS mandatory
	DESCRIPTION
		"&<caiW2kOs.101>The date and time that the agent was last started."
	::= { w2kConfigGeneralGroup 2 }


w2kConfigGeneralWarmStartTime OBJECT-TYPE
	SYNTAX DisplayString
	ACCESS read-only
	STATUS mandatory
	DESCRIPTION
		"&<caiW2kOs.102>The date and time that the agent's configuration was last changed."
	::= { w2kConfigGeneralGroup 3 }


w2kConfigGeneralCpuPollTime OBJECT-TYPE
	SYNTAX DisplayString
	ACCESS read-only
	STATUS mandatory
	DESCRIPTION
		"&<caiW2kOs.103>The date and time that the agent last polled the processor information."
	::= { w2kConfigGeneralGroup 4 }


