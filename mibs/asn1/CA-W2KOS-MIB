-- *****************************************************************************
-- Mounts Status Group
-- *****************************************************************************


w2kStatusMntCount OBJECT-TYPE
	SYNTAX INTEGER
	ACCESS read-only
	STATUS mandatory
	DESCRIPTION
		"&<caiW2kOs.531>The number of entries in the w2kStatusMntTable."
	::= { w2kStatusMntGroup 1 }


w2kStatusMntTable OBJECT-TYPE
	SYNTAX SEQUENCE OF W2kStatusMntRow
	ACCESS not-accessible
	STATUS mandatory
	DESCRIPTION
		"&<caiW2kOs.532>A table containing information about all monitored mounts."
	::= { w2kStatusMntGroup 2 }


w2kStatusMntEntry OBJECT-TYPE
	SYNTAX W2kStatusMntRow
	ACCESS not-accessible
	STATUS mandatory
	DESCRIPTION
		"&<caiW2kOs.154>The table entry."
	INDEX { w2kStatusMntName }
	::= { w2kStatusMntTable 1 }


W2kStatusMntRow ::=
	SEQUENCE {
		w2kStatusMntName DisplayString,
		w2kStatusMntDescription DisplayString,
		w2kStatusMntAutoWatcherName DisplayString,
		w2kStatusMntAggLagValue INTEGER,
		w2kStatusMntAggLag INTEGER,
		w2kStatusMntAggStatus INTEGER,
		w2kStatusMntRelToValue DisplayString,
		w2kStatusMntRelToRef DisplayString,
		w2kStatusMntRelToMonitor INTEGER,
		w2kStatusMntRelToStatus INTEGER,
		w2kStatusMntLossAction INTEGER,
		w2kStatusMntLossStatus INTEGER,
		w2kStatusMntCallBackRef DisplayString,
		w2kStatusMntButton INTEGER
	}


w2kStatusMntName OBJECT-TYPE
	SYNTAX DisplayString
	ACCESS read-only
	STATUS mandatory
	DESCRIPTION
		"&<caiW2kOs.534>Name of the watcher, monitoring a mount with this name. Important: This name is case insensitive."
	::= { w2kStatusMntEntry 1 }


w2kStatusMntDescription OBJECT-TYPE
	SYNTAX DisplayString
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
		"&<caiW2kOs.136>Any description for this watcher."
	::= { w2kStatusMntEntry 2 }


