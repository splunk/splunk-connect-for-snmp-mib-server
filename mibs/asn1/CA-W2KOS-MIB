	SYNTAX INTEGER
	ACCESS read-only
	STATUS mandatory
	DESCRIPTION
		"&<caiW2kOs.499>The number of consecutive poll intervals on which any status was not in the Ok or Up state."
	::= { w2kStatusQuotEntry 4 }


w2kStatusQuotAggLag OBJECT-TYPE
	SYNTAX INTEGER
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
		"&<caiW2kOs.137>The number of consecutive poll intervals on which any status is not in the Ok or Up state, before the aggregate status changes. Valid values are greater than 0."
	::= { w2kStatusQuotEntry 5 }


w2kStatusQuotAggStatus OBJECT-TYPE
	SYNTAX INTEGER {
		unknown(1),
		ok(2),
		warning(3),
		critical(4)
	}
	ACCESS read-only
	STATUS mandatory
	DESCRIPTION
		"&<caiW2kOs.576>The current status of the quota as a 'worst case' aggregate of all its statuses."
	::= { w2kStatusQuotEntry 6 }


w2kStatusQuotExist OBJECT-TYPE
	SYNTAX INTEGER {
		should-exist(1),
		should-not-exist(2)
	}
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
		"&<caiW2kOs.221>Flag indicating whether to raise an alert if a quota is existent or if it is not-existent."
	DEFVAL { 0 }
	::= { w2kStatusQuotEntry 7 }


