        ::= { cdx6500SPST3201SummEntry 30 }

        -- The 3201 Control Objects
         
        cdx6500Cont3201Table OBJECT IDENTIFIER  ::=
            { cdx6500Controls 11 }

        -- The 3201 Port Control Table

        cdx6500Cont3201PortTable OBJECT-TYPE
            SYNTAX SEQUENCE OF Cdx6500Cont3201PortEntry
            ACCESS not-accessible
            STATUS mandatory
            DESCRIPTION
                "This table contains the 3201 Port Control parameters."
        ::= { cdx6500Cont3201Table 1 }

        cdx6500Cont3201PortEntry OBJECT-TYPE
            SYNTAX Cdx6500Cont3201PortEntry
            ACCESS not-accessible
            STATUS mandatory
            DESCRIPTION
                "Entries in the 3201 Port control Table."
            INDEX { iso3201PContPortNum }
        ::= { cdx6500Cont3201PortTable 1 }

        Cdx6500Cont3201PortEntry ::=
            SEQUENCE {
                iso3201PContPortNum
                    INTEGER,
                iso3201ContPortBoot
                    INTEGER,
                iso3201ContPortEnable
                    INTEGER,
                iso3201ContPortDisable
                    INTEGER,
                iso3201ContPortResetStats
                    INTEGER
                }

        iso3201PContPortNum OBJECT-TYPE
            SYNTAX INTEGER (1..54)
            ACCESS not-accessible
            STATUS mandatory
            DESCRIPTION
                "3201 port number ."
        ::= { cdx6500Cont3201PortEntry 1 }

        iso3201ContPortBoot OBJECT-TYPE
            SYNTAX INTEGER {
                    boot   (1),
                    noBoot (2)
                    }
            ACCESS write-only
            STATUS mandatory
            DESCRIPTION
                "Boots the specified 3201 port.
                 boot   : Boot the port
                 noBoot : Do not boot the port."
        ::= { cdx6500Cont3201PortEntry 2 }

        iso3201ContPortEnable OBJECT-TYPE
            SYNTAX INTEGER {
                    enable   (1),
                    noEnable (2)
                    }
            ACCESS write-only
            STATUS mandatory
            DESCRIPTION
                "Enables the specified 3201 Port.
                 enable   : Enable the Port
                 noEnable : Do not enable the port." 
        ::= { cdx6500Cont3201PortEntry 3 }

        iso3201ContPortDisable OBJECT-TYPE
            SYNTAX INTEGER {
                    disable   (1),
                    noDisable (2)
                    }
            ACCESS  write-only
            STATUS  mandatory
            DESCRIPTION
                "Disables the specified 3201 Port.
                 disable   : Disable Port
                 noDisable : Do not disable Port."
        ::= { cdx6500Cont3201PortEntry 4 }

        iso3201ContPortResetStats OBJECT-TYPE
            SYNTAX INTEGER {
                    reset   (1),
                    noreset (2) 
                    }
            ACCESS write-only
            STATUS mandatory
            DESCRIPTION
                "Reset statistics for the specified 3201 Port
                 reset   : Reset port statistics
                 noReset : Do not reset port statistics."
        ::= { cdx6500Cont3201PortEntry 5 }

        -- The 3201 Station Controls Table
        -- This table contains the station control parameters for 
        -- 3201 station

        cdx6500Cont3201StnTable OBJECT-TYPE
            SYNTAX SEQUENCE OF Cdx6500Cont3201StnEntry
            ACCESS not-accessible
            STATUS mandatory
            DESCRIPTION
                "This table contains the 3201 Station Control parameters."
        ::= { cdx6500Cont3201Table 2 }

        cdx6500Cont3201StnEntry OBJECT-TYPE
            SYNTAX Cdx6500Cont3201StnEntry
            ACCESS not-accessible
            STATUS mandatory
            DESCRIPTION
                "Entries in the 3201 Station Control Table."
            INDEX { iso3201SContPortNum, iso3201ContStnNum }
        ::= { cdx6500Cont3201StnTable 1 }

        Cdx6500Cont3201StnEntry ::=
            SEQUENCE {
                iso3201SContPortNum
                    INTEGER,
                iso3201ContStnNum
                    INTEGER,
                iso3201ContStnBoot
                    INTEGER,
                iso3201ContStnEnable
                    INTEGER,
                iso3201ContStnDisable
                    INTEGER,
                iso3201ContStnResetStats
                    INTEGER
                }

        iso3201SContPortNum OBJECT-TYPE
            SYNTAX INTEGER (1..54)
            ACCESS not-accessible
            STATUS mandatory
            DESCRIPTION
                "3201 port number."
        ::= { cdx6500Cont3201StnEntry 1 }

        iso3201ContStnNum OBJECT-TYPE
            SYNTAX INTEGER (1..16)
            ACCESS not-accessible
            STATUS mandatory
            DESCRIPTION
                "3201 station number."
        ::= { cdx6500Cont3201StnEntry 2 }

        iso3201ContStnBoot OBJECT-TYPE
            SYNTAX INTEGER {
                    boot   (1),
                    noBoot (2) 
                    }
            ACCESS write-only
            STATUS mandatory
            DESCRIPTION
                "Boots the secified 3201 station.
                 boot   : Boot the station
                 noBoot : Do not boot Station."
        ::= { cdx6500Cont3201StnEntry 3 }

        iso3201ContStnEnable OBJECT-TYPE
            SYNTAX INTEGER {
                    enable   (1),
                    noEnable (2)
                    }
            ACCESS write-only
            STATUS mandatory
            DESCRIPTION
                "Enables the specified 3201 station
                 enable   : Enable station
                 noEnable : Do not enable station."
        ::= { cdx6500Cont3201StnEntry 4 }

        iso3201ContStnDisable OBJECT-TYPE
            SYNTAX INTEGER {
                    disable   (1),
                    noDisable (2)  
                    }
            ACCESS  write-only
            STATUS  mandatory
            DESCRIPTION
                "Disables the specified 3201 Station
                 disable   : Disable Station
                 noDisable : Do not disable Station."
        ::= { cdx6500Cont3201StnEntry 5 }

        iso3201ContStnResetStats OBJECT-TYPE
            SYNTAX INTEGER {
                    reset   (1),
                    noReset (2) 
                    }
            ACCESS write-only
            STATUS mandatory
            DESCRIPTION
                "Reset statistics for the specified 3201 station
                 reset   : Reset station statistics
                 noReset : Do not reset station statistics."
        ::= { cdx6500Cont3201StnEntry  6 }

