            SYNTAX INTEGER
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
                "Interface number of the configured E1 channel."
        ::= {cdx6500T1E1VGStatE1ABCDStateEntry 1 }

        cdx6500T1E1VGStatE1ABCDStateChannelNumber OBJECT-TYPE
            SYNTAX INTEGER (1..30)
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
                "Channel number of the configured E1 channel."
        ::= {cdx6500T1E1VGStatE1ABCDStateEntry 2 }

        cdx6500T1E1VGStatE1ABCDStateCurrentRxState OBJECT-TYPE
            SYNTAX DisplayString
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
                "Current RX ABCD state of the configured E1 channels."
        ::= { cdx6500T1E1VGStatE1ABCDStateEntry 3 }

        -- The T1E1VG Control Table
        cdx6500ContT1E1VGTable OBJECT IDENTIFIER
        ::= { cdx6500Controls 19 }

        cdx6500T1E1VGContEntry OBJECT-TYPE
            SYNTAX Cdx6500T1E1VGContEntry 
            ACCESS not-accessible
            STATUS mandatory
            DESCRIPTION
                "Each control applies to a single T1E1VG Interface."
            INDEX { t1e1vgContIfNumber }
        ::= { cdx6500ContT1E1VGTable 1 }

        Cdx6500T1E1VGContEntry ::=
            SEQUENCE {
                t1e1vgContIfNumber 
                    INTEGER,
                t1e1vgContIfBoot 
                    INTEGER
                }

        t1e1vgContIfNumber OBJECT-TYPE
            SYNTAX INTEGER 
            ACCESS not-accessible
            STATUS mandatory
            DESCRIPTION
                "Interface number of the T1E1VG."
        ::= { cdx6500T1E1VGContEntry 1 }

        t1e1vgContIfBoot OBJECT-TYPE
            SYNTAX INTEGER {
                   boot       (1),
                   noboot     (2)
                   }
            ACCESS write-only
            STATUS mandatory
            DESCRIPTION
                "Boot operations for the specified T1E1VG.
                 boot   : Boots the T1E1VG Interface 
                 noboot : Do not Boots the T1E1VG Interface."
        ::= { cdx6500T1E1VGContEntry 2 }

        END
