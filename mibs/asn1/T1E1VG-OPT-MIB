        -- The last 24 hour Error Statistics Table

        cdx6500T1E1VGStatTotal24HoursReportTable OBJECT-TYPE
            SYNTAX SEQUENCE OF Cdx6500T1E1VGStatTotal24HoursReportEntry
            ACCESS not-accessible
            STATUS mandatory
            DESCRIPTION
                "This table holds a report of the cumulative sum
                 of the statistics for the previous 24 hour interval."
        ::= { cdx6500STT1E1VGGroup 2 }

        cdx6500T1E1VGStatTotal24HoursReportEntry OBJECT-TYPE
            SYNTAX Cdx6500T1E1VGStatTotal24HoursReportEntry
            ACCESS not-accessible
            STATUS mandatory
            DESCRIPTION
                "Each entry contains statistics information for one T1 port."
            INDEX  { cdx6500T1E1VGStatTotal24HoursReportIfNumber }
        ::= { cdx6500T1E1VGStatTotal24HoursReportTable 1 }

        Cdx6500T1E1VGStatTotal24HoursReportEntry ::=
            SEQUENCE {
                cdx6500T1E1VGStatTotal24HoursReportIfNumber
                    INTEGER,
                cdx6500T1E1VGStatTotal24HoursReportLES
                    Gauge,
                cdx6500T1E1VGStatTotal24HoursReportLCV
                    Gauge,
                cdx6500T1E1VGStatTotal24HoursReportPCV
                    Gauge,
                cdx6500T1E1VGStatTotal24HoursReportCSS
                    Gauge,
                cdx6500T1E1VGStatTotal24HoursReportES
                    Gauge,
                cdx6500T1E1VGStatTotal24HoursReportBES
                    Gauge,
                cdx6500T1E1VGStatTotal24HoursReportSES
                    Gauge,
                cdx6500T1E1VGStatTotal24HoursReportSEFS
                    Gauge,
                cdx6500T1E1VGStatTotal24HoursReportUAS
                    Gauge
                }

        cdx6500T1E1VGStatTotal24HoursReportIfNumber OBJECT-TYPE
            SYNTAX INTEGER 
            ACCESS read-only
            STATUS mandatory
