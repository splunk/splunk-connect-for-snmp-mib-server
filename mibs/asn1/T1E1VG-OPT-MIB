            ACCESS not-accessible
            STATUS mandatory
            DESCRIPTION
                "The ABCD State Table contains the current
                 RX ABCD state of the Configured DS0 Channels."
        ::= { cdx6500STT1E1VGGroup 5 }

        cdx6500T1E1VGStatE1ABCDStateEntry OBJECT-TYPE
            SYNTAX Cdx6500T1E1VGStatE1ABCDStateEntry
            ACCESS not-accessible
            STATUS mandatory
            DESCRIPTION
                "An Entry in the ABCD State Table."
            INDEX { cdx6500T1E1VGStatE1ABCDStateIfNumber,
                    cdx6500T1E1VGStatE1ABCDStateChannelNumber }
        ::= { cdx6500T1E1VGStatE1ABCDStateTable 1 }

        Cdx6500T1E1VGStatE1ABCDStateEntry ::=
