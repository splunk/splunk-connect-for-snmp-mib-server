        cdx6500T1E1VGCfgEntryNumber OBJECT-TYPE
            SYNTAX INTEGER 
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
                "Entry number of the T1E1VG Table."
        ::= { cdx6500T1E1VGEntry 1 }

        cdx6500T1E1VGCfgInterfaceType OBJECT-TYPE
            SYNTAX DisplayString
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
                "This is a T1E1VG  having interface type."
        ::= { cdx6500T1E1VGEntry 2 }

        cdx6500T1E1VGCfgFormat OBJECT-TYPE
            SYNTAX INTEGER {
                    vg-format-frational (1),
                    vg-format-channelize (2),
                    vg-format-isdn (3),
                    vg-format-ccs (4)
                    } 
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
                "This parameter is used to select the format 
                 of the T1E1VG interface.
		 vg-format-frational :DS0's are predefined by the TELCO or the User
                 vg-format-channelize :DS0 is predefined by the TELCO or the User
                 vg-format-isdn  :DS0 is defined by the ISDN (Q.931) D-Channel
                 vg-format-ccs  :DS0 is defined by the signalling channel (Voice)."
        ::= { cdx6500T1E1VGEntry 3 }

        cdx6500T1E1VGCfgSwitchType OBJECT-TYPE
            SYNTAX INTEGER {
                    unspecified  (1),
                    ni2 	(2),
                    ess5        (3),
                    dms100      (4),
                    ntt         (5),
                    tso14      (6),
                    etsi       (7),
                    qsig       (8)
                    }
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
                "The ISDN switch type. This specifies the type of
                 equipment to which this span is connected.
                 The possible choices are:
                 ni2	  : National ISDN version 2
                 5ess     : AT&T 5ESS Switch
                 dms100   : Northern Telecom DMS-100 Switch
                 ntt      : Nippon Telephone and Telegraph switch (Japan)
                 tso14    : AUSTEL Technical Standard 014 for Australia
                 etsi     : Euro ISDN PRI (NET5)
		 qsig     : ISDN QSIG PRI."
        ::= { cdx6500T1E1VGEntry 4 }

        cdx6500T1E1VGCfgUserNetworkSide OBJECT-TYPE
            SYNTAX INTEGER {
                    user    (1),
                    network (2)
                    }
                ACCESS read-only
                STATUS mandatory
                DESCRIPTION
                    "This parameter specifies whether the T1E1VG 
                     is emulating the network or user side of the
                     ISDN network.
                     user    : The T1 or E1 port is emulating the user,
                               ex.  the T1 or E1 port is to be connected
                               to an ISDN network.
                     network : The T1 or E1 port is emulating the network,
                               ex. the T1 or E1 port is to be connected
                               to an ISDN PBX."
        ::= { cdx6500T1E1VGEntry 5 }

        cdx6500T1E1VGCfgDirectBchannel OBJECT-TYPE
            SYNTAX INTEGER {
                    high-to-low    (1),
                    low-to-high (2)
                    }
                ACCESS read-only
                STATUS mandatory
                DESCRIPTION
                    "This parameter specifies the direction of B-Channel
		      assignment on outbound calls made from this port.
			high-to-low : B-Channels are selected starting with the highest
                			 numbered available channel.
   			high-to-low : B-Channels are selected starting with the lowest
                		       numbered available channel."
        ::= { cdx6500T1E1VGEntry 6 }

        cdx6500T1E1VGCfgBchRange OBJECT-TYPE
            SYNTAX DisplayString 
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
                "Specify the B-Channel assignments that will be used
		 for the ISDN/QSIG calls.
		  Select Individual or Time Slot ranges, e.g. 1-3,10,13-15
		  will include Time Slots 1,2,3,10,13,14,15."
        ::= { cdx6500T1E1VGEntry 7 }

        cdx6500T1E1VGCfgCallAddrReplace OBJECT-TYPE
            SYNTAX INTEGER {
                    none    (1)
                    }
                ACCESS read-only
                STATUS mandatory
                DESCRIPTION
		 "This parameter is a PLACE HOLDER item."
        ::= { cdx6500T1E1VGEntry 8 }

        cdx6500T1E1VGCfgIsdnQsigOpt OBJECT-TYPE
            SYNTAX DisplayString
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
                "This parameter is a PLACE HOLDER item."
        ::= { cdx6500T1E1VGEntry 9 }

        cdx6500T1E1VGCfgFilteredIE OBJECT-TYPE
            SYNTAX DisplayString
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
                "This parameter is a PLACE HOLDER item."
        ::= { cdx6500T1E1VGEntry 10 }

        cdx6500T1E1VGCfgReplIsdnQsig OBJECT-TYPE
            SYNTAX DisplayString
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
                "This parameter is a PLACE HOLDER item."
        ::= { cdx6500T1E1VGEntry 11 }

        cdx6500T1E1VGCfgCallPartyNumType OBJECT-TYPE
            SYNTAX INTEGER {
                    unknown (1),
                    international (2),
                    national  (3),
                    subscriber  (5)
                    }
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
                 "This is the Calling/Called Party Number Type as defined by ITU-T
		   unknown       - Unknown
   		   international - International number
   		   national      - National number
   		   subscriber    - Subscriber number."
        ::= { cdx6500T1E1VGEntry 12 }

        cdx6500T1E1VGCfgCallPartyNumPlan OBJECT-TYPE
            SYNTAX INTEGER {
                    unknown (1),
                    isdn (2),
                    telephony  (3),
                    private  (10)
                    }
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
	         "This is the Calling/Called Party Number Plan as defined by ITU-T
   			unknown   - Unknown
   			iSDN      - ISDN numbering plan (E.164)
   			telephony - Standard Telephone numbering plan
   			private   - Private numbering plan."
        ::= { cdx6500T1E1VGEntry 13 }

        cdx6500T1E1VGCfg1stChanPort OBJECT-TYPE
            SYNTAX INTEGER 
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
                 "*Note that: This port must be configured as default."
        ::= { cdx6500T1E1VGEntry 14 }

        cdx6500T1E1VGCfg1stChanSlot OBJECT-TYPE
            SYNTAX DisplayString
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
                 "Specify the Time Slot assignments, 1 to 24 for T1 or 1 to 31 for E1.
	           Select Individual or Time Slot ranges, e.g. 1-3,10,13-15
		   will include Time Slots 1,2,3,10,13,14,15."
        ::= { cdx6500T1E1VGEntry 15 }

        cdx6500T1E1VGCfg1stChanDS0Rate OBJECT-TYPE
            SYNTAX INTEGER {
		rate-56k	(1),
		rate-64k	(2)
		} 
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
	      "Specify the DS0 Rate for T1."
        ::= { cdx6500T1E1VGEntry 16 }

        cdx6500T1E1VGCfg2ndChanPort OBJECT-TYPE
            SYNTAX INTEGER
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
                 "0       : No Data activity on the Second FT1/FE1 Channel.
		  Port No.: Associated to the Second FT1/FE1 Channel."
        ::= { cdx6500T1E1VGEntry 17 }

        cdx6500T1E1VGCfg2ndChanSlot OBJECT-TYPE
            SYNTAX DisplayString 
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
                 "Specify the Time Slot assignments, 1 to 24 for T1 or 1 to 31 for E1.
                   Select Individual or Time Slot ranges, e.g. 1-3,10,13-15
                   will include Time Slots 1,2,3,10,13,14,15."
        ::= { cdx6500T1E1VGEntry 18 }

        cdx6500T1E1VGCfg2ndChanDS0Rate OBJECT-TYPE
            SYNTAX INTEGER {
                rate-56k     (1),
                rate-64k     (2)
                }
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
              "Specify the DS0 Rate for T1."
        ::= { cdx6500T1E1VGEntry 19 }

        cdx6500T1E1VGCfg3rdChanPort OBJECT-TYPE
            SYNTAX INTEGER
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
                 "0       : No Data activity on the Third FT1/FE1 Channel.
                  Port No.: Associated to the Third FT1/FE1 Channel."
        ::= { cdx6500T1E1VGEntry 20 }

        cdx6500T1E1VGCfg3rdChanSlot OBJECT-TYPE
            SYNTAX DisplayString 
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
                 "Specify the Time Slot assignments, 1 to 24 for T1 or 1 to 31 for E1.
                   Select Individual or Time Slot ranges, e.g. 1-3,10,13-15
                   will include Time Slots 1,2,3,10,13,14,15."
        ::= { cdx6500T1E1VGEntry 21 }

        cdx6500T1E1VGCfg3rdChanDS0Rate OBJECT-TYPE
            SYNTAX INTEGER {
                rate-56k     (1),
                rate-64k     (2)
                }
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
              "Specify the DS0 Rate for T1."
        ::= { cdx6500T1E1VGEntry 22 }

        cdx6500T1E1VGCfgLineFramingType OBJECT-TYPE
            SYNTAX INTEGER {
                    sf (1),
                    esf  (2),
                    e1 (3),
                    e1crc  (4),
                    e1crc-febe  (5)
                    }
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
               "Indicate type of framing used by the DS1 circuit.
		    esf : Extended Super Frame
   		    sf  : Super Frame (Also known as D4 framing type)
		    e1  : D2048S or D2048U
                    e1-crc  : D2048S with CRC
                    e1-crc-febe: D2048S with CRC and Si=FEBE."
        ::= { cdx6500T1E1VGEntry 23 }

        cdx6500T1E1VGCfgLineCoding OBJECT-TYPE
            SYNTAX INTEGER {
                    ami  (1),
                    b8zs   (2),
		    hdb3   (3) 
                    }
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
	        "Selects variety of zero suppression used on the T1 link.
		   ami : Alternate Mark Inversion.
		   b8zs : Bipolar 8 Zero Substitution.
		   hdb3 : High Density Bipolar 3"
        ::= { cdx6500T1E1VGEntry 24 }

        cdx6500T1E1VGCfgTransmitClock OBJECT-TYPE
            SYNTAX INTEGER {
                    rec  (1),
                    int (2)
                    }
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
                "Selects the source of the transmit clock.
                 int  : Internal Oscillator.
                 rec  : Received Timing."
        ::= { cdx6500T1E1VGEntry 25 }

        cdx6500T1E1VGCfgLineImpedance OBJECT-TYPE
            SYNTAX INTEGER {
                    ohms-120 (1),
                    ohms-75 (2)
                    }
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
                "Selects the line impedance as 75 ohms or 120 ohms."
        ::= { cdx6500T1E1VGEntry 26 }

        cdx6500T1E1VGCfgLineBuildOut OBJECT-TYPE
            SYNTAX INTEGER
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
                 "Select the Line Build Out to match the physical interface.
                  For DSX interfaces, this value is based on cable
			0 - 0ft to 133ft
     			1 - 134ft to 266ft
     			2 - 267ft to 399ft
     			3 - 400ft to 533ft
     			4 - 534ft to 655ft

                  For DS1 interfaces, this value is based on the
			0 - 0db
     			5 - -7.5db
     			6 - -15db
     			7 - -22.5db
     			4 - Not valid for DS1 interfaces"
        ::= { cdx6500T1E1VGEntry 27 }   

        cdx6500T1E1VGCfgRecvSense OBJECT-TYPE
            SYNTAX INTEGER {
                    low (1),
                    high (2)
                    }
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
	       "Select HIGH (-36DB) if line is marginal."
        ::= { cdx6500T1E1VGEntry 28 }

        cdx6500T1E1VGCfgFacilityDataLink OBJECT-TYPE
            SYNTAX INTEGER {
                    none (1),
                    ansi (3),
                    att  (2)
                    }
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
                "This describes the use of the facilities data link,
                 and may have the following capabilities:
                 none : indicates that the device does  not use
                        the FDL.
                 ansi : refers to the  FDL  exchange recommended 
                        by ANSI (Ansi-T1-403).
                 att  : refers to ESF FDL exchanges (Att-54016)."
        ::= { cdx6500T1E1VGEntry 29 }

        cdx6500T1E1VGCfgV54RecvRLBK OBJECT-TYPE
            SYNTAX INTEGER {
                    disable (1),
                    enable (2)
                    }
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
	       "enable : Respond to incoming V.54 loopback requests.
                disable: Do not respond to incoming V.54 loopback requests."
        ::= { cdx6500T1E1VGEntry 30 }

        cdx6500T1E1VGCfgThresholdValueLES OBJECT-TYPE
            SYNTAX INTEGER (1..255)
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
                "Threshold value of Line Errored Seconds (LESs)
                 to report alarms on CTP when exceeded."
        ::= { cdx6500T1E1VGEntry 31  }

        cdx6500T1E1VGCfgThresholdValueLCV OBJECT-TYPE
            SYNTAX INTEGER (1..255)
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
                "Threshold value of Line Code Violations (LCVs)
                 to report alarms on CTP when exceeded."
        ::= { cdx6500T1E1VGEntry 32 }

        cdx6500T1E1VGCfgThresholdValuePCV OBJECT-TYPE
            SYNTAX INTEGER (1..255)
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
                "Threshold value of Path Code Violations (PCVs)
                 to report alarms on CTP when exceeded."
        ::= { cdx6500T1E1VGEntry 33  }


        cdx6500T1E1VGCfgThresholdValueCSS OBJECT-TYPE
            SYNTAX INTEGER (1..255)
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
                "Threshold value of Controlled Slip Seconds (CSSs)
                 to report alarms on CTP when exceeded."
        ::= { cdx6500T1E1VGEntry 34 }

        cdx6500T1E1VGCfgThresholdValueES OBJECT-TYPE
            SYNTAX INTEGER (1..255)
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
                "Threshold value of Errored Seconds (ESs)
                 to report alarms on CTP when exceeded."
        ::= { cdx6500T1E1VGEntry 35 }

        cdx6500T1E1VGCfgThresholdValueBES OBJECT-TYPE
            SYNTAX INTEGER (1..255)
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
                "Threshold value of Bursty Errored Seconds (BESs)
                 to report alarms on CTP when exceeded."
        ::= { cdx6500T1E1VGEntry 36 }

        cdx6500T1E1VGCfgThresholdValueSES OBJECT-TYPE
            SYNTAX INTEGER (1..255)
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
                "Threshold value of Severely Errored Seconds (SESs)
                 to report alarms on CTP when exceeded."
        ::= { cdx6500T1E1VGEntry 37 }

        cdx6500T1E1VGCfgThresholdValueSEFS OBJECT-TYPE
            SYNTAX INTEGER (1..255)
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
                "Threshold value of Severely Errored Framing Seconds (SEFSs)
                 to report alarms on CTP when exceeded."
        ::= { cdx6500T1E1VGEntry 38 }

        cdx6500T1E1VGCfgThresholdValueUAS OBJECT-TYPE
            SYNTAX INTEGER (1..255)
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
                "Threshold value of Unavailable Seconds (UASs)
                 to report alarms on CTP when exceeded."
        ::= { cdx6500T1E1VGEntry 39 }

        cdx6500T1E1VGCfgBchNumScheme OBJECT-TYPE
            SYNTAX INTEGER {
                    nb-1-30 (1),
                    nb-1-31 (2)
                    }
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
               "This parameter specifies the B-Channel numbering scheme 
		used by the ISDN service provider.
		  1-31  : B-Channels are numbered 1 to 31.
		  1-30  : B-Channels are numbered 1 to 30."
        ::= { cdx6500T1E1VGEntry 40 }


        -- The T1E1VG Statistics Table
        
        cdx6500STT1E1VGTable OBJECT-TYPE
            SYNTAX SEQUENCE OF Cdx6500STT1E1VGEntry
            ACCESS not-accessible
            STATUS mandatory
            DESCRIPTION
                "This table holds statistics information for T1E1VG."
        ::= { cdx6500STT1E1VGGroup 1 }
        
        cdx6500STT1E1VGEntry OBJECT-TYPE
            SYNTAX Cdx6500STT1E1VGEntry 
            ACCESS not-accessible
            STATUS mandatory
            DESCRIPTION
                "Each entry contains statistics information for one T1E1VG interface."
            INDEX { cdx6500T1E1VGStatIfNumber }            
        ::= { cdx6500STT1E1VGTable 1 }
        
        Cdx6500STT1E1VGEntry ::=
            SEQUENCE {
                cdx6500T1E1VGStatIfNumber 
