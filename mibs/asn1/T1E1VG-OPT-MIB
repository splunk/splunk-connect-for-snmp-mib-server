        -- T1 Channel State Table

        cdx6500T1E1VGStatT1ChannelStateTable OBJECT-TYPE
            SYNTAX SEQUENCE OF Cdx6500T1StatT1ChannelStateEntry
            ACCESS not-accessible
            STATUS mandatory
            DESCRIPTION
                "The Channel State Table contains the current
                 RX Channel state of the Configured DS0 Channels."
        ::= { cdx6500STT1E1VGGroup 4 }

        cdx6500T1E1VGStatT1ChannelStateEntry OBJECT-TYPE
            SYNTAX Cdx6500T1StatT1ChannelStateEntry 
            ACCESS not-accessible
            STATUS mandatory
            DESCRIPTION
                "An Entry in the ABCD State Table."
            INDEX { cdx6500T1E1VGStatT1ChannelStateIfNumber,
                    cdx6500T1E1VGStatT1ChannelStateABCDNumber }
        ::= { cdx6500T1E1VGStatT1ChannelStateTable 1 }

        Cdx6500T1StatT1ChannelStateEntry ::=
            SEQUENCE {
                cdx6500T1E1VGStatT1ChannelStateIfNumber 
                    INTEGER,
                cdx6500T1E1VGStatT1ChannelStateABCDNumber 
                    INTEGER,
                cdx6500T1E1VGStatT1ChannelStateCurrentRxState
                    DisplayString
                }

        cdx6500T1E1VGStatT1ChannelStateIfNumber OBJECT-TYPE
            SYNTAX INTEGER 
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
                "Interface number of the configured T1 channel."
        ::= {cdx6500T1E1VGStatT1ChannelStateEntry 1 }

        cdx6500T1E1VGStatT1ChannelStateABCDNumber OBJECT-TYPE
            SYNTAX INTEGER (1..24)
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
                "ABCD number of the configured T1 channel."
        ::= {cdx6500T1E1VGStatT1ChannelStateEntry 2 }

        cdx6500T1E1VGStatT1ChannelStateCurrentRxState OBJECT-TYPE
            SYNTAX DisplayString
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
                "Current RX Channel state of the configured T1 channels."
        ::= { cdx6500T1E1VGStatT1ChannelStateEntry 3 }

        -- E1ABCD State Table

        cdx6500T1E1VGStatE1ABCDStateTable OBJECT-TYPE
