        -- Version 2.15.1.1, modified 2001-05-31 17:28:45-04 

        T1E1VG-OPT-MIB DEFINITIONS ::= BEGIN

        IMPORTS
            Counter, Gauge, enterprises
                FROM RFC1155-SMI

            OBJECT-TYPE
                FROM RFC-1212;

        -- Codex MIB Group definitions

                      codex OBJECT IDENTIFIER ::= { enterprises 449 }
         cdxProductSpecific OBJECT IDENTIFIER ::= { codex 2 }
                    cdx6500 OBJECT IDENTIFIER ::= { cdxProductSpecific 1 }

      cdx6500Configuration  OBJECT IDENTIFIER ::= { cdx6500 2 }
     cdx6500CfgGeneralGroup OBJECT IDENTIFIER ::= { cdx6500Configuration 2 }

          cdx6500Statistics OBJECT IDENTIFIER ::= { cdx6500 3 }
 cdx6500StatOtherStatsGroup OBJECT IDENTIFIER ::= { cdx6500Statistics 2 }
       cdx6500STT1E1VGGroup OBJECT IDENTIFIER ::= { cdx6500StatOtherStatsGroup 15 }

            cdx6500Controls OBJECT IDENTIFIER ::= { cdx6500 4 }

        -- Textual Conventions

        DisplayString ::= OCTET STRING


        -- T1E1VG Configuration Table

        cdx6500T1E1VGTable OBJECT-TYPE
            SYNTAX SEQUENCE OF Cdx6500T1E1VGEntry
            ACCESS not-accessible
            STATUS mandatory
            DESCRIPTION
                "This table contains the T1E1VG Configuration
                 parameters."
        ::= { cdx6500CfgGeneralGroup 25 }

        cdx6500T1E1VGEntry OBJECT-TYPE
            SYNTAX Cdx6500T1E1VGEntry 
            ACCESS not-accessible
            STATUS mandatory
            DESCRIPTION
                "A T1E1VG Table Entry. Each entry
                 contains the configuration parameters for a single
                 T1E1VG  Table."
            INDEX { cdx6500T1E1VGCfgEntryNumber }
        ::= { cdx6500T1E1VGTable 1 }

        Cdx6500T1E1VGEntry ::=
            SEQUENCE {
