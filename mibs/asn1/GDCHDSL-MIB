	SYNTAX  GdcHdslT1CurrentEntry
        ACCESS  not-accessible
        STATUS  mandatory
        DESCRIPTION
                "An entry in the Current table."
        INDEX   { gdcHdslT1CurrentIndex }
        ::= { gdcHdslT1CurrentTable 1 }

GdcHdslT1CurrentEntry ::=
         SEQUENCE {
                 gdcHdslT1CurrentIndex
                 SCinstance,
        	 gdcHdslT1CurrentStats
		 OCTET STRING
         }

gdcHdslT1CurrentIndex OBJECT-TYPE
	SYNTAX SCinstance
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION
    		"The index value which uniquely identifies the interface to
    		which this entry is applicable.  This has the form of a
    		SCinstance which defines the slot, line, drop, and interface,
    		which in this case can be an T1 interface."
        ::= { gdcHdslT1CurrentEntry 1 }


gdcHdslT1CurrentStats OBJECT-TYPE
        SYNTAX OCTET STRING (SIZE(16))
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
               "The number of Current ES, SES, UAS, BES,LOFC,CSS.
		encountered by  T1 interface in the current 15
                minute interval."
        ::= {gdcHdslT1CurrentEntry 2 }



-- ********************************************************************
-- HDSL T1 Interval  Performance Tables	  // applicable for T1 interface only
-- ********************************************************************
-- The Interval Table contains various statistics
-- collected by each T1 Interface over the previous 24 hours of
-- operation.  The past 24 hours are broken into 96 completed
-- 15 minute intervals.

gdcHdslT1IntervalTable OBJECT-TYPE
	SYNTAX  SEQUENCE OF GdcHdslT1IntervalEntry
        ACCESS  not-accessible
        STATUS  mandatory
        DESCRIPTION
                "The  T1 Interval table."
        ::= { gdcHdslPerformance 6 }

gdcHdslT1IntervalEntry OBJECT-TYPE
        SYNTAX  GdcHdslT1IntervalEntry
        ACCESS  not-accessible
        STATUS  mandatory
        DESCRIPTION
                "An entry in the Interval table."
        INDEX   { gdcHdslT1IntervalIndex, gdcHdslT1IntervalNumber }
        ::= { gdcHdslT1IntervalTable 1 }

GdcHdslT1IntervalEntry ::=
         SEQUENCE {
             gdcHdslT1IntervalIndex
                 SCinstance,
             gdcHdslT1IntervalNumber
                 INTEGER,
              gdcHdslT1IntervalStats
	 OCTET STRING
         }

gdcHdslT1IntervalIndex OBJECT-TYPE
	SYNTAX  SCinstance
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION
    		"The index value which uniquely identifies the interface to
    		which this entry is applicable.  This has the form of a
    		SCinstance which defines the slot, line, drop, and interface,
    		which in this case is T1 interface."
    	::= { gdcHdslT1IntervalEntry 1 }    	   

gdcHdslT1IntervalNumber OBJECT-TYPE
	SYNTAX  INTEGER (1..96)
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION
                "A number between 1 and 96, where 1 is the most
                recently completed 15 minute interval and 96 is
                the least recently completed 15 minutes  inter-
                val   (assuming   that  all  96  intervals  are
                valid)."
        ::= { gdcHdslT1IntervalEntry 2 }


gdcHdslT1IntervalStats OBJECT-TYPE
        SYNTAX OCTET STRING (SIZE(16))
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
    		" This variable represents stats(ES, SES, UAS, BES,LOFC,CSS.) for T1 interface."
    	::= { gdcHdslT1IntervalEntry 3}


