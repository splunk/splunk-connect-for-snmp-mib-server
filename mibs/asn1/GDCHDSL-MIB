-- ********************************************************************
-- GDC HDSL Diagnostics
-- ********************************************************************

-- Diagnostics pertain to the E1/T1 and Loop interfaces only.
--

gdcHdslDiagTable OBJECT-TYPE
    	SYNTAX SEQUENCE OF GdcHdslDiagEntry
    	ACCESS not-accessible
    	STATUS mandatory
    	DESCRIPTION
    		"The GDC HDSL Diagnostics table. "
	::= { gdcHdslDiagnostics 1 }

gdcHdslDiagEntry OBJECT-TYPE
    	SYNTAX GdcHdslDiagEntry
    	ACCESS not-accessible
    	STATUS mandatory
    	DESCRIPTION
    		"The GDC HDSL Diagnostics table entry. "
	INDEX { gdcHdslDiagIndex }
    	::= { gdcHdslDiagTable 1 }

GdcHdslDiagEntry ::= 
    	SEQUENCE {
                        gdcHdslDiagIndex SCinstance,
			gdcHdslLoopback INTEGER,
			gdcHdslBertTest INTEGER
	    }

gdcHdslDiagIndex OBJECT-TYPE
    	SYNTAX SCinstance
    	ACCESS read-only
    	STATUS mandatory
    	DESCRIPTION
    		"The index value which uniquely identifies the interface to
	    	which this entry is applicable.  This has the form of a
	    	SCinstance which defines the slot, line, drop, and sub-identifier,
	    	which in this case is a network interface."
    	::= { gdcHdslDiagEntry 1 }

gdcHdslLoopback OBJECT-TYPE
    	SYNTAX INTEGER {
			noLoop(1),
			lineLoop(2),
			localLoop(3),
			lineandlocalLoop(4),
--			payload(5)
			dataLoop(6),
			remoteLoop(7)
			}
    	ACCESS read-write
    	STATUS mandatory
    	DESCRIPTION
    		" This variable supports the action of a diagnostic loop 
		  at the point indicated.
		  dataLoop & remoteLoop are only supported by uas7722 "
    	::= { gdcHdslDiagEntry 2 }

gdcHdslBertTest OBJECT-TYPE
    	SYNTAX INTEGER {
			inhibit(1),
			enable(2),
			reset(3)
	        }			
    	ACCESS read-write
    	STATUS mandatory
    	DESCRIPTION
    		" This variable supports the action of Bit error rate test. When
		set to inhibit(1) no bert test is in process. When set to enable (2)
		BERT is in process"

    	::= { gdcHdslDiagEntry 3 }


-- ********************************************************************
-- GDC HDSL Diagnostic Results
-- ********************************************************************

gdcHdslDiagResultsTable OBJECT-TYPE
    	SYNTAX SEQUENCE OF GdcHdslDiagResultsEntry
    	ACCESS not-accessible
    	STATUS mandatory
    	DESCRIPTION
    		"The GDC HDSL Diagnostics table. "
	::= { gdcHdslDiagnosticResults 1 }

gdcHdslDiagResultsEntry OBJECT-TYPE
    	SYNTAX GdcHdslDiagResultsEntry
    	ACCESS not-accessible
    	STATUS mandatory
    	DESCRIPTION
    		"The GDC HDSL Diagnostics table entry. "
	INDEX { gdcHdslDiagResultsIndex }
    	::= { gdcHdslDiagResultsTable 1 }

GdcHdslDiagResultsEntry ::= 
    	SEQUENCE {
                        gdcHdslDiagResultsIndex SCinstance,
		        gdcHdslTestExecutionStatus INTEGER,
			gdcHdslDiagResultErr INTEGER,
		        gdcHdslDiagResultInterval INTEGER,
			gdcHdslV54RDLTest	INTEGER
	    }

gdcHdslDiagResultsIndex OBJECT-TYPE
    	SYNTAX SCinstance
    	ACCESS read-only
    	STATUS mandatory
    	DESCRIPTION
    		"The index value which uniquely identifies the interface to
	    	which this entry is applicable.  This has the form of a
	    	SCinstance which defines the slot, line, drop, and sub-identifier,
	    	which in this case is a network interface."
    	::= { gdcHdslDiagResultsEntry 1 }

gdcHdslTestExecutionStatus OBJECT-TYPE
    	SYNTAX INTEGER {
    			inSync(1),		
    			notInSync(2)
    		}
    	ACCESS read-only
    	STATUS mandatory
    	DESCRIPTION
    		"The current execution status of the diagnostic test.
		When set to inSync(1), BERT test is in sync and BER rate is valid.
		When set to notInSync(2), BERT test is not in Sync and BER rate is 
		not valid. "
    	::= { gdcHdslDiagResultsEntry 2 }

gdcHdslDiagResultErr OBJECT-TYPE
    	SYNTAX INTEGER (0..65535)
    	ACCESS read-only
    	STATUS mandatory
    	DESCRIPTION
    		"The results of the last diagnostic test.  This can be the
	    	current test running or the last completed test.

    		Note that the interpretation of these results may be affected
	    	by the value of the gdcTestExecutionStatus object."
    	::= { gdcHdslDiagResultsEntry 3 }

gdcHdslDiagResultInterval OBJECT-TYPE
    	SYNTAX INTEGER (0..65535)
    	ACCESS read-only
    	STATUS mandatory
    	DESCRIPTION
    		"This variable represents the Ber test intervals.
		A time interval is defined as the time required for transmission
		of a block of 2^24 bits. "
    	::= { gdcHdslDiagResultsEntry 4 }

gdcHdslV54RDLTest OBJECT-TYPE
    	SYNTAX INTEGER {
    			notInV54Test(1),		
    			txV54Test(2)
    		}
    	ACCESS read-only
    	STATUS mandatory
    	DESCRIPTION
    		"This variable indicates that the front panel RL button is selected
		and transmitting a V54 pattern. Applicable only to DC730 units."
    	::= { gdcHdslDiagResultsEntry 5 }


-- ********************************************************************
-- HDSL Current Performance Tables  //Not applicable for T1 interface
-- ********************************************************************
-- The current table contains various statistics being
-- collected for the current 15 minute interval.
  
gdcHdslCurrentTable OBJECT-TYPE
	SYNTAX  SEQUENCE OF GdcHdslCurrentEntry
        ACCESS  not-accessible
        STATUS  mandatory
        DESCRIPTION
		"The GDC Hdsl Current Performance table.  Information
		 in the entries of this table support the HDSL Scheduled
		 Performance Reports."
        ::= { gdcHdslPerformance 1 }

gdcHdslCurrentEntry OBJECT-TYPE
	SYNTAX  GdcHdslCurrentEntry
        ACCESS  not-accessible
        STATUS  mandatory
        DESCRIPTION
                "An entry in the Current table."
        INDEX   { gdcHdslCurrentIndex }
        ::= { gdcHdslCurrentTable 1 }

GdcHdslCurrentEntry ::=
         SEQUENCE {
             gdcHdslCurrentIndex
                 SCinstance,
             gdcHdslCurrentESs
                 Gauge,
             gdcHdslCurrentSESs
                 Gauge,
             gdcHdslCurrentUASs
                 Gauge,
             gdcHdslCurrentDMs
                 Gauge,
             gdcHdslCurrentFEBEs
                 Gauge,
	     gdcHdslCurrentStats
		 OCTET STRING
         }

gdcHdslCurrentIndex OBJECT-TYPE
	SYNTAX SCinstance
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION
    		"The index value which uniquely identifies the interface to
    		which this entry is applicable.  This has the form of a
    		SCinstance which defines the slot, line, drop, and interface,
    		which in this case can be an E1 or Loop interface."
        ::= { gdcHdslCurrentEntry 1 }

gdcHdslCurrentESs OBJECT-TYPE
        SYNTAX  Gauge
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION
                "The number of Errored Seconds, encountered  by
                an E1 or Loop interface in the current 15 minute inter-
                val."
        ::= { gdcHdslCurrentEntry 2 }

gdcHdslCurrentSESs OBJECT-TYPE
        SYNTAX  Gauge
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION
                "The number of Severely Errored Seconds encoun-
                tered  by  a Loop or E1 interface  in the current 15
                minute interval."
        ::= { gdcHdslCurrentEntry 3 }

gdcHdslCurrentUASs OBJECT-TYPE
        SYNTAX  Gauge
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION
                "The number of Unavailable Seconds  encountered
                by a Loop or E1 interface in the current 15 minute in-
                terval."
        ::= { gdcHdslCurrentEntry 4 }

gdcHdslCurrentDMs OBJECT-TYPE
        SYNTAX Gauge
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
                "The number of Degraded Minutes  (DMs)  encoun-
                tered  by  a Loop or E1  interface  in the current 15
                minute interval."
        ::= { gdcHdslCurrentEntry 5 }

gdcHdslCurrentFEBEs OBJECT-TYPE
        SYNTAX Gauge
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
                "The number of Far End Block Errors encountered
                by a Loop or E1 interface in the current 15
                minute interval."
        ::= {gdcHdslCurrentEntry 6 }

gdcHdslCurrentStats OBJECT-TYPE
        SYNTAX OCTET STRING (SIZE(15))
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
               "The number of Current ES, SES, UAS, DM and FEBE's
		encountered by a Loop or E1 interface in the current 15
                minute interval."
        ::= {gdcHdslCurrentEntry 7 }


-- ********************************************************************
-- HDSL Interval  Performance Tables   //Not applicable for T1 interface
-- ********************************************************************
-- The Interval Table contains various statistics
-- collected by each Loop or E1 Interface over the previous 24 hours of
-- operation.  The past 24 hours are broken into 96 completed
-- 15 minute intervals.

gdcHdslIntervalTable OBJECT-TYPE
	SYNTAX  SEQUENCE OF GdcHdslIntervalEntry
        ACCESS  not-accessible
        STATUS  mandatory
        DESCRIPTION
                "The Loop or E1 Interval table."
        ::= { gdcHdslPerformance 2 }

gdcHdslIntervalEntry OBJECT-TYPE
        SYNTAX  GdcHdslIntervalEntry
        ACCESS  not-accessible
        STATUS  mandatory
        DESCRIPTION
                "An entry in the Interval table."
        INDEX   { gdcHdslIntervalIndex, gdcHdslIntervalNumber }
        ::= { gdcHdslIntervalTable 1 }

GdcHdslIntervalEntry ::=
         SEQUENCE {
             gdcHdslIntervalIndex
                 SCinstance,
             gdcHdslIntervalNumber
                 INTEGER,
             gdcHdslIntervalESs
                 Gauge,
            gdcHdslIntervalSESs
                 Gauge,
             gdcHdslIntervalUASs
                 Gauge,
             gdcHdslIntervalDMs
                 Gauge,
             gdcHdslIntervalFEBEs
                 Gauge,
	     gdcHdslIntervalStats
		 OCTET STRING
         }

gdcHdslIntervalIndex OBJECT-TYPE
	SYNTAX  SCinstance
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION
    		"The index value which uniquely identifies the interface to
    		which this entry is applicable.  This has the form of a
    		SCinstance which defines the slot, line, drop, and interface,
    		which in this case is a Loop or E1 interface."
    	::= { gdcHdslIntervalEntry 1 }    	   

gdcHdslIntervalNumber OBJECT-TYPE
	SYNTAX  INTEGER (1..96)
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION
                "A number between 1 and 96, where 1 is the most
                recently completed 15 minute interval and 96 is
                the least recently completed 15 minutes  inter-
                val   (assuming   that  all  96  intervals  are
                valid)."
        ::= { gdcHdslIntervalEntry 2 }


gdcHdslIntervalESs OBJECT-TYPE
        SYNTAX  Gauge
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION
                "The number of Errored Seconds encountered by a
                Loop or E1  interface in one of the previous 96, indi-
                vidual 15 minute, intervals."
        ::= { gdcHdslIntervalEntry 3 }

gdcHdslIntervalSESs OBJECT-TYPE
        SYNTAX  Gauge
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION
                "The number of Severely Errored Seconds encoun-
                tered by a Loop or E1 interface in one of the previous
                96, individual 15 minute, intervals."
        ::= { gdcHdslIntervalEntry 4 }

gdcHdslIntervalUASs OBJECT-TYPE
        SYNTAX  Gauge
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION
                "The number of Unavailable Seconds  encountered
                by  a  Loop or E1 interface in one of the previous 96,
                individual 15 minute, intervals."
        ::= { gdcHdslIntervalEntry 5 }

gdcHdslIntervalDMs OBJECT-TYPE
        SYNTAX Gauge
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
                "The number of Degraded Minutes  (DMs)  encoun-
                tered by a Loop or E1 interface in one of the previous
                96, individual 15 minute, intervals."
        ::= { gdcHdslIntervalEntry 6 }

gdcHdslIntervalFEBEs OBJECT-TYPE
        SYNTAX Gauge
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
    		" This variable represents the HDSL loops Far end block errors."
    	::= { gdcHdslIntervalEntry 7 }

gdcHdslIntervalStats OBJECT-TYPE
        SYNTAX OCTET STRING (SIZE(15))
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
    		" This variable represents stats at the HDSL loops."
    	::= { gdcHdslIntervalEntry 8 }


-- ********************************************************************
-- HDSL Total Performance Tables	//Not applicable for T1 interface
-- ********************************************************************
-- The Loop/E1 Total Table contains the cumulative sum of the
-- various statistics for the 24 hour period preceding the
-- current interval.

gdcHdslTotalTable OBJECT-TYPE
	SYNTAX  SEQUENCE OF GdcHdslTotalEntry
        ACCESS  not-accessible
        STATUS  mandatory
        DESCRIPTION
	"The GDC HDSL Total Performance table.  Information 
	 in the entries of this table support the HDSL 
	 Scheduled Performance Reports. 24 hour interval."
        ::= { gdcHdslPerformance 3 }

gdcHdslTotalEntry OBJECT-TYPE
        SYNTAX  GdcHdslTotalEntry
        ACCESS  not-accessible
        STATUS  mandatory
        DESCRIPTION
           "An entry in the Total table."
        INDEX   { gdcHdslTotalIndex }
           ::= { gdcHdslTotalTable 1 }

GdcHdslTotalEntry ::=
         SEQUENCE {
             gdcHdslTotalIndex
                 SCinstance,
             gdcHdslTotalESs
                 Gauge,
             gdcHdslTotalSESs
                 Gauge,
             gdcHdslTotalUASs
                 Gauge,
             gdcHdslTotalDMs
                 Gauge,
             gdcHdslTotalFEBEs
                 Gauge,
             gdcHdslTotalStats
                 OCTET STRING
         }

gdcHdslTotalIndex OBJECT-TYPE
	SYNTAX  SCinstance
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION
    		"The index value which uniquely identifies the interface to
    		which this entry is applicable.  This has the form of a
    		SCinstance which defines the slot, line, drop, and interface,
    		which in this case is a Loop or E1 interface."
        ::= { gdcHdslTotalEntry 1 }

gdcHdslTotalESs OBJECT-TYPE
        SYNTAX  Gauge
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION
                "The number of Errored Seconds encountered by a
                Loop or E1interface in the previous 24 hour interval"
        ::= { gdcHdslTotalEntry 2 }

gdcHdslTotalSESs OBJECT-TYPE
        SYNTAX  Gauge
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION
                "The number of Severely Errored Seconds encoun-
                tered  by  a  Loop or E1 interface in the previous 24
                hour interval."
        ::= { gdcHdslTotalEntry 3 }

gdcHdslTotalUASs OBJECT-TYPE
        SYNTAX  Gauge
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION
                "The number of Unavailable Seconds  encountered
                by  a Loop or E1 interface in the previous 24 hour in-
                terval."
        ::= { gdcHdslTotalEntry 4 }

gdcHdslTotalDMs OBJECT-TYPE
        SYNTAX Gauge
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
                "The number of Degraded Minutes  (DMs)  encoun-
                tered  by  a  Loop or E1  interface in the previous 24
                hour interval."
        ::= { gdcHdslTotalEntry 5 }

gdcHdslTotalFEBEs OBJECT-TYPE
        SYNTAX Gauge
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
                "The number of Far End Block Errors  en-
                countered  by a Loop or E1 interface in the current 15
                minute interval."
        ::= {gdcHdslTotalEntry 6 }

gdcHdslTotalStats OBJECT-TYPE
        SYNTAX OCTET STRING (SIZE(15))
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
                "The number of Total stats encountered  by a Loop or E1
		 interface in the current 15 minute interval."
        ::= {gdcHdslTotalEntry 7 }



--*******************************************************************
-- Loop/E1/T1 Performance Interval Maintenace Table
--*******************************************************************

gdcHdslIntervalMaintenanceTable OBJECT-TYPE
	SYNTAX SEQUENCE OF GdcHdslIntervalMaintenanceEntry
     	ACCESS not-accessible
     	STATUS mandatory
    	DESCRIPTION
     		"The GDC Loop/E1/T1 Interval  table. "
         ::= { gdcHdslPerformance 4 }
 	
gdcHdslIntervalMaintenanceEntry OBJECT-TYPE
 	SYNTAX GdcHdslIntervalMaintenanceEntry
     	ACCESS not-accessible
     	STATUS mandatory
     	DESCRIPTION
     		"The GDC Loop/E1/T1  Interval table entry. "
     	INDEX { gdcHdslIntervalMaintenanceIndex }
     	::= { gdcHdslIntervalMaintenanceTable 1 }
 
GdcHdslIntervalMaintenanceEntry ::= 
     	SEQUENCE {
     	    		gdcHdslIntervalMaintenanceIndex SCinstance,
         		gdcHdslResetIntervals INTEGER,
			gdcHdslNumberofValidIntervals INTEGER,
			gdcHdslResetMajorAlarm	INTEGER,
			gdcHdslResetMinorAlarm	INTEGER			
     		}
 
gdcHdslIntervalMaintenanceIndex OBJECT-TYPE
     	SYNTAX SCinstance
     	ACCESS read-only
     	STATUS mandatory
     	DESCRIPTION
     		"The index value which uniquely identifies the interface to
     		which this entry is applicable.  This has the form of a
     		SCinstance which defines the slot, line, drop, and interface,
     		which in this case is a Loop or E1/T1 interface."
     	::= { gdcHdslIntervalMaintenanceEntry 1 }  

 
gdcHdslResetIntervals OBJECT-TYPE
     	SYNTAX INTEGER {
     			norm(1),
     			reset(2)	
     		}
     	ACCESS read-write 
     	STATUS mandatory
         DESCRIPTION " This variable is used to reset Loop/E1/T1 performance intervals.
 			When it is set to reset, the Loop/E1/T1 performance tables are
 			reset to zero."
     	::= { gdcHdslIntervalMaintenanceEntry 2 }

gdcHdslNumberofValidIntervals OBJECT-TYPE
     	SYNTAX INTEGER (1..96)
     	ACCESS read-only 
     	STATUS mandatory
         DESCRIPTION " This variable is used to read the number of intervals collected.
		       Each interval is an increment of 15 minutes."
     	::= { gdcHdslIntervalMaintenanceEntry 3 }

gdcHdslResetMajorAlarm	OBJECT-TYPE
	SYNTAX	INTEGER {
			clearAlarm(1), -- write only
			normalAlarm(2) -- read only
			}
     	ACCESS read-write 
     	STATUS mandatory
         DESCRIPTION " This variable is used to reset alarm per Loop basis."
     	::= { gdcHdslIntervalMaintenanceEntry 4 }
 
gdcHdslResetMinorAlarm	OBJECT-TYPE
	SYNTAX	INTEGER {
			clearAlarm(1), -- write only
			normalAlarm(2) -- read only
			}
     	ACCESS read-write 
     	STATUS mandatory
         DESCRIPTION " This variable is used to reset alarm per Loop basis."
     	::= { gdcHdslIntervalMaintenanceEntry 5 }

--
-- ********************************************************************
-- HDSL T1 Current Performance Tables  // applicable for T1 interface only
-- ********************************************************************
-- The current table contains various statistics being
-- collected for the current 15 minute interval.
  
