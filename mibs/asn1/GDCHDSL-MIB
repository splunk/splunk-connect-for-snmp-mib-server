 			gdcHdslDteDTRstatus
 				INTEGER,
 		        gdcHdslDteDCDstatus
 				INTEGER,
 			gdcHdslDteRTSstatus
 				INTEGER,
 			gdcHdslDteDSRstatus
 				INTEGER,
			gdcHdslDteStatus
				OCTET STRING
     		}

gdcHdslDteStatusIndex OBJECT-TYPE
	SYNTAX SCinstance
	ACCESS read-only
	STATUS mandatory
	DESCRIPTION
     		"The index value which uniquely identifies the interface to
     		which this entry is applicable.  This has the form of a
     		SCinstance which defines the slot, line, drop. "
	::= { gdcHdslDteStatusEntry 1 }
 
gdcHdslDteDTRstatus OBJECT-TYPE
	SYNTAX	INTEGER {
 			off(1),
 			on(2)
 		}
        ACCESS	read-only
        STATUS	mandatory
        DESCRIPTION
 		"DTR EIA status indicator "
        ::={ gdcHdslDteStatusEntry 2 }
 

 gdcHdslDteDCDstatus OBJECT-TYPE
         SYNTAX	INTEGER {
 			off(1),
 			on(2)
 		}
