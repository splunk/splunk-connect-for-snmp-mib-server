         SYNTAX	INTEGER {
 			off(1),
 			on(2)
 		}
         ACCESS	read-only
         STATUS	mandatory
         DESCRIPTION
 		"DSR EIA indicator"
         ::={ gdcHdslDteStatusEntry 5 }
 
gdcHdslDteStatus	OBJECT-TYPE	
	 SYNTAX		OCTET STRING (SIZE(1))
         ACCESS	read-only
         STATUS	mandatory
         DESCRIPTION
			"Dte leads status
		P1.7 not used
		  .6 EIA DCD
		  .5 EIA DTR
		  .4 EIA RTS
		  .3 EIA DSR
		  .2 not used
		  .1 not used
		  .0 not used     "

         ::={ gdcHdslDteStatusEntry 6 } 

-- ********************************************************************
-- GDC HDSL Fractionalization
-- ********************************************************************

hdslFracTable OBJECT-TYPE
	SYNTAX  SEQUENCE OF HdslFracEntry
        ACCESS  not-accessible
        STATUS  mandatory
        DESCRIPTION
                "The DS1 Fractional table."
        ::= { gdcFractional 1 }

hdslFracEntry OBJECT-TYPE
        SYNTAX  HdslFracEntry
        ACCESS  not-accessible
        STATUS  mandatory
        DESCRIPTION
                "An entry in the DS1 Fractional table."
        INDEX   { hdslFracIndex }
        ::= { hdslFracTable 1 }

HdslFracEntry ::=
        SEQUENCE {
             hdslFracIndex
                 SCinstance,
             channelmapping
                 INTEGER,
	     gdcHdslHighway
		 INTEGER,
             gdcHdslTimeSlot16
                 INTEGER
--           numberofTimeSlots
--                 INTEGER
         }

hdslFracIndex OBJECT-TYPE
        SYNTAX SCinstance
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
     		"The index value which uniquely identifies the interface to
     		which this entry is applicable.  This has the form of a
     		SCinstance which defines the slot, line, drop. "
        ::= { hdslFracEntry 1 }

channelmapping OBJECT-TYPE
        SYNTAX  INTEGER 
        ACCESS  read-write
        STATUS  mandatory
        DESCRIPTION
                "The object contains the channel mapping for multipoint. Time slot 0 is used
		  in all loops.

		P1.8 f/u
		P1.7 \
		P1.6  \
		P1.5   \
		P1.4    \   This byte contains the starting time slot
		P1.3    /
		P1.2   /
		P1.1  /
		P1.0 /

		P2.8 f/u
		P2.7 \
		P2.6  \
		P2.5   \
		P2.4    \   This byte contains the number of consecutive time slots
		P2.3    /
		P2.2   /
		P2.1  /
		P2.0 /"

        ::= { hdslFracEntry 2 }

gdcHdslHighway      OBJECT-TYPE
    SYNTAX INTEGER {
        notAssigned(1),
        highway1(2),
        highway2(3),
        highway3(4),
        highway4(5),
        highway5(6),
        highway6(7),
        highway7(8),
        highway8(9)
       }
    ACCESS read-write
    STATUS mandatory
    DESCRIPTION
        "Object to select the data highway for Loop n."
    ::= { hdslFracEntry 3 }

gdcHdslTimeSlot16  OBJECT-TYPE
    SYNTAX INTEGER {
               used(1),
               skipped(2)
    }
    ACCESS read-write
    STATUS mandatory
    DESCRIPTION
        "Object to select if Time Slot 16 is being used either to pass data,
         or skipped from passing any data for Loop n."
    ::= { hdslFracEntry 4 }

--numberofTimeSlots OBJECT-TYPE
--        SYNTAX  INTEGER (1..18)
--        ACCESS  read-write
--        STATUS  mandatory
--        DESCRIPTION
--                "Number of consecutive time slots for this entry."
--        ::= { hdslFracEntry 3 }

--*****************************************************************************
--       Alarm Config  Table FOR New Products.(currently SC710D2 is supported)
--*****************************************************************************
hdslAlarmConfigTable OBJECT-TYPE
     SYNTAX SEQUENCE OF HdslAlarmConfigEntry
     ACCESS not-accessible
     STATUS mandatory
     DESCRIPTION
	 "The hdslAlarmConfigTable contains entries that configure alarm
	 reporting.  The structure of the table is such that alarm
	 configuration is supported on a unit and interface basis, and
	 then on an alarm type basis within the interface.  For
	 simplicity sake alarms, be they unit or interface
	 related, are represented in one table."
     ::= { gdcHdslAlarmThresholds 1 }

hdslAlarmConfigEntry OBJECT-TYPE
     SYNTAX HdslAlarmConfigEntry
     ACCESS not-accessible
     STATUS mandatory
     DESCRIPTION
	 "An entry in the hdslAlarmConfigTable table."
     INDEX { hdslAlarmConfigIndex, hdslAlarmConfigIdentifier }
    ::= { hdslAlarmConfigTable 1 }

HdslAlarmConfigEntry ::=
     SEQUENCE {
	 hdslAlarmConfigIndex
     SCinstance,
	 hdslAlarmConfigIdentifier
	     OBJECT IDENTIFIER,
	 hdslAlarmThreshold
	     INTEGER
	     }

hdslAlarmConfigIndex OBJECT-TYPE
     SYNTAX SCinstance
     ACCESS read-only
     STATUS mandatory
     DESCRIPTION
	 "The index value which uniquely identifies the interface to
	 which this entry is applicable."
     ::= { hdslAlarmConfigEntry 1 }

hdslAlarmConfigIdentifier OBJECT-TYPE
     SYNTAX OBJECT IDENTIFIER
     ACCESS read-only
     STATUS mandatory
     DESCRIPTION
	 "The unique alarm identifier assigned to this alarm type.
	 The format of this identifier is an OBJECT IDENTIFIER that has
	 the following format: {iso(1) org(3) dod(6) internet(1)
	 private(4) enterprises(1) gdc(498) xxx(x) alarm(z) yyy(y) 
	 where xxx(x) is the administratively assigned family object 
	 identifier (z) is the object identifier for alarms in the family
	 defined MIB and yyy(y) is the administratively assigned alarm 
	 type identifier for this alarm."
     ::= { hdslAlarmConfigEntry 2 }

hdslAlarmThreshold OBJECT-TYPE
    SYNTAX INTEGER {
			thres1E04(1),
			thres1E05(2),
			thres1E06(3),
			thres1E07(4),
			thres1E08(5)
--			thres1E09(6)
		}
	ACCESS  read-write
	STATUS  mandatory
	DESCRIPTION
		"This function sets/reads the Major or Minor BER alarm threshold criteria."
   ::= { hdslAlarmConfigEntry 3 }

END
