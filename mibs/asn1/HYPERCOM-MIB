    --Table of hypHldcDropStatus
    --hypHldcDropStatus description (Table format)
    hypHldcDropStatusTable OBJECT-TYPE
        SYNTAX   SEQUENCE OF HypHldcDropStatusEntry
        ACCESS   not-accessible
        STATUS   mandatory
        DESCRIPTION
        "A table containing information about
         drop status"
        ::= { hypHdlc 52 }

        hypHldcDropStatusEntry OBJECT-TYPE
        SYNTAX   HypHldcDropStatusEntry
        ACCESS   not-accessible
        STATUS   mandatory
        DESCRIPTION
        "drop status"
        INDEX { hypHldcDropStatusIndex }
        ::= { hypHldcDropStatusTable 1 }

        HypHldcDropStatusEntry ::=
        SEQUENCE {
            hypHldcDropStatusIndex
                INTEGER,
            hypHldcDropHdlcAddress
                INTEGER,
            hypHldcDropControlStatus
                INTEGER,
            hypHldcDropMsgIn
                Counter,
            hypHldcDropMsgOut
                Counter
            }

        hypHldcDropStatusIndex  OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "The ifIndex Value of the corresponding ifEntry."
        ::= { hypHldcDropStatusEntry 1 }

        hypHldcDropHdlcAddress  OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "HDLC controller address"
        ::= { hypHldcDropStatusEntry 2 }

        hypHldcDropControlStatus  OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "Controller status"
        ::= { hypHldcDropStatusEntry 3 }

        hypHldcDropMsgIn  OBJECT-TYPE
        SYNTAX Counter
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "Count of incomming messages"
        ::= { hypHldcDropStatusEntry 4 }

        hypHldcDropMsgOut  OBJECT-TYPE
        SYNTAX Counter
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "Count of outgoing messages"
        ::= { hypHldcDropStatusEntry 5 }




    --End of Table hypHldcDropStatus  drop status


    -- The sdlc Group
    -- The sdlc Group contains information about the
    -- sdlc Parameters in a Hypercom IEN.

    hypSdlcTableTable OBJECT-TYPE
        SYNTAX   SEQUENCE OF HypSdlcTableEntry
        ACCESS   not-accessible
        STATUS   mandatory
        DESCRIPTION
        "A table containing information about
         SDLC Table"
        ::= { hypSdlc 1 }

        hypSdlcTableEntry OBJECT-TYPE
        SYNTAX   HypSdlcTableEntry
        ACCESS   not-accessible
        STATUS   mandatory
        DESCRIPTION
        "SDLC Table"
        INDEX { hypSdlcTableIndex }
        ::= { hypSdlcTableTable 1 }

        HypSdlcTableEntry ::=
        SEQUENCE {
            hypSdlcIfIndex
                Counter,
            hypSdlcRexmits
                Counter,
            hypSdlcFrameErrs
                Counter,
            hypSdlcInSnrm
                Counter,
            hypSdlcOutSnrp
                Counter,
            hypSdlcInRnrs
                Counter,
            hypSdlcOutRnrs
                Counter,
            hypSdlcInTestFrames
                Counter,
            hypSdlcOutTestFrames
                Counter,
            hypSdlcInDm
                Counter,
            hypSdlcOutDm
                Counter,
            hypSdlcInUa
                Counter,
            hypSdlcOutUa
                Counter,
            hypSdlcInFrmr
                Counter,
            hypSdlcOutFrmr
                Counter
            }


