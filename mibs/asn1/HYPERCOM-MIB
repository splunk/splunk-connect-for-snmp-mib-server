        STATUS   mandatory
        DESCRIPTION
        "Frame Relay PVC interface"
        INDEX { hypFrPVCirStatusIndex }
        ::= { hypFrPVCirStatusTable 1 }

        HypFrPVCirStatusEntry ::=
        SEQUENCE {
            hypFrPVCIfIndex
                INTEGER,
            hypFrPVCDLCINumber
                INTEGER,
            hypFrPVCStatus
                INTEGER,
            hypFrPVCframesReceived
                Counter,
            hypFrPVCframesSent
                Counter
            }

        hypFrPVCIfIndex  OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "The ifIndex  (FR Card Instance)"
        ::= { hypFrPVCirStatusEntry 1 }

