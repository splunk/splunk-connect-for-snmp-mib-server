


    -- hypercom MIB
    -- Request for Comments: Draft             Hypercom Inc.


       HYPERCOM-MIB DEFINITIONS ::= BEGIN


    -- Title:      Hypercom Concise MIB



    -- Rev  :      R2.0.0    24 APR. 1992
    --      :      R2.0.1    08 SEP. 1992
    --      :      R2.0.2    10 FEB. 1993
    --      :      R2.0.3    07 OCT. 1993
    --      :      R2.0.4    10 NOV. 1993
    --      :      R2.0.5    07 JAN. 1994
    --      :      R2.0.6    27 JAN. 1994
    --      :      R2.0.7    02 SEP. 1994
    --      :      R2.0.8    02 JUN. 1995
    --      :      R2.0.9    10 SEP  1995
    --      :      R2.1.0    03 OCT. 1995
	--		:	   R2.1.1	 30 SEP. 2002
	--      :      R2.1.2    06 NOV. 2002



       IMPORTS
           OBJECT-TYPE
               FROM RFC-1212
           enterprises, Counter, TimeTicks, NetworkAddress, Gauge
               FROM RFC1155-SMI
           mib-2
               FROM RFC1213-MIB
           MacAddress, DisplayString
               FROM SNMPv2-TC
           TRAP-TYPE
               FROM RFC-1215;



    --   MacAddress ::= OCTET STRING (SIZE (6))     (see RFC1286)


        hypercom           OBJECT IDENTIFIER ::= { enterprises 626 }
        hypercomIEN        OBJECT IDENTIFIER ::= { hypercom 2 }
        hypIenMib             OBJECT IDENTIFIER ::= { hypercomIEN 1 }

        hypSystem             OBJECT IDENTIFIER ::= { hypIenMib 1  }
        hypIp                 OBJECT IDENTIFIER ::= { hypIenMib 2  }
        hypIcmp               OBJECT IDENTIFIER ::= { hypIenMib 3  }
        hypTcp                OBJECT IDENTIFIER ::= { hypIenMib 4  }
        hypUdp                OBJECT IDENTIFIER ::= { hypIenMib 5  }
        hypIpx                OBJECT IDENTIFIER ::= { hypIenMib 6  }

        hypHdlc               OBJECT IDENTIFIER ::= { hypIenMib 7  }
        hypSdlc               OBJECT IDENTIFIER ::= { hypIenMib 8  }
        hypRtm                OBJECT IDENTIFIER ::= { hypIenMib 9  }
        hypX25                OBJECT IDENTIFIER ::= { hypIenMib 10 }
        hypX28                OBJECT IDENTIFIER ::= { hypIenMib 11 }
        hypSna                OBJECT IDENTIFIER ::= { hypIenMib 12 }

        hypFRelayMib          OBJECT IDENTIFIER ::= { hypIenMib 13 }
        hypDs1Mib             OBJECT IDENTIFIER ::= { hypIenMib 14 }
        hypLanBridgeRouter    OBJECT IDENTIFIER ::= { hypIenMib 15 }
        hypLanStatistics      OBJECT IDENTIFIER ::= { hypIenMib 16 }
        hypBridgeStations     OBJECT IDENTIFIER ::= { hypIenMib 17 }
        hypIpRemoteSubnets    OBJECT IDENTIFIER ::= { hypIenMib 18 }
        hypTokenRingIf        OBJECT IDENTIFIER ::= { hypIenMib 19 }
        hypRemoteLanSegList   OBJECT IDENTIFIER ::= { hypIenMib 20 }
        hypAlarmSystem        OBJECT IDENTIFIER ::= { hypIenMib 21 }
        hypAdmin              OBJECT IDENTIFIER ::= { hypIenMib 22 }

    -- NOTE: Hypercom TRAP's use  hypercomAlarmSystem for the
    --       ENTERPRISE keyword




    -- Future version of this Hypercom private MIB may contain the following:
    --
    -- (p45) IEN Node Port MEMORY access for retrieval, load, copy  (add to system group)
    -- (p31) Stat Threshold Parameters, also reset to default
    -- (p34) Reset NetView Alarm Counters
    -- (p38) DownLoad tables and configuration data
    -- (p53) Bus activity Statistics (incl clear counters)
    --
    -- (p54) VRU (Voice Rec) statistics
    -- (p56) Port Dial Out request and status (status may be a TRAP)
    -- (p60) High Speed Modem status
    -- Modem Eye Pattern Data Monitor
    -- IEN Node DCP/CAS related commands
    -- Test commands
    -- X.25 call establishment
    -- Downloadable system cfg, x.25/SNA  and HDLC/X.28
    -- CIM program download






    -- This is the Private MIB module for Hypercom IEN Systems.
    -- The following groups describe the variables within the Hypercom
    -- product lines. Specific groups may or may not be present
    -- depending on the actual installed card types and the software
    -- options present in the managed IEN device.




    --Table of hypIpTable
    --hypIpTable description (Table format)
    hypSysTableTable OBJECT-TYPE
        SYNTAX   SEQUENCE OF HypSysTableEntry
        ACCESS   not-accessible
        STATUS   mandatory
        DESCRIPTION
        "A table containing information about
         the Hypercom System"
        ::= { hypSystem 1 }

        hypSysTableEntry OBJECT-TYPE
        SYNTAX   HypSysTableEntry
        ACCESS   not-accessible
        STATUS   mandatory
        DESCRIPTION
        "Hypercom System Table"
        INDEX { hypSysTableIndex }
        ::= { hypSysTableTable 1 }

        HypSysTableEntry ::=
        SEQUENCE {
            hypSysBusAddr
                INTEGER,
            hypSysSwName
                DisplayString,
            hypPortMode
                DisplayString,
            hypSysStatus
                DisplayString,
            hypSysCuAddress
                INTEGER,
            hypSysModeSwitch1
                INTEGER,
            hypSysModeSwitch2
                INTEGER,
            hypSysUpTime
                INTEGER,
            hypSysDownTime
                INTEGER,
            hypSysRestart
                INTEGER
            }


    -- The SYSTEM Group
    -- Implementation of this group is mandatory for all
    -- Hypercom IEN Systems although some specific members
    -- may or may not apply to all Hypercom IEN devices.

    hypSysBusAddr  OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "Card BUS Address switch setting"
        ::= { hypSysTableEntry 1 }

    hypSysSwName  OBJECT-TYPE
        SYNTAX DisplayString
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "Name and version number of the SW module  (SIZE (0..20)"
        ::= { hypSysTableEntry 2 }

    hypPortMode  OBJECT-TYPE
        SYNTAX DisplayString
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "Port Card Operating mode ('E'=EPROM, 'R'=RAM)"
        ::= { hypSysTableEntry 3 }

    hypSysStatus  OBJECT-TYPE
        SYNTAX DisplayString
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "Card Display status display values"
        ::= { hypSysTableEntry 4 }

    hypSysCuAddress  OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "Card CU Address switch setting"
        ::= { hypSysTableEntry 5 }

    hypSysModeSwitch1  OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "Card Mode switch 1 setting"
        ::= { hypSysTableEntry 6 }

    hypSysModeSwitch2  OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "Card Mode switch 2 setting"
        ::= { hypSysTableEntry 7 }

    hypSysUpTime  OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "System Up time in minutes"
        ::= { hypSysTableEntry 8 }

    hypSysDownTime  OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "System Down time in minutes"
        ::= { hypSysTableEntry 9 }

    hypSysRestart  OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "System Restart"
        ::= { hypSysTableEntry 10 }




    -- The IP Group
    -- The IP Group contains information about the IP
    -- in a Hypercom IEN.
    -- For clearing counters see the hypSystem group object hypClearStatCounters



    --Table of hypIpTable
    --hypIpTable description (Table format)
    hypIpTableTable OBJECT-TYPE
        SYNTAX   SEQUENCE OF HypIpTableEntry
        ACCESS   not-accessible
        STATUS   mandatory
        DESCRIPTION
        "A table containing information about
         IP Table"
        ::= { hypIp 1 }

        hypIpTableEntry OBJECT-TYPE
        SYNTAX   HypIpTableEntry
        ACCESS   not-accessible
        STATUS   mandatory
        DESCRIPTION
        "IP Table"
        INDEX { hypIpTableIndex }
        ::= { hypIpTableTable 1 }

        HypIpTableEntry ::=
        SEQUENCE {
            hypIpIfIndex
                INTEGER,
            hypIpIn
                Counter,
            hypIpOut
                Counter,
            hypIpAverage
                Gauge,
            hypIpInSec
                Gauge,
            hypIpInMax
                Gauge,
            hypUnknownFrame
                Counter,
            hypTimeToLive
                INTEGER,
            hypMsgIn
                Counter,
            hypMsgOut
                Counter,
            hypUnreachIn
                Counter,
            hypUnreachOut
                Counter,
            hypPingIn
                Counter,
            hypPingOut
                Counter,
            hypDataIn
                Counter,
            hypDataOut
                Counter,
            hypBadFrames
                Counter,
            hypUnknownPort
                Counter
            }

        hypIpIfIndex  OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "The ifIndex  (IP Interface Instance)"
        ::= { hypIpTableEntry 1 }

        hypIpIn  OBJECT-TYPE
        SYNTAX Counter
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "The number of IP packet received since last reset"
        ::= { hypIpTableEntry 2 }

        hypIpOut  OBJECT-TYPE
        SYNTAX Counter
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "The number of IP packet sent since last reset"
        ::= { hypIpTableEntry 3 }

        hypIpAverage  OBJECT-TYPE
        SYNTAX Gauge
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "Average number of IP frame received/sec since last reset"
        ::= { hypIpTableEntry 4 }

        hypIpInSec  OBJECT-TYPE
        SYNTAX Gauge
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "Currently number of IP frame received/sec"
        ::= { hypIpTableEntry 5 }

        hypIpInMax  OBJECT-TYPE
        SYNTAX Gauge
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "Maximum number of IP frame received/sec since last reset"
        ::= { hypIpTableEntry 6 }

        hypUnknownFrame  OBJECT-TYPE
        SYNTAX Counter
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "Unknown protocol frame received"
        ::= { hypIpTableEntry 7 }

        hypTimeToLive  OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "IP Time To Live (TTL) value"
        ::= { hypIpTableEntry 8 }

    hypMsgIn  OBJECT-TYPE
        SYNTAX Counter
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "Messages received"
        ::= { hypIpTableEntry 9 }

    hypMsgOut  OBJECT-TYPE
        SYNTAX Counter
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "Messages sent"
        ::= { hypIpTableEntry 10 }

    hypUnreachIn  OBJECT-TYPE
        SYNTAX Counter
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "Destination unreachable received"
        ::= { hypIpTableEntry 11 }

    hypUnreachOut  OBJECT-TYPE
        SYNTAX Counter
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "Destination unreachable sent"
        ::= { hypIpTableEntry 12 }

    hypPingIn  OBJECT-TYPE
        SYNTAX Counter
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "Pings received"
        ::= { hypIpTableEntry 13 }

    hypPingOut  OBJECT-TYPE
        SYNTAX Counter
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "Pings sent"
        ::= { hypIpTableEntry 14 }

    hypDataIn  OBJECT-TYPE
        SYNTAX Counter
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "Datagrams in"
        ::= { hypIpTableEntry 15 }

    hypDataOut  OBJECT-TYPE
        SYNTAX Counter
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "Datagrams out"
        ::= { hypIpTableEntry 16 }

    hypBadFrames  OBJECT-TYPE
        SYNTAX Counter
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "Bad frames in"
        ::= { hypIpTableEntry 17 }

    hypUnknownPort  OBJECT-TYPE
        SYNTAX Counter
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "Unknown port received"
        ::= { hypIpTableEntry 18 }

    --End of Table hypIpTable  IP Table




    -- The IPX Group
    -- The IPX Group contains information about the IPX
    -- in a Hypercom IEN.
    -- For clearing counters see the hypSystem group object hypClearStatCounters



    --Table of hypIpTable
    --hypIpTable description (Table format)
    hypIpxTableTable OBJECT-TYPE
        SYNTAX   SEQUENCE OF HypIpxTableEntry
        ACCESS   not-accessible
        STATUS   mandatory
        DESCRIPTION
        "A table containing information about
         IPX Table"
        ::= { hypIpx 1 }

        hypIpxTableEntry OBJECT-TYPE
        SYNTAX   HypIpxTableEntry
        ACCESS   not-accessible
        STATUS   mandatory
        DESCRIPTION
        "IPX Table"
        INDEX { hypIpxTableIndex }
        ::= { hypIpxTableTable 1 }

        HypIpxTableEntry ::=
        SEQUENCE {
            hypIpxIfIndex
                INTEGER,
            hypIpx8023pkt
                Counter,
            hypIpxEthIIpkt
                Counter,
            hypIpxSNAPpkt
                Counter,
            hypIpx8022pkt
                Counter,
            hypIpxIn
                Counter,
            hypIpxOut
                Counter,
            hypIpxAlErrs
                Counter,
            hypIpxCrcErrs
                Counter,
            hypIpxRxOvRun
                Counter,
            hypIpxColl
                Counter,
            hypIpxRipSup
                Counter,
            hypIpxRipReqRcv
                Counter,
            hypIpxRipReqSent
                Counter,
            hypIpxRipRspRcved
                Counter,
            hypIpxRipRspSent
                Counter,
            hypIpxSapSup
                Counter,
            hypIpxSapReqRcved
                Counter,
            hypIpxSapReqSent
                Counter,
            hypIpxSapRspRcved
                Counter,
            hypIpxSapRspSent
                Counter
            }
        hypIpxIfIndex  OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "The ifIndex  (IPX Interface Instance)"
        ::= { hypIpxTableEntry 1 }

        hypIpx8023pkt  OBJECT-TYPE
        SYNTAX Counter
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "The number of 802.3 packets"
        ::= { hypIpxTableEntry 2 }

        hypIpxEthIIpkt  OBJECT-TYPE
        SYNTAX Counter
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "The number of Ethernet II packets"
        ::= { hypIpxTableEntry 3 }

        hypIpxSNAP  OBJECT-TYPE
        SYNTAX Counter
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "The number of SNAP packets"
        ::= { hypIpxTableEntry 4 }

        hypIpx8022pkt  OBJECT-TYPE
        SYNTAX Counter
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "The number of 802.2 packets"
        ::= { hypIpxTableEntry 5 }

        hypIpxIn  OBJECT-TYPE
        SYNTAX Counter
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "The number of IPX packet received since last reset"
        ::= { hypIpxTableEntry 6 }

        hypIpxOut  OBJECT-TYPE
        SYNTAX Counter
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "The number of IPX packet sent since last reset"
        ::= { hypIpxTableEntry 7 }

        hypIpxAlErrs OBJECT-TYPE
        SYNTAX Counter
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "The number of IPX packet Alignment Errors "
        ::= { hypIpxTableEntry 8 }

        hypIpxCrcErrs  OBJECT-TYPE
        SYNTAX Counter
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "The number of IPX packet CRC Errors "
        ::= { hypIpxTableEntry 9 }

        hypIpxRxOvRun  OBJECT-TYPE
        SYNTAX Counter
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "The number of IPX packet Receiver Overrun Errors "
        ::= { hypIpxTableEntry 10 }

        hypIpxColl  OBJECT-TYPE
        SYNTAX Counter
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "The number of IPX packet Collisions "
        ::= { hypIpxTableEntry 11 }

        hypIpxRipSup  OBJECT-TYPE
        SYNTAX Counter
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "The number of IPX packet RIP Supplier "
        ::= { hypIpxTableEntry 12 }

        hypIpxRipReqRcv OBJECT-TYPE
        SYNTAX Counter
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "The number of IPX packet RIP Requests Received "
        ::= { hypIpxTableEntry 13 }

        hypIpxRipReqSent OBJECT-TYPE
        SYNTAX Counter
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "The number of IPX packet RIP Requests Sent "
        ::= { hypIpxTableEntry 14 }

        hypIpxRipRspRcved  OBJECT-TYPE
        SYNTAX Counter
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "The number of IPX packet Responses Received "
        ::= { hypIpxTableEntry 15 }

        hypIpxRipRspSent  OBJECT-TYPE
        SYNTAX Counter
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "The number of IPX packet Responses Sent "
        ::= { hypIpxTableEntry 16 }

        hypIpxSapSup  OBJECT-TYPE
        SYNTAX Counter
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "The number of IPX packet SAP Supplier"
        ::= { hypIpxTableEntry 17 }

        hypIpxSapReqRcved OBJECT-TYPE
        SYNTAX Counter
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "The number of IPX packet SAP Requests Received"
        ::= { hypIpxTableEntry 18 }

        hypIpxSapReqSent  OBJECT-TYPE
        SYNTAX Counter
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "The number of IPX packet SAP Requests Sent"
        ::= { hypIpxTableEntry 19 }

        hypIpxSapRspRcved OBJECT-TYPE
        SYNTAX Counter
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "The number of IPX packet SAP Responses Received"
        ::= { hypIpxTableEntry 20 }

        hypIpxSapRspSent OBJECT-TYPE
        SYNTAX Counter
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "The number of IPX packet SAP Responses Sent"
        ::= { hypIpxTableEntry 21 }



    --End of Table hypIpTable  IPX Table



    -- The icmp Group
    -- The icmp Group contains information about the
    -- icmp Parameters in a Hypercom IEN.

    hypMsgIn  OBJECT-TYPE
        SYNTAX Counter
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "Messages received"
        ::= { hypIcmp 1 }

    hypMsgOut  OBJECT-TYPE
        SYNTAX Counter
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "Messages sent"
        ::= { hypIcmp 2 }

    hypUnreachIn  OBJECT-TYPE
        SYNTAX Counter
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "Destination unreachable received"
        ::= { hypIcmp 3 }

    hypUnreachOut  OBJECT-TYPE
        SYNTAX Counter
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "Destination unreachable sent"
        ::= { hypIcmp 4 }

    hypPingIn  OBJECT-TYPE
        SYNTAX Counter
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "Pings received"
        ::= { hypIcmp 5 }

    hypPingOut  OBJECT-TYPE
        SYNTAX Counter
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "Pings sent"
        ::= { hypIcmp 6 }






    -- The tcp Group
    -- The tcp Group contains information about the
    -- tcp Parameters in a Hypercom IEN.

    hypCntConnections  OBJECT-TYPE
        SYNTAX Counter
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "number of active TCP connections"
        ::= { hypTcp 1 }



    --Table of hypTcpConnections
    --hypTcpConnections description (Table format)
    hypTcpConnectionsTable OBJECT-TYPE
        SYNTAX   SEQUENCE OF HypTcpConnectionsEntry
        ACCESS   not-accessible
        STATUS   mandatory
        DESCRIPTION
        "A table containing information about
         "
        ::= { hypTcp 2 }

        hypTcpConnectionsEntry OBJECT-TYPE
        SYNTAX   HypTcpConnectionsEntry
        ACCESS   not-accessible
        STATUS   mandatory
        DESCRIPTION
        ""
        INDEX { hypTcpConnectionsIndex }
        ::= { hypTcpConnectionsTable 1 }

        HypTcpConnectionsEntry ::=
        SEQUENCE {
            hypTcpConnectionsIndex
                INTEGER,
            hypFramesIn
                Counter,
            hypFramesOut
                Counter,
            hypTcpPort
                INTEGER,
            hypTcpStatus
                INTEGER,
            hypIpAddr
                NetworkAddress
            }

        hypTcpConnectionsIndex  OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "The ifIndex Value of the corresponding ifEntry."
        ::= { hypTcpConnectionsEntry 1 }

        hypFramesIn  OBJECT-TYPE
        SYNTAX Counter
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "Frames received"
        ::= { hypTcpConnectionsEntry 2 }

        hypFramesOut  OBJECT-TYPE
        SYNTAX Counter
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "Frames sent"
        ::= { hypTcpConnectionsEntry 3 }

        hypTcpPort  OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "TCP port number"
        ::= { hypTcpConnectionsEntry 4 }

        hypTcpStatus  OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "Connection status: Idle(0), ARPsent(1), SYNsent(2), Active(3)"
        ::= { hypTcpConnectionsEntry 5 }

        hypIpAddr  OBJECT-TYPE
        SYNTAX NetworkAddress
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION
        "Remote IP address"
        ::= { hypTcpConnectionsEntry 6 }




    --End of Table hypTcpConnections






    -- The udp Group
    -- The udp Group contains information about the
    -- udp Parameters in a Hypercom IEN.

    hypDataIn  OBJECT-TYPE
        SYNTAX Counter
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "Datagrams in"
        ::= { hypUdp 1 }

    hypDataOut  OBJECT-TYPE
        SYNTAX Counter
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "Datagrams out"
        ::= { hypUdp 2 }

    hypBadFrames  OBJECT-TYPE
        SYNTAX Counter
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "Bad frames in"
        ::= { hypUdp 3 }

    hypUnknownPort  OBJECT-TYPE
        SYNTAX Counter
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "Unknown port received"
        ::= { hypUdp 4 }





    -- The hdlc Group
    -- The hdlc Group contains information about the
    -- hdlc line driver port including the extended hdlc
    -- and hdlc lan driver port
    -- parameters in a Hypercom IEN.



    hypHdlcPortOperMode  OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "Port operating mode"
        ::= { hypHdlc 1 }

    -- 00 = hdlc leased line multipoint
    -- 01 = hdlc dial=up mode
    -- 02 = hdlc/visa combined interface
    -- 03 = visa only dial-up mode
    -- 0B = token ring interface


    -- Port Availability Statistics
    hypHdlcBuffMax  OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "Maximum number of buffers allocated"
        ::= { hypHdlc 2 }

    hypHdlcBuffThreshold  OBJECT-TYPE
        SYNTAX Counter
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "Number of times the buffer threshold was reached"
        ::= { hypHdlc 3 }

    hypHdlcBuffAllocFail  OBJECT-TYPE
        SYNTAX Counter
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "Number of buffer allocation failures"
        ::= { hypHdlc 4 }

    hypHdlcMemoryMode  OBJECT-TYPE
        SYNTAX Counter
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "Memory mode, 'E' = EPROM,   'R' = RAM mode"
        ::= { hypHdlc 5 }

    hypHdlcAlarmMode  OBJECT-TYPE
        SYNTAX Counter
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "Alarm mode, 'N' = No Minor TRAP's, 'Y' = All Alarms"
        ::= { hypHdlc 6 }


    -- HDLC Call Processing Statistics
    hypHdlcSeizures  OBJECT-TYPE
        SYNTAX Counter
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "Incoming call seizures."
        ::= { hypHdlc 7 }

    hypHdlcLostSeizures  OBJECT-TYPE
        SYNTAX Counter
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "aborts due to lost seizure."
        ::= { hypHdlc 8 }

    hypHdlcLostCarrier  OBJECT-TYPE
        SYNTAX Counter
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "aborts due to lost carrier."
        ::= { hypHdlc 9 }

    hypHdlcNoTrain  OBJECT-TYPE
        SYNTAX Counter
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "aborts due to can't train modem."
        ::= { hypHdlc 10 }

    hypHdlcCompleted  OBJECT-TYPE
        SYNTAX Counter
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "completed calls."
        ::= { hypHdlc 11 }

    hypHdlcVisa300  OBJECT-TYPE
        SYNTAX Counter
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "Completed VISA 300 BPS calls."
        ::= { hypHdlc 12 }

    hypHdlcVisa1200  OBJECT-TYPE
        SYNTAX Counter
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "Completed VISA 1200 BPS calls."
        ::= { hypHdlc 13 }

    hypHdlcVisa2400  OBJECT-TYPE
        SYNTAX Counter
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "Completed VISA 2400 BPS calls."
        ::= { hypHdlc 14 }

    hypHdlcSdlc1200  OBJECT-TYPE
        SYNTAX Counter
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "Completed SDLC 1200 BPS calls."
        ::= { hypHdlc 15 }

    hypHdlcSdlc2400  OBJECT-TYPE
        SYNTAX Counter
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "Completed SDLC 2400 BPS calls."
