        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "number of Remote LAN segment/bridge numbers (max 32)"
        ::= { hypRemoteLanSegList 2 }



    --Table of hypRemoteLanSeg
    --hypRemoteLanSeg description (Table format)
    hypRemoteLanSegTable OBJECT-TYPE
        SYNTAX   SEQUENCE OF HypRemoteLanSegEntry
        ACCESS   not-accessible
        STATUS   mandatory
        DESCRIPTION
        "A table containing information about
         Remote LAN segments"
        ::= { hypRemoteLanSegList 3 }

        hypRemoteLanSegEntry OBJECT-TYPE
        SYNTAX   HypRemoteLanSegEntry
        ACCESS   not-accessible
        STATUS   mandatory
        DESCRIPTION
        "Remote LAN segments"
        INDEX { hypRemoteLanSegIndex }
        ::= { hypRemoteLanSegTable 1 }

        HypRemoteLanSegEntry ::=
        SEQUENCE {
            hypRemoteLanSegIndex
                INTEGER,
            hypRemoteLanSegBrg
                MacAddress
            }

        hypRemoteLanSegIndex  OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "The ifIndex Value of the corresponding ifEntry."
        ::= { hypRemoteLanSegEntry 1 }

        hypRemoteLanSegBrg  OBJECT-TYPE
        SYNTAX MacAddress
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "Remote LAN segment/bridge number"
        ::= { hypRemoteLanSegEntry 2 }




    --End of Table hypRemoteLanSeg  Remote LAN segments




