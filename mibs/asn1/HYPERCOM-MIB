    --Table of hypSnaLUStatus
    --hypSnaLUStatus description (Table format)
    hypSnaLUStatusTable OBJECT-TYPE
        SYNTAX   SEQUENCE OF HypSnaLUStatusEntry
        ACCESS   not-accessible
        STATUS   mandatory
        DESCRIPTION
        "A table containing information about
         port status fields"
        ::= { hypSna 40 }

        hypSnaLUStatusEntry OBJECT-TYPE
        SYNTAX   HypSnaLUStatusEntry
        ACCESS   not-accessible
        STATUS   mandatory
        DESCRIPTION
        "port status fields"
        INDEX { hypSnaLUStatusIndex }
        ::= { hypSnaLUStatusTable 1 }

        HypSnaLUStatusEntry ::=
        SEQUENCE {
            hypSnaLUStatusIndex
                INTEGER,
            hypSnaLUStatus
                INTEGER
            }

        hypSnaLUStatusIndex  OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "The ifIndex Value of the corresponding ifEntry."
        ::= { hypSnaLUStatusEntry 1 }

        hypSnaLUStatus  OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "SNA Logical Unit status"
        ::= { hypSnaLUStatusEntry 2 }




    --End of Table hypSnaLUStatus  port status fields
    -- snaLUStatus Status value:
    --    X0 - LU  acquired [in session]
    --    X1 - LU  released [no session]
    --    X2 - LU  inactive [no ACTLU]
    --    0X - LU  remote connected
    --    1X - LU  remote not connected
    --    2X - LU  remote not defined



    -- end of sna Group



    -- The FRAME RELAY Group
    -- from  RFC 1315 adopted for Hypercom Private MIB
    -- Frame Relay Interface


    -- Frame Relay Data Link Connection


    --Table of hypFrDLConn
    --hypFrDLConn description (Table format)
    hypFrDLConnTable OBJECT-TYPE
        SYNTAX   SEQUENCE OF HypFrDLConnEntry
        ACCESS   not-accessible
        STATUS   mandatory
        DESCRIPTION
        "A table containing information about
         Frame Relay Data Link Connection"
        ::= { hypFRelayMib 1 }

        hypFrDLConnEntry OBJECT-TYPE
        SYNTAX   HypFrDLConnEntry
        ACCESS   not-accessible
        STATUS   mandatory
        DESCRIPTION
        "Frame Relay Data Link Connection"
        INDEX { hypFrDLConnIndex }
        ::= { hypFrDLConnTable 1 }

        HypFrDLConnEntry ::=
        SEQUENCE {
            hypFrDLCIfIndex
                INTEGER,
            hypFrT1PollInterval
                INTEGER,
            hypFrT2PollVerification
                INTEGER,
            hypFrT3MsgFrequenctTimer
                INTEGER,
            hypFrN1PollStatusCnt
                Counter,
            hypFrN2ErrorThreshold
                Counter,
            hypFrN3MonitorEventCnt
                Counter,
            hypFrN4MsgFrequencyCnt
                Counter,
            hypFrMgtType
                INTEGER,
            hypFrAddrFmt
                INTEGER,
            hypFrFramesReceived
                Counter,
            hypFrFramesSent
                Counter,
            hypFrCntFECN
                Counter,
            hypFrCntBECN
                Counter,
            hypFrLmiSent
                INTEGER,
            hypFrLmiRecv
                INTEGER,
            hypFrPhyIf
                INTEGER
            }

        hypFrDLCIfIndex  OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "The ifIndex  (FR Card Instance)"
        ::= { hypFrDLConnEntry 1 }

        hypFrT1PollInterval  OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "T1 poll interval in seconds."
        ::= { hypFrDLConnEntry 2 }

        hypFrT2PollVerification  OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "T2 poll verification in seconds."
        ::= { hypFrDLConnEntry 3 }

        hypFrT3MsgFrequenctTimer  OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "T3 message frequency timer in seconds."
        ::= { hypFrDLConnEntry 4 }

        hypFrN1PollStatusCnt  OBJECT-TYPE
        SYNTAX Counter
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "N1 poll status count."
        ::= { hypFrDLConnEntry 5 }

        hypFrN2ErrorThreshold  OBJECT-TYPE
        SYNTAX Counter
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "N2 error threshold."
        ::= { hypFrDLConnEntry 6 }

        hypFrN3MonitorEventCnt  OBJECT-TYPE
        SYNTAX Counter
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "N3 monitored events count."
        ::= { hypFrDLConnEntry 7 }

        hypFrN4MsgFrequencyCnt  OBJECT-TYPE
        SYNTAX Counter
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "N4 message frequency count."
        ::= { hypFrDLConnEntry 8 }

        hypFrMgtType  OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "Management type  { LMI (0), ANSI_AnnexD (1) }"
        ::= { hypFrDLConnEntry 9 }

        hypFrAddrFmt  OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "Address format (2..4-byte format)   ( 2..4 )"
        ::= { hypFrDLConnEntry 10 }

        hypFrFramesReceived  OBJECT-TYPE
        SYNTAX Counter
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "Total frame received."
        ::= { hypFrDLConnEntry 11 }

        hypFrFramesSent  OBJECT-TYPE
        SYNTAX Counter
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "Total Frame sent."
        ::= { hypFrDLConnEntry 12 }

        hypFrCntFECN  OBJECT-TYPE
        SYNTAX Counter
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "Frame count with FECN"
        ::= { hypFrDLConnEntry 13 }

        hypFrCntBECN  OBJECT-TYPE
        SYNTAX Counter
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "Frame count with BECN"
        ::= { hypFrDLConnEntry 14 }

        hypFrLmiSent  OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "Total LMI polls sent"
        ::= { hypFrDLConnEntry 15 }

        hypFrLmiRecv  OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "Total LMI responses received"
        ::= { hypFrDLConnEntry 16 }

        hypFrPhyIf  OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "Physical interface status"
        ::= { hypFrDLConnEntry 17 }

    --   NotConnected (1),
    --   Connected    (2),
    --   Loopback     (3),
    --   PortStopped  (4)  port stopped by NMS



    --End of Table hypFrDLConn  Frame Relay Data Link Connection


    -- Frame Relay Circuits


    --Table of hypFrPVCirStatus
    --hypFrPVCirStatus description (Table format)
    hypFrPVCirStatusTable OBJECT-TYPE
        SYNTAX   SEQUENCE OF HypFrPVCirStatusEntry
        ACCESS   not-accessible
        STATUS   mandatory
        DESCRIPTION
