        SYNTAX INTEGER
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "Count of X25 Logical Channel status"
        ::= { hypX25 39 }



    --Table of hypX25LogChanStatus
    --hypX25LogChanStatus description (Table format)
    hypX25LogChanStatusTable OBJECT-TYPE
        SYNTAX   SEQUENCE OF HypX25LogChanStatusEntry
        ACCESS   not-accessible
        STATUS   mandatory
        DESCRIPTION
        "A table containing information about
         x25 Logical Channel status fields"
        ::= { hypX25 40 }

        hypX25LogChanStatusEntry OBJECT-TYPE
        SYNTAX   HypX25LogChanStatusEntry
        ACCESS   not-accessible
        STATUS   mandatory
        DESCRIPTION
        "x25 Logical Channel status fields"
        INDEX { hypX25LogChanStatusIndex }
        ::= { hypX25LogChanStatusTable 1 }

        HypX25LogChanStatusEntry ::=
        SEQUENCE {
            hypX25LogChanStatusIndex
                INTEGER,
            hypX25LogChanStatus
                INTEGER
            }

        hypX25LogChanStatusIndex  OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "The ifIndex Value of the corresponding ifEntry."
        ::= { hypX25LogChanStatusEntry 1 }

        hypX25LogChanStatus  OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "x25 Logical Channel status"
        ::= { hypX25LogChanStatusEntry 2 }

    -- x25LogChanStatus Status value:
    --    00 - LCN connected
    --    01 - LCN not conected
    --    02 - LCN not defined



    --End of Table hypX25LogChanStatus  x25 Logical Channel status fields





    -- The x28 Group
    -- The x28 Group contains information about the x28
    -- functionality in a Hypercom IEN.

    hypX28PortStat  OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "X28 Port Status 0=Com, 01=Cleared, xx=other"
        ::= { hypX28 1 }

    hypX28LastClearDn  OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "X28 Last Clear Down Code (Hex 00-FF)"
        ::= { hypX28 2 }

    --  X28 Last Clear Down Codes are as follows:
    --    0XX = X.25 NETWORK ORIGINATED CLEAR CODES
    --    000 = REMOTE DTE INITIATED CLEAR DOWN
    --    2XX = IEN Node INTERNAL ERROR
    --    222 = PORT NEVER ACTIVE
    --    232 = IEN Node LINE STOPPED BY NMS
    --    237 = TERMINAL DISCONNECTED (BY CARRIER DROP)
    --    242 = TERMINAL ATTEMPTED TO CONNECT WHILE
    --          X.25 RESTART IN PROGRESS






    hypX28CntConnAttem  OBJECT-TYPE
        SYNTAX Counter
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "X28 Total number of connection attempts"
        ::= { hypX28 3 }

    hypX28CntConn  OBJECT-TYPE
        SYNTAX Counter
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "X28 Total number of connections"
        ::= { hypX28 4 }

    hypX28CntCommAttem  OBJECT-TYPE
        SYNTAX Counter
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "X28 Total number of connection attempts"
        ::= { hypX28 5 }

    hypX28CntFromLine  OBJECT-TYPE
        SYNTAX Counter
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "X28 Total number messages from line"
        ::= { hypX28 6 }

    hypX28CntToLine  OBJECT-TYPE
        SYNTAX Counter
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "X28 Total number messages to line"
        ::= { hypX28 7 }



    -- Port Availability Statistics
    hypX28BuffMax  OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "Maximum number of Buffers allocated"
        ::= { hypX28 8 }

