                MacAddress,
            hypLanBrStatus
                INTEGER,
            hypFeature
                INTEGER
            }

        hypLanStationIndex  OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "The ifIndex Value of the corresponding ifEntry."
        ::= { hypLanStationEntry 1 }

        hypHwAddr  OBJECT-TYPE
        SYNTAX MacAddress
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "hardware address"
        ::= { hypLanStationEntry 2 }

        hypLanBrStatus  OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "status (0 = good, non-zero=error)"
        ::= { hypLanStationEntry 3 }

        hypFeature  OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "station feature"
        ::= { hypLanStationEntry 4 }




    --End of Table hypLanStation  LAN stations
    --       Station Feature
    --               0x011B = bridge/REM/RPS
    --               0x0100 = bridge/router
    --               0x0000 = LAN station



    -- The ipRemoteSubnets Group
    -- The ipRemoteSubnets Group contains information about the
    -- ip Remote Subnets in a Hypercom IEN.

    hypCntRemoteSubnets  OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "number of IP Remote Subnets (max 16)"
        ::= { hypIpRemoteSubnets 1 }



    --Table of hypRemoteSubnets
    --hypRemoteSubnets description (Table format)
    hypRemoteSubnetsTable OBJECT-TYPE
        SYNTAX   SEQUENCE OF HypRemoteSubnetsEntry
        ACCESS   not-accessible
        STATUS   mandatory
        DESCRIPTION
        "A table containing information about
         IP Remote Subnets"
        ::= { hypIpRemoteSubnets 2 }

        hypRemoteSubnetsEntry OBJECT-TYPE
        SYNTAX   HypRemoteSubnetsEntry
        ACCESS   not-accessible
        STATUS   mandatory
        DESCRIPTION
        "IP Remote Subnets"
        INDEX { hypRemoteSubnetsIndex }
        ::= { hypRemoteSubnetsTable 1 }

        HypRemoteSubnetsEntry ::=
        SEQUENCE {
            hypRemoteSubnetsIndex
                INTEGER,
            hypIpSubnetAddress
                MacAddress
            }

        hypRemoteSubnetsIndex  OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "The ifIndex Value of the corresponding ifEntry."
        ::= { hypRemoteSubnetsEntry 1 }

        hypIpSubnetAddress  OBJECT-TYPE
        SYNTAX MacAddress
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "IP Subnet Address"
        ::= { hypRemoteSubnetsEntry 2 }




    --End of Table hypRemoteSubnets  IP Remote Subnets
    hypRemSubCntLanStations  OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "number of LAN Stations (max 16)"
        ::= { hypIpRemoteSubnets 3 }



    --Table of hypLanStationsTable
    --hypLanStationsTable description (Table format)
    hypLanStationsTableTable OBJECT-TYPE
        SYNTAX   SEQUENCE OF HypLanStationsTableEntry
        ACCESS   not-accessible
        STATUS   mandatory
        DESCRIPTION
        "A table containing information about
         Lan stations"
        ::= { hypIpRemoteSubnets 4 }

        hypLanStationsTableEntry OBJECT-TYPE
        SYNTAX   HypLanStationsTableEntry
        ACCESS   not-accessible
        STATUS   mandatory
        DESCRIPTION
        "Lan stations"
        INDEX { hypLanStationsTableIndex }
        ::= { hypLanStationsTableTable 1 }

        HypLanStationsTableEntry ::=
        SEQUENCE {
            hypLanStationsIndex
                INTEGER,
            hypRemSubIpAddress
                NetworkAddress,
            hypRemSumMacAddress
                MacAddress,
            hypIp
                INTEGER,
            hypIpx
                INTEGER,
            hypSR
                INTEGER,
            hypRouter
                INTEGER,
            hypIpRemSubStatus
                INTEGER
            }

        hypLanStationsIndex  OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "The ifIndex Value of the corresponding ifEntry."
        ::= { hypLanStationsTableEntry 1 }

        hypRemSubIpAddress  OBJECT-TYPE
        SYNTAX NetworkAddress
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "IP Address"
        ::= { hypLanStationsTableEntry 2 }

        hypRemSumMacAddress  OBJECT-TYPE
        SYNTAX MacAddress
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "MAC Address"
        ::= { hypLanStationsTableEntry 3 }

        hypIp  OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "Status of ip,     0 =not enabled"
        ::= { hypLanStationsTableEntry 4 }

        hypIpx  OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "Status of ipx,    0 =not enabled"
        ::= { hypLanStationsTableEntry 5 }

        hypSR  OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "Status of SR,     0 =not enabled"
        ::= { hypLanStationsTableEntry 6 }

        hypRouter  OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "Status of router, 0 =not enabled"
        ::= { hypLanStationsTableEntry 7 }

        hypIpRemSubStatus  OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "Status, 0=active, non-zero=error"
        ::= { hypLanStationsTableEntry 8 }




    --End of Table hypLanStationsTable  Lan stations




    -- The tokenRingIf Group
