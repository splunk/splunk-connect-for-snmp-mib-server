        DEFVAL	{ 100 }
        ::= { wfBrSr 17 }

    wfBrSrBaseNbCurrentRifEntries OBJECT-TYPE
        SYNTAX	INTEGER
        ACCESS	read-only
        STATUS	mandatory
        DESCRIPTION
