    wfBrSrInterfaceEncapsFormat OBJECT-TYPE
        SYNTAX	INTEGER {
    		    proprietary(1),
    		    standard(2)
    		}
        ACCESS	read-write
        STATUS	mandatory
        DESCRIPTION
                "Non-Token Ring encapsulation format"
        DEFVAL	{ proprietary }
        ::= { wfBrSrInterfaceEntry 24 }

    wfBrSrInterfaceStpEnable OBJECT-TYPE
        SYNTAX	INTEGER {
    		    enabled(1),
    		    disabled(2)
    		}
        ACCESS	read-write
        STATUS	mandatory
        DESCRIPTION
                "Enable/Disable parameter. Default is enabled.
