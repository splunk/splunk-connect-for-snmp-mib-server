	setparmTable OBJECT-TYPE
	    SYNTAX SEQUENCE OF SetparmEntry
	    ACCESS not-accessible
	    STATUS mandatory
	    DESCRIPTION
		"SET Parameter Entry Table"
	::= { setparminfo 1 }

	setparmEntry OBJECT-TYPE
	    SYNTAX SetparmEntry
	    ACCESS not-accessible
	    STATUS mandatory
	    DESCRIPTION
		"SET Parameter Entry"
	    INDEX  { setparmIndex }
	::= { setparmTable 1 }

	SetparmEntry ::=
	    SEQUENCE {
			setparmIndex
				INTEGER,
			setparmName
				DisplayString,
			setparmDesc
				DisplayString,
			setparmCategory
				DisplayString,
			setparmFlags
				DisplayString,
			setparmCurrent
				DisplayString,
			setparmMinimum
				DisplayString,
			setparmMaximum
				DisplayString
		    }

	setparmIndex OBJECT-TYPE
	    SYNTAX INTEGER
	    ACCESS read-only
	    STATUS mandatory
	    DESCRIPTION
		"SET Parameter Index Pointer"
	::= { setparmEntry 1 }

	setparmName OBJECT-TYPE
	    SYNTAX DisplayString (SIZE (0..80))
	    ACCESS read-only
	    STATUS mandatory
	    DESCRIPTION
		"SET Parameter Name"
	::= { setparmEntry 2 }

	setparmDesc OBJECT-TYPE
	    SYNTAX DisplayString (SIZE (0..576))
	    ACCESS read-only
	    STATUS mandatory
	    DESCRIPTION
		"SET Parameter Description"
	::= { setparmEntry 3 }

	setparmCategory OBJECT-TYPE
	    SYNTAX DisplayString (SIZE (0..24))
	    ACCESS read-only
	    STATUS mandatory
	    DESCRIPTION
		"SET Parameter Category"
	::= { setparmEntry 4 }

	setparmFlags OBJECT-TYPE
	    SYNTAX DisplayString (SIZE (0..80))
	    ACCESS read-only
	    STATUS mandatory
	    DESCRIPTION
		"SET Parameter Flags"
	::= { setparmEntry 5 }

	setparmCurrent OBJECT-TYPE
	    SYNTAX DisplayString (SIZE (0..80))
	    ACCESS read-only
	    STATUS mandatory
	    DESCRIPTION
		"SET Parameter Current Value"
	::= { setparmEntry 6 }

	setparmMinimum OBJECT-TYPE
	    SYNTAX DisplayString (SIZE (0..80))
	    ACCESS read-only
	    STATUS mandatory
	    DESCRIPTION
		"SET Parameter Minimum Value"
	::= { setparmEntry 7 }

	setparmMaximum OBJECT-TYPE
	    SYNTAX DisplayString (SIZE (0..80))
	    ACCESS read-only
	    STATUS mandatory
	    DESCRIPTION
		"SET Parameter Maximum Value"
	::= { setparmEntry 8 }



