	    ACCESS not-accessible
	    STATUS mandatory
	    DESCRIPTION
		"Configured Network Entry"
	    INDEX  { netIndex }
	::= { netTable 1 }

	NetEntry ::=
	    SEQUENCE {
			netIndex
				INTEGER,
			netDriverName
				DisplayString,
			netDriverDesc
				DisplayString,
			netFrameDesc
				DisplayString,
			netLogicalName
				DisplayString,
			netProtocolDesc
				DisplayString,
			netNodeID
				OCTET STRING,
			netLineSpeed
				INTEGER,
			netMaxPacketSize
				INTEGER,
			netMaxRecvSize
				INTEGER,
			netMaxProtocolSize
				INTEGER,
			netPrimaryIOPort
				DisplayString,
			netPrimaryMemoryDecode
				DisplayString,
			netPrimaryInterrupt
				DisplayString,
			netPrimaryDMA
				DisplayString,
			netSecondaryIOPort
				DisplayString,
			netSecondaryMemoryDecode
				DisplayString,
			netSecondaryInterrupt
				DisplayString,
			netSecondaryDMA
				DisplayString,
			netTotalRxPacketCount
				INTEGER,
			netPacketRxOverflowCount
				INTEGER,
			netPacketRxTooBigCount
				INTEGER,
			netPacketRxTooSmallCount
				INTEGER,
			netPacketRxMiscErrorCount
				INTEGER,
			netChecksumErrorCount
				INTEGER,
			netHardwareRxMismatchCount
				INTEGER,
			netTotalTxPacketCount
				INTEGER,
			netNoECBAvailableCount
				INTEGER,
			netPacketTxTooBigCount
				INTEGER,
			netPacketTxTooSmallCount
				INTEGER,
			netPacketTxMiscErrorCount
				INTEGER,
			netRetryTxCount
				INTEGER
		    }

