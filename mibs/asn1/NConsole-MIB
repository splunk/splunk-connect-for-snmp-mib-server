NConsole-MIB DEFINITIONS ::= BEGIN
 
IMPORTS
		DisplayString FROM SNMPv2-TC
	TimeTicks, OBJECT-TYPE, enterprises FROM RFC1155-SMI;

avanti			OBJECT IDENTIFIER ::= { enterprises 1117 }
nconsolemib		OBJECT IDENTIFIER ::= { avanti 1 }
serverinfo		OBJECT IDENTIFIER ::= { nconsolemib 1 }
configinfo		OBJECT IDENTIFIER ::= { nconsolemib 2 }
memoryinfo		OBJECT IDENTIFIER ::= { nconsolemib 3 }
moduleinfo		OBJECT IDENTIFIER ::= { nconsolemib 4 }
netinfo			OBJECT IDENTIFIER ::= { nconsolemib 5 }
nicstats		OBJECT IDENTIFIER ::= { nconsolemib 6 }
setparminfo		OBJECT IDENTIFIER ::= { nconsolemib 7 }
serverstats		OBJECT IDENTIFIER ::= { nconsolemib 8 }
servertrends		OBJECT IDENTIFIER ::= { nconsolemib 9 }
volinfo			OBJECT IDENTIFIER ::= { nconsolemib 10 }
voltrends		OBJECT IDENTIFIER ::= { nconsolemib 11 }
driveinfo		OBJECT IDENTIFIER ::= { nconsolemib 12 }
drivetrends		OBJECT IDENTIFIER ::= { nconsolemib 13 }
nictrends		OBJECT IDENTIFIER ::= { nconsolemib 14 }
protocolinfo		OBJECT IDENTIFIER ::= { nconsolemib 15 }
protocoltrends		OBJECT IDENTIFIER ::= { nconsolemib 16 }
sftserverinfo		OBJECT IDENTIFIER ::= { nconsolemib 17 }
sftmemoryinfo		OBJECT IDENTIFIER ::= { nconsolemib 18 }
sftmoduleinfo		OBJECT IDENTIFIER ::= { nconsolemib 19 }
sftnetinfo		OBJECT IDENTIFIER ::= { nconsolemib 20 }
sftsetparminfo		OBJECT IDENTIFIER ::= { nconsolemib 21 }
sftservertrends		OBJECT IDENTIFIER ::= { nconsolemib 22 }
smpinfo			OBJECT IDENTIFIER ::= { nconsolemib 23 }
smptrends		OBJECT IDENTIFIER ::= { nconsolemib 24 }



