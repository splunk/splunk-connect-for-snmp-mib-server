	nicCount OBJECT-TYPE
	    SYNTAX INTEGER
	    ACCESS read-only
	    STATUS mandatory
	    DESCRIPTION
		"Configured NICs (Network Interface Cards)"
	::= { nicstats 1 }

	nicTable OBJECT-TYPE
	    SYNTAX SEQUENCE OF NicEntry
	    ACCESS not-accessible
	    STATUS mandatory
	    DESCRIPTION
		"Configured NIC Entry Table"
	::= { nicstats 2 }

	nicEntry OBJECT-TYPE
	    SYNTAX NicEntry
	    ACCESS not-accessible
	    STATUS mandatory
	    DESCRIPTION
		"Configured NIC Entry"
	    INDEX  { nicIndex }
	::= { nicTable 1 }

	NicEntry ::=
	    SEQUENCE {
			nicIndex
				INTEGER,
			nicNodeID
				OCTET STRING,
			nicCurrRxPacketCount
				INTEGER,
			nicAvgRxPacketCount
				INTEGER,
			nicPeakRxPacketCount
				INTEGER,
			nicCumlRxPacketCount
				INTEGER,
			nicCurrTxPacketCount
