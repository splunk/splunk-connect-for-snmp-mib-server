	moduleCount OBJECT-TYPE
	    SYNTAX INTEGER
	    ACCESS read-only
	    STATUS mandatory
	    DESCRIPTION
		"Loaded Modules"
	::= { moduleinfo 1 }

	moduleTable OBJECT-TYPE
	    SYNTAX SEQUENCE OF ModuleEntry
	    ACCESS not-accessible
	    STATUS mandatory
	    DESCRIPTION
		"Loaded Module Entry Table"
	::= { moduleinfo 2 }

	moduleEntry OBJECT-TYPE
	    SYNTAX ModuleEntry
	    ACCESS not-accessible
	    STATUS mandatory
	    DESCRIPTION
		"Loaded Module Entry"
	    INDEX  { moduleIndex }
	::= { moduleTable 1 }

	ModuleEntry ::=
	    SEQUENCE {
			moduleIndex
				INTEGER,
			moduleName
				DisplayString,
			moduleDesc
				DisplayString,
			moduleCopyright
				DisplayString,
			moduleVersion
				DisplayString,
			moduleCompileDate
				DisplayString,
			moduleCodeSize
				INTEGER,
			moduleDataSize
				INTEGER,
			moduleSmallMemoryAlloc
				INTEGER,
			moduleMediumMemoryAlloc
				INTEGER,
			moduleLargeMemoryAlloc
				INTEGER,
			moduleActiveProcesses
				INTEGER,
			moduleActiveScreens
				INTEGER
		    }

	moduleIndex OBJECT-TYPE
	    SYNTAX INTEGER
	    ACCESS read-only
	    STATUS mandatory
	    DESCRIPTION
		"Loaded Module Index Pointer"
	::= { moduleEntry 1 }

	moduleName OBJECT-TYPE
	    SYNTAX DisplayString (SIZE (0..36))
	    ACCESS read-only
	    STATUS mandatory
	    DESCRIPTION
		"Loaded Module Name"
	::= { moduleEntry 2 }

	moduleDesc OBJECT-TYPE
	    SYNTAX DisplayString (SIZE (0..128))
	    ACCESS read-only
	    STATUS mandatory
	    DESCRIPTION
