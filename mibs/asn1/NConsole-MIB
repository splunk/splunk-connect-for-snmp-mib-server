	    STATUS mandatory
	    DESCRIPTION
		"Protocol Trend Entry Table"
	::= { protocoltrends 1 }

	protocolTrendEntry OBJECT-TYPE
	    SYNTAX ProtocolTrendEntry
	    ACCESS not-accessible
	    STATUS mandatory
	    DESCRIPTION
		"Protocol Trend Entry"
	    INDEX  { protocolTrendCycleIndex, protocolTrendProtocolIndex }
	::= { protocolTrendsTable 1 }

	ProtocolTrendEntry ::=
	    SEQUENCE {
			protocolTrendCycleIndex
				INTEGER,
			protocolTrendProtocolIndex
				INTEGER,
			protocolTrendStartTime
				DisplayString,
			protocolTrendStartUTC
				INTEGER,
			protocolTrendStopTime
				DisplayString,
			protocolTrendStopUTC
				INTEGER,
			protocolTrendDesc
				DisplayString,
			protocolTrendAvgRxPacketCount
				INTEGER,
			protocolTrendPeakRxPacketCount
				INTEGER,
			protocolTrendCumlRxPacketCount
				INTEGER,
			protocolTrendAvgTxPacketCount
				INTEGER,
			protocolTrendPeakTxPacketCount
				INTEGER,
			protocolTrendCumlTxPacketCount
				INTEGER
		    }

	protocolTrendCycleIndex OBJECT-TYPE
	    SYNTAX INTEGER
	    ACCESS read-only
	    STATUS mandatory
	    DESCRIPTION
		"Protocol Trend Cycle Index"
	::= { protocolTrendEntry 1 }

	protocolTrendProtocolIndex OBJECT-TYPE
	    SYNTAX INTEGER
	    ACCESS read-only
	    STATUS mandatory
	    DESCRIPTION
		"Protocol Trend Protocol Index"
	::= { protocolTrendEntry 2 }

	protocolTrendStartTime OBJECT-TYPE
	    SYNTAX DisplayString (SIZE (0..32))
	    ACCESS read-only
	    STATUS mandatory
	    DESCRIPTION
		"Protocol Trend Start Time"
	::= { protocolTrendEntry 3 }

	protocolTrendStartUTC OBJECT-TYPE
	    SYNTAX INTEGER
	    ACCESS read-only
	    STATUS mandatory
	    DESCRIPTION
		"Protocol Trend Start Time GMT (UTC secs)"
	::= { protocolTrendEntry 4 }

	protocolTrendStopTime OBJECT-TYPE
	    SYNTAX DisplayString (SIZE (0..32))
	    ACCESS read-only
	    STATUS mandatory
	    DESCRIPTION
		"Protocol Trend Stop Time"
	::= { protocolTrendEntry 5 }

	protocolTrendStopUTC OBJECT-TYPE
	    SYNTAX INTEGER
	    ACCESS read-only
	    STATUS mandatory
	    DESCRIPTION
		"Protocol Trend Stop Time GMT (UTC secs)"
	::= { protocolTrendEntry 6 }

	protocolTrendDesc OBJECT-TYPE
	    SYNTAX DisplayString (SIZE (0..32))
	    ACCESS read-only
	    STATUS mandatory
	    DESCRIPTION
		"Protocol Trend Description"
	::= { protocolTrendEntry 7 }

	protocolTrendAvgRxPacketCount OBJECT-TYPE
	    SYNTAX INTEGER
	    ACCESS read-only
	    STATUS mandatory
	    DESCRIPTION
		"Protocol Trend Average Rx Packets"
	::= { protocolTrendEntry 8 }

	protocolTrendPeakRxPacketCount OBJECT-TYPE
	    SYNTAX INTEGER
	    ACCESS read-only
	    STATUS mandatory
	    DESCRIPTION
		"Protocol Trend Peak Rx Packets"
	::= { protocolTrendEntry 9 }

	protocolTrendCumlRxPacketCount OBJECT-TYPE
	    SYNTAX INTEGER
	    ACCESS read-only
	    STATUS mandatory
	    DESCRIPTION
		"Protocol Trend Cumulative Rx Packets"
	::= { protocolTrendEntry 10 }

	protocolTrendAvgTxPacketCount OBJECT-TYPE
	    SYNTAX INTEGER
	    ACCESS read-only
	    STATUS mandatory
	    DESCRIPTION
		"Protocol Trend Average Tx Packets"
	::= { protocolTrendEntry 11 }

	protocolTrendPeakTxPacketCount OBJECT-TYPE
	    SYNTAX INTEGER
	    ACCESS read-only
	    STATUS mandatory
	    DESCRIPTION
		"Protocol Trend Peak Tx Packets"
	::= { protocolTrendEntry 12 }

	protocolTrendCumlTxPacketCount OBJECT-TYPE
	    SYNTAX INTEGER
	    ACCESS read-only
	    STATUS mandatory
	    DESCRIPTION
		"Protocol Trend Cumulative Tx Packets"
	::= { protocolTrendEntry 13 }



	smpCount OBJECT-TYPE
	    SYNTAX INTEGER
	    ACCESS read-only
	    STATUS mandatory
	    DESCRIPTION
		"SMP Processors"
	::= { smpinfo 1 }

	smpTable OBJECT-TYPE
	    SYNTAX SEQUENCE OF SmpEntry
	    ACCESS not-accessible
	    STATUS mandatory
	    DESCRIPTION
		"SMP Processor Entry Table"
	::= { smpinfo 2 }

	smpEntry OBJECT-TYPE
	    SYNTAX SmpEntry
	    ACCESS not-accessible
	    STATUS mandatory
	    DESCRIPTION
		"SMP Processor Entry"
	    INDEX  { smpIndex }
	::= { smpTable 1 }

	SmpEntry ::=
	    SEQUENCE {
			smpIndex
				INTEGER,
			smpCurrUtil
				INTEGER,
			smpAvgUtil
				INTEGER,
			smpPeakUtil
				INTEGER
		    }

	smpIndex OBJECT-TYPE
	    SYNTAX INTEGER
	    ACCESS read-only
	    STATUS mandatory
	    DESCRIPTION
		"SMP Processor Index Pointer"
	::= { smpEntry 1 }

	smpCurrUtil OBJECT-TYPE
	    SYNTAX INTEGER
	    ACCESS read-only
	    STATUS mandatory
	    DESCRIPTION
		"SMP Processor Current Utilization"
	::= { smpEntry 2 }

	smpAvgUtil OBJECT-TYPE
	    SYNTAX INTEGER
	    ACCESS read-only
	    STATUS mandatory
	    DESCRIPTION
		"SMP Processor Average Utilization (current cycle)"
	::= { smpEntry 3 }

	smpPeakUtil OBJECT-TYPE
	    SYNTAX INTEGER
	    ACCESS read-only
	    STATUS mandatory
	    DESCRIPTION
		"SMP Processor Peak Utilization (current cycle)"
	::= { smpEntry 4 }



	smpTrendTable OBJECT-TYPE
	    SYNTAX SEQUENCE OF SmpTrendEntry
	    ACCESS not-accessible
	    STATUS mandatory
	    DESCRIPTION
		"SMP Processor Trends Entry Table"
	::= { smptrends 1 }

	smpTrendEntry OBJECT-TYPE
	    SYNTAX SmpTrendEntry
	    ACCESS not-accessible
	    STATUS mandatory
	    DESCRIPTION
		"SMP Processor Trends Entry"
	    INDEX  { smpTrendCycleIndex, smpTrendProcessorIndex }
	::= { smpTrendTable 1 }

	SmpTrendEntry ::=
	    SEQUENCE {
			smpTrendCycleIndex
				INTEGER,
			smpTrendProcessorIndex
				INTEGER,
			smpTrendStartTime
				DisplayString,
			smpTrendStartUTC
				INTEGER,
			smpTrendStopTime
				DisplayString,
			smpTrendStopUTC
				INTEGER,
			smpTrendAvgUtil
				INTEGER,
			smpTrendPeakUtil
				INTEGER
		    }

	smpTrendCycleIndex OBJECT-TYPE
	    SYNTAX INTEGER
	    ACCESS read-only
	    STATUS mandatory
	    DESCRIPTION
		"SMP Trend Cycle Index"
	::= { smpTrendEntry 1 }

	smpTrendProcessorIndex OBJECT-TYPE
	    SYNTAX INTEGER
	    ACCESS read-only
	    STATUS mandatory
	    DESCRIPTION
		"SMP Trend Processor Index"
	::= { smpTrendEntry 2 }

	smpTrendStartTime OBJECT-TYPE
	    SYNTAX DisplayString (SIZE (0..32))
	    ACCESS read-only
	    STATUS mandatory
	    DESCRIPTION
		"SMP Trend Start Time"
	::= { smpTrendEntry 3 }

	smpTrendStartUTC OBJECT-TYPE
	    SYNTAX INTEGER
	    ACCESS read-only
	    STATUS mandatory
	    DESCRIPTION
		"SMP Trend Start Time GMT (UTC secs)"
	::= { smpTrendEntry 4 }

	smpTrendStopTime OBJECT-TYPE
	    SYNTAX DisplayString (SIZE (0..32))
	    ACCESS read-only
	    STATUS mandatory
	    DESCRIPTION
		"SMP Trend Stop Time"
	::= { smpTrendEntry 5 }

	smpTrendStopUTC OBJECT-TYPE
	    SYNTAX INTEGER
	    ACCESS read-only
	    STATUS mandatory
	    DESCRIPTION
		"SMP Trend Stop Time GMT (UTC secs)"
	::= { smpTrendEntry 6 }

	smpTrendAvgUtil OBJECT-TYPE
	    SYNTAX INTEGER
	    ACCESS read-only
	    STATUS mandatory
	    DESCRIPTION
		"SMP Trend Processor Average Utilization"
	::= { smpTrendEntry 7 }

	smpTrendPeakUtil OBJECT-TYPE
	    SYNTAX INTEGER
	    ACCESS read-only
	    STATUS mandatory
	    DESCRIPTION
		"SMP Trend Processor Peak Utilization"
	::= { smpTrendEntry 8 }

END
