-- AAA server table definition.  A Direct-Request host name entry refers
-- to one of the AAA servers in this table.
usrUserManAAAServersTable OBJECT-TYPE
    SYNTAX SEQUENCE OF UumAAAServersEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "A table of AAA Servers configured. Several default servers
         are automatically entered. Adding a new entry requires the
         following objects to be defined:-
         uumAAAServersName
         uumAAAServersPref
         uumAAAServersAAAType"
::= { usrUserManGroup 52 }
    uumAAAServersEntry    OBJECT-TYPE
        SYNTAX      UumAAAServersEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            ""
        INDEX   { uumAAAServersName, uumAAAServersPref}
    ::= { usrUserManAAAServersTable 1 }
    UumAAAServersEntry ::=
        SEQUENCE {
        uumAAAServersName           DisplayString,
        uumAAAServersPref           Integer32,
        uumAAAServerIpAddress       IpAddress,
        uumAAAServersSecret         OCTET STRING,
        uumAAAServersPort           Integer32,
        uumAAAServersEncryption     INTEGER,
        uumAAAServersState          INTEGER,
        uumAAAServersHostName       UsrDisplayString,
        uumAAAServersPassThru       INTEGER, 
        uumAAAServersSourcePort     Integer32,
        uumAAAServersStatus         INTEGER,
        uumAAAServersMirrorGroup    Integer32,
        uumAAAServersAAAType        INTEGER,
        uumAAAServersProtocol       INTEGER,
        uumAAAServersTimeout        Integer32,
        uumAAAServersRetransmit     Integer32,
        uumAAAServersSuccessCount   Counter32,
        uumAAAServersFailureCount   Counter32,
        uumAAAServersNoResponseCount Counter32,
        uumAAAServersContactInterval Integer32,
        uumAAAServersOperationalStatus INTEGER,
        uumAAAServersEnableNasRebootIndication INTEGER,
        uumAAAServersRebootAckReceived INTEGER,
		uumAAAServersMask  			INTEGER,
        uumAAAServersSelectionMask  UsrAAADisplay
    }
uumAAAServersName  OBJECT-TYPE
    SYNTAX      DisplayString
    MAX-ACCESS  not-accessible      
    STATUS      current
    DESCRIPTION
        "Server name; max number of characters is 32."
    --CONFIGURABLE
::= { uumAAAServersEntry 1 }
uumAAAServersPref  OBJECT-TYPE
    SYNTAX      Integer32 (1..109)
    MAX-ACCESS  not-accessible      
    STATUS      current
    DESCRIPTION
        "Server preference. This object can be SET to value
         1-100, 101-109 are reserved."
    --CONFIGURABLE
::= { uumAAAServersEntry 2 }
uumAAAServerIpAddress    OBJECT-TYPE
    SYNTAX      IpAddress
    MAX-ACCESS  read-write
