-- User managers NAI construction table definition
usrUserManNAIConfigTable    OBJECT-TYPE
    SYNTAX        SEQUENCE OF UumNAIConfigEntry
    MAX-ACCESS    not-accessible
    STATUS        current
    DESCRIPTION
        "A table for configuring NAI construction in case there is no authentication."
::= { usrUserManGroup 232 }
    uumNAIConfigEntry    OBJECT-TYPE
        SYNTAX        UumNAIConfigEntry
        MAX-ACCESS    not-accessible
        STATUS        current
        DESCRIPTION
            "An entry in the NAI construction table."
        INDEX    { uumNAIIndex }
    ::= { usrUserManNAIConfigTable 1 }
    UumNAIConfigEntry ::= 
        SEQUENCE {
        uumNAIIndex             Integer32,
        uumNAIMSIDType          INTEGER,
        uumNAIMSIDRangeMin      Integer32,
        uumNAIMSIDRangeMax      Integer32,
        uumNAIRealm             DisplayString,
        uumNAIStatus            RowStatus,
        uumNAIMSIDStrRangeMin   OCTET STRING,
        uumNAIMSIDStrRangeMax   OCTET STRING,
        uumNAIAuthType          INTEGER
    }
uumNAIIndex    OBJECT-TYPE
    SYNTAX        Integer32 (1..100)
    MAX-ACCESS    read-only
    STATUS        current
    DESCRIPTION
        "The index of the NAI construction table, it can support up 100 entries."
::= {uumNAIConfigEntry 1 }
uumNAIMSIDType    OBJECT-TYPE
    SYNTAX        INTEGER {
                          imsi(1),
                          min(2),
                          irm(3)
                          }
    MAX-ACCESS    read-write
    STATUS        current
    DESCRIPTION
        "The type of the MSID"
    --CONFIGURABLE
::= {uumNAIConfigEntry 2 }
uumNAIMSIDRangeMin    OBJECT-TYPE
    SYNTAX        Integer32
    MAX-ACCESS    not-accessible
    STATUS        obsolete
    DESCRIPTION
        "The lower bound of the MSID range. This value is replaced by its
         string counterpart below"
::= {uumNAIConfigEntry 3 }
uumNAIMSIDRangeMax    OBJECT-TYPE
    SYNTAX        Integer32
    MAX-ACCESS    not-accessible
    STATUS        obsolete
    DESCRIPTION
        "The upper bound of the MSID range. This value is replaced by its 
         string counterpart below"
::= {uumNAIConfigEntry 4 }
uumNAIRealm    OBJECT-TYPE
    SYNTAX        DisplayString
    MAX-ACCESS    read-write
    STATUS        current
    DESCRIPTION
        "The realm of the MSID in the range specified"
    --CONFIGURABLE
::= {uumNAIConfigEntry 5 }
uumNAIStatus    OBJECT-TYPE
    SYNTAX        RowStatus
    MAX-ACCESS    read-write
    STATUS        current
    DESCRIPTION
        "This object is used to add, modify, and delete rows for 
         the NAI construction table."
    --CONFIGURABLE
::= {uumNAIConfigEntry 6 }
uumNAIMSIDStrRangeMin OBJECT-TYPE
    SYNTAX        OCTET STRING (SIZE (1..15))
    MAX-ACCESS    read-write
    STATUS        current
    DESCRIPTION
        "The MSI range requires 15 digits which cannot be accomodated
        within an     INTEGER. Hence the older object is replaced with this
        string object"
    --CONFIGURABLE
::= {uumNAIConfigEntry 7 }
uumNAIMSIDStrRangeMax OBJECT-TYPE
    SYNTAX        OCTET STRING (SIZE (1..15))
    MAX-ACCESS    read-write
    STATUS        current
    DESCRIPTION
        "The MSI range requires 15 digits which cannot be accomodated
        within an     INTEGER. Hence the older object is replaced with this
        string object"
    --CONFIGURABLE
::= {uumNAIConfigEntry 8 }
uumNAIAuthType OBJECT-TYPE
    SYNTAX      INTEGER {
                        reject(1),
                        accept(2),
                        auth(3)
                        }
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Action taken when a NAI of this type is constructed:
        reject - drop the call using NAI in accounting record
        accept - accept the call without consulting AAA using
                 PDSNDefault user session parameters
        auth   - consult AAA using constructed NAI as user name.
        use AAA reply for session parameters if AAA accepts
        use AAA reject to drop the call"
    DEFVAL {accept}
    --CONFIGURABLE
::=     {uumNAIConfigEntry 9}
-- User managers TOD table definition
usrUserManTODConfigTable    OBJECT-TYPE
    SYNTAX        SEQUENCE OF UumTODConfigEntry
    MAX-ACCESS    not-accessible
    STATUS        current
    DESCRIPTION
        "A table for configuring Time Of Day Timer."
::= { usrUserManGroup 233 }
    uumTODConfigEntry    OBJECT-TYPE
        SYNTAX        UumTODConfigEntry
        MAX-ACCESS    not-accessible
        STATUS        current
        DESCRIPTION
            "An entry in the TOD table."
        INDEX    {uumTODIndex }
    ::= { usrUserManTODConfigTable 1 }
    UumTODConfigEntry ::= 
        SEQUENCE {
        uumTODIndex     Integer32,
        uumTODHour      Integer32,
        uumTODMinute    Integer32,
        uumTODSecond    Integer32,
        uumTODStatus    RowStatus
        }
uumTODIndex    OBJECT-TYPE
    SYNTAX        Integer32 (1..5)
    MAX-ACCESS    read-only
    STATUS        current
    DESCRIPTION
        "The index of the TOD table, it can support up 5 entries."
::= {uumTODConfigEntry 1 }
uumTODHour    OBJECT-TYPE
    SYNTAX        Integer32 (0..23)
    MAX-ACCESS    read-write
    STATUS        current
    DESCRIPTION
        "The hour of the TOD timer, it is a 24 hour clock."
    --CONFIGURABLE
::= {uumTODConfigEntry 2 }
uumTODMinute    OBJECT-TYPE
    SYNTAX        Integer32 (0..59)
    MAX-ACCESS    read-write
    STATUS        current
    DESCRIPTION
        "The minute of the TOD timer."
    --CONFIGURABLE
::= {uumTODConfigEntry 3 }
uumTODSecond    OBJECT-TYPE
    SYNTAX        Integer32 (0..59)
    MAX-ACCESS    read-write
    STATUS        current
    DESCRIPTION
        "The second of the TOD timer."
    --CONFIGURABLE
::= {uumTODConfigEntry 4 }
uumTODStatus    OBJECT-TYPE
    SYNTAX        RowStatus
    MAX-ACCESS    read-write
    STATUS        current
    DESCRIPTION
         "This object is used to add and delete rows from the TOD table."
    --CONFIGURABLE
::= {uumTODConfigEntry 5 }
