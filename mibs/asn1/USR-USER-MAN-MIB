USR-USER-MAN-MIB DEFINITIONS ::= BEGIN
    IMPORTS
        OBJECT-TYPE, OBJECT-IDENTITY, NOTIFICATION-TYPE, 
        MODULE-IDENTITY,
        Counter32, IpAddress, Integer32
            FROM SNMPv2-SMI
        TruthValue, RowStatus, DisplayString, DateAndTime
            FROM SNMPv2-TC
        common, UsrUserName, UsrDisplayString, UsrAAADisplay
            FROM USR-DEFINITIONS-MIB;
usrUserMan     MODULE-IDENTITY
    LAST-UPDATED "200304081719Z"
    ORGANIZATION "CommWorks Corporation, a 3Com Company"
    CONTACT-INFO "Customer Support"
    DESCRIPTION
        "The commWorks enterprise defined MIB objects for UserMan."
