::= { usrUserManGroup 57 }
-- ===========AAA Domain Table===========
-- AAA Domain table definition.
--      
usrUserManAAADomainTable OBJECT-TYPE
    SYNTAX SEQUENCE OF UumAAADomainEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION 
        ""
::= { usrUserManGroup 67 }
    uumAAADomainEntry    OBJECT-TYPE 
        SYNTAX      UumAAADomainEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
        "A table of AAA Domain Servers configured. Several default 
         servers are automatically entered. Adding a new entry 
         requires the following objects to be defined:-
         uumAAADomainName
         uumAAADomainAAAType
         uumAAADomainState"
        INDEX   { uumAAADomainName, uumAAADomainAAAType }
    ::= { usrUserManAAADomainTable 1 }
    UumAAADomainEntry ::=
        SEQUENCE {
        uumAAADomainName            UsrDisplayString,
        uumAAADomainAAAType         INTEGER,
        uumAAADomainServersUnavail  UsrDisplayString,
        uumAAADomainSearchAlg       INTEGER,
        uumAAADomainDNSIpRotation   INTEGER,
        uumAAADomainInterimAccountingInterval Integer32,
        uumAAADomainMirroring       INTEGER,
        uumAAADomainState           INTEGER,
        uumAAADomainStatus          INTEGER,
        uumAAADomainRebootAckReceived INTEGER
    }   
uumAAADomainName        OBJECT-TYPE
    SYNTAX      UsrDisplayString
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION 
        "The name of the AAA domain.
         Currently restricted to 32 characters in PDSN."
    --CONFIGURABLE
::= { uumAAADomainEntry 1 }
uumAAADomainAAAType  OBJECT-TYPE
    SYNTAX      INTEGER {
                        auth(1),
                        acco(2) 
                        }  
    MAX-ACCESS  not-accessible 
    STATUS      current
    DESCRIPTION  
        "AAA Server type. Can be Authorizing/Accounting/AAA"
    --CONFIGURABLE
::= { uumAAADomainEntry 2 }
uumAAADomainServersUnavail      OBJECT-TYPE
    SYNTAX      UsrDisplayString (SIZE (0..64))
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "This is the default behaviour/server if other servers 
         are unavailable. It can be DENY/PERMIT or the Server Name.
         If it is the Server Name it is an Alphanumeric string.
         It must begin with a letter or digit, contain letters, 
         digits, '.' or '-' and end with a letter or digit."
    DEFVAL { "DENY" }
    --CONFIGURABLE
::= { uumAAADomainEntry 3 }
uumAAADomainSearchAlg    OBJECT-TYPE
    SYNTAX      INTEGER {
                        fall-Through(1),
                        round-Robin(2),
                        active-server(3), -- not implemented
                        preferred(4),     -- not implemented
                        load-balancing(5)
                        }
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Domain Server Search Algorithm"
    DEFVAL { fall-Through }
    --CONFIGURABLE
::= { uumAAADomainEntry 4 }
uumAAADomainDNSIpRotation    OBJECT-TYPE
    SYNTAX      INTEGER {
                        enabled(1),
                        disabled(2)
                        }
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Indicates whether this domain should be looked up using DNS."
    DEFVAL { disabled }
    --CONFIGURABLE
::= { uumAAADomainEntry 5 }
uumAAADomainInterimAccountingInterval    OBJECT-TYPE
    SYNTAX      Integer32 (0..65535)
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Checkpoint Interval for looking up this domain.
        Exceeding the time setting will lead to trying for the
        next domain in the table. Current PDSN implementation
        supports range of 5-3600."
    DEFVAL { 240 }
    --CONFIGURABLE
::= { uumAAADomainEntry 6 }
uumAAADomainMirroring    OBJECT-TYPE
    SYNTAX      INTEGER {
                        enabled(1),
                        disabled(2)
                        }
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Indicates whether the information for this domain is
         mirrored or not."
    DEFVAL { disabled }
    --CONFIGURABLE
::= { uumAAADomainEntry 7 }
uumAAADomainState    OBJECT-TYPE
    SYNTAX      INTEGER {
                        enable(1),
                        disable(2)
                        }
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Is this domain enabled to be used ? "
    --CONFIGURABLE
::= { uumAAADomainEntry 8 }
uumAAADomainStatus    OBJECT-TYPE
    SYNTAX      INTEGER {
                        unknown(0),
                        delete(1)
                        -- inactive(2) - deprecated
                        -- active(3) - deprecated
                        }
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Used to delete an entry. Reading this alue is meaningless.
         active/inactive deprecated as inapplicable to wireless."
    --CONFIGURABLE
::= { uumAAADomainEntry 9 }
uumAAADomainRebootAckReceived    OBJECT-TYPE
    SYNTAX      INTEGER {
                        no(0),
                        yes(1)
                        }
    MAX-ACCESS  not-accessible
    STATUS  obsolete
    DESCRIPTION
        "Did this domain receive reboot ack. Leave this last in the table"
::= { uumAAADomainEntry 10 }
