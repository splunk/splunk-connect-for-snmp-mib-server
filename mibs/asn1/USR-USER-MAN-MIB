-- User managers table definition
usrUserManTable    OBJECT-TYPE
    SYNTAX SEQUENCE OF UumEntry
    MAX-ACCESS    not-accessible
    STATUS        current
    DESCRIPTION
        ""
::= { usrUserManGroup 25 }
    uumEntry    OBJECT-TYPE
    SYNTAX    UumEntry
    MAX-ACCESS    not-accessible
    STATUS        current
    DESCRIPTION
        "To create an entry in the table the following data
         is required:- uumUserName, uumUserPassword,
         uumServiceType and uumStatus = enabled."
    INDEX    { uumUserName }
::= { usrUserManTable 1 }
    UumEntry ::=
        SEQUENCE {
        uumUserName             UsrUserName,
        uumStatus               INTEGER,
        uumUserPassword         OCTET STRING,
        uumServiceType          Integer32,
        uumFramedProtocol       INTEGER,
        uumFramedIpAddress      IpAddress,
        uumFramedIpNetmask      IpAddress,
        uumFramedRouting        INTEGER,
        uumFilterIdInput        OCTET STRING,
        uumFramedMtu            Integer32,
        uumFramedCompression    INTEGER,
        uumLoginIpHost          IpAddress,
        uumLoginService         INTEGER,
        uumLoginPort            Integer32,
        uumReplyMessage         OCTET STRING,
        uumCallbackNumberOne    OCTET STRING,
        uumExpiration           OCTET STRING,
        uumFramedRoute          OCTET STRING,
        uumFramedIpxNetwork     OCTET STRING,
        uumSessionTimeout       Integer32,
        uumIdleTimeout          Integer32,
        uumBearerCapabilities   INTEGER,
        uumSpeedOfConnection    INTEGER,
        uumMaxChannels          Integer32,
        uumChannelExpansion     Integer32,
        uumChannelDecrement     Integer32,
        uumExpansionAlgorithm   INTEGER,
        uumCompressionAlgorithm INTEGER,
        uumReceiveAccMap        Integer32,
        uumTransmitAccMap       Integer32,
        uumWhereCompression     INTEGER,
        uumCompressionResetMode INTEGER,
        uumMinCompressionSize   Integer32,
        uumLocationType         INTEGER,
        uumIp                   INTEGER,
        uumIpx                  INTEGER,
        uumFilterZones          INTEGER,
        uumFilterIdOutput       OCTET STRING,
        uumAppletalk            INTEGER,
        uumBridging             INTEGER,
        uumSpoofing             INTEGER,
        uumHostType             INTEGER,
        uumSendName             OCTET STRING,
        uumSendPassword         OCTET STRING,
        uumStartTime            DateAndTime,
        uumEndTime              DateAndTime,
        uumSendScript1          OCTET STRING,
        uumReplyScript1         OCTET STRING,
        uumSendScript2          OCTET STRING,
        uumReplyScript2         OCTET STRING,
        uumSendScript3          OCTET STRING,
        uumReplyScript3         OCTET STRING,
        uumSendScript4          OCTET STRING,
        uumReplyScript4         OCTET STRING,
        uumSendScript5          OCTET STRING,
        uumReplyScript5         OCTET STRING,
        uumSendScript6          OCTET STRING,
        uumReplyScript6         OCTET STRING,
        uumTerminalType         OCTET STRING,
        uumAppletalkNetworkRange OCTET STRING,
        uumLocalIpAddress       IpAddress,
        uumRoutingProtocol      INTEGER,   -- deprecated
        uumCallbackNumberTwo    OCTET STRING,
        uumModemGroup           OCTET STRING,
        uumIpxRouting           INTEGER,
        uumIpxWan               INTEGER,
        uumIpRipSendDefault     INTEGER,
        uumIpRipSendRoutes      INTEGER,
        uumIpRipSendSubnets     INTEGER,
        uumIpRipAcceptDefault   INTEGER,
        uumIpRipSplitHorizon    INTEGER,
        uumIpRipPoisonReverse   INTEGER,
        uumIpRipFlashUpdate     INTEGER,
        uumIpRipSimpleAuth      INTEGER,
        uumIpRipV1Send          INTEGER,
        uumIpRipV1Receive       INTEGER,
        uumIpRipV2Receive       INTEGER,
        uumIpRipSilent          INTEGER,
        uumSimpleAuthPassword   OCTET STRING,
        uumIpDefaultRouteOption INTEGER,
        uumIpRoutingRipV1       INTEGER,
        uumIpRoutingRipV2       INTEGER,
        uumIpRoutingOspf        INTEGER,
        uumOspfAddresslessIfIndex Integer32,
        uumIpSecPolicyFile      DisplayString,
        uumIpSecPolicyAccess    INTEGER,
        uumIpSecPolicyType      INTEGER,
        uumIpSecPolicyConfiguration INTEGER,
        uumLoginHostName        OCTET STRING,
        uumIgmpInterfaceQueryInterval Integer32,
        uumIgmpInterfaceQueryMaxResponseTime Integer32,
        uumIgmpInterfaceVersion Integer32,
        uumIgmpInterfaceRobustness Integer32,
        uumIgmpInterfaceStatus  INTEGER,
        uumIgmpInterfaceMcastFwdEnabled    INTEGER,
        uumIpxRipUpdate         Integer32,
        uumIpxRipAgeMultiplier  Integer32,
        uumIpxSapUpdate         Integer32,
        uumIpxSapAgeMultiplier  Integer32,
        uumEncryptionAlgorithm  INTEGER,
        uumTunnelType           INTEGER,
        uumTunnelMediumType     INTEGER,
        uumTunnelClientEndpoint OCTET STRING,
        uumTunnelServerEndpoint OCTET STRING,
        uumTunnelPassword       OCTET STRING,
        uumTunnelPrivateGroupID OCTET STRING,
        uumTunnelSecurity       INTEGER,
        uumPrimaryDNSServer     IpAddress,
        uumSecondaryDNSServer   IpAddress,
        uumUserTapStatus        INTEGER,
        uumUserTapFormat        INTEGER,
        uumUserTapOutput        INTEGER,
        uumUserTapFacility      INTEGER,
        uumUserTapLoglevel      INTEGER,
        uumUserTapAddress       IpAddress,
        uumMulticastProxy       INTEGER,
        uumNasIpAddress         IpAddress,
        uumNasPort              Integer32,
        uumFramedCallbackId     Integer32,
        uumState                OCTET STRING,    
        uumClass                OCTET STRING,    
        uumCalledStationId      OCTET STRING,    
        uumCallingStationId     OCTET STRING,    
        uumNasIdentifier        OCTET STRING,
        uumCipIfName            OCTET STRING,
        uumPPPLocalIpAddress    IpAddress,
        uumPPPRemoteIpAddress   IpAddress,
        uumSessionvars          IpAddress,
        uumChatScriptName       OCTET STRING,
        uumPortLimit            Integer32,
        uumPppSourceIpFilter    Integer32,
        uumCallbackType         INTEGER,
        uumIsdnDataCallType     INTEGER,
        uumDNISReAuth           INTEGER,
        uumReChapTimeout        Integer32,
        uumNextHopGateway       IpAddress,
        uumLocalTcpPort         Integer32,
        uumConnectionvars       IpAddress,
        uumShowConnVar          Integer32,
        uumSpecialXonXoff       INTEGER,
        uumTelnetOptions        Integer32,
        uumMlPPPFragThreshold   Integer32,
        uumVPNName              OCTET STRING,
        uumVPNID                Integer32,
        uumVPNGateway           IpAddress,
        uumTrafficThreshold     Integer32,
        uumTunnelAssignmentId   OCTET STRING,
        uumKeepAliveInterval    Integer32,
		uumTunnelClientAuthId	OCTET STRING,
    	uumCorrelationId		DisplayString
    }
uumUserName    OBJECT-TYPE
    SYNTAX        UsrUserName
    MAX-ACCESS    not-accessible
    STATUS        current
    DESCRIPTION
        "The users name"
::= { uumEntry 1 }
uumStatus    OBJECT-TYPE
    SYNTAX        INTEGER {
                        delete(1),
                        inactive(2),
                        active(3),
                        enabled(4),
                        disabled(5),
                        active-disabled(6)
                        }
    MAX-ACCESS    read-write
    STATUS        current
    DESCRIPTION
        "The status field for the user. This must be set to
         'enabled' when creating an entry after which it 
         will automatically revert to inactive when the
         entry is created. For an existing user the only
         allowed values are delete(1) and disabled(5)"
    --CONFIGURABLE
::= { uumEntry 2 }
uumUserPassword    OBJECT-TYPE
    SYNTAX        OCTET STRING (SIZE (1..15))
    MAX-ACCESS    read-write
    STATUS        current
    DESCRIPTION
        "The user's password.
         Reading this object will return NULL."
    --CONFIGURABLE
::= { uumEntry 3 }
uumServiceType    OBJECT-TYPE
    SYNTAX        Integer32 
    MAX-ACCESS    read-write
    STATUS        current
    DESCRIPTION
        "The bitmap indicating one or more types of user:
        login 0x1, framed or network 0x2, callback or dialback 0x4, 
        dial-out 0x8, administrative or manage 0x10, administrative-
        readonly or manage_readonly 0x20, and direct-modem-connect 0x40.
        DIALOUT and CALLBACK are mutually exclusive.
        CALLBACK cannot be set on its own it needs to be set as
        CALLBACK/LOGIN(0x05) or CALLBACK/NETWORK(0x06).
        MANAGE can only be SET via the CLI."        
    --CONFIGURABLE
::= { uumEntry 4 }
uumFramedProtocol    OBJECT-TYPE
    SYNTAX        INTEGER {
                        ppp(1),
                        slip(2),
                        arap(3),
