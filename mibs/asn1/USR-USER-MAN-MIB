usrUserManTrapsV2 OBJECT-IDENTITY
    STATUS  current
    DESCRIPTION
            "Events for the userman process"
::= { usrUserManTraps 0 }
usrUserManAuthFailure NOTIFICATION-TYPE
    OBJECTS {
            usrUserManRadiusIP,
            usrUserManRadiusAltIP,
            usrUserManRadiusTertiaryIP
            }
    STATUS      current
    DESCRIPTION     
        "This trap is generated when authentication fails for a user."
    --#TYPE    "Alarm"
    --#SUMMARY  "Authorization failure from %d"
    --#ARGUMENTS { 0 }
    --#SEVERITY MAJOR
    --#STATE    OPERATIONAL
    --#PROBABLECAUSE "AuthorizationError"
::= { usrUserManTrapsV2 1 }
usrUserManRadiusBusyoutTrap NOTIFICATION-TYPE  
    OBJECTS {
            usrUserManRadiusIP,
            usrUserManRadiusAltIP,
            usrUserManRadiusTertiaryIP,
            usrUserManRadiusAcctIP,
            usrUserManRadiusAcctFirstBackupIP,
            usrUserManRadiusAcctSecondBackupIP,
            usrUserManRadiusAcctAltIP, 
            usrUserManRadiusAcctAltFirstBackupIP,
            usrUserManRadiusAcctAltSecondBackupIP
            }
    STATUS      current
    DESCRIPTION    
        "This trap is generated when none of the authentication 
         or accounting servers can be contacted from the NAS 
         and we mark the ports busy.
         Not Applicable to wireless components."
    --#TYPE "Alarm"
::= { usrUserManTrapsV2 2 }
usrUserManHarpDownTrap NOTIFICATION-TYPE  
    OBJECTS {
            uumAAAServerIpAddress
            }
    STATUS      current
    DESCRIPTION
        "This trap is generated when a HARP does not respond
        within usrUserManHarpDownDetectionTimer defined seconds
        and when HARP down detection is present and server is
        marked UM_DOWN.
        It sends the IP address of the failing HARP server.
        This event is only generated under these conditins if
        HarpDownDetection feature is enabled."
    --#TYPE     "ALARM"
    --#SUMMARY  "HARP Server %d failure"
    --#ARGUMENTS { 0 }
    --#SEVERITY MAJOR
    --#STATE    OPERATIONAL
    --#PROBABLECAUSE "networkError
::= { usrUserManTrapsV2 3 }
usrUserManAAAAuthFailure NOTIFICATION-TYPE
    OBJECTS {
        	uumAAAServerIpAddress,
			uumUserName,
			uumCorrelationId
            }
    STATUS      current
    DESCRIPTION     
        "This is an Auth Fail trap for the new AAA implementation"
    --#TYPE    "Alarm"
    --#SUMMARY  "Authorization failure from %d"
    --#ARGUMENTS { 0 }
    --#SEVERITY MAJOR
    --#STATE    OPERATIONAL
    --#PROBABLECAUSE "AuthorizationError"
::= { usrUserManTrapsV2 4 }
usrUserManAAAUpTrap NOTIFICATION-TYPE  
    OBJECTS {
            uumAAAServerIpAddress
            }
    STATUS      current
    DESCRIPTION
        "This trap is generated when the HARP/AAA is marked operational from
		down state. Two trigger schemes are detected for this trap. 
		1. If the HarpDownDetection feature is enabled, the trap is generated
		either after a configured amount of time (OP. Renewal status interval)
  		or is generated on receiving a response from the HARP server which is
		marked down.
		2. If the  HarpDownDetection feature is disabled, the trap is generated
		after a configured amount of time (OP. Renewal status interval)"
    --#TYPE     "ALARM"
    --#SUMMARY  "HARP(AAA) Server %d is operational"
    --#ARGUMENTS { 0 }
    --#SEVERITY MAJOR
    --#STATE    OPERATIONAL
    --#PROBABLECAUSE "HARP(AAA) server is operational"
::= { usrUserManTrapsV2 5 }
END
