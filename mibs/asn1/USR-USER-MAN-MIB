::= { usrUserManGroup 138 }
-- The usrUserManVTSTermTable is used to terminate a session using
-- the unique index key made up of usrUserManVTSTermUserName,
-- usrUserManVTSTermSessionId, usrUserManVTSTermDynamic    IpAddress,
-- and usrUserManVTSTermSessionKey.
usrUserManVTSTermTable    OBJECT-TYPE
    SYNTAX        SEQUENCE OF UsrUserManVTSTermEntry
    MAX-ACCESS    not-accessible
    STATUS        current
    DESCRIPTION
        ""
::= { usrUserManGroup 139 }
    usrUserManVTSTermEntry    OBJECT-TYPE
        SYNTAX        UsrUserManVTSTermEntry
        MAX-ACCESS    not-accessible
        STATUS        current
        DESCRIPTION
            ""
        INDEX    { usrUserManVTSTermUserName, usrUserManVTSTermSessionId,
                   usrUserManVTSTermDynamic, usrUserManVTSTermSessionKey }
    ::= { usrUserManVTSTermTable 1 }
    UsrUserManVTSTermEntry ::= 
        SEQUENCE {
        usrUserManVTSTermUserName   UsrUserName,
        usrUserManVTSTermSessionId  OCTET STRING,
        usrUserManVTSTermDynamic    IpAddress,
        usrUserManVTSTermSessionKey OCTET STRING,
        usrUserManVTSTermAction     INTEGER
    }
usrUserManVTSTermUserName    OBJECT-TYPE
    SYNTAX        UsrUserName
    MAX-ACCESS    not-accessible
    STATUS        current
    DESCRIPTION
        "The users name"
::= { usrUserManVTSTermEntry 1 }
usrUserManVTSTermSessionId    OBJECT-TYPE
    SYNTAX        OCTET STRING  (SIZE(0..255))
    MAX-ACCESS    not-accessible
    STATUS        current
    DESCRIPTION
        "The unique id for the active session"
::= { usrUserManVTSTermEntry 2 }
usrUserManVTSTermDynamic    OBJECT-TYPE
    SYNTAX        IpAddress
    MAX-ACCESS    not-accessible
    STATUS        current
    DESCRIPTION
        "The dynamic IP address for the active session"
::= { usrUserManVTSTermEntry 3 }
usrUserManVTSTermSessionKey    OBJECT-TYPE
    SYNTAX        OCTET STRING (SIZE(16))
    MAX-ACCESS    not-accessible
    STATUS        current
    DESCRIPTION
        "A unique key to identify the active session"
::= { usrUserManVTSTermEntry 4 }
usrUserManVTSTermAction    OBJECT-TYPE
    SYNTAX        INTEGER {
                        disconnectSession(1)
                        }
    MAX-ACCESS    read-write
    STATUS        current
    DESCRIPTION
        "The action to perform on the active session."
::= { usrUserManVTSTermEntry 5 }
-- The usrUserManVTSVerifyTable is used to determine if a session
-- is still active.
usrUserManVTSVerifyTable    OBJECT-TYPE
    SYNTAX        SEQUENCE OF UsrUserManVTSVerifyEntry
    MAX-ACCESS    not-accessible
    STATUS        current
    DESCRIPTION
        ""
::= { usrUserManGroup 140 }
    usrUserManVTSVerifyEntry    OBJECT-TYPE
        SYNTAX        UsrUserManVTSVerifyEntry
        MAX-ACCESS    not-accessible
        STATUS        current
        DESCRIPTION
            ""
        INDEX    { usrUserManVTSVerifySessionId }
::= { usrUserManVTSVerifyTable 1 }
    UsrUserManVTSVerifyEntry ::= 
        SEQUENCE {
        usrUserManVTSVerifySessionId    OCTET STRING,
        usrUserManVTSVerifyUserName     UsrUserName
    }
usrUserManVTSVerifySessionId    OBJECT-TYPE
    SYNTAX        OCTET STRING  (SIZE(0..255))
    MAX-ACCESS    not-accessible
    STATUS        current
    DESCRIPTION
        "The unique id for the active session"
::= { usrUserManVTSVerifyEntry 1 }
usrUserManVTSVerifyUserName    OBJECT-TYPE
    SYNTAX        UsrUserName
    MAX-ACCESS    read-only
    STATUS        current
    DESCRIPTION
        "The users name"
::= { usrUserManVTSVerifyEntry 2 }
