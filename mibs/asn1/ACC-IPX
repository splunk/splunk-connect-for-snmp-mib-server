			SYNTAX	INTEGER { 
				off(0),
				down(1),
				start(2),
				stop(3),
				up(4)
			}	
			ACCESS	read-only
			STATUS	mandatory
			DESCRIPTION
	        " Operational status of this port.
		"
			::= { accIpxPortStatEntry 15 }
