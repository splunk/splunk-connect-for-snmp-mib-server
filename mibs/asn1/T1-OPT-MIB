            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
                "The average call duration of all outbound ISDN calls made
                 from this node, since the last statistics reset."
        ::= { cdx6500T1StatISDNStatusEntry 17 }

        cdx6500T1StatDchannelState OBJECT-TYPE
            SYNTAX INTEGER {
                    up   (1),
                    down (2),
                    na   (100)
                    }
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
                "Specifies the status of the D Channel.
                 up   : The D Channel is up.
                 down : The D Channel is down.
                 na   : Value not available."
        ::= { cdx6500T1StatISDNStatusEntry 18 }

        cdx6500T1ISDNCallSummaryTable OBJECT-TYPE
            SYNTAX SEQUENCE OF Cdx6500T1StatISDNCallSummaryEntry
            ACCESS not-accessible
            STATUS mandatory
            DESCRIPTION
                "This table holds ISDN call summary information for T1 ports."
        ::= { cdx6500PSTT1E1PortTable 9 }

        cdx6500T1StatISDNCallSummaryEntry OBJECT-TYPE
            SYNTAX Cdx6500T1StatISDNCallSummaryEntry
            ACCESS not-accessible
            STATUS mandatory
            DESCRIPTION
                "An entry in the ISDN call summary table."
            INDEX { cdx6500T1StatISDNPortNumber,
                    cdx6500T1StatISDNBChannelNumber }
        ::= { cdx6500T1ISDNCallSummaryTable 1 }

        Cdx6500T1StatISDNCallSummaryEntry ::=
            SEQUENCE {
                cdx6500T1StatISDNBChannelNumber
                    INTEGER,
                cdx6500T1StatISDNBChannelStatus
                    INTEGER,
                cdx6500T1StatISDNCallDirection
                    INTEGER,
                cdx6500T1StatISDNCallingNumber
                    DisplayString,
                cdx6500T1StatISDNCalledNumber
                    DisplayString,
                cdx6500T1StatISDNDS0Rate
                    INTEGER,
                cdx6500T1StatISDNVirtualPortNumber
                    INTEGER,
                cdx6500T1StatISDNTimeCallStarted
                    DisplayString,
                cdx6500T1StatISDNPortNumber
                    PhysicalPortNumber
                }

        cdx6500T1StatISDNBChannelNumber OBJECT-TYPE
            SYNTAX INTEGER (1..23)
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
                "B-Channel number of the ISDN Channel."
        ::= { cdx6500T1StatISDNCallSummaryEntry 1 }

        cdx6500T1StatISDNBChannelStatus OBJECT-TYPE
            SYNTAX INTEGER {
                    idle         (1),
                    ringing      (2),
                    connected    (3),
                    outOfService (4),
                    na           (100)
                    }
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
                "Status of the ISDN B-Channel.
                 The posssible values are:
                 idle          : The B-Channel is in the idle state.
                 ringing       : The B-Channel is in the ringing state.
                 connected     : The B-Channel is in the connected state.
                 outOfService  : The B-Channel is out of service.
                 na            : Value not available."
        ::= { cdx6500T1StatISDNCallSummaryEntry 2 }
     
        cdx6500T1StatISDNCallDirection OBJECT-TYPE
            SYNTAX INTEGER {
                    inbound  (1),
                    outbound (2),
                    invalid  (3),
                    na       (100)
                    }
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
                "Direction of the ISDN call on the B-Channel.
                 The posssible values are:
                 inbound  : The current call is in the inbound direction.
                 outbound : The current call is in the outbound direction.
                 invalid  : There is no call present.
                 na       : Value not available."
        ::= { cdx6500T1StatISDNCallSummaryEntry 3 }

        cdx6500T1StatISDNCallingNumber OBJECT-TYPE
            SYNTAX DisplayString (SIZE (0..15))
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
                "This is the phone number of the party originating the 
                 ISDN call."
        ::= { cdx6500T1StatISDNCallSummaryEntry 4 }

        cdx6500T1StatISDNCalledNumber OBJECT-TYPE
            SYNTAX DisplayString (SIZE (0..15))
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
                "This is the phone number of the party at the destination 
                 end of the ISDN call."
        ::= { cdx6500T1StatISDNCallSummaryEntry 5 }

        cdx6500T1StatISDNDS0Rate OBJECT-TYPE
            SYNTAX INTEGER {
                    ds056k (1),
                    ds064k (2),
                    na     (100)
                    }
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
                "DS0 Rate of the ISDN B-Channel.
                 The posssible values are:
                 ds056k : The DS0 Rate is 56K.
                 ds064k : The DS0 Rate is 64K.
                 na     : Value not available."
        ::= { cdx6500T1StatISDNCallSummaryEntry 6 }

        cdx6500T1StatISDNVirtualPortNumber OBJECT-TYPE
            SYNTAX INTEGER
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
                "Virtual Port Number of the port using this B-Channel.
                 A value of 0 implies that no virtual port is
                 currently using the B-Channel."
        ::= { cdx6500T1StatISDNCallSummaryEntry 7 }

        cdx6500T1StatISDNTimeCallStarted OBJECT-TYPE
            SYNTAX DisplayString
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
                "This is the duration that the current call on this 
                 B-Channel has been connected."
        ::= { cdx6500T1StatISDNCallSummaryEntry 8 }

        cdx6500T1StatISDNPortNumber OBJECT-TYPE
            SYNTAX PhysicalPortNumber
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
                "Port number of the T1 port."
        ::= {cdx6500T1StatISDNCallSummaryEntry 9 }

        -- The last 24 hour Error Statistics Table

        cdx6500T1StatLast24HoursReportTable OBJECT-TYPE
            SYNTAX SEQUENCE OF Cdx6500T1StatLast24HoursReportEntry
            ACCESS not-accessible
            STATUS mandatory
            DESCRIPTION
                "This table holds a report of the cumulative sum
                 of the statistics for the previous 24 hour interval."
        ::= { cdx6500PSTT1E1PortTable 11 }
        
        cdx6500T1StatLast24HoursReportEntry OBJECT-TYPE
            SYNTAX Cdx6500T1StatLast24HoursReportEntry
            ACCESS not-accessible
            STATUS mandatory
            DESCRIPTION
                "Each entry contains statistics information for one T1 port."
            INDEX  { cdx6500T1StatLast24HoursReportPortNumber }
        ::= { cdx6500T1StatLast24HoursReportTable 1 }

        Cdx6500T1StatLast24HoursReportEntry ::=
            SEQUENCE {
                cdx6500T1StatLast24HoursReportPortNumber
                    PhysicalPortNumber,
                cdx6500T1StatLast24HoursReportLES
                    Gauge,
                cdx6500T1StatLast24HoursReportLCV
                    Gauge,
                cdx6500T1StatLast24HoursReportPCV
                    Gauge,
                cdx6500T1StatLast24HoursReportCSS
                    Gauge,
                cdx6500T1StatLast24HoursReportES
                    Gauge,
                cdx6500T1StatLast24HoursReportBES
                    Gauge,
                cdx6500T1StatLast24HoursReportSES
                    Gauge,
                cdx6500T1StatLast24HoursReportSEFS
                    Gauge,
                cdx6500T1StatLast24HoursReportUAS
                    Gauge
                }

        cdx6500T1StatLast24HoursReportPortNumber OBJECT-TYPE
            SYNTAX PhysicalPortNumber
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
                "Port number of the T1 port."
        ::= { cdx6500T1StatLast24HoursReportEntry 1 }
     
        cdx6500T1StatLast24HoursReportLES OBJECT-TYPE
            SYNTAX Gauge
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
                "The number of Line Errored Seconds (LESs)
                 encountered by the T1 interface in the 
                 previous 24 hour interval. "
        ::= { cdx6500T1StatLast24HoursReportEntry 2 }

        cdx6500T1StatLast24HoursReportLCV OBJECT-TYPE
            SYNTAX Gauge
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
                "The number of Line Code Violations (LCVs)
                 encountered by the T1 interface in the 
                 previous 24 hour interval."
        ::= { cdx6500T1StatLast24HoursReportEntry 3 }

        cdx6500T1StatLast24HoursReportPCV OBJECT-TYPE
            SYNTAX Gauge
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
                "The number of Path Coding  Violations (PCVs)
                 encountered by the T1 interface in the 
                 previous 24 hour interval."
        ::= { cdx6500T1StatLast24HoursReportEntry 4 }

        cdx6500T1StatLast24HoursReportCSS OBJECT-TYPE
            SYNTAX Gauge
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
                "The number of Controlled Slip Seconds (CSSs)
                 encountered by the T1 interface in the 
                 previous 24 hour interval."
        ::= { cdx6500T1StatLast24HoursReportEntry 5 }

        cdx6500T1StatLast24HoursReportES OBJECT-TYPE
            SYNTAX Gauge
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
                "The number of Errored Seconds (ESs) 
                 encountered by the T1 interface in the 
                 previous 24 hour interval."
        ::= { cdx6500T1StatLast24HoursReportEntry 6 }

        cdx6500T1StatLast24HoursReportBES OBJECT-TYPE
            SYNTAX Gauge
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
                "The number of Bursty Errored Seconds (BESs)
                 encountered by the T1 interface in the 
                 previous 24 hour interval."
        ::= { cdx6500T1StatLast24HoursReportEntry 7 }

        cdx6500T1StatLast24HoursReportSES OBJECT-TYPE
            SYNTAX Gauge
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
                "The number of Severely Errored Seconds (SESs)
                 encountered by the T1 interface in the
                 previous 24 hour interval."
        ::= { cdx6500T1StatLast24HoursReportEntry 8 }

        cdx6500T1StatLast24HoursReportSEFS OBJECT-TYPE
            SYNTAX Gauge
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
                "The number of Severely Errored Framing Seconds (SEFSs)
                 encountered by the T1 interface in the previous
                 24 hour interval."
        ::= { cdx6500T1StatLast24HoursReportEntry 9 }

        cdx6500T1StatLast24HoursReportUAS OBJECT-TYPE
            SYNTAX Gauge
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
                "The number of Unavailable Seconds (UASs)
                 encountered by the T1 interface in the 
                 previous 24 hour interval."
        ::= { cdx6500T1StatLast24HoursReportEntry 10 }

        -- The current 15 minute interval Error Statistics Table

        cdx6500T1StatCurrent15MinutesReportTable OBJECT-TYPE
