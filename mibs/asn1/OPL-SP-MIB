--
-- SCF System Actual Power Consumption
--
--	This group provides System Actual Power Consumption information
--

scfSystemActualPowerConsumptionValue OBJECT-TYPE
	SYNTAX      Integer32
	MAX-ACCESS  read-only
	STATUS      current
	DESCRIPTION
		"Power consumption of the system. Set to 0 if this feature
		is not supported."
	::= { scfSystemActualPowerConsumption 1 }

scfSystemActualPowerConsumptionUnit OBJECT-TYPE
	SYNTAX      DisplayString
	MAX-ACCESS  read-only
	STATUS      current
	DESCRIPTION
		"Unit of system power consumption."
	::= { scfSystemActualPowerConsumption 2 }

scfSystemActualPowerMinPollingInterval OBJECT-TYPE
	SYNTAX      Integer32
	MAX-ACCESS  read-only
	STATUS      current
	DESCRIPTION
		"Minimum interval time in seconds between updates of
		scfSystemActualPowerConsumptionValue. It is strongly
		recommended to get object scfSystemActualPowerConsumptionValue
		with intervals longer than this value. It is meaningless to
		get value at smaller intervals, because, internally,
		scfSystemActualPowerConsumptionValue will not be updated.
		When this feature is not supported, maximum
		value of INTEGER type is set as interval value."
	::= { scfSystemActualPowerConsumption 3 }

