	SYNTAX      SEQUENCE OF ScfBoardEntry
	MAX-ACCESS  not-accessible
	STATUS      current
	DESCRIPTION
		"Board table."
	::= { scfBoardInfo 2 }

scfBoardEntry OBJECT-TYPE
	SYNTAX      ScfBoardEntry
	MAX-ACCESS  not-accessible
	STATUS      current
	DESCRIPTION
		"Board table entry."
	INDEX   { 
		scfBoardType, 
		scfBoardId 
	}
	::= { scfBoardTable 1 }

ScfBoardEntry ::=
	SEQUENCE {
		scfBoardType ScfComponentType,
		scfBoardId ScfIndex,
		scfBoardName DisplayString,
		scfBoardAdditionalInfo DisplayString,
		scfBoardXsbs Integer32,
		scfBoardState ScfStateTC,
		scfBoardCODEnabled ScfCodState,
		scfBoardSubType ScfBoardSubTypeTC
	}

scfBoardType OBJECT-TYPE
	SYNTAX      ScfComponentType
	MAX-ACCESS  read-only
	STATUS      current
	DESCRIPTION
		"Board type."
	::= { scfBoardEntry 1 }
	
scfBoardId OBJECT-TYPE
	SYNTAX      ScfIndex
	MAX-ACCESS  read-only
	STATUS      current
	DESCRIPTION
		"Board ID."
	::= { scfBoardEntry 2 }
	
scfBoardName OBJECT-TYPE
	SYNTAX      DisplayString
	MAX-ACCESS  read-only
	STATUS      current
	DESCRIPTION
		"Board description."
	::= { scfBoardEntry 3 }
	
scfBoardAdditionalInfo OBJECT-TYPE
	SYNTAX      DisplayString
	MAX-ACCESS  read-only
	STATUS      current
	DESCRIPTION
		"Additional board info."
	::= { scfBoardEntry 4 }
	
scfBoardXsbs OBJECT-TYPE
	SYNTAX      Integer32
	MAX-ACCESS  read-only
	STATUS      current
	DESCRIPTION
		"The total number of XSB partitions for this board."
	::= { scfBoardEntry 5 }

scfBoardState OBJECT-TYPE
	SYNTAX      ScfStateTC
	MAX-ACCESS  read-only
	STATUS      current
	DESCRIPTION
		"Board operating state."
	::= { scfBoardEntry 6 }

scfBoardCODEnabled OBJECT-TYPE
	SYNTAX	    ScfCodState
	MAX-ACCESS  read-only
	STATUS      current
	DESCRIPTION
		"COD State of the board."
	::= { scfBoardEntry 7 }

scfBoardSubType OBJECT-TYPE
	SYNTAX	    ScfBoardSubTypeTC
	MAX-ACCESS  read-only
	STATUS      current
	DESCRIPTION
		"Board subtype."
	::= { scfBoardEntry 8 }

