		scfMemorySubType ScfComponentType,
		scfMemorySubId ScfIndex,
		scfMemoryName DisplayString,
		scfMemoryCapacity Integer32,
		scfMemoryAdditionalInfo DisplayString,
		scfMemoryState ScfStateTC
	}

