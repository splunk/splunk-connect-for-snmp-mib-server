	INDEX   { scfDomainId }
	::= { scfDomainTable 1 }

ScfDomainEntry ::=
	SEQUENCE {
		scfDomainId ScfIndex,
		scfDomainCpuNumber Integer32,
		scfDomainMemoryCapacity Integer32,
		scfDomainObpVersion DisplayString,
		scfDomainObpAdditionalInfo DisplayString,
		scfDomainOsMachine DisplayString,
		scfDomainOsRelease DisplayString,
		scfDomainOsSysName DisplayString,
		scfDomainOsNodeName DisplayString,
		scfDomainOsVersion DisplayString,
		scfDomainOsAdditionalInfo DisplayString,
		scfDomainValid ScfValidStatus,
		scfDomainXsbs DisplayString,
		scfDomainStatus ScfDomainStatusTC,
		scfDomainErrorStatus ScfErrorStatus,
		scfDomainConfigurationPolicy ScfDomainConfigPolicy
	}

