--
-- SCF XSB Table
--

scfXsbTable OBJECT-TYPE
	SYNTAX      SEQUENCE OF ScfXsbEntry
	MAX-ACCESS  not-accessible
	STATUS      current
	DESCRIPTION
		"XSB table."
	::= { scfXsbInfo 3 }

scfXsbEntry OBJECT-TYPE
	SYNTAX      ScfXsbEntry
	MAX-ACCESS  not-accessible
	STATUS      current
	DESCRIPTION
		"XSB table entry."
	INDEX   { scfXsbId }
	::= { scfXsbTable 1 }

ScfXsbEntry ::=
	SEQUENCE {
		scfXsbId ScfXsbIndex,
            	scfXsbStatus ScfStateTC,
            	scfXsbErrorStatus ScfErrorStatus,
            	scfXsbDomainId Integer32,
            	scfXsbDrStatus ScfDRState,
            	scfXsbSubStatusPower ScfUsageState,
            	scfXsbSubStatusTest ScfTestState,
            	scfXsbSubStatusAssignment ScfAssignmentState,
            	scfXsbSubStatusConnectivity ScfConnectivityState,
            	scfXsbSubStatusConfiguration ScfConfigurationState,
		scfXsbSetupDID Integer32,
            	scfXsbNextDID Integer32
	}

scfXsbId OBJECT-TYPE
	SYNTAX      ScfXsbIndex
	MAX-ACCESS  read-only
	STATUS      current
	DESCRIPTION
		"XSB ID."
	::= { scfXsbEntry 1 }

scfXsbStatus OBJECT-TYPE
	SYNTAX      ScfStateTC
	MAX-ACCESS  read-only
	STATUS      current
	DESCRIPTION
		"XSB operational state."
	::= { scfXsbEntry 2 }

scfXsbErrorStatus OBJECT-TYPE
	SYNTAX      ScfErrorStatus
	MAX-ACCESS  read-only
	STATUS      current
	DESCRIPTION
		"XSB error status."
	::= { scfXsbEntry 3 }

scfXsbDomainId OBJECT-TYPE
	SYNTAX      Integer32
	MAX-ACCESS  read-only
	STATUS      current
	DESCRIPTION
		"Active domain ID of the XSB."
	::= { scfXsbEntry 4 }

scfXsbDrStatus OBJECT-TYPE
	SYNTAX      ScfDRState
	MAX-ACCESS  read-only
	STATUS      current
	DESCRIPTION
		"Domain reconfiguration status of the XSB."
	::= { scfXsbEntry 5 }

scfXsbSubStatusPower OBJECT-TYPE
	SYNTAX      ScfUsageState
	MAX-ACCESS  read-only
	STATUS      current
	DESCRIPTION
		"XSB detail status power state."
	::= { scfXsbEntry 6 }

scfXsbSubStatusTest OBJECT-TYPE
	SYNTAX      ScfTestState
	MAX-ACCESS  read-only
	STATUS      current
	DESCRIPTION
		"XSB detail status test state."
	::= { scfXsbEntry 7 }

