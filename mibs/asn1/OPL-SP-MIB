-- ***************************************************************************
-- *                            scfMIBNotifGroups                            *
-- ***************************************************************************

scfNotificationsGroup NOTIFICATION-GROUP
	NOTIFICATIONS {
		scfSPFailover, scfComponentEvent, scfComponentStatusEvent,
		scfXsbModeChangeEvent, scfDomainStatusAlarm,
		scfDomainXsbEvent, scfIoBoxEvent, scfIoBoxLedEvent,
		scfIoBoxTemperatureEvent, scfModeSwitchEvent
    	}
    	STATUS current
    	DESCRIPTION
        	"The collection of notifications used to indicate events,
		alarms, and state changes that occur in the system."
    	::= { scfMIBNotifGroups 1 }

