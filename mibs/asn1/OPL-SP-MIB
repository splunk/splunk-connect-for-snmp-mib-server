--
-- SCF I/O Box Information
--
--	This group provides information for the I/O Box expansion chassis that
--   are attached to the System and the components which make it up.
--   The components include I/O Boats, Link Cards, and Power Supplies/Fans.
--
--	The tables within this object group are all related based on a common
--   index, IoBoxId.  Each table has their own definition of this index,
--   however, each entry in the scfIoBoxTable by using it's scfIoBoxId value
--   can be used in part to find augmenting information pertaining to that
--   I/O Box chassis including Boat, Link Card, Power Supply/Fan, and sensor
--   information for the Box and Box components.
--

scfIoBoxNumber OBJECT-TYPE
	SYNTAX      Integer32
	MAX-ACCESS  read-only
	STATUS      current
	DESCRIPTION
		"Total count of I/O Box Table entries."
	::= { scfIoBoxInfo 1 }

scfIoBoxTable OBJECT-TYPE
	SYNTAX      SEQUENCE OF ScfIoBoxEntry
	MAX-ACCESS  not-accessible
	STATUS      current
	DESCRIPTION 
		"I/O Box table."
	::= { scfIoBoxInfo 2 }

scfIoBoxEntry OBJECT-TYPE
	SYNTAX      ScfIoBoxEntry
	MAX-ACCESS  not-accessible
	STATUS      current
	DESCRIPTION
		"I/O Box entry."
	INDEX   { scfIoBoxId }
	::= { scfIoBoxTable 1 }

ScfIoBoxEntry ::=
	SEQUENCE {
		scfIoBoxId ScfIoBoxIndex,
		scfIoBoxLocationLED ScfIoBoxLEDState,
		scfIoBoxOverTempLED ScfIoBoxLEDState,
		scfIoBoxServiceReqLED ScfIoBoxLEDState,
		scfIoBoxActiveLED ScfIoBoxLEDState,
		scfIoBoxPartNumber DisplayString,
		scfIoBoxSerialNumber DisplayString,
		scfIoBoxDashLevel DisplayString
	}

scfIoBoxId OBJECT-TYPE
	SYNTAX         ScfIoBoxIndex
	MAX-ACCESS     read-only
	STATUS         current
	DESCRIPTION    
		"Unique I/O Box identifier.  The persistent unique identifier
		for an I/O Box is it's serial number."
	::= { scfIoBoxEntry 1 }

scfIoBoxLocationLED OBJECT-TYPE
	SYNTAX      ScfIoBoxLEDState
	MAX-ACCESS  read-only
	STATUS      current
	DESCRIPTION
		"State of the Location LED."
	::= { scfIoBoxEntry 2 }

scfIoBoxOverTempLED OBJECT-TYPE
	SYNTAX      ScfIoBoxLEDState
	MAX-ACCESS  read-only
	STATUS      current
	DESCRIPTION    
		"State of the Over Temperature LED."
	::= { scfIoBoxEntry 3 }

scfIoBoxServiceReqLED OBJECT-TYPE
	SYNTAX      ScfIoBoxLEDState
	MAX-ACCESS  read-only
	STATUS      current
	DESCRIPTION    
		"State of the Service Request LED."
	::= { scfIoBoxEntry 4 }

scfIoBoxActiveLED OBJECT-TYPE
	SYNTAX      ScfIoBoxLEDState
	MAX-ACCESS  read-only
	STATUS      current
	DESCRIPTION    
		"State of the Active LED."
	::= { scfIoBoxEntry 5 }

scfIoBoxPartNumber OBJECT-TYPE
	SYNTAX      DisplayString
	MAX-ACCESS  read-only
	STATUS      current
	DESCRIPTION
		"I/O Box part number."
	::= { scfIoBoxEntry 6 }
	
scfIoBoxSerialNumber OBJECT-TYPE
	SYNTAX      DisplayString
	MAX-ACCESS  read-only
	STATUS      current
	DESCRIPTION
		"I/O Box serial number."
	::= { scfIoBoxEntry 7 }

scfIoBoxDashLevel OBJECT-TYPE
	SYNTAX      DisplayString
	MAX-ACCESS  read-only
	STATUS      current
	DESCRIPTION
		"I/O Box dash level."
	::= { scfIoBoxEntry 8 }

