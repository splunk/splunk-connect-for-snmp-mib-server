--
-- SCF IOBox Sensor Table
--

scfIoBoxSensorNumber OBJECT-TYPE
	SYNTAX      Integer32
	MAX-ACCESS  read-only
	STATUS      current
	DESCRIPTION
		"Total count of I/O Box environmental sensor table entries."
	::= { scfIoBoxInfo 9 }

scfIoBoxSensorTable OBJECT-TYPE
	SYNTAX      SEQUENCE OF ScfIoBoxSensorEntry
	MAX-ACCESS  not-accessible
	STATUS      current
	DESCRIPTION
		"I/O Box environmental sensor table."
	::= { scfIoBoxInfo 10 }

scfIoBoxSensorEntry OBJECT-TYPE
	SYNTAX      ScfIoBoxSensorEntry
	MAX-ACCESS  not-accessible
	STATUS      current
	DESCRIPTION
		"I/O Box environmental sensor entry."
	INDEX   {
		scfIoBoxSensorIoBoxId,
		scfIoBoxSensorIoBoxSubType,
		scfIoBoxSensorIoBoxSubId,
		scfIoBoxSensorId
	}
	::= { scfIoBoxSensorTable 1 }

ScfIoBoxSensorEntry ::=
	SEQUENCE {
		scfIoBoxSensorIoBoxId ScfIoBoxIndex,
		scfIoBoxSensorIoBoxSubType ScfIoBoxComponentType,
		scfIoBoxSensorIoBoxSubId ScfIndex,
		scfIoBoxSensorId ScfIndex,
		scfIoBoxSensorDescription DisplayString,
		scfIoBoxSensorUnits DisplayString,
		scfIoBoxSensorValue Integer32,
		scfIoBoxSensorEnabledAlarms ScfIoBoxEnabledAlarms,
		scfIoBoxSensorMinAlarm Integer32,
		scfIoBoxSensorMaxAlarm Integer32
	}

scfIoBoxSensorIoBoxId OBJECT-TYPE
	SYNTAX         ScfIoBoxIndex
	MAX-ACCESS     read-only
	STATUS         current
	DESCRIPTION    
		"Unique I/O Box identifier for a sensor entry.  The 
		persistent unique identifier for an I/O Box is it's serial 
		number."
	::= { scfIoBoxSensorEntry 1 }

scfIoBoxSensorIoBoxSubType OBJECT-TYPE
	SYNTAX         ScfIoBoxComponentType
	MAX-ACCESS     read-only
	STATUS         current
	DESCRIPTION    
		"Type of the I/O Box component that the sensor
		is located."
	::= { scfIoBoxSensorEntry 2 }


scfIoBoxSensorIoBoxSubId OBJECT-TYPE
	SYNTAX         ScfIndex
	MAX-ACCESS     read-only
	STATUS         current
	DESCRIPTION    
		"Identifier for the I/O Box component that the sensor
		is located."
	::= { scfIoBoxSensorEntry 3 }

scfIoBoxSensorId OBJECT-TYPE
	SYNTAX         ScfIndex
	MAX-ACCESS     read-only
	STATUS         current
	DESCRIPTION
		"Identifier of the sensor."
	::= { scfIoBoxSensorEntry 4 }

scfIoBoxSensorDescription OBJECT-TYPE
	SYNTAX         DisplayString
	MAX-ACCESS     read-only
	STATUS         current
	DESCRIPTION
		"A description of the environmental sensor."
	::= { scfIoBoxSensorEntry 5 }

scfIoBoxSensorUnits OBJECT-TYPE
	SYNTAX         DisplayString
	MAX-ACCESS     read-only
	STATUS         current
	DESCRIPTION
		"The units for the environmental sensor value."
	::= { scfIoBoxSensorEntry 6 }

scfIoBoxSensorValue OBJECT-TYPE
	SYNTAX         Integer32
	MAX-ACCESS     read-only
	STATUS         current
	DESCRIPTION    
		"The value read from the environmental sensor.

		This object can be compared with the Sensor Minimum Alarm
		and Maximum Alarm objects in this table to determine the value
		status.  In addition, the Enabled Alarm should be
		used to determine what this value can be compared against,
		MinAlarm, MaxAlarm, or both.

		If a value has exceeded the Minimum or Maximum, then the user
		will be notified through a change in the I/O Box
		component LED or temperature event trap.  Through the
		comparisions described here, further information can be
		gathered as to the cause."
	::= { scfIoBoxSensorEntry 7 }

scfIoBoxSensorEnabledAlarms OBJECT-TYPE
	SYNTAX         ScfIoBoxEnabledAlarms
	MAX-ACCESS     read-only
	STATUS         current
	DESCRIPTION    
		"The environmental alarm threshold values to be utilized
		for this sensor.  The values immediately follow this object."
	::= { scfIoBoxSensorEntry 8 }

scfIoBoxSensorMinAlarm OBJECT-TYPE
	SYNTAX         Integer32
	MAX-ACCESS     read-only
	STATUS         current
	DESCRIPTION    
		"The environmental sensor minimum threshold value before
		an alarm is generated.  Not utilized for all sensors."
	::= { scfIoBoxSensorEntry 9 }

scfIoBoxSensorMaxAlarm OBJECT-TYPE
	SYNTAX         Integer32
	MAX-ACCESS     read-only
	STATUS         current
	DESCRIPTION    
		"The environmental sensor maximum threshold value before
		an alarm is generated.  Not utilized for all sensors."
	::= { scfIoBoxSensorEntry 10 }

--
-- SCF Component Information
--
--	This group provides FRU and Status information for every component 
--   in the system.
--

scfComponentNumber OBJECT-TYPE
	SYNTAX      Integer32
	MAX-ACCESS  read-only
	STATUS      current
	DESCRIPTION
		"Total count of Component Table entries."
	::= { scfComponentInfo 1 }

--
-- SCF Component Table
--

scfComponentTable OBJECT-TYPE
	SYNTAX      SEQUENCE OF ScfComponentEntry
	MAX-ACCESS  not-accessible
	STATUS      current
	DESCRIPTION
		"Hardware Component table."
	::= { scfComponentInfo 2 }

scfComponentEntry OBJECT-TYPE
	SYNTAX      ScfComponentEntry
	MAX-ACCESS  not-accessible
	STATUS      current
	DESCRIPTION
		"Component table entry."
	INDEX   {
		scfComponentBoardType,
		scfComponentBoardId,
		scfComponentModuleType,
		scfComponentModuleId,
		scfComponentSubType,
		scfComponentSubId
	}
	::= { scfComponentTable 1 }

ScfComponentEntry ::=
	SEQUENCE {
		scfComponentBoardType ScfComponentType,
		scfComponentBoardId ScfIndex,
		scfComponentModuleType ScfComponentType,
		scfComponentModuleId ScfIndex,
		scfComponentSubType ScfComponentType,
		scfComponentSubId ScfIndex,
		scfComponentAdditionalInfo DisplayString,
		scfComponentPartNumber DisplayString,
		scfComponentSerialNumber DisplayString,
		scfComponentProductName DisplayString,
		scfComponentManufacturer DisplayString,
		scfComponentManufactureDate DisplayString,
		scfComponentStatus ScfStateTC,
		scfComponentErrorStatus ScfErrorStatus
	}

scfComponentBoardType OBJECT-TYPE
	SYNTAX 	    ScfComponentType
	MAX-ACCESS  read-only
	STATUS      current
	DESCRIPTION
		"Component board type."
	::= { scfComponentEntry 1 }
	
scfComponentBoardId OBJECT-TYPE
	SYNTAX      ScfIndex
	MAX-ACCESS  read-only
	STATUS      current
	DESCRIPTION
		"Component board ID."
	::= { scfComponentEntry 2 }
	
scfComponentModuleType OBJECT-TYPE
	SYNTAX      ScfComponentType
	MAX-ACCESS  read-only
	STATUS      current
	DESCRIPTION
		"Component module type."
	::= { scfComponentEntry 3 }
	
scfComponentModuleId OBJECT-TYPE
	SYNTAX      ScfIndex
	MAX-ACCESS  read-only
	STATUS      current
	DESCRIPTION
		"Component module ID."
	::= { scfComponentEntry 4 }
	
scfComponentSubType OBJECT-TYPE
	SYNTAX 	    ScfComponentType
	MAX-ACCESS  read-only
	STATUS      current
	DESCRIPTION
		"Component sub-part type."
	::= { scfComponentEntry 5 }
	
scfComponentSubId OBJECT-TYPE
	SYNTAX      ScfIndex
	MAX-ACCESS  read-only
	STATUS      current
	DESCRIPTION
		"Component sub-part ID."
	::= { scfComponentEntry 6 }
	
scfComponentAdditionalInfo OBJECT-TYPE
	SYNTAX      DisplayString
	MAX-ACCESS  read-only
	STATUS      current
	DESCRIPTION
		"Additional component info."
	::= { scfComponentEntry 7 }

scfComponentPartNumber OBJECT-TYPE
	SYNTAX      DisplayString
	MAX-ACCESS  read-only
	STATUS      current
	DESCRIPTION
		"Component part number."
	::= { scfComponentEntry 8 }
		
scfComponentSerialNumber OBJECT-TYPE
	SYNTAX      DisplayString
	MAX-ACCESS  read-only
	STATUS      current
	DESCRIPTION
		"Component serial number."
	::= { scfComponentEntry 9 }
	
scfComponentProductName OBJECT-TYPE
	SYNTAX      DisplayString
	MAX-ACCESS  read-only
	STATUS      current
	DESCRIPTION
		"Component product name."
	::= { scfComponentEntry 10 }

scfComponentManufacturer OBJECT-TYPE
	SYNTAX      DisplayString
	MAX-ACCESS  read-only
	STATUS      current
	DESCRIPTION
		"Component manufacturer."
	::= { scfComponentEntry 11 }
	
scfComponentManufactureDate OBJECT-TYPE
	SYNTAX      DisplayString
	MAX-ACCESS  read-only
	STATUS      current
	DESCRIPTION
		"Component manufacture date.

		This date varies for different components.  For many components
		this object will provide IPMI format, which is 3 bytes of the
		number of minutes from 0:00 hrs 1/1/96. In this case the most
		significant byte is first.  In some components, 2 bytes will be
		provided with the first byte year(yy) in binary-coded
		decimal(BCD), and the second byte week(ww) in BCD."
	::= { scfComponentEntry 12 }
				
scfComponentStatus OBJECT-TYPE
	SYNTAX	    ScfStateTC
	MAX-ACCESS  read-only
	STATUS      current
	DESCRIPTION
		"Component operational status."
	::= { scfComponentEntry 13 }
		
scfComponentErrorStatus OBJECT-TYPE
	SYNTAX      ScfErrorStatus
	MAX-ACCESS  read-only
	STATUS      current
	DESCRIPTION
		"Error status of component."
	::= { scfComponentEntry 14 }
	

-- ***************************************************************************
-- *                                scfMIBTraps                              *
-- ***************************************************************************

--
-- MIB Notification data
--
--   The following objects may be optionally appended to any notification.
--

scfTrapEventType OBJECT-TYPE
    	SYNTAX	    ScfTrapEventTypeTC
    	MAX-ACCESS  accessible-for-notify
    	STATUS      current
    	DESCRIPTION 
		"The event type of this notification.  This provides additional
		information on the reason that the notification was sent."
    	::= { scfMIBTrapData 1 }

scfTrapStatusEventType OBJECT-TYPE
    	SYNTAX	    ScfTrapStatusEventTypeTC
    	MAX-ACCESS  accessible-for-notify
    	STATUS      current
    	DESCRIPTION 
		"The status event type of this notification.  This provides
		additional information on the reason that the notification
		was sent."
    	::= { scfMIBTrapData 2 }

scfTrapDomainStatusAlarmType OBJECT-TYPE
    	SYNTAX	    ScfDomainStatusAlarmType
    	MAX-ACCESS  accessible-for-notify
    	STATUS      current
    	DESCRIPTION 
		"The type of this domain alarm that provides additional
		information on the reason that the notification was sent."
    	::= { scfMIBTrapData 3 }

scfTrapIoBoxId OBJECT-TYPE
	SYNTAX         ScfIoBoxIndex
	MAX-ACCESS     accessible-for-notify
	STATUS         current
	DESCRIPTION    
		"Trap unique I/O Box identifier.  The persistent
		unique identifier for an I/O Box is it's serial 
		number."
	::= { scfMIBTrapData 4 }

scfTrapIoBoxSubType OBJECT-TYPE
	SYNTAX         ScfIoBoxComponentType
	MAX-ACCESS     accessible-for-notify
	STATUS         current
	DESCRIPTION    
		"Type of the I/O Box component that a trap
		originated from."
	::= { scfMIBTrapData 5 }

scfTrapIoBoxSubId OBJECT-TYPE
	SYNTAX         ScfIndex
	MAX-ACCESS     accessible-for-notify
	STATUS         current
	DESCRIPTION    
		"Identifier for the I/O Box component that a trap
		originated from."
	::= { scfMIBTrapData 6 }

scfTrapIoBoxTempEventType OBJECT-TYPE
	SYNTAX         ScfTrapIoBoxTempEventTypeTC
	MAX-ACCESS     accessible-for-notify
	STATUS         current
	DESCRIPTION
		"The event type of this notification.  This provides additional
		information on the reason that the notification was sent."
	::= { scfMIBTrapData 7 }

scfTrapIoBoxLedType OBJECT-TYPE
	SYNTAX         ScfIoBoxLEDType
	MAX-ACCESS     accessible-for-notify
	STATUS         current
	DESCRIPTION    
		"LED Type for the I/O Box component that a trap
		originated from."
	::= { scfMIBTrapData 8 }

scfTrapIoBoxLedValue OBJECT-TYPE
	SYNTAX         ScfIoBoxLEDState
	MAX-ACCESS     accessible-for-notify
	STATUS         current
	DESCRIPTION    
		"LED Value for the I/O Box component LED that a trap
		originated from."
	::= { scfMIBTrapData 9 }

scfTrapModeSwitchEventType OBJECT-TYPE
    	SYNTAX	    ScfTrapModeSwitchEventTypeTC
    	MAX-ACCESS  accessible-for-notify
    	STATUS      current
    	DESCRIPTION 
		"The panel switch event type of this notification. This
		provides additional information on the reason that the
		notification was sent."
    	::= { scfMIBTrapData 10 }

--
-- MIB Notifications
--

scfSPFailover NOTIFICATION-TYPE
    	OBJECTS {
		scfAgentIndex
    	}
    	STATUS      current
    	DESCRIPTION
        	"Indicates that a failover from the active SP to the standby 
		SP has occurred.  The Board ID included is the ID of the 
		SP that has been failed over to."
	::= { scfMIBTrapPrefix 1 }

scfComponentEvent NOTIFICATION-TYPE
    	OBJECTS {
		scfComponentSerialNumber,
		scfTrapEventType
    	}
    	STATUS      current
    	DESCRIPTION
        	"Indicates that a component has been added to or removed from
		the system, or that a component-specific event has occurred.
		When indicating that a component has been added to or removed
		from the system, the event will be based on the highest level
		component possible.  For example, if a CMU is removed, then
		only a trap for the CMU is generated and not for the CPU and
		Memory that are on that CMU."
	::= { scfMIBTrapPrefix 2 }

scfComponentStatusEvent NOTIFICATION-TYPE
    	OBJECTS { 
		scfComponentStatus,
		scfTrapStatusEventType
    	}
    	STATUS      current
    	DESCRIPTION
        	"Indicates that a component has changed to a status
		of interest, including fault, degraded, and recovery from
		a previous fault or degraded state condition."
	::= { scfMIBTrapPrefix 3 }

scfXsbModeChangeEvent NOTIFICATION-TYPE
    	OBJECTS {
		scfBoardXsbs
    	}
    	STATUS      current
    	DESCRIPTION
        	"Indicates that an XSB mode has been changed to either
		a divided or non-divided state."
	::= { scfMIBTrapPrefix 4 }

scfDomainStatusAlarm NOTIFICATION-TYPE
    	OBJECTS { 
		scfDomainStatus,
		scfTrapDomainStatusAlarmType
    	}
    	STATUS      current
    	DESCRIPTION
        	"Indicates that a status alarm condition has occurred on a 
		domain.  This includes a status change and a panic."
	::= { scfMIBTrapPrefix 5 }

scfDomainXsbEvent NOTIFICATION-TYPE
    	OBJECTS { 
		scfXsbDomainId,
		scfTrapEventType
    	}
    	STATUS      current
    	DESCRIPTION
        	"Indicates that an XSB has been added to or removed from
		a domain."
	::= { scfMIBTrapPrefix 6 }

scfIoBoxEvent NOTIFICATION-TYPE
    	OBJECTS { 
		scfTrapIoBoxId,
		scfTrapIoBoxSubType,
		scfTrapIoBoxSubId,
		scfTrapEventType
    	}
    	STATUS      current
    	DESCRIPTION
        	"Indicates that an I/O Box component has been added to or
		removed from the system."
	::= { scfMIBTrapPrefix 7 }

scfIoBoxLedEvent NOTIFICATION-TYPE
    	OBJECTS { 
		scfTrapIoBoxId,
		scfTrapIoBoxSubType,
		scfTrapIoBoxSubId,
		scfTrapIoBoxLedType,
		scfTrapIoBoxLedValue
    	}
    	STATUS      current
    	DESCRIPTION
        	"Indicates that an I/O Box component LED changed.  Depending
		on the LED type, this will notify of errors detected in an
		I/O Box component."
	::= { scfMIBTrapPrefix 8 }

scfIoBoxTemperatureEvent NOTIFICATION-TYPE
    	OBJECTS { 
		scfTrapIoBoxId,
		scfTrapIoBoxSubType,
		scfTrapIoBoxSubId,
		scfTrapIoBoxTempEventType
    	}
    	STATUS      current
    	DESCRIPTION
        	"Indicates that an I/O Box temperature sensor has triggered
		an event."
	::= { scfMIBTrapPrefix 9 }

scfModeSwitchEvent NOTIFICATION-TYPE
    	OBJECTS {
		scfTrapModeSwitchEventType,
		scfModeSwitch
    	}
    	STATUS      current
    	DESCRIPTION
        	"Indicates that a panel switch has triggered an event."
	::= { scfMIBTrapPrefix 10 }

-- ***************************************************************************
-- *                         scfCompliance                            	     *
-- ***************************************************************************

scfCompliance MODULE-COMPLIANCE
	STATUS current
    	DESCRIPTION
        	"The compliance statement for SNMP entities which 
		implement this MIB."
    	MODULE -- this MODULE
        	MANDATORY-GROUPS {
            		scfInfoGroup,
	    		scfStateGroup,
	    		scfMonitorInfoGroup,
	    		scfSystemInfoGroup,
	    		scfDomainInfoGroup,
	    		scfXsbInfoGroup,
			scfLsbInfoGroup,
	    		scfBoardInfoGroup,
	    		scfCpuInfoGroup,
	    		scfMemoryInfoGroup,
	    		scfIoBoxInfoGroup,
	    		scfComponentInfoGroup,
			scfNotificationObjectGroup,
            		scfNotificationsGroup,
	    		scfSystemActualPowerConsumptionGroup,
	    		scfSystemAmbientTemperatureGroup,
	    		scfSystemPermittedPowerConsumptionGroup,
	    		scfSystemAirFlowGroup
        	}
	::= { scfMIBCompliances 1 }

-- ***************************************************************************
-- *                            scfMIBObjectGroups                           *
-- ***************************************************************************

