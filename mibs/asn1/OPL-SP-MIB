	SYNTAX      ScfConfigurationState
	MAX-ACCESS  read-only
	STATUS      current
	DESCRIPTION
		"XSB detail status configuration state."
	::= { scfXsbEntry 10 }

scfXsbSetupDID OBJECT-TYPE
	SYNTAX      Integer32
	MAX-ACCESS  read-only
	STATUS      current
	DESCRIPTION
		"The Setup Domain ID.  Shows the Domain ID where the XSB
		will be added."
	::= { scfXsbEntry 11 }

scfXsbNextDID OBJECT-TYPE
	SYNTAX      Integer32
	MAX-ACCESS  read-only
	STATUS      current
	DESCRIPTION
		"Next Domain ID.  Shows the Domain ID where the XSB is
		preassigned."
	::= { scfXsbEntry 12 }

--
-- SCF LSB Information
--
--	This group provides information specific to all Logical System
--   Boards(LSBs).  
--

scfLsbType OBJECT-TYPE
	SYNTAX      Integer32
	MAX-ACCESS  read-only
	STATUS      current
	DESCRIPTION
		"Type of LSB rule for this system."
	::= { scfLsbInfo 1 }

scfLsbNumber OBJECT-TYPE
	SYNTAX      Integer32
	MAX-ACCESS  read-only
	STATUS      current
	DESCRIPTION
		"Total count of LSB Table entries."
	::= { scfLsbInfo 2 }

--
-- SCF LSB Table
--
--	This table contains Domain Component List(DCL) information
--   for each LSB.
--

scfLsbTable OBJECT-TYPE
	SYNTAX      SEQUENCE OF ScfLsbEntry
	MAX-ACCESS  not-accessible
	STATUS      current
	DESCRIPTION
		"LSB table."
	::= { scfLsbInfo 3 }

scfLsbEntry OBJECT-TYPE
	SYNTAX      ScfLsbEntry
	MAX-ACCESS  not-accessible
	STATUS      current
	DESCRIPTION
		"LSB table entry."
	INDEX   { scfLsbDomainId, scfLsbId }
	::= { scfLsbTable 1 }

ScfLsbEntry ::=
	SEQUENCE {
		scfLsbDomainId ScfIndex,
		scfLsbId ScfIndex,
		scfLsbXsbId DisplayString,
		scfLsbNoMem ScfUsageState,
            	scfLsbNoIo ScfUsageState,
            	scfLsbFloatingBoard ScfUsageState
	}

scfLsbDomainId OBJECT-TYPE
	SYNTAX      ScfIndex
	MAX-ACCESS  read-only
	STATUS      current
	DESCRIPTION
		"LSB Domain ID."
	::= { scfLsbEntry 1 }

scfLsbId OBJECT-TYPE
	SYNTAX      ScfIndex
	MAX-ACCESS  read-only
	STATUS      current
	DESCRIPTION
		"LSB ID."
	::= { scfLsbEntry 2 }

scfLsbXsbId OBJECT-TYPE
	SYNTAX      DisplayString
	MAX-ACCESS  read-only
	STATUS      current
	DESCRIPTION
		"Current XSB ID that is registered to this LSB."
	::= { scfLsbEntry 3 }

scfLsbNoMem OBJECT-TYPE
	SYNTAX      ScfUsageState
	MAX-ACCESS  read-only
	STATUS      current
	DESCRIPTION
		"DCL no-mem use setting.  Reflects if the memory which is
		part of this LSB should not be used in the Domain."
	::= { scfLsbEntry 4 }

scfLsbNoIo OBJECT-TYPE
	SYNTAX      ScfUsageState
	MAX-ACCESS  read-only
	STATUS      current
	DESCRIPTION
		"DCL no-io use setting.  Reflects if the I/O which is
		part of this LSB should not be used in the Domain"
	::= { scfLsbEntry 5 }

scfLsbFloatingBoard OBJECT-TYPE
	SYNTAX      ScfUsageState
	MAX-ACCESS  read-only
	STATUS      current
	DESCRIPTION
		"DCL floating board use setting.  Reflects if the LSB
		is considered a floating board which eases DR operations."
	::= { scfLsbEntry 6 }

--
-- SCF Board Information
--
--	This group provides information pertaining to specific board components
--   within a System.  Currently this is limited to CPU Memory Board Units(CMU)
--   and I/O Units(IOU).
--

scfBoardNumber OBJECT-TYPE
	SYNTAX      Integer32
	MAX-ACCESS  read-only
	STATUS      current
	DESCRIPTION
		"Total count of Board Table entries."
	::= { scfBoardInfo 1 }

