-- ***************************************************************************
-- *                           scfObjects               	             *
-- ***************************************************************************

--
-- SCF Information
--
--	This group provides general information pertaining to the System
--   Control Facility(SCF).
--

scfAgentId OBJECT-TYPE
	SYNTAX      Integer32
	MAX-ACCESS  read-only
	STATUS      current
	DESCRIPTION
		"SCF board ID which this SNMP agent is currently running on."
	::= { scfInfo 1 }

scfAgentNumber OBJECT-TYPE
	SYNTAX      Integer32
	MAX-ACCESS  read-only
	STATUS      current
	DESCRIPTION
		"Total count of available SCF boards."
	::= { scfInfo 2 }

--
-- SCF Agent Table
--

scfAgentTable OBJECT-TYPE
	SYNTAX      SEQUENCE OF ScfAgentEntry
	MAX-ACCESS  not-accessible
	STATUS      current
	DESCRIPTION
		"Table of available SCF boards."
	::= { scfInfo 3 }

scfAgentEntry OBJECT-TYPE
	SYNTAX      ScfAgentEntry
	MAX-ACCESS  not-accessible
	STATUS      current
	DESCRIPTION
		"Available SCF board entry."
	INDEX   { scfAgentIndex }
	::= { scfAgentTable 1 }

ScfAgentEntry ::=
	SEQUENCE {
		scfAgentIndex ScfIndex,
		scfXcpVersion DisplayString
	}

scfAgentIndex OBJECT-TYPE
	SYNTAX      ScfIndex
	MAX-ACCESS  read-only
	STATUS      current
	DESCRIPTION
		"SCF board ID."
	::= { scfAgentEntry 1 }

scfXcpVersion OBJECT-TYPE
	SYNTAX      DisplayString
	MAX-ACCESS  read-only
	STATUS      current
	DESCRIPTION
		"SCF board XSCF Control Package(XCP) version."
	::= { scfAgentEntry 2 }

--
-- SCF IP Address Information
--
scfIpAddressPortNumber OBJECT-TYPE
	SYNTAX      Integer32
	MAX-ACCESS  read-only
	STATUS      current
	DESCRIPTION
		"SNMP communication port for all SCF LAN IP Addresses."
	::= { scfInfo 4 }

scfIpAddressNumber OBJECT-TYPE
	SYNTAX      Integer32
	MAX-ACCESS  read-only
	STATUS      current
	DESCRIPTION
		"Total count of IP Address table entries."
	::= { scfInfo 5 }

--
-- SCF IP Address Table
--

scfIpAddressTable OBJECT-TYPE
	SYNTAX      SEQUENCE OF ScfIpAddressEntry
	MAX-ACCESS  not-accessible
	STATUS      current
	DESCRIPTION
		"Table of available management LAN IPs on SCF."
	::= { scfInfo 6 }

scfIpAddressEntry OBJECT-TYPE
	SYNTAX      ScfIpAddressEntry
	MAX-ACCESS  not-accessible
	STATUS      current
	DESCRIPTION
		"SCF LAN IP Address entry."
	INDEX   { scfIpAddressIndex }
	::= { scfIpAddressTable 1 }

ScfIpAddressEntry ::=
	SEQUENCE {
		scfIpAddressIndex ScfIndex,
		scfIpAddress IpAddress
	}

scfIpAddressIndex OBJECT-TYPE
	SYNTAX      ScfIndex
	MAX-ACCESS  read-only
	STATUS      current
	DESCRIPTION
		"SCF IP Address table entry index."
	::= { scfIpAddressEntry 1 }

scfIpAddress OBJECT-TYPE
	SYNTAX      IpAddress
	MAX-ACCESS  read-only
	STATUS      current
	DESCRIPTION
		"IP Address of a management LAN port."
	::= { scfIpAddressEntry 2 }

--
-- SCF State Information
--
--	This group provides overall status information known to the SCF.
--

scfSystemState OBJECT-TYPE
	SYNTAX      ScfErrorStatus
	MAX-ACCESS  read-only
	STATUS      current
	DESCRIPTION
		"Overall system state."
	::= { scfState 1 }

scfFirmwareState OBJECT-TYPE
	SYNTAX       ScfErrorStatus
	MAX-ACCESS   read-only
	STATUS       current
	DESCRIPTION
		"SCF software state."
	::= { scfState 2 }

scfHardwareState OBJECT-TYPE
	SYNTAX      ScfErrorStatus
	MAX-ACCESS  read-only
	STATUS      current
	DESCRIPTION
		"Overall system hardware state."
	::= { scfState 3 }

scfModeSwitch OBJECT-TYPE
	SYNTAX      ScfModeSwitchState
	MAX-ACCESS  read-only
	STATUS      current
	DESCRIPTION
		"Operator panel switch state."
	::= { scfState 4 }

--
-- SCF Monitor Information
--
--	This group provides environmental information for a variety of
--   components within the system.
--

scfMonitorNumber OBJECT-TYPE
	SYNTAX      Integer32
	MAX-ACCESS  read-only
	STATUS      current
	DESCRIPTION
		"Total count of environmental monitor table entries."
	::= { scfMonitorInfo 1 }

--
-- SCF Monitor Table
--

scfMonitorTable OBJECT-TYPE
	SYNTAX      SEQUENCE OF ScfMonitorEntry
	MAX-ACCESS  not-accessible
	STATUS      current
	DESCRIPTION
		"Monitor table.  Contains environmental monitor
		information for system components."
	::= { scfMonitorInfo 2 }

scfMonitorEntry OBJECT-TYPE
	SYNTAX      ScfMonitorEntry
	MAX-ACCESS  not-accessible
	STATUS      current
	DESCRIPTION
		"Environmental monitor table entry."
	INDEX   {
		scfMonitorBoardType,
		scfMonitorBoardId,
		scfMonitorModuleType,
		scfMonitorModuleId,
		scfMonitorType,
		scfMonitorId
	}
	::= { scfMonitorTable 1 }

ScfMonitorEntry ::=
	SEQUENCE {
		scfMonitorBoardType ScfComponentType,
		scfMonitorBoardId ScfIndex,
		scfMonitorModuleType ScfComponentType,
		scfMonitorModuleId ScfIndex,
		scfMonitorType ScfMonitorTypeTC,
		scfMonitorId ScfIndex,
		scfMonitorDescription DisplayString,
		scfMonitorAdditionalInfo DisplayString,
		scfMonitorUnits DisplayString,
		scfMonitorStatus ScfValidStatus,
		scfMonitorValue Integer32,
		scfMonitorValueStatus ScfErrorStatus
	}

scfMonitorBoardType OBJECT-TYPE
	SYNTAX      ScfComponentType
	MAX-ACCESS  read-only
	STATUS      current
	DESCRIPTION
		"Monitor component board type."
	::= { scfMonitorEntry 1 }
	
scfMonitorBoardId OBJECT-TYPE
	SYNTAX      ScfIndex
	MAX-ACCESS  read-only
	STATUS      current
	DESCRIPTION
		"Monitor component board ID."
	::= { scfMonitorEntry 2 }

scfMonitorModuleType OBJECT-TYPE
	SYNTAX      ScfComponentType
	MAX-ACCESS  read-only
	STATUS	    current
	DESCRIPTION
		"Monitor component module type."
	::= { scfMonitorEntry 3 }
   
scfMonitorModuleId OBJECT-TYPE
	SYNTAX	    ScfIndex
	MAX-ACCESS  read-only
	STATUS	    current
	DESCRIPTION
		"Monitor component module ID."
	::= { scfMonitorEntry 4 }

scfMonitorType OBJECT-TYPE
	SYNTAX      ScfMonitorTypeTC
	MAX-ACCESS  read-only
	STATUS      current
	DESCRIPTION
		"Monitor component type."
	::= { scfMonitorEntry 5 }

scfMonitorId OBJECT-TYPE
	SYNTAX      ScfIndex
	MAX-ACCESS  read-only
	STATUS      current
	DESCRIPTION
		"Monitor ID."
	::= { scfMonitorEntry 6 }
		
scfMonitorDescription OBJECT-TYPE
	SYNTAX      DisplayString
	MAX-ACCESS  read-only
	STATUS      current
	DESCRIPTION
		"Text description of a monitor component."
	::= { scfMonitorEntry 7 }
	
scfMonitorAdditionalInfo OBJECT-TYPE
	SYNTAX      DisplayString
	MAX-ACCESS  read-only
	STATUS      current
	DESCRIPTION
		"Additional monitor info."
	::= { scfMonitorEntry 8 }
	
scfMonitorUnits OBJECT-TYPE
	SYNTAX      DisplayString
	MAX-ACCESS  read-only
	STATUS      current
	DESCRIPTION
		"Units of value."
	::= { scfMonitorEntry 9 }
	
scfMonitorStatus OBJECT-TYPE
	SYNTAX      ScfValidStatus
	MAX-ACCESS  read-only
	STATUS      current
	DESCRIPTION
		"Status of monitor component.  Reflects if the sensor
		could be accessed or should not be used based on
		the containing components state."
	::= { scfMonitorEntry 10 }
	
