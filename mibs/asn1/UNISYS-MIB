        u-fddiMACShortGrpAddr OBJECT-TYPE
           SYNTAX  OCTET STRING (SIZE (2))
           ACCESS  read-write
           STATUS  mandatory
           ::= { u-fddiMACAddressGrp 2 }


        u-fddiMACCfgGrp OBJECT-TYPE
           SYNTAX  SEQUENCE {
		u-fddiMACUpstreamNeighbor
			OCTET STRING,
		u-fddiMACDownstreamNeighbor
			OCTET STRING,
		u-fddiMACResourceIndex
			INTEGER,
		u-fddiMACConnectedResId
			INTEGER
	   }
           ACCESS  read-only
           STATUS  mandatory
           ::= { u-fddiMACEntry 2 }


        u-fddiMACUpstreamNeighbor OBJECT-TYPE
           SYNTAX  OCTET STRING
           ACCESS  read-only
           STATUS  mandatory
           ::= { u-fddiMACCfgGrp 1 }


        u-fddiMACDownstreamNeighbor OBJECT-TYPE
           SYNTAX  OCTET STRING
           ACCESS  read-only
           STATUS  mandatory
           ::= { u-fddiMACCfgGrp 2 }


        u-fddiMACResourceIndex OBJECT-TYPE
           SYNTAX  INTEGER
           ACCESS  read-only
           STATUS  mandatory
           ::= { u-fddiMACCfgGrp 3 }


        u-fddiMACConnectedResId OBJECT-TYPE
           SYNTAX  INTEGER
           ACCESS  read-only
           STATUS  mandatory
           ::= { u-fddiMACCfgGrp 4 }


        u-fddiMACOperGrp OBJECT-TYPE
           SYNTAX  SEQUENCE {
		u-fddiMACTReq
			INTEGER,
		u-fddiMACTMax
			INTEGER,
		u-fddiMACTvxValue
			INTEGER,
		u-fddiMACTMin
			INTEGER
	   }
           ACCESS  read-write
           STATUS  mandatory
           ::= { u-fddiMACEntry 3 }


        u-fddiMACTReq OBJECT-TYPE
           SYNTAX  INTEGER (u-fddiMACTMin..u-fddiMACTMax)
           ACCESS  read-write
           STATUS  mandatory
           ::= { u-fddiMACOperGrp 1 }


        u-fddiMACTMax OBJECT-TYPE
           SYNTAX  INTEGER (u-fddiMACTMin..540)
           ACCESS  read-write
           STATUS  mandatory
           ::= { u-fddiMACOperGrp 2 }


        u-fddiMACTvxValue OBJECT-TYPE
           SYNTAX  INTEGER (1..5000)
           ACCESS  read-write
           STATUS  mandatory
           ::= { u-fddiMACOperGrp 3 }


        u-fddiMACTMin OBJECT-TYPE
           SYNTAX  INTEGER (4000..u-fddiMACTMax)
           ACCESS  read-write
           STATUS  mandatory
           ::= { u-fddiMACOperGrp 4 }


        u-fddiMACCountersGrp OBJECT-TYPE
           SYNTAX  SEQUENCE {
		u-fddiMACReceiveCt
			Counter,
		u-fddiMACTransmitCt
			Counter,
		u-fddiMACTokenCt
			Counter
	   }
           ACCESS  read-only
           STATUS  mandatory
           ::= { u-fddiMACEntry 4 }


        u-fddiMACReceiveCt OBJECT-TYPE
           SYNTAX  Counter
           ACCESS  read-only
           STATUS  mandatory
           ::= { u-fddiMACCountersGrp 1 }


        u-fddiMACTransmitCt OBJECT-TYPE
           SYNTAX  Counter
           ACCESS  read-only
           STATUS  mandatory
           ::= { u-fddiMACCountersGrp 2 }


        u-fddiMACTokenCt OBJECT-TYPE
           SYNTAX  Counter
           ACCESS  read-only
           STATUS  mandatory
           ::= { u-fddiMACCountersGrp 3 }


        u-fddiMACErrorCntrsGrp OBJECT-TYPE
           SYNTAX  SEQUENCE {
		u-fddiMACTvxExpiredCt
			Counter,
		u-fddiMACNotCopiedCt
			Counter,
		u-fddiMACLateCt
			Counter,
		u-fddiMACReceeiveOverflowCt
			Counter
	   }
           ACCESS  read-only
           STATUS  mandatory
           ::= { u-fddiMACEntry 5 }


        u-fddiMACTvxExpiredCt OBJECT-TYPE
           SYNTAX  Counter
           ACCESS  read-only
           STATUS  mandatory
           ::= { u-fddiMACErrorCntrsGrp 1 }


        u-fddiMACNotCopiedCt OBJECT-TYPE
           SYNTAX  Counter
           ACCESS  read-only
           STATUS  mandatory
           ::= { u-fddiMACErrorCntrsGrp 2 }


        u-fddiMACLateCt OBJECT-TYPE
           SYNTAX  Counter
           ACCESS  read-only
           STATUS  mandatory
           ::= { u-fddiMACErrorCntrsGrp 3 }


        u-fddiMACReceiveOverflowCt OBJECT-TYPE
           SYNTAX  Counter
           ACCESS  read-only
           STATUS  mandatory
           ::= { u-fddiMACErrorCntrsGrp 4 }


        u-fddiMACSTATUSGrp OBJECT-TYPE
           SYNTAX  SEQUENCE {
		u-fddiMACRingOpCt
			Counter,
		u-fddiMACEnteredBeaconCt
			Counter
	   }
           ACCESS  read-only
           STATUS  mandatory
           ::= { u-fddiMACEntry 6 }


        u-fddiMACRingOpCt OBJECT-TYPE
           SYNTAX  Counter
           ACCESS  read-only
           STATUS  mandatory
           ::= { u-fddiMACSTATUSGrp 1 }


        u-fddiMACEnteredBeaconCt OBJECT-TYPE
           SYNTAX  Counter
           ACCESS  read-only
           STATUS  mandatory
           ::= { u-fddiMACSTATUSGrp 2 }

        u-fddiPHYCfgGrp OBJECT-TYPE
           SYNTAX  SEQUENCE {
		u-fddiPHYType
			INTEGER,
		u-fddiPHYConnectState
			INTEGER,
		u-fddiPHYRemotePHYType
			INTEGER,
		u-fddiPHYRemoteMACIndicated
			INTEGER --* BOOLEAN *--,
		u-fddiPHYResourceIndex
			INTEGER,
		u-fddiPHYConnectedResId
			INTEGER
	   }
           ACCESS  read-only
           STATUS  mandatory
           ::= { u-fddiPHYEntry 1 }


        u-fddiPHYType OBJECT-TYPE
           SYNTAX  INTEGER {
		a (1),
		b (2),
		m (3),
		s (4)
	   }
           ACCESS  read-only
           STATUS  mandatory
           ::= { u-fddiPHYCfgGrp 1 }


        u-fddiPHYConnectState OBJECT-TYPE
           SYNTAX  INTEGER {
		disabled (1),
		connecting (2),
		standby (3),
		active (4)
	   }
           ACCESS  read-only
           STATUS  mandatory
           ::= { u-fddiPHYCfgGrp 2 }


        u-fddiPHYRemotePHYType OBJECT-TYPE
           SYNTAX  INTEGER {
		a (1),
		b (2),
		m (3),
		s (4),
		unknown (5)
	   }
           ACCESS  read-only
           STATUS  mandatory
           ::= { u-fddiPHYCfgGrp 3 }


        u-fddiPHYRemoteMACIndicated OBJECT-TYPE
           SYNTAX  INTEGER --* BOOLEAN *-- 
           ACCESS  read-only
           STATUS  mandatory
           ::= { u-fddiPHYCfgGrp 4 }


        u-fddiPHYResourceIndex OBJECT-TYPE
           SYNTAX  INTEGER
           ACCESS  read-only
           STATUS  mandatory
           ::= { u-fddiPHYCfgGrp 5 }


        u-fddiPHYConnectedResId OBJECT-TYPE
           SYNTAX  INTEGER
           ACCESS  read-only
           STATUS  mandatory
           ::= { u-fddiPHYCfgGrp 6 }


        u-fddiPHYAction OBJECT-TYPE
           SYNTAX  INTEGER {
		enabled  (0),
		disabled (1)
	   }
           ACCESS  read-write
           STATUS  mandatory
           ::= { u-fddiPHYEntry 2 }

END



