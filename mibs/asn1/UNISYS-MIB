			INTEGER
	   }
           ACCESS  read-only
           STATUS  mandatory
           ::= { u-fddiSMT 3 }


        u-fddiSMTDasScmState OBJECT-TYPE
           SYNTAX  INTEGER {
                isolated (1),
                wrap-a (2),
                wrap-b (3),
                thru (4)
           }
           ACCESS  read-only
           STATUS  mandatory
           ::= { u-fddiSMTOperationalGrp 1 }

        u-fddiMACAddressGrp OBJECT-TYPE
           SYNTAX  SEQUENCE {
		u-fddiMACSMTLongAddr
			OCTET STRING,
