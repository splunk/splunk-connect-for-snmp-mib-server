        STATUS          current
        DESCRIPTION     "The total number of current PPTP tunnel sessions."
        ::= { stnSessionsPptp 3 }


    stnSessionPptpTunnelCurrentActiveCount OBJECT-TYPE
        SYNTAX          Unsigned32
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION     "The total number of current active PPTP 
                         tunnel sessions."
        ::= { stnSessionsPptp 4 }


    --
