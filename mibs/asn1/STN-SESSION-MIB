    stnSessionTelnetCurrentActiveCount OBJECT-TYPE
        SYNTAX          Unsigned32
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION     "The total number of current active 
                         Telnet sessions."
        ::= { stnSessionsTelnet 4 }


    --
    -- FTP session counters
    --

    stnSessionFtpTotalCount OBJECT-TYPE
