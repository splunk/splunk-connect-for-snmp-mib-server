    stnSessionIkeCurrentActiveCount OBJECT-TYPE
        SYNTAX          Unsigned32
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION     "The total number of current active 
                         IKE sessions."
        ::= { stnSessionsIke 4 }


    --
    -- Telnet session counters
    --

    stnSessionTelnetTotalCount OBJECT-TYPE
        SYNTAX          Unsigned32
        MAX-ACCESS      read-only
