			::= { zhoneConsole 3 }
		
		consoleLastLogoutTime OBJECT-TYPE
			SYNTAX TimeStamp
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"The sysuptime at which the last user logged out. A value
				of 0 is returned if a logout has never occured OR if
				a user is currently logged in.
				"
			::= { zhoneConsole 4 }
		
