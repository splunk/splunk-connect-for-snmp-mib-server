	INDEX		{DmiComponentIndex, a28SymbiosControllerIndex}
	::= {tSymbiosController 1}

a28SymbiosControllerIndex OBJECT-TYPE
	SYNTAX		DmiInteger
	ACCESS		read-only
	STATUS		mandatory
	DESCRIPTION	"An index into the Symbios Vendor Unique Controller table.
 This index corresponds to the Storage Controller index."
	::= {eSymbiosController 1}

a28LastRunDateForDv OBJECT-TYPE
	SYNTAX		DmiDateX
	ACCESS		read-only
	STATUS		mandatory
	DESCRIPTION	"This indicates the date and time of the last time Domain
Validation was completed on this controller."
	::= {eSymbiosController 2}

a28IoFactorForDvErrorRate OBJECT-TYPE
	SYNTAX		DmiInteger
	ACCESS		read-write
	STATUS		mandatory
	DESCRIPTION	"This indicates the I/O factor to use for calculating the
Domain Validation Error Rate."
	::= {eSymbiosController 3}

a28TimeFactorForDvErrorRate OBJECT-TYPE
	SYNTAX		DmiInteger
	ACCESS		read-write
	STATUS		mandatory
	DESCRIPTION	"This indicates the time factor to use for calculating the
Domain Validation Error Rate."
	::= {eSymbiosController 4}

a28ErrorCountFactorForDvErrorRate OBJECT-TYPE
	SYNTAX		DmiInteger
	ACCESS		read-write
	STATUS		mandatory
	DESCRIPTION	"This indicates the error count factor to use for calculating
the Domain Validation Error Rate."
	::= {eSymbiosController 5}

a28DvStatus OBJECT-TYPE
	SYNTAX		INTEGER {
		vNotApplicable                          	(0),
		vDomainValidationCompletedByTheDriver   	(1),
		vDomainValidationCompletedWithNoChanges 	(2),
		vDomainValidationCompletedWithChanges   	(3),
		vDomainValidationCurrentlyRunning       	(4),
		vDomainValidationTerminated             	(5),
		vDomainValidationNeedsToBeRun           	(6)
	}
	ACCESS		read-write
	STATUS		mandatory
	DESCRIPTION	"This indicates the status of Domain Validation for this controll
r.  Writing a value of 4 to this attribute will cause Domain Validation to
be run."
	::= {eSymbiosController 6}

SMiftomib ::= SEQUENCE {
	a99MibName                              	DmiDisplaystring,
	a99MibOid                               	DmiDisplaystring,
	a99DisableTrap                          	DmiInteger
}

tMiftomib OBJECT-TYPE
	SYNTAX		SEQUENCE OF SMiftomib
	ACCESS		not-accessible
	STATUS		mandatory
	DESCRIPTION	"This group defines attributes required for DMI to SNMP translati
n."
	::= {dmtfGroups 99}

eMiftomib OBJECT-TYPE
	SYNTAX		SMiftomib
	ACCESS		not-accessible
	STATUS		mandatory
	DESCRIPTION	""
	INDEX		{DmiComponentIndex}
	::= {tMiftomib 1}

a99MibName OBJECT-TYPE
	SYNTAX		DmiDisplaystring
	ACCESS		read-only
	STATUS		mandatory
	DESCRIPTION	"The MIB name that defines this MIF"
	::= {eMiftomib 1}

a99MibOid OBJECT-TYPE
	SYNTAX		DmiDisplaystring
	ACCESS		read-only
	STATUS		mandatory
	DESCRIPTION	"The MIB OID that corresponds to this MIF"
	::= {eMiftomib 2}

a99DisableTrap OBJECT-TYPE
	SYNTAX		DmiInteger
	ACCESS		read-write
	STATUS		mandatory
	DESCRIPTION	"This attribute can be changed to disable sending oftraps
from this component"
	::= {eMiftomib 3}


