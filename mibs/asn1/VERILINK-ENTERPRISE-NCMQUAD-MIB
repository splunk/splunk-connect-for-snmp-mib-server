	-- the NCM Advanced T1 Port Configuration Table

	ncmadvancedT1ConfigTable OBJECT-TYPE
	SYNTAX  SEQUENCE OF NcmAdvancedT1ConfigEntry
	ACCESS  not-accessible
	STATUS  mandatory
	DESCRIPTION
	"The ncmadvanced T1 Configuration table."
        --DEFAULT test-function    quad_test
        --DEFAULT next-function    quad_next
	 ::= { ncm-quad 11004 }


	ncmadvancedT1ConfigEntry OBJECT-TYPE
	SYNTAX  NcmAdvancedT1ConfigEntry
	ACCESS  not-accessible
	STATUS  mandatory
	DESCRIPTION
	"An entry in the ncmadvanced T1 Configuration table."
	 INDEX   { ncmadvancedT1LineIndex }
	 ::= { ncmadvancedT1ConfigTable 1 }


	NcmAdvancedT1ConfigEntry ::=
	 SEQUENCE {
	     ncmadvancedT1LineIndex
		 INTEGER,
	     ncmportIdentifier
		 INTEGER,
	     ncmfdlMode
		 INTEGER,
	     ncmfdlStandard
		INTEGER,
	     ncmfdlPerformanceReport
		INTEGER,
             ncmfdlLBEnableDisable
                INTEGER,
             ncmfdlLLBT1BOPMsg
                INTEGER,
             ncmfdlPLBT1BOPMsg
                INTEGER,
	     ncmfdlIdlePattern
		INTEGER,
	     ncmfdlMonitoringCsuType
		INTEGER
		}



	ncmadvancedT1LineIndex OBJECT-TYPE
	SYNTAX  INTEGER (1..'7fffffff'h)
	ACCESS  read-only
	STATUS  mandatory
	DESCRIPTION
	"The index value which uniquely identifies  the
	 interface  to which this entry is applicable."

	::= { ncmadvancedT1ConfigEntry 1 }


	ncmportIdentifier OBJECT-TYPE
	SYNTAX INTEGER {

		       port-1(0),
		       port-2(1),
		       port-3(2),
		       port-4(3)

	       }
	  ACCESS read-write
	  STATUS mandatory
	  DESCRIPTION
	  "The variable specifies the port identifier and port_3 and
	  port_4 are for Quad T1 only."

	::= { ncmadvancedT1ConfigEntry 2 }


	ncmfdlMode OBJECT-TYPE
	SYNTAX INTEGER {

		       terminated(0),
		       pass(1)

	       }
	  ACCESS read-write
	  STATUS mandatory
	  DESCRIPTION
	  "The variable specifies the ncmfdl mode which is bit 0 of  ncmfdl
	  Parameter1."

	::= { ncmadvancedT1ConfigEntry 3 }

	ncmfdlStandard OBJECT-TYPE
	SYNTAX INTEGER {

		       reserved(0),
		       type-54016(4),
		       type-T1-403(8),
		       type-both(12)

	       }
	  ACCESS read-write
	  STATUS mandatory
	  DESCRIPTION
	  "The variable specifies the ncmfdl standard which is bits 1
	  and bit 2 of ncmfdl Parameter1."

	::= { ncmadvancedT1ConfigEntry 4 }

	ncmfdlPerformanceReport OBJECT-TYPE
	SYNTAX INTEGER {

		       disable(0),
		       user(32),
		       telco(64)


	       }
	  ACCESS read-write
	  STATUS mandatory
	  DESCRIPTION
	  "The variable specifies the ncmfdl standard which is bits 3
	  and bit 4 of ncmfdl Parameter1."

	::= { ncmadvancedT1ConfigEntry 5 }


          ncmfdlLBEnableDisable OBJECT-TYPE
          SYNTAX INTEGER {

                       disabled(0),
                       enabled(1)

	       }
	  ACCESS read-write
	  STATUS mandatory
	  DESCRIPTION
          "The variable specifies the Disable/Enable of the ncmfdl Loopback
           which is bit 0 of the ncmfdl_loop"

           ::= { ncmadvancedT1ConfigEntry 6 }


          ncmfdlLLBT1BOPMsg OBJECT-TYPE
          SYNTAX INTEGER {

                       disabled(0),
                       enabled(2)

	       }
	  ACCESS read-write
	  STATUS mandatory
	  DESCRIPTION
          "The variable specifies the Disable/Enable of the send/receive of
           the LLB T1.403 BOP Messages. It is the bit 1 of the ncmfdl_loop"

           ::= { ncmadvancedT1ConfigEntry 7 }


          ncmfdlPLBT1BOPMsg OBJECT-TYPE
          SYNTAX INTEGER {

                       disabled(0),
                       enabled(4)

	       }
	  ACCESS read-write
	  STATUS mandatory
	  DESCRIPTION
          "The variable specifies the Disable/Enable of the send/receive of
           the PLB T1.403 BOP Messages. It is the bit 2 of the ncmfdl_loop"

           ::= { ncmadvancedT1ConfigEntry 8 }


	ncmfdlIdlePattern OBJECT-TYPE
	SYNTAX INTEGER {

		       all-ones(0),
		       flags(1)

	       }
	  ACCESS read-write
	  STATUS mandatory
	  DESCRIPTION
	  "The variable specifies the ncmfdl idle pattern which is bit 0 of ncmfdl
	  Parameter2."

	::= { ncmadvancedT1ConfigEntry 9 }


	ncmfdlMonitoringCsuType OBJECT-TYPE
	SYNTAX INTEGER {

		       polling(0),
		       no-polling(4),
		       unsolicited-messages(8)


	       }
	  ACCESS read-write
	  STATUS mandatory
	  DESCRIPTION
	  "The variable specifies the ncmfdl standard which is bits 1
	  and bit 2 of ncmfdl Parameter2."

	::= { ncmadvancedT1ConfigEntry 10 }


     -- the QUAD Sync Time  Table


	 ncmquadSyncTable OBJECT-TYPE
	     SYNTAX  SEQUENCE OF NcmCSUSyncEntry
	     ACCESS  not-accessible
	     STATUS  mandatory
                --DEFAULT test-function    quad_test
        	--DEFAULT next-function    sync_next
	    ::= { ncm-quad 11005 }


	 ncmquadSyncEntry OBJECT-TYPE
	     SYNTAX  NcmCSUSyncEntry
	     ACCESS  not-accessible
	     STATUS  mandatory
	     DESCRIPTION
		"An entry in the QUAD Sync Time table."
	    INDEX   { ncmquadLineIndex }
	    ::= { ncmquadSyncTable 1 }

     NcmCSUSyncEntry ::=
	 SEQUENCE {
	     ncmquadSyncIndex
		 INTEGER,
	     ncmquadSyncEndId
		 INTEGER,
	     ncmquadClkSyncSource
		 INTEGER,
	     ncmquadClkCardPrim
		 INTEGER,
	     ncmquadClkSyncPrimary
		 INTEGER,
	     ncmquadAutoRestorePrim
		 INTEGER,
	     ncmquadClkCardAlt1
		 INTEGER,
	     ncmquadClkSyncAlt1
		 INTEGER,
	     ncmquadAutoRestore1
		 INTEGER,
	     ncmquadClkCardAlt2
		 INTEGER,
	     ncmquadClkSyncAlt2
		 INTEGER,
	     ncmquadAutoRestore2
		 INTEGER

	}
	ncmquadSyncIndex OBJECT-TYPE
	     SYNTAX  INTEGER (1..'7fffffff'h)
	     ACCESS  read-only
	     STATUS  mandatory
	     DESCRIPTION
		"This object is the identifier of a  Inter-
		face on a managed device.  If there is an ifEn-
		try that is directly associated with  this  and
		only  this  DS1  interface,  it should have the
		same value as ifIndex.   Otherwise,  the  value
		exceeds  ifNumber,  and  is a unique identifier
		following this rule: inside  interfaces  (e.g.,
		equipment  side)  with even numbers and outside
		interfaces  (e.g.,  network   side)   with   odd
		numbers."
	    ::= { ncmquadSyncEntry 1 }


	 ncmquadSyncEndId  OBJECT-TYPE
	     SYNTAX  INTEGER {
			near-End(0),
			far-End(1)
		     }
	     ACCESS  read-only
	     STATUS  mandatory
	     DESCRIPTION
		"This value for this object indicates which
		 end of the equipment it is configuring."
	    ::= { ncmquadSyncEntry 2 }

	ncmquadClkSyncSource OBJECT-TYPE
	    SYNTAX INTEGER {
			primary(0),
			alt-1(1),
			alt-2(2)
		    }
	    ACCESS  read-only
	    STATUS  mandatory
	    DESCRIPTION
		"This variable indicates Current Clk Source Active i.e.,
		 primary, Alt_1 or Alt_2 source."
	    ::= { ncmquadSyncEntry 3 }

	ncmquadClkCardPrim OBJECT-TYPE
             SYNTAX  INTEGER {
			c-1(0),
			c-2(1),
			c-3(2),
			c-4(3),
			c-5(4),
			c-6(5),
			c-7(6),
			c-8(7),
			c-9(8),
			c-10(9),
			c-11(10),
			c-12(11),
			c-13(12)
		   }
             ACCESS  read-write
             STATUS  mandatory
             DESCRIPTION
		" This variable indicates the primary clock source from which
		  card of the that shelf. "
	    ::= { ncmquadSyncEntry 4 }

	ncmquadClkSyncPrimary OBJECT-TYPE
	    SYNTAX INTEGER {
			internal(0),
			external(1),
			masterNet1(2),
			masterNet2(3),
			masterNet3(4),
			masterNet4(5)
		    }
	    ACCESS  read-write
	    STATUS  mandatory
	    DESCRIPTION
		"This variable indicates the Clock source internal, external or
		network port. Setting Auto restore one can have auto restore 
		Capability."
	    ::= { ncmquadSyncEntry 5 }


	ncmquadAutoRestorePrim OBJECT-TYPE
	    SYNTAX INTEGER {
			disable(0),
			enable(1)
		    }
	    ACCESS  read-write
	    STATUS  mandatory
	    DESCRIPTION
		"This variable indicates the Clock source auto restore or not for 
		Internal, external or network clocks." 
	    ::= { ncmquadSyncEntry 6 }

        ncmquadClkCardAlt1 OBJECT-TYPE
             SYNTAX INTEGER { 
                        c-1(0),
                        c-2(1),
                        c-3(2),
                        c-4(3),
                        c-5(4),
                        c-6(5),
                        c-7(6),
                        c-8(7),
                        c-9(8),
                        c-10(9),
                        c-11(10),
                        c-12(11),
                        c-13(12)
		   }
             ACCESS  read-write
             STATUS  mandatory
             DESCRIPTION
		" This variable indicates the primary clock source from which
		  card of the that shelf. "
	    ::= { ncmquadSyncEntry 7 }

	ncmquadClkSyncAlt1 OBJECT-TYPE
	    SYNTAX INTEGER {
			internal(0),
			external(1),
			masterNet1(2),
			masterNet2(3),
			masterNet3(4),
			masterNet4(5)
		    }
	    ACCESS  read-write
	    STATUS  mandatory
	    DESCRIPTION
		"This variable indicates the Clock source internal, external or
		network port. Setting Auto restore one can have auto restore 
		Capability."
	    ::= { ncmquadSyncEntry 8 }


	ncmquadAutoRestore1 OBJECT-TYPE
	    SYNTAX INTEGER {
			disable(0),
			enable(1)
		    }
	    ACCESS  read-write
	    STATUS  mandatory
	    DESCRIPTION
		"This variable indicates the Clock source auto restore or not for 
		Internal, external or network clocks." 
	    ::= { ncmquadSyncEntry 9 }

	 ncmquadClkCardAlt2 OBJECT-TYPE
             SYNTAX INTEGER { 
                        c-1(0),
                        c-2(1),
                        c-3(2),
                        c-4(3),
                        c-5(4),
                        c-6(5),
                        c-7(6),
                        c-8(7),
                        c-9(8),
                        c-10(9),
                        c-11(10),
                        c-12(11),
                        c-13(12)
		   }
             ACCESS  read-write
             STATUS  mandatory
             DESCRIPTION
		" This variable indicates the primary clock source from which
		  card of the that shelf. "
	    ::= { ncmquadSyncEntry 10 }

	ncmquadClkSyncAlt2 OBJECT-TYPE
	    SYNTAX INTEGER {
			internal(0),
			external(1),
			masterNet1(2),
			masterNet2(3),
			masterNet3(4),
			masterNet4(5)
		    }
	    ACCESS  read-write
	    STATUS  mandatory
	    DESCRIPTION
		"This variable indicates the Clock source internal, external or
		network port. Setting Auto restore one can have auto restore 
		Capability."
	    ::= { ncmquadSyncEntry 11 }

	ncmquadAutoRestore2 OBJECT-TYPE
	    SYNTAX INTEGER {
			disable(0),
			enable(1)
		    }
	    ACCESS  read-write
	    STATUS  mandatory
	    DESCRIPTION
		"This variable indicates the Clock source auto restore or not for 
		Internal, external or network clocks." 
	    ::= { ncmquadSyncEntry 12 }

