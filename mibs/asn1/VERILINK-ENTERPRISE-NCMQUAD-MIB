     -- The Total Loss Table contains the cumulative sum of the
     -- various statistics for the 24 hour period preceding the
     -- current interval.

     -- "The Loss Total table.  24 hour interval."
	 ncmquadLossTotalTable OBJECT-TYPE
	     SYNTAX  SEQUENCE OF NcmCsuLossTotalEntry
	     ACCESS  not-accessible
	     STATUS  mandatory
        --DEFAULT test-function    quad_test
        --DEFAULT next-function    quad_next
	    ::= { ncm-quad 11003 }


	 ncmquadLTotalEntry OBJECT-TYPE
	     SYNTAX  NcmCsuLossTotalEntry
	     ACCESS  not-accessible
	     STATUS  mandatory
	     DESCRIPTION
		"An entry in the DS1 Total Loss table."
	    INDEX   { ncmquadLTotalIndex }
	    ::= { ncmquadLossTotalTable 1 }

     NcmCsuLossTotalEntry ::=
	 SEQUENCE {
	     ncmquadLTotalIndex
		 INTEGER,
	     ncmquadTotalEndId
		 INTEGER,
	     ncmquadLTotalASs
		 Gauge,
	     ncmquadLTotalLOSs
		 Gauge,
	     ncmquadLTotalAISs
		 Gauge,
	     ncmquadLTotalLOFs
		 Gauge,
	     ncmquadLTotalOFs
		 Gauge,
	     ncmquadLTotalESs
		 Gauge,
	     ncmquadLTotalSESs
		 Gauge,
	     ncmquadLTotalSEFs
		 Gauge,
	     ncmquadLTotalUASs
		 Gauge,
	     ncmquadLTotalBESs
		 Gauge,
             ncmquadLTotalESAs
		 Gauge,
	     ncmquadLTotalSASs
		 Gauge,
	     ncmquadLTotalCSSs
		 Gauge,
	     ncmquadLTotalLOFCs
		 Gauge

             
	 }

	 ncmquadLTotalIndex OBJECT-TYPE
	     SYNTAX  INTEGER (1..'7fffffff'h)
	     ACCESS  read-only
	     STATUS  mandatory
	     DESCRIPTION
		"The index value which uniquely identifies  the
		DS1  interface  to which this entry is applica-
		ble.  The interface identified by a  particular
		value  of  this  index is the same interface as
		identified by the same value as a ncmquadLineIndex
		object instance."
	    ::= { ncmquadLTotalEntry 1 }

	 ncmquadTotalEndId  OBJECT-TYPE
	     SYNTAX  INTEGER {
			near-End(0),
			far-End(1)
		     }
	     ACCESS  read-only
	     STATUS  mandatory
	     DESCRIPTION
		"This value for this object indicates which
		 end of the equipment it is accessing."
	    ::= { ncmquadLTotalEntry 2 }

	 ncmquadLTotalASs OBJECT-TYPE
	     SYNTAX  Gauge
	     ACCESS  read-only
	     STATUS  mandatory
	     DESCRIPTION
		"The number of Available Seconds encountered by a
		DS1 interface in the previous 24 hour interval.
		 Remark:  Available Seconds does not support for QUAD 2164."
	    ::= { ncmquadLTotalEntry 3 }


	 ncmquadLTotalLOSs OBJECT-TYPE
	     SYNTAX  Gauge
	     ACCESS  read-only
	     STATUS  mandatory
	     DESCRIPTION
		"The number of Loss Of Signal Seconds encoun-
		tered  by  a  DS1  interface in the previous 24
		hour interval."
	    ::= { ncmquadLTotalEntry 4 }


	 ncmquadLTotalAISs OBJECT-TYPE
	     SYNTAX  Gauge
	     ACCESS  read-only
	     STATUS  mandatory
	     DESCRIPTION
		"The number of Alarm Indication Signal Seconds
		encountered  by a DS1 interface in the previous
		24 hour interval."
	    ::= { ncmquadLTotalEntry 5 }


	 ncmquadLTotalLOFs OBJECT-TYPE
