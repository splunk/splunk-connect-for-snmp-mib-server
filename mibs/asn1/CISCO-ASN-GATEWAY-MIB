    SYNTAX          CagwPathEntry
    MAX-ACCESS      not-accessible
    STATUS          obsolete
    DESCRIPTION
        "Path Information."
    INDEX           {
                        cagwInstanceIndex,
                        cagwPathRemoteIpType,
                        cagwPathRemoteIp,
                        cagwPathType
                    } 
    ::= { cagwPathTable 1 }

