                ::= { cwacsEntry 30 }

cwacsEgrRcvEFCI1 OBJECT-TYPE
        SYNTAX     Counter32
        UNITS      "cells"
        MAX-ACCESS read-only
        STATUS     current
        DESCRIPTION
