			OBJECTS { sheL2cpIfIndex, sheL2cpOperState, sheL2cpTimer1, sheL2cpTimer2, sheL2cpTimer3, 
				sheL2cpControllerIpAddress, sheL2cpAccessNodeIdentifier, sheL2cpAlarmSeverityIndex, sheL2cpPortPartitionId, sheL2cpAdjacencyState, 
				sheL2cpPartitionId }
			STATUS current
			DESCRIPTION 
				"A collection of objects providing information about
				a VDSL Line."
			::= { sheL2cpGroups 1 }

		
		sheL2cpControlGroup OBJECT-GROUP
			OBJECTS { sheL2cpControlRequest, sheL2cpControlStatus, sheL2cpControlIndex, sheL2cpControlReqResult, sheL2cpControlTimer, 
				sheL2cpControlTimeStamp, sheL2cpControlControllerIpAddress, sheL2cpControlTimer1, sheL2cpControlTimer2, sheL2cpControlTimer3, 
				sheL2cpControlAlarmSeverityIndex, sheL2cpControlPartitionId, sheL2cpControlAccessNodeIdentifier }
			STATUS current
			DESCRIPTION 
				"Description."
			::= { sheL2cpGroups 2 }

		
		sheL2cpNotificationGroup NOTIFICATION-GROUP
			NOTIFICATIONS { sheL2cpLinkDown, sheL2cpIfModified, sheL2cpPartitionPortDataChanged, sheL2cpIfCreated, sheL2cpIfDeleted
				 }
			STATUS current
			DESCRIPTION 
				"This group supports notifications of significant 
				conditions associated with VDSL Lines."
			::= { sheL2cpGroups 3 }

		
		sheL2cpObsoleteGroup OBJECT-GROUP
			OBJECTS { sheL2cpHostIfIndex, sheL2cpHostIfVlanId, sheL2cpHostIfIpAddress, sheL2cpHostIfNetmask, sheL2cpHostIfControlRequest, 
				sheL2cpHostIfControlIndex, sheL2cpHostIfControlStatus, sheL2cpHostIfControlReqResult, sheL2cpHostIfControlTimer, sheL2cpHostIfControlTimeStamp, 
				sheL2cpHostIfControlVlanId, sheL2cpHostIfControlIpAddress, sheL2cpHostIfControlNetmask }
			STATUS obsolete
			DESCRIPTION 
				"Description."
			::= { sheL2cpGroups 4 }

		
		sheL2cpObsoleteNotificationGroup NOTIFICATION-GROUP
			NOTIFICATIONS { sheL2cpHostIfCreated, sheL2cpHostIfModified, sheL2cpHostIfDeleted }
			STATUS obsolete
			DESCRIPTION 
				"Description."
			::= { sheL2cpGroups 5 }

		
		sheL2cpCompliances OBJECT IDENTIFIER ::= { sheL2cpConformance 2 }

		
--   this module
-- this module
-- this module
		sheL2cpMibCompliance MODULE-COMPLIANCE
			STATUS current
			DESCRIPTION 
				"The compliance statement for SNMP entities which 
				manage VDSL interfaces in SHE."
			MODULE -- this module
				MANDATORY-GROUPS { sheL2cpGroup, sheL2cpControlGroup, sheL2cpNotificationGroup }
			::= { sheL2cpCompliances 1 }

		
	
	END

