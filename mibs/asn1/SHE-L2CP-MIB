		SheL2cpOperStateType ::= INTEGER
			{
			up(1),
			down(2)
			}

--   The states of the adjacency protocol.
		SheL2cpAdjacencyStateType ::= INTEGER
			{
			l2cpStateRESET(1),
			l2cpStateSYNSENT(2),
			l2cpStateSYNRCVD(3),
			l2cpStateESTAB(4)
			}

--  The ID of the partition managed by an L2CP protocol instance (see RFC 3292).
-- Or the partition to which a port is assigned.
-- 0...255: These are really partition IDs.
--     256: no partition (e.g. in case when DSLAM does not support partitions)
--     257: invalid partition (e.g. a port does not belong to any partition)
		SheL2cpPartitionIdType ::= INTEGER (0..255 | 256 | 257)

		SheL2cpControlRequestType ::= INTEGER
			{
			createL2cpIf(1),
			deleteL2cpIf(2),
			modifyL2cpIf(3),
			createL2cpHostIf(6),
			modifyL2cpHostIf(7),
			deleteL2cpHostIf(8),
			assignPortToPartition(10)
			}

		SheL2cpControlRequestResultType ::= INTEGER
			{
			commExecStarted(30),
			paramFaulty(31),
			requestBusy(32),
			setDataFailed(34),
			aswTimeout(35),
			commandFailed(36),
			commandSucceeded(37),
			wrongL2cpIfIndex(40),
			tooManyL2cpInterfaces(41),
			wrongL2cpPortPartitionIndex(50),
			wrongL2cpHostIfIndex(60),
			tooManyL2cpHostInterfaces(61)
			}

	
	
