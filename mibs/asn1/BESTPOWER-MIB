        SYNTAX INTEGER
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
                "The utility line current in 1/10 Ampere AC."
        ::= { upsInput 4 }

upsInputPower OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
                "The utility line power in 1/10 Watts."
        ::= { upsInput 5 }

