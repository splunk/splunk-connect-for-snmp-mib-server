	VARIATION	frDlcmiRowStatus
	    ACCESS	read-only
	    DESCRIPTION	"Row creation is not required for the frDlcmiTable."

	VARIATION	frCircuitState
	    ACCESS	read-only
	    DESCRIPTION	"Row creation is not required for the frCircuitTable."

	VARIATION	frCircuitCommittedBurst
	    ACCESS	read-only
	    DESCRIPTION	"Row creation is not required for the frCircuitTable."

	VARIATION	frCircuitExcessBurst
	    ACCESS	read-only
	    DESCRIPTION	"Row creation is not required for the frCircuitTable."

	VARIATION	frCircuitThroughput
	    ACCESS	read-only
	    DESCRIPTION	"Row creation is not required for the frCircuitTable."

	VARIATION	frCircuitMulticast
	    ACCESS	read-only
	    DESCRIPTION	"Row creation is not required for the frCircuitTable."

	VARIATION	frCircuitLogicalIfIndex
	    ACCESS	read-only
	    DESCRIPTION	"Row creation is not required for the frCircuitTable."

	VARIATION	frCircuitRowStatus
	    ACCESS	read-only
	    DESCRIPTION	"Row creation is not required for the frCircuitTable."


    SUPPORTS		ISDN-MIB -- draft-ietf-isdnmib-snmp-isdn-mib-04.txt
	INCLUDES	{ isdnMibBasicRateGroup, isdnMibSignalingGroup,
			  isdnMibEndpointGroup }

	VARIATION	isdnBasicTblIfType
	    ACCESS	read-only
	    DESCRIPTION	"No write access via SNMP."

