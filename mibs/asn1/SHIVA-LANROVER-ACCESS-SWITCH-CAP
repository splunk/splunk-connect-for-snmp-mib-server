--
-- Conformance statements for Shiva LanRover Access Switch
--

-- These conformance statements note what IETF MIBs, defined in the
-- SNMPv2-SMI syntax are implemented, are implemented.
-- The agent itself is an SNMPv1 agent.

-- Note that the conformance statements may be a bit off in older
-- software releases that implement obsolete Internet Draft versions of
-- MIBs.  This is because we have to test compilation of this MIB against the
-- latest versions of the MIB, which may have different OBJECT-GROUPS
-- and NOTIFICATION-GROUPS, or may have obsoleted variables.


SHIVA-LANROVER-ACCESS-SWITCH-CAP DEFINITIONS ::= BEGIN

IMPORTS
    MODULE-IDENTITY
        FROM SNMPv2-SMI
    AGENT-CAPABILITIES
        FROM SNMPv2-CONF
    conformance
        FROM SHIVA-MIB;

shivaLanRoverAccessSwitchCapabilities MODULE-IDENTITY
    LAST-UPDATED	"0005012130Z"
    ORGANIZATION	"Intel Network Systems Inc"
    CONTACT-INFO	"Intel Network Systems Inc
			28 Crosby Drive
			Bedford, MA  01730"
    DESCRIPTION		"The Shiva LanRover Access Switch Capabilities module"
    REVISION		"9811061830Z"
    DESCRIPTION		"The initial revision of this module."
    REVISION		"9811111830Z"
    DESCRIPTION		"Correct DESCRIPTION in MODULE-IDENTITY."
    REVISION            "0005012130Z"
    DESCRIPTION         "Add release 6.1.1p1"
    ::= { conformance 4 }

shivaLanRoverAccessSwitchAgent457 AGENT-CAPABILITIES
    PRODUCT-RELEASE	"LanRover Access Switch 4.5.7"
    STATUS		current
    DESCRIPTION		"SNMP Agent in LanRover Access Switch 4.5.7 firmware.
			Describes only those MIBs which have
			SMIv2 MODULE-COMPLIANCE macro."

    SUPPORTS		IF-MIB		-- RFC 1573
	INCLUDES	{ ifGeneralGroup, ifPacketGroup,
			  ifRcvAddressGroup, ifStackGroup }

	VARIATION	ifAdminStatus
	    SYNTAX	INTEGER { up(1) }
	    ACCESS	read-only
	    DESCRIPTION	"No write access via SNMP, or any other means.
			No administrative method to disable an interface."

	VARIATION	ifOperStatus
	    SYNTAX	INTEGER { up(1), down(2), unknown(4), dormant(5) }
	    DESCRIPTION	"There is no testing state in this system."

	VARIATION	ifLinkUpDownTrapEnable
	    ACCESS	read-only
	    DESCRIPTION	"No write access via SNMP, or any other means."

	VARIATION	ifPromiscuousMode
	    ACCESS	read-only
	    DESCRIPTION	"No write access via SNMP, mode not available."

	VARIATION	ifStackStatus
	    ACCESS	read-only
	    DESCRIPTION	"No write access via SNMP."

	VARIATION	ifRcvAddressStatus
	    ACCESS	read-only
	    DESCRIPTION "No write access via SNMP, or any other
	    means."

    SUPPORTS		DNS-RESOLVER-MIB	-- RFC 1612
	INCLUDES	{ dnsResConfigGroup, dnsResCounterGroup }

        VARIATION	dnsResConfigMaxCnames
	    ACCESS	read-only
	    DESCRIPTION	"This object need not be writable."

        VARIATION	dnsResConfigSbeltName
	    ACCESS	read-only
	    DESCRIPTION	"This object need not be writable."

        VARIATION	dnsResConfigSbeltRecursion
	    ACCESS	read-only
	    DESCRIPTION	"This object need not be writable."

        VARIATION	dnsResConfigSbeltPref
	    ACCESS	read-only
	    DESCRIPTION	"This object need not be writable."

        VARIATION	dnsResConfigReset
	    ACCESS	read-only
	    DESCRIPTION	"This object need not be writable."


    -- Ethernet MIB supported only on LanRover Access Switch/E.

    SUPPORTS		EtherLike-MIB	-- RFC 1650
	INCLUDES	{ etherStatsGroup, etherCollisionTableGroup }


    -- RS-232-MIB supported FOR UART1, UART2, and all modems
    -- on the Digital Modem Card.

    SUPPORTS		RS-232-MIB	-- RFC 1659
	INCLUDES	{ rs232Group, rs232AsyncGroup }

	VARIATION	rs232PortType
	    SYNTAX	INTEGER { other(1), rs232(2) }
	    DESCRIPTION	"Internal modems are described as
			other.  Only other port type is RS-232."

	VARIATION	rs232PortInSpeed
	    ACCESS	read-only
	    DESCRIPTION	"No write access via SNMP."

	VARIATION	rs232PortOutSpeed
	    ACCESS	read-only
	    DESCRIPTION	"No write access via SNMP."

	VARIATION	rs232PortInFlowType
	    ACCESS	read-only
	    DESCRIPTION	"No write access via SNMP."

	VARIATION	rs232PortOutFlowType
	    ACCESS	read-only
	    DESCRIPTION	"No write access via SNMP."

	VARIATION	rs232AsyncPortBits
	    ACCESS	read-only
	    DESCRIPTION	"No write access via SNMP."

	VARIATION	rs232AsyncPortStopBits
	    ACCESS	read-only
	    DESCRIPTION	"No write access via SNMP."

	VARIATION	rs232AsyncPortParity
	    ACCESS	read-only
	    DESCRIPTION	"No write access via SNMP."

	VARIATION	rs232AsyncPortAutobaud
	    SYNTAX	INTEGER { disabled(2) }
	    ACCESS	read-only
	    DESCRIPTION	"No autobaud support.  No write access
			via SNMP, but no options to set."


    SUPPORTS		RIPv2-MIB	-- RFC 1724
	INCLUDES	{ rip2GlobalGroup, rip2IfStatGroup,
			  rip2IfConfGroup }

	VARIATION	rip2IfConfAuthType
	    ACCESS	read-only
	    DESCRIPTION	"No write access via SNMP."

	VARIATION	rip2IfConfAuthKey
	    ACCESS	read-only
	    DESCRIPTION	"No write access via SNMP."

	VARIATION	rip2IfConfSend
	    SYNTAX	INTEGER {
	                    doNotSend (1),
			    ripVersion1 (2),
			    rip1Compatible (3),
			    ripVersion2 (4)
			}
	    ACCESS	read-only
	    DESCRIPTION	"No write access via SNMP.  No Demand RIP."

	VARIATION	rip2IfConfReceive
	    ACCESS	read-only
	    DESCRIPTION	"No write access via SNMP."

	VARIATION	rip2IfConfDefaultMetric
	    ACCESS	read-only
	    DESCRIPTION	"No write access via SNMP."

	VARIATION	rip2IfConfStatus
	    SYNTAX	INTEGER { active(1) }
	    ACCESS	read-only
	    DESCRIPTION	"No write access via SNMP."

	VARIATION	rip2IfConfSrcAddress
	    ACCESS	read-only
	    DESCRIPTION	"No write access via SNMP."


    SUPPORTS		FRAME-RELAY-DTE-MIB	
			-- draft-ietf-iplpdn-frmib-dte-05.asn,
			-- which is a superset of RFC 1315 but in SMIv2.
	INCLUDES	{ frPortGroup, frCircuitGroup, frErrGroup, 
			  frTrapGroup, frNotificationGroup }

	VARIATION	frDlcmiState
	    ACCESS	read-only
	    DESCRIPTION	"Row creation is not required for the frDlcmiTable."

	VARIATION	frDlcmiAddress
	    ACCESS	read-only
	    DESCRIPTION	"Row creation is not required for the frDlcmiTable."

	VARIATION	frDlcmiAddressLen
	    ACCESS	read-only
	    DESCRIPTION	"Row creation is not required for the frDlcmiTable."

