	::= { usrGwcwanTraps 12 }
	usrGwcwanT1E1loopBack NOTIFICATION-TYPE
    OBJECTS {
	  usrGwcwanT1E1TrapEnaDS1Index
	}
    STATUS  current
	DESCRIPTION
	"This trap is generated for an active Near End loopback condition
