   mscMpanlDlciVcCalledNpi
      INTEGER,
   mscMpanlDlciVcCalledDna
      DigitString,
   mscMpanlDlciVcCalledLcn
      Unsigned32,
   mscMpanlDlciVcCallingNpi
      INTEGER,
   mscMpanlDlciVcCallingDna
      DigitString,
   mscMpanlDlciVcCallingLcn
      Unsigned32,
   mscMpanlDlciVcAccountingEnabled
      INTEGER,
   mscMpanlDlciVcFastSelectCall
      INTEGER,
   mscMpanlDlciVcPathReliability
      INTEGER,
   mscMpanlDlciVcAccountingEnd
      INTEGER,
   mscMpanlDlciVcPriority
      INTEGER,
   mscMpanlDlciVcSegmentSize
      Unsigned32,
   mscMpanlDlciVcMaxSubnetPktSize
      Unsigned32,
   mscMpanlDlciVcRcosToNetwork
      INTEGER,
   mscMpanlDlciVcRcosFromNetwork
      INTEGER,
   mscMpanlDlciVcEmissionPriorityToNetwork
      INTEGER,
   mscMpanlDlciVcEmissionPriorityFromNetwork
      INTEGER,
   mscMpanlDlciVcDataPath
      AsciiString }

