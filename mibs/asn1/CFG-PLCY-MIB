   ::= {  sitaraCfgPlcyNotifns  10 }


sitaraCfgPlcyNotifnsClassEntryUpdate  NOTIFICATION-TYPE
   OBJECTS     {  
           sitaraCfgPlcyCbqClassAlias,         
           sitaraCfgPlcyCbqClassPathName,         
           sitaraCfgPlcyCbqClassEnabled,          
           sitaraCfgPlcyCbqClassLanBandWidth,     
