        sonusIsdnsgSwitchProfileT308    Integer32,
        sonusIsdnsgSwitchProfileT309    Integer32,
        sonusIsdnsgSwitchProfileT310    Integer32,
        sonusIsdnsgSwitchProfileT312    Integer32,
        sonusIsdnsgSwitchProfileT313    Integer32,
        sonusIsdnsgSwitchProfileT314    Integer32,
        sonusIsdnsgSwitchProfileT318    Integer32,
        sonusIsdnsgSwitchProfileT319    Integer32,
        sonusIsdnsgSwitchProfileT322    Integer32,
        sonusIsdnsgSwitchProfileK       Integer32,
        sonusIsdnsgSwitchProfileT200    Integer32,
        sonusIsdnsgSwitchProfileT203    Integer32,
        sonusIsdnsgSwitchProfileN200    Integer32,
        sonusIsdnsgSwitchProfileN201    Integer32,
        sonusIsdnsgSwitchProfileAdminState  SonusAdminState,
        sonusIsdnsgSwitchProfileRowStatus RowStatus,
        sonusIsdnsgSwitchProfileBchannelInitialRestart              INTEGER
    }

sonusIsdnsgSwitchProfileListIndex OBJECT-TYPE
    SYNTAX          Integer32 (1..128)
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "The index of the ISDN Switch Profile entry."
    ::= { sonusIsdnsgSwitchProfileEntry 1 }

