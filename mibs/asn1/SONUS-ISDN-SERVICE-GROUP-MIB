    ::= { sonusIsdnsgProfileEntry 3 }

sonusIsdnsgProfileHuntAlgorithm OBJECT-TYPE
    SYNTAX          INTEGER { lowlow(1), highhigh(2), circularforward(3), 
                        circularbackward(4) }
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION     
            "The hunting method used to select channels for calls destined
             to the Service Group destination."
    DEFVAL          { lowlow }
    ::= { sonusIsdnsgProfileEntry 4 }

sonusIsdnsgProfileCost OBJECT-TYPE
    SYNTAX          Integer32 (0..100)
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION     
            "The routing cost to the ISDN Service Group."
    DEFVAL          { 50 }
    ::= { sonusIsdnsgProfileEntry 5 }

