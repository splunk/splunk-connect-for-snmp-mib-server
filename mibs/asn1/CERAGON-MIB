        SYNTAX  SEQUENCE OF GnGigabitEthernetCfgEntry
        ACCESS  not-accessible
        STATUS mandatory
        DESCRIPTION
        "This table defines the configuration of the Gigabit Ethernet port."
        ::= { gnGigabitEthernetCfg 1 }

gnGigabitEthernetCfgEntry OBJECT-TYPE
        SYNTAX GnGigabitEthernetCfgEntry
        ACCESS  not-accessible
        STATUS mandatory
        DESCRIPTION
        "This is an entry in the Gigabit Ethernet configuration table."
        INDEX {  ifIndex }
        ::= { gnGigabitEthernetCfgTable 1 }

GnGigabitEthernetCfgEntry ::=
            SEQUENCE {
        gnGigabitEthernetCfgPauseFrameGenerating
                INTEGER,
        gnGigabitEthernetCfgMuteOnExcError
             INTEGER,
        gnGigabitEthernetCfgMuteOnSd
             INTEGER,
        gnGigabitEthernetCfgMuteOnRemoteRadioFault
             INTEGER,
		gnGigabitEthernetCfgSpeedAndDuplex
             INTEGER,
        gnGigabitEthernetCfgSchedulerPriorityOption
             INTEGER,
        gnGigabitEthernetCfgSchedulerQueue1Weight
             INTEGER,
        gnGigabitEthernetCfgSchedulerQueue2Weight
             INTEGER,
        gnGigabitEthernetCfgSchedulerQueue3Weight
             INTEGER,
        gnGigabitEthernetCfgSchedulerQueue4Weight
             INTEGER,
        gnGigabitEthernetCfgClassifierFirstPrioUDP
             INTEGER,
        gnGigabitEthernetCfgClassifierPrioBitSource
             INTEGER,
        gnGigabitEthernetCfgClassifierGroupMinVlanId
             INTEGER,
        gnGigabitEthernetCfgClassifierGroupMaxVlanId
             INTEGER,
        gnGigabitEthernetCfgClassifierGroupVlanPriority
             INTEGER,
        gnGigabitEthernetCfgClassifierGroupVlanSet
             INTEGER,
        gnGigabitEthernetCfgAcmMuteOnMinConstellation
             INTEGER,
        gnGigabitEthernetCfgEnableAutomaticTxMute
             INTEGER,
		gnGigabitEthernetCfgTrafficBISTAdmin
             INTEGER,
        gnGigabitEthernetCfgClearStatistics
             INTEGER,
        gnGigabitEthernetCfgPauseFrameForwarding
             INTEGER,
        gnGigabitEthernetCfgPhyLoopback
             INTEGER
}

gnGigabitEthernetCfgPauseFrameGenerating OBJECT-TYPE
        SYNTAX  INTEGER { enable(2), disable(3) }
        ACCESS  read-write
        STATUS mandatory
        DESCRIPTION
        " This parameter defines the option to generate pause frames when the Tx FIFO reaches his
          threshold level (enable) or to (when disabled) stop generate pause frame."
        ::= { gnGigabitEthernetCfgEntry  1 }

gnGigabitEthernetCfgMuteOnExcError OBJECT-TYPE
        SYNTAX  INTEGER { enable(2), disable(3) }
        ACCESS  read-write
        STATUS mandatory
        DESCRIPTION
        " This parameter defines whether to mute GBE Tx on excessive BER."
        ::= { gnGigabitEthernetCfgEntry  2 }

gnGigabitEthernetCfgMuteOnSd OBJECT-TYPE
        SYNTAX  INTEGER { enable(2), disable(3) }
        ACCESS  read-write
        STATUS mandatory
        DESCRIPTION
        " This parameter defines whether to mute GBE Tx port on signal degrade."
        ::= { gnGigabitEthernetCfgEntry  3 }

gnGigabitEthernetCfgMuteOnRemoteRadioFault OBJECT-TYPE
        SYNTAX  INTEGER { enable(2), disable(3) }
        ACCESS  read-write
        STATUS mandatory
        DESCRIPTION
        " This parameter defines whether to mute GBE Tx port on exe error."
        ::= { gnGigabitEthernetCfgEntry  4 }

gnGigabitEthernetCfgSpeedAndDuplex OBJECT-TYPE
	SYNTAX  INTEGER
	{
		autoNegotiation(2),
	 	thousandBasedFullDuplex(3),
	 	thousandBasedHalfDuplex(4),
        hundredBasedFullDuplex(5),
        hundredBasedHalfDuplex(6),
        tenBasedFullDuplex(7),
        tenBasedHalfDuplex(8)
	}
	ACCESS  read-write
	STATUS mandatory
	DESCRIPTION
	" This parameter defines the speed and duplex of the gigabit port, this parameter is not relevant when using
	an electric SFP giga line interface."
	::= { gnGigabitEthernetCfgEntry  5 }

gnGigabitEthernetCfgSchedulerPriorityOption OBJECT-TYPE
        SYNTAX  INTEGER { strict(2), hrr(3) }
        ACCESS  read-write
        STATUS mandatory
        DESCRIPTION
                "This parameter defines the algorithm, which the Ethernet packets are taken from the 4 queues.
                 Strict - The Packets are taken according to strict order (from queue #1 till queue #4).
                 HRR - Hierarchal Round Robin - the Ethernet packets are taken according to the configure weight."
        ::= { gnGigabitEthernetCfgEntry  6 }

gnGigabitEthernetCfgSchedulerQueue1Weight OBJECT-TYPE
        SYNTAX  INTEGER { strict(17) }
        ACCESS  read-write
        STATUS mandatory
        DESCRIPTION
                "This parameter set the weight of the queue."
        ::= { gnGigabitEthernetCfgEntry  7 }

gnGigabitEthernetCfgSchedulerQueue2Weight OBJECT-TYPE
        SYNTAX  INTEGER ( 1..16 )
        ACCESS  read-write
        STATUS mandatory
        DESCRIPTION
                "This parameter set the weight of the queue."
        ::= { gnGigabitEthernetCfgEntry  8 }

gnGigabitEthernetCfgSchedulerQueue3Weight OBJECT-TYPE
        SYNTAX  INTEGER ( 1..16 )
        ACCESS  read-write
        STATUS mandatory
        DESCRIPTION
                "This parameter set the weight of the queue."
        ::= { gnGigabitEthernetCfgEntry  9 }

gnGigabitEthernetCfgSchedulerQueue4Weight OBJECT-TYPE
        SYNTAX  INTEGER ( 1..16 )
        ACCESS  read-write
        STATUS mandatory
        DESCRIPTION
                "This parameter set the weight of the queue."
        ::= { gnGigabitEthernetCfgEntry  10 }

gnGigabitEthernetCfgClassifierFirstPrioUDP OBJECT-TYPE
        SYNTAX  INTEGER { enable(2),disable(3) }
        ACCESS  read-write
        STATUS mandatory
        DESCRIPTION
                "This parameter set the administrative status for priority override to UDP packets."
        ::= { gnGigabitEthernetCfgEntry  11 }

