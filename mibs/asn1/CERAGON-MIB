        SYNTAX  INTEGER {
                enable(2),
                disable(3)
        }
        ACCESS  read-write
        STATUS mandatory
        DESCRIPTION
        " Enable Disable drawer wayside channel "
        DEFVAL { enable }
        ::= { gnMuxCfgXEntry 2 }

