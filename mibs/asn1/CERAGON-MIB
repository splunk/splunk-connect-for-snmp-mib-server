gnGenDrawerXSlot1Status OBJECT-TYPE
        SYNTAX  INTEGER {
                notExist(2),
                powerOff(3),
                powerOn(4),
				dbReady(5)
        }
        ACCESS  read-only
        STATUS mandatory
