        SYNTAX  INTEGER {
                connected(2),
                disconnected(3)
        }
        ACCESS  read-only
        STATUS mandatory
        DESCRIPTION
        "Is the connection between the local manager and
         the remote manager."
        ::= { gnMdmStatXEntry 3 }

