        SYNTAX  INTEGER  {
                low(2),
                high(3)
                }
        ACCESS  read-write
        STATUS mandatory
        DESCRIPTION
        " "
        DEFVAL { high }
        ::= { gnMdmCfgXEntry 2 }

gnMdmCfgXLinkId OBJECT-TYPE
        SYNTAX INTEGER (0..65535)
        ACCESS  read-write
        STATUS mandatory
                DESCRIPTION
                "0 for 'old' system that doesn't support the link ID.
                the user can enter values from 1 to 65535"
            DEFVAL        {0}
        ::= { gnMdmCfgXEntry 3 }

