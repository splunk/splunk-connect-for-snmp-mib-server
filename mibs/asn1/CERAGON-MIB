        SYNTAX  INTEGER
        {
                local(1),
                remote(2)
        }
        ACCESS  read-only
           STATUS mandatory
        DESCRIPTION
              "This value indicates device to which the external alarm
                is connected - 1 Local, 2 Remote"
        ::= { gnAgnInExternAlarmEntry 1 }

