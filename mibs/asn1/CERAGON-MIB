        SYNTAX  INTEGER (0..20000)
        ACCESS  read-write
        STATUS mandatory
                DESCRIPTION
                  "This is the hold-off time in milliseconds.
                 Possible values: 0..20000, 100 mSec steps. "
            DEFVAL        {0}
        ::= { gnTrailCfgEntry 11 }


