        "This is an entry in the modem configuration table."
        INDEX {  gnMdmCfgXId }
        ::= { gnMdmCfgXTable 1 }

GnMdmCfgXEntry ::=
            SEQUENCE {
        gnMdmCfgXId
                INTEGER,
        gnMdmCfgXLatencyType
                INTEGER,
        gnMdmCfgXLinkId
                INTEGER,
        gnMdmCfgXRadioSide
                INTEGER,
        gnMdmCfgXMrmcConf
                INTEGER,
        gnMdmCfgXIfLoopback
                INTEGER,
        gnMdmCfgXHwReset
                INTEGER,
        gnMdmCfgXPrbsTest
                INTEGER,
        gnMdmCfgXClearCounters
                INTEGER,
        gnMdmCfgXAcmOperationMode
                INTEGER,
        gnMdmCfgXAcmMaximumConstellation
                INTEGER
}

gnMdmCfgXId OBJECT-TYPE
        SYNTAX  INTEGER
        {
                modem1(3),
                modem2(4)
        }
    ACCESS  read-only
           STATUS mandatory
    DESCRIPTION
    "This value indicates which device is involved
        in the specific row."
    ::= { gnMdmCfgXEntry 1 }

