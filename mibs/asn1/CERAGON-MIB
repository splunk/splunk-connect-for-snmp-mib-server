        SYNTAX  DisplayString(SIZE(0..40))
        ACCESS  read-write
        STATUS mandatory
        DESCRIPTION
        "for NMS use"
        ::= { gnTrailPassThroughEntry 2 }

gnGigabitEthernetCfg OBJECT IDENTIFIER ::= { gnAccess 10 }
