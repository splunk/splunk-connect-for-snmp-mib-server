        SYNTAX  INTEGER (5..12)
        ACCESS  read-write
        STATUS mandatory
                DESCRIPTION
                  "This is the wait-to-restore time in minutes.
                 Possible values: 5..12. "
            DEFVAL        {5}
        ::= { gnTrailCfgEntry 13 }

gnTrailCfgSignalLabelMismatch OBJECT-TYPE
        SYNTAX  INTEGER{
                sendAlarm(2),
                sendAIS(3)
        }
        ACCESS  read-write
        STATUS mandatory
                DESCRIPTION
                  " In case of a V5 mismatch error, send or don't send AIS."
            DEFVAL        { sendAlarm }
