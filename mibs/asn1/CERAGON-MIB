        SYNTAX  INTEGER {
                        noOperation(2),
                        auxToIdc(3),
                        idcToAux(4)
        }
        ACCESS  read-write
        STATUS mandatory
        DESCRIPTION
        "This parameter synchronize between hardware and data base of auxiliary channel."
        DEFVAL { noOperation }
        ::= { gnAuxGeneralEntry 2 }

