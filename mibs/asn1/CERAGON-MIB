        SYNTAX  INTEGER {
                unknown(2),
                v2(3),  -- Phos ODU
                v1(4),  -- Fibe ODU
                v3(5),  -- Titan ODU
                v4(6),  -- Vanad RFU
                v5(7),  -- Ippon SP RFU
                v6(8),  -- Ippon HP RFU
