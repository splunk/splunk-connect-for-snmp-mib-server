gnGigabitEthernetStat OBJECT IDENTIFIER ::= { gnAccess 12 }
gnGigabitEthernetStatTable OBJECT-TYPE
        SYNTAX  SEQUENCE OF GnGigabitEthernetStatEntry
        ACCESS  not-accessible
        STATUS mandatory
        DESCRIPTION
        "The Gigabit Ethernet status table."
        ::= { gnGigabitEthernetStat 1 }

gnGigabitEthernetStatEntry OBJECT-TYPE
        SYNTAX GnGigabitEthernetStatEntry
        ACCESS  not-accessible
        STATUS mandatory
        DESCRIPTION
        "This is an entry in the Gigabit Ethernet status table."
        INDEX {  ifIndex }
        ::= { gnGigabitEthernetStatTable 1 }

GnGigabitEthernetStatEntry ::=
            SEQUENCE {
        gnGigabitEthernetStatBistErrorSeconds
           	INTEGER,
		gnGigabitEthernetStatSpeedAndDuplexSupport
			OCTET STRING,
        gnGigabitEthernetStatValidIntervals
           	INTEGER,
		gnGigabitEthernetStatSpeedAndDuplexMode
           	INTEGER
}

gnGigabitEthernetStatBistErrorSeconds OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS  read-only
        STATUS mandatory
        DESCRIPTION
        " Traffic BIST results (error seconds)"
        ::= { gnGigabitEthernetStatEntry 1 }

gnGigabitEthernetStatSpeedAndDuplexSupport OBJECT-TYPE
        SYNTAX OCTET STRING(SIZE(1))
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION
        "This field is used only by the element management application.
		 bit #7 - 1 auto negotiation is supported 0 otherwise (MSB).
		 bit #6 - 1 1000 full is supported 0 otherwise.
		 bit #5 - 1 1000 half is supported 0 otherwise.
		 bit #4 - 1 100 full is supported 0 otherwise.
		 bit #3 - 1 100 half is supported 0 otherwise.
		 bit #2 - 1 10 full is supported 0 otherwise.
		 bit #1 - 1 10 half is supported 0 otherwise.
		 bit #0 - not used (LSB)."
              ::= { gnGigabitEthernetStatEntry 2 }

gnGigabitEthernetStatValidIntervals OBJECT-TYPE
        SYNTAX INTEGER (0..96)
        ACCESS  read-only
        STATUS mandatory
        DESCRIPTION
        "The number of previous valid intervals for which valid data has been stored
         for the specific GBE interface."
        ::= { gnGigabitEthernetStatEntry 3 }

gnGigabitEthernetStatSpeedAndDuplexMode OBJECT-TYPE
	SYNTAX  INTEGER
	{
	 	thousandBasedFullDuplex(3),
	 	thousandBasedHalfDuplex(4),
        hundredBasedFullDuplex(5),
        hundredBasedHalfDuplex(6),
        tenBasedFullDuplex(7),
        tenBasedHalfDuplex(8)
	}
    ACCESS  read-only
    STATUS mandatory
    DESCRIPTION
    "Current speed and duplex mode of the giga port."
    ::= { gnGigabitEthernetStatEntry 4 }

gnGigabitEthernetMon OBJECT IDENTIFIER ::=  { gnAccess 13 }
gnGigabitEthernetMonCurrTable OBJECT-TYPE
        SYNTAX  SEQUENCE OF GnGigabitEthernetMonCurrEntry
        ACCESS  not-accessible
        STATUS mandatory
        DESCRIPTION
        "This table maintains the PM for the gigabit ethernet table. "
        ::= { gnGigabitEthernetMon 1 }

gnGigabitEthernetMonCurrEntry OBJECT-TYPE
        SYNTAX GnGigabitEthernetMonCurrEntry
        ACCESS  not-accessible
        STATUS mandatory
        DESCRIPTION
        "This is an entry in the PDH
         Counters Table. This table contains an interface index for every interface
         which type is GBE(VAL_ifType_ethernetCsmacd-6, according to RFC1573)."
        INDEX {  ifIndex }
        ::= { gnGigabitEthernetMonCurrTable 1 }

GnGigabitEthernetMonCurrEntry ::=
            SEQUENCE {
        gnGigabitEthernetMonCurrPacketErrorRate
             INTEGER,
        gnGigabitEthernetMonCurrIDF
             INTEGER,
        gnGigabitEthernetMonCurrDayPacketErrorRate
             INTEGER,                     
        gnGigabitEthernetMonCurrDayIDF
             INTEGER        
}

gnGigabitEthernetMonCurrPacketErrorRate OBJECT-TYPE
        SYNTAX  INTEGER (0..100)
        ACCESS  read-only
        STATUS mandatory
        DESCRIPTION
        "The packet error rate ( 100*RadioReceivedCrcFrame/(RadioReceivedFrame) )
         starting from the present 15 minute period, the calculation is in percent."
        ::= { gnGigabitEthernetMonCurrEntry 1}

gnGigabitEthernetMonCurrIDF OBJECT-TYPE
        SYNTAX  INTEGER {
                        off(2),
                        on(3)
        }
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "IDF = Invalid Data Flag in current 15 minutes interval."
        DEFVAL { off }
        ::= { gnGigabitEthernetMonCurrEntry 2 }

gnGigabitEthernetMonCurrDayPacketErrorRate OBJECT-TYPE
        SYNTAX  INTEGER (0..100)
        ACCESS  read-only
        STATUS mandatory
        DESCRIPTION
        "The packet error rate ( 100*RadioReceivedCrcFrame/(RadioReceivedFrame) )
         starting from the present 24 hour period, the calculation is in percent."
        ::= { gnGigabitEthernetMonCurrEntry 3}

gnGigabitEthernetMonCurrDayIDF OBJECT-TYPE
        SYNTAX  INTEGER {
                        off(2),
                        on(3)
        }
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "IDF = Invalid Data Flag in current 24 hour interval."
        DEFVAL { off }
        ::= { gnGigabitEthernetMonCurrEntry 4 }

gnGigabitEthernetMonIntervalTable OBJECT-TYPE
        SYNTAX  SEQUENCE OF GnGigabitEthernetMonIntervalEntry
        ACCESS  not-accessible
        STATUS mandatory
        DESCRIPTION
        "This table maintains the PM of the GBE per interval."
        ::= { gnGigabitEthernetMon 2 }


gnGigabitEthernetMonIntervalEntry OBJECT-TYPE
        SYNTAX GnGigabitEthernetMonIntervalEntry
        ACCESS  not-accessible
        STATUS mandatory
        DESCRIPTION
        "This is an entry in the GBE PM Table. This table contains an interface index for every interface
         which type is GBE(VAL_ifType_ethernetCsmacd-6, according to RFC1573).
         This table is indexed by an interval index, between 1 to 96 and representing each one 15 minutes.
         The oldest is 96."
        INDEX {  ifIndex, gnGigabitEthernetMonIntervalIdx}
        ::= { gnGigabitEthernetMonIntervalTable 1 }

GnGigabitEthernetMonIntervalEntry ::=
            SEQUENCE {
        gnGigabitEthernetMonIntervalIdx
             INTEGER,
        gnGigabitEthernetMonIntervalPacketErrorRate
             INTEGER,
        gnGigabitEthernetMonIntervalIDF
             INTEGER
}

gnGigabitEthernetMonIntervalIdx OBJECT-TYPE
        SYNTAX INTEGER (0..96)
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "This table is indexed per interval number. Each interval
        is of 15 minutes and the oldest is 96."
        ::= { gnGigabitEthernetMonIntervalEntry 1 }

gnGigabitEthernetMonIntervalPacketErrorRate OBJECT-TYPE
        SYNTAX  INTEGER(0..100)
        ACCESS  read-only
        STATUS mandatory
        DESCRIPTION
        "The packet error rate per interval( 100*RadioReceivedCrcFrame/(RadioReceivedFrame) )
         per 15 minutes interval, the calculation is in percent."
        ::= { gnGigabitEthernetMonIntervalEntry 2}

gnGigabitEthernetMonIntervalIDF OBJECT-TYPE
        SYNTAX  INTEGER {
                        off(2),
                        on(3)
        }
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "IDF = Invalid Data Flag."
        DEFVAL { off }
        ::= { gnGigabitEthernetMonIntervalEntry 3 }


gnGigabitEthernetMonDayTable OBJECT-TYPE
        SYNTAX  SEQUENCE OF GnGigabitEthernetMonDayEntry
        ACCESS  not-accessible
        STATUS mandatory
        DESCRIPTION
        "This table maintains the PM of the GBE per 24 hours interval."
        ::= { gnGigabitEthernetMon 3 }

gnGigabitEthernetMonDayEntry OBJECT-TYPE
        SYNTAX GnGigabitEthernetMonDayEntry
        ACCESS  not-accessible
        STATUS mandatory
        DESCRIPTION
        "This is an entry in the GBE PM Table. This table contains an interface index for every interface
         which type is GBE(VAL_ifType_ethernetCsmacd-6, according to RFC1573).
         This table is indexed by an interval index, between 1
         to 30 and representing each one 24 hours.
         The oldest is 30."
        INDEX {  ifIndex, gnGigabitEthernetMonDayIdx}
        ::= { gnGigabitEthernetMonDayTable 1 }

GnGigabitEthernetMonDayEntry ::=
            SEQUENCE {
        gnGigabitEthernetMonDayIdx
             INTEGER,
        gnGigabitEthernetMonDayPacketErrorRate
             INTEGER,
        gnGigabitEthernetMonDayIDF
             INTEGER
}

gnGigabitEthernetMonDayIdx OBJECT-TYPE
        SYNTAX INTEGER (1..30)
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "This table is indexed per interval number. Each interval
        is of 24 hours and the oldest is 30."
        ::= { gnGigabitEthernetMonDayEntry 1 }

gnGigabitEthernetMonDayPacketErrorRate OBJECT-TYPE
        SYNTAX  INTEGER(0..100)
        ACCESS  read-only
        STATUS mandatory
        DESCRIPTION
        "The packet error rate per interval( 100*RadioReceivedCrcFrame/(RadioReceivedFrame) )
         per 24 hour interval, the calculation is in percent."
        ::= { gnGigabitEthernetMonDayEntry 2}

gnGigabitEthernetMonDayIDF OBJECT-TYPE
        SYNTAX  INTEGER {
                        off(2),
                        on(3)
        }
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "IDF = Invalid Data Flag."
        DEFVAL { off }
        ::= { gnGigabitEthernetMonDayEntry 3 }

gnEthStatistic OBJECT IDENTIFIER ::= { gnAccess 14 }
gnEthStatisticTable OBJECT-TYPE
        SYNTAX  SEQUENCE OF GnEthStatisticEntry
        ACCESS  not-accessible
        STATUS mandatory
        DESCRIPTION
        "This table illustrate the gigabit ethernet statistics."
        ::= { gnEthStatistic 1 }

gnEthStatisticEntry OBJECT-TYPE
        SYNTAX GnEthStatisticEntry
        ACCESS  not-accessible
        STATUS mandatory
        DESCRIPTION
        "This is an entry in the gigabit ethernet statistics table.
         This table illustrate the MSB value of the RMON II statistics."
        INDEX {  ifIndex }
        ::= { gnEthStatisticTable 1 }

GnEthStatisticEntry ::=
            SEQUENCE {
        gnEthStatisticIfInOctetsMsb
             Counter,
        gnEthStatisticAFrameReceivedOkLsb
             Counter,
        gnEthStatisticAFrameReceivedOkMsb
             Counter,
        gnEthStatisticCRCAlignErrorsMsb
             Counter,
        gnEthStatisticIfInUcastPktsMsb
             Counter,
        gnEthStatisticBroadcastPktsMsb
             Counter,
        gnEthStatisticMulticastPktsMsb
             Counter,
        gnEthStatisticUndersizePktsMsb
             Counter,
        gnEthStatisticOversizePktsMsb
             Counter,
        gnEthStatisticPkts64OctetMsb
             Counter,
        gnEthStatisticPkts65to127OctetMsb
             Counter,
        gnEthStatisticPkts128to255OctetMsb
             Counter,
        gnEthStatisticPkts256to511OctetMsb
             Counter,
        gnEthStatisticPkts512to1023OctetMsb
             Counter,
        gnEthStatisticPkts1024to1518OctetMsb
             Counter,
        gnEthStatisticRadioTransmitFramesLsb
             Counter,
        gnEthStatisticRadioTransmitFramesMsb
             Counter,
        gnEthStatisticDroppedPacketsLsb
             Counter,
        gnEthStatisticDroppedPacketsMsb
             Counter,
        gnEthStatisticRadioReceivedFramesLsb
             Counter,
        gnEthStatisticRadioReceivedFramesMsb
             Counter,
        gnEthStatisticRadioReceivedCrcFramesLsb
             Counter,
        gnEthStatisticRadioReceivedCrcFramesMsb
             Counter,
        gnEthStatisticRadioSyncUnlockEventsLsb
             Counter,
        gnEthStatisticRadioSyncUnlockEventsMsb
             Counter,
        gnEthStatisticAFrameTransmittedOkLsb
             Counter,
        gnEthStatisticAFrameTransmittedOkMsb
             Counter,
        gnEthStatisticIfOutOctetsMsb
             Counter,
        gnEthStatisticEtherStatsPktsMsb
             Counter,
        gnEthStatisticEtherStatsOctetsMsb
             Counter
		}

gnEthStatisticIfInOctetsMsb OBJECT-TYPE
        SYNTAX Counter 
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "The total number of octets received on the
         interface, including framing characters (MSB)."
        ::= { gnEthStatisticEntry 1 }

gnEthStatisticAFrameReceivedOkLsb OBJECT-TYPE
        SYNTAX Counter 
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "Sum of frames received without error (LSB)."
        ::= { gnEthStatisticEntry 2 }

gnEthStatisticAFrameReceivedOkMsb OBJECT-TYPE
        SYNTAX Counter 
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "Sum of frames received without error (MSB)."
        ::= { gnEthStatisticEntry 3 }

gnEthStatisticCRCAlignErrorsMsb OBJECT-TYPE
        SYNTAX Counter 
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "The total number of packets received that
        had a length (excluding framing bits, but
        including FCS octets) of between 64 and 1518
        octets, inclusive, but had either a bad
        Frame Check Sequence (FCS) with an integral
        number of octets (FCS Error) or a bad FCS with
        a non-integral number of octets (Alignment Error), (MSB)."
        ::= { gnEthStatisticEntry 4 }

gnEthStatisticIfInUcastPktsMsb OBJECT-TYPE
        SYNTAX Counter 
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "The number of subnetwork-unicast packets
        delivered to a higher-layer protocol (MSB)."
        ::= { gnEthStatisticEntry 5 }

gnEthStatisticBroadcastPktsMsb OBJECT-TYPE
        SYNTAX Counter 
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "The total number of good packets received that were
        directed to the broadcast address.  Note that this
        does not include multicast packets (MSB)."
        ::= { gnEthStatisticEntry 6 }

gnEthStatisticMulticastPktsMsb OBJECT-TYPE
        SYNTAX Counter 
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "The total number of good packets received that were
        directed to a multicast address.  Note that this number
        does not include packets directed to the broadcast
        address (MSB)."
        ::= { gnEthStatisticEntry 7 }

gnEthStatisticUndersizePktsMsb OBJECT-TYPE
        SYNTAX Counter 
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "The total number of packets received that were
        less than 64 octets long (excluding framing bits,
        but including FCS octets) and were otherwise well
        formed (MSB)."
        ::= { gnEthStatisticEntry 8 }

gnEthStatisticOversizePktsMsb OBJECT-TYPE
        SYNTAX Counter 
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "The total number of packets received that were
        longer than 12000 octets (excluding framing bits,
        but including FCS octets) and were otherwise
        well formed. (MSB)"
        ::= { gnEthStatisticEntry 9 }

gnEthStatisticPkts64OctetMsb OBJECT-TYPE
        SYNTAX Counter 
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "The total number of packets (including bad
        packets) received that were 64 octets in length
        (excluding framing bits but including FCS octets), (MSB)."
        ::= { gnEthStatisticEntry 10 }

gnEthStatisticPkts65to127OctetMsb OBJECT-TYPE
        SYNTAX Counter 
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "The total number of packets (including bad
        packets) received that were between
        65 and 127 octets in length inclusive
        (excluding framing bits but including FCS octets), (MSB)."
        ::= { gnEthStatisticEntry 11 }

gnEthStatisticPkts128to255OctetMsb OBJECT-TYPE
        SYNTAX Counter 
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "The total number of packets (including bad
        packets) received that were between
        128 and 255 octets in length inclusive
        (excluding framing bits but including FCS octets), (MSB)."
        ::= { gnEthStatisticEntry 12 }

gnEthStatisticPkts256to511OctetMsb OBJECT-TYPE
        SYNTAX Counter 
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "The total number of packets (including bad
        packets) received that were between
        256 and 511 octets in length inclusive
        (excluding framing bits but including FCS octets), (MSB)."
        ::= { gnEthStatisticEntry 13 }

gnEthStatisticPkts512to1023OctetMsb OBJECT-TYPE
        SYNTAX Counter 
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "The total number of packets (including bad
        packets) received that were between
        512 and 1023 octets in length inclusive
        (excluding framing bits but including FCS octets), (MSB)."
        ::= { gnEthStatisticEntry 14 }

gnEthStatisticPkts1024to1518OctetMsb OBJECT-TYPE
        SYNTAX Counter 
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "The total number of packets (including bad
        packets) received that were between
        1024 and 1518 octets in length inclusive
        (excluding framing bits but including FCS octets), (MSB)."
        ::= { gnEthStatisticEntry 15 }

gnEthStatisticRadioTransmitFramesLsb OBJECT-TYPE
        SYNTAX Counter 
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
		"Sum of frame transmitted to radio (LSB)"
        ::= { gnEthStatisticEntry 16 }

gnEthStatisticRadioTransmitFramesMsb OBJECT-TYPE
        SYNTAX Counter 
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
		"Sum of frame transmitted to radio (MSB)"
        ::= { gnEthStatisticEntry 17 }

 gnEthStatisticDroppedPacketsLsb OBJECT-TYPE
        SYNTAX Counter 
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
		"number of frames dropped (LSB)"
        ::= { gnEthStatisticEntry 18 }

 gnEthStatisticDroppedPacketsMsb OBJECT-TYPE
        SYNTAX Counter 
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
		"number of frames dropped (MSB)"
        ::= { gnEthStatisticEntry 19 }

 gnEthStatisticRadioReceivedFramesLsb OBJECT-TYPE
        SYNTAX Counter 
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
		"Sum of frames received by radio (LSB)"
        ::= { gnEthStatisticEntry 20 }

 gnEthStatisticRadioReceivedFramesMsb OBJECT-TYPE
        SYNTAX Counter 
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
		"Sum of frames received by radio (MSB)"
        ::= { gnEthStatisticEntry 21 }

gnEthStatisticRadioReceivedCrcFramesLsb OBJECT-TYPE
        SYNTAX Counter 
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
		"sum of frames received with error (LSB)"
        ::= { gnEthStatisticEntry 22 }

gnEthStatisticRadioReceivedCrcFramesMsb OBJECT-TYPE
        SYNTAX Counter 
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
		"sum of frames received with error (MSB)"
        ::= { gnEthStatisticEntry 23 }

gnEthStatisticRadioSyncUnlockEventsLsb OBJECT-TYPE
        SYNTAX Counter 
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
		"sum of GFP not locked events (LSB)"
        ::= { gnEthStatisticEntry 24 }

gnEthStatisticRadioSyncUnlockEventsMsb OBJECT-TYPE
        SYNTAX Counter 
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
		"sum of GFP not locked events (MSB)"
        ::= { gnEthStatisticEntry 25 }

gnEthStatisticAFrameTransmittedOkLsb OBJECT-TYPE
        SYNTAX Counter 
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "Sum of valid frames transmitted to line (LSB)."
        ::= { gnEthStatisticEntry 26 }

gnEthStatisticAFrameTransmittedOkMsb OBJECT-TYPE
        SYNTAX Counter 
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "Sum of valid frames transmitted to line (MSB)."
        ::= { gnEthStatisticEntry 27 }

gnEthStatisticIfOutOctetsMsb OBJECT-TYPE
        SYNTAX Counter 
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "The total number of octets transmitted out of the
        interface, including framing characters (MSB)."
        ::= { gnEthStatisticEntry 28 }

gnEthStatisticEtherStatsPktsMsb OBJECT-TYPE
        SYNTAX Counter 
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
         "The total number of packets (including bad packets,
         broadcast packets, and multicast packets) received (MSB)."
        ::= { gnEthStatisticEntry 29 }

gnEthStatisticEtherStatsOctetsMsb OBJECT-TYPE
        SYNTAX Counter 
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
         "The total number of octets of data (including
         those in bad packets) received on the
         network (excluding framing bits but including
         FCS octets)."
        ::= { gnEthStatisticEntry 30 }

