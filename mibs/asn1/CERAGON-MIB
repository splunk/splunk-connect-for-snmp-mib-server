        SYNTAX  INTEGER {
                        off(2),
                        on(3)
        }
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "IDF = Invalid Data Flag."
        DEFVAL { off }
        ::= { gnPdhMonDayEntry 7 }
