        SYNTAX  INTEGER {
                                east(2),
                     west(3)
                }
        ACCESS  read-write
        STATUS mandatory
              DESCRIPTION ""
            DEFVAL        {east}
        ::= { gnMdmCfgXEntry 4 }

gnMdmCfgXMrmcConf        OBJECT-TYPE
        SYNTAX  INTEGER        {
                                mrmc1528(1),
                            	mrmc1550(2),
                                mrmc3156(3),
                                mrmc3150(4),
                                mrmc1228(5),
                                mrmc1540(6),
                            	mrmc4410(7),
                                mrmc4420(8),
                                mrmc4440(9),
                                mrmc1125(10),
                                mrmc2050(11),
                                mrmc2030(12),
                                mrmc1340(13),
                                mrmc3756(18),
                                acm56(24),
                                acm50(25),
                                acm28(26)
                }
    ACCESS  read-write
    STATUS  mandatory
    DESCRIPTION
        "The Link data capacity"
         ::= { gnMdmCfgXEntry 5 }

