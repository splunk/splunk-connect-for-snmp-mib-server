        SYNTAX  INTEGER
        {
                yes(2),
                no(3)
        }
        ACCESS  read-only
        STATUS mandatory
        DESCRIPTION
        "This is indicate the application file DSS support."
        ::= { gnApplicFileEntry 12 }

