        DEFVAL { 25 }
        ::= { gnOduCfgXEntry 17 }

gnOduCfgXOperation OBJECT-TYPE
        SYNTAX  INTEGER
        {
                noOperation(2),
        setOduDefaultConf(3),
        resetODU(4),
                swResetODU(5)
        }
        ACCESS  read-write
        STATUS mandatory
        DESCRIPTION
                "This parameter allows to perform ODU operations.
                  (3) will SET all ODU default configurations.
                  (4) will implement ODU reset."
        DEFVAL { noOperation }
        ::= { gnOduCfgXEntry 18 }

