        SYNTAX  INTEGER {
                                noAction(2),
                     switchToProtection(3),
                                switchToWorking(4)
                }
        ACCESS  read-write
        STATUS mandatory
              DESCRIPTION ""
            DEFVAL        {noAction}
        ::= { gnTrailCfgEntry 10 }

