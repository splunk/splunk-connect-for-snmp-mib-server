        SYNTAX  INTEGER {
                        noloopback(2),
                        waySideLoop(3)
        }
        ACCESS  read-write
        STATUS mandatory
        DESCRIPTION
        "This parameter controls the WAYSIDE channel loopback diagnostic operations."
        DEFVAL { noloopback }
        ::= { gnMuxCfgXEntry 3 }

