        SYNTAX  INTEGER
        ACCESS  read-only
        STATUS mandatory
        DESCRIPTION
        "This is the application file General Purpose."
        ::= { gnApplicFileEntry 9 }

gnApplicFileSize OBJECT-TYPE
        SYNTAX  INTEGER
        ACCESS  read-only
        STATUS mandatory
        DESCRIPTION
        "This is the application file size in bytes."
        ::= { gnApplicFileEntry 10 }

gnApplicFileCompressed OBJECT-TYPE
        SYNTAX  INTEGER
        {
                yes(2),
                no(3)
        }        
		ACCESS  read-only
        STATUS mandatory
        DESCRIPTION
        "This is indicate the application file compress status."
        ::= { gnApplicFileEntry 11 }

