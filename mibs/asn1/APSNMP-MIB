--
-- Name:     Apollo SNMP MIB
-- Purpose:  MIB for the APSNMP.DLL Extension agent for Windows NT
-- File:     APSNMP.MIB
-- Revision: 1.0
-- Author:   Dean A. Gratton
-- Compile:  MIBCC SMI.MIB APSNMP.MIB
--
-- OIDS:
-- Brand Private Enterprise Number:		3716
--   Windows NT Operating System:		1
--     ApolloStatsForNT					1
--     apolloTrapsForNT					2
--   Unix Operating System:				2
--     ApolloStatsForUnix				1
--     apolloTrapsForUnix				2
--
-- Base OID for Brand WindowsNT:  1.3.6.1.4.1.3716.1.1
-- Base OID for Brand Unix:       1.3.6.1.4.1.3716.1.2
--
-- Revision History:
--     18-11-98		1.00    Initial Revision
--

    APSNMP-MIB DEFINITIONS ::= BEGIN
    IMPORTS
	TRAP-TYPE FROM RFC-1215
        enterprises
            FROM RFC1155-SMI
        OBJECT-TYPE
            FROM RFC-1212
        DisplayString
            FROM RFC1213-MIB;

    brand                      OBJECT IDENTIFIER ::= { enterprises 3716       }
    windowsNT                  OBJECT IDENTIFIER ::= { brand 1                }

    apolloStatsForNT           OBJECT IDENTIFIER ::= { windowsNT 1            }
    apolloThresholdForNT       OBJECT IDENTIFIER ::= { windowsNT 2            }
	apolloTrapsForNT           OBJECT IDENTIFIER ::= { windowsNT 3            }

	statsForPort               OBJECT IDENTIFIER ::= { apolloStatsForNT 1     }
	statsForCalls              OBJECT IDENTIFIER ::= { apolloStatsForNT 2     } 
	statsForSession            OBJECT IDENTIFIER ::= { apolloStatsForNT 3     }
	statsForData               OBJECT IDENTIFIER ::= { apolloStatsForNT 4     }
	statsForError              OBJECT IDENTIFIER ::= { apolloStatsForNT 5     }
	statsForDriver             OBJECT IDENTIFIER ::= { apolloStatsForNT 6     }
	statsForCompression        OBJECT IDENTIFIER ::= { apolloStatsForNT 7     }

	statsSessionForGeneral     OBJECT IDENTIFIER ::= { statsForSession  1     }
	statsSessionForApollo      OBJECT IDENTIFIER ::= { statsForSession  2     }
	statsSessionForPPP	       OBJECT IDENTIFIER ::= { statsForSession  3     }

	thresholdForPort           OBJECT IDENTIFIER ::= { apolloThresholdForNT 1 }
	thresholdForCalls          OBJECT IDENTIFIER ::= { apolloThresholdForNT 2 }
	thresholdForSession        OBJECT IDENTIFIER ::= { apolloThresholdForNT 3 }
	thresholdForData           OBJECT IDENTIFIER ::= { apolloThresholdForNT 4 }
	thresholdForError          OBJECT IDENTIFIER ::= { apolloThresholdForNT 5 }
	thresholdForDriver         OBJECT IDENTIFIER ::= { apolloThresholdForNT 6 }
	thresholdForCompression    OBJECT IDENTIFIER ::= { apolloThresholdForNT 7 }

	thresholdSessionForGeneral OBJECT IDENTIFIER ::= { thresholdForSession  1 }
	thresholdSessionForApollo  OBJECT IDENTIFIER ::= { thresholdForSession  2 }
	thresholdSessionForPPP	   OBJECT IDENTIFIER ::= { thresholdForSession  3 }

