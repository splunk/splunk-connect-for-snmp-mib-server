    ::= 73

  trapsTotalErrorRatePerSecond TRAP-TYPE
    ENTERPRISE      apolloTrapsForNT
    DESCRIPTION
      "Trap"
    ::= 74

-- DRIVER
  trapsCurrentClock TRAP-TYPE
    ENTERPRISE      apolloTrapsForNT
    DESCRIPTION
      "Trap"
    ::= 75

  trapsCurrentMemoryUsedByDriver TRAP-TYPE
    ENTERPRISE      apolloTrapsForNT
    DESCRIPTION
      "Trap"
    ::= 76

  trapsMaximumMemoryUsedByDriver TRAP-TYPE
    ENTERPRISE      apolloTrapsForNT
    DESCRIPTION
      "Trap"
    ::= 77

  trapsPercentCurrentMemoryUsedByDriver TRAP-TYPE
    ENTERPRISE      apolloTrapsForNT
    DESCRIPTION
      "Trap"
    ::= 78

  trapsPercentMaximumMemoryUsedByDriver TRAP-TYPE
    ENTERPRISE      apolloTrapsForNT
    DESCRIPTION
      "Trap"
    ::= 79

  trapsTotalMemoryAvailableToTheDriver TRAP-TYPE
    ENTERPRISE      apolloTrapsForNT
    DESCRIPTION
      "Trap"
    ::= 80

-- COMPRESSION
  trapsBytesIntoCompression TRAP-TYPE
    ENTERPRISE      apolloTrapsForNT
    DESCRIPTION
      "Trap"
    ::= 81

  trapsBytesOutCompression TRAP-TYPE
    ENTERPRISE      apolloTrapsForNT
    DESCRIPTION
      "Trap"
    ::= 82

  trapsBytesIntoDecompression TRAP-TYPE
    ENTERPRISE      apolloTrapsForNT
    DESCRIPTION
      "Trap"
    ::= 83

  trapsBytesOutDecompression TRAP-TYPE
    ENTERPRISE      apolloTrapsForNT
    DESCRIPTION
      "Trap"
    ::= 84

