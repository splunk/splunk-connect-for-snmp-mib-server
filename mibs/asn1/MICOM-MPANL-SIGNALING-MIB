	SYNTAX  SEQUENCE OF McmMSMLinkStatusEntry
	ACCESS  not-accessible
	STATUS  mandatory
	DESCRIPTION
 	  "NAME = ; 
    	   DESC = The PANL Link Status Parameters \
		  table.;
   	   HELP = ;
    	   CAPABILITIES = NET_DISP, VPN_DISP;"
 	::= { status 1 }

