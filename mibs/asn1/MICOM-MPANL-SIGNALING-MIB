--  The DNA Status Table

--  This table provides the netlink ifindex value 
--  and DNA status associated with a specific DNA
--  prefix number of an attached unit. (attached
--  either directly or cascaded via a DCE interface) 

mcmMSMDNAStatusTable OBJECT-TYPE
	SYNTAX  SEQUENCE OF McmMSMDNAStatusEntry
	ACCESS  not-accessible
	STATUS  mandatory
	DESCRIPTION
 	  "NAME = ; 
    	   DESC = The Associated DNA Status Parameters \
		  table.;
   	   HELP = ;
    	   CAPABILITIES = NET_DISP, VPN_DISP;"
 	::= { status 3}

mcmMSMDNAStatusEntry OBJECT-TYPE
	SYNTAX  McmMSMDNAStatusEntry 
	ACCESS  not-accessible
	STATUS  mandatory
 	DESCRIPTION
   	  "NAME = ; 
    	   DESC = An entry in the associated DNA status \
            		  table.;
    	   HELP = ;
    	   CAPABILITIES = ;"
 	INDEX   { mcmMSMDNAStatusPrefixNumber }
	::= { mcmMSMDNAStatusTable 1 }

McmMSMDNAStatusEntry ::= SEQUENCE {
	mcmMSMDNAStatusPrefixNumber	DisplayString,
    	mcmMSMDNAStatusIfIndex         	INTEGER,
	mcmMSMDNAStatusAssociation	INTEGER
	}

 mcmMSMDNAStatusPrefixNumber OBJECT-TYPE
    SYNTAX  DisplayString (SIZE(1..31))
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
      "NAME = ;
       DESC = DNA Prefix number of the attached device \
 	      (directly or cascaded).; 
       HELP = ;
       CAPABILITIES = ;"
    ::= { mcmMSMDNAStatusEntry 1 }

 mcmMSMDNAStatusIfIndex OBJECT-TYPE
    SYNTAX  INTEGER(1..255)
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
      "NAME = ;
       DESC = Ifindex value showing the appropriate DCE \
	      PANL interface corresponding to the specified \
	      DNA prefix.;
       HELP = ;
       CAPABILITIES = ;"
    ::= { mcmMSMDNAStatusEntry 2 }

 mcmMSMDNAStatusAssociation OBJECT-TYPE
    SYNTAX  INTEGER {
		associatedDNA 	(1),
		deassociatedDNA	(2)
		}
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
      "NAME = ;
       DESC = DNA Association Status corresponding to \
	      the specified DNA Prefix.; 
       HELP = ;
       CAPABILITIES = ;"
    ::= { mcmMSMDNAStatusEntry 3 }

