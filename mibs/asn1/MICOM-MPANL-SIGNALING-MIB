	::= { control 1 }

mcmMSMLAPFConnectionsCntrEntry OBJECT-TYPE
	SYNTAX  McmMSMLAPFConnectionsCntrEntry
	ACCESS  not-accessible
	STATUS  obsolete
	DESCRIPTION
	  "NAME = ;
	   DESC = An entry in the MSM LAP-F connections counter \
		  reset table.;
	   HELP = ;
	   CAPABILITIES = ;"
	INDEX   { mcmMSMLAPFConnectionsCntrIndex }
	::= { mcmMSMLAPFConnectionsCntrTable 1 }

McmMSMLAPFConnectionsCntrEntry ::= SEQUENCE {
	mcmMSMLAPFConnectionsCntrIndex   	INTEGER,
	mcmMSMLAPFConnectionsCntrAction 	INTEGER
	}

mcmMSMLAPFConnectionsCntrIndex OBJECT-TYPE
	SYNTAX	INTEGER (1..255)
	ACCESS	read-only
	STATUS	obsolete
	DESCRIPTION
	  "NAME = ;
	   DESC = The value of this object corresponds to a value \
		  of IfIndex, and is used as the index into the \
		  MSM LAP-F connections counter reset table. \
		  This index specifies the particular row of counter \
		  objects to be reset, and has the identical value as \
		  mcmMSMStatsLAPFConnIfIndex.;
	   HELP = ;
	   CAPABILITIES = ;"
	::= { mcmMSMLAPFConnectionsCntrEntry 1 }

mcmMSMLAPFConnectionsCntrAction OBJECT-TYPE
	SYNTAX  INTEGER { 
		reset (1) 
		}
	ACCESS	write-only
	STATUS  obsolete
	DESCRIPTION
	  "NAME = ;
	   DESC = Setting this object to reset(1) will zero \
		  the values of a specific row of counters in \
		  the MPANL Signaling Statistics for LAP-F \
		  connections table.;
	   HELP = When set to reset(1), a row containing \
		  the following counter objects are reset: \
		    mcmMSMStatsLAPFConnReestablished \
		    mcmMSMStatsLAPFConnEstablished \
		    mcmMSMStatsLAPFConnDisconnects;
	   CAPABILITIES = ;"
	::= { mcmMSMLAPFConnectionsCntrEntry 2 }



-- *** TRAP Messages ***
mcmMSMProfileReceivedFromPassport TRAP-TYPE
	ENTERPRISE	micom-msm
	VARIABLES	{ mcmSysAsciiTimeOfDay, 
			  mcmMSMLinkStatusIfIndex, 
		       	  mcmMSMDTELinkStatsRemoteCompName 
			}
	DESCRIPTION
	  "NAME = ;
	   DESC = PANL Signaling Protocol established. \ 
		  Severity Level: INFORMATIVE.;
       HELP = Profile is received from Passport. This is a Normal\
                  condition.;"
	::= 1

mcmMpanlInterfaceLinkUp TRAP-TYPE
	ENTERPRISE	micom-msm
	VARIABLES	{ mcmSysAsciiTimeOfDay, mcmMSMLinkStatusIfIndex }
	DESCRIPTION
	  "NAME = ;
	   DESC = This trap will be generated when the LAPF link  \
                  connection for the PANL signaling comes up. \
		  Severity Level: INFORMATIVE.;
       HELP = The MSM LAPF link connection Is up. Normal MSM LAPF \
		  Link establishment.;"
	::= 2

mcmMpanlInterfaceLinkDown TRAP-TYPE
	ENTERPRISE	micom-msm
	VARIABLES	{ mcmSysAsciiTimeOfDay, mcmMSMLinkStatusIfIndex } 
	DESCRIPTION
	  "NAME = ;
	   DESC = This trap will be generated when the LAPF link  \
                  connection for the PANL signaling goes down. \
		  Severity Level: CRITICAL.;
           HELP = Please check device configuration, and connection.\
                  If problem persist, You may need field \
                  personnel's assistance.;"
	::= 3

mcmMpanlPrefixDNAhasNotBeenConfigured TRAP-TYPE
	ENTERPRISE	micom-msm
	VARIABLES	{ mcmSysAsciiTimeOfDay } 
	DESCRIPTION
	  "NAME = ;
	   DESC = This trap will be generated from the MPANL \
                  signaling module. It will trigger if the \
                  DNA has not been configured. \
		  Severity Level: CRITICAL.;
           HELP = Please provision(configure) the DNA address \
                  of the access device. This may imply the DNA address \
		  is missing.;"
	::= 4
   
mcmMpanlPrefixDNAChangedWithoutDeassociation TRAP-TYPE
	ENTERPRISE	micom-msm
	VARIABLES	{ mcmSysAsciiTimeOfDay, 
			  mcmMSMDNAStatusPrefixNumber, 
			  mcmMSMDNAStatusIfIndex 
			} 
	DESCRIPTION
	  "NAME = ;
	   DESC = This trap will be generated from the PANL \
                  signaling module. It will trigger if the \
                  latest association will displace the earlier \
		  assoiciation with a different netlink. \
		  Severity Level: INFORMATIVE.;
       HELP = Please verify the DNA association number.;" 
	::= 5
   
mcmMpanlIncompatibleType TRAP-TYPE
	ENTERPRISE	micom-msm
	VARIABLES	{ mcmSysAsciiTimeOfDay, 
			  mcmMSMLinkStatusIfIndex, 
			  mcmMSMLinkStatusRemoteGenCfgType 
			} 
	DESCRIPTION
	  "NAME = ;
	   DESC = This trap will be generated from the PANL \
                  signaling module. It will trigger if a micom \
		  access device tries to attempt to communicate \
		  with a nortel access device, which is prohibited. \
		  Severity Level: CRITICAL.;
	   HELP = It is noted that micom access device does not \
	  	  communicate with the nortel access device.;" 
	::= 6
   

END



