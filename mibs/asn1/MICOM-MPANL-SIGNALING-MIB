-- Based on MPANL Signaling Module Design Document (Rev 1.5,
-- dated 5/2/97)
-- 
-- MIB Revision : 1.0 dated 5-5-97
-- Change Desc. : Initial submittal - R. Carmona. 
-- 
-- MIB Revision : 1.1 dated 7-16-97
-- Change Desc. : Replaced index of Statistics and Counter Reset tables
--		  from Node ID to IfIndex as per J. Nakano e-mail
--		  on 7/14/97. - R. Carmona. 
-- MIB Revision : 1.2 dated 10-13-97
-- Change Desc. : Replaced MPA with 4400 - B. Jafari
--
-- MIB Revision : 1.3 dated 10-22-97
-- Change Desc. : Split the node and line profile for both configuration
--                and statistics. Requested by James Shum     - S. Gaur
--
-- MIB Revision : 1.4 dated 10-27-97
-- Change Desc. : Range added for Indices.  Component names changed to 
--		  display strings.  Changed mcmMSMDTELinkStatsProStatus to
--		  mcmMSMDTELinkStatsStatus. - K. Yeh
--
-- MIB Revision : 1.5 dated 11-12-97
-- Change Desc. : Added traps to trigger when  profile received, link-up
--                and link-down.            - S. Gaur
--                
--
-- MIB Revision : 1.5 dated 12-09-97
-- Change Desc. : Added trap to trigger if DNA is not configured.
--                                          - S. Gaur
-- 
-- *** RELEASE 3.0 ***
-- MIB Revision : 1.6 dated 6-09-98
-- Change Desc. : Added status to group Object Identifiers.
--				  Changed all references of 4400 to a generic name
--				  such as "access device".
--				  Added NSP_DISP and VPN_DISP Capabilities to the 
--				  (mcm/nvm)MSMProfileCfgGroup scalar objects.
--				  Obsoleted the row status object "nvmMSMDTELinkCfgEntryStatus".
--				  Obsoleted the object "mcmMSMStatsLAPFConnReestablished".
--				  Added additional statistic counters to the 
--				  mcmMSMStatsLAPFConnTable.
--				  Deprecated the mcmMSMDTELinkStatsTable.
--				  Added a common DCE/DTE PANL Link Status table,
--				  "mcmMSMLinkStatusTable".
--				  Added a DCE only PANL Link Status Table, 
--				  "mcmMSMDCELinkStatusTable".
--				  Added a DNA Status Table, "mcmMSMDNAStatusTable".
--				  Obsoleted the objects in mcmMSMLAPFConnectionsCntrTable.
--				  Modified the trap objects mcmMpanlInterfaceLinkUp and
--				  mcmMpanlInterfaceLinkDown by changing an object in the 
--				  variable list to mcmMSMLinkStatusIfIndex.
--				  Added two new traps: 
--				    mcmMpanlPrefixDNAChangedWithoutDeassociation
--					mcmMpanlIncompatibleType
--				  
--
-- MIB Revision : 1.7 dated 6-09-98
-- Change Desc. : Changed the access from read-write to read-only for the
--                nvmMSMDTELinkCfgDTEReceiverBW object.
--                Deprecated the (mcm/nvm)MSMDTELinkCfgDTEMaxFrameSize 
--                objects and the mcmMSMDTELinkCfgDTEReceiverBW object.
--                as per J. Nakano and C. Wu. 
--                                                             - S. Gaur
-- MIB Revision : 1.8 dated 8-7-98
-- Change Desc. : Changed the access back to read-write for the
--                nvmMSMDTELinkCfgDTEReceiverBW object.
--                Un-Deprecated the (mcm/nvm)MSMDTELinkCfgDTEMaxFrameSize 
--                objects and the mcmMSMDTELinkCfgDTEReceiverBW object.
--                Enhanced the descriptions for all of the above 
--                objects as well as for the 
--                (mcm/nvm)MSMDTELinkCfgDCEReceiverBW and
--                (mcm/nvm)MSMDTELinkCfgDCEMaxFrameSize objects
--                as per J. Nakano. 
--                Increased the range to 1 to 31 for
--                (mcm/nvm)MSMProfileCfgGroupDNAPrefix, 
--                and  mcmMSMDNAStatusPrefixNumber.
--                                                              - S. Gaur
-- 
--                                                             
-- MIB Revision : 1.9 dated 8-24-98
-- Change Desc. : For trap mcmMSMProfileReceivedFromPassport, the
--                varbind mcmMSMDTELinkCfgIfIndex was changed to 
--                mcmMSMLinkStatusIfIndex as per J. Nakano.
-- 
--                                                               - S. Gaur

MICOM-MPANL-SIGNALING-MIB DEFINITIONS ::= BEGIN
	IMPORTS
		Counter 
                   		 	FROM RFC1155-SMI
		OBJECT-TYPE
                   			FROM RFC-1212
		DisplayString
							FROM RFC1213-MIB
		micom-oscar
							FROM MICOM-OSCAR-MIB
        TRAP-TYPE
                            FROM RFC-1215
        mcmSysAsciiTimeOfDay
                            FROM MICOM-SYS-MIB;


-- group OBJECT IDENTIFIERs
	

	micom-msm		OBJECT IDENTIFIER ::= {micom-oscar 19}
	configuration	OBJECT IDENTIFIER ::= {micom-msm 1}
	control			OBJECT IDENTIFIER ::= {micom-msm 2}
	statistics		OBJECT IDENTIFIER ::= {micom-msm 3}
	status			OBJECT IDENTIFIER ::= {micom-msm 4}


-- Note: The following group contains 2 related tables for all configuration
--       functionality. 
--	 1) The Standard or Operational Parameters (Operational) provide the
--	 SNMP management interface with the Frame Relay Access Device 
--	 operational modules.
--	 Objects defined for this interface begin with the 'mcm' prefix.
--	 2) The Configuration Parameters (Configuration) provide the SNMP 
--	 management interface with a stored Frame Relay Access Device configuration. 
--	 Objects defined for this interface begin with the 'nvm' prefix.
--	


--  Operational Parameters (Operational) Group


--  the MPANL Signaling Profile Parameters Group (Standard/Operational Parameters)

--  This group provides the parameters to view the profile of the access device.  

mcmMSMProfileCfgGroup  OBJECT IDENTIFIER ::= { configuration 1 }


mcmMSMProfileCfgGroupNodeID OBJECT-TYPE
	SYNTAX	INTEGER (1..65535)
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = The value of this object represents a unique \
		  identifier for an access device. (Operational);
	   HELP = ;
	   CAPABILITIES = NET_DISP, VPN_DISP;"
	::= { mcmMSMProfileCfgGroup 1 }


mcmMSMProfileCfgGroupCustomerID OBJECT-TYPE
	SYNTAX	INTEGER (0..65535)
   	ACCESS  read-only
     	STATUS  mandatory
     	DESCRIPTION
          "NAME = ;
	   DESC = The value of this object uniquely identifies \
		  a customer to which a given access device belongs. \
		  (Operational);
	   HELP = ;
	   CAPABILITIES = NET_DISP, VPN_DISP;"
    	::= { mcmMSMProfileCfgGroup 2 }

mcmMSMProfileCfgGroupDNAPrefix OBJECT-TYPE
	SYNTAX	DisplayString (SIZE(1..31))
	ACCESS  read-only
     	STATUS  mandatory
     	DESCRIPTION
       	  "NAME = ; 
	   DESC = The value of this object specifies the prefix of the \
    	  Directory Network Address which uniquely identifies \
		  the unit within the network topology. (Operational);
	   HELP = ;
	   CAPABILITIES = NET_DISP, VPN_DISP;"
	::= { mcmMSMProfileCfgGroup 3 }


--  "Parallel" Configuration Parameters (Configuration)Group


--  the MPANL Signaling Profile Parameters Group (Configuration Parameters)

--  This table provides the parameters to define the profile of the Frame 
--  Relay Access Device. (Configuration)

nvmMSMProfileCfgGroup OBJECT IDENTIFIER ::= { configuration 2 }


nvmMSMProfileCfgGroupNodeID OBJECT-TYPE
	SYNTAX	INTEGER (1..65535)
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = The value of this object represents a unique \
		  identifier for an access device. (Configuration);
	   HELP = ;
	   CAPABILITIES = NET_CFG, VPN_DISP;"
	::= { nvmMSMProfileCfgGroup 1 }


nvmMSMProfileCfgGroupCustomerID OBJECT-TYPE
	SYNTAX	INTEGER (0..65535)
    	ACCESS  read-write
     	STATUS  mandatory
     	DESCRIPTION
          "NAME = ;
	   DESC = The value of this object uniquely identifies \
		  a customer to which a given access device belongs. \
		  Also, this is used by the NAC in registering the \
		  Egress table. (Configuration);
	   HELP = ;
	   CAPABILITIES = NET_CFG, VPN_DISP;"
     	::= { nvmMSMProfileCfgGroup 2}

nvmMSMProfileCfgGroupDNAPrefix OBJECT-TYPE
	SYNTAX	DisplayString (SIZE(1..31))
	ACCESS  read-write
     	STATUS  mandatory
     	DESCRIPTION
       	  "NAME = ; 
	   DESC = The value of this object specifies the prefix of the \
		  Directory Network Address which uniquely identifies \
		  the unit within the network topology. (Configuration);
	   HELP = ;
	   CAPABILITIES = NET_CFG, VPN_DISP;"
	::= { nvmMSMProfileCfgGroup 3 }


--  Operational Parameters (Operational) Table


--  the MPANL DTE Signaling Line Parameters Group (Standard/Operational Parameters)

--  This group provides the parameters to view the DTE line configuration
--  of the access device.  


mcmMSMDTELinkCfgTable OBJECT-TYPE
	SYNTAX  SEQUENCE OF McmMSMDTELinkCfgEntry
	ACCESS  not-accessible
	STATUS  mandatory
	DESCRIPTION
 	  "NAME = ; 
    	   DESC = The MPANL DTE Signaling Line Parameters \
		  table. (Operational);
   	   HELP = ;
    	   CAPABILITIES = NET_CFG, VPN_DISP;"
 	::= { configuration 3 }

mcmMSMDTELinkCfgEntry OBJECT-TYPE
	SYNTAX  McmMSMDTELinkCfgEntry 
	ACCESS  not-accessible
	STATUS  mandatory
 	DESCRIPTION
   	  "NAME = ; 
    	   DESC = An entry in the MPANL DTE Signaling Line Parameters \
            		  table. (Operational);
    	   HELP = ;
    	   CAPABILITIES = ;"
 	INDEX   { mcmMSMDTELinkCfgIfIndex }
	::= { mcmMSMDTELinkCfgTable 1 }

McmMSMDTELinkCfgEntry ::= SEQUENCE {
    mcmMSMDTELinkCfgIfIndex                 INTEGER,
	mcmMSMDTELinkCfgMaxSubChannelRange	INTEGER,
	mcmMSMDTELinkCfgDTEReceiverBW		INTEGER,
	mcmMSMDTELinkCfgDCEReceiverBW		INTEGER,
	mcmMSMDTELinkCfgDTEMaxFrameSize		INTEGER,
	mcmMSMDTELinkCfgDCEMaxFrameSize		INTEGER

	}

mcmMSMDTELinkCfgIfIndex OBJECT-TYPE
    SYNTAX  INTEGER (1..255)
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
      "NAME = ;
       DESC = Ifindex associated with the MPANL DTE line  \
              parameters.;
              
       HELP = ;
       CAPABILITIES = ;"
    ::= { mcmMSMDTELinkCfgEntry 1 }


mcmMSMDTELinkCfgMaxSubChannelRange OBJECT-TYPE
	SYNTAX	INTEGER (17..255)
    	ACCESS  read-only
     	STATUS  mandatory
     	DESCRIPTION
          "NAME = ;
	   DESC = The value of this object represents the highest \
		  DLCI value to be used for sub-channel (CID) allocation. \
		  (Note that the current default value, 63, may change \
		  depending on the VoFR agreements.)  Note that user \
		  DLCIs on the MPANL start at DLCI-17.  This \
		  information is signaled from the DTE to the DCE. \
		  (Operational);
	   HELP = ;
	   CAPABILITIES = ;"
	DEFVAL { 63 }
     	::= { mcmMSMDTELinkCfgEntry 2}

mcmMSMDTELinkCfgDTEReceiverBW OBJECT-TYPE
	SYNTAX	INTEGER (16000..2048000)
	ACCESS  read-only
     	STATUS  mandatory
     	DESCRIPTION
       	  "NAME = ; 
	   DESC = The value of this object represents the maximum \
		  throughput (in bits/sec) that the MPANL DCE is \
		  capable of transmitting(not less than 16Kbs \
                  and not exceeding the Access Link bandwidth). \
                  Several SVCs inside the netlink may use this \
                  bandwidth. Each SVC will consume a part of \
                  this bandwidth.\
		  (Operational);
	   HELP = ;
	   CAPABILITIES = ;"
	::= { mcmMSMDTELinkCfgEntry 3}

mcmMSMDTELinkCfgDCEReceiverBW OBJECT-TYPE
	SYNTAX	INTEGER (16000..2048000)
	ACCESS  read-only
     	STATUS  mandatory
     	DESCRIPTION
       	  "NAME = ; 
	   DESC = The value of this object represents the maximum \
		  throughput (in bits/sec) that the MPANL DTE is \
		  capable of transmitting(not less than 16Kbs \
                  and not exceeding the Access Link bandwidth). \
                  Several SVCs inside the netlink may use this \
                  bandwidth. Each SVC will consume a part of this \
                  bandwidth.\
		  (Operational);
	   HELP = ;
	   CAPABILITIES = ;"
	::= { mcmMSMDTELinkCfgEntry 4}

mcmMSMDTELinkCfgDTEMaxFrameSize OBJECT-TYPE
	SYNTAX	INTEGER (50..4100)
	ACCESS  read-only
     	STATUS   mandatory
     	DESCRIPTION
       	  "NAME = ; 
	   DESC = The value of this object represents the maximum \
		  frame size (in bytes) that the MPANL DCE is capable \
		  of transmitting. This value is signaled from the \
                  DTE to the DCE where it is used by the VoFR procedure \
                  in the wan driver to manage frame delay by fragmenting \
                  VoFR frames. Multiple VoFR frames can constitute a frame \
                  relay frame.\
		  (Operational);
	   HELP = ;
	   CAPABILITIES = ;"
	::= { mcmMSMDTELinkCfgEntry 5}

mcmMSMDTELinkCfgDCEMaxFrameSize OBJECT-TYPE
	SYNTAX	INTEGER (50..4100)
	ACCESS  read-only
     	STATUS  mandatory
     	DESCRIPTION
       	  "NAME = ; 
	   DESC = The value of this object represents the maximum \
		  frame size (in bytes) that the MPANL DTE is capable \
		  of transmitting. This value is signaled from the \
                  DTE to the DCE where it is used by the VoFR procedure \
                  in the wan driver to manage frame delay by fragmenting \
                  VoFR frames. Multiple VoFR frames can constitute a frame \
                  relay frame.\
		  (Operational);
	   HELP = ;
	   CAPABILITIES = ;"
	::= { mcmMSMDTELinkCfgEntry 6}


--  the MPANL DTE Signaling Line Parameters Table (Configuration Parameters)

--  This group provides the parameters to define the DTE line configuration
--  of the access device.  

nvmMSMDTELinkCfgTable OBJECT-TYPE
	SYNTAX  SEQUENCE OF NvmMSMDTELinkCfgEntry
	ACCESS  not-accessible
	STATUS  mandatory
	DESCRIPTION
 	  "NAME = ; 
    	   DESC = The MPANL DTE Signaling Line Parameters \
		  table. (Configuration);
   	   HELP = ;
    	   CAPABILITIES = NET_CFG, VPN_DISP;"
 	::= { configuration 4 }

nvmMSMDTELinkCfgEntry OBJECT-TYPE
	SYNTAX  NvmMSMDTELinkCfgEntry 
	ACCESS  not-accessible
	STATUS  mandatory
 	DESCRIPTION
   	  "NAME = ; 
    	   DESC = An entry in the MPANL DTE Signaling Line Parameters \
            		  table. (Configuration);
    	   HELP = ;
    	   CAPABILITIES = ;"
 	INDEX   { nvmMSMDTELinkCfgIfIndex }
	::= { nvmMSMDTELinkCfgTable 1 }

NvmMSMDTELinkCfgEntry ::= SEQUENCE {
        nvmMSMDTELinkCfgIfIndex         INTEGER,
	nvmMSMDTELinkCfgMaxSubChannelRange	INTEGER,
	nvmMSMDTELinkCfgDTEReceiverBW		INTEGER,
	nvmMSMDTELinkCfgDCEReceiverBW		INTEGER,
	nvmMSMDTELinkCfgDTEMaxFrameSize		INTEGER,
	nvmMSMDTELinkCfgDCEMaxFrameSize		INTEGER,
    nvmMSMDTELinkCfgEntryStatus         INTEGER
	}

nvmMSMDTELinkCfgIfIndex OBJECT-TYPE
    SYNTAX  INTEGER (1..255)
    ACCESS  read-write
    STATUS  mandatory
    DESCRIPTION
      "NAME = ;
       DESC = Ifindex associated with the MPANL DTE line  \
              parameters.;
       HELP = ;
       CAPABILITIES = ;"
    ::= { nvmMSMDTELinkCfgEntry 1 }


nvmMSMDTELinkCfgMaxSubChannelRange OBJECT-TYPE
	SYNTAX	INTEGER (17..255)
    	ACCESS  read-write
     	STATUS  mandatory
     	DESCRIPTION
          "NAME = ;
	   DESC = The value of this object represents the highest \
		  DLCI value to be used for sub-channel allocation. \
		  (Note that the current default value, 63, may change \
		  depending on the VoFR agreements.)  Note that user \
		  DLCIs on the MPANL start at DLCI-17.  This \
		  information is signaled from the DTE to the DCE. \
		  (Configuration);
	   HELP = ;
	   CAPABILITIES = ;"
	DEFVAL { 63 }
     	::= { nvmMSMDTELinkCfgEntry 2}

nvmMSMDTELinkCfgDTEReceiverBW OBJECT-TYPE
	SYNTAX	INTEGER (16000..2048000)
	ACCESS  read-write
     	STATUS  mandatory
     	DESCRIPTION
       	  "NAME = ; 
	   DESC = The value of this object represents the maximum \
		  throughput (in bits/sec) that the MPANL DCE is \
		  capable of transmitting(not less than 16Kbs \
                  and not exceeding the Access Link bandwidth). \
                  Several SVCs inside the netlink may use this \
                  bandwidth. Each SVC will consume a part of \
                  this bandwidth.\
		  (Configuration);
	   HELP = ;
	   CAPABILITIES = ;"
	::= { nvmMSMDTELinkCfgEntry 3}

nvmMSMDTELinkCfgDCEReceiverBW OBJECT-TYPE
	SYNTAX	INTEGER (16000..2048000)
	ACCESS  read-write
     	STATUS  mandatory
     	DESCRIPTION
       	  "NAME = ; 
	   DESC = The value of this object represents the maximum \
		  throughput (in bits/sec) that the MPANL DTE is \
		  capable of transmitting(not less than 16Kbs \
                  and not exceeding the Access Link bandwidth). \
                  Several SVCs inside the netlink may use this \
                  bandwidth. Each SVC will consume a part of this \
                  bandwidth.\
		  (Configuration);
	   HELP = ;
	   CAPABILITIES = ;"
	::= { nvmMSMDTELinkCfgEntry 4}

nvmMSMDTELinkCfgDTEMaxFrameSize OBJECT-TYPE
	SYNTAX	INTEGER (50..4100)
	ACCESS  read-write
     	STATUS  mandatory
     	DESCRIPTION
       	  "NAME = ; 
	   DESC = The value of this object represents the maximum \
		  frame size (in bytes) that the MPANL DCE is capable \
		  of transmitting. This value is signaled from the \
                  DTE to the DCE where it is used by the VoFR procedure \
                  in the wan driver to manage frame delay by fragmenting \
                  VoFR frames. Multiple VoFR frames can constitute a frame \
                  relay frame.\
		  (Configuration);
	   HELP = ;
	   CAPABILITIES = ;"
	::= { nvmMSMDTELinkCfgEntry 5}

nvmMSMDTELinkCfgDCEMaxFrameSize OBJECT-TYPE
	SYNTAX	INTEGER (50..4100)
	ACCESS  read-write
     	STATUS  mandatory
     	DESCRIPTION
       	  "NAME = ; 
	   DESC = The value of this object represents the maximum \
		  frame size (in bytes) that the MPANL DTE is capable \
		  of transmitting. This value is signaled from the \
                  DTE to the DCE where it is used by the VoFR procedure \
                  in the wan driver to manage frame delay by fragmenting \
                  VoFR frames. Multiple VoFR frames can constitute a frame \
                  relay frame.\
		  (Configuration);
	   CAPABILITIES = ;"
	::= { nvmMSMDTELinkCfgEntry 6}

nvmMSMDTELinkCfgEntryStatus OBJECT-TYPE
    SYNTAX  INTEGER {
        modify (1)
        }
        ACCESS  write-only
        STATUS 	obsolete 
        DESCRIPTION
          "NAME = ;
       DESC = This object was obsoleted as of release \
	      3.0; and is included here only to support \
	      backwards compatibility for software \
	      versions prior to release 3.0. \
	      This object is used in the MSM profile table \
              to add new rows, modify existing rows, \
              and delete existing rows. (Configuration);
       HELP = ;
       CAPABILITIES = ;"
    ::= { nvmMSMDTELinkCfgEntry 7  }



--  the MPANL Signaling Statistics Table (Operational Parameters - Counters)

--  This table provides counters to be used for monitoring during debugging and
--  performance characterization.  These counters are maintained as one per LAP-F
--  connection.

mcmMSMStatsLAPFConnTable OBJECT-TYPE
	SYNTAX  SEQUENCE OF McmMSMStatsLAPFConnEntry
	ACCESS  not-accessible
	STATUS  mandatory
	DESCRIPTION
	  "NAME = ; 
	   DESC = The MPANL Signaling Statistics for LAP-F connections \
		  table.; \
	   HELP = ;
	   CAPABILITIES = NET_CFG, VPN_DISP;"
	::= { statistics 1 }

mcmMSMStatsLAPFConnEntry OBJECT-TYPE
	SYNTAX  McmMSMStatsLAPFConnEntry
	ACCESS  not-accessible
	STATUS  mandatory
	DESCRIPTION
	  "NAME = ; 
	   DESC = An entry in the MPANL Signaling Statistics for LAP-F \
		  connections table.; \

	   HELP = ;
	   CAPABILITIES = ;"
	INDEX   { mcmMSMStatsLAPFConnIfIndex }
	::= { mcmMSMStatsLAPFConnTable 1 }

McmMSMStatsLAPFConnEntry ::= SEQUENCE {
	mcmMSMStatsLAPFConnIfIndex			INTEGER,
	mcmMSMStatsLAPFConnReestablished	Counter,
	mcmMSMStatsLAPFConnEstablished		Counter,
	mcmMSMStatsLAPFConnDisconnects		Counter,
	mcmMSMStatsProfileTxCnt				Counter,
	mcmMSMStatsProfileRxCnt				Counter,
	mcmMSMStatsRestartReqTxCnt			Counter,
	mcmMSMStatsRestartReqRxCnt			Counter,
	mcmMSMStatsDnaAssociationCnt		Counter,
	mcmMSMStatsDnaDeassociationCnt		Counter,
	mcmMSMStatsPANLInfoElementsTxCnt	Counter,
	mcmMSMStatsPANLInfoElementsRxCnt	Counter
	}

mcmMSMStatsLAPFConnIfIndex OBJECT-TYPE
	SYNTAX	INTEGER (1..255)
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = The value of this object corresponds to a value \
		  of IfIndex, and is used as the index into the \
		  MSM Statistics for LAP-F connections table.;
	   HELP = ;
	   CAPABILITIES = ;"
	::= { mcmMSMStatsLAPFConnEntry 1 }

mcmMSMStatsLAPFConnReestablished OBJECT-TYPE
	SYNTAX	Counter
    	ACCESS  read-only
     	STATUS  obsolete
     	DESCRIPTION
          "NAME = ;
           DESC = This object was obsoleted as of release \
	      	  3.0; and is included here only to support \
	          backwards compatibility for software \
	          versions prior to release 3.0. \
		  Counter representing the number of times the LAP-F \
		  connection was attempted to be re-established.  This counter \
		  indicates the number of CONNECT_REQ messages \
		  sent to the LAP-F module.;
	   HELP = ;
	   CAPABILITIES = ;"
     	::= { mcmMSMStatsLAPFConnEntry 2 }

mcmMSMStatsLAPFConnEstablished OBJECT-TYPE
	SYNTAX	Counter
	ACCESS  read-only
     	STATUS  mandatory
     	DESCRIPTION
       	  "NAME = ; 
	   DESC = Counter representing the number of times the LAP-F \
		  connection was established.  This counter indicates \
		  the number of CONNECT_IND messages sent by \
		 the LAP-F module.;
	   HELP = ;
	   CAPABILITIES = ;"
	::= { mcmMSMStatsLAPFConnEntry 3 }

mcmMSMStatsLAPFConnDisconnects OBJECT-TYPE
	SYNTAX	Counter
    	ACCESS  read-only
     	STATUS  mandatory
     	DESCRIPTION
          "NAME = ;
	   DESC = Counter representing the number of times a LAP-F \
		  connection was broken.  This counter indicates \
		  the number of DISCONNECT_IND messages sent by \
		 the LAP-F module.;
	   HELP = ;
	   CAPABILITIES = ;"
     	::= { mcmMSMStatsLAPFConnEntry 4 }

mcmMSMStatsProfileTxCnt OBJECT-TYPE
	SYNTAX	Counter
    	ACCESS  read-only
     	STATUS  mandatory
     	DESCRIPTION
          "NAME = ;
	   DESC = Counter represents the number of profiles \
		  sent without the occurrence of a restart request.;
	   HELP = ;
	   CAPABILITIES = ;"
     	::= { mcmMSMStatsLAPFConnEntry 5 }

mcmMSMStatsProfileRxCnt OBJECT-TYPE
	SYNTAX	Counter
    	ACCESS  read-only
     	STATUS  mandatory
     	DESCRIPTION
          "NAME = ;
	   DESC = Counter represents the number of profiles \
		  received without the occurrence of a restart request.;
	   HELP = ;
	   CAPABILITIES = ;"
     	::= { mcmMSMStatsLAPFConnEntry 6 }

mcmMSMStatsRestartReqTxCnt OBJECT-TYPE
	SYNTAX	Counter
    	ACCESS  read-only
     	STATUS  mandatory
     	DESCRIPTION
          "NAME = ;
	   DESC = Counter representing the number \
		  of restart requests sent.;
	   HELP = ;
	   CAPABILITIES = ;"
     	::= { mcmMSMStatsLAPFConnEntry 7 }

mcmMSMStatsRestartReqRxCnt OBJECT-TYPE
	SYNTAX	Counter
    	ACCESS  read-only
     	STATUS  mandatory
     	DESCRIPTION
          "NAME = ;
	   DESC = Counter representing the number \
		  of restart requests received.;
	   HELP = ;
	   CAPABILITIES = ;"
     	::= { mcmMSMStatsLAPFConnEntry 8 }

mcmMSMStatsDnaAssociationCnt OBJECT-TYPE
	SYNTAX	Counter
    	ACCESS  read-only
     	STATUS  mandatory
     	DESCRIPTION
          "NAME = ;
	   DESC = Counter representing the number \
		  of DNA prefix associations.;
	   HELP = ;
	   CAPABILITIES = ;"
     	::= { mcmMSMStatsLAPFConnEntry 9 }

mcmMSMStatsDnaDeassociationCnt OBJECT-TYPE
	SYNTAX	Counter
    	ACCESS  read-only
     	STATUS  mandatory
     	DESCRIPTION
          "NAME = ;
	   DESC = Counter representing the number \
		  of DNA prefix de-associations.;
	   HELP = ;
	   CAPABILITIES = ;"
     	::= { mcmMSMStatsLAPFConnEntry 10 }

mcmMSMStatsPANLInfoElementsTxCnt OBJECT-TYPE
	SYNTAX	Counter
    	ACCESS  read-only
     	STATUS  mandatory
