			qbDwsIotIfStatsSignalLossErrs,
			qbDwsIotIfStatsRxAal1Cells,
			qbDwsIotIfStatsTxAal1Cells
 		}
	    STATUS      current
    	DESCRIPTION
        	"The set of all accessible objects in the IOT Group of this MIB."
    	::= { qbDwsGroups 2 }

	qbDwsPktGroupInfo OBJECT-GROUP
		OBJECTS {
			qbAtmIfIndex,
