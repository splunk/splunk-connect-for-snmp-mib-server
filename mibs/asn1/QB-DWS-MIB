    	DESCRIPTION
        	"The set of all accessible objects in the Packet Group ofthis MIB."
    	::= { qbDwsGroups 3 }

qbDwsIotAddrGroupInfo OBJECT-GROUP
		OBJECTS {
			qbIotSerialNum 		
		}
	    STATUS      current
    	DESCRIPTION
        	"The set of all accessible objects in the IOT address group of this MIB."
    	::= { qbDwsGroups 4 }


