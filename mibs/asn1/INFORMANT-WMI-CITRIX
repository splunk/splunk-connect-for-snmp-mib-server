    ctxGTLicensePLD WtcsDisplayString,
    ctxGTLicensePLDFullName WtcsDisplayString,
    ctxGTLicenseSerialNumber WtcsDisplayString,
    ctxGTLicenseSubscriptionDate DateAndTime
}

