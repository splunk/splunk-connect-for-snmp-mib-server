    SYNTAX          DisplayString (SIZE(0..31))
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "Identifies the serial number of the node which created the
             parameter file loaded by this node."
    ::= { sonusParamStatusObjects 16 }

sonusParamSavePrimarySrvrState OBJECT-TYPE
    SYNTAX          INTEGER { unknown(1), failed(2), failing(3), 
                        behind(4), current(5), ahead(6) }
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "The current state of parameter saving to this NFS server."
    ::= { sonusParamStatusObjects 17 }

sonusParamSavePrimarySrvrReason OBJECT-TYPE
    SYNTAX          INTEGER { success(1), create(2), fopen(3), header(4), 
                        timer(5), nfs(6), flush(7), write(8), close(9), 
                        move(10), memory(11), none(12) }
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "The failure code associated with the last parameter save
             pass to this NFS server."
    ::= { sonusParamStatusObjects 18 }

sonusParamSaveSecondarySrvrState OBJECT-TYPE
