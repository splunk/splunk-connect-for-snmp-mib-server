sonusNfs OBJECT IDENTIFIER
    ::= { sonusNodeMIBObjects 6 }

sonusNfsAdmnTable OBJECT-TYPE
    SYNTAX          SEQUENCE OF SonusNfsAdmnEntry
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION     
            "This table specifies the configurable NFS options for each MNS in each shelf."
    ::= { sonusNfs 1 }

sonusNfsAdmnEntry OBJECT-TYPE
    SYNTAX          SonusNfsAdmnEntry
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION     
            ""
    INDEX           { sonusNfsAdmnShelfIndex, sonusNfsAdmnSlotIndex }
    ::= { sonusNfsAdmnTable 1 }

SonusNfsAdmnEntry ::=
    SEQUENCE {
        sonusNfsAdmnShelfIndex          Integer32,
        sonusNfsAdmnSlotIndex           Integer32,
        sonusNfsAdmnMountServer         INTEGER,
        sonusNfsAdmnUnmountServer       INTEGER,
        sonusNfsAdmnToggleActiveServer  INTEGER,
        sonusNfsAdmnSetWritable         INTEGER
    }

sonusNfsAdmnShelfIndex OBJECT-TYPE
    SYNTAX          Integer32 (1..6)
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "Identifies the target shelf.  This object returns the value of
             sonusNodeShelfStatIndex which was used to index into this table."
    ::= { sonusNfsAdmnEntry 1 }

sonusNfsAdmnSlotIndex OBJECT-TYPE
    SYNTAX          Integer32 (1..2)
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "Identifies the target MNS module slot within the shelf."
    ::= { sonusNfsAdmnEntry 2 }

sonusNfsAdmnMountServer OBJECT-TYPE
    SYNTAX          INTEGER { noop(1), primary(2), secondary(3) }
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION     
            "Mounts the Primary or Secondary NFS server using mount parameters
             obtained from the NVS Boot parameters sonusBparamNfsPrimary or
             sonusBparamNfsSecondary, and sonusBparamNfsMountPri or
             sonusBparamNfsMountSec.  Continuous retries will occur until the mount
             succeeds.  This object is always read as noop(1)."
    ::= { sonusNfsAdmnEntry 3 }

sonusNfsAdmnUnmountServer OBJECT-TYPE
    SYNTAX          INTEGER { noop(1), standby(2) }
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION     
            "Unmounts the standby NFS server.  Note: unmounting this server will
             disrupt any file I/O currently taking place on it.  Continuous retries
             will occur until the unmount succeeds.  This object is always read as
             noop(1)."
    ::= { sonusNfsAdmnEntry 4 }

sonusNfsAdmnToggleActiveServer OBJECT-TYPE
    SYNTAX          INTEGER { noop(1), toggle(2) }
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION     
            "Toggles the Active NFS server between the Primary and Secondary,
             provided both servers are currently mounted.  This object is always
             read as noop(1)."
    ::= { sonusNfsAdmnEntry 5 }

sonusNfsAdmnSetWritable OBJECT-TYPE
    SYNTAX          INTEGER { noop(1), primary(2), secondary(3) }
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION     
            "Clears the perception of a read-only condition on the Primary or
             Secondary NFS server, so that server will be considered viable as
             the Active server.  This object is always read as noop(1)."
    ::= { sonusNfsAdmnEntry 6 }

sonusNfsStatTable OBJECT-TYPE
    SYNTAX          SEQUENCE OF SonusNfsStatEntry
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION     
            "This table specifies NFS status objects for each MNS in each shelf."
    ::= { sonusNfs 2 }

sonusNfsStatEntry OBJECT-TYPE
    SYNTAX          SonusNfsStatEntry
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION     
            ""
    INDEX           { sonusNfsStatShelfIndex, sonusNfsStatSlotIndex }
    ::= { sonusNfsStatTable 1 }

SonusNfsStatEntry ::=
    SEQUENCE {
        sonusNfsStatShelfIndex          Integer32,
        sonusNfsStatSlotIndex           Integer32,
        sonusNfsStatPrimaryStatus       INTEGER,
        sonusNfsStatSecondaryStatus     INTEGER,
        sonusNfsStatActiveServer        INTEGER,
        sonusNfsStatStandbyServer       INTEGER,
        sonusNfsStatPrimaryIP           IpAddress,
        sonusNfsStatSecondaryIP         IpAddress,
        sonusNfsStatPrimaryMount        DisplayString,
        sonusNfsStatSecondaryMount      DisplayString,
        sonusNfsStatPrimaryLastError    INTEGER,
        sonusNfsStatSecondaryLastError  INTEGER
    }

sonusNfsStatShelfIndex OBJECT-TYPE
    SYNTAX          Integer32 (1..6)
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "Identifies the target shelf.  This object returns the value of
             sonusNodeShelfStatIndex which was used to index into this table."
    ::= { sonusNfsStatEntry 1 }

sonusNfsStatSlotIndex OBJECT-TYPE
    SYNTAX          Integer32 (1..2)
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "Identifies the target MNS module slot within the shelf."
    ::= { sonusNfsStatEntry 2 }

sonusNfsStatPrimaryStatus OBJECT-TYPE
    SYNTAX          INTEGER { mounted(1), mounting(2), unmounted(3), 
                        unmounting(4), readOnly(5), testing(6), 
                        failed(7), invalid(8) }
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "Indicates the mount status of the Primary NFS server: mounted and
             functioning properly, attemping to mount, indefinitely unmounted,
             attempting to unmount, mounted but read-only, testing connectivity,
             server failure, or invalid NFS parameters."
    ::= { sonusNfsStatEntry 7 }

sonusNfsStatSecondaryStatus OBJECT-TYPE
    SYNTAX          INTEGER { mounted(1), mounting(2), unmounted(3), 
                        unmounting(4), readOnly(5), testing(6), 
                        failed(7), invalid(8) }
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "Indicates the mount status of the Secondary NFS server: mounted and
             functioning properly, attemping to mount, indefinitely unmounted,
             attempting to unmount, mounted but read-only, testing connectivity,
             server failure, or invalid NFS parameters."
    ::= { sonusNfsStatEntry 8 }

sonusNfsStatActiveServer OBJECT-TYPE
    SYNTAX          INTEGER { none(1), primary(2), secondary(3) }
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "Indicates the current Active NFS server.  This may change due to NFS
             failures or NFS administrative operations."
    ::= { sonusNfsStatEntry 9 }

sonusNfsStatStandbyServer OBJECT-TYPE
    SYNTAX          INTEGER { none(1), primary(2), secondary(3) }
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "Indicates the current Standby NFS server.  This may change due to NFS
             failures or NFS administrative operations."
    ::= { sonusNfsStatEntry 10 }

sonusNfsStatPrimaryIP OBJECT-TYPE
    SYNTAX          IpAddress
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "The IP Address of the currently mounted Primary NFS server.  This may
             differ from the NVS settings if the user modified sonusBparamNfsPrimary
             without unmounting and remounting the Primary server."
    ::= { sonusNfsStatEntry 11 }

sonusNfsStatSecondaryIP OBJECT-TYPE
    SYNTAX          IpAddress
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "The IP Address of the currently mounted Secondary NFS server.  This
             may differ from the NVS settings if the user modified
             sonusBparamNfsSecondary without unmounting and remounting the Secondary
             server."
    ::= { sonusNfsStatEntry 12 }

sonusNfsStatPrimaryMount OBJECT-TYPE
    SYNTAX          DisplayString (SIZE(2..32))
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "The mount point currently in use on the Primary NFS server.  This may
             differ from the NVS settings if the user modified sonusBparamNfsMountPri
             without unmounting and remounting the Primary server."
    ::= { sonusNfsStatEntry 13 }

sonusNfsStatSecondaryMount OBJECT-TYPE
    SYNTAX          DisplayString (SIZE(2..32))
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "The mount point currently in use on the Secondary NFS server.  This may
             differ from the NVS settings if the user modified sonusBparamNfsMountSec
             without unmounting and remounting the Secondary server."
    ::= { sonusNfsStatEntry 14 }

sonusNfsStatPrimaryLastError OBJECT-TYPE
    SYNTAX          INTEGER { none(1), badVolumeStructure(2), 
                        tooManyFiles(3), volumeFull(4), 
                        serverHardError(5), quotaExceeded(6), 
                        staleNfsHandle(7), nfsTimeout(8), 
                        rpcCanNotSend(9), noAccess(10), other(11) }
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "Last consequential error received from the Primary NFS server.
             This object is reset to none(1) if the server is remounted."
    ::= { sonusNfsStatEntry 15 }

sonusNfsStatSecondaryLastError OBJECT-TYPE
    SYNTAX          INTEGER { none(1), badVolumeStructure(2), 
                        tooManyFiles(3), volumeFull(4), 
                        serverHardError(5), quotaExceeded(6), 
                        staleNfsHandle(7), nfsTimeout(8), 
                        rpcCanNotSend(9), noAccess(10), other(11) }
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "Last consequential error received from the Secondary NFS server.
             This object is reset to none(1) if the server is remounted."
    ::= { sonusNfsStatEntry 16 }

