    SYNTAX          Integer32 (1..16)
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "Identifies the target slot within the shelf."
    ::= { sonusNodeSlotAdmnEntry 2 }

sonusNodeSlotAdmnReset OBJECT-TYPE
    SYNTAX          INTEGER { undefined(1), reset(2), coredump(3) }
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION     
            "Setting this object to reset(2) or coredump(3), will physically reset 
             the server module installed in the indexed slot.  This
             object always reads as undefined(1).   This object bypasses
             all consistency and safety checks.  This object is 
             intended for evaluation testing."
    ::= { sonusNodeSlotAdmnEntry 3 }

sonusNvsConfig OBJECT IDENTIFIER
    ::= { sonusNodeMIBObjects 2 }

sonusNvsConfigTable OBJECT-TYPE
    SYNTAX          SEQUENCE OF SonusNvsConfigEntry
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION     
            "This table specifies the Boot Parameters that apply only to
             the MNS present in the indexed shelf and slot.  The Boot
             Parameters can only be accessed if the shelf is actively
             part of the node."
    ::= { sonusNvsConfig 1 }

sonusNvsConfigEntry OBJECT-TYPE
    SYNTAX          SonusNvsConfigEntry
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION     
            ""
    INDEX           { sonusBparamShelfIndex }
    ::= { sonusNvsConfigTable 1 }

SonusNvsConfigEntry ::=
    SEQUENCE {
        sonusBparamShelfIndex           Integer32,
        sonusBparamUnused               Integer32,
        sonusBparamPasswd               DisplayString,
        sonusBparamIpAddrSlot1Port0     IpAddress,
        sonusBparamIpMaskSlot1Port0     IpAddress,
        sonusBparamIpGatewaySlot1Port0  IpAddress,
        sonusBparamIfSpeedTypeSlot1Port0 INTEGER,
        sonusBparamIpAddrSlot1Port1     IpAddress,
        sonusBparamIpMaskSlot1Port1     IpAddress,
        sonusBparamIpGatewaySlot1Port1  IpAddress,
        sonusBparamIfSpeedTypeSlot1Port1 INTEGER,
        sonusBparamIpAddrSlot1Port2     IpAddress,
        sonusBparamIpMaskSlot1Port2     IpAddress,
        sonusBparamIpGatewaySlot1Port2  IpAddress,
        sonusBparamIfSpeedTypeSlot1Port2 INTEGER,
        sonusBparamIpAddrSlot2Port0     IpAddress,
        sonusBparamIpMaskSlot2Port0     IpAddress,
        sonusBparamIpGatewaySlot2Port0  IpAddress,
        sonusBparamIfSpeedTypeSlot2Port0 INTEGER,
        sonusBparamIpAddrSlot2Port1     IpAddress,
        sonusBparamIpMaskSlot2Port1     IpAddress,
        sonusBparamIpGatewaySlot2Port1  IpAddress,
        sonusBparamIfSpeedTypeSlot2Port1 INTEGER,
        sonusBparamIpAddrSlot2Port2     IpAddress,
        sonusBparamIpMaskSlot2Port2     IpAddress,
        sonusBparamIpGatewaySlot2Port2  IpAddress,
        sonusBparamIfSpeedTypeSlot2Port2 INTEGER,
        sonusBparamBootDelay            Integer32,
        sonusBparamCoreState            SonusAdminState,
        sonusBparamCoreLevel            INTEGER,
        sonusBparamBaudRate             Integer32,
        sonusBparamNfsPrimary           IpAddress,
        sonusBparamNfsSecondary         IpAddress,
        sonusBparamNfsMountPri          DisplayString,
        sonusBparamNfsMountSec          DisplayString,
        sonusBparamNfsPathUpgrade       DisplayString,
        sonusBparamNfsPathLoad          DisplayString,
        sonusBparamPrimName             DisplayString,
        sonusBparamSecName              DisplayString,
        sonusBparamMasterState          INTEGER,
        sonusBparamParamMode            INTEGER,
        sonusBparamCliScript            DisplayString,
        sonusBparamUId                  Integer32,
        sonusBparamGrpId                Integer32,
        sonusBparamCoreDumpLimit        Integer32,
        sonusBparamNfsPathSoftware      DisplayString
    }

