qmsIFAdmin	OBJECT IDENTIFIER ::= { qmsIF 1 }
qmsIFSetup	OBJECT IDENTIFIER ::= { qmsIF 2 }

qmsIFConfig        OBJECT IDENTIFIER ::= { qmsIFSetup 1 }
qmsIFNetware       OBJECT IDENTIFIER ::= { qmsIFSetup 5 }
qmsIFApple         OBJECT IDENTIFIER ::= { qmsIFSetup 6 }
qmsIFSnmpcontrol   OBJECT IDENTIFIER ::= { qmsIFSetup 8 }
qmsIFLservlmgr     OBJECT IDENTIFIER ::= { qmsIFSetup 9 }

qmsCfgUser	OBJECT IDENTIFIER ::= {qmsConfig 10}
qmsCfgAdmin	OBJECT IDENTIFIER ::= {qmsConfig 20}
qmsCfgSecurity	OBJECT IDENTIFIER ::= {qmsConfig 30}

-- The System Group

qmsPtrSysStatus OBJECT-TYPE
	SYNTAX	DisplayString (SIZE (0..255))
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION
			"A text string indicating the current printer status."
	::= { qmsPtrSys 1 }

qmsPtrSysNamePrinter OBJECT-TYPE
	SYNTAX	DisplayString (SIZE (0..255))
	ACCESS	read-only
	STATUS	optional
	DESCRIPTION
			"A text string indicating the current printer name."
	::= { qmsPtrSys 2 }

-- The Interfaces Table

qmsPtrIfTable OBJECT-TYPE
	SYNTAX	SEQUENCE OF QMSPtrIfEntry
	ACCESS	not-accessible
	STATUS	mandatory
	DESCRIPTION
			"A list of interfaces supported by this printer and
			their currently selected default emulations."
	::= { qmsPtrSys 3 }

qmsPtrIfEntry OBJECT-TYPE
	SYNTAX	QMSPtrIfEntry
	ACCESS	not-accessible
	STATUS	mandatory
	DESCRIPTION
			"An interface entry containing an interface and it
			currently selected dafault emulation."
	INDEX	{ qmsPtrIfIndex }
	::= { qmsPtrIfTable 1 }

QMSPtrIfEntry ::=
	SEQUENCE {
		qmsPtrIfIndex
			INTEGER,
		qmsPtrIfName
			DisplayString (SIZE (0..255)),
		qmsPtrIfDefEmu
			INTEGER
	}

qmsPtrIfIndex OBJECT-TYPE
	SYNTAX	INTEGER
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION
			"A unique value for each interface.  Its value ranges between 1
			and the number of intrfaces, qmsPtrIfNumber.  This value must
			remain the same between successive printer power cycles."
	::= { qmsPtrIfEntry 1 }

qmsPtrIfName OBJECT-TYPE
	SYNTAX	DisplayString (SIZE (0..255))
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION
			"The name of the interface."
	::= { qmsPtrIfEntry 2 }

qmsPtrIfDefEmu OBJECT-TYPE
	SYNTAX	INTEGER {
				ultrascript(1),		--  following values have been incremented by one in
				ibm-proprinter(2),	--  observance of the rule against using an OID of 0.
				diablo-630(3),
				ti-855(4),
				hp-gl(5),
				hp-pcl(6),
				hex-dump(7),
				ti-855-dp(8),
				ti-810(9),
				epson-fx(10),
				dec-ansi(11),
				ibm-ext-ascii(12),
				ln03(13),
				tektronix-4014(14),
				quic-2(15),
				impress(16),
				code-v(17),
				r1(18),
				ccitt(19),
				line-printer(20),
				hp-pcl-5(21),
				esp(201),
				not-supported(65534),	--  Special case, this variable not supported by the printer.
				wild-card(65535)		--  Left at original value to stay within a word.
			}
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION
			"The name of the currently selected default emulation
			for this interface."
	::= { qmsPtrIfEntry 3 }

-- The Input/Output Bins Table

qmsPtrBinTable OBJECT-TYPE
	SYNTAX	SEQUENCE OF QMSPtrBinEntry
	ACCESS	not-accessible
	STATUS	mandatory
	DESCRIPTION
			"A list of input/output paper trays supported by
			this printer and their currently selected paper sizes
			and bin names.  Note: paper sizes are not assigned to
			output paper trays."
	::= { qmsPtrSys 4 }

qmsPtrBinEntry OBJECT-TYPE
	SYNTAX	QMSPtrBinEntry
	ACCESS	not-accessible
	STATUS	mandatory
	DESCRIPTION
			"A paper tray entry containing its currently selected
			default paper size and paper tray name."
	INDEX	{ qmsPtrBinIndex }
	::= { qmsPtrBinTable 1 }

QMSPtrBinEntry ::=
	SEQUENCE {
		qmsPtrBinIndex		INTEGER,
		qmsPtrBinType		INTEGER,
		qmsPtrBinId			INTEGER,
		qmsPtrBinPaper		DisplayString (SIZE (0..255)),
		qmsPtrBinName		DisplayString (SIZE (0..255))
	}

qmsPtrBinIndex OBJECT-TYPE
	SYNTAX	INTEGER
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION
			"A unique value for each input/output bin.  Its value ranges
			between 1 and the number of input/output bins, qmsPtrBinNumber.
			This value must remain the same between successive printer power
			cycles."
	::= { qmsPtrBinEntry 1 }

qmsPtrBinType OBJECT-TYPE
	SYNTAX	INTEGER {
			output(1),	-- must use as passed by the MB.
			input(2)	-- was "output(0), input(1)"
			}
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION
			"The input/output tray designator (input=1, output=0)."
	::= { qmsPtrBinEntry 2 }

qmsPtrBinId OBJECT-TYPE
	SYNTAX	INTEGER
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION
			"The input tray designator."
	::= { qmsPtrBinEntry 3 }

qmsPtrBinPaper OBJECT-TYPE
	SYNTAX	DisplayString (SIZE (0..255))
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION
			"The name of the currently selected paper size for this
			paper tray (applies to input paper trays only)."
	::= { qmsPtrBinEntry 4 }

qmsPtrBinName OBJECT-TYPE
	SYNTAX	DisplayString (SIZE (0..255))
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION
			"The name currently assigned to this paper tray."
	::= { qmsPtrBinEntry 5 }

-- The Emulations Group

-- The Jobs Group
-- The Current Job

qmsPtrJobsCurStatus OBJECT-TYPE
	SYNTAX	INTEGER {
					spooling(1),
					interpreting(2),
					rasterizing(4),
					printing(8),
					terminating(16),
					waiting(32),
					spooled(64),
                                        printed(128),
                                        canceled(256),
					internal(32768)
				}
		ACCESS	read-only
		STATUS	mandatory
		DESCRIPTION
				"The current job entries status."
		::= { qmsPtrJobs 1 }

	qmsPtrJobsCurSheet OBJECT-TYPE
		SYNTAX	INTEGER
		ACCESS	read-only
		STATUS	mandatory
		DESCRIPTION
				"The sheet being imaged for the current job."
		::= { qmsPtrJobs 2 }

	qmsPtrJobsCurPage OBJECT-TYPE
		SYNTAX	INTEGER
		ACCESS	read-only
		STATUS	mandatory
		DESCRIPTION
				"The page being imaged for the current job."
		::= { qmsPtrJobs 3 }

-- The Jobs Table
-- (includes all jobs currently being processed by the printer)

qmsPtrJobsTable OBJECT-TYPE
	SYNTAX	SEQUENCE OF QMSPtrJobsEntry
	ACCESS	not-accessible
	STATUS	mandatory
	DESCRIPTION
			"A list of the status, byte size, emulation, page number,
			sheet number, channel number, comment string, job title,
			and job originator string for all jobs currently being
			processed by the printer."
	::= { qmsPtrJobs 4 }

qmsPtrJobsEntry OBJECT-TYPE
	SYNTAX	QMSPtrJobsEntry
	ACCESS	not-accessible
	STATUS	mandatory
	DESCRIPTION
			"The current job entry containing its current status, sheet
			number, and page number."
	INDEX	{ qmsPtrJobsIndex }
	::= { qmsPtrJobsTable 1 }

QMSPtrJobsEntry ::=
	SEQUENCE {
		qmsPtrJobsIndex				INTEGER,
		qmsPtrJobsStatus			INTEGER,
		qmsPtrJobsEmulation			INTEGER,
		qmsPtrJobsPage				INTEGER,
		qmsPtrJobsSheet				INTEGER,
		qmsPtrJobsChannel			INTEGER,
		qmsPtrJobsComment			DisplayString (SIZE (0..255)),
		qmsPtrJobsTitle				DisplayString (SIZE (0..255)),
		qmsPtrJobsOwner				DisplayString (SIZE (0..255))
	}

qmsPtrJobsIndex OBJECT-TYPE
	SYNTAX	INTEGER
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION
			"A unique value for each currently active job.  Its value ranges
			between 1 and the number of currently active jobs,
			qmsPtrJobsNumber.  This value must remain the same between
			successive printer power cycles."
	::= { qmsPtrJobsEntry 1 }

qmsPtrJobsStatus OBJECT-TYPE
	SYNTAX	INTEGER {
				spooling(1),
				interpreting(2),
				rasterizing(4),
				printing(8),
				terminating(16),
				waiting(32),
				spooled(64),
                                printed(128),
                                canceled(256),
				internal(32768)
			}
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION
			"The job status."
	::= { qmsPtrJobsEntry 2 }

qmsPtrJobsEmulation OBJECT-TYPE
	SYNTAX	INTEGER {
				ultrascript(1),		-- actual value should be 0 (zero).
				ibm-proprinter(2),
				diablo-630(3),
				ti-855(4),
				hp-gl(5),
				hp-pcl(6),
				hex-dump(7),
				ti-855-dp(8),
				ti-810(9),
				epson-fx(10),
				dec-ansi(11),
				ibm-ext-ascii(12),
				ln03(13),
				tektronix-4014(14),
				quic-2(15),
				impress(16),
				code-v(17),
				r1(18),
				ccitt(19),
				line-printer(20),
				hp-pcl-5(21),
                                tiff(22),
                                cals(23),
                                cgm(25),
                                hp-pcl-xl(26),
				esp(201),
				wild-card(65536)
			}
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION
			"The emulation selected for the processing of this job."
	::= { qmsPtrJobsEntry 4 }

qmsPtrJobsPage OBJECT-TYPE
	SYNTAX	INTEGER
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION
			"The number of pages defined for this job.  Note: Sheets
			are defined as the physical sheet of paper as opposed to
			the number of logical page images appearing on the physical
			sheet of paper."
	::= { qmsPtrJobsEntry 5 }

qmsPtrJobsSheet OBJECT-TYPE
	SYNTAX	INTEGER
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION
			"The number of sheets defined for this job.  Note: Sheets
			are defined as the physical sheet of paper as opposed to
			the number of logical page images appearing on the physical
			sheet of paper."
	::= { qmsPtrJobsEntry 6 }

qmsPtrJobsChannel OBJECT-TYPE
	SYNTAX	INTEGER {
				other(1),	-- was "other(0)"
				rs-232(12),
				parallel(13),
				appletalk(14),
				optional-io(16)
			}
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION
			"The interface over which this job was communicated to
			the printer."
	::= { qmsPtrJobsEntry 7 }

qmsPtrJobsComment OBJECT-TYPE
	SYNTAX	DisplayString (SIZE (0..255))
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION
			"A user defined text string sent with this job."
	::= { qmsPtrJobsEntry 8 }

qmsPtrJobsTitle OBJECT-TYPE
	SYNTAX	DisplayString (SIZE (0..255))
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION
			"A user defined text string, usually the name of the job."
	::= { qmsPtrJobsEntry 9 }

qmsPtrJobsOwner OBJECT-TYPE
	SYNTAX	DisplayString (SIZE (0..255))
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION
			"A user defined text string, usually the username of the
			person originating the job."
	::= { qmsPtrJobsEntry 10 }

-- The Job Completion Statistics
--     Accumulation of all jobs processed by the printer since the
--     last power-up.

qmsPtrJobsStatPages OBJECT-TYPE
	SYNTAX	INTEGER
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION
			"The total number of pages printed for all jobs processed
			by the printer since the last power cycle.  Note: Sheets
			are defined as the physical sheet of paper as opposed to
			the number of logical page images appearing on the physical
			sheet of paper."
	::= { qmsPtrJobs 6 }

qmsPtrJobsStatSheets OBJECT-TYPE
	SYNTAX	INTEGER
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION    
			"The total number of sheets printed for all jobs processed
			by the printer since the last power cycle.  Note: Sheets
			are defined as the physical sheet of paper as opposed to
			the number of logical page images appearing on the physical
			sheet of paper."
	::= { qmsPtrJobs 7 }



--- The Optional I/O Administration group

QmsIFAdminMenu ::= SEQUENCE {
        qmsIFAdminUnitStatus           INTEGER,
        qmsIFAdminConfigStatus         INTEGER,
        qmsIFAdminOldIntAddr           IpAddress
}

qmsIFAdminUnitStatus OBJECT-TYPE
        SYNTAX  INTEGER {
                running(1),
                resetOnNoClient(2),
                hardReset(3)
        }
        ACCESS  read-write
        STATUS  mandatory
        DESCRIPTION
        "The units's operational status.  A value other
		than (1) will reset the unit."
        ::= { qmsIFAdmin 1 }

qmsIFAdminConfigStatus OBJECT-TYPE
        SYNTAX  INTEGER {
                configUnknown1(1),
                configUnknown2(2),
                configLoadRequest(3),
                configSaveRequest(4),
                configDefaultRequest(5)
        }
        ACCESS  read-write
        STATUS  mandatory
        DESCRIPTION
        "The units's configuration status.
         read-only values:
           (1) reserved for future use.
           (2) reserved for future use.
         write-only values :
           (3) will load permanent values .
           (4) will save current configuration permanently.
           (5) load default configuration."
        ::= { qmsIFAdmin 2 }


qmsIFAdminOldIntAddr OBJECT-TYPE
        SYNTAX  IpAddress
        ACCESS  read-write
        STATUS  mandatory
        DESCRIPTION
        "The Internet Address at power-on or before change by
         RARP or 'arp -s'."
        ::= { qmsIFAdmin 3 }



--- The Optional I/O Configuration group
--- These objects correspond to the parameters of the product.

    -- The Configuration Menu

QmsIFConfigMenu ::= SEQUENCE {
        intAddr			IpAddress,
        ethAddr			NetworkAddress,
        defRout			IpAddress,
        netMask			IpAddress,
        tcpEnb			INTEGER,
        tftpEnb			INTEGER,
        tnPort			INTEGER,
        rtnOpt			INTEGER,
        trP1            INTEGER,
        bootP           INTEGER,
        rarp            INTEGER
}


intAddr OBJECT-TYPE
        SYNTAX  IpAddress
        ACCESS  read-write
        STATUS  mandatory
        DESCRIPTION
        "The Internet Address."
        ::= { qmsIFConfig 1 }

ethAddr OBJECT-TYPE
        SYNTAX  NetworkAddress
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION
        "The Ethernet Address."
        ::= { qmsIFConfig 2 }

defRout OBJECT-TYPE
        SYNTAX  IpAddress
        ACCESS  read-write
        STATUS  mandatory
        DESCRIPTION
        "The Default Router."
        ::= { qmsIFConfig 3 }

netMask OBJECT-TYPE
        SYNTAX  IpAddress
        ACCESS  read-write
        STATUS  mandatory
        DESCRIPTION
        "The Net Mask."
        ::= { qmsIFConfig 4 }

tcpEnb  OBJECT-TYPE
        SYNTAX  INTEGER {
                no(1),
                yes(2)
        }
        ACCESS  read-write
        STATUS  mandatory
        DESCRIPTION
        "The TCP protocol stack enable switch."
        ::= { qmsIFConfig 5 }


tftpEnb OBJECT-TYPE
        SYNTAX  INTEGER {
                no(1),
                yes(2)
        }
        ACCESS  read-write
        STATUS  mandatory
        DESCRIPTION
        "The TFTP protocol enable switch."
        ::= { qmsIFConfig 13 }

tnPort  OBJECT-TYPE
        SYNTAX  INTEGER
        ACCESS  read-write
        STATUS  obsolete
        DESCRIPTION
        "The Telnet TCP Port.

        This object is obsolete."
        ::= { qmsIFConfig 16 }

rtnOpt  OBJECT-TYPE
        SYNTAX  INTEGER {
                no(1),
                yes(2)
        }
        ACCESS  read-write
        STATUS  obsolete
        DESCRIPTION
        "The Reverse Telnet option handling switch.

        This object is obsolete."
        ::= { qmsIFConfig 18 }

trP1  OBJECT-TYPE
        SYNTAX  INTEGER
        ACCESS  read-write
        STATUS  mandatory
        DESCRIPTION
        "The Transport 1 Port."
        ::= { qmsIFConfig 30 }

bootP  OBJECT-TYPE
        SYNTAX  INTEGER {
			yes(2),
			no(1)
        }
        ACCESS  read-write
        STATUS  mandatory
        DESCRIPTION
        "Boot Strap protocal enable/disable."
        ::= { qmsIFConfig 698 }

rarp  OBJECT-TYPE
        SYNTAX  INTEGER {
			yes(2),
			no(1)
        }
        ACCESS  read-write
        STATUS  mandatory
        DESCRIPTION
        "Reverse address resolution enable/disable."
        ::= { qmsIFConfig 699 }

spooling OBJECT-TYPE
        SYNTAX  INTEGER {
			on(1),
			onInputIdle(2),
			onIdle(3)
        }
        ACCESS  read-write
        STATUS  mandatory
        DESCRIPTION
        "Job spooling option."
        ::= { qmsIFConfig 9000 }
	

    -- The Netware Menu

QmsIFNetwareMenu ::= SEQUENCE {
        netwEnb           INTEGER,
        psName           DisplayString,
        confServ           DisplayString,
        msgLog           INTEGER,
        nwMode           INTEGER,
        psPoll           INTEGER,
        fr-802-3           INTEGER,
        fr-eth-2           INTEGER,
        fr-802-2           INTEGER,
        fr-snap           INTEGER,
        login1           DisplayString,
        login2           DisplayString,
        login3           DisplayString,
        login4           DisplayString,
        login5           DisplayString,
        login6           DisplayString,
        login7           DisplayString,
        login8           DisplayString,
        login9           DisplayString,
        login10           DisplayString,
        login11           DisplayString,
        login12           DisplayString,
        login13           DisplayString,
        login14           DisplayString,
        login15           DisplayString,
        login16           DisplayString,
        rprinter1           DisplayString,
        rprinter2           DisplayString,
        rprinter3           DisplayString,
        rprinter4           DisplayString,
        rprinter5           DisplayString,
        rprinter6           DisplayString,
        rprinter7           DisplayString,
        rprinter8           DisplayString
}


