        -- The Appletalk Menu

QmsIFAppletalkMenu ::= SEQUENCE {
        atlkEnb           INTEGER,
        atlkZone           DisplayString,
        atlkZonerEnb           INTEGER,
        atlkLWEnb           INTEGER,
        atlkQMSRemConEnb           INTEGER,
        atlkConnType           INTEGER
}

atlkEnb OBJECT-TYPE
        SYNTAX INTEGER {
                no(1),
                yes(2)
        }
        ACCESS  read-write
        STATUS  mandatory
        DESCRIPTION
        "The AppleTalk protocol stack enable switch."
        ::= { qmsIFApple 500 }


