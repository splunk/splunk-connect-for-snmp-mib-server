ch2LimHyst OBJECT-TYPE
	SYNTAX		DisplayString (SIZE (0..8))
	ACCESS		read-only
	STATUS		mandatory
	DESCRIPTION	"Channel 2 temperature hysteressis."
::= { channel2 7 }

ch2Delay OBJECT-TYPE
	SYNTAX		INTEGER (0..65534)
	ACCESS		read-only
	STATUS		mandatory
	DESCRIPTION	"Channel 2 temperature alarm delay [s]."
::= { channel2 8 }

-----------------------------------------------------------------------
-- channel3
-----------------------------------------------------------------------
