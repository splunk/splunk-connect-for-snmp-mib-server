--
-- Unit Configuration Group
--

-- ============================================================================
--
-- The following variables describe the (not-user-settable) configuration
-- of the unit: its type, number of ports, etc.
--
dlcUnitHwConfig OBJECT IDENTIFIER ::= { dl-new-t1 1 }

 dlcUnitModelType OBJECT-TYPE
  SYNTAX INTEGER {
   vxEncore         ( 1),
   dl100Encore      ( 2),
   dl1200           ( 3),
   dl2400           ( 4),
   dl600            ( 5),
   dl600Encore      ( 6),
   ensembleEncore   ( 7),
   soloEncore       ( 8),
   soloE1Encore     ( 9),
   ensembleE1Encore (10),
   soloT1CSU        (11),
   ensembleT1CSU    (12),
   soloSelectT1Module (13),
   soloSelectE1Module (14),
   soloSelectT1     (15),
   soloSelectE1     (16),
   dl4200T1         (17),
   dl4200E1         (18),
   dl5440T1         (19),
   dl5440E1         (20)
  }
  ACCESS read-only
  STATUS mandatory
  DESCRIPTION
   "The unit model type."
  --DEFAULT test-function it_exists
  ::= { dlcUnitHwConfig 1 }


 dlcUnitHwRev OBJECT-TYPE
  SYNTAX DisplayString (SIZE (1..4))
  ACCESS read-only
  STATUS mandatory
  DESCRIPTION
   "The ASCII code of the hardware revision number."
  --DEFAULT test-function it_exists
  ::= { dlcUnitHwConfig 2 }


 dlcUnitHwOptions OBJECT-TYPE
  SYNTAX INTEGER ( 0..1023 )
  ACCESS read-only
  STATUS mandatory
  DESCRIPTION
   "A bit map, reported as the sum of all the values that
   correspond to installed options:
    realTimeClock  (  1)
    netPortT1  (  2)
    netPortE1  (  4)
    auxPortT1  (  8)
    auxPortE1  ( 16)
    otherNetT1  ( 32)
    otherNetE1  ( 64)
    reserved1   (128)
    reserved2   (256)
    reserved3   (512)

   `netPortT1' and `netPortE1' are mutually exclusive
   and one of the two will always be TRUE.
   `auxPortT1' and `auxPortE1' are mutually exclusive;
   if both are FALSE, there is no AUX port present.
   `otherNetT1' and `otherNetE1' are mutually exclusive;
   if both are FALSE, there is no CSU port present.
   "
  --DEFAULT test-function it_exists
  ::= { dlcUnitHwConfig 3 }


 dlcUnitSwRev OBJECT-TYPE
  SYNTAX DisplayString (SIZE (1..4))
  ACCESS read-only
  STATUS mandatory
  DESCRIPTION
   "The ASCII code of the software revision number."
  --DEFAULT test-function it_exists
  --DEFAULT get-function get_string
  --DEFAULT cookie  (char *)EclipseSwRev
  ::= { dlcUnitHwConfig 4 }


 dlcUnitDataPorts OBJECT-TYPE
  SYNTAX INTEGER (1..32)
  ACCESS read-only
  STATUS mandatory
  DESCRIPTION
   "The number of data ports installed on the unit."
  --DEFAULT test-function it_exists
  ::= { dlcUnitHwConfig 5 }

 dlcUnitRam OBJECT-TYPE
  SYNTAX INTEGER
  ACCESS read-only
  STATUS mandatory
  DESCRIPTION
   "The amount of usable ram in the unit,
   in KBytes."
  --DEFAULT test-function it_exists
  --DEFAULT get-function get_u16_supplier
  --DEFAULT cookie  (char *)&GDB.Hw.ext_1.config.ramSize
  ::= { dlcUnitHwConfig 6 }

 dlcUnitRom OBJECT-TYPE
  SYNTAX INTEGER
  ACCESS read-only
  STATUS mandatory
  DESCRIPTION
   "The amount of usable rom in the unit,
   in KBytes."
  --DEFAULT test-function it_exists
  --DEFAULT get-function get_u16_supplier
  --DEFAULT cookie  (char *)&GDB.Hw.ext_1.config.romSize
  ::= { dlcUnitHwConfig 7 }

 dlcUnitFlash OBJECT-TYPE
  SYNTAX INTEGER
  ACCESS read-only
  STATUS mandatory
  DESCRIPTION
   "The amount of FLASH ROM in the unit, in KBytes.
   If dlcUnitFlash is 0, the Downloadable Code option
   is not installed."
  --DEFAULT test-function it_exists
  --DEFAULT get-function get_u16_supplier
  --DEFAULT cookie  (char *)&GDB.Hw.ext_1.config.flashSize
  ::= { dlcUnitHwConfig 8 }


 dlcUnitSlotNum OBJECT-TYPE
  SYNTAX INTEGER (0..16)
  ACCESS read-only
  STATUS mandatory
  DESCRIPTION
   "The slot number the unit is in. If it's a stand alone unit,
    this object will return 0."
  --DEFAULT get-function get_u8_supplier
  --DEFAULT cookie (char *)&GDB.Hw.ext_3.config.SlotNum
  ::= { dlcUnitHwConfig 9 }


 dlcUnitMibRev OBJECT-TYPE
  SYNTAX DisplayString (SIZE (1..4))
  ACCESS read-only
  STATUS mandatory
  DESCRIPTION
   "The ASCII code of the MIB revision number."
  --DEFAULT test-function it_exists
  --DEFAULT get-function get_string
  --DEFAULT cookie  (char *)EclipseMibRev
  ::= { dlcUnitHwConfig 10 }


 dlcUnitFeatures OBJECT-TYPE
  SYNTAX INTEGER {
   inband         (    1),
   ipm32          (    2),
   ipm96          (    4),
   rmon2          (    8),
   sla            (   16),
   dni            (   32),
   dte2           (   64),
   reserved4      (  128),
   reserved5      (  256),
   reserved6      (  512),
   reserved7      ( 1024),
   reserved8      ( 2048)
  }
  ACCESS read-only
  STATUS mandatory
  DESCRIPTION
   "Feature configuration for Solo Select. The value returned is a bit mask
    of which feature key is on/capable when the corresponding bit is 1."
  --DEFAULT test-function it_exists
  ::= { dlcUnitHwConfig 11 }


-- ============================================================================
--
-- Following are the user-settable configuration items
-- that affect the unit as a whole
-- (vs. configuration items that are specific to a port).
--
dlcUnitConfig OBJECT IDENTIFIER ::= { dl-new-t1 2 }

 dlcUnitId OBJECT-TYPE
  SYNTAX DisplayString (SIZE (1..6))
  ACCESS read-write
  STATUS mandatory
  DESCRIPTION
   "A name that uniquely and globally identifies the unit.
   The Unit ID must consist of (upper-case) letters 'A'..'Z'
   and digits '0'..'9'.
   The first character of the Unit ID may not be a digit."
  --DEFAULT get-function get_string_supplier
  --DEFAULT cookie  (char *)&GDB.Dbms.config.unit.id
  ::= { dlcUnitConfig 1 }

 dlcUnitProtectMode OBJECT-TYPE
  SYNTAX Boolean
  ACCESS read-write
  STATUS mandatory
  DESCRIPTION
   "When true, the unit configuration
   can not be changed from the front panel
   nor can tests be initiated or terminated
   from the front panel."
  --DEFAULT test-function test_bool_supplier
  --DEFAULT get-function get_bool_supplier
  --DEFAULT cookie  (char *)&GDB.Dbms.config.unit.protect
  ::= { dlcUnitConfig 2 }

 dlcUnitYellowEnable OBJECT-TYPE
  SYNTAX Boolean
  ACCESS read-write
  STATUS mandatory
  DESCRIPTION
   "When false, yellow alarm conditions are ignored
   when received from a Dsx1 port,
   and the unit will never transmit a Yellow Alarm signal
   on a Dsx1 port.
   This applies to all ports: mainNet, aux, otherNet."
  --DEFAULT get-function get_bool_supplier
  --DEFAULT cookie  (char *)&GDB.Dbms.config.unit.yellowAlarm
  ::= { dlcUnitConfig 3 }

 dlcUnitNetPassFdl OBJECT-TYPE
  SYNTAX PortId
  ACCESS read-write
  STATUS mandatory
  DESCRIPTION
   "Indicates which Dsx1 DTE port's FDL
   is connected to the `mainNet' FDL
   when this unit has no traffic of its own.
   Meaninigful values are `aux', `otherNet', and `noPort'."
  ::= { dlcUnitConfig 4 }


-- -----------------------------------------------------------------------------
--
-- Configuration items for determining where a unit derives its timing.
--
 dlcUnitMainClockSource OBJECT-TYPE
  SYNTAX PortId 
  ACCESS read-write
  STATUS mandatory
  DESCRIPTION
   "This is the primary transmit timing source."
  --DEFAULT test-function test_clockSource_supplier
  --DEFAULT get-function get_clockSource_supplier
  --DEFAULT cookie (char *)&GDB.Dbms.config.unit.clock.main
  ::= { dlcUnitConfig 5 }

 dlcUnitAltClockSource OBJECT-TYPE
  SYNTAX PortId 
  ACCESS read-write
  STATUS mandatory
  DESCRIPTION
   "This is the alternate transmit timing source.
   It is used when the primary source is not available."
  --DEFAULT test-function test_clockSource_supplier
  --DEFAULT get-function get_clockSource_supplier
  --DEFAULT cookie (char *)&GDB.Dbms.config.unit.clock.backup
  ::= { dlcUnitConfig 6 }

 dlcUnitExtClockRate OBJECT-TYPE
  SYNTAX INTEGER (56..2048)
  ACCESS read-write
  STATUS mandatory
  DESCRIPTION
   "The signal arriving at the BNC connector
   on the back of the unit, in units of KHz.
   Must be a multiple of 56 or 64 KHz (n*56 or n*64)
   or 1544.

   For T1 units, the largest acceptable value is 1544.
   For E1 units, the value can go up to 2048.

   For Solo CSU products, the value is fixed at 1544
   and cannot be changed."
  --DEFAULT test-function test_u16_supplier
  --DEFAULT get-function get_u16_supplier
  --DEFAULT cookie (char *)&GDB.Dbms.config.unit.clock.extRate
  ::= { dlcUnitConfig 7 }

-- ----------------------------------------------------------------------------
--
-- Configuration items that control how and when the unit performs diagnostics
--
 LoopCodeType ::= INTEGER {
   standard (1),
   alternate (2),
   v54  (3),
   reserved1 (4),
   reserved2 (5),
   reserved3 (6),
   disable  (7)
  }

 dlcUnitFullBandwidthLoopCode OBJECT-TYPE
  SYNTAX LoopCodeType
  ACCESS read-write
  STATUS mandatory
  DESCRIPTION
   "Determines the loop codes we transmit for the
   Full Bandwidth Loop Up Remote and Loop Down Remote tests
   and the code we respond to
   when it's received on the NET port.
   For the fullBandwidthLoopCode,
   `v54' is an invalid value."
  --DEFAULT test-function test_loopCode_supplier
  --DEFAULT get-function get_loopCode_supplier
  --DEFAULT cookie (char *)&GDB.Dbms.config.unit.loopCode
  ::= { dlcUnitConfig 8 }

 dlcUnitFractionalLoopCode OBJECT-TYPE
  SYNTAX LoopCodeType
  ACCESS read-write
  STATUS mandatory
  DESCRIPTION
   "Determines the loop codes we transmit for the
   Fractional Loop Up Remote and Loop Down Remote tests
   and the code we respond to
   when it's received on fractional bandwidth."
  --DEFAULT test-function test_loopCode_supplier
  --DEFAULT get-function get_loopCode_supplier
  --DEFAULT cookie (char *)&GDB.Dbms.config.unit.fracLoopCode
  ::= { dlcUnitConfig 9 }

 dlcUnitTestLength OBJECT-TYPE
  SYNTAX INTEGER
  ACCESS read-write
  STATUS mandatory
  DESCRIPTION
   "Determines the time interval after which
   tests are automatically terminated.
   Valid values are 0, 1, 15 and 60.
    0 stands for unlimited,
    1 for 1 minute,
    15 for 15 minutes
    and 60 for 60 minutes."
  ::= { dlcUnitConfig 10 }

 dlcUnitUserPattern1 OBJECT-TYPE
  SYNTAX DisplayString (SIZE (0..24))
  ACCESS read-write
  STATUS mandatory
  DESCRIPTION
   "This is a sequence of the characters '0' and '1'
   that represents the first of two user patterns.
   The first character represents the first
   bit in the sequence."
  --DEFAULT test-function test_userTestPattern_supplier
  --DEFAULT get-function get_userTestPattern_supplier
  --DEFAULT cookie (char *)&GDB.Dbms.config.diag.pattern[0]
  ::= { dlcUnitConfig 11 }

 dlcUnitUserPattern2 OBJECT-TYPE
  SYNTAX DisplayString (SIZE (0..24))
  ACCESS read-write
  STATUS mandatory
  DESCRIPTION
   "This is a sequence of the characters '0' and '1'
   that represents the second of two user patterns.
   The first character represents the first
   bit in the sequence."
  --DEFAULT test-function test_userTestPattern_supplier
  --DEFAULT get-function get_userTestPattern_supplier
  --DEFAULT cookie (char *)&GDB.Dbms.config.diag.pattern[1]
  ::= { dlcUnitConfig 12 }

-- ----------------------------------------------------------------------------
--
-- Configuration items controlling what alarms we report
-- and when we report them.
--
 dlcUnitBlockAllAlarms OBJECT-TYPE
  SYNTAX Boolean 
  ACCESS read-write
  STATUS mandatory
  DESCRIPTION
   "When dlcUnitBlockAllAlarms is TRUE,
   the unit will never send any traps."
  --DEFAULT test-function test_bool_supplier
  --DEFAULT get-function get_bool_supplier
  --DEFAULT cookie (char *)&GDB.Dbms.config.alarm.unit.blockAllAlarms
  ::= { dlcUnitConfig 13 }

 dlcUnitDsx1TrapEnableTable OBJECT-TYPE
  SYNTAX SEQUENCE OF DlcDsx1TrapEnableEntry
  ACCESS not-accessible
  STATUS mandatory
  DESCRIPTION
   "A list of traps that are related to a Dsx1 port."
  --DEFAULT test-function test_dlcDsx1AlarmTrapEnable
  --DEFAULT get-function get_dlcDsx1AlarmTrapEnable
  --DEFAULT next-function next_dlcDsx1Table
  ::= { dlcUnitConfig 14 }

 dlcDsx1TrapEnableEntry OBJECT-TYPE
  SYNTAX DlcDsx1TrapEnableEntry
  ACCESS not-accessible
  STATUS mandatory
  DESCRIPTION
   "An entry in the Dsx1 Trap Enable table."
  INDEX { dlcDsx1TrapPortId }
  ::= { dlcUnitDsx1TrapEnableTable 1 }

 DlcDsx1TrapEnableEntry ::=
  SEQUENCE {
   dlcDsx1TrapPortId                    PortId,
   dlcDsx1BpvThresholdTrap              INTEGER,
   dlcDsx1OofThresholdTrap              INTEGER,
   dlcDsx1CrcThresholdTrap              INTEGER,
   dlcDsx1LossOfSignalTrapEnable        Boolean,
   dlcDsx1LossOfSyncTrapEnable          Boolean,
   dlcDsx1ReceiveAIStrapEnable          Boolean,
   dlcDsx1ReceiveYellowAlarmTrapEnable  Boolean,
   dlcDsx1ReceiveRemoteAlarmTrapEnable  Boolean,
   dlcDsx1PSfailureTrapEnable           Boolean,
   dlcDsx1CntlCrdMissingTrapEnable      Boolean,
   dlcDsx1FdlLinkTrapEnable             Boolean,
   dlcDsx1IbCrcThresholdTrap            INTEGER,
   dlcDsx1InbandLinkTrapEnable          Boolean
  }

  dlcDsx1TrapPortId OBJECT-TYPE
   SYNTAX PortId
   ACCESS read-only
   STATUS mandatory
   DESCRIPTION
    "Uniquely identifies this port.
    Valid values are `mainNet', `aux', `otherNet'."
   --DEFAULT test-function test_%n
   --DEFAULT get-function get_compl_0
   ::= { dlcDsx1TrapEnableEntry 1 }

  dlcDsx1BpvThresholdTrap OBJECT-TYPE
   SYNTAX INTEGER ( 0..99 ) 
   ACCESS read-write
   STATUS mandatory
   DESCRIPTION
    "Defines Bipolar Violation Error Rate threshold
    in units of BPV errors per minute.
    When this threshold is exceeded,
    the unit will send a trap to the SNMP manager.
    The threshold is of the form M x 10 ^ -E
    where M is a mantissa and E the exponent.
    Both are single digit values.
    M and E are represented in this value
    as one number:
     (M) x 10 + (E).
    For example, the threshold 7x10^-3 BPVs/min
    is encoded as the integer 73.
    A value of 0 disables the trap altogether.

    Valid values for dlcDsx1BpvThresholdTrap are:
        13 23 33 43 53 63
        14 24 34 44 54 64 74 84 94
        15 25 35 45 55 65 75 85 95
        16 26 36 46 56 66 76 86 96
        17 27 37 47 57 67 77 87 97
           28 38 48 58 68 78 88 98
    or, from 6E-3 to 2E-8.

    N.B. The BPV Threshold Alarm exists only for
    the mainNet and otherNet Port IDs,
    so setting the Threshold for the aux PortId
    will always fail.
    The BPV Threshold is always reported as 0
    for the aux PortId."
   --DEFAULT test-function test_dlcDsx1ThresholdTrap
   --DEFAULT get-function get_dlcDsx1ThresholdTrap
   ::= { dlcDsx1TrapEnableEntry 2 }

  dlcDsx1OofThresholdTrap OBJECT-TYPE
   SYNTAX INTEGER ( 0..99 )
   ACCESS read-write
   STATUS mandatory
   DESCRIPTION
    "Defines the Out Of Frame Error Rate threshold
    in units of OOF errors per minute.
    The threshold is encoded
    the same as dlcDsx1BpvThresholdTrap.
    A value of 0 disables the trap altogether.

    Valid values for dlcDsx1OofThresholdTap are:
        12 22 22 42 52 62 72
        13 23 33 43 53 63 73 83 93
        14 24 34 44 54 64 74 84 94
        15 25 35 45 55 65 75 85 95
              36 46 56 66 76 86 96
    or, from 7E-2 to 3E-6.

    N.B. The OOF Threshold Alarm exists only for
    the mainNet and otherNet Port IDs,
    so setting the Threshold for the aux PortId
    will always fail.
    The OOF Threshold is always reported as 0
    for the aux PortId."
   --DEFAULT test-function test_dlcDsx1ThresholdTrap
   --DEFAULT get-function get_dlcDsx1ThresholdTrap
   ::= { dlcDsx1TrapEnableEntry 3 }

  dlcDsx1CrcThresholdTrap OBJECT-TYPE
   SYNTAX INTEGER ( 0..99 )
   ACCESS read-write
   STATUS mandatory
   DESCRIPTION
    "Defines the CRC-6 Error Rate threshold
    in units of CRC errors per minute.
    The threshold is encoded
    the same as dlcDsx1BpvThresholdTrap.
    A value of 0 disables the trap altogether.

    Valid values for dlcDsx1CrcThresholdTrap are:
        10
        11 21 31 41 51 61 71 81 91
        12 22 32 42 52 62 72 82 92
        13 23 33 43 53 63 73 83 93
        14 24 34 44 54 64 74 84 94
                    55 65 75 85 95
    or, from 1E0 to 5E-5.

    N.B. The CRC Threshold Alarm exists only for
    the mainNet and otherNet Port IDs,
    so setting the Threshold for the aux PortId
    will always fail.
    The CRC Threshold is always reported as 0
    for the aux PortId."
   --DEFAULT test-function test_dlcDsx1ThresholdTrap
   --DEFAULT get-function get_dlcDsx1ThresholdTrap
   ::= { dlcDsx1TrapEnableEntry 4 }

  dlcDsx1LossOfSignalTrapEnable OBJECT-TYPE
   SYNTAX Boolean
   ACCESS read-write
   STATUS mandatory
   DESCRIPTION
    "Enables or disables a trap
    upon loss of network signal (LOS)
    on a Dsx1 port."
   ::= { dlcDsx1TrapEnableEntry 5 }

  dlcDsx1LossOfSyncTrapEnable OBJECT-TYPE
   SYNTAX Boolean
   ACCESS read-write
   STATUS mandatory
   DESCRIPTION
    "Enables or disables a trap
    upon loss of network framing (LOF)
    on a Dsx1 port."
   ::= { dlcDsx1TrapEnableEntry 6 }

  dlcDsx1ReceiveAIStrapEnable OBJECT-TYPE
   SYNTAX Boolean
   ACCESS read-write
   STATUS mandatory
   DESCRIPTION
    "Enables or disables a trap
    upon detection of AIS (Blue Alarm Signal)
    on a Dsx1 port."
   ::= { dlcDsx1TrapEnableEntry 7 }

  dlcDsx1ReceiveYellowAlarmTrapEnable OBJECT-TYPE
   SYNTAX Boolean
   ACCESS read-write
   STATUS mandatory
   DESCRIPTION
    "Enables or disables a trap
    upon detection of Yellow Alarm Signal
    on a Dsx1 port."
   ::= { dlcDsx1TrapEnableEntry 8 }

  dlcDsx1ReceiveRemoteAlarmTrapEnable OBJECT-TYPE
   SYNTAX Boolean
   ACCESS read-write
   STATUS mandatory
   DESCRIPTION
    "Enables or disables a trap
    upon detection of Remote Alarm Indicator
    on an E1 Dsx1 port.

    For T1 units, this object's value
    will always be read as 0
    and set operations will succeed,
    but the value will not be changed."
   --DEFAULT test-function test_%n
   ::= { dlcDsx1TrapEnableEntry 9 }

  dlcDsx1PSfailureTrapEnable OBJECT-TYPE
   SYNTAX Boolean
   ACCESS read-write
   STATUS mandatory
   DESCRIPTION
    "Enables or disables a trap
    upon a power supply failure.

    This object doesn't really belong
    in the dlcDsx1TrapEnableTable.
    It is specific to units
    that are installed in a shelf.
    When accessing this object,
    the index (portId) is ignored."
   ::= { dlcDsx1TrapEnableEntry 10 }

  dlcDsx1CntlCrdMissingTrapEnable OBJECT-TYPE
   SYNTAX Boolean
   ACCESS read-write
   STATUS mandatory
   DESCRIPTION
    "Enables or disables a trap
    upon detection of a missing 
    controller card.

    This object doesn't really belong
    in the dlcDsx1TrapEnableTable.
    It is specific to units
    that are installed in a shelf.
    When accessing this object,
    the index (portId) is ignored."
   ::= { dlcDsx1TrapEnableEntry 11 }

  dlcDsx1FdlLinkTrapEnable OBJECT-TYPE
   SYNTAX Boolean
   ACCESS read-write
   STATUS mandatory
   DESCRIPTION
    "Enables or disables a trap
    upon detection of the remote unit is 
    not responding to DLC FDL messages."
   ::= { dlcDsx1TrapEnableEntry 12 }

 dlcDsx1IbCrcThresholdTrap OBJECT-TYPE
  SYNTAX INTEGER ( 0..99 )
  ACCESS read-write
  STATUS mandatory
  DESCRIPTION
   "Defines the In-Band CRC Error Rate threshold
   in units of CRC errors per minute.
   The threshold is encoded
   the same as dlcDsx1BpvThresholdTrap.
   A value of 0 disables the trap altogether.
   For the Eclipse, valid values of
   dlcDsx1IbCrcThresholdTrap range from 47 to 91.
   N.B. The CRC Threshold Alarm exists only for
   the mainNet port."
  --DEFAULT test-function	test_dlcDsx1ThresholdTrap
  --DEFAULT get-function	get_dlcDsx1ThresholdTrap
  ::= { dlcDsx1TrapEnableEntry 13 }

  dlcDsx1InbandLinkTrapEnable OBJECT-TYPE
   SYNTAX Boolean
   ACCESS read-write
   STATUS mandatory
   DESCRIPTION
    "Enables or disables a trap
    upon detection of the inband link."
   ::= { dlcDsx1TrapEnableEntry 14 }

 dlcUnitDataDteLossTrapEnableTable OBJECT-TYPE
  SYNTAX SEQUENCE OF DlcDataDteLossTrapEnableEntry
  ACCESS not-accessible
  STATUS mandatory
  DESCRIPTION
   "Enables or disables a trap
   upon the loss of signal on a Data DTE port."
  --DEFAULT next-function next_dlcDataTable 
  ::= { dlcUnitConfig 15 }

 dlcDataDteLossTrapEnableEntry OBJECT-TYPE
  SYNTAX DlcDataDteLossTrapEnableEntry
  ACCESS not-accessible
  STATUS mandatory
  DESCRIPTION
   "An entry in the Data DTE Loss Trap Enable table."
  INDEX { dlcDataLossPortId }
  ::= { dlcUnitDataDteLossTrapEnableTable 1 }

 DlcDataDteLossTrapEnableEntry ::=
  SEQUENCE {
   dlcDataLossPortId PortId,
   dlcDataLossEnable Boolean
  }


  dlcDataLossPortId OBJECT-TYPE
   SYNTAX PortId
   ACCESS read-only
   STATUS mandatory
   DESCRIPTION
    "Uniquely identifies this port.
    Valid values are `data1' through `data32'."
   --DEFAULT get-function get_compl_0
   ::= { dlcDataDteLossTrapEnableEntry 1 }

  dlcDataLossEnable OBJECT-TYPE
   SYNTAX Boolean
   ACCESS read-write
   STATUS mandatory
   DESCRIPTION
    "Says whether to enable this trap."
   ::= { dlcDataDteLossTrapEnableEntry 2 }

 dlcUnitExternalAlarmInputTrapEnable OBJECT-TYPE
  SYNTAX Boolean
  ACCESS read-write
  STATUS mandatory
  DESCRIPTION
   "Enables or disables a trap
   upon detection of an alarm condition
   on the External Alarm Input."
  --DEFAULT get-function get_bool_supplier
  --DEFAULT cookie (char *)&GDB.Dbms.config.alarm.external.enableInput
  ::= { dlcUnitConfig 16 }

 dlcUnitExternalAlarmInputContacts OBJECT-TYPE
  SYNTAX INTEGER {
   normallyOpen (1),
   normallyClosed (2)
  }
  ACCESS read-write
  STATUS mandatory
  DESCRIPTION
   "When set to normallyOpen, an alarm is declared
   when the contacts close ... and vice-versa."
  ::= { dlcUnitConfig 17 }

 dlcUnitExternalAlarmInputMessage OBJECT-TYPE
  SYNTAX DisplayString (SIZE (0..20))
  ACCESS read-write
  STATUS mandatory
  DESCRIPTION
   "A character string that describes what's going on
   when an alarm on the External Alarm Input is detected."
  --DEFAULT get-function get_string_supplier
  --DEFAULT cookie (char *)&GDB.Dbms.config.alarm.external.msg
  ::= { dlcUnitConfig 18 }

 dlcUnitExternalAlarmOutputContacts OBJECT-TYPE
  SYNTAX INTEGER {
   normallyOpen (1),
   normallyClosed (2)
  }
  ACCESS read-write
  STATUS mandatory
  DESCRIPTION
   "When set to normallyOpen,
   the contacts close when any enabled alarm occurs
   and vice-versa.
   Pushing any button on the units Front Panel
   will cause the alarm contacts to open again
   until another alarm is recognized."
  ::= { dlcUnitConfig 19 }

 dlcUnitExternalAlarmOutputTrapEnable OBJECT-TYPE
  SYNTAX Boolean
  ACCESS read-write
  STATUS mandatory
  DESCRIPTION
   "Enables or disables the external alarm output contacts."
  --DEFAULT get-function get_bool_supplier
  --DEFAULT cookie (char *)&GDB.Dbms.config.alarm.external.enableOutput
  ::= { dlcUnitConfig 20 }

-- ============================================================================
--
-- Configuration items controlling the configuration and operation
-- of all our Dsx1 ports.
--

 dlcUnitDsx1ConfigTable OBJECT-TYPE
  SYNTAX SEQUENCE OF DlcDsx1ConfigEntry
  ACCESS not-accessible
  STATUS mandatory
  DESCRIPTION
   "A table of configuration items for a Dsx1 port."
  --DEFAULT next-function next_dlcDsx1Table 
  ::= { dlcUnitConfig 21 }

 dlcDsx1ConfigEntry OBJECT-TYPE
  SYNTAX DlcDsx1ConfigEntry
  ACCESS not-accessible
  STATUS mandatory
  DESCRIPTION
   "An entry in the Dsx1 configuration table."
  INDEX { dlcDsx1ConfigPortId }
  ::= { dlcUnitDsx1ConfigTable 1 }

 DlcDsx1ConfigEntry ::=
  SEQUENCE {
   dlcDsx1ConfigPortId  PortId,
   dlcDsx1Framing   INTEGER,
   dlcDsx1LineCode   INTEGER,
   dlcDsx1LineMatching  INTEGER,
   dlcDsx1DacsMode   Boolean,
   dlcDsx1UseDlcFdlProtocol Boolean,
   dlcDsx1UseAnsiProtocol  Boolean,
   dlcDsx1Bit7Stuffing  Boolean,
   dlcDsx1InBandBit  INTEGER
  }


  dlcDsx1ConfigPortId OBJECT-TYPE
   SYNTAX PortId
   ACCESS read-only
   STATUS mandatory
   DESCRIPTION
    "Uniquely identifies this port.
    Valid values are `mainNet', `aux', `otherNet'."
   --DEFAULT get-function get_compl_0
   ::= { dlcDsx1ConfigEntry 1 }

  dlcDsx1Framing OBJECT-TYPE
   SYNTAX INTEGER {
    esfFraming (1),
    d4Framing (2),
    crc4Enabled (3),
    crc4Disabled (4),
    unstructured (5)
   }
   ACCESS read-write
   STATUS mandatory
   DESCRIPTION
    "Dsx1 framing type:
    T1 --> D4 or ESF;
    E1 --> CRC4 or not.
    Unstructured is E1 only,
    when in this mode, the E1 line acts like
    a 2048 Bps pipe with no framing information."
   ::= { dlcDsx1ConfigEntry 2 }

  dlcDsx1LineCode OBJECT-TYPE
   SYNTAX INTEGER {
    b8zsCode (1),
    amiCode  (2),
    hdb3Code (3)
   }
   ACCESS read-write
   STATUS mandatory
   DESCRIPTION
    "Dsx1 line code type, AMI or B8ZS.
    E1 lines are always HDB3."
   ::= { dlcDsx1ConfigEntry 3 }

  dlcDsx1LineMatching OBJECT-TYPE
   SYNTAX INTEGER {
    buildOut0 ( 1),
    buildOut7p5 ( 2),
    buildOut15 ( 3),
    length0  ( 4),
    length133 ( 5),
    length266 ( 6),
    length399 ( 7),
    length533 ( 8),
    ohms75  ( 9),
    ohms120  (10)
   }
   ACCESS read-write
   STATUS mandatory
   DESCRIPTION
    "Dsx1 line build out or equalization:
    For a T1 NET port, LBO is 0, -7.5 or -15dB.
    For a T1 AUX port, EQ is 0..533 feet.
    For any E1 port, impedance is 75 or 120 Ohms."
   ::= { dlcDsx1ConfigEntry 4 }

  dlcDsx1DacsMode OBJECT-TYPE
   SYNTAX Boolean
   ACCESS read-write
   STATUS mandatory
   DESCRIPTION
    "True if DACS mode is in effect for this port."
   --DEFAULT get-function get_bool_supplier
   --DEFAULT cookie (char *)&GDB.Dbms.config.unit.dacsMode
   ::= { dlcDsx1ConfigEntry 5 }

  dlcDsx1UseDlcFdlProtocol OBJECT-TYPE
   SYNTAX Boolean
   ACCESS read-write
   STATUS mandatory
   DESCRIPTION
    "True when DLC FDL protocol is enabled
    over this port's FDL."
   ::= { dlcDsx1ConfigEntry 6 }

  dlcDsx1UseAnsiProtocol OBJECT-TYPE
   SYNTAX Boolean
   ACCESS read-write
   STATUS mandatory
   DESCRIPTION
    "True if ANSI Dsx1.403 is enabled over the FDL."
   ::= { dlcDsx1ConfigEntry 7 }

  dlcDsx1Bit7Stuffing OBJECT-TYPE
   SYNTAX Boolean
   ACCESS read-write
   STATUS mandatory
   DESCRIPTION
    "When true, bit 7 stuffing
    is turned on for a T1 Aux port
    to provide pulse density protection.
    Valid only on a T1 AUX port."
   ::= { dlcDsx1ConfigEntry 8 }

  dlcDsx1InBandBit OBJECT-TYPE
   SYNTAX INTEGER (1..8)
   ACCESS read-write
   STATUS mandatory
   DESCRIPTION
    "Specifies which bit in Timeslot 1
    of an E1 link is used to pass message traffic.
    Not implemented for T1 ports."
   ::= { dlcDsx1ConfigEntry 9 }

-- ============================================================================
--
-- Configuration items controlling the configuration and operation
-- of our DATA ports.
--

 dlcUnitDataDteConfigTable OBJECT-TYPE
  SYNTAX SEQUENCE OF DlcDataDteConfigEntry
  ACCESS not-accessible
  STATUS mandatory
  DESCRIPTION
   "The data DTE configuration table."
  --DEFAULT next-function next_dlcDataTable 
  ::= { dlcUnitConfig 22 }

 dlcDataDteConfigEntry OBJECT-TYPE
  SYNTAX DlcDataDteConfigEntry
  ACCESS not-accessible
  STATUS mandatory
  DESCRIPTION
   "An entry in the data DTE configuration table."
  INDEX { dlcDataConfigPortId }
  ::= { dlcUnitDataDteConfigTable 1 }

 DlcDataDteConfigEntry ::=
  SEQUENCE {
   dlcDataConfigPortId  PortId,
   dlcDataConfigEncoding  INTEGER,
   dlcDataConfigLoss  INTEGER,
   dlcDataConfigMode  INTEGER,
   dlcDataConfigFormat  INTEGER,
   dlcDataConfigTransmitTiming INTEGER
  }


  dlcDataConfigPortId OBJECT-TYPE
   SYNTAX PortId
   ACCESS read-only
   STATUS mandatory
   DESCRIPTION
    "Uniquely identifies this port.
    Valid values are `data1' through `data32'."
   --DEFAULT get-function get_compl_0
   ::= { dlcDataDteConfigEntry 1 }

  dlcDataConfigEncoding OBJECT-TYPE
   SYNTAX INTEGER {
    normal  (1),
    hdlc  (2),
    deferred (3),
    forced  (4)
   }
   ACCESS read-write
   STATUS mandatory
   DESCRIPTION
    "Data DTE encoding type."
   ::= { dlcDataDteConfigEntry 2 }

  dlcDataConfigLoss OBJECT-TYPE
   SYNTAX INTEGER {
    never   (1),
    rtsLoss   (2),
    dtrLoss   (3),
    dataDependentLoss (4)
   }
   ACCESS read-write
   STATUS mandatory
   DESCRIPTION
    "The criterion to declare a Loss of Data DTE."
   ::= { dlcDataDteConfigEntry 3 }

  dlcDataConfigMode OBJECT-TYPE
   SYNTAX INTEGER {
    dteMode (1),
    dceMode (2)
   }
   ACCESS read-write
   STATUS mandatory
   DESCRIPTION
    "In dteMode the unit is the DTE
    and the external equipment the DCE."
   ::= { dlcDataDteConfigEntry 4 }

  dlcDataConfigFormat OBJECT-TYPE
   SYNTAX INTEGER {
    v35 (1),
    rs449 (2)
   }
   ACCESS read-write
   STATUS mandatory
   DESCRIPTION
    "Selects V.35 or RS449 format."
   ::= { dlcDataDteConfigEntry 5 }

  dlcDataConfigTransmitTiming OBJECT-TYPE
   SYNTAX INTEGER {
    scte  (1),
    sct  (2),
    sctInverted (3)
   }
   ACCESS read-write
   STATUS mandatory
   DESCRIPTION
    "Selects the Data DTE tramsmit timing source.
    Can be SCTE, SCT or inverted SCT."
   ::= { dlcDataDteConfigEntry 6 }

-- ============================================================================
--
-- Configuration items controlling the unit multiplexer.
--
-- There are two mux-buses in the Eclipse,
-- so we define a table with two indexes: primary/secondary and slot number.
--

 dlcUnitMuxConfigTable OBJECT-TYPE
  SYNTAX SEQUENCE OF DlcMuxConfigEntry
  ACCESS not-accessible
  STATUS mandatory
  DESCRIPTION
   "This is the timeslot map configuration."
  --DEFAULT next-function next_dlcUnitMuxConfigTable 
  ::= { dlcUnitConfig 23 }

 dlcMuxConfigEntry OBJECT-TYPE
  SYNTAX DlcMuxConfigEntry
  ACCESS not-accessible
  STATUS mandatory
  DESCRIPTION
   "An entry in the timeslot map."
  INDEX { dlcMuxConfigBusId, dlcMuxConfigSlotNumber }
  ::= { dlcUnitMuxConfigTable 1 }
 
 DlcMuxConfigEntry ::=
  SEQUENCE {
   dlcMuxConfigBusId INTEGER,
   dlcMuxConfigSlotNumber INTEGER,
   dlcMuxConfigPortId PortId
  }


  dlcMuxConfigBusId OBJECT-TYPE
   SYNTAX INTEGER {
    primary  (1),
    secondary (2)
   }
   ACCESS read-only
   STATUS mandatory
   DESCRIPTION
    "Identifies which muxBus this timeslot is on."
   --DEFAULT get-function get_compl_0
   ::= { dlcMuxConfigEntry 1 }

  dlcMuxConfigSlotNumber OBJECT-TYPE
   SYNTAX INTEGER (1..31)
   ACCESS read-only
   STATUS mandatory
   DESCRIPTION
    "Uniquely identifies this timeslot."
   --DEFAULT get-function get_compl_1
   ::= { dlcMuxConfigEntry 2 }

  dlcMuxConfigPortId OBJECT-TYPE
   SYNTAX PortId
   ACCESS read-write
   STATUS mandatory
   DESCRIPTION
    "Says which DTE port receives data
    from the indicated muxBus
    during this timeslot."
   ::= { dlcMuxConfigEntry 3 }

 
