-- =============================================================================
--
-- Link Based Test parameters
--
	dlcUnitConfigLinkTest OBJECT IDENTIFIER ::= { dlcUnitConfig 33 }


	dlcLinkTestState OBJECT-TYPE
	SYNTAX INTEGER {
			disabled (1),
			enabled (2),
			running (3),
			idle (4)
	}
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
	"Specifies the status configuration of link based test.
	Only setable to values disabled(1) and enabled(2) to end and start
        the test respectively. Values running(3) and idle(4) are returned
        for status of the test."
	::= { dlcUnitConfigLinkTest 1 }

	dlcLinkTestAddress OBJECT-TYPE
	SYNTAX IpAddress
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
	"Specifies the address for the next link based test to run."
	::= { dlcUnitConfigLinkTest 2 }

	dlcLinkTestDlci OBJECT-TYPE
	SYNTAX INTEGER ( 0..1023 )
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
	"Specifies the frame relay DLCI for the next link based test
	to run. This value is used when the unit is in frameRelay
	InbandMode only and if the next link based test is not done
	on the commPort."
	::= { dlcUnitConfigLinkTest 3 }

	dlcLinkTestPort OBJECT-TYPE
	SYNTAX INTEGER {
		comm	(1),
		net	(2),
		dte	(3),
                eth     (4)
	}
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
	"Specifies which port to run the next link based test."
	::= { dlcUnitConfigLinkTest 4 }

	dlcLinkTestLength OBJECT-TYPE
	SYNTAX INTEGER
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
	"Specifies the time length of the next test to run in minutes."
	::= { dlcUnitConfigLinkTest 5 }

	dlcLinkTestInterval OBJECT-TYPE
	SYNTAX INTEGER (1..3600)
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
	"Specifies the repetition interval in seconds between packets
	 for the next link based test."
	::= { dlcUnitConfigLinkTest 6 }

	dlcLinkTestPacketSize OBJECT-TYPE
	SYNTAX INTEGER (0..1500)
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
	"Specifies the packet size for the next link based test."
	::= { dlcUnitConfigLinkTest 7 }

	dlcLinkTestPattern OBJECT-TYPE
	SYNTAX INTEGER {
		ping511	(1),
		ping2047 (2),
		ping1-0 (3),
		ping-all-1 (4),
		ping-all-0 (5)
	}
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
	"Specifies the test pattern for the next link based test."
	::= { dlcUnitConfigLinkTest 8 }


