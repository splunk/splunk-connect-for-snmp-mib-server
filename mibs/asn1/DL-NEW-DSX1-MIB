-- *****************************************************************************
-- *****************************************************************************
-- *****************************************************************************
-- *****************************************************************************
--
-- Unit Test Group
--
-- To start a test running against a port,
-- just set one of the values in the dlcUnitTestTable.
-- To stop a test, set the testType to `noTest'.
--

dlcUnitTest OBJECT IDENTIFIER ::= { dl-new-t1 5 }

 dlcUnitTestTable OBJECT-TYPE
  SYNTAX SEQUENCE OF DlcUnitTestEntry
  ACCESS not-accessible
  STATUS mandatory
  DESCRIPTION
   "A table containing all currently active tests.
   The initial release of the product's firmware
   does not include multiple tests.
   However, a later version will allow
   a number of tests to be active at a time.
   This MIB object structure allows any number of tests
   to be running at any time.
   Upon receiving a SET request,
   the unit compares this table
   with the list of currently active tests
   and terminates or initiates any tests as required.
   However, this does not apply to Link Based Test.
   dlcUnitConfigLinkTest group contains objects to
   configure and start Link Based Test.
   This table only contains the statistics for Link
   Based Test."
  ::= { dlcUnitTest 1 }

 dlcUnitTestEntry OBJECT-TYPE
  SYNTAX DlcUnitTestEntry
  ACCESS not-accessible
  STATUS mandatory
  DESCRIPTION
   "An entry in the network configuration table."
  INDEX { dlcUnitTestPortId }
  ::= { dlcUnitTestTable 1 }

 DlcUnitTestEntry ::=
  SEQUENCE {
   dlcUnitTestPortId		PortId,
   dlcUnitTestType		TestType,
   dlcUnitTestPatternStatus	INTEGER,
   dlcUnitTestPatternErrors	Gauge,
   dlcLinkTestSentPackets	Gauge,
   dlcLinkTestReceivedPackets	Gauge,
   dlcLinkTestErroredPackets	Gauge,
   dlcLinkTestMissingPackets	Gauge,
   dlcLinkTestAverageRoundTrip	Gauge,
   dlcLinkTestMaximumRoundTrip	Gauge
  }

  dlcUnitTestPortId OBJECT-TYPE
   SYNTAX PortId
   ACCESS read-only
   STATUS mandatory
   DESCRIPTION
    "Identifies a port
    that a test might be running on.
    By convention, Self Test and all
    full-bandwidth tests (loopbacks, patterns, etc.)
    are assigned to the netPort.
    For E1 unit, if it is in unstructured mode,
    only full-bandwidth tests can be performed."
   --DEFAULT get-function get_compl_0
   --DEFAULT next-function next_dlcPortTable 
   ::= { dlcUnitTestEntry 1 }

  dlcUnitTestType OBJECT-TYPE
   SYNTAX TestType
   ACCESS read-write
   STATUS mandatory
   DESCRIPTION
    "Says what test is running on this port.
                 Note that the following test nomenclature
                 is equivalent:
                 SNMP        Terminal UI    Front Panel
                 1in8           1:7             1:7
                 3in24          3:24            3.24
                 1in1           1:1             1:1
                 AllOne         All 1           1's
                 AllZero        All 0           0's
                 1in3           1:2             1:2
                 1in5           1:4             1:4
                 The SNMP test identifiers use the names on the left.
                 The terminal User Interface uses the names in the
                 middle column, and the unit's front panel uses the 
                 names in the right column."
   --DEFAULT next-function next_dlcPortTable 
   ::= { dlcUnitTestEntry 2 }

  dlcUnitTestPatternStatus OBJECT-TYPE
   SYNTAX INTEGER {
    search  (1),
    locked  (2),
    overflow (3),
    off  (4),
    lockedAfterLoss (5)
   }
   ACCESS read-only
   STATUS mandatory
   DESCRIPTION
    "The current state of the Pattern Test module."
   --DEFAULT next-function next_dlcPortTable 
   ::= { dlcUnitTestEntry 3 }

  dlcUnitTestPatternErrors OBJECT-TYPE
   SYNTAX Gauge
   ACCESS read-write
   STATUS mandatory
   DESCRIPTION
    "An ongoing count of errors detected by
    the Pattern Generator/Detector.
    Can be read and written (to zero)."
   --DEFAULT next-function next_dlcPortTable 
   ::= { dlcUnitTestEntry 4 }

  dlcLinkTestSentPackets OBJECT-TYPE
   SYNTAX Gauge
   ACCESS read-only
   STATUS mandatory
   DESCRIPTION
    "Number of sent packets for the currently running link based
    test."
   ::= { dlcUnitTestEntry 5 }

  dlcLinkTestReceivedPackets OBJECT-TYPE
