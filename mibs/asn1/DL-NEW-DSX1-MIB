  dlcDLCItable OBJECT-TYPE
  SYNTAX SEQUENCE OF DlcDLCItableEntry
  ACCESS not-accessible
  STATUS mandatory
  DESCRIPTION
	"Table for current DLCI"
  ::= { dlcUnitConfig 39 }

  dlcDLCItableEntry OBJECT-TYPE
  SYNTAX DlcDLCItableEntry
  ACCESS not-accessible
  STATUS mandatory
  DESCRIPTION
	"An entry for a DLCI"
  INDEX { dlcDLCInumber, dlcDTECIR, dlcNETCIR }
  --DEFAULT get-function-async get_async_dlcDLCITable
  --DEFAULT set-function-async null_set_async
  --DEFAULT test-function-async test_async_dlcDLCITable
  --DEFAULT next-function-async next_async_dlcDLCITable
  ::= { dlcDLCItable 1 }

  DlcDLCItableEntry ::=
  SEQUENCE {
	dlcDLCInumber	INTEGER,
	dlcDTECIR	INTEGER,
	dlcNETCIR	INTEGER,
	dlcDLCIstatus	INTEGER
  }

  dlcDLCInumber OBJECT-TYPE
  SYNTAX INTEGER (1..1023)
  ACCESS read-only
  STATUS mandatory
  DESCRIPTION
	"Identifies DLCI number."
  ::= { dlcDLCItableEntry 1 }

  dlcDTECIR OBJECT-TYPE
  SYNTAX INTEGER
  ACCESS read-only
  STATUS mandatory
  DESCRIPTION
	"This DLCI's DTE Confirmed Information Rate (CIR)"
  ::= { dlcDLCItableEntry 2 }

  dlcNETCIR OBJECT-TYPE
  SYNTAX INTEGER
  ACCESS read-only
  STATUS mandatory
  DESCRIPTION
	"This DLCI's NET Confirmed Information Rate (CIR)"
  ::= { dlcDLCItableEntry 3 }

  dlcDLCIstatus OBJECT-TYPE
  SYNTAX  INTEGER {
	enabled	(1),
	disabled (2),
	active (3),
	inactive (4)
  }
  ACCESS read-write
  STATUS mandatory
  DESCRIPTION
	"This object represents status of the entry.
	When read, it will return status 'active' or 'inactive'
	representing the status of the row.
	When written, this object is used to add or delete entry.
	Setting to 'enabled' will add a new entry, setting to
	'disabled' will delete the entry."
  ::= { dlcDLCItableEntry 4 }

--
-- Utilization Table
--

  dlcUtilizationTable OBJECT-TYPE
  SYNTAX SEQUENCE OF DlcUtilTableEntry
  ACCESS not-accessible
  STATUS mandatory
  DESCRIPTION
	"Table for lifetime Utilization statistics."
  ::= { dl-new-t1 7 }

  dlcUtilTableEntry OBJECT-TYPE
  SYNTAX DlcUtilTableEntry
  ACCESS not-accessible
  STATUS mandatory
  DESCRIPTION
	"An entry for the utilization table."
  INDEX { dlcUtilDLCINumber }
  --DEFAULT get-function-async get_async_dlcUtilTable_counter
  --DEFAULT set-function-async null_set_async
  --DEFAULT test-function-async null_test_async
  --DEFAULT next-function-async next_async_dlcUtilTable
  ::= { dlcUtilizationTable 1 }

  DlcUtilTableEntry ::=
  SEQUENCE {
	dlcUtilDLCINumber	INTEGER,
	dlcUtilEncodedValue	OCTET STRING,
	dlcUtilTimestamp	Counter,
	dlcUtilLessThan20	Counter,
	dlcUtil20-40		Counter,
	dlcUtil40-60		Counter,
	dlcUtil60-80		Counter,
	dlcUtil80-100		Counter,
	dlcUtilMoreThan100	Counter
  }

  dlcUtilDLCINumber OBJECT-TYPE
  SYNTAX INTEGER
  ACCESS read-only
  STATUS mandatory
  DESCRIPTION
	"Identifies the DLCI number of the utilization statistics
	of this entry/row."
  ::= { dlcUtilTableEntry 1 }

  dlcUtilEncodedValue OBJECT-TYPE
  SYNTAX OCTET STRING
  ACCESS read-only
  STATUS mandatory
  DESCRIPTION
	"This object represents the value of the whole row	
	of the table. The intent of object is to increase
	the efficiency of retrieving the table. For each
	object value in this table, which are all of Counter
	type, it will be represented with 4 bytes of data."
  --DEFAULT get-function-async get_async_dlcUtilEncodedValue
  ::= { dlcUtilTableEntry 2 }

  dlcUtilTimestamp OBJECT-TYPE
  SYNTAX Counter
  ACCESS read-only
  STATUS mandatory
  DESCRIPTION
	"Identifies timestamp when this entry is last updated."
  ::= { dlcUtilTableEntry 3 }

  dlcUtilLessThan20 OBJECT-TYPE
  SYNTAX Counter
  ACCESS read-only
  STATUS mandatory
  DESCRIPTION
	"The number of times utilization is less than 20% of
        Committed Information Rate (CIR)."
  ::= { dlcUtilTableEntry 4 }

  dlcUtil20-40 OBJECT-TYPE
  SYNTAX Counter
  ACCESS read-only
  STATUS mandatory
  DESCRIPTION
	"The number of times utilization is between 20% and 40% of
        Committed Information Rate (CIR)."
  ::= { dlcUtilTableEntry 5 }

  dlcUtil40-60 OBJECT-TYPE
  SYNTAX Counter
  ACCESS read-only
  STATUS mandatory
  DESCRIPTION
	"The number of times utilization is between 40% and 60% of
        Committed Information Rate (CIR)."
  ::= { dlcUtilTableEntry 6 }

  dlcUtil60-80 OBJECT-TYPE
  SYNTAX Counter
  ACCESS read-only
  STATUS mandatory
  DESCRIPTION
	"The number of times utilization is between 60% and 80% of
        Committed Information Rate (CIR)."
  ::= { dlcUtilTableEntry 7 }

  dlcUtil80-100 OBJECT-TYPE
  SYNTAX Counter
  ACCESS read-only
  STATUS mandatory
  DESCRIPTION
	"The number of times utilization is between 80% and 100% of
        Committed Information Rate (CIR)."
  ::= { dlcUtilTableEntry 8 }

  dlcUtilMoreThan100 OBJECT-TYPE
  SYNTAX Counter
  ACCESS read-only
  STATUS mandatory
  DESCRIPTION
	"The number of times utilization is more than 100% of
        Committed Information Rate (CIR)."
  ::= { dlcUtilTableEntry 9 }

--
--   LMI Group
--

  dlcLMIConditioningGroup OBJECT IDENTIFIER
  ::= { dlcUnitConfig 41 }

  dlcLMIConfiguration OBJECT IDENTIFIER
  ::= { dlcLMIConditioningGroup 1}

  dlcLMIEnable OBJECT-TYPE
  SYNTAX INTEGER {
          enabled (1),
          disabled (2)
         }
  ACCESS read-write
  STATUS mandatory
  DESCRIPTION
	"This object specifies whether to enable or disable LMI
	Conditioning."
  ::= { dlcLMIConfiguration 1 }

  dlcMaintenanceDLCI OBJECT-TYPE
  SYNTAX INTEGER (16..991)
  ACCESS read-write
  STATUS mandatory
  DESCRIPTION
	"This objects specifies the status of the LMI Spoofing."
  ::= { dlcLMIConfiguration 2 }

  dlcManagementDLCI OBJECT-TYPE
  SYNTAX INTEGER (16..991)
  ACCESS read-write
  STATUS mandatory
  DESCRIPTION
	"This objects specifies the number of management DLCI
	between 16 and 991 inclusive. Must not be the same as
	maintenance DLCI. Default is 989."
  ::= { dlcLMIConfiguration 3 }

  dlcManagementDLCIEnable OBJECT-TYPE
  SYNTAX INTEGER {
          enabled (1),
          disabled (2)
         }
  ACCESS read-write
  STATUS mandatory
  DESCRIPTION
	"This object specifies whether to enable or disable
	Management DLCI."
  ::= { dlcLMIConfiguration 4 }

  dlcSpoofingProtocolType OBJECT-TYPE
  SYNTAX INTEGER {
          frf1-0  (1),
	  annex-d (2),
          annex-a (3)
         }
  ACCESS read-write
  STATUS mandatory
  DESCRIPTION
	"This objects configures which LMI message type to 'spoof',
	in the event that spoofing is activated. The possible choices
        are FRF 1.0 (Frame Relay Forum's early LMI adaptation),
	Annex A (ITU Q.933, annex A), and Annex D (ANSI T1.617, annex D).
	Default is Annex D."
  ::= { dlcLMIConfiguration 5 }
  
  dlcDTESpoofingEnable OBJECT-TYPE
  SYNTAX INTEGER {
          enabled (1),
          disabled (2)
         }
  ACCESS read-write
  STATUS mandatory
  DESCRIPTION
	"If the value of this object is 'enabled', LMI spoofing to
         the DTE port is done automatically."
  ::= { dlcLMIConfiguration 6 }
  
  dlcNetSpoofingEnable OBJECT-TYPE
  SYNTAX INTEGER {
          enabled (1),
          disabled (2)
         }
  ACCESS read-write
  STATUS mandatory
  DESCRIPTION
	"If the value of this object is 'enabled', LMI spoofing to
         the Network port is done automatically."
  ::= { dlcLMIConfiguration 7 }
  
  dlcLinkIntegrityVerificationPollingTimer OBJECT-TYPE
  SYNTAX INTEGER (5..30)
  ACCESS read-write
  STATUS mandatory
  DESCRIPTION
	"This object is the interval in seconds between Link
	Integrity Verification Messages. This is known as T391
	in the standards. Possible values are 5 to 30 seconds
	in increments of 5. Default is 10 seconds." 
  ::= { dlcLMIConfiguration 8 }

  dlcFullStatusPollingCounter OBJECT-TYPE
  SYNTAX INTEGER (1..255)
  ACCESS read-write
  STATUS mandatory
  DESCRIPTION
	"This value corresponds to N391 in the ITU and ANSI standards.
        It is the number of polling cycles of the type before the
        unit generates the full status request.
	Possible values are 1 to 255. Default is 10." 
  ::= { dlcLMIConfiguration 9 }

  dlcLMIErrorEvent OBJECT-TYPE
  SYNTAX INTEGER (1..10)
  ACCESS read-write
  STATUS mandatory
  DESCRIPTION
	"This object configures the parameter which sets the number
        of errors that can occur on the LMI link in LMI Error
        Monitored Events before reporting an interface down.
        Applies to the DTE and Network	ports. This is used in the
        same manner as the N392 parameter (N2) in the standards.
        Possible values are 1 to 10. Default is 1." 
  ::= { dlcLMIConfiguration 10 }

  dlcLMIErrorMonitorEvent OBJECT-TYPE
  SYNTAX INTEGER (1..10)
  ACCESS read-write
  STATUS mandatory
  DESCRIPTION
	"This object configures parameter which	sets the number of
        events that are used with the LMI Error Event to define
        the error ratio that is used to report when the LMI link
        is down. Applies to the DTE and Network	ports.
        This value is used in the same manner as the N393 parameter
        (N3) in the standards."
  ::= { dlcLMIConfiguration 11 }

  dlcLMIErrorFreeEvent OBJECT-TYPE
  SYNTAX INTEGER (1..10)
  ACCESS read-write
  STATUS mandatory
  DESCRIPTION
	"This object configures the parameter which sets the number
        of error-free messages that must be received in LMI Error
        Free Monitored Events prior to declaring an interface is up.
        Applies to the DTE and Network	ports. For network spoofing,
        this is the number of error-free events that will cause the
        DSU to believe the network is back up and runnig, and the
        DSU will stop spoofing. This is used in the same manner as
        the N392 parameter (N2) in the standards.
        Possible values are 1 to 10. Default is 1." 
  ::= { dlcLMIConfiguration 12 }

  dlcLMIErrorFreeMonitorEvent OBJECT-TYPE
  SYNTAX INTEGER (1..10)
  ACCESS read-write
  STATUS mandatory
  DESCRIPTION
	"This object configures parameter which	sets the number of
        events that are used with the LMI Error Free Event to define
        the error ratio that is used to report that the LMI link
        is up. Applies to the DTE and Network ports.
        This is used in the same manner as the N393 parameter (N3)
        in the standards."
  ::= { dlcLMIConfiguration 13 }

  dlcDTEResponseTimer OBJECT-TYPE
  SYNTAX INTEGER (5..30)
  ACCESS read-write
  STATUS mandatory
  DESCRIPTION
	"This object configures the parameter which sets the number
        of seconds between the sending of a status enquiry message
        and the receipt of a response. Applies to the DTE port.
        This value is similar in nature to the T392 (T2) parameter
        in the standards. Possible values are 5 to 30 in increments
        of 5. Default is 15."
  ::= { dlcLMIConfiguration 14 }

  dlcLMIUnitLocation OBJECT-TYPE
  SYNTAX INTEGER {
          cpe (1),
          co (2)
         }
  ACCESS read-write
  STATUS mandatory
  DESCRIPTION
	"This object configures where the unit is located so that LMI
         spoofing could be done properly. If 'cpe' is selected, it means
         the unit is located in Customer Premise. If 'co' is selected,
         the unit is in Central Office."
  ::= { dlcLMIConfiguration 15 }


  dlcLMIStatus OBJECT IDENTIFIER
  ::= { dlcLMIConditioningGroup 2 }

  dlcSpoofingStatus OBJECT-TYPE
  SYNTAX INTEGER {
          normal (1),
          dte-spoofing (2),
          net-spoofing (3),
          dte-and-net-spoofing (4)
         }
  ACCESS read-only
  STATUS mandatory
  DESCRIPTION
	"This object configures automatic LMI spoofing to the router.
	'True' means LMI spoofing will occur automatically,
	'False' means it's disabled."
  ::= { dlcLMIStatus 1 }

  dlcDTEInterfaceLMIStatus OBJECT-TYPE
  SYNTAX INTEGER {
          unknown (1),
          lmi-up (2),
          lmi-down (3)
         }
  ACCESS read-only
  STATUS mandatory
  DESCRIPTION
	"This object indicates the LMI status of DTE interface."
  ::= { dlcLMIStatus 2 }
  
  dlcNetInterfaceLMIStatus OBJECT-TYPE
  SYNTAX INTEGER {
          unknown (1),
          lmi-up (2),
          lmi-down (3)
         }
  ACCESS read-only
  STATUS mandatory
  DESCRIPTION
	"This object indicates the LMI status of Net interface."
  ::= { dlcLMIStatus 3 }
  
--
--   DL4200 MIB object
--

  dlcInbandDtePort OBJECT-TYPE
  SYNTAX INTEGER {
          data1 (1),
          data2 (2)
         }
  ACCESS read-write
  STATUS mandatory
  DESCRIPTION
	"This object configures which DTE port has inband management."
  ::= { dlcUnitConfig 42 }

-- *****************************************************************************
-- *****************************************************************************
-- *****************************************************************************
-- *****************************************************************************
--
-- Traps
--
 
  startTest TRAP-TYPE
  ENTERPRISE dl-new-t1
  VARIABLES { dlcUnitTestPortId, dlcUnitTestType }
  DESCRIPTION
  "Sent upon the initiation of a new test condition."
  ::= 1
 
 
  endTest TRAP-TYPE
  ENTERPRISE dl-new-t1
  VARIABLES { dlcUnitTestPortId, dlcUnitTestType }
  DESCRIPTION
  "Sent upon the termination of a test condition."
  ::= 2
 

  startAlarm TRAP-TYPE
  ENTERPRISE dl-new-t1
  VARIABLES { dlcAlarmPort, dlcAlarmType }
  DESCRIPTION
  "Indicates the beginning of a new alarm condition."
  ::= 3

 
  endAlarm TRAP-TYPE
  ENTERPRISE dl-new-t1
  VARIABLES { dlcAlarmPort, dlcAlarmType }
  DESCRIPTION
  "Indicates the end of an alarm condition."
  ::= 4


  manualConfigChange TRAP-TYPE
  ENTERPRISE dl-new-t1
  DESCRIPTION
  "Generated when the configuration was changed manually
   on the unit."
  ::= 5

END
