-- Unit User Archives
--
-- The eclipse has the capability to support two NET ports,
-- so we define several sets of User Archives.
--

dlcUnitUserArchive OBJECT IDENTIFIER ::= { dl-new-t1 4 }

 dlcUnitUserArchiveValidTable OBJECT-TYPE
  SYNTAX SEQUENCE OF DlcUserArchiveValidEntry
  ACCESS not-accessible
  STATUS mandatory
  DESCRIPTION
   "A table of all port-related status"
  --DEFAULT next-function next_dlcArchiveTable 
  ::= { dlcUnitUserArchive 1 }

 dlcUserArchiveValidEntry OBJECT-TYPE
  SYNTAX DlcUserArchiveValidEntry
  ACCESS not-accessible
  STATUS mandatory
  DESCRIPTION
   "An entry in the user archive valid-intervals table."
  INDEX { dlcValidPortId }
  ::= { dlcUnitUserArchiveValidTable 1 }

 DlcUserArchiveValidEntry ::=
  SEQUENCE {
   dlcValidPortId  PortId,
   dlcValidIntervals INTEGER
  }

  dlcValidPortId OBJECT-TYPE
   SYNTAX PortId
   ACCESS read-only
   STATUS mandatory
   DESCRIPTION
    "Identifies which port
    (`net' or `otherNet')
    this entry is describing."
   --DEFAULT get-function get_compl_0
   ::= { dlcUserArchiveValidEntry 1 }

  dlcValidIntervals OBJECT-TYPE
   SYNTAX INTEGER ( 1..96 )
   ACCESS read-write
   STATUS mandatory
   DESCRIPTION
    "The number of valid intervals
    in the user's error register database
    for this port.
    Writing a 0 to this variable
    will clear the entire archive
    for this port.
    Writing any other value is a no-op."
   --DEFAULT get-function get_u8_supplier
   --DEFAULT cookie (char *)&GDB.Dbms.archive.local.misc.validIntervals
   ::= { dlcUserArchiveValidEntry 2 }

