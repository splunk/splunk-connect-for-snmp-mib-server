-- *****************************************************************************
--
-- Delay Monitor Status Group
--
-- To start a test running against a port,
-- just set one of the values in the dlcDelayMonitorStatusTable.
-- To stop a test, set the testType to `noTest'.
--

 dlcUnitDelayMonitorStatus OBJECT IDENTIFIER ::= { dl-new-t1 6 }

 dlcDelayMonitorStatusTable OBJECT-TYPE
  SYNTAX SEQUENCE OF DlcDelayMonitorStatusEntry
  ACCESS not-accessible
  STATUS mandatory
  DESCRIPTION
  "Table of Delay Monitor status."
  ::= { dlcUnitDelayMonitorStatus 1 }

 dlcDelayMonitorStatusEntry OBJECT-TYPE
  SYNTAX DlcDelayMonitorStatusEntry
  ACCESS not-accessible
  STATUS mandatory
  DESCRIPTION
   "An entry in the network configuration table."
  INDEX { dlcDelayMonitorStatusIndex }
  ::= { dlcDelayMonitorStatusTable 1 }

 DlcDelayMonitorStatusEntry ::=
  SEQUENCE {
   dlcDelayMonitorStatusIndex		INTEGER,
   dlcDelayMonitorSentPackets		Gauge,
   dlcDelayMonitorReceivedPackets	Gauge,
   dlcDelayMonitorErroredPackets	Gauge,
   dlcDelayMonitorMissingPackets	Gauge,
   dlcDelayMonitorAverageRoundTrip	Gauge,
   dlcDelayMonitorMaximumRoundTrip	Gauge
  }

  dlcDelayMonitorStatusIndex OBJECT-TYPE
   SYNTAX INTEGER ( 1..4 )
   ACCESS read-only
   STATUS mandatory
   DESCRIPTION
    "Uniquely identifies which Delay Monitor test in the
    configuration table."
   ::= { dlcDelayMonitorStatusEntry 1 }

  dlcDelayMonitorSentPackets OBJECT-TYPE
   SYNTAX Gauge
   ACCESS read-only
   STATUS mandatory
   DESCRIPTION
    "Number of sent packets for the currently running delay monitor
    test."
   ::= { dlcDelayMonitorStatusEntry 2 }

  dlcDelayMonitorReceivedPackets OBJECT-TYPE
   SYNTAX Gauge
   ACCESS read-only
   STATUS mandatory
   DESCRIPTION
    "Number of good received packets for the currently running
    delay monitor test."
   ::= { dlcDelayMonitorStatusEntry 3 }

  dlcDelayMonitorErroredPackets OBJECT-TYPE
   SYNTAX Gauge
   ACCESS read-only
   STATUS mandatory
   DESCRIPTION
    "Number of errored received packets for the currently running
    delay monitor test."
   ::= { dlcDelayMonitorStatusEntry 4 }

  dlcDelayMonitorMissingPackets OBJECT-TYPE
   SYNTAX Gauge
   ACCESS read-only
   STATUS mandatory
   DESCRIPTION
    "Number of missing packets for the currently running delay
    monitor test."
   ::= { dlcDelayMonitorStatusEntry 5 }

  dlcDelayMonitorAverageRoundTrip OBJECT-TYPE
   SYNTAX Gauge
   ACCESS read-only
   STATUS mandatory
   DESCRIPTION
    "Average round trip time in milli seconds for the currently
    running delay monitor test."
    ::= { dlcDelayMonitorStatusEntry 6 }

  dlcDelayMonitorMaximumRoundTrip OBJECT-TYPE
   SYNTAX Gauge
   ACCESS read-only
   STATUS mandatory
   DESCRIPTION
