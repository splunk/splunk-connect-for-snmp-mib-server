 dlcUnitModemConfig OBJECT IDENTIFIER ::= { dlcUnitConfig 27 }

  dlcModemPhoneNum1 OBJECT-TYPE
   SYNTAX DisplayString (SIZE (0..20))
   ACCESS read-write
   STATUS mandatory
   DESCRIPTION
    "The first phone number to dial
    when the unit needs to set up a 
    modem connection."
   --DEFAULT test-function test_string_supplier
   --DEFAULT get-function get_string_supplier
   --DEFAULT cookie (char *)&GDB.Dbms.config.comm.phoneNumber[0]
   ::= { dlcUnitModemConfig 1 }

  dlcModemPhoneNum2 OBJECT-TYPE
   SYNTAX DisplayString (SIZE (0..20))
   ACCESS read-write
   STATUS mandatory
   DESCRIPTION
    "The second phone number to try 
    if the phone number failed in 
    setting up a modem connection."
   --DEFAULT test-function test_string_supplier
   --DEFAULT get-function get_string_supplier
   --DEFAULT cookie (char *)&GDB.Dbms.config.comm.phoneNumber[1]
   ::= { dlcUnitModemConfig 2 }

  dlcModemInitString1 OBJECT-TYPE
   SYNTAX DisplayString (SIZE (1..20))
   ACCESS read-write
   STATUS mandatory
   DESCRIPTION
    "In setting up a modem connection, 
    the first group of commands used  
    to initialize the modem."
   --DEFAULT test-function test_string_supplier
   --DEFAULT get-function get_string_supplier
   --DEFAULT cookie (char *)&GDB.Dbms.ext_5.config.comm.modemInitStr1
   ::= { dlcUnitModemConfig 3 }

  dlcModemInitString2 OBJECT-TYPE
   SYNTAX DisplayString (SIZE (1..60))
   ACCESS read-write
   STATUS mandatory
   DESCRIPTION
    "In setting up a modem connection, 
    the second group of commands used
    to initialize the modem.  There is a
    delay between sending the first and 
    second groups of initialzation commands."
   --DEFAULT test-function test_string_supplier
   --DEFAULT get-function get_string_supplier
   --DEFAULT cookie (char *)&GDB.Dbms.ext_5.config.comm.modemInitStr2
   ::= { dlcUnitModemConfig 4 }

-- ============================================================================
--
-- Configuration items related to inband management
--
	dlcUnitInbandMode OBJECT-TYPE
	SYNTAX INTEGER {
		noInband	( 1 ),
		frameRelay 	( 2 ),
		ciscoHdlc	( 3 )
	}
	ACCESS read-only
	STATUS mandatory
	DESCRIPTION
	"The type of traffic expected on the link for inband management and IPM."
	::= { dlcUnitConfig 28 }


-- ****************************************************************************
-- ****************************************************************************
-- ****************************************************************************
-- ****************************************************************************
--
-- Unit Status Group
--
--
-- New types defined in this MIB
--

-- UnitStatusItem is used to represent the status of the unit.
-- It is the sum of the following binary values.
-- More than one such item can be in effect, thus it is a bitmap.
-- When `normal' is active, no other item is active.
--
 UnitStatusItem ::= INTEGER {
   normal    (  1),
   noClock    (  2),
   testInProgress   (  4),
   sendKeepAlive   (  8),
   sendYellowAlarm   ( 16),
   reserved1   ( 32),
   reserved2   ( 64),
   reserved3   (128),
   reserved4   (256),
   reserved5   (512)
  }


-- PortStatusItem represents the status of the received signal on a port.
-- More than one such item can be in effect.
-- When `normal' is active, no other item is active.
--
-- Note that PortStatusItem is used for all ports (Dsx1 and Data DTE).
--
-- For E1 ports, the Eclipse reports "yellowAlarmDetected" upon recognition
-- of Remote Alarm Indication.
--
 PortStatusItem ::= INTEGER {
   normal    ( 1),
   lossOfSignal   ( 2),
   lossOfSync   ( 3),
   yellowAlarmDetected  ( 4),
   blueAlarmDetected  ( 5),
   setCodeReceived   ( 6),
   resetCodeReceived  ( 7),
   excessiveErrorRate  ( 8),
   localTest   ( 9),
    eBitReceived   (10),
    reserved1   (11),
    reserved2   (12),
    reserved3   (13),
    reserved4   (14)
  }

-- =============================================================================
-- =============================================================================
--
-- The following variables describe the (not-user-settable) status
-- of the unit.
--
dlcUnitStatus OBJECT IDENTIFIER ::= { dl-new-t1 3 }

 dlcUnitCurrentStatus OBJECT-TYPE
  SYNTAX UnitStatusItem
  ACCESS read-only
  STATUS mandatory
  DESCRIPTION
   "What's going on in this unit?"
  --DEFAULT test-function it_exists
  ::= { dlcUnitStatus 1 }

 dlcUnitErrorFreeSeconds OBJECT-TYPE
  SYNTAX Gauge
  ACCESS read-only
  STATUS mandatory
  DESCRIPTION
   "Represents the percent of error free seconds
   over the last 24 hours.
   The number is multiplied by 10
   and rounded to the nearest integer.
   For example, 94.2% is encoded as 942."
  --DEFAULT test-function it_exists
  --DEFAULT get-function get_u16_supplier
  --DEFAULT cookie (char *)&GDB.Dbms.archive.local.misc.efs
  ::= { dlcUnitStatus 2 }

 dlcUnitLastSelfTestResult OBJECT-TYPE
  SYNTAX INTEGER ( 0..1023 )
  ACCESS read-only
  STATUS mandatory
  DESCRIPTION
   "The result of the last self test.
   unitLastSelfTest is a bitmap ... the sum of:
      0 Selftest passed 
      1 ROM checksum failure
      2 RAM test failure
      4 EEPROM CRC failure
      8 DTE test failure
     16 Pattern loss during test
     32 FLASH checksum failure
     64 reserved
    128 reserved
    256 reserved
    512 reserved
   A result of 0 means no errors occurred.
   "
  --DEFAULT test-function it_exists
  ::= { dlcUnitStatus 3 }

