                  trpzApConfRadioProfileRFSpectralScanGroupRev2,
                  trpzApConfRadioProf32ServiceProfileTableGroup,
                  trpzApConfRadioProf32SnoopFilterTableGroup,
                  trpzApConfRemoteSiteTableGroupRev2
                }

