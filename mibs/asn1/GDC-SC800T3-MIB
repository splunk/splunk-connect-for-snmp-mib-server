-- ********************************************************************
--  			Interval Near End
-- ********************************************************************
sc800t3NearEndIntervalTable 	OBJECT-TYPE
    SYNTAX  SEQUENCE OF Sc800t3NearEndIntervalEntry
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
        "The sc800t3 Near End Interval table."
    ::= { sc800t3Performance 7 }

sc800t3NearEndIntervalEntry 	OBJECT-TYPE
    SYNTAX  Sc800t3NearEndIntervalEntry
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
        "An entry in the Interval table."
    INDEX   { sc800t3NearEndIntervalIndex, sc800t3NearEndIntervalNumber }
    ::= { sc800t3NearEndIntervalTable 1 }

Sc800t3NearEndIntervalEntry ::=
    SEQUENCE {
        sc800t3NearEndIntervalIndex		SCinstance,
        sc800t3NearEndIntervalNumber		INTEGER,
        sc800t3NearEndIntervalStats		OCTET STRING
}

sc800t3NearEndIntervalIndex OBJECT-TYPE
    SYNTAX  SCinstance
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
        "Integer value which uniquely identifies the SC800T3 to which this
         entry is applicable. SCinstance is defined to be SLDi where:
         S (byte value) - physical shelf slot location (01-32)
         L (byte value) - line number (01)
         D (byte value) - drop number (00)
         i (byte value) - interface number (00)"
    ::= { sc800t3NearEndIntervalEntry 1 }

sc800t3NearEndIntervalNumber 	OBJECT-TYPE
    SYNTAX  INTEGER (1..31)
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
        "A number between 1 and 31, where 1 is the most recently completed 
        minute interval and 31 is the least recently completed 15 minutes  
        interval   (assuming   that  all  31  intervals  are valid)."
    ::= { sc800t3NearEndIntervalEntry 2 }

sc800t3NearEndIntervalStats OBJECT-TYPE
	 SYNTAX OCTET STRING (SIZE (11))
	 ACCESS read-only
	 STATUS mandatory
	 DESCRIPTION
		"Returns a bitwise snapshot of the  statistics.

		Octet 1  bit 7 - parity   
			 bit 6 - Invalid Data Flag
			 bit 5 - n/a
			 bit 4 - n/a
			 bit 3 - n/a
			 bit 2 - 2^9    Errored Seconds (ES)
			 bit 1 - 2^8
			 bit 0 - 2^7 

		Octet 2  bit 7 - parity   
			 bit 6 - 2^6    Errored Seconds (ES)
			 bit 5 - 2^5 
			 bit 4 - 2^4 
			 bit 3 - 2^3 
			 bit 2 - 2^2 
			 bit 1 - 2^1
			 bit 0 - 2^0

		Octet 3  bit 7 - parity Severely Errored Seconds (SES)  
			 bit 6 - Invalid Data Flag
			 bit 5 - 2^5
			 bit 4 - 2^4
			 bit 3 - 2^3
			 bit 2 - 2^2
			 bit 1 - 2^1
			 bit 0 - 2^0

		Octet 4  bit 7 - parity Severely Errored Frames (SAS)  
			 bit 6 - Invalid Data Flag
			 bit 5 - 2^5
			 bit 4 - 2^4
			 bit 3 - 2^3
			 bit 2 - 2^2
			 bit 1 - 2^1
			 bit 0 - 2^0 

		Octet 5  bit 7 - parity Unavailable Seconds (UAS)  
			 bit 6 - Invalid Data Flag
			 bit 5 - 2^5 
			 bit 4 - 2^4 
			 bit 3 - 2^3 
			 bit 2 - 2^2 
			 bit 1 - 2^1 
			 bit 0 - 2^0

		Octet 6  bit 7 - parity Code Violations (CV)    	  
			 bit 6 - Invalid Data Flag
			 bit 5 - n/a 
			 bit 4 - n/a 
			 bit 3 - n/a 
			 bit 2 - n/a
			 bit 1 - n/a
			 bit 0 - n/a

		Octet 7  bit 7 - parity   
			 bit 6 - 2^13   Code Violations (CV)
			 bit 5 - 2^12
			 bit 4 - 2^11
			 bit 3 - 2^10
			 bit 2 - 2^9
			 bit 1 - 2^8
			 bit 0 - 2^7

		Octet 8  bit 7 - parity   
			 bit 6 - 2^6    Code Violations (CV)
			 bit 5 - 2^5 
			 bit 4 - 2^4 
			 bit 3 - 2^3 
			 bit 2 - 2^2
			 bit 1 - 2^1
			 bit 0 - 2^0

		Octet 9  bit 7 - parity   
			 bit 6 - Invalid Data Flag
			 bit 5 - n/a
			 bit 4 - n/a
			 bit 3 - n/a
			 bit 2 - 2^9    Errored Seconds Line (ESl)
			 bit 1 - 2^8
			 bit 0 - 2^7 

		Octet 10 bit 7 - parity   
			 bit 6 - 2^6    Errored Seconds Line (ESl)
			 bit 5 - 2^5 
			 bit 4 - 2^4 
			 bit 3 - 2^3 
			 bit 2 - 2^2 
			 bit 1 - 2^1
			 bit 0 - 2^0

		Octet 11 bit 7 - parity   
			 bit 6 - n/a    Interval (1-31)
			 bit 5 - n/a 
			 bit 4 - 2^4 
			 bit 3 - 2^3 
			 bit 2 - 2^2 
			 bit 1 - 2^1
			 bit 0 - 2^0"
    ::= { sc800t3NearEndIntervalEntry 3 }

-- ********************************************************************
--  			Interval Far End
-- ********************************************************************
sc800t3FarEndIntervalTable 	OBJECT-TYPE
    SYNTAX  SEQUENCE OF Sc800t3FarEndIntervalEntry
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
        "The sc800t3 Far End Interval table."
    ::= { sc800t3Performance 9 }

sc800t3FarEndIntervalEntry 	OBJECT-TYPE
    SYNTAX  Sc800t3FarEndIntervalEntry
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
        "An entry in the Interval table."
    INDEX   { sc800t3FarEndIntervalIndex, sc800t3FarEndIntervalNumber }
    ::= { sc800t3FarEndIntervalTable 1 }

Sc800t3FarEndIntervalEntry ::=
    SEQUENCE {
        sc800t3FarEndIntervalIndex	SCinstance,
        sc800t3FarEndIntervalNumber	INTEGER,
        sc800t3FarEndIntervalStats	OCTET STRING
}

sc800t3FarEndIntervalIndex OBJECT-TYPE
    SYNTAX  SCinstance
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
        "Integer value which uniquely identifies the SC800T3 to which this
         entry is applicable. SCinstance is defined to be SLDi where:
         S (byte value) - physical shelf slot location (01-32)
         L (byte value) - line number (01)
         D (byte value) - drop number (00)
         i (byte value) - interface number (00)"
    ::= { sc800t3FarEndIntervalEntry 1 }

sc800t3FarEndIntervalNumber 	OBJECT-TYPE
    SYNTAX  INTEGER (1..31)
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
        "A number between 1 and 31, where 1 is the most recently completed 
        minute interval and 31 is the least recently completed 15 minutes  
        interval   (assuming   that  all  31  intervals  are valid)."
    ::= { sc800t3FarEndIntervalEntry 2 }

sc800t3FarEndIntervalStats OBJECT-TYPE
	 SYNTAX OCTET STRING (SIZE (9))
	 ACCESS read-only
	 STATUS mandatory
	 DESCRIPTION
		"Returns a bitwise snapshot of the statistics.

		Octet 1  bit 7 - parity   
			 bit 6 - Invalid Data Flag
			 bit 5 - n/a
			 bit 4 - n/a
			 bit 3 - n/a
			 bit 2 - 2^9    Errored Seconds (ES)
			 bit 1 - 2^8
			 bit 0 - 2^7 

		Octet 2  bit 7 - parity   
			 bit 6 - 2^6    Errored Seconds (ES)
			 bit 5 - 2^5 
			 bit 4 - 2^4 
			 bit 3 - 2^3 
			 bit 2 - 2^2 
			 bit 1 - 2^1
			 bit 0 - 2^0

		Octet 3  bit 7 - parity Severely Errored Seconds (SES)  
			 bit 6 - Invalid Data Flag
			 bit 5 - 2^5
			 bit 4 - 2^4
			 bit 3 - 2^3
			 bit 2 - 2^2
			 bit 1 - 2^1
			 bit 0 - 2^0

		Octet 4  bit 7 - parity Severely Errored Frames (SAS)  
			 bit 6 - Invalid Data Flag
			 bit 5 - 2^5
			 bit 4 - 2^4
			 bit 3 - 2^3
			 bit 2 - 2^2
			 bit 1 - 2^1
			 bit 0 - 2^0 

		Octet 5  bit 7 - parity Unavailable Seconds (UAS)  
			 bit 6 - Invalid Data Flag
			 bit 5 - 2^5 
			 bit 4 - 2^4 
			 bit 3 - 2^3 
			 bit 2 - 2^2 
			 bit 1 - 2^1 
			 bit 0 - 2^0

		Octet 6  bit 7 - parity Code Violations (CV)    	  
			 bit 6 - Invalid Data Flag
			 bit 5 - n/a 
			 bit 4 - n/a 
			 bit 3 - n/a 
			 bit 2 - n/a
			 bit 1 - n/a
			 bit 0 - n/a

		Octet 7  bit 7 - parity   
			 bit 6 - 2^13   Code Violations (CV)
			 bit 5 - 2^12
			 bit 4 - 2^11
			 bit 3 - 2^10
			 bit 2 - 2^9
			 bit 1 - 2^8
			 bit 0 - 2^7

		Octet 8  bit 7 - parity   
			 bit 6 - 2^6    Code Violations (CV)
			 bit 5 - 2^5 
			 bit 4 - 2^4 
			 bit 3 - 2^3 
			 bit 2 - 2^2
			 bit 1 - 2^1
			 bit 0 - 2^0

		Octet 9  bit 7 - parity   
			 bit 6 - n/a    Interval (1-31)
			 bit 5 - n/a 
			 bit 4 - 2^4 
			 bit 3 - 2^3 
			 bit 2 - 2^2 
			 bit 1 - 2^1
			 bit 0 - 2^0"

    ::= { sc800t3FarEndIntervalEntry 3}

-- ********************************************************************
--  			Previous Near End
-- ********************************************************************
