 -- ********************************************************************
 -- GDC Local SC800T3 Alarm Configuration
 -- ********************************************************************

 sc800t3LocalAlarmConfigTable OBJECT-TYPE
     SYNTAX SEQUENCE OF Sc800t3LocalAlarmConfigEntry
     ACCESS not-accessible
     STATUS mandatory
     DESCRIPTION
	 "The sc800t3LocalAlarmConfigTable contains entries that configure 
	 alarm
	 reporting.  The structure of the table is such that alarm
	 configuration is supported on a unit and interface basis, and
	 then on an alarm type basis within the interface.  For
	 simplicity sake alarms, be they unit or interface
	 related, are represented in one table."
     ::= { sc800t3LocalAlarms 1 }

 sc800t3LocalAlarmConfigEntry OBJECT-TYPE
     SYNTAX Sc800t3LocalAlarmConfigEntry
     ACCESS not-accessible
     STATUS mandatory
     DESCRIPTION
	 "An entry in the GDC Local Alarm Configuration table."
     INDEX { sc800t3LocalAlarmConfigIndex }
     ::= { sc800t3LocalAlarmConfigTable 1 }

 Sc800t3LocalAlarmConfigEntry ::=
     SEQUENCE {
	 sc800t3LocalAlarmConfigIndex
	     SCinstance,
	 sc800t3OOF
	     INTEGER,
	 sc800t3AISLOCAL
	     INTEGER,
	 sc800t3TIMLOS
	     INTEGER,
	 sc800t3TXDLOS
	     INTEGER,
	 sc800t3RXDLOS
	     INTEGER,
	 sc800t3LOS
	     INTEGER,
	 sc800t3TPSFAIL
	     INTEGER,
	 sc800t3BPSFAIL
	     INTEGER,
	 sc800t3DTECLKLOS
	     INTEGER,
	 sc800t3DTERDYLOS
	     INTEGER,
	 sc800t3RAILOCAL
	     INTEGER,
	 sc800t3THRESHESL
	     INTEGER,
         sc800t3THRESHES
             INTEGER,
         sc800t3THRESHSES
             INTEGER,
         sc800t3THRESHSAS
             INTEGER,
         sc800t3THRESHUAS
             INTEGER,
         sc800t3THRESHCV
             INTEGER
     }

 sc800t3LocalAlarmConfigIndex OBJECT-TYPE
     SYNTAX SCinstance
     ACCESS read-only
     STATUS mandatory
     DESCRIPTION
	 "The index value which uniquely identifies the interface to
	 which this entry is applicable."
     ::= { sc800t3LocalAlarmConfigEntry 1 }

 sc800t3OOF       OBJECT-TYPE
	SYNTAX  INTEGER {
			disabled(1),
			enabledMajor(2),
			enabledMinor(3)
			}
	ACCESS  read-write
	STATUS  mandatory
	DESCRIPTION
		"Disables or enables the alarm on the Major buss or Minor
		buss."
     ::= { sc800t3LocalAlarmConfigEntry 2 }

 sc800t3AISLOCAL       OBJECT-TYPE
	SYNTAX  INTEGER {
			disabled(1),
			enabledMajor(2),
			enabledMinor(3)
			}
	ACCESS  read-write
	STATUS  mandatory
	DESCRIPTION
		"Disables or enables the alarm on the Major buss or Minor
		buss."
     ::= { sc800t3LocalAlarmConfigEntry 3 }

 sc800t3TIMLOS       OBJECT-TYPE
	SYNTAX  INTEGER {
			disabled(1),
			enabledMajor(2),
			enabledMinor(3)
			}
	ACCESS  read-write
	STATUS  mandatory
	DESCRIPTION
		"Disables or enables the alarm on the Major buss or Minor
		buss."
     ::= { sc800t3LocalAlarmConfigEntry 4 }

 sc800t3TXDLOS       OBJECT-TYPE
