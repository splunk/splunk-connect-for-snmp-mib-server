    SYNTAX PnniNodeId 
    MAX-ACCESS not-accessible
    STATUS current
    DESCRIPTION
	"The nodes on the Reachability tree."
    ::= { pnniSpanningTreeMapEntry 1 }

pnniSpanningTreeMapStatus OBJECT-TYPE
   SYNTAX  INTEGER {
		uptodate(1),
		linkadded(2),
		linkdeleted(3),
		treeclean(4),
		pglchanged(5)
	   }
    MAX-ACCESS read-only
    STATUS current
    DESCRIPTION
	"The status of the spanning tree. If links are added or
	removed then the spanning tree may not be up to date for a
	while until the tree is updated. If there are link added
	and deleted, the status will be linkdeleted"
    ::= { pnniSpanningTreeMapEntry 2 }

