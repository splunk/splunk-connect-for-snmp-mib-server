    SYNTAX INTEGER {
	hlink(1),
	uplink(2)
	}
    MAX-ACCESS read-only
    STATUS current
    DESCRIPTION
	"The type of the link that connects this noe to the remote node"
    ::= { pnniSpanningTreeMapEntry 5 }
