TPT-DDOS-MIB

DEFINITIONS ::= BEGIN

IMPORTS
  MODULE-IDENTITY, OBJECT-TYPE, 
  Unsigned32, Counter64
    FROM SNMPv2-SMI

  tpt-tpa-objs
    FROM TPT-TPAMIBS-MIB
  ;

tpt-ddos MODULE-IDENTITY
  LAST-UPDATED "201605251854Z" -- May 25, 2016
  ORGANIZATION "Trend Micro, Inc."
  CONTACT-INFO "www.trendmicro.com"
  DESCRIPTION
