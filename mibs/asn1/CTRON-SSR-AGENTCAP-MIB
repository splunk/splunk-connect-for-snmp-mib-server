CTRON-SSR-AGENTCAP-MIB DEFINITIONS ::= BEGIN

IMPORTS
    MODULE-IDENTITY
       FROM SNMPv2-SMI
    AGENT-CAPABILITIES
       FROM  SNMPv2-CONF
    etherMIB
       FROM EtherLike-MIB
    ifMIB
       FROM IF-MIB
    ipMIB
       FROM IP-MIB
    udpMIB
       FROM UDP-MIB
    tcpMIB
       FROM TCP-MIB
    bgp
       FROM BGP4-MIB
    ospf
       FROM OSPF-MIB
    ospfTrap
       FROM OSPF-TRAP-MIB
    rip2
       FROM RIPv2-MIB
    dot1dBase
       FROM BRIDGE-MIB
    qBridgeMIB
       FROM Q-BRIDGE-MIB       
    pBridgeMIB
       FROM P-BRIDGE-MIB
    frameRelayDTE
       FROM FRAME-RELAY-DTE-MIB
    ppp
       FROM PPP-LCP-MIB
    pppSecurity
       FROM PPP-SEC-MIB
    pppIp
       FROM PPP-IP-NCP-MIB
    ds1
       FROM DS1-MIB
    ds3
       FROM DS3-MIB
    sonetMIB
       FROM SONET-MIB
    atmMIB
       FROM ATM-MIB
    radiusAuthClientMIB
       FROM RADIUS-AUTH-CLIENT-MIB
    rmon
       FROM RMON-MIB   
    rmon
       FROM RMON2-MIB    
    vrrpMIB
       FROM VRRP-MIB
    ctLFAP
       FROM CTRON-LFAP-MIB
    policyMIB
       FROM CTRON-SSR-POLICY-MIB
    ssrConfigMIB
       FROM CTRON-SSR-CONFIG-MIB
    hardwareMIB
       FROM CTRON-SSR-HARDWARE-MIB
    capacityMIB
       FROM CTRON-SSR-CAPACITY-MIB
    ssrMibs 
       FROM CTRON-SSR-SMI-MIB; 

ctronSsrAgentCapabilityMIB MODULE-IDENTITY
   LAST-UPDATED
     "0002190000Z"
   ORGANIZATION
     "Cabletron Systems, Inc."
   CONTACT-INFO
