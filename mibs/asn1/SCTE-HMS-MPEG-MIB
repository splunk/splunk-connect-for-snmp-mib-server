  ::= { mpegMIBGroups 4 }

mpegMappingsGroup  OBJECT-GROUP
  OBJECTS { mpegProgramMappingOutputProgIndex, 
    mpegProgramMappingOutputTSIndex,
    mpegProgramMappingInputProgIndex, 
    mpegProgramMappingInputTSIndex }
  STATUS     current
