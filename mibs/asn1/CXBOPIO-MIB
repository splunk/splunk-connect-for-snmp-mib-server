CXBOPIO-MIB DEFINITIONS ::= BEGIN


IMPORTS
   Counter                         FROM RFC1155-SMI
   OBJECT-TYPE                     FROM RFC-1212
   TRAP-TYPE                       FROM RFC-1215
   cxBOPIO,
   Alias                           FROM CXProduct-SMI;


--#CURRENT-VERSION 2

--  Active Configuration Table

--  The table describing:
--      active (current) parameters used in each BOP/COP ports.
--
--      parameters required to configure traps in each BOP/COP ports.
--
--      status and statistics for each BOP/COP port.


bopSapOprTable OBJECT-TYPE
   SYNTAX  SEQUENCE OF BopSapOprEntry
   ACCESS  not-accessible
   STATUS  mandatory
   DESCRIPTION
"A table containing the current configuration information about each
 BOP/COP port."
   ::= { cxBOPIO 1 }


   bopSapOprEntry OBJECT-TYPE
      SYNTAX  BopSapOprEntry
      ACCESS  not-accessible
      STATUS  mandatory
      DESCRIPTION
"The configuration information for a specific BOP/COP port."
      INDEX { bopSapOprNumber }
      ::= { bopSapOprTable 1 }
   BopSapOprEntry ::=
      SEQUENCE {
         bopSapOprNumber
            INTEGER,
         bopSapOprAlias
            Alias,
         bopSapOprProtocol
            INTEGER,
         bopSapOprPortClockSource
            INTEGER,
         bopSapOprPortSpeed
            INTEGER,
         bopSapOprPortDuplex
            INTEGER,
         bopSapOprPortMaxFrameSize
            INTEGER,
         bopSapOprPortDataEncoding
            INTEGER,
         bopSapOprPortIdleCondition
            INTEGER,
         bopSapOprPortPreAmbleLength
            INTEGER,
         bopSapOprPortFlagSharing
            INTEGER,
         bopSapOprPortFilterMask
            INTEGER,
         bopSapOprPortFilterAddress1
            INTEGER,
         bopSapOprPortFilterAddress2
            INTEGER,
         bopSapOprPortFilterAddress3
            INTEGER,
         bopSapOprPortFilterAddress4
            INTEGER,
         bopSapOprPortCrc
            INTEGER,
         bopSapOprPortDtrUpTimer
            INTEGER,
         bopSapOprPortTxWindow
            INTEGER,            
         bopSapOprPortDialMode
            INTEGER,
         bopSapOprPortLoopback
            INTEGER,
         bopSapOprPortSignalSamplingPeriod
            INTEGER,
         bopSapOprPortDcdDtrSignalSamples
            INTEGER,
         bopSapOprPortCtsRtsSignalSamples
            INTEGER,
         bopSapOprPortDsrDrsSignalSamples
            INTEGER,
         bopSapOprPortTmLlSignalSamples
            INTEGER,
         bopSapOprPortRiRlSignalSamples
            INTEGER,
         bopSapOprPortStatisticsTimer
            INTEGER,
         bopSapOprPortCarrierAction
            INTEGER,
         bopSapOprPortDataGenerator
            INTEGER,
         bopSapOprPortGenFrames
            INTEGER,
         bopSapOprPortGenFrameSize
            INTEGER,
         bopSapOprPortGenFrameHeader
            INTEGER,
         bopSapOprPortThreshBandwidth
            INTEGER,
         bopSapOprPortActualSpeed
            INTEGER,
         bopSapOprCodeSet
            INTEGER,
         bopSapOprParity
            INTEGER,
         bopOprPortTrap
            INTEGER,
         bopOprTxClockTrap
            INTEGER,
         bopOprControlLine
            INTEGER,
         bopOprControlStats
            INTEGER,
         bopStatOprPortType
            INTEGER,
         bopStatOprPortInterfaceType
            INTEGER,
         bopStatOprPortState
            INTEGER,
         bopStatOprTxClockState
            INTEGER,
         bopStatOprDCDState
            INTEGER,
         bopStatOprDTRState
            INTEGER,
         bopStatOprRTSState
            INTEGER,
         bopStatOprCTSState
            INTEGER,
         bopStatOprDSRState
            INTEGER,
         bopStatOprDRSState
            INTEGER,
         bopStatOprTMState
            INTEGER,
         bopStatOprLLState
            INTEGER,
         bopStatOprRIState
            INTEGER,
         bopStatOprRLState
            INTEGER,
         bopStatOprTxFrames
            Counter,
         bopStatOprRxFrames
            Counter,
         bopStatOprLocalLoopbackFrames
            Counter,
         bopStatOprTxBps
            Counter,
         bopStatOprRxBps
            Counter,
         bopStatOprTxBpsMax
            Counter,
         bopStatOprRxBpsMax
            Counter,
         bopStatOprTxFps
            Counter,
         bopStatOprRxFps
            Counter,
         bopStatOprTxFpsMax
            Counter,
         bopStatOprRxFpsMax
            Counter,
         bopStatOprTxUnderrunFrames
            Counter,
         bopStatOprTxCtsLostFrames
            Counter,
         bopStatOprTxBadStateDiscards
            Counter,
         bopStatOprTxResetDiscards
            Counter,
         bopStatOprTxSysCongestionDiscards
            Counter,
         bopStatOprTxInvWinDiscards
            Counter,
         bopStatOprTxInvRLpbkWinDiscards
            Counter,
         bopStatOprRxBadCrcFrames
            Counter,
         bopStatOprRxAbortedFrames
            Counter,
         bopStatOprRxNonIntegralBitFrames
            Counter,
         bopStatOprRxLongFrames
            Counter,
         bopStatOprRxOverrunFrames
            Counter,
         bopStatOprRxCdLostFrames
            Counter,
         bopStatOprRxBadStateDiscards
            Counter,
         bopStatOprRxBusyDiscards
            Counter,
         bopStatOprPortStateChanges
            Counter,
         bopStatOprTxClockStateChanges
            Counter,
         bopStatOprDCDStateChanges
            Counter,
         bopStatOprDTRStateChanges
            Counter,
         bopStatOprRTSStateChanges
            Counter,
         bopStatOprCTSStateChanges
            Counter,
         bopStatOprDSRStateChanges
            Counter,
         bopStatOprDRSStateChanges
            Counter,
         bopStatOprTMStateChanges
            Counter,
         bopStatOprLLStateChanges
            Counter,
         bopStatOprRIStateChanges
            Counter,
         bopStatOprRLStateChanges
            Counter,
         bopStatOprPortResets
            Counter,
         bopStatOprRxGenError
            Counter
         }


   bopSapOprNumber OBJECT-TYPE
      SYNTAX  INTEGER
      ACCESS  read-only
      STATUS  mandatory
      DESCRIPTION
"Defines the SAP ID using 2,3 or 4 digits. The first digit (A) is the slot #,
 the second digit (B) is the port #, and the remaining digits (CD) are the
 channel #.  If a 2 digit number is entered, digits CD are assumed to be 0 and
 are inserted automatically when reading object.  If a 3 digit number is
 entered, digit C is assumed to be 0 and is inserted automatically when
 reading object.

Ranges of Values for  A: 1-7 for CX900, 1-4 for CX1000
Ranges of Values for  B: 1-5 for CX900, 1-4 for CX1000
Ranges of Values for CD: 00-30

Default Value: not applicable"
      ::= { bopSapOprEntry 1 }


   bopSapOprAlias OBJECT-TYPE
      SYNTAX  Alias
      ACCESS  read-only
      STATUS  mandatory
      DESCRIPTION
"Determines the active textual name uniquely identifying this port SAP.

Range of Values: 0 - 16 alphanumeric characters beginning with a 
non-numeric character

Default Value: undefined"
      ::= { bopSapOprEntry 2 }


   bopSapOprProtocol OBJECT-TYPE
      SYNTAX  INTEGER {
               hdlc        (1),
               bsc3270     (2),
               borroughs   (3),
               sperry      (4),
               bsc3780     (5)               
               }
      ACCESS  read-only
      STATUS  mandatory
      DESCRIPTION
"Determines if the port is HDLC or Bisync.
Only HDLC is supported by ISDN and T1/E1 ports.

Indicates in the operative
configuration the type of bisync protocol 
used by the controllers of the
port associated with this SAP. 

Options:
hdlc
bsc3270    = Bisync 3270 controllers
borroughs  = Borroughs controllers
sperry     = Sperry controllers
bsc3780    = Bisync 2780/3780 controllers

Default Value: hdlc"
--#STARTING-VERSION 2
      ::= { bopSapOprEntry 3 }


-- gap (1) for future parameter

   
bopSapOprPortClockSource OBJECT-TYPE
      SYNTAX  INTEGER {
               internal  (1),
               external  (2),
               split     (3),
               slave     (4)
               }
      ACCESS  read-only
      STATUS  mandatory
      DESCRIPTION
"Determines the active data clock source for this port.

Options:
internal (1): Tx and Rx clocks are internally generated

external (2): Tx and Rx clocks are externally supplied on separate pins

split (3): Tx clock is internally generated and Rx clock is externally supplied

slave (4): Tx and Rx clocks are externally supplied on the same pins

Note: Split clocking is not supported in CX900 slots 5, 6 and 7.

Default Value: external (locked to internal for ISDN)"
      ::= { bopSapOprEntry 5 }


   bopSapOprPortSpeed OBJECT-TYPE
      SYNTAX  INTEGER (375 .. 2048000)
      ACCESS  read-only
      STATUS  mandatory
      DESCRIPTION
"Determines the input and output speed in bits per second for this port.

Note: Meaningful only if 'bopSapOprPortClockSource' is 'internal' or 'split'.

Ranges of Values: 375-2048000; limited to 1875-256000 for CX900 slots 5,6,7.

Default Value: 64,000"
      ::= { bopSapOprEntry 6 }


   bopSapOprPortDuplex OBJECT-TYPE
      SYNTAX  INTEGER {
               half            (1),
               full            (2)
               }
      ACCESS  read-only
      STATUS  mandatory
      DESCRIPTION
"Determines whether this port is communicating simultaneously in both
 directions (full-duplex) or alternates between directions (half-duplex).
 Not applicable for ISDN and T1/E1 ports.

Options:
half (1): half-duplex

full (2): full-duplex

Default Value: full"
      ::= { bopSapOprEntry 7 }


   bopSapOprPortMaxFrameSize OBJECT-TYPE
      SYNTAX  INTEGER (32..9000)
      ACCESS  read-only
      STATUS  mandatory
      DESCRIPTION
"Determines the active maximum frame size supported by this port. 

Ranges of Values: 32-9000

Default Value: 1,600 (512 for ISDN)"
      ::= { bopSapOprEntry 8 }


   
bopSapOprPortDataEncoding OBJECT-TYPE
      SYNTAX  INTEGER {
               nrz             (1),
               nrzi-mark       (2),
               nrzi-space      (3)
               }
      ACCESS  read-only
      STATUS  mandatory
      DESCRIPTION
"Determines the active data formatting and encoding used by this port.

Options:
n/a (0): object is not applicable

nrz (1): NRZ (locked for ISDN 'D' channel; ISDN 'B' channel can use only this 
value or NRZI-Space value)

nrzi-mark (2): NRZI-Mark

nrzi-space (3): NRZI-Space (ISDN 'B' channel can use only this value or NRZ 
value)

Note: CX900 Slot/port/channel 51x-71x can use only NRZ or NRZI-Space.

Default Value: nrz"
      ::= { bopSapOprEntry 9 }


   
bopSapOprPortIdleCondition OBJECT-TYPE
      SYNTAX  INTEGER {
               flags           (1),
               marks           (2),
               as400           (3)
               }
      ACCESS  read-only
      STATUS  mandatory
      DESCRIPTION
"Determines the active idle line condition supported by this port.

Options:
flags (1): flags

marks (2): marks

as400 (3): AS400

Default Value: flags"
      ::= { bopSapOprEntry 10 }


   bopSapOprPortPreAmbleLength OBJECT-TYPE
      SYNTAX  INTEGER (0 .. 16)
      ACCESS  read-only
      STATUS  mandatory
      DESCRIPTION
"Determines the minimum number of flags or sync pairs that will be inserted between frames
 by the port's transmitter.

For HDLC frames:
A value of zero causes flag sharing between frames.
A value of one means no flag sharing and zero flags inserted between frames.
A value of two means no flag sharing and one flag inserted between frames.
A value of three means no flag sharing and two flags inserted between frames.
Etc.

For Bisync frames:
A value of zero or one causes one sync pair to be inserted between frames.
A value of two causes two sync pairs to be inserted between frames.
A value of three causes three sync pairs to be inserted between frames.
Etc.

Ranges of Values: 0 -16

Default Value: 1"
      ::= { bopSapOprEntry 11 }


   bopSapOprPortFlagSharing OBJECT-TYPE
      SYNTAX  INTEGER {
               disabled        (1),
               enabled         (2)
               }
      ACCESS  read-only
      STATUS  obsolete
      DESCRIPTION
"Determines whether back-to-back Tx frames share opening and closing flag.
This value is meaningful only if 'bopSapAdmPortPreAmbleLength' is set to 
zero.

Options:
disabled (1): back-to-back frames do not share flag

enabled (2): back-to-back frames share flag

Default Value: disabled"
--#ENDING-VERSION 2      
::= { bopSapOprEntry 12 }


   bopSapOprPortFilterMask OBJECT-TYPE
      SYNTAX  INTEGER (0 .. 255)
      ACCESS  read-only
      STATUS  obsolete
      DESCRIPTION
"Indicates which bits of the 8-bit HDLC address are being used for address
 filtering on this port (CX900 or USIO only).

Note: When a bit is set to '1', the position of bits will be checked. When a bit is 
set to '0', it will not be checked.

Ranges of Values: 0 - 255

Default Value: 0"
--#ENDING-VERSION 2
      ::= { bopSapOprEntry 13 }


   bopSapOprPortFilterAddress1 OBJECT-TYPE
      SYNTAX  INTEGER (0 .. 255)
      ACCESS  read-only
      STATUS  obsolete
      DESCRIPTION
"Indicates one of four 8-bit addresses used for address filtering (CX900 or
 USIO only).

Ranges of Values: 0 - 255

Default Value: 126"
--#ENDING-VERSION 2
      ::= { bopSapOprEntry 14 }


   bopSapOprPortFilterAddress2 OBJECT-TYPE
      SYNTAX  INTEGER (0 .. 255)
      ACCESS  read-only
      STATUS  obsolete
      DESCRIPTION
"Indicates one of four 8-bit addresses used for address filtering (CX900 or
 USIO only).

Ranges of Values: 0 - 255

Default Value: 126"
--#ENDING-VERSION 2
      ::= { bopSapOprEntry 15 }


   bopSapOprPortFilterAddress3 OBJECT-TYPE
      SYNTAX  INTEGER (0 .. 255)
      ACCESS  read-only
      STATUS  obsolete
      DESCRIPTION
"Indicates one of four 8-bit addresses used for address filtering (CX900 or
 USIO only).

Ranges of Values: 0 - 255

Default Value: 126"
--#ENDING-VERSION 2
      ::= { bopSapOprEntry 16 }


   bopSapOprPortFilterAddress4 OBJECT-TYPE
      SYNTAX  INTEGER (0 .. 255)
      ACCESS  read-only
      STATUS  obsolete
      DESCRIPTION
"Indicates one of four 8-bit addresses used for address filtering (CX900 or
 USIO only).

Ranges of Values: 0 - 255

Default Value: 126"
--#ENDING-VERSION 2
      ::= { bopSapOprEntry 17 }



   bopSapOprPortCrc OBJECT-TYPE
      SYNTAX  INTEGER {
               crc-ccitt    (1),
               crc-32       (2)
               }
      ACCESS  read-only
      STATUS  obsolete
      DESCRIPTION
"Determines which algorithm is being used for error detection.

Options:
crc-ccitt (1): CRC-ITU-T (uses X15+X12+X5+1 polynomial; ITU-T formerly CCITT) 
(locked for ISDN 'D' channel; can be selected for ISDN 'B' channel)

crc-32 (2): CRC-32 (for Ethernet; uses X32+X26+X23+X22+X16+X12+X11+ 
X10+X8+X7+X5+X4+X2+X+1) (can be selected for ISDN 'B' channel)

Default Value: crc-ccitt (1)"
--#ENDING-VERSION 2
      ::= { bopSapOprEntry 18 }


   bopSapOprPortDtrUpTimer OBJECT-TYPE
      SYNTAX  INTEGER (0..65535)
      ACCESS  read-only
      STATUS  mandatory
      DESCRIPTION
"Determines the time in seconds that the port must wait for DCD (DTR if DCE)
 after raising DTR (DCD if DCE) to determine if a connection can be established.
 This object has no meaning in ISDN connections.

Note: This value will be set to zero if the port does not support an application 
requiring it.

Ranges of Values: 0 - 65535

Default Value: 0"
      ::= { bopSapOprEntry 19 }


   bopSapOprPortTxWindow OBJECT-TYPE
      SYNTAX  INTEGER (1..255)
      ACCESS  read-only
      STATUS  mandatory
      DESCRIPTION
"Determines the active maximum number of buffers that may be queued onto the
 port driver (for transmission) by the associated upper layer.
 For ISDN ports, the value is locked at 8.

Ranges of Values: 1 - 255

Default Value: 255"
   ::= { bopSapOprEntry 20 }

   bopSapOprPortDialMode OBJECT-TYPE
      SYNTAX  INTEGER {
               dial-in-only   (1),
               dial-out-only  (2),
               dial-in-out    (3)
               }
      ACCESS  read-only
      STATUS  mandatory
      DESCRIPTION
"Identifies the operation mode when setting up connections. 
Options:
n/a (0): object is not applicable
dial-in-only  (1): Only accept incoming calls; Will not make any outgoing calls.
dial-out-only (2): Only make outgoing calls; Will not accept any incoming calls.
dial-in-out    (3): Accept incoming calls and make outgoing calls. 

Ranges of Values: 1 - 3

Default Value: dial-in-out"
	--#STARTING-VERSION 2
      ::= { bopSapOprEntry 21 }

-- gap (6) for future parameters

   bopSapOprPortLoopback OBJECT-TYPE
      SYNTAX  INTEGER {
               none            (1),
               local           (2),
               remote          (3),
               both            (4),
               isdn-uif-uif    (5),
               isdn-uif-idl2   (6),
               isdn-uif-framer (7),
               isdn-uif-external       (8)
               }
      ACCESS  read-only
      STATUS  mandatory
      DESCRIPTION
"Determines whether loopback is enabled and what kind of loopback is enabled.

Options: 
n/a (0): object is not applicable

none  (1): Loopback is not enabled

local (2): Local loopback is enabled (return outbound frames to the originating
 software layer without transmitting them)

remote (3): Remote loopback is enabled (forces the port to return (transmit) 
 any frame received on the line, and causes the port to deny communication
 with any upper layer that requests service)

both (4):Both local and remote loopbacks are enabled.

isdn-uif-uif (5): ISDN loopback is enabled at U-Interface (forces TB1Q signal 
from a remote transmitter to pass through the entire U-Interface Transceiver 
before being transmitted back to the remote receiver)

isdn-uif-idl2 (6): ISDN loopback is enabled at IDL2 Controller (forces IDL2 
Controller to take in B and D channel data on IDL RX pin and send it out on 
IDL2 TX pin)

isdn-uif-framer (7): ISDN loopback is enabled at framer (forces B and D 
channel data in on D-in pin and M channel data in on SCP which together 
undergo framing and deframing functions before B and D channel data go out 
D-out pin and M channel data go out SCP)

isdn-uif-external (8): ISDN loopback is enabled at external line (forces TB1Q 
signal coming in on D-in pin to pass through Tx Driver pins, the U-Interface 
circuit and Rx input pins before recovering it and sending it out the D-out 
pin)

Default Value: none"
      ::= { bopSapOprEntry 28 }


   bopSapOprPortSignalSamplingPeriod OBJECT-TYPE
      SYNTAX  INTEGER (3..100)
      ACCESS  read-write
      STATUS  mandatory
      DESCRIPTION
"Determines the active sampling period in 100 millisecond increments that this
 port uses to sample signals. This object has no meaning in ISDN connections.

Ranges of Values: 3 - 100

Default Value: 5

Configuration Changed: operative "
      ::= { bopSapOprEntry 29 }


   bopSapOprPortDcdDtrSignalSamples OBJECT-TYPE
      SYNTAX  INTEGER (1..30)
      ACCESS  read-write
      STATUS  mandatory
      DESCRIPTION
"Determines the active number of sampling periods that this port must wait
 before validating DCD (DTR if DCE).
 This object has no meaning in ISDN connections.

Ranges of Values: 1 - 30

Default Value: 3

Configuration Changed: operative "
      ::= { bopSapOprEntry 30 }


   bopSapOprPortCtsRtsSignalSamples OBJECT-TYPE
      SYNTAX  INTEGER (1..30)
      ACCESS  read-write
      STATUS  mandatory
      DESCRIPTION
"Determines the active number of sampling periods that this port must wait
 before validating CTS (RTS if DCE).
 This object has no meaning in ISDN connections.

Ranges of Values: 1-30

Default Value: 3

Configuration Changed: operative "
      ::= { bopSapOprEntry 31 }


   bopSapOprPortDsrDrsSignalSamples OBJECT-TYPE
      SYNTAX  INTEGER (1..30)
      ACCESS  read-write
      STATUS  mandatory
      DESCRIPTION
"Determines the active number of sampling periods that this port must wait
 before validating DRS (DSR if DCE).
 This object has no meaning in ISDN connections.

Ranges of Values: 1 - 30

Default Value: 3

Configuration Changed: operative "
      ::= { bopSapOprEntry 32 }


   bopSapOprPortTmLlSignalSamples OBJECT-TYPE
      SYNTAX  INTEGER (1..30)
      ACCESS  read-write
      STATUS  mandatory
      DESCRIPTION
"Determines the active number of sampling periods that this port must wait
 before validating TM (LL if DCE).
 This object has no meaning in ISDN connections.

Ranges of Values: 1 - 30

Default Value: 3

Configuration Changed: operative "
      ::= { bopSapOprEntry 33 }


   bopSapOprPortRiRlSignalSamples OBJECT-TYPE
      SYNTAX  INTEGER (1..30)
      ACCESS  read-write
      STATUS  mandatory
      DESCRIPTION
"Determines the active number of sampling periods that this port must wait
 before validating RI (RL if DCE).
 This object has no meaning in ISDN connections.

Ranges of Values: 1 - 30

Default Value: 3

Configuration Changed: operative "
      ::= { bopSapOprEntry 34 }


   bopSapOprPortStatisticsTimer OBJECT-TYPE
      SYNTAX  INTEGER (0..60)
      ACCESS  read-only
      STATUS  mandatory
      DESCRIPTION
"Determines the active period in seconds during which BOP performance
 statistics are measured.

Note: If set to zero, BOP performance statistics are not measured.

Ranges of Values: 0 - 60

Default Value: 0"
      ::= { bopSapOprEntry 35 }


  
 bopSapOprPortCarrierAction OBJECT-TYPE
      SYNTAX  INTEGER {
               disabled      (1),
               enabled       (2)
               }
      ACCESS  read-write
      STATUS  mandatory
      DESCRIPTION
"Determines if the port driver informs it's associated upper layer that
service is no longer available following loss of DCD if DTE, or DTR if DCE.

Options:
disabled (1): do not notify upper layer (locked for ISDN and T1/E1 ports)

enabled (2): notify upper layer

Default Value: enabled (2)

Configuration Changed: operative "
      ::= { bopSapOprEntry 36 }


   bopSapOprPortDataGenerator OBJECT-TYPE
      SYNTAX  INTEGER {
               disabled        (1),
               enabled         (2),
               enabled-verify  (3),
               retrigger       (4)
               }
      ACCESS  read-write
      STATUS  mandatory
      DESCRIPTION
"Determines whether this port acts as an internal data generator.

Options:
n/a (0): object is not applicable

disabled (1): port is not a data generator

enabled (2): port is a data generator

enabled-verify (3): port is a data generator and its incoming data is 
verified

retrigger (4): port restarts data generation with defined parameters

Default Value: disabled (1)

Configuration Changed: operative "
      ::= { bopSapOprEntry 37 }


   bopSapOprPortGenFrames OBJECT-TYPE
      SYNTAX  INTEGER (0..255)
      ACCESS  read-write
      STATUS  mandatory
      DESCRIPTION
"Determines the number of frames that the port configured as an internal
 data generator will generate at one time.

Ranges of Values: 0 - 255

Note: When set to zero, frames are generated continuously

Default Value: 1

Configuration Changed: operative "
      ::= { bopSapOprEntry 38 }


   bopSapOprPortGenFrameSize OBJECT-TYPE
      SYNTAX  INTEGER (2..65535)
      ACCESS  read-write
      STATUS  mandatory
      DESCRIPTION
"Determines the size in bytes of internally generated frames (excluding flags).

Ranges of Values: 2 - 65,535

Note: When set to zero, frames are sized randomly.

Default Value: 128

Configuration Changed: operative "
      ::= { bopSapOprEntry 39 }


   bopSapOprPortGenFrameHeader OBJECT-TYPE
      SYNTAX  INTEGER (0..65535)
      ACCESS  read-write
      STATUS  mandatory
      DESCRIPTION
"Determines the 16-bit header that is inserted in the first two octets after
 the opening flag of each internally generated frame.

Ranges of Values: 0 - 65,535

Default Value: 1

Configuration Changed: operative "
      ::= { bopSapOprEntry 40 }


   bopSapOprPortThreshBandwidth OBJECT-TYPE
      SYNTAX  INTEGER (0..65535)
      ACCESS  read-only
      STATUS  mandatory
      DESCRIPTION
"Determines the value of the minimum critical bandwidth that determines
whether Frame Relay should filter certain restricted types of packets or not.

The value entered should be a multiple of 1 Kilo bits per second.

Ranges of Values: 0 - 65,535

Default Value: 0

Configuration Changed: operative "
      --#STARTING-VERSION 1
      ::= { bopSapOprEntry 41 }


   bopSapOprPortActualSpeed OBJECT-TYPE
      SYNTAX  INTEGER (375 .. 2048000)
      ACCESS  read-only
      STATUS  mandatory
      DESCRIPTION
"Determines the actual calculated output speed in bits per second of the port.

Ranges of Values: 375 - 2,048,000

Default Value: 0"
      --#STARTING-VERSION 1
      ::= { bopSapOprEntry 42 }


   bopSapOprCodeSet  OBJECT-TYPE
        SYNTAX  INTEGER { 
            ebcdic    (1),  
            ascii     (2) 
                }
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION
"Indicates the code used to communicate with the Bisync device. 
Options:
(0) N/A: object is not applicable
(1) EBCDIC code set
(2) ASCII code set
Default value: ebcdic"
--#STARTING-VERSION 2
      ::= { bopSapOprEntry 43 }


   bopSapOprParity OBJECT-TYPE
    SYNTAX  INTEGER {
        none    (1),  
        odd     (2),
        even    (3),
        space   (4),
        mark    (5)
        }
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
"Indicates the parity used to communicate with the Bisync device
    Options:
        (0) N/A:   Object is not applicable
        (1) none:  No parity
        (2) odd:   Odd parity
        (3) even:  Even parity
        (4) space: Low parity (always 0's)
        (5) mark:  High parity (always 1's)
        
Default value: none"
--#STARTING-VERSION 2
      ::= { bopSapOprEntry 44 }


