DOCS-BPI2-MIB DEFINITIONS ::= BEGIN

-- From file: "DOCS-BPI-PLUS-MIB.my"
-- Compile options "4 7 F H N W 03 06 0B 0G 0N 0T"

IMPORTS
    SnmpAdminString
            FROM SNMP-FRAMEWORK-MIB
    MacAddress, RowStatus, TruthValue, DateAndTime
            FROM SNMPv2-TC-v1
    ifIndex
            FROM IF-MIB
    docsIfMib
            FROM DOCS-IF-MIB
    InetAddressType, InetAddress
            FROM INET-ADDRESS-MIB
    Counter, IpAddress
            FROM RFC1155-SMI
    OBJECT-TYPE
            FROM RFC-1212;

docsBpi2MIB OBJECT IDENTIFIER ::= { docsIfMib 6 }
