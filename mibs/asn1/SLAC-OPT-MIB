	cdx6500slacsStationNum OBJECT-TYPE
	    SYNTAX INTEGER (1..250)
	    ACCESS read-only
	    STATUS mandatory
	    DESCRIPTION
		"Refers to the station number for this type."
	::= { cdx6500PCTSLACStationEntry 1 }                        

	cdx6500slacsStationType OBJECT-TYPE
	    SYNTAX INTEGER {
			token-ring    (0),
			frame-relay   (1),
			ethernet      (2),
			ethernet-2    (3),
			token-ring-2  (4)}
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
                "Refers to the station type."
        ::= { cdx6500PCTSLACStationEntry 2 }

        cdx6500slacsFRPortNum OBJECT-TYPE
            SYNTAX INTEGER (1..54)
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
                "Refers to the physical Frame Relay port number."
        ::= { cdx6500PCTSLACStationEntry 3 }
                        
        cdx6500slacsFRStationNum OBJECT-TYPE
            SYNTAX INTEGER (1..32)
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
                "Refers to the Frame Relay Station Number"
        ::= { cdx6500PCTSLACStationEntry 4 }
                        
        cdx6500slacsExternAddr OBJECT-TYPE
            SYNTAX DisplayString (SIZE(17))
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
                "The MAC address of the External LAN device associated with this station."
        ::= { cdx6500PCTSLACStationEntry 5 }
                        
	cdx6500slacsInternAddr OBJECT-TYPE
            SYNTAX DisplayString (SIZE(17))
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
                "The station address on the multidrop line."
        ::= { cdx6500PCTSLACStationEntry 6 }

        cdx6500slacs1490encap OBJECT-TYPE
            SYNTAX INTEGER {
                  	  llc       (1),
                          ban       (2),
			  na	    (100)}
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
                "Frame level window size must be set to the same value in
                 devices at each end of the link.
		 NA = Not applicable"
        ::= { cdx6500PCTSLACStationEntry 7 }
                        

	cdx6500slacsbnimacAddr OBJECT-TYPE
            SYNTAX DisplayString (SIZE(17))
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
                "The Destination MAC address inserted into the BAN 
		 encapsulated frame."
        ::= { cdx6500PCTSLACStationEntry 8 }

        cdx6500slacsbanmacAddr OBJECT-TYPE
            SYNTAX DisplayString (SIZE(17))
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
                "The source MAC address inserted into the BAN 
		 encapsulated frame."
        ::= { cdx6500PCTSLACStationEntry 9 }

        cdx6500slacsFrdlciRing OBJECT-TYPE
            SYNTAX DisplayString (SIZE(3))
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
                "The virtual ring number that is shared with the 
		 other PUs that are associated with the same DLCI.
		 If 0 then ring number will not be inserted into RIF field."
        ::= { cdx6500PCTSLACStationEntry 10 }
         
        cdx6500slacsInterSap OBJECT-TYPE
            SYNTAX DisplayString (SIZE(2))
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
                "The local SAP of this station."
        ::= { cdx6500PCTSLACStationEntry 11 }

        cdx6500slacsExternSap OBJECT-TYPE
            SYNTAX DisplayString (SIZE(2))
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
                "The SAP of the device associated with this station."
        ::= { cdx6500PCTSLACStationEntry 12 }

        cdx6500slacsExternPU OBJECT-TYPE
            SYNTAX INTEGER {type-2 (1),
			    type-21 (2),
			    type-4 (3)}	 
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
                "The PU type of the device associated with this station."
        ::= { cdx6500PCTSLACStationEntry 13 }

        cdx6500slacsLinkType OBJECT-TYPE
            SYNTAX INTEGER {slac (1),
			    len  (2),
			    na   (100)}

            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
                "The link type of this station."
        ::= { cdx6500PCTSLACStationEntry 14 }

        cdx6500slacsEthernetType OBJECT-TYPE
            SYNTAX INTEGER {ethernet (1),  
                            versionii (2),
		  	    na        (100)}
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
                "The type of ethernet frame (802.3 or Ethernet Version 2)
		 that is trasmitted for this station."
        ::= { cdx6500PCTSLACStationEntry 15 }

        cdx6500slacsInitiateTest OBJECT-TYPE
            SYNTAX INTEGER {	off (1),
			    	on  (2),
				wait (3),
				xid-wait (4),
				na       (100)}
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
                "Controls how TEST poll frames are locally initiated."
        ::= { cdx6500PCTSLACStationEntry 16 }

        cdx6500slacsRemoteCPName OBJECT-TYPE
            SYNTAX DisplayString (SIZE(8))
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
                "The control point name for the remote AS/400 that this station is
                 attaching to."
        ::= { cdx6500PCTSLACStationEntry 17 }

        cdx6500slacsLocalCPName OBJECT-TYPE
            SYNTAX DisplayString (SIZE(8))
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
