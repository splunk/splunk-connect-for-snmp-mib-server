        -- The SLAC Station Controls Table
        -- This table contains Control paramaters 
        -- for the SLAC Station table.
                        
        cdx6500ContSLACStationTable OBJECT-TYPE
            SYNTAX SEQUENCE OF Cdx6500ContSLACStationEntry
            ACCESS not-accessible
            STATUS mandatory
            DESCRIPTION
                "All of the control parameters relevant
                 to the SLAC Station table."
        ::= { cdx6500ContSLACStation 1 }
                        
        cdx6500ContSLACStationEntry OBJECT-TYPE
            SYNTAX Cdx6500ContSLACStationEntry
            ACCESS not-accessible
            STATUS mandatory
            DESCRIPTION
                "A row of SLAC Station Control parameters."
            INDEX { cdx6500slacsContStationType, cdx6500slacsContStationNum }
        ::= { cdx6500ContSLACStationTable 1 }
                        
        Cdx6500ContSLACStationEntry ::=
             SEQUENCE {
                 cdx6500slacsContStationType
                     INTEGER,
                 cdx6500slacsContStationNum
                     INTEGER,
                 cdx6500slacsContBootStation
                     INTEGER,
                 cdx6500slacsContDisableStation
                     INTEGER,
                 cdx6500slacsContEnableStation
                     INTEGER,
                 cdx6500slacsContResetStnStats
                     INTEGER
                 }
                        
        cdx6500slacsContStationType OBJECT-TYPE
            SYNTAX INTEGER {
                token-ring                    (0),
                frame-relay                   (1),
                ethernet                      (2),
                second-ethernet                  (3),
                second-token-ring                (4)}
            ACCESS not-accessible
            STATUS mandatory
            DESCRIPTION
                "Refers to the physical port number."
        ::= { cdx6500ContSLACStationEntry 1 }
                        
        cdx6500slacsContStationNum OBJECT-TYPE
            SYNTAX INTEGER (1..250)
            ACCESS not-accessible
            STATUS mandatory
            DESCRIPTION
                "Station reference number."
        ::= { cdx6500ContSLACStationEntry 2 }
                        
        cdx6500slacsContBootStation OBJECT-TYPE
            SYNTAX INTEGER {
                    boot   (1),
                    noBoot (2)
                    }
            ACCESS write-only
            STATUS mandatory
            DESCRIPTION
                "Boots the specified SLAC station.
                 boot   - Boot Station
                 noBoot - Do not boot Station"
        ::= { cdx6500ContSLACStationEntry 3 }
                        
        cdx6500slacsContDisableStation OBJECT-TYPE
            SYNTAX INTEGER {
                    disable   (1),
                    noDisable (2)
                    }
            ACCESS write-only
            STATUS mandatory
            DESCRIPTION
                "Disables the specified SLAC station.
                 disable   - Disable Station.
                 noDisable - Do not disable Station."
        ::= { cdx6500ContSLACStationEntry 4 }
                        
        cdx6500slacsContEnableStation OBJECT-TYPE
            SYNTAX INTEGER {
                    enable   (1),
                    noEnable (2)
                    }
            ACCESS write-only
            STATUS mandatory
            DESCRIPTION
                "Enables the specified SLAC station.
                 enable   - Enable Station.
                 noEnable - Not not enable Station."
        ::= { cdx6500ContSLACStationEntry 5 }
                        
        cdx6500slacsContResetStnStats OBJECT-TYPE
            SYNTAX INTEGER {
                    reset   (1),
                    noReset (2)
                    }
            ACCESS write-only
            STATUS mandatory
            DESCRIPTION
                "Resets statistics for the specified SLAC station
                 reset   - Reset Station Statistics
                 noReset - Do not reset Station Statistics"
        ::= { cdx6500ContSLACStationEntry 6 }

        END
