        -- Version 2.15.1.1, modified   2001-05-31 17:28:38-04

        SLAC-OPT-MIB DEFINITIONS ::= BEGIN

        IMPORTS
                mgmt, NetworkAddress, Counter, Gauge, enterprises
		    FROM RFC1155-SMI

                OBJECT-TYPE
                    FROM RFC-1212;

               -- Codex MIB Group definitions

                         codex OBJECT IDENTIFIER ::= { enterprises 449 }
            cdxProductSpecific OBJECT IDENTIFIER ::= { codex 2 }
                       cdx6500 OBJECT IDENTIFIER ::= { cdxProductSpecific 1 }

         cdx6500Configuration  OBJECT IDENTIFIER ::= { cdx6500 2 }
       cdx6500CfgProtocolGroup OBJECT IDENTIFIER ::= { cdx6500Configuration 1 }
        cdx6500CfgGeneralGroup OBJECT IDENTIFIER ::= { cdx6500Configuration 2 }
    cdx6500PCTSLACStationGroup OBJECT IDENTIFIER ::= { cdx6500CfgProtocolGroup 10 }

             cdx6500Statistics OBJECT IDENTIFIER ::= { cdx6500 3 }
      cdx6500StatProtocolGroup OBJECT IDENTIFIER ::= { cdx6500Statistics 1 }
    cdx6500PSTSLACStationGroup OBJECT IDENTIFIER ::= { cdx6500StatProtocolGroup 7 }

               cdx6500Controls OBJECT IDENTIFIER ::= { cdx6500 4 }
        cdx6500ContSLACStation OBJECT IDENTIFIER ::= { cdx6500Controls 20 }

        -- Textual conventions

        Counter16  ::= INTEGER (0..65535)
	
        DisplayString ::= OCTET STRING

	-- TEXTUAL-CONVENTION
	--  Status
	--    mandatory
	--  Descr
	--    Represents the LEN Type list.

        -- This data type is used to model textual information taken
        -- from the NVT ASCII character set. By convention, objects
        -- with this syntax are declared as having SIZE (0..255)

        -- The SLAC Station Configuration Table
        -- This table contains Configuration paramaters 
        -- for the SLAC Station table.
	                      
        cdx6500PCTSLACStationTable OBJECT-TYPE
            SYNTAX SEQUENCE OF Cdx6500PCTSLACStationEntry
            ACCESS not-accessible
            STATUS mandatory
            DESCRIPTION
                "All of the configuration parameters relevant
                 to the SLAC Station table."
        ::= { cdx6500PCTSLACStationGroup 1 }

    	cdx6500PCTSLACStationEntry OBJECT-TYPE
            SYNTAX Cdx6500PCTSLACStationEntry
            ACCESS not-accessible
            STATUS mandatory
            DESCRIPTION
                "A row of SLAC station configuration parameters."
            INDEX { cdx6500slacsStationType, cdx6500slacsStationNum }
        ::= { cdx6500PCTSLACStationTable 1 }

        Cdx6500PCTSLACStationEntry ::=
            SEQUENCE {
		cdx6500slacsStationNum
		    INTEGER,
	 	cdx6500slacsStationType
		    INTEGER,
                cdx6500slacsFRPortNum
                    INTEGER,
                cdx6500slacsFRStationNum
                    INTEGER,
                cdx6500slacsExternAddr
                    DisplayString,
                cdx6500slacsInternAddr
