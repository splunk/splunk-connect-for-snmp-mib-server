   SEQUENCE {
      sysPnacPaePortNumber
         InterfaceIndex,
      sysPnacPaePortStatus
         PaeControlledPortStatus,
      sysPnacPaeMultiAuthEnable
         INTEGER,
	  sysPnacPaeProtocolMode
         Integer32,
	  sysPnacPaePiggybackMode
         INTEGER,
	  sysPnacPaeVlanAssignment
         INTEGER,
	  sysPnacPaeSecureVlan
         INTEGER
		 
   }

