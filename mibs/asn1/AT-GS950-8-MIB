      dhcpSnoopBindindDBPortNumber
         Integer32,
      dhcpSnoopBindindDBType
         INTEGER,
      dhcpSnoopBindindDBLeaseTime
         Unsigned32,
      dhcpSnoopBindindDBRowStatus
         RowStatus
   }

