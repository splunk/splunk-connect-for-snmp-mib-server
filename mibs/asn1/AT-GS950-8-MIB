   SYNTAX        Integer32 (-1..255)
   MAX-ACCESS    read-write
   STATUS    current
   DESCRIPTION
      "Layer 4 protocol type.
        0-255: Layer 4 protocol number
		-1: Ignored"   
   ::=  { atiAclClassifierEntry 15 }  
   
