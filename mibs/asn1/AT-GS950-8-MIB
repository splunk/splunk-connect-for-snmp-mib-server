   ::= { sysSntpGeneralGroup 6 }   
   
-- SNTP Time Zone

sysSntpTzDSTStatus OBJECT-TYPE
   SYNTAX      INTEGER {
                enable(1),
                disable(2)
               }
   MAX-ACCESS  read-write 
   STATUS      current 
   DESCRIPTION 
      "This object is for enabling or disabling Time Zone Daylight Saving Time
       feature in the system."
   DEFVAL { enable } 
   ::= { sysSntpTzGroup 1 }

