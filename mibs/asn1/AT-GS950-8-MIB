               }
   MAX-ACCESS  read-only
   STATUS      current
   DESCRIPTION
      "Current Port Role of the port for this spanning
       tree instance."
   ::= { dot1sMstiPortEntry 15 }

dot1sMstiPortInfoSemState OBJECT-TYPE
   SYNTAX      INTEGER {
                disabled (0),
                aged (1),
                update (2),
                superiordesg (3),
                repeatdesg (4),
                inferiordesg (5),
                notdesg (6),
                current (7),
                receive (8),
                other (9)
               }
   MAX-ACCESS  read-only
   STATUS      current
