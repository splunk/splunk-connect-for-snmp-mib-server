-- ----------------------------------------------------------------------------

   l2tAuthStats OBJECT IDENTIFIER ::= { l2tAuthentication 1 }

   l2tAuthTunnelSuccesses OBJECT-TYPE
      SYNTAX Counter 
      ACCESS read-only      
      STATUS mandatory
      DESCRIPTION
      "The number of successful tunnel authentications performed."
      ::= { l2tAuthStats 1 }

   l2tAuthTunnelFailures OBJECT-TYPE
      SYNTAX Counter 
      ACCESS read-only      
      STATUS mandatory
      DESCRIPTION
      "The number of tunnel authentications which failed."
      ::= { l2tAuthStats 2 }

   l2tAuthUserSuccesses OBJECT-TYPE
      SYNTAX Counter 
      ACCESS read-only      
      STATUS mandatory
      DESCRIPTION
      "The number of successful user authentications performed."
      ::= { l2tAuthStats 3 }

   l2tAuthUserFailures OBJECT-TYPE
      SYNTAX Counter 
      ACCESS read-only      
      STATUS mandatory
      DESCRIPTION
      "The number of user authentications which failed."
           ::= { l2tAuthStats 4 }

-- ----------------------------------------------------------------------------
-- The L2T Tunnel Authentication Failure Table
--
-- This table contains objects that can be used to monitor 
-- tunnel authentication failures.                       
-- ----------------------------------------------------------------------------

   l2tAuthFailures OBJECT IDENTIFIER ::= { l2tAuthentication 2 }

   l2tAuthFailTunnelTable OBJECT-TYPE
      SYNTAX SEQUENCE OF L2tAuthFailTunnelEntry
      ACCESS not-accessible
      STATUS mandatory
      DESCRIPTION
         "The L2T tunnel authentication failure table."
      ::= { l2tAuthFailures 1 }

   l2tAuthFailTunnelEntry OBJECT-TYPE
      SYNTAX L2tAuthFailTunnelEntry
      ACCESS not-accessible
      STATUS mandatory
      DESCRIPTION
         "An L2T tunnel authentication failure entry. "
      INDEX { l2tAuthFailTunnelIndex }
      ::= { l2tAuthFailTunnelTable 1 }

   L2tAuthFailTunnelEntry ::= SEQUENCE {
      l2tAuthFailTunnelIndex     INTEGER,      
      l2tAuthFailTunnelHostName  DisplayString,
      l2tAuthFailTunnelIpAddress IpAddress,
      l2tAuthFailTunnelTime      TimeTicks     
   }

   l2tAuthFailTunnelIndex OBJECT-TYPE
      SYNTAX INTEGER(1..65535)
      ACCESS read-only      
      STATUS mandatory
      DESCRIPTION
         "The index of the tunnel authentication failure  
          table.  The value of the index is a number which
          begins at one and is incremented with each tunnel
          authentication failure.  It will wrap at 65535."
      ::= { l2tAuthFailTunnelEntry 1 }

   l2tAuthFailTunnelHostName OBJECT-TYPE
      SYNTAX DisplayString       
      ACCESS read-only      
      STATUS mandatory
      DESCRIPTION
         "The host name used on the failed tunnel 
          authentication attempt."
      ::= { l2tAuthFailTunnelEntry 2 }

   l2tAuthFailTunnelIpAddress OBJECT-TYPE
      SYNTAX IpAddress
      ACCESS read-only      
      STATUS mandatory
      DESCRIPTION
         "The IP address associated with the failed 
          tunnel authentication attempt."
      ::= { l2tAuthFailTunnelEntry 3 }

   l2tAuthFailTunnelTime      OBJECT-TYPE
      SYNTAX TimeTicks      
      ACCESS read-only
      STATUS mandatory
      DESCRIPTION
         "The time of the failure in hundredths of seconds."
      ::= { l2tAuthFailTunnelEntry 4 }


