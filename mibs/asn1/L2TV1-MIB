-- ----------------------------------------------------------------------------
-- The L2T Tunnel History Table
--
-- This table contains objects that can be used to monitor
-- previously active tunnels. 
-- ----------------------------------------------------------------------------
                
   l2tHistTunnelTable OBJECT-TYPE
      SYNTAX SEQUENCE OF L2tHistTunnelEntry
      ACCESS not-accessible
      STATUS mandatory
      DESCRIPTION
         "The L2T tunnel history table."
      ::= { l2tHistory 1 }       

   l2tHistTunnelEntry OBJECT-TYPE
      SYNTAX L2tHistTunnelEntry
      ACCESS not-accessible
      STATUS mandatory
      DESCRIPTION
         "An L2T tunnel history entry. "
      INDEX { l2tHistTunnelIndex }
      ::= { l2tHistTunnelTable 1 }

   L2tHistTunnelEntry ::= SEQUENCE {
      l2tHistTunnelIndex            INTEGER,
      l2tHistTunnelTunnelIndex      INTEGER,
      l2tHistTunnelRemoteHostName   DisplayString,
      l2tHistTunnelRemoteIpAddress  IpAddress, 
      l2tHistTunnelRemotePort       INTEGER,     
      l2tHistTunnelLocalIpAddress   IpAddress, 
      l2tHistTunnelLocalPort        INTEGER,     
      l2tHistTunnelUpTime           TimeTicks,
      l2tHistTunnelTotalSessions    Counter,
      l2tHistTunnelType             INTEGER,
      l2tHistTunnelInOctets         Counter,
      l2tHistTunnelInPkts           Counter,
      l2tHistTunnelInDiscards       Counter,
      l2tHistTunnelOutOctets        Counter,
      l2tHistTunnelOutPkts          Counter,
      l2tHistTunnelOutDiscards      Counter
  }

   l2tHistTunnelIndex OBJECT-TYPE
      SYNTAX INTEGER(1..65535)
      ACCESS read-only      
      STATUS mandatory
      DESCRIPTION
         "The index of the tunnel history table.
          The value of the index is a number which
          begins at one and is incremented with each tunnel
          that ends. The value of this object will wrap at 65535."
      ::= { l2tHistTunnelEntry 1 }

   l2tHistTunnelTunnelIndex OBJECT-TYPE
      SYNTAX INTEGER
      ACCESS read-only      
      STATUS mandatory
      DESCRIPTION
         "The value which was used as the tunnel stats table
          index."          
      ::= { l2tHistTunnelEntry 2 }

   l2tHistTunnelRemoteHostName OBJECT-TYPE
      SYNTAX DisplayString 
      ACCESS read-only      
      STATUS mandatory
      DESCRIPTION
         "The remote host name which was used for this 
          previous tunnel."
      ::= { l2tHistTunnelEntry 3 }

   l2tHistTunnelRemoteIpAddress OBJECT-TYPE
      SYNTAX IpAddress  
      ACCESS read-only      
      STATUS mandatory
      DESCRIPTION
         "The remote IP address which was used for this 
          previous tunnel."
      ::= { l2tHistTunnelEntry 4 }

   l2tHistTunnelRemotePort OBJECT-TYPE
      SYNTAX INTEGER
      ACCESS read-only
      STATUS mandatory
      DESCRIPTION
         "The remote UDP port which was used for this
          previous tunnel."
      ::= { l2tHistTunnelEntry 5 }

   l2tHistTunnelLocalIpAddress OBJECT-TYPE
      SYNTAX IpAddress  
      ACCESS read-only      
      STATUS mandatory
      DESCRIPTION
         "The local IP address which was used for this
          previous tunnel."
      ::= { l2tHistTunnelEntry 6 }

   l2tHistTunnelLocalPort OBJECT-TYPE
      SYNTAX INTEGER
      ACCESS read-only
      STATUS mandatory
      DESCRIPTION
         "The local UDP port which was used for this
          previous tunnel."
      ::= { l2tHistTunnelEntry 7 }

   l2tHistTunnelUpTime OBJECT-TYPE
      SYNTAX TimeTicks
      ACCESS read-only      
      STATUS mandatory
      DESCRIPTION
         "The length of time the tunnel was active in
          hundredths of seconds. "
      ::= { l2tHistTunnelEntry 8 }

   l2tHistTunnelTotalSessions  OBJECT-TYPE
      SYNTAX Counter
      ACCESS read-only      
      STATUS mandatory
      DESCRIPTION
         "The total number of sessions which used this tunnel."
      ::= { l2tHistTunnelEntry 9 }

   l2tHistTunnelType OBJECT-TYPE
      SYNTAX INTEGER {
                l2tpTunnel(1),
                l2fTunnel(2),
                pptpTunnel(3)
             }
      ACCESS read-only      
      STATUS mandatory
      DESCRIPTION
         "The type of Layer-2 Tunnel.  Possible values are 
            1 = Layer-2 Tunnel Protocol Tunnel,
            2 = Layer-2 Forwarding Tunnel,
            3 = Point-to-Point Tunnel Protocol Tunnel."
      ::= { l2tHistTunnelEntry 10 }

   l2tHistTunnelInOctets OBJECT-TYPE
      SYNTAX Counter
      ACCESS read-only      
      STATUS mandatory
      DESCRIPTION
         "The number of octets received by this tunnel."
      ::= { l2tHistTunnelEntry 11 }

   l2tHistTunnelInPkts OBJECT-TYPE
      SYNTAX Counter
      ACCESS read-only      
      STATUS mandatory
      DESCRIPTION
         "The number of packets received by this tunnel."
      ::= { l2tHistTunnelEntry 12 }

   l2tHistTunnelInDiscards OBJECT-TYPE
      SYNTAX Counter
      ACCESS read-only      
      STATUS mandatory
      DESCRIPTION
         "The number of packets which were discarded during
          receive processing."
      ::= { l2tHistTunnelEntry 13 }

   l2tHistTunnelOutOctets OBJECT-TYPE
      SYNTAX Counter
      ACCESS read-only      
      STATUS mandatory
      DESCRIPTION
         "The number of octets sent by this tunnel."
      ::= { l2tHistTunnelEntry 14 }

   l2tHistTunnelOutPkts OBJECT-TYPE
      SYNTAX Counter
      ACCESS read-only      
      STATUS mandatory
      DESCRIPTION
         "The number of packets sent by this tunnel."
      ::= { l2tHistTunnelEntry 15 }

   l2tHistTunnelOutDiscards OBJECT-TYPE
      SYNTAX Counter
      ACCESS read-only      
      STATUS mandatory
      DESCRIPTION
         "The number of packets which were discarded during
          send processing."
      ::= { l2tHistTunnelEntry 16 }

-- ----------------------------------------------------------------------------
-- The L2T Session History Table
--
-- This table contains objects that can be used to monitor
-- previously active sessions.
-- ----------------------------------------------------------------------------

   l2tHistSessTable OBJECT-TYPE
      SYNTAX SEQUENCE OF L2tHistSessEntry
      ACCESS not-accessible
      STATUS mandatory
      DESCRIPTION
         "The L2T session history table."
      ::= { l2tHistory 2 }

   l2tHistSessEntry OBJECT-TYPE
      SYNTAX L2tHistSessEntry
      ACCESS not-accessible
      STATUS mandatory
      DESCRIPTION
         "An L2T session history entry. "
      INDEX { l2tHistSessIndex }
      ::= { l2tHistSessTable 1 }

   L2tHistSessEntry ::= SEQUENCE {
      l2tHistSessIndex            INTEGER,
      l2tHistSessTunnelIndex      INTEGER,
      l2tHistSessLocalCallId      INTEGER,
      l2tHistSessRemoteIpAddress  IpAddress, 
      l2tHistSessLocalIpAddress   IpAddress, 
      l2tHistSessUpTime           TimeTicks,
      l2tHistSessDataRecvOctets   Counter,
      l2tHistSessDataRecvPackets  Counter,
      l2tHistSessDataRecvDiscards Counter,
      l2tHistSessDataSendOctets   Counter,
      l2tHistSessDataSendPackets  Counter,
      l2tHistSessDataSendDiscards Counter,
      l2tHistSessAuthMethod       INTEGER,
      l2tHistSessEncryptDecrypt   INTEGER
  }

   l2tHistSessIndex OBJECT-TYPE
      SYNTAX INTEGER(1..65535)
      ACCESS read-only      
      STATUS mandatory
      DESCRIPTION
         "The index of the session history table.
          The value of the index is a number which
          begins at one and is incremented with each tunnel
          that ends. The value of this object will wrap at 65535."
      ::= { l2tHistSessEntry 1 }

   l2tHistSessTunnelIndex OBJECT-TYPE
      SYNTAX INTEGER
      ACCESS read-only      
      STATUS mandatory
      DESCRIPTION
         "The value which was used as the tunnel stats table
          index."
      ::= { l2tHistSessEntry 2 }

   l2tHistSessLocalCallId OBJECT-TYPE
      SYNTAX INTEGER
      ACCESS read-only      
      STATUS mandatory
      DESCRIPTION
         "The value which was used as the session stats table
          index."
      ::= { l2tHistSessEntry 3 }

   l2tHistSessRemoteIpAddress OBJECT-TYPE
      SYNTAX IpAddress
      ACCESS read-only      
      STATUS mandatory
      DESCRIPTION
         "The remote IP address which was used for this
          session."
      ::= { l2tHistSessEntry 4 }

   l2tHistSessLocalIpAddress OBJECT-TYPE
      SYNTAX IpAddress
      ACCESS read-only      
      STATUS mandatory
      DESCRIPTION
         "The local IP address which was used for this
          session."
      ::= { l2tHistSessEntry 5 }

   l2tHistSessUpTime OBJECT-TYPE
      SYNTAX TimeTicks
      ACCESS read-only      
      STATUS mandatory
      DESCRIPTION
         "The length of time the session was been active in
          hundredths of seconds. "
      ::= { l2tHistSessEntry 6 }

   l2tHistSessDataRecvOctets OBJECT-TYPE
      SYNTAX Counter
      ACCESS read-only      
      STATUS mandatory
      DESCRIPTION
         "The number of octets received on the data tunnel for
          this session.  This value was accumulated BEFORE
          determining whether or not the packet should be
          uncompressed."
      ::= { l2tHistSessEntry 7 }

   l2tHistSessDataRecvPackets OBJECT-TYPE
      SYNTAX Counter
      ACCESS read-only      
      STATUS mandatory
      DESCRIPTION
         "The number packets received on the data tunnel for
          this session."
      ::= { l2tHistSessEntry 8 }

   l2tHistSessDataRecvDiscards OBJECT-TYPE
      SYNTAX Counter
      ACCESS read-only      
      STATUS mandatory
      DESCRIPTION
         "The number packets which were discarded during
          the data tunnels receive processing for this session."
      ::= { l2tHistSessEntry 9 }

   l2tHistSessDataSendOctets OBJECT-TYPE
      SYNTAX Counter
      ACCESS read-only      
      STATUS mandatory
      DESCRIPTION
         "The number of octets sent on the data tunnel for
          this session.  This value is accumulated AFTER
          the packet was compressed."
      ::= { l2tHistSessEntry 10 }

   l2tHistSessDataSendPackets OBJECT-TYPE
      SYNTAX Counter
      ACCESS read-only      
      STATUS mandatory
      DESCRIPTION
         "The number packets sent on the data tunnel for
          this session."
      ::= { l2tHistSessEntry 11 }

   l2tHistSessDataSendDiscards OBJECT-TYPE
      SYNTAX Counter
      ACCESS read-only      
      STATUS mandatory
      DESCRIPTION
         "The number packets which were discarded during
          the data tunnels send processing for this session."
      ::= { l2tHistSessEntry 12 }

   l2tHistSessAuthMethod OBJECT-TYPE
      SYNTAX INTEGER {
                 text(1),
                 pppchap(2),
                 ppppap(3),
                 none(4)
             }
      ACCESS read-only      
      STATUS mandatory
      DESCRIPTION
         "This object contains the proxy authentication method
          which was employed by the LAC for the session."
      ::= { l2tHistSessEntry 13 }

   l2tHistSessEncryptDecrypt  OBJECT-TYPE
      SYNTAX INTEGER {
                 yes(1),
                 no(2)
             }
      ACCESS read-only      
      STATUS mandatory
      DESCRIPTION
         "This object reflects whether or not the encryption and
          and decryption is being performed for the session."
      ::= { l2tHistSessEntry 14 }


-- ----------------------------------------------------------------------------
-- The L2T Host Table                       
--
-- This table permits the monitoring of potential L2T hosts.      
-- ----------------------------------------------------------------------------

   l2tHostTable OBJECT-TYPE
      SYNTAX SEQUENCE OF L2tHostEntry
      ACCESS not-accessible
      STATUS mandatory
      DESCRIPTION
         "The L2T host table."
      ::= { l2tHosts 1 }

   l2tHostEntry OBJECT-TYPE
      SYNTAX L2tHostEntry
      ACCESS not-accessible
      STATUS mandatory
      DESCRIPTION
         "An L2T host entry." 
      INDEX { l2tHostIndex }
      ::= { l2tHostTable 1 }

   L2tHostEntry ::= SEQUENCE {
      l2tHostIndex             INTEGER,
      l2tHostName              DisplayString,
      l2tHostIpAddress         IpAddress,
      l2tHostTunnelStatus      INTEGER
   }

   l2tHostIndex OBJECT-TYPE
      SYNTAX INTEGER(1..65535)
      ACCESS read-only
      STATUS mandatory
      DESCRIPTION
         "The index of the host table." 
      ::= { l2tHostEntry 1 }

   l2tHostName OBJECT-TYPE
      SYNTAX DisplayString
      ACCESS read-only 
      STATUS mandatory
      DESCRIPTION
         "The host name which may used for a Layer-2 tunnel."
      ::= { l2tHostEntry 2 }

   l2tHostIpAddress OBJECT-TYPE
      SYNTAX IpAddress 
      ACCESS read-only 
      STATUS mandatory
      DESCRIPTION
         "The IP Address associated with the host name."
      ::= { l2tHostEntry 3 }

   l2tHostTunnelStatus OBJECT-TYPE
      SYNTAX INTEGER {
                 active(1),
                 inactive(2)
             }
      ACCESS read-only
      STATUS mandatory
      DESCRIPTION
         "The current tunnel status of the host."
      ::= { l2tHostEntry 4 }


-- ----------------------------------------------------------------------------
-- The L2T Tunnel Connectivity Test Table
--
-- This table permits the testing of connectively for protential
-- L2T Tunnels.
-- ----------------------------------------------------------------------------

   l2tTunnelConnectTestTable OBJECT-TYPE
      SYNTAX SEQUENCE OF L2tTunnelConnectTestEntry
      ACCESS not-accessible
      STATUS mandatory
      DESCRIPTION
         "The L2T tunnel connectivity test table."
      ::= { l2tTests 1 }

   l2tTunnelConnectTestEntry OBJECT-TYPE
      SYNTAX L2tTunnelConnectTestEntry
      ACCESS not-accessible
      STATUS mandatory
      DESCRIPTION
         "An L2T tunnel connectivity test entry."
      INDEX { l2tTunnelConnectTestIndex }
      ::= { l2tTunnelConnectTestTable 1 }

   L2tTunnelConnectTestEntry ::= SEQUENCE {
      l2tTunnelConnectTestIndex       INTEGER,
      l2tTunnelConnectTestHostName    DisplayString,
      l2tTunnelConnectTestResult      INTEGER,
      l2tTunnelConnectTestType        INTEGER,
      l2tTunnelConnectTestStatus      INTEGER
   }

   l2tTunnelConnectTestIndex OBJECT-TYPE
      SYNTAX INTEGER(1..65535)
      ACCESS read-only
      STATUS mandatory
      DESCRIPTION
         "The index of the tunnel connectivity test table.
          The value of the index is a number which was used
          during row creation."
      ::= { l2tTunnelConnectTestEntry 1 }

   l2tTunnelConnectTestHostName OBJECT-TYPE
      SYNTAX DisplayString
      ACCESS read-write
      STATUS mandatory
      DESCRIPTION
         "The host name used to establish a Layer-2 tunnel.
      
          This object is used to trigger a connectivity test
          to a specific host.  A new test is triggered by 
          SETTING this object using a non-existent index    
          and SETTING l2tTunnelConnectTestStatus to createAndGo.
          For example to trigger a new connectivity test 
          when the table currently contains row 1 through 4,
          one would send a SET as follows:                    
           SNMP SET l2tTunnelConnectHostName.5   = Host-To-Test
                    l2tTunnelConnectTestStatus.5 = createAndGo
          See l2tHostName for the possible host names which
          may be specified.
          
          Existing entries may NOT be modified."             
      ::= { l2tTunnelConnectTestEntry 2 }

   l2tTunnelConnectTestResult OBJECT-TYPE
      SYNTAX INTEGER {
                 inProgress(1),
                 successful(2),
                 tunnelAlreadyUp(3),
                 hostNotResponding(4),
                 localAuthFailure(5),
                 remoteAuthFailure(6)
             }
      ACCESS read-only
      STATUS mandatory
      DESCRIPTION
         "The result of the tunnel connectivity test.           
          1 = test is in progress
          2 = test ended successfully
          3 = tunnel is already active
          4 = host did not respond
          5 = local authentication failure
          6 = remote authentication failure."
      ::= { l2tTunnelConnectTestEntry 3 }

   l2tTunnelConnectTestType OBJECT-TYPE
      SYNTAX INTEGER {
                l2tpTunnel(1),
                l2fTunnel(2),
                pptpTunnel(3),
                none(99)
             }
      ACCESS read-only
      STATUS mandatory
      DESCRIPTION
         "The type of Layer-2 Tunnel.  Possible values are:
          1  = Layer-2 Tunnel Protocol Tunnel
          2  = Layer-2 Forwarding Tunnel
          3  = Point-to-Point Tunnel Protocol Tunnel
          99 = no tunnel created."                    
      ::= { l2tTunnelConnectTestEntry 4 }

   l2tTunnelConnectTestStatus OBJECT-TYPE
      SYNTAX INTEGER {
                active(1),     
                createAndGo(4) 
             }
      ACCESS read-write
      STATUS mandatory
      DESCRIPTION
         "The status of the test entry. Possible values are:
          1  = active entry                    
          4  = createAndGo              
         
          This object is used to trigger a connectivity test
          to a specific host.  A new test is triggered by
          SETTING this object to createAndGo using a non-existent
          index and SETTING l2tTunnelConnectTestHostName to a 
          specific host name.  For example to trigger a new 
          connectivity test when the table currently contains 
          row 1 through 4, one would send a SET as follows:
           SNMP SET l2tTunnelConnectHostName.5   = Host-To-Test
                    l2tTunnelConnectTestStatus.5 = createAndGo
          See l2tHostName for the possible host names which
          may be specified.

          Existing entries may NOT be modified."
      ::= { l2tTunnelConnectTestEntry 5 }

-- ----------------------------------------------------------------------------
-- The L2T Tunnel Response Time Test Table
--
-- This table permits the testing the response time of an existing
-- L2T Tunnel.
-- ----------------------------------------------------------------------------

   l2tTunnelRspTimeTestTable OBJECT-TYPE
      SYNTAX SEQUENCE OF L2tTunnelRspTimeTestEntry
      ACCESS not-accessible
      STATUS mandatory
      DESCRIPTION
         "The L2T tunnel response time test table."
      ::= { l2tTests 2 }

   l2tTunnelRspTimeTestEntry OBJECT-TYPE
      SYNTAX L2tTunnelRspTimeTestEntry
      ACCESS not-accessible
      STATUS mandatory
      DESCRIPTION
         "An L2T tunnel response time test entry."
      INDEX { l2tTunnelRspTimeTestIndex }
      ::= { l2tTunnelRspTimeTestTable 1 }

   L2tTunnelRspTimeTestEntry ::= SEQUENCE {
      l2tTunnelRspTimeTestIndex       INTEGER,
      l2tTunnelRspTimeTestHostName    DisplayString,
      l2tTunnelRspTimeTestResult      INTEGER,
      l2tTunnelRspTimeTestRtripTime   INTEGER,
      l2tTunnelRspTimeTestStatus      INTEGER
   }

   l2tTunnelRspTimeTestIndex OBJECT-TYPE
      SYNTAX INTEGER(1..65535)
      ACCESS read-only
      STATUS mandatory
      DESCRIPTION
         "The index of the tunnel connectivity test table.
          The value of the index is a number which was used
          during row creation."
      ::= { l2tTunnelRspTimeTestEntry 1 }

   l2tTunnelRspTimeTestHostName OBJECT-TYPE
      SYNTAX DisplayString
      ACCESS read-write
      STATUS mandatory
      DESCRIPTION
         "The host name used to test the round-trip response
          time of an existing Layer-2 Tunnel.

          This object is used to trigger a response time test
          of an existing Layer-2 to a specific host.  A new test
          is triggered by SETTING this object using a non-existent
          index and SETTING l2tTunnelRspTimeTestStatus to createAndGo.
          For example to trigger a new connectivity test
          when the table currently contains row 1 through 4,
          one would send a SET as follows:
           SNMP SET l2tTunnelRspTimeHostName.5   = Host-To-Test
                    l2tTunnelRspTimeTestStatus.5 = createAndGo
          See l2tHistTunnelRemoteHostName for the possible host
          names which may be specified.

          Existing entries may NOT be modified."
      ::= { l2tTunnelRspTimeTestEntry 2 }

   l2tTunnelRspTimeTestResult OBJECT-TYPE
      SYNTAX INTEGER {
                 inProgress(1),
                 successful(2),
                 hostNotResponding(3),
                 tunnelDown(4)
             }
      ACCESS read-only
      STATUS mandatory
      DESCRIPTION
         "The result of the tunnel connectivity test.
          1 = test is in progress
          2 = test ended successfully
          3 = host did not respond
          4 = a tunnel to this host is NOT available."
      ::= { l2tTunnelRspTimeTestEntry 3 }

   l2tTunnelRspTimeTestRtripTime OBJECT-TYPE
      SYNTAX INTEGER
      ACCESS read-only
      STATUS mandatory
      DESCRIPTION
         "The round time in milliseconds of response time test."
      ::= { l2tTunnelRspTimeTestEntry 4 }

   l2tTunnelRspTimeTestStatus OBJECT-TYPE
      SYNTAX INTEGER {
                active(1),
                createAndGo(4)
             }
      ACCESS read-write
      STATUS mandatory
      DESCRIPTION
         "The status of the test entry. Possible values are:
          1  = active entry
          4  = createAndGo

          This object is used to trigger a response time test
          of an existing Layer-2 to a specific host.  A new test
          is triggered by SETTING this object using a non-existent
          index to createAndGo and SETTING l2tTunnelRspTimeHostName
          to a specific host.  For example to trigger a new response
          time test when the table currently contains row 1 through 4,
          one would send a SET as follows:
           SNMP SET l2tTunnelRspTimeHostName.5   = Host-To-Test
                    l2tTunnelRspTimeTestStatus.5 = createAndGo
          See l2tHistTunnelRemoteHostName for the possible host
          names which may be specified.

          Existing entries may NOT be modified."
      ::= { l2tTunnelRspTimeTestEntry 5 }

 
-- ----------------------------------------------------------------------------
-- The L2T TRAP Control Group         
--
-- This group of objects controls the sending L2T TRAPs.   
-- ----------------------------------------------------------------------------

   l2tTrapCntlTunnelStart OBJECT-TYPE
      SYNTAX INTEGER {
                enabled(1),
                disabled(2)
             }
      ACCESS read-write
      STATUS mandatory
      DESCRIPTION
         "This object defines the administrative state of sending the L2T
          Tunnel Start TRAP "
      DEFVAL { enabled }
      ::= { l2tTrapCntl 1 }      

   l2tTrapCntlTunnelStop OBJECT-TYPE
      SYNTAX INTEGER {
                enabled(1),
                disabled(2)
             }
      ACCESS read-write
      STATUS mandatory
      DESCRIPTION
         "This object defines the administrative state of sending the L2T
          Tunnel Stop TRAP "
      DEFVAL { enabled }
      ::= { l2tTrapCntl 2 }      

   l2tTrapCntlAuthFailTunnel OBJECT-TYPE
      SYNTAX INTEGER {
                enabled(1),
                disabled(2)
             }
      ACCESS read-write
      STATUS mandatory
      DESCRIPTION
         "This object defines the administrative state of sending the L2T
          Tunnel Authentication Failure TRAP "
      DEFVAL { enabled }
      ::= { l2tTrapCntl 3 }      

   l2tTrapCntlAuthFailUser OBJECT-TYPE
      SYNTAX INTEGER {
                enabled(1),
                disabled(2)
             }
      ACCESS read-write
      STATUS mandatory
      DESCRIPTION
         "This object defines the administrative state of sending the L2T
          User Authentication Failure TRAP "
      DEFVAL { enabled }
      ::= { l2tTrapCntl 4 }      

-- ----------------------------------------------------------------------------
-- The L2T TRAP Group
-- Describes TRAPs which can be generated by the Layer Two Transport Protocol
-- ----------------------------------------------------------------------------

l2tTunnelStart TRAP-TYPE
    ENTERPRISE ibmIROCroutingl2t
    VARIABLES  { l2tTunnelLocalTunnelControlId,
                 l2tTunnelType }
    DESCRIPTION
       "This TRAP is generated when a Layer-2 Tunnel 
        is created."
    ::= 1

l2tTunnelStop TRAP-TYPE
    ENTERPRISE ibmIROCroutingl2t
    VARIABLES  { l2tTunnelLocalTunnelControlId,
                 l2tTunnelType,
                 l2tTunnelUpTime }
    DESCRIPTION
       "This TRAP is generated when a Layer-2 Tunnel 
        ends."       
    ::= 2

l2tAuthFailTunnel TRAP-TYPE
    ENTERPRISE ibmIROCroutingl2t
    VARIABLES  { l2tAuthFailTunnelHostName,
                 l2tAuthFailTunnelIpAddress }
    DESCRIPTION
       "This TRAP is generated when a Layer-2 Tunnel 
        is NOT created due to an authentication failure."
    ::= 3

l2tAuthFailUser TRAP-TYPE
    ENTERPRISE ibmIROCroutingl2t
    VARIABLES  { l2tAuthFailUserId }
    DESCRIPTION
       "This TRAP is generated when a Session for a   
        Layer-2 Tunnel is NOT created due to an 
        authentication failure."
    ::= 4

END
