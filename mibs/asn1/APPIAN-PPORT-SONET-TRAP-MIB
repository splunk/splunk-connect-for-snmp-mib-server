-- acSonetLineProtectionTraps  OBJECT IDENTIFIER ::= { acSonetLineProtection 0 }

acSonetLineProtectionSwitchAlarmTrap  NOTIFICATION-TYPE
    OBJECTS { 
        acChassisCurrentTime, acChassisRingId, 
        acSonetLineProtectionNodeId,
        acSonetLineProtectionSlot,
        acSonetLineProtectionPort
        }
    STATUS current
    DESCRIPTION
    "Protection switch is currently active on this SONET interface."
    
    ::= { acSonetLineTraps 21 }
    
acSonetLineProtectionSwitchClearTrap  NOTIFICATION-TYPE
    OBJECTS { 
        acChassisCurrentTime, acChassisRingId, 
        acSonetLineProtectionNodeId,
        acSonetLineProtectionSlot,
        acSonetLineProtectionPort
        }
    STATUS current
    DESCRIPTION
    "Protection switch has been released on this SONET interface."
    
    ::= { acSonetLineTraps 22 }
    
