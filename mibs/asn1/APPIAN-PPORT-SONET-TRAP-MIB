--
--  Title:  Appian SONET Traps MIB
--
--  Dated:  03/04/2k
--  Author: D. Theriault
--
--  Description:
--
--  This MIB file contains the SONET trap definitions for the Appian
--  Optical Services Access Platform(s).
--
--
APPIAN-PPORT-SONET-TRAP-MIB DEFINITIONS ::= BEGIN

IMPORTS

    acChassisCurrentTime, acChassisRingId           FROM APPIAN-CHASSIS-MIB

    acSonet, 
    acSonetPort, 
    acSonetSection, 
    acSonetLine, 
    acSonetFarEndLine, 
    acSonetPath, 
    acSonetFarEndPath, 
    acSonetVT, 
    acSonetFarEndVT,
    
    acSonetPortNodeId, 
    acSonetPortSlot, 
    acSonetPortPort, 
    
    acSonetSectionTIMNodeId, 
    acSonetSectionTIMSlot, 
    acSonetSectionTIMPort,
    
    acSonetSection15MinuteIntervalNodeId,
    acSonetSection15MinuteIntervalSlot, 
    acSonetSection15MinuteIntervalPort, 
    acSonetSection15MinuteIntervalNumber, 
    
    acSonetSectionDayIntervalNodeId,
    acSonetSectionDayIntervalSlot, 
    acSonetSectionDayIntervalPort, 
    acSonetSectionDayIntervalNumber, 
    
    acSonetLine15MinuteIntervalNodeId,
    acSonetLine15MinuteIntervalSlot,
    acSonetLine15MinuteIntervalPort,    
    acSonetLine15MinuteIntervalNumber,    
    
    acSonetLineDayIntervalNodeId,
    acSonetLineDayIntervalSlot,
    acSonetLineDayIntervalPort,    
    acSonetLineDayIntervalNumber,    
    
    acSonetFarEndLine15MinuteIntervalNodeId,
    acSonetFarEndLine15MinuteIntervalSlot,
    acSonetFarEndLine15MinuteIntervalPort,
    acSonetFarEndLine15MinuteIntervalNumber,
        
    acSonetFarEndLineDayIntervalNodeId,
    acSonetFarEndLineDayIntervalSlot,
    acSonetFarEndLineDayIntervalPort,
    acSonetFarEndLineDayIntervalNumber,
        
    acSonetLineProtectionNodeId, 
    acSonetLineProtectionSlot,
    acSonetLineProtectionPort,
    
    acSonetPath15MinuteIntervalNodeId,
    acSonetPath15MinuteIntervalSlot,
    acSonetPath15MinuteIntervalPort,
    acSonetPath15MinuteIntervalPath,
    acSonetPath15MinuteIntervalNumber,
    
    acSonetPathDayIntervalNodeId,
    acSonetPathDayIntervalSlot,
    acSonetPathDayIntervalPort,
    acSonetPathDayIntervalPath,
    acSonetPathDayIntervalNumber,

    acSonetPathTIMNodeId,
    acSonetPathTIMSlot,
    acSonetPathTIMPort,
    acSonetPathTIMPath,

    acSonetPathPLMNodeId,
    acSonetPathPLMSlot,
    acSonetPathPLMPort,
    acSonetPathPLMPath,

    acSonetFarEndPath15MinuteIntervalNodeId,
    acSonetFarEndPath15MinuteIntervalSlot,
    acSonetFarEndPath15MinuteIntervalPort,
    acSonetFarEndPath15MinuteIntervalPath,
    acSonetFarEndPath15MinuteIntervalNumber,

    acSonetFarEndPathDayIntervalNodeId,
    acSonetFarEndPathDayIntervalSlot,
    acSonetFarEndPathDayIntervalPort,
    acSonetFarEndPathDayIntervalPath,
    acSonetFarEndPathDayIntervalNumber,

    acSonetVT15MinuteIntervalNodeId,
    acSonetVT15MinuteIntervalSlot,
    acSonetVT15MinuteIntervalPort,        
    acSonetVT15MinuteIntervalVT,
    acSonetVT15MinuteIntervalNumber,
    
    acSonetVTDayIntervalNodeId,
    acSonetVTDayIntervalSlot,
    acSonetVTDayIntervalPort,        
    acSonetVTDayIntervalVT,
    acSonetVTDayIntervalNumber,
    
    acSonetFarEndVT15MinuteIntervalNodeId,
    acSonetFarEndVT15MinuteIntervalSlot,
    acSonetFarEndVT15MinuteIntervalPort,        
    acSonetFarEndVT15MinuteIntervalVT,        
    acSonetFarEndVT15MinuteIntervalNumber,  

    acSonetPathProtectionNodeId,
    acSonetPathProtectionSlot, 
    acSonetPathProtectionPort,
    acSonetPathProtectionPath,

    acSonetVTProtectionNodeId,
    acSonetVTProtectionSlot,  
    acSonetVTProtectionPort,
    acSonetVTProtectionVT,

    acSonetFarEndVTDayIntervalNodeId,
    acSonetFarEndVTDayIntervalSlot,
    acSonetFarEndVTDayIntervalPort,        
    acSonetFarEndVTDayIntervalVT,
    acSonetFarEndVTDayIntervalNumber                    FROM APPIAN-PPORT-SONET-MIB
    MODULE-IDENTITY, NOTIFICATION-TYPE                  FROM SNMPv2-SMI;
    

acSonetTraps MODULE-IDENTITY

    LAST-UPDATED                "0003041600Z"
    ORGANIZATION                "Appian Communications, Inc."
    CONTACT-INFO                "Douglas Stahl"
    DESCRIPTION                 "The MIB module to describe SONET/SDH Traps."

    REVISION                    "0003041600Z"
    DESCRIPTION                 "Engineering draft, not for release."

    ::= { acSonet 4 }

-- =============================================================================    
--
-- SONET Port Traps
--
-- =============================================================================
    
acSonetPortTraps  OBJECT IDENTIFIER ::= { acSonetPort 0 }

acSonetPortFacilityLoopbackActiveTrap  NOTIFICATION-TYPE
    OBJECTS { 
        acChassisCurrentTime, acChassisRingId, 
        acSonetPortNodeId, 
        acSonetPortSlot, 
        acSonetPortPort
        }
    STATUS  current
    DESCRIPTION
    "The SONET physical interface has entered a loopback mode where the 
     received optical data is sent out the optical transmitter."
    
    ::= { acSonetPortTraps 1 }
acSonetPortFacilityLoopbackClearTrap  NOTIFICATION-TYPE
    OBJECTS { 
        acChassisCurrentTime, acChassisRingId, 
        acSonetPortNodeId, 
        acSonetPortSlot, 
        acSonetPortPort
        }
    STATUS  current
    DESCRIPTION
    "The SONET physical interface has disabled a loopback mode and is 
     now transmitting and receiving optical data normally."
    
    ::= { acSonetPortTraps 2 }

acSonetPortSectionDCCFailureAlarmTrap NOTIFICATION-TYPE
    OBJECTS { 
        acChassisCurrentTime, acChassisRingId, 
        acSonetPortNodeId, 
        acSonetPortSlot, 
        acSonetPortPort
        }
    STATUS  current
    DESCRIPTION
    "Section DCC for this SONET port has failed."
    
    ::= { acSonetPortTraps 3 }

acSonetPortSectionDCCFailureClearTrap NOTIFICATION-TYPE
    OBJECTS { 
        acChassisCurrentTime, acChassisRingId, 
        acSonetPortNodeId, 
        acSonetPortSlot, 
        acSonetPortPort
        }
    STATUS  current
    DESCRIPTION
    "Section DCC for this SONET port has recovered."
    
    ::= { acSonetPortTraps 4 }

acSonetPortLineDCCFailureAlarmTrap NOTIFICATION-TYPE
    OBJECTS { 
        acChassisCurrentTime, acChassisRingId, 
        acSonetPortNodeId, 
        acSonetPortSlot, 
        acSonetPortPort
        }
    STATUS  current
    DESCRIPTION
    "Line DCC for this SONET port has failed."
    
    ::= { acSonetPortTraps 5 }

acSonetPortLineDCCFailureClearTrap NOTIFICATION-TYPE
    OBJECTS { 
        acChassisCurrentTime, acChassisRingId, 
        acSonetPortNodeId, 
        acSonetPortSlot, 
        acSonetPortPort
        }
    STATUS  current
    DESCRIPTION
    "Line DCC for this SONET port has recovered."
    
    ::= { acSonetPortTraps 6 }

acSonetPortAppianDCCFailureAlarmTrap NOTIFICATION-TYPE
    OBJECTS { 
        acChassisCurrentTime, acChassisRingId, 
        acSonetPortNodeId, 
        acSonetPortSlot, 
        acSonetPortPort
        }
    STATUS  current
    DESCRIPTION
