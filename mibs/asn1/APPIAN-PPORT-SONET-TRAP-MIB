acSonetSectionTraps  OBJECT IDENTIFIER ::= { acSonetSection 0 }

acSonetSectionLOSAlarmTrap NOTIFICATION-TYPE
    OBJECTS { 
        acChassisCurrentTime, acChassisRingId,
        acSonetSection15MinuteIntervalNodeId,
        acSonetSection15MinuteIntervalSlot,
        acSonetSection15MinuteIntervalPort,
        acSonetSection15MinuteIntervalNumber
        }
    STATUS  current
    DESCRIPTION
    "Loss of signal failure."
    
    ::= { acSonetSectionTraps 1 }

acSonetSectionLOSClearTrap NOTIFICATION-TYPE
    OBJECTS { 
        acChassisCurrentTime, acChassisRingId,
        acSonetSection15MinuteIntervalNodeId,
        acSonetSection15MinuteIntervalSlot,
        acSonetSection15MinuteIntervalPort,
        acSonetSection15MinuteIntervalNumber
        }
    STATUS  current
    DESCRIPTION
    "Loss of signal recovered."
    
    ::= { acSonetSectionTraps 2 }

acSonetSectionLOFAlarmTrap NOTIFICATION-TYPE
    OBJECTS { 
        acChassisCurrentTime, acChassisRingId, 
        acSonetSection15MinuteIntervalNodeId,
        acSonetSection15MinuteIntervalSlot,
        acSonetSection15MinuteIntervalPort,
        acSonetSection15MinuteIntervalNumber
        }
    STATUS  current
    DESCRIPTION
    "Loss of frame failure at section level."
    
    ::= { acSonetSectionTraps 3 }

acSonetSectionLOFClearTrap NOTIFICATION-TYPE
    OBJECTS { 
        acChassisCurrentTime, acChassisRingId, 
        acSonetSection15MinuteIntervalNodeId,
        acSonetSection15MinuteIntervalSlot,
        acSonetSection15MinuteIntervalPort,
        acSonetSection15MinuteIntervalNumber
        }
    STATUS  current
    DESCRIPTION
    "Loss of frame at section level recovered."
    
    ::= { acSonetSectionTraps 4 }

acSonetSectionTraceAlarmTrap NOTIFICATION-TYPE
    OBJECTS { 
        acChassisCurrentTime, acChassisRingId, 
        acSonetSectionTIMNodeId,
        acSonetSectionTIMSlot,
        acSonetSectionTIMPort
        }
    STATUS  current
    DESCRIPTION
    "The section trace identifier received for the SONET interface was
    unexpected."
    
    ::= { acSonetSectionTraps 5 }
    
acSonetSectionTraceClearTrap NOTIFICATION-TYPE
    OBJECTS { 
        acChassisCurrentTime, acChassisRingId, 
        acSonetSectionTIMNodeId,
        acSonetSectionTIMSlot,
        acSonetSectionTIMPort
        }
    STATUS  current
    DESCRIPTION
