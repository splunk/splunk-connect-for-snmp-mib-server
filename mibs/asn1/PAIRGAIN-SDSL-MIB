                Short,
        sdslCurr24hUASNet
                Short,
        sdslCurr24hESCust
                Short,
        sdslCurr24hUASCust
                Short
        }

    sdslCurr24hESNet  OBJECT-TYPE
        SYNTAX      Short
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "SDSL Current 24-hour ES count on the Network side of the span."
        ::= {sdslCurr24hPerformanceEntry 1}

    sdslCurr24hUASNet OBJECT-TYPE
        SYNTAX      Short
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "SDSL 24-hour UAS count on the Network side of the span."
        ::= {sdslCurr24hPerformanceEntry 2}

    sdslCurr24hESCust OBJECT-TYPE
        SYNTAX      Short
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "SDSL 24-hour ES count on the Customer side of the span."
        ::= {sdslCurr24hPerformanceEntry 3}

    sdslCurr24hUASCust OBJECT-TYPE
        SYNTAX      Short
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "SDSL 24-hour UAS count on the Customer side of the span."
        ::= {sdslCurr24hPerformanceEntry 4}

    ----------------------------------------------------
    -- SDSL Statistics
    ----------------------------------------------------
    sdslStatisticsTable  OBJECT-TYPE
        SYNTAX      SEQUENCE OF SdslStatisticsEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "This table lists statistics for margin, and pulse attenuation,
            of the SDSL loop."
        ::= {sdslMibObjects 4}

    sdslStatisticsEntry  OBJECT-TYPE
        SYNTAX      SdslStatisticsEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "Entries of the SDSL line statistics table "
        INDEX       {ifIndex}
        ::= {sdslStatisticsTable 1}

    SdslStatisticsEntry ::= SEQUENCE {
        sdslTipRingReversal
            Byte,
        sdslUpState
            Byte,
        sdslMarginNet
            Byte,
        sdslMarginHighNet
            Byte,
        sdslMarginLowNet
            Byte,
        sdslPulseAttenuationNet
            Byte,
        sdslMarginCust
            Byte,
        sdslMarginHighCust
            Byte,
        sdslMarginLowCust
            Byte,
        sdslPulseAttenuationCust
            Byte
	}

    sdslTipRingReversal OBJECT-TYPE
        SYNTAX      Byte
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "Current TIP/RING state on the SDSL span:
                 0 - TIP/RING is not reversed.
                 1 - TIP/RING is reversed.
            "
        ::= {sdslStatisticsEntry 1}

    sdslUpState  OBJECT-TYPE
        SYNTAX      Byte
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "Current loop state of the span:
                 0 - loop is up.
                 1 - loop is acquiring/converging.
                 2 - loop is signaling.
            "
        ::= {sdslStatisticsEntry 2}

    sdslMarginNet  OBJECT-TYPE
        SYNTAX      Byte
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "Current Margin on the Network side of the span."
        ::= {sdslStatisticsEntry 3}

    sdslMarginHighNet  OBJECT-TYPE
        SYNTAX      Byte
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "Highest Margin on the Network side of the span."
        ::= {sdslStatisticsEntry 4}

    sdslMarginLowNet  OBJECT-TYPE
        SYNTAX      Byte
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "Lowest Margin on the Network side of the span."
        ::= {sdslStatisticsEntry 5}

    sdslPulseAttenuationNet  OBJECT-TYPE
        SYNTAX      Byte
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "Pulse Attenuation on the Network side of the span."
        ::= {sdslStatisticsEntry 6}

    sdslMarginCust  OBJECT-TYPE
        SYNTAX      Byte
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "Current Margin on the Customer side of the span."
        ::= {sdslStatisticsEntry 7}

    sdslMarginHighCust  OBJECT-TYPE
        SYNTAX      Byte
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "Highest Margin on the Customer side of the span."
        ::= {sdslStatisticsEntry 8}

    sdslMarginLowCust  OBJECT-TYPE
        SYNTAX      Byte
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "Lowest Margin on the Customer side of the span."
        ::= {sdslStatisticsEntry 9}

    sdslPulseAttenuationCust  OBJECT-TYPE
        SYNTAX      Byte
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "Pulse Attenuation on the Customer side of the span."
        ::= {sdslStatisticsEntry 10}


----------------------------------------------------------------------
-- SDSL Line Settings Descriptions.
-- This table assigns the profile (stored on the AMC) for a particular Line.
----------------------------------------------------------------------
    sdslLineTable  OBJECT-TYPE
        SYNTAX      SEQUENCE OF SdslLineEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "This table lists which Profiles (located at the AMC)  are
            assigned to the particular SDSL Line."
        ::= {sdslMibObjects 5}

    sdslLineEntry  OBJECT-TYPE
        SYNTAX      SdslLineEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "Entries of the SDSL line table "
        INDEX       {ifIndex}
        ::= {sdslLineTable 1}

    SdslLineEntry ::= SEQUENCE {
        sdslLineConfProfile
            SdslLineProfileType,
        sdslLineAlarmConfProfile
            SdslLineProfileType
	}


    sdslLineConfProfile OBJECT-TYPE
        SYNTAX      SdslLineProfileType
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
        	"The value of this object identifies the row
            in the SDSL Line Configuration Profile Table,
            (sdslLineConfProfileTable), which applies for this
            SDSL line.  The Profiles are stored on the Management
			card.  In the case which the configuration
			profile has not been set, the value will be set to '0'."
        ::= { sdslLineEntry 1 }

	sdslLineAlarmConfProfile OBJECT-TYPE
        SYNTAX      SdslLineProfileType
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
        	"The value of this object identifies the row
            in the SDSL Line Alarm Configuration Profile Table,
       		(sdslLineAlarmConfProfileTable), which applies for this
       		SDSL line.  In the case which the configuration
			profile has not been set, the value will be set to '0'."
        ::= { sdslLineEntry 2 }




    ----------------------------------------------------
    -- SDSL Alarms Table
	----------------------------------------------------
    sdslAlarmTable  OBJECT-TYPE
        SYNTAX      SEQUENCE OF SdslAlarmEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "This table provides the SDSL alarms indication."
        ::= {sdslMibObjects 6}

    sdslAlarmEntry  OBJECT-TYPE
        SYNTAX      SdslAlarmEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "Entries of the SDSL span alarm table "
        INDEX       {ifIndex}
        ::= {sdslAlarmTable 1}

    SdslAlarmEntry ::= SEQUENCE {
        sdslMarginAlarmNet
            Byte,
        sdslESAlarmNet
            Byte,
        sdslUASAlarmNet
            Byte,
        sdslMarginAlarmCust
            Byte,
        sdslESAlarmCust
            Byte,
        sdslUASAlarmCust
            Byte,
        sdslLOSWAlarm
            Byte
    }

    sdslMarginAlarmNet OBJECT-TYPE
        SYNTAX      Byte
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "Margin Threshold Exceeded on the Network side of the span."
        ::= {sdslAlarmEntry 1}

    sdslESAlarmNet OBJECT-TYPE
        SYNTAX      Byte
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "Errored Seconds threshold exceeded on the Network side
             of the span."
        ::= {sdslAlarmEntry 2}

    sdslUASAlarmNet OBJECT-TYPE
        SYNTAX      Byte
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "Unavailable Seconds threshold exceeded on the Network
             side of the span."
        ::= {sdslAlarmEntry 3}

    sdslMarginAlarmCust OBJECT-TYPE
        SYNTAX      Byte
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "Margin Threshold Exceeded on the Customer side of the span."
        ::= {sdslAlarmEntry 4}


    sdslESAlarmCust OBJECT-TYPE
        SYNTAX      Byte
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "Errored Seconds Exceeded on the Customer side of the span."
        ::= {sdslAlarmEntry 5}

    sdslUASAlarmCust OBJECT-TYPE
        SYNTAX      Byte
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "Unavailable Seconds threshold exceeded on the customer side
			of the span."
        ::= {sdslAlarmEntry 6}

    sdslLOSWAlarm  OBJECT-TYPE
        SYNTAX      Byte
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "Loss of Sync Word (LOSW) occurred on the Network side of
			the SDSL loop.
            "
        ::= {sdslAlarmEntry 7}


    ----------------------------------------------------
    -- SDSL Alarms History Table for SDSL
    ----------------------------------------------------
    sdslAlarmHistoryTable  OBJECT-TYPE
        SYNTAX      SEQUENCE OF SdslAlarmHistoryEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "This table provides the SDSL alarms history"
        ::= {sdslMibObjects 7}

    sdslAlarmHistoryEntry  OBJECT-TYPE
        SYNTAX      SdslAlarmHistoryEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "Entries of the SDSL Alarm History table "
        INDEX       {ifIndex}
        ::= {sdslAlarmHistoryTable 1}

    SdslAlarmHistoryEntry ::= SEQUENCE {
        sdslMarginAlarmFirst
            PgSdslTimeStamp,
        sdslMarginAlarmLast
            PgSdslTimeStamp,
        sdslMarginAlarmCount
            Short,
        sdslESAlarmFirst
            PgSdslTimeStamp,
        sdslESAlarmLast
            PgSdslTimeStamp,
        sdslESAlarmCount
            Short,
        sdslUASAlarmFirst
            PgSdslTimeStamp,
        sdslUASAlarmLast
            PgSdslTimeStamp,
        sdslUASAlarmCount
            Short,
        sdslLOSWAlarmFirst
            PgSdslTimeStamp,
        sdslLOSWAlarmLast
            PgSdslTimeStamp,
        sdslLOSWAlarmCount
            Short
    }


    sdslMarginAlarmFirst  OBJECT-TYPE
        SYNTAX      PgSdslTimeStamp
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "Date and time Margin threshold exceeded alarm first occurred"
        ::= {sdslAlarmHistoryEntry 1}

    sdslMarginAlarmLast  OBJECT-TYPE
        SYNTAX      PgSdslTimeStamp
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "Date and time Margin threshold exceeded alarm last occurred"
        ::= {sdslAlarmHistoryEntry 2}

    sdslMarginAlarmCount  OBJECT-TYPE
        SYNTAX      Short
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "The number of times the Margin threshold exceeded alarm has
			occurred
            "
        ::= {sdslAlarmHistoryEntry 3}

    sdslESAlarmFirst OBJECT-TYPE
        SYNTAX      PgSdslTimeStamp
