        STATUS      current
        DESCRIPTION
            "Date and time Errored Seconds threshold exceeded alarm first occurred
             "
        ::= {sdslAlarmHistoryEntry 4}

    sdslESAlarmLast OBJECT-TYPE
        SYNTAX      PgSdslTimeStamp
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "Date and time Errored Seconds threshold exceeded alarm last occurred
             "
        ::= {sdslAlarmHistoryEntry 5}

     sdslESAlarmCount OBJECT-TYPE
        SYNTAX      Short
