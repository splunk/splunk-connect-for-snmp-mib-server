DEFINITIONS ::= BEGIN

-- SECTION 1: Top Level Definitions

IMPORTS
    Counter32, Integer32, MODULE-IDENTITY, OBJECT-TYPE, NOTIFICATION-TYPE
        FROM SNMPv2-SMI
    OBJECT-TYPE
        FROM RFC-1212
    pgainSDSL
        FROM PAIRGAIN-COMMON-HD-MIB
    ifIndex
        FROM RFC1213-MIB
    DisplayString, RowStatus, TruthValue
		FROM SNMPv2-TC;


-- Module Identity SDSL (required for SNMP v2)
pgsdslMIB MODULE-IDENTITY
    LAST-UPDATED    "9910070000Z"
    ORGANIZATION    "Pairgain Technologies Inc."
        CONTACT-INFO    "
                    14402 Franklin Avenue
                    Tustin, CA  92780
                    "
    DESCRIPTION "The module defines the SDSL MIB "
    ::= {pgainSDSL 1}

sdslMibObjects        OBJECT IDENTIFIER ::= { pgsdslMIB 1 }

-- Define a one byte integer
    Byte ::= INTEGER (0..255)

-- Define a two bytes integer
    Short ::= INTEGER (0..65535)

    SdslLineProfileType ::= INTEGER (0..65535)


-- Define a time stamp structure. The time stamp consists of 8 bytes,
-- with the following format :
--     byte 1    : reserved
--     bytes 2,3 : four digit year for y2k compliance,
--     byte 4    : month of year,
--     byte 5    : day of month,
--     byte 6    : hour (24 hour format)
--     byte 7    : minutes
--     byte 8    : seconds


    PgSdslTimeStamp ::= OCTET STRING (SIZE(8))

    PgSdslAlarmSeverityMode ::=
       INTEGER {
               disable(1),
               enable(2),
               notAvailable(3)
       }



    ----------------------------------------------------
    -- SDSL 15-minute Performance History
    ----------------------------------------------------
    sdsl15mPerformanceTable  OBJECT-TYPE
        SYNTAX      SEQUENCE OF Sdsl15mPerformanceEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "This table provides the 15-minute performance history monitoring
             of the last 24 hours for all SDSL Errored Seconds (ES),
             and Unavailable Seconds (UAS)."
        ::= {sdslMibObjects 1}

    sdsl15mPerformanceEntry  OBJECT-TYPE
        SYNTAX      Sdsl15mPerformanceEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "Entries of the SDSL 15-min performance table.  All 96 entries for a
            24 hour period are zeroed out and available at power up.  Not all
            of the returned values are legitimate until the appropriate amount of time
            has elapsed."
        INDEX       {ifIndex, sdsl15mHistoryId}
        ::= {sdsl15mPerformanceTable 1}

    Sdsl15mPerformanceEntry ::= SEQUENCE {
        sdsl15mHistoryId
                Short,
        sdsl15mESNet
                Short,
        sdsl15mUASNet
                Short,
        sdsl15mESCust
                Short,
        sdsl15mUASCust
                Short
        }


    sdsl15mHistoryId  OBJECT-TYPE
        SYNTAX      Short
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "Identifies the history ID within the 96 15-minute performance
             history.
            "
        ::= {sdsl15mPerformanceEntry 1}

    sdsl15mESNet  OBJECT-TYPE
        SYNTAX      Short
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "SDSL 15-min ES count on Network side of the span."
        ::= {sdsl15mPerformanceEntry 2}

    sdsl15mUASNet OBJECT-TYPE
        SYNTAX      Short
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "SDSL 15-min UAS count on Network side of the span."
        ::= {sdsl15mPerformanceEntry 3}

    sdsl15mESCust OBJECT-TYPE
        SYNTAX      Short
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "SDSL 15-min ES count on the Customer side of the span."
        ::= {sdsl15mPerformanceEntry 4}


    sdsl15mUASCust OBJECT-TYPE
        SYNTAX      Short
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "SDSL 15-min UAS count on the Customer side of the span."
        ::= {sdsl15mPerformanceEntry 5}




    ----------------------------------------------------
    -- SDSL 24-Hour Performance History
    ----------------------------------------------------
    sdsl24hPerformanceTable  OBJECT-TYPE
        SYNTAX      SEQUENCE OF Sdsl24hPerformanceEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "This table provides 24-hour performance history monitoring of the
             last 7 days for all SDSL Errored Seconds (ES), and Unavailable
             Seconds (UAS). All 7 entries for the 7-day history are zeroed out
             and available at power up.  Not all of the returned values are
             legitimate until the appropriate number of days has elapsed."
        ::= {sdslMibObjects 2}

    sdsl24hPerformanceEntry  OBJECT-TYPE
        SYNTAX      Sdsl24hPerformanceEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "Entries of the SDSL 24-hour performance table "
        INDEX	{ifIndex, sdsl24hHistoryId}
        ::= {sdsl24hPerformanceTable 1}

    Sdsl24hPerformanceEntry ::= SEQUENCE {
        sdsl24hHistoryId
                Short,
        sdsl24hESNet
                Short,
        sdsl24hUASNet
                Short,
        sdsl24hESCust
                Short,
        sdsl24hUASCust
                Short
        }



    sdsl24hHistoryId  OBJECT-TYPE
        SYNTAX      Short
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "Identify the history ID within the 7 24-hour performance
             history.
            "
        ::= {sdsl24hPerformanceEntry 1}

    sdsl24hESNet  OBJECT-TYPE
        SYNTAX      Short
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "SDSL 24-hour ES count on the Network side of the span."
        ::= {sdsl24hPerformanceEntry 2}


    sdsl24hUASNet OBJECT-TYPE
        SYNTAX      Short
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "SDSL 24-hour UAS count on the Network side of the span."
        ::= {sdsl24hPerformanceEntry 3}



    sdsl24hESCust OBJECT-TYPE
        SYNTAX      Short
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "SDSL 24-hour ES count on the Customer side of the span."
        ::= {sdsl24hPerformanceEntry 4}


    sdsl24hUASCust OBJECT-TYPE
        SYNTAX      Short
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "SDSL 24-hour UAS count on the Customer side of the span."
        ::= {sdsl24hPerformanceEntry 5}




    ----------------------------------------------------
    -- SDSL Current 24-Hour Performance Counts
    ----------------------------------------------------
    sdslCurr24hPerformanceTable  OBJECT-TYPE
        SYNTAX      SEQUENCE OF SdslCurr24hPerformanceEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
