         ::= {aptisProtrapsV2 3}

    aptis-ac1down NOTIFICATION-TYPE
        OBJECTS   { trapGenNum, trapPath, trapSeverity }
        STATUS    obsolete
        DESCRIPTION
            "The AC1 power supply has stopped functioning.

             This trap is no longer supported."
        ::= {aptisProtrapsV2 4}

