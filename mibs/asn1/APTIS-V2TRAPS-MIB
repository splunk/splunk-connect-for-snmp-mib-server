-- The following variables are for use in the varbinds of traps only.
-- They cannot be retrieved by the NMS.
          
      trapGenNum              OBJECT-TYPE
            SYNTAX            INTEGER (0..2147483647)
            MAX-ACCESS        accessible-for-notify
            STATUS            current
            DESCRIPTION      
                "A unigue incrementing integer assigned for each trap."

      ::= { aptis-traps 2 }

      trapPath                OBJECT-TYPE
            SYNTAX            OCTET STRING
            MAX-ACCESS        accessible-for-notify
            STATUS            current
            DESCRIPTION      
                "A path string to the object associated with the trap."

      ::= { aptis-traps 3 }

      trapSeverity            OBJECT-TYPE
            SYNTAX            INTEGER (0..7)
            MAX-ACCESS        accessible-for-notify
            STATUS            current
            DESCRIPTION      
                "The severity of this trap, 7 being most severe."

      ::= { aptis-traps 4 }

      trapString              OBJECT-TYPE
            SYNTAX            OCTET STRING
            MAX-ACCESS        accessible-for-notify
            STATUS            current
            DESCRIPTION      
                "Extra text associated with the trap."

      ::= { aptis-traps 5 }

      trapIpAddr              OBJECT-TYPE
            SYNTAX            IpAddress
            MAX-ACCESS        accessible-for-notify
            STATUS            current
            DESCRIPTION      
                "An IP address associated with the trap."

      ::= { aptis-traps 6 }

      trapSlotNum             OBJECT-TYPE
            SYNTAX            INTEGER
            MAX-ACCESS        accessible-for-notify
            STATUS            current
            DESCRIPTION      
                "A slot number associated with the trap."

      ::= { aptis-traps 7 }

      trapPortNum             OBJECT-TYPE
            SYNTAX            INTEGER
            MAX-ACCESS        accessible-for-notify
            STATUS            current
            DESCRIPTION      
                "A port number associated with the trap."

      ::= { aptis-traps 8 }

      trapTunnelType          OBJECT-TYPE
            SYNTAX            INTEGER
                              {
                              unknown(0),
                              none(1),
                              other(2),
                              l2f(3),
                              l2tp(4)
                              }
            MAX-ACCESS        accessible-for-notify
            STATUS            current
            DESCRIPTION      
                "A tunnel type associated with the trap."

      ::= { aptis-traps 9 }

      trapUserName            OBJECT-TYPE
            SYNTAX            OCTET STRING
            MAX-ACCESS        accessible-for-notify
            STATUS            current
            DESCRIPTION      
                "The user name assiciated with this trap."

      ::= { aptis-traps 10 }

      trapAuthThreshold       OBJECT-TYPE
            SYNTAX            INTEGER
            MAX-ACCESS        accessible-for-notify
            STATUS            current
            DESCRIPTION      
                "The number of consecutive authorization failures."

      ::= { aptis-traps 11 }

      trapFilterName          OBJECT-TYPE
            SYNTAX            OCTET STRING
            MAX-ACCESS        accessible-for-notify
            STATUS            current
            DESCRIPTION      
                "The filter name assiciated with this trap."

      ::= { aptis-traps 12 }

      trapTempCurrent         OBJECT-TYPE
            SYNTAX            INTEGER
            UNITS             "Degrees Celsius"
            MAX-ACCESS        accessible-for-notify
            STATUS            current
            DESCRIPTION      
                  "Current temperature in degrees Celsius."

      ::= { aptis-traps 13 }

      trapTempConfigWarningLevel OBJECT-TYPE
            SYNTAX            INTEGER
            UNITS             "Degrees Celsius"
            MAX-ACCESS        accessible-for-notify
            STATUS            current
            DESCRIPTION      
                  "Configured warning level temperature in degrees
                   Celsius."

      ::= { aptis-traps 14 }

      trapSessionId OBJECT-TYPE
            SYNTAX            INTEGER
            MAX-ACCESS        accessible-for-notify
            STATUS            current
            DESCRIPTION      
                  "A session id number associated with the trap."

      ::= { aptis-traps 15 }

      trapVpopId OBJECT-TYPE
            SYNTAX            INTEGER
            MAX-ACCESS        accessible-for-notify
            STATUS            current
            DESCRIPTION      
                  "A VPOP (Virtual Point of Presence) id associated
                   with the trap."

      ::= { aptis-traps 16 }

      trapModemCount OBJECT-TYPE
            SYNTAX            INTEGER
            MAX-ACCESS        accessible-for-notify
            STATUS            current
            DESCRIPTION      
                  "The number of available modems."

      ::= { aptis-traps 17 }
