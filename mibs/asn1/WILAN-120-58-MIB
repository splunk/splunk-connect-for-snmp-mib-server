-- System Commands

commands           OBJECT IDENTIFIER ::= { awe120-58 4 }

rebootCurrent      OBJECT-TYPE
                   SYNTAX       INTEGER { reboot(1) }
                   MAX-ACCESS   read-write
                   STATUS       current
                   DESCRIPTION  "Reboot Current System Image"
                   ::= { commands 1 }

rebootImage        OBJECT-TYPE
                   SYNTAX       DisplayString (SIZE (0..15))
                   MAX-ACCESS   read-write
                   STATUS       current
                   DESCRIPTION  "Reboot Specified System Image"
                   ::= { commands 2 }

rebootNewRfConfig  OBJECT-TYPE
                   SYNTAX       INTEGER { reboot(1) }
                   MAX-ACCESS   read-write
                   STATUS       current
                   DESCRIPTION  "Reboot New RF Configuration"
                   ::= { commands 3 }

restFactConfReboot OBJECT-TYPE
                   SYNTAX       INTEGER { restore(1) }
                   MAX-ACCESS   read-write
                   STATUS       current
                   DESCRIPTION  "Restore Factory Configuration and Reboot"
                   ::= { commands 4 }

saveConfToFlash    OBJECT-TYPE
                   SYNTAX       INTEGER { save(1) }
                   MAX-ACCESS   read-write
                   STATUS       current
                   DESCRIPTION  "Save Current Configuration To Flash"
                   ::= { commands 5 }

resetRadioStats    OBJECT-TYPE
                   SYNTAX       INTEGER { reset(1) }
                   MAX-ACCESS   read-write
                   STATUS       current
                   DESCRIPTION  "Reset Radio Statistics"
                   ::= { commands 6 }

resetEthernetStats OBJECT-TYPE
                   SYNTAX       INTEGER { reset(1) }
                   MAX-ACCESS   read-write
                   STATUS       current
                   DESCRIPTION  "Reset Ethernet Statistics"
                   ::= { commands 7 }

END



