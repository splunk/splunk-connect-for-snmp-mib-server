-- System Status

status             OBJECT IDENTIFIER ::= { awe120-58 2 }

totalHours         OBJECT-TYPE
                   SYNTAX       Counter
                   MAX-ACCESS   read-only
                   STATUS       current
                   DESCRIPTION  "Cumulative Run-Time Hours"
                   ::= { status 1 }

systemHours        OBJECT-TYPE
                   SYNTAX       Counter
                   MAX-ACCESS   read-only
                   STATUS       current
                   DESCRIPTION  "Current Run-Time Hours Since Powerup"
                   ::= { status 2 }

loginOkays         OBJECT-TYPE
                   SYNTAX       Counter
                   MAX-ACCESS   read-only
                   STATUS       current
                   DESCRIPTION  "Number of Successful Logins"
                   ::= { status 3 }

loginFails         OBJECT-TYPE
                   SYNTAX       Counter
                   MAX-ACCESS   read-only
                   STATUS       current
                   DESCRIPTION  "Number of Unsuccessful Login Attempts"
                   ::= { status 4 }

localUser          OBJECT-TYPE
                   SYNTAX       INTEGER { none(0), user(1), supervisor(2) }
                   MAX-ACCESS   read-only
                   STATUS       current
                   DESCRIPTION  "Local User Login Status"
                   ::= { status 5 }

telnetUser         OBJECT-TYPE
                   SYNTAX       INTEGER { none(0), user(1), supervisor(2) }
                   MAX-ACCESS   read-only
                   STATUS       current
                   DESCRIPTION  "Telnet User Login Status"
                   ::= { status 6 }

