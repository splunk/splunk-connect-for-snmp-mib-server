-- The System Image List

systemImageList  OBJECT-TYPE
                 SYNTAX      SEQUENCE OF SystemImageEntry
                 MAX-ACCESS  not-accessible
                 STATUS      current
                 DESCRIPTION "System Image List"
                ::= { config 100 }

systemImageEntry   OBJECT-TYPE
                   SYNTAX       SystemImageEntry
                   MAX-ACCESS   not-accessible
                   STATUS       current
                   DESCRIPTION  "System Image List Entry"
                   INDEX        { systemImageNumber }
                   ::= { systemImageList 1 }

SystemImageEntry ::=
                   SEQUENCE {
                     systemImageNumber
                         INTEGER,
                     systemImageName
                         DisplayString (SIZE (0..15)),
                     systemImageRevn
                         DisplayString (SIZE (0..15)),
                     systemImageDate
                         DisplayString (SIZE (0..15)),
                     systemImageTime
                         DisplayString (SIZE (0..15)),
                     systemImageRevn
                         DisplayString (SIZE (0..15)),
                     systemImageSize
                         INTEGER,
                     systemImageText
                         DisplayString (SIZE (0..15))
                   }

systemImageNumber  OBJECT-TYPE
                   SYNTAX       INTEGER
                   MAX-ACCESS   read-only
                   STATUS       current
                   DESCRIPTION  "System Image Number"
                   ::= { systemImageEntry 1 }

systemImageName    OBJECT-TYPE
                   SYNTAX       DisplayString (SIZE (0..15))
                   MAX-ACCESS   read-only
                   STATUS       current
                   DESCRIPTION  "System Image File Name"
                   ::= { systemImageEntry 2 }

systemImageRevn    OBJECT-TYPE
                   SYNTAX       DisplayString (SIZE (0..15))
                   MAX-ACCESS   read-only
                   STATUS       current
                   DESCRIPTION  "System Image Revision Identifier"
                   ::= { systemImageEntry 3 }

systemImageDate    OBJECT-TYPE
                   SYNTAX       DisplayString (SIZE (0..15))
                   MAX-ACCESS   read-only
                   STATUS       current
                   DESCRIPTION  "System Image File Date"
                   ::= { systemImageEntry 4 }

systemImageTime    OBJECT-TYPE
                   SYNTAX       DisplayString (SIZE (0..15))
                   MAX-ACCESS   read-only
                   STATUS       current
                   DESCRIPTION  "System Image File Time"
                   ::= { systemImageEntry 5 }

systemImageSize    OBJECT-TYPE
                   SYNTAX       INTEGER
                   MAX-ACCESS   read-only
                   STATUS       current
                   DESCRIPTION  "System Image File Size"
                   ::= { systemImageEntry 6 }

systemImageText    OBJECT-TYPE
                   SYNTAX       DisplayString (SIZE (0..15))
                   MAX-ACCESS   read-only
                   STATUS       current
                   DESCRIPTION  "System Image Descriptive Text"
                   ::= { systemImageEntry 7 }

-- The Remote Unit Configuration Table

-- remoteTable        OBJECT-TYPE
--                    SYNTAX      SEQUENCE OF RemoteEntry
--                    MAX-ACCESS  not-accessible
--                    STATUS      current
--                    DESCRIPTION "Remote Unit Configuration Table."
--                   ::= { config 101 }

-- remoteEntry        OBJECT-TYPE
--                    SYNTAX       RemoteEntry
--                    MAX-ACCESS   not-accessible
--                    STATUS       current
--                    DESCRIPTION  "Remote Unit Configuration Table Entry"
--                   INDEX        { remoteNumber }
--                    ::= { remoteTable 1 }

-- RemoteEntry ::=
--                    SEQUENCE {
--                      remoteNumber
--                          INTEGER,
--                      remotePrio
--                          INTEGER,
--                      remoteUnits
--                          INTEGER
--                    }

-- remoteNumber       OBJECT-TYPE
--                    SYNTAX       INTEGER
--                    MAX-ACCESS   read-only
--                    STATUS       current
--                    DESCRIPTION  "Remote Unit Station Id"
--                    ::= { remoteEntry 1 }

-- remotePrio         OBJECT-TYPE
--                    SYNTAX       INTEGER
--                    MAX-ACCESS   read-write
--                    STATUS       current
--                    DESCRIPTION  "Remote Unit Polling Priority"
--                    ::= { remoteEntry 2 }

-- remoteUnits        OBJECT-TYPE
--                    SYNTAX       INTEGER
