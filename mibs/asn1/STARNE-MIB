			starPortM32RxLfdErrorCount			INTEGER,
			starPortM32RxIntBufOverFlwCount		INTEGER,
			starPortM32RxAbortCount				INTEGER,
			starPortM32RxShortFrameCount		INTEGER,
			starPortM32RxDiscardFrameCount		INTEGER,
			starPortM32RxPduCount				INTEGER,
			starPortM32RxByteCount				INTEGER,
			starPortM32TxErrorCount				INTEGER,
			starPortM32TxPduCount				INTEGER,
			starPortM32TxByteCount				INTEGER
		}

	starPortM32SlotId	OBJECT-TYPE
