	-----------------------------------------------------------------------
	-- Star Rcl Info Subgroup (from FutureSoft - 12/12/95)
	-----------------------------------------------------------------------
	starRclInfo		OBJECT IDENTIFIER ::= { starSlot 20 }

	-----------------------------------------------------------------------
	-- Star Rcl Info Table
	-----------------------------------------------------------------------
	starRclInfoTbl	 OBJECT-TYPE
		SYNTAX  SEQUENCE OF StarRclInfoEntry
		ACCESS  not-accessible
		STATUS  mandatory
		DESCRIPTION
			"This table contains duplication related objects."
		::= { starRclInfo 1 }

	starRclInfoEntry	 OBJECT-TYPE
		SYNTAX  StarRclInfoEntry
		ACCESS  not-accessible
		STATUS  mandatory
		DESCRIPTION
			"An entry into RclInfoTbl."
		INDEX { starRclInfoSlotId }
		::= { starRclInfoTbl 1 }

	StarRclInfoEntry ::=
		SEQUENCE {
			starRclInfoSlotId			INTEGER ,
			starRclNumRegAppl			INTEGER ,
			starRclNumSwitchovers		INTEGER ,
			starRclSwitchoverThreshold	INTEGER ,
			starRclSwitchoverTimeLimit	INTEGER ,
			starRclSwitchoverResetTime	INTEGER ,
			starRclLockoutFlag			INTEGER ,
			starRclHmcTestStatus		INTEGER ,
			starRclHmcTestResult		INTEGER ,
			starRclModuleStatus		INTEGER ,
			starRclSwitchoverReason	INTEGER ,
			starRclSwitchoverTime	INTEGER
		}

	starRclInfoSlotId	OBJECT-TYPE
		SYNTAX  INTEGER
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"This object indicates the slot id."
		::= { starRclInfoEntry 1 }

	starRclNumRegAppl  OBJECT-TYPE
		SYNTAX  INTEGER
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"This object indicates the number of
			 applications registered with the RCL for
			 data shadowing."
		::= { starRclInfoEntry 2 }

	starRclNumSwitchovers	 OBJECT-TYPE
		SYNTAX  INTEGER 
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"Indicates the number of times switchover has occurred
			since the time the system had come up."
		::= { starRclInfoEntry 3 }

	starRclSwitchoverThreshold	OBJECT-TYPE
		SYNTAX  INTEGER 
		ACCESS  read-write
		STATUS  mandatory
		DESCRIPTION
			"Indicates the threshold of the number of times
			 switchover can occur within the specified time."
		::= { starRclInfoEntry 4 }

	starRclSwitchoverTimeLimit	OBJECT-TYPE
		SYNTAX  INTEGER 
		ACCESS  read-write
		STATUS  mandatory
		DESCRIPTION
			"Indicates the time within which number of switchovers
			 should not exceed the maximum."
		::= { starRclInfoEntry 5 }

	starRclSwitchoverResetTime	OBJECT-TYPE
		SYNTAX  INTEGER 
		ACCESS  read-write
		STATUS  mandatory
		DESCRIPTION
			"Indicates the time for which there should be no 
			 switchover before the module can come out of lockout state."
		::= { starRclInfoEntry 6 }

	starRclLockoutFlag		 OBJECT-TYPE
		SYNTAX  INTEGER {
			lock (1),
			unlock (2)
		}
		ACCESS  read-write
		STATUS  mandatory
		DESCRIPTION
			"If this object is set(lock) then a switchover of
			 active to standby or standby to active cannot take place."
		::= { starRclInfoEntry 7 }
	
	starRclHmcTestStatus	OBJECT-TYPE
		SYNTAX  INTEGER {
			idle (1),
			testInProgress (2),
			testInitiatedByOtherModule (3),
			initiateTest (4)
		}
		ACCESS  read-write
		STATUS  mandatory
		DESCRIPTION
			"Indicates the status of the HMC test.
			 A set(write) to 'initiatetest' is performed
			 to initiate the diagnostic."
		::= { starRclInfoEntry 8 }

	starRclHmcTestResult	OBJECT-TYPE
		SYNTAX  INTEGER {
			testSuccess (1),
			testCancelDueToStateChange (2),
			testCancelDueToTimeout (3),
			testFail (4)
		}
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"Indicates the result of the last HMC test carried out."
		::= { starRclInfoEntry 9 }

	starRclModuleStatus		OBJECT-TYPE
		SYNTAX  INTEGER {
			init (1),
			active (2),
			standby (3),
			standalone (4)
		}
		ACCESS  read-write
		STATUS  mandatory
		DESCRIPTION
			"Indicates the status of the board .
			 A set(write) to 'standby' is done
			 to make the module to switch to standby mode."
		::= { starRclInfoEntry 10 }

	starRclSwitchoverReason	OBJECT-TYPE
		SYNTAX  INTEGER {
			eject (1),
			powerFail (2),
			cpuFail (3),
			reset (4),
			redundantModuleUp (5),
			requestFromRednModule (6),
			requestFromNms (7),
			rcldHealthCheckFail (8),
			hmcdTestFail (9),
			hmcRegisterFail (10)
		}
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"Indicates the reason for the last switchover"
		::= { starRclInfoEntry 11 }

	starRclSwitchoverTime	OBJECT-TYPE
		SYNTAX  INTEGER
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"Indicates the time when the last switchover occurred"
		::= { starRclInfoEntry 12 }

	-----------------------------------------------------------------------
	-- Star Rcl Reg Info Table
	-----------------------------------------------------------------------
	starRclRegInfoTbl			OBJECT-TYPE
		SYNTAX  SEQUENCE OF StarRclRegInfoEntry
		ACCESS  not-accessible
		STATUS  mandatory
		DESCRIPTION
			"This table contains duplication related variables."
		::= { starRclInfo  2 }

	starRclRegInfoEntry	 OBJECT-TYPE
		SYNTAX  StarRclRegInfoEntry
		ACCESS  not-accessible
		STATUS  mandatory
		DESCRIPTION
			"An entry into spuRclRegInfoTbl."
		INDEX { starRclRegInfoSlotId, starRclRegKey }
		::= { starRclRegInfoTbl 1 }

	StarRclRegInfoEntry ::=
		SEQUENCE {
			starRclRegInfoSlotId	INTEGER ,
			starRclRegKey			INTEGER ,
			starRclRegTaskName		OCTET STRING ,
			starRclRegQueueName		OCTET STRING
		}

	starRclRegInfoSlotId OBJECT-TYPE
		SYNTAX  INTEGER
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"This object indicates the slot id."
		::= { starRclRegInfoEntry 1 }

	starRclRegKey		OBJECT-TYPE
		SYNTAX  INTEGER
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"This object indicates the key name with which
			 the application was registered."
		::= { starRclRegInfoEntry 2 }

	starRclRegTaskName   OBJECT-TYPE
		SYNTAX  OCTET STRING ( SIZE (0..3 ) )
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"This object indicates the task name for the
			 corresponding key."
		::= { starRclRegInfoEntry 3 }

	starRclRegQueueName  OBJECT-TYPE
		SYNTAX  OCTET STRING ( SIZE (0..3 ) )
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"This object indicates the queue name for the
			 corresponding key."
		::= { starRclRegInfoEntry 4 }

	-----------------------------------------------------------------------
	-- Star CAS Info Subgroup
	-----------------------------------------------------------------------
	starCasInfo			OBJECT IDENTIFIER ::= { starSlot 21 }

	-----------------------------------------------------------------------
	-- Star CAS Status Info Subgroup
	-----------------------------------------------------------------------
	starCasStatusInfoTbl	OBJECT-TYPE
		SYNTAX	SEQUENCE OF StarCasStatusInfoEntry
		ACCESS	not-accessible
		STATUS	mandatory
		DESCRIPTION
			"This Table has info about the CAS status."
		::={ starCasInfo 1 }

	starCasStatusInfoEntry	OBJECT-TYPE
		SYNTAX	StarCasStatusInfoEntry
		ACCESS  not-accessible
		STATUS  mandatory
		DESCRIPTION	
			"Specifies the Cas status ."
		INDEX { starCasStatusSlotId, starCasStatusPortId, 
				starCasStatusChannelId }
		::= { starCasStatusInfoTbl 1 }

	StarCasStatusInfoEntry ::=
		SEQUENCE {
			starCasStatusSlotId					INTEGER,
			starCasStatusPortId					INTEGER,
			starCasStatusChannelId				INTEGER,
			starCasStatusConfigured				INTEGER,	
			starCasStatusCurrentStatus			INTEGER,
			starCasStatusStatusChange	 		INTEGER,
			starCasStatusNewStatusSampleNum		INTEGER
		}

	starCasStatusSlotId	OBJECT-TYPE
		SYNTAX  INTEGER
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"This object indicates the slot id."
		::= { starCasStatusInfoEntry 1 }

	starCasStatusPortId	OBJECT-TYPE
		SYNTAX  INTEGER
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"This object indicates the port id."
		::= { starCasStatusInfoEntry 2 }

	starCasStatusChannelId	OBJECT-TYPE
		SYNTAX  INTEGER
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"This object indicates the channel id."
		::= { starCasStatusInfoEntry 3 }

	starCasStatusConfigured	 OBJECT-TYPE
		SYNTAX  INTEGER
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"This object indicates if the channel is configured."
		::= { starCasStatusInfoEntry 4 }

	starCasStatusCurrentStatus	OBJECT-TYPE
		SYNTAX  INTEGER
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"This object indicates the ON-Hook or OFF-Hook status."
		::= { starCasStatusInfoEntry 5 }

	starCasStatusStatusChange	 OBJECT-TYPE
		SYNTAX  INTEGER
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"This object indicates the state change of a channel."
		::= { starCasStatusInfoEntry 6 }

	 starCasStatusNewStatusSampleNum	 OBJECT-TYPE
		SYNTAX  INTEGER
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"This object indicates the number of times the
			 the CAS polling routine should see this channel in
			 a certain state to flag a change in state."
		::= { starCasStatusInfoEntry 7 }

	-----------------------------------------------------------------------
	-- Star CAS Status Port Info Subgroup
	-----------------------------------------------------------------------
	starCasStatusPortInfoTbl	OBJECT-TYPE
		SYNTAX	SEQUENCE OF StarCasStatusPortInfoEntry
		ACCESS	not-accessible
		STATUS	mandatory
		DESCRIPTION
