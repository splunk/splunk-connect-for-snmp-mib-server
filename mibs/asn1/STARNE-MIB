	-----------------------------------------------------------------------
	-- Physical Loopback for FIU_T3(DS1) by sunah 99.9.14
	-----------------------------------------------------------------------
	starPhysicalLoopbackTable OBJECT-TYPE
		SYNTAX  SEQUENCE OF PhysicalLoopbackTable 
		ACCESS  not-accessible
		STATUS  mandatory
		DESCRIPTION
			" Set and Get Physical Loopback for FIU_T3(DS1)"
		::= { starPort 21 }

	 starPhysicalLoopbackEntry   OBJECT-TYPE
        SYNTAX  PhysicalLoopbackTable
        ACCESS  not-accessible
        STATUS  mandatory
        DESCRIPTION
            ""
        INDEX   { starSlotIndex, starPortIndex,
                  starPhysicalLoopbackDs1Port, starPhysicalLoopbackMethod,
                  starPhysicalLoopbackCtrl }
        ::= { starPhysicalLoopbackTable 1 }
 
    PhysicalLoopbackTable ::=
		SEQUENCE {
			starPhysicalLoopbackDs1Port    INTEGER,
			starPhysicalLoopbackMethod     INTEGER,
			starPhysicalLoopbackCtrl       INTEGER,
			starPhysicalLoopbackStatus     INTEGER,
			starPhysicalLoopbackDescr      OCTET STRING
		}
	
	starPhysicalLoopbackDs1Port    OBJECT-TYPE
        SYNTAX  INTEGER
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION
            "FIU_T3(Ds1) Port "
        ::= { starPhysicalLoopbackEntry 1 }

	starPhysicalLoopbackMethod    OBJECT-TYPE
		SYNTAX  INTEGER
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION
			"19 : External Loopback 
			 20 : Internal Loopback"
		::= { starPhysicalLoopbackEntry 2 }


	starPhysicalLoopbackCtrl    OBJECT-TYPE
		SYNTAX  INTEGER{
			set(1),
			release(2)
		}
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION
			"1 : Set Loopback 
			 2 : Release Loopback"
		::= { starPhysicalLoopbackEntry 3 }

	starPhysicalLoopbackStatus    OBJECT-TYPE
		SYNTAX  INTEGER
        ACCESS  read-write
        STATUS  mandatory
        DESCRIPTION
			" Get or Set a physical Loopback for each port"	
		::= { starPhysicalLoopbackEntry 4 }

	starPhysicalLoopbackDescr    OBJECT-TYPE
		SYNTAX  OCTET STRING
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			" Get a physical Loopback status for all ports "
		::= { starPhysicalLoopbackEntry 5 }
 
    ----------------------------------------------------------------------
    -- starPPP Group
    ----------------------------------------------------------------------
    starPPP      OBJECT IDENTIFIER ::= { starPort 22 }

    starPPPLinkStatusTable   OBJECT-TYPE
           SYNTAX    SEQUENCE OF StarPPPLinkStatusEntry
           ACCESS    not-accessible
           STATUS    mandatory
           DESCRIPTION
                     "A table containing PPP-link specific variables
                     for this PPP implementation."
           ::= { starPPP 1 }

    starPPPLinkStatusEntry   OBJECT-TYPE
           SYNTAX    StarPPPLinkStatusEntry
           ACCESS    not-accessible
           STATUS    mandatory
           DESCRIPTION
                     "Management information about a particular PPP
                     Link."
	       INDEX     { starSlotIndex, starPortIndex }
           ::= { starPPPLinkStatusTable 1 }


    StarPPPLinkStatusEntry ::= SEQUENCE {
           starPPPLinkStatusPhysicalIndex
                INTEGER,
           starPPPLinkStatusBadAddresses
                Counter,
           starPPPLinkStatusBadControls
                Counter,
           starPPPLinkStatusPacketTooLongs
                Counter,
           starPPPLinkStatusBadFCSs
                Counter,
           starPPPLinkStatusLocalMRU
                INTEGER,
           starPPPLinkStatusRemoteMRU
                INTEGER,
           starPPPLinkStatusLocalToPeerACCMap
                OCTET STRING,
           starPPPLinkStatusPeerToLocalACCMap
                OCTET STRING,
           starPPPLinkStatusLocalToRemotePC
                INTEGER,
           starPPPLinkStatusRemoteToLocalPC
                INTEGER,
           starPPPLinkStatusLocalToRemoteACFC
                INTEGER,
           starPPPLinkStatusRemoteToLocalACFC
                INTEGER,
           starPPPLinkStatusTransmitFcsSize
                INTEGER,
           starPPPLinkStatusReceiveFcsSize
                INTEGER
           }

    starPPPLinkStatusPhysicalIndex   OBJECT-TYPE
           SYNTAX    INTEGER(0..2147483647)
           ACCESS    read-only
           STATUS    mandatory
           DESCRIPTION
                     "The value of ifIndex that identifies the
                     lower-level interface over which this PPP Link
                     is operating. This interface would usually be
                     an HDLC or RS-232 type of interface. If there
                     is no lower-layer interface element, or there
                     is no ifEntry for the element, or the element
                     can not be identified, then the value of this
                     object is 0.  For example, suppose that PPP is
                     operating over a serial port. This would use
                     two entries in the ifTable. The PPP could be
                     running over `interface' number 123 and the
                     serial port could be running over `interface'
                     number 987.  Therefore, ifSpecific.123 would
                     contain the OBJECT IDENTIFIER ppp
                     starPPPLinkStatusPhysicalIndex.123 would contain
                     987, and ifSpecific.987 would contain the
                     OBJECT IDENTIFIER for the serial-port's media-
                     specific MIB."
           ::= { starPPPLinkStatusEntry 1 }


    starPPPLinkStatusBadAddresses   OBJECT-TYPE
           SYNTAX    Counter
           ACCESS    read-only
           STATUS    mandatory
           DESCRIPTION
                     "The number of packets received with an
                     incorrect Address Field. This counter is a
                     component of the ifInErrors variable that is
                     associated with the interface that represents
                     this PPP Link."
           REFERENCE
                     "Section 3.1, Address Field, of RFC1331."
           ::= { starPPPLinkStatusEntry 2 }


    starPPPLinkStatusBadControls   OBJECT-TYPE
           SYNTAX    Counter
           ACCESS    read-only
           STATUS    mandatory
           DESCRIPTION
                     "The number of packets received on this link
                     with an incorrect Control Field. This counter
                     is a component of the ifInErrors variable that
                     is associated with the interface that
                     represents this PPP Link."
           REFERENCE
                     "Section 3.1, Control Field, of RFC1331."
           ::= { starPPPLinkStatusEntry 3 }


    starPPPLinkStatusPacketTooLongs   OBJECT-TYPE
           SYNTAX    Counter
           ACCESS    read-only
           STATUS    mandatory
           DESCRIPTION
                     "The number of received packets that have been
                     discarded because their length exceeded the
                     MRU. This counter is a component of the
                     ifInErrors variable that is associated with the
                     interface that represents this PPP Link. NOTE,
                     packets which are longer than the MRU but which
                     are successfully received and processed are NOT
                     included in this count."
           ::= { starPPPLinkStatusEntry 4 }


    starPPPLinkStatusBadFCSs   OBJECT-TYPE
           SYNTAX    Counter
           ACCESS    read-only
           STATUS    mandatory
           DESCRIPTION
                     "The number of received packets that have been
                     discarded due to having an incorrect FCS. This
                     counter is a component of the ifInErrors
                     variable that is associated with the interface
                     that represents this PPP Link."
           ::= { starPPPLinkStatusEntry 5 }


      starPPPLinkStatusLocalMRU   OBJECT-TYPE
           SYNTAX    INTEGER(1..2147483647)
           ACCESS    read-only
           STATUS    mandatory
           DESCRIPTION
                     "The current value of the MRU for the local PPP
                     Entity. This value is the MRU that the remote
                     entity is using when sending packets to the
                     local PPP entity. The value of this object is
                     meaningful only when the link has reached the
                     open state (ifOperStatus is up)."
           ::= { starPPPLinkStatusEntry 6 }


    starPPPLinkStatusRemoteMRU   OBJECT-TYPE
           SYNTAX    INTEGER(1..2147483647)
           ACCESS    read-only
           STATUS    mandatory
           DESCRIPTION
                     "The current value of the MRU for the remote
                     PPP Entity. This value is the MRU that the
                     local entity is using when sending packets to
                     the remote PPP entity. The value of this object
                     is meaningful only when the link has reached
                     the open state (ifOperStatus is up)."
           ::= { starPPPLinkStatusEntry 7 }

      
    starPPPLinkStatusLocalToPeerACCMap   OBJECT-TYPE
           SYNTAX    OCTET STRING (SIZE (0..4))
           ACCESS    read-only
           STATUS    mandatory
           DESCRIPTION
                     "The current value of the ACC Map used for
                     sending packets from the local PPP entity to
                     the remote PPP entity. The value of this object
                     is meaningful only when the link has reached
                     the open state (ifOperStatus is up)."
           ::= { starPPPLinkStatusEntry 8 }


    starPPPLinkStatusPeerToLocalACCMap   OBJECT-TYPE
           SYNTAX    OCTET STRING (SIZE (0..4))
           ACCESS    read-only
           STATUS    mandatory
           DESCRIPTION
                     "The ACC Map used by the remote PPP entity when
                     transmitting packets to the local PPP entity.
                     The value of this object is meaningful only
                     when the link has reached the open state
                     (ifOperStatus is up)."
           ::= { starPPPLinkStatusEntry 9 }

     
    starPPPLinkStatusLocalToRemotePC OBJECT-TYPE
           SYNTAX    INTEGER {
                     enabled(1),
                     disabled(2)
                }
           ACCESS    read-only
           STATUS    mandatory
           DESCRIPTION
                     "Indicates whether the local PPP entity will
                     use Protocol Compression when transmitting
                     packets to the remote PPP entity. The value of
                     this object is meaningful only when the link
                     has reached the open state (ifOperStatus is
                     up)."
           ::= { starPPPLinkStatusEntry 10 }

 
    starPPPLinkStatusRemoteToLocalPC OBJECT-TYPE
           SYNTAX    INTEGER {
                     enabled(1),
                     disabled(2)
                }
           ACCESS    read-only
           STATUS    mandatory
           DESCRIPTION
                     "Indicates whether the remote PPP entity will
                     use Protocol Compression when transmitting
                     packets to the local PPP entity. The value of
                     this object is meaningful only when the link
                     has reached the open state (ifOperStatus is
                     up)."
           ::= { starPPPLinkStatusEntry 11 }


    starPPPLinkStatusLocalToRemoteACFC   OBJECT-TYPE
           SYNTAX    INTEGER {
                     enabled(1),
                     disabled(2)
                }
           ACCESS    read-only
           STATUS    mandatory
           DESCRIPTION
                     "Indicates whether the local PPP entity will
                     use Address and Control Compression when
                     transmitting packets to the remote PPP entity.
                     The value of this object is meaningful only
                     when the link has reached the open state
                     (ifOperStatus is up)."
           ::= { starPPPLinkStatusEntry 12 }


    starPPPLinkStatusRemoteToLocalACFC   OBJECT-TYPE
           SYNTAX    INTEGER {
                     enabled(1),
                     disabled(2)
                }
           ACCESS    read-only
           STATUS    mandatory
           DESCRIPTION
                     "Indicates whether the remote PPP entity will
                     use Address and Control Compression when
                     transmitting packets to the local PPP entity.
                     The value of this object is meaningful only
                     when the link has reached the open state
                     (ifOperStatus is up)."
           ::= { starPPPLinkStatusEntry 13 }


    starPPPLinkStatusTransmitFcsSize   OBJECT-TYPE
           SYNTAX    INTEGER (0..128)
           ACCESS    read-only
           STATUS    mandatory
           DESCRIPTION
                     "The size of the Frame Check Sequence (FCS) in
                     bits that the local node will generate when
                     sending packets to the remote node. The value
                     of this object is meaningful only when the link
                     has reached the open state (ifOperStatus is
                     up)."
           ::= { starPPPLinkStatusEntry 14 }


    starPPPLinkStatusReceiveFcsSize   OBJECT-TYPE
           SYNTAX    INTEGER (0..128)
           ACCESS    read-only
           STATUS    mandatory
           DESCRIPTION
                     "The size of the Frame Check Sequence (FCS) in
                     bits that the remote node will generate when
                     sending packets to the local node. The value of
                     this object is meaningful only when the link
                     has reached the open state (ifOperStatus is
                     up)."
           ::= { starPPPLinkStatusEntry 15 }


    starPPPLqrTable   OBJECT-TYPE
           SYNTAX    SEQUENCE OF StarPPPLqrEntry
           ACCESS    not-accessible
           STATUS    mandatory
           DESCRIPTION
                     "Table containing the LQR parameters and
                     statistics for the local PPP entity."
           ::= { starPPP 2 }


    starPPPLqrEntry   OBJECT-TYPE
           SYNTAX    StarPPPLqrEntry
           ACCESS    not-accessible
           STATUS    mandatory
           DESCRIPTION
                     "LQR information for a particular PPP link. A
                     PPP link will have an entry in this table if
                     and only if LQR Quality Monitoring has been
                     successfully negotiated for said link."
           INDEX     { starSlotIndex, starPortIndex }
           ::= { starPPPLqrTable 1 }


    StarPPPLqrEntry ::= SEQUENCE {
           starPPPLqrQuality
                INTEGER,
           starPPPLqrInGoodOctets
                Counter,
           starPPPLqrLocalPeriod
                INTEGER,
           starPPPLqrRemotePeriod
                INTEGER,
           starPPPLqrOutLQRs
                Counter,
           starPPPLqrInLQRs
                Counter
      }

    starPPPLqrQuality   OBJECT-TYPE
           SYNTAX    INTEGER {
                     good(1),
                     bad(2),
                     not-determined(3)
                }
           ACCESS    read-only
           STATUS    mandatory
           DESCRIPTION
                     "The current quality of the link as declared by
                     the local PPP entity's Link-Quality Management
                     modules. No effort is made to define good or
                     bad, nor the policy used to determine it. The
                     not-determined value indicates that the entity
                     does not actually evaluate the link's quality.
                     This value is used to disambiguate the
                     `determined to be good' case from the `no
                     determination made and presumed to be good'
                     case."
           ::= { starPPPLqrEntry 1 }


    starPPPLqrInGoodOctets   OBJECT-TYPE
           SYNTAX    Counter
           ACCESS    read-only
           STATUS    mandatory
           DESCRIPTION
                     "The LQR InGoodOctets counter for this link."
           REFERENCE
                     "Section 2.2, Counters, of RFC1333."
           ::= { starPPPLqrEntry 2 }


    starPPPLqrLocalPeriod   OBJECT-TYPE
           SYNTAX    INTEGER(1..2147483647)
           ACCESS    read-only
           STATUS    mandatory
           DESCRIPTION
                     "The LQR reporting period, in hundredths of a
                     second that is in effect for the local PPP
                     entity."
           REFERENCE
                     "Section 2.5, Configuration Option Format, of
                     RFC1333."
           ::= { starPPPLqrEntry 3 }


    starPPPLqrRemotePeriod   OBJECT-TYPE
           SYNTAX    INTEGER(1..2147483647)
           ACCESS    read-only
           STATUS    mandatory
           DESCRIPTION
                     "The LQR reporting period, in hundredths of a
                     second, that is in effect for the remote PPP
                     entity."
           REFERENCE
                     "Section 2.5, Configuration Option Format, of
                     RFC1333."
           ::= { starPPPLqrEntry 4 }

    starPPPLqrOutLQRs   OBJECT-TYPE
           SYNTAX    Counter
           ACCESS    read-only
           STATUS    mandatory
           DESCRIPTION
                     "The value of the OutLQRs counter on the local
                     node for the link identified by ifIndex."
           REFERENCE
                     "Section 2.2, Counters, of RFC1333."
           ::= { starPPPLqrEntry 5 }


    starPPPLqrInLQRs   OBJECT-TYPE
           SYNTAX    Counter
           ACCESS    read-only
           STATUS    mandatory
           DESCRIPTION
                     "The value of the InLQRs counter on the local
                     node for the link identified by ifIndex."
           REFERENCE
                     "Section 2.2, Counters, of RFC1333."
           ::= { starPPPLqrEntry 6 }



    starPPPIpTable   OBJECT-TYPE
           SYNTAX    SEQUENCE OF StarPPPIpEntry
           ACCESS    not-accessible
           STATUS    mandatory
           DESCRIPTION
                     "Table containing the IP parameters and
                     statistics for the local PPP entity."
           ::= { starPPP 3 }


    starPPPIpEntry   OBJECT-TYPE
           SYNTAX    StarPPPIpEntry
           ACCESS    not-accessible
           STATUS    mandatory
           DESCRIPTION
                     "IPCP status information for a particular PPP
                     link."
           INDEX     { starSlotIndex, starPortIndex }
           ::= { starPPPIpTable 1 }


    StarPPPIpEntry ::= SEQUENCE {
           starPPPIpOperStatus
                INTEGER,
           starPPPIpLocalToRemoteCP
                INTEGER,
           starPPPIpRemoteToLocalCP
                INTEGER,
           starPPPIpRemoteMaxSlotId
                INTEGER,
           starPPPIpLocalMaxSlotId
                INTEGER
      }

	starPPPIpOperStatus   OBJECT-TYPE
           SYNTAX    INTEGER {opened(1), not-opened(2)}
           ACCESS    read-only
           STATUS    mandatory
           DESCRIPTION
                     "The operational status of the IP network
                     protocol. If the value of this object is up
                     then the finite state machine for the IP
                     network protocol has reached the Opened state."
           ::= { starPPPIpEntry 1 }


    starPPPIpLocalToRemoteCP   OBJECT-TYPE
           SYNTAX    INTEGER {
                     none(1),
                     vj-tcp(2)
                }
           ACCESS    read-only
           STATUS    mandatory
           DESCRIPTION
                     "The IP compression protocol that the local
                     PPP-IP entity uses when sending packets to the
                     remote PPP-IP entity. The value of this object
                     is meaningful only when the link has reached
                     the open state (starPPPIpOperStatus is opened)."
           ::= { starPPPIpEntry 2 }


    starPPPIpRemoteToLocalCP   OBJECT-TYPE
           SYNTAX    INTEGER {
                     none(1),
                     vj-tcp(2)
                }
           ACCESS    read-only
           STATUS    mandatory
           DESCRIPTION
                     "The IP compression protocol that the remote
                     PPP-IP entity uses when sending packets to the
                     local PPP-IP entity. The value of this object
                     is meaningful only when the link has reached
                     the open state (starPPPIpOperStatus is opened)."
           ::= { starPPPIpEntry 3 }

    starPPPIpRemoteMaxSlotId   OBJECT-TYPE
           SYNTAX    INTEGER(0..255)
           ACCESS    read-only
           STATUS    mandatory
           DESCRIPTION
                     "The Max-Slot-Id parameter that the remote node
                     has advertised and that is in use on the link.
                     If vj-tcp header compression is not in use on
                     the link then the value of this object shall be
                     0. The value of this object is meaningful only
                     when the link has reached the open state
                     (starPPPIpOperStatus is opened)."
           ::= { starPPPIpEntry 4 }


    starPPPIpLocalMaxSlotId   OBJECT-TYPE
           SYNTAX    INTEGER(0..255)
           ACCESS    read-only
           STATUS    mandatory
           DESCRIPTION
                     "The Max-Slot-Id parameter that the local node
                     has advertised and that is in use on the link.
                     If vj-tcp header compression is not in use on
                     the link then the value of this object shall be
                     0. The value of this object is meaningful only
                     when the link has reached the open state
                     (starPPPIpOperStatus is opened)."
           ::= { starPPPIpEntry 5 }

	-----------------------------------------------------------------------
	-- Star System Vpl Statistics Table
	-----------------------------------------------------------------------
	starVplStatsTable		OBJECT-TYPE
		SYNTAX  SEQUENCE OF VplStatsEntry
		ACCESS  not-accessible
		STATUS  mandatory
		DESCRIPTION
			"This table contains statistics gathered on a specific
			port and is indexed by Slot number, Port number and
