		SEQUENCE {
			starSvcmActiveCallCnt				   INTEGER,
			starSvcmLastTxCause					 INTEGER,
			starSvcmPtoPOrgActiveConnectionCnt	  INTEGER,
			starSvcmPtoPTermActiveConnectionCnt	 INTEGER,
			starSvcmPtoMPOrgActiveConnectionCnt	 INTEGER,
			starSvcmPtoMPTermActiveConnectionCnt	INTEGER,
			starSvcmConnectionSetupFailCnt		  INTEGER,
			starSvcmTxCallProceedingMsgCnt		  INTEGER,
			starSvcmRxCallProceedingMsgCnt		  INTEGER,
			starSvcmTxConnectMsgCnt				 INTEGER,
			starSvcmRxConnectMsgCnt				 INTEGER,
			starSvcmTxSetupMsgCnt				   INTEGER,
			starSvcmRxSetupMsgCnt				   INTEGER,
			starSvcmTxReleaseMsgCnt				 INTEGER,
			starSvcmRxReleaseMsgCnt				 INTEGER,
			starSvcmTxReleaseCompleteMsgCnt		 INTEGER,
			starSvcmRxReleaseCompleteMsgCnt		 INTEGER,
			starSvcmTxRestartMsgCnt				 INTEGER,
			starSvcmRxRestartMsgCnt				 INTEGER,
			starSvcmTxRestartAckMsgCnt			  INTEGER,
			starSvcmRxRestartAckMsgCnt			  INTEGER,
			starSvcmTxStatusMsgCnt				  INTEGER,
			starSvcmRxStatusMsgCnt				  INTEGER,
			starSvcmTxStatusEnquiryMsgCnt		   INTEGER,
			starSvcmRxStatusEnquiryMsgCnt		   INTEGER
		}
 
