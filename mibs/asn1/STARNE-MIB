	-----------------------------------------------------------------------
	-- Star System Slot Table
	-----------------------------------------------------------------------
	starSlotTable		OBJECT-TYPE 
		SYNTAX  SEQUENCE OF SlotEntry
		ACCESS  not-accessible
		STATUS  mandatory
		DESCRIPTION
			"This table contains information on each Star board in
			 a specific Node"
		::= { starSlot 1 }

	starSlotEntry	OBJECT-TYPE
		SYNTAX	SlotEntry
		ACCESS	not-accessible
		STATUS	mandatory
		DESCRIPTION
			""
		INDEX	{ starSlotIndex }
		::= { starSlotTable 1 }

	SlotEntry ::=
		SEQUENCE {
			starSlotIndex					INTEGER,
			starSlotModuleType				INTEGER,
			starSlotInterfaceType			INTEGER,
			starSlotModuleDescr				DisplayString,
			starSlotMaxPortNo				INTEGER,
			starSlotModuleCpuUtilization	INTEGER,
			starSlotModuleHwVersion			INTEGER,
			starSlotModuleSwVersion			DisplayString,
			starSlotLastInstalledTime		TimeTicks,
			starSlotSSBStatus				INTEGER,
			starSlotCellPathStatus			INTEGER,
			starSlotLineClkSource			INTEGER,
			starSlotVpiVciRange				INTEGER
		}

	starSlotIndex	OBJECT-TYPE
		SYNTAX	INTEGER (1..14)
		ACCESS	read-only
		STATUS	mandatory
		DESCRIPTION
			"Index into this table"
		::= { starSlotEntry 1 }

	starSlotModuleType	OBJECT-TYPE
		SYNTAX	INTEGER
		ACCESS	read-only
		STATUS	mandatory
		DESCRIPTION
			"Specifies type of board in this slot, AIU, 
			 PIU, etc"
		::= { starSlotEntry 2 }

	starSlotInterfaceType	OBJECT-TYPE
		SYNTAX	INTEGER
		ACCESS	read-only
		STATUS	mandatory
		DESCRIPTION
			"Specifies interface of board in this slot, T1, 
			 E3, etc"
		::= { starSlotEntry 3 }

	starSlotModuleDescr	OBJECT-TYPE
		SYNTAX	DisplayString
		ACCESS	read-only
		STATUS	mandatory
		DESCRIPTION
			"Character description of the board in this slot"
		::= { starSlotEntry 4 }

	starSlotMaxPortNo	OBJECT-TYPE
		SYNTAX	INTEGER
		ACCESS	read-only
		STATUS	mandatory
		DESCRIPTION
			"Maximum number of I/O ports in this slot"
		::= { starSlotEntry 5 }

	starSlotModuleCpuUtilization	OBJECT-TYPE
		SYNTAX	INTEGER (0..100)
		ACCESS	read-only
		STATUS	mandatory
		DESCRIPTION
			"Module CPU utilization in percentage * 100 units"
		::= { starSlotEntry 6 }

	starSlotModuleHwVersion	OBJECT-TYPE
		SYNTAX	INTEGER
		ACCESS	read-only
		STATUS	mandatory
		DESCRIPTION
			"Hardware version number of the hardware installed on
			 the board in this slot"
		::= { starSlotEntry 7 }

	starSlotModuleSwVersion	OBJECT-TYPE
		SYNTAX	DisplayString
		ACCESS	read-only
		STATUS	mandatory
		DESCRIPTION
			"Display version of the software running on
			 the board in this slot"
		::= { starSlotEntry 8 }

	starSlotLastInstalledTime	OBJECT-TYPE
		SYNTAX	TimeTicks
		ACCESS	read-only
		STATUS	mandatory
		DESCRIPTION
			"Recently installed sysUpTime of module in this slot"
		::= { starSlotEntry 9 }

	starSlotSSBStatus	OBJECT-TYPE
		SYNTAX	INTEGER
		ACCESS	read-only
		STATUS	mandatory
		DESCRIPTION
