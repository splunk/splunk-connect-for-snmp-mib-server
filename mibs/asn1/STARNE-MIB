	-----------------------------------------------------------------------
	-- Star System SVC Route Table
	-----------------------------------------------------------------------
	starSvcRouteTable   OBJECT-TYPE
		SYNTAX  SEQUENCE OF SvcRouteEntry
		ACCESS  not-accessible
		STATUS  mandatory
		DESCRIPTION
			""
		::= { starConn 14 }
 
	starSvcRouteEntry   OBJECT-TYPE
		SYNTAX  SvcRouteEntry
		ACCESS  not-accessible
		STATUS  mandatory
		DESCRIPTION
			""
		INDEX   { starSvcRouteGCIDSlotId, starSvcRouteGCIDPortId,
				  starSvcRouteGCIDIfType, starSvcRouteGCIDConnId,
				  starSvcRouteGCIDLeafId, starSvcRouteHopIndex }
		::= { starSvcRouteTable 1 }
 
	SvcRouteEntry ::=
		SEQUENCE {
			starSvcRouteGCIDSlotId			INTEGER,
			starSvcRouteGCIDPortId			INTEGER,
			starSvcRouteGCIDIfType			INTEGER,
			starSvcRouteGCIDConnId			INTEGER,
			starSvcRouteGCIDLeafId			INTEGER,
			starSvcRouteHopIndex			INTEGER,
			starSvcRouteOutgoingNeId		INTEGER,
			starSvcRouteOutgoingNodeType	INTEGER,
			starSvcRouteOutgoingModuleId	INTEGER,
			starSvcRouteOutgoingPortId		INTEGER
		}
 
	starSvcRouteGCIDSlotId  OBJECT-TYPE
		SYNTAX  INTEGER
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"slot # of GCID for svcm"
		::= { starSvcRouteEntry 1 }
 
	starSvcRouteGCIDPortId  OBJECT-TYPE
		SYNTAX  INTEGER
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"port # of GCID for svcm"
		::= { starSvcRouteEntry 2 }
 
	starSvcRouteGCIDIfType  OBJECT-TYPE
		SYNTAX  INTEGER
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"interface type of GCID for svcm"
		::= { starSvcRouteEntry 3 }
 
	starSvcRouteGCIDConnId  OBJECT-TYPE
		SYNTAX  INTEGER
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"connection # of GCID for svcm
			 VPI = 16bits & VCI = 16bits for ATM,
			 DLCI = 32bits for FRAME RELAY or,
			 TIMESLOT = 32bits for PDH"
		::= { starSvcRouteEntry 4 }
 
	starSvcRouteGCIDLeafId  OBJECT-TYPE
		SYNTAX  INTEGER
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			""
		::= { starSvcRouteEntry 5 }
 
	starSvcRouteHopIndex	OBJECT-TYPE
		SYNTAX  INTEGER
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"Sequential index into this table, 0=First Hop,
			 1=Next Hop, etc."
		::= { starSvcRouteEntry 6 }
 
	starSvcRouteOutgoingNeId	OBJECT-TYPE
		SYNTAX  INTEGER
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"Current Star Node Id"
		::= { starSvcRouteEntry 7 }
 
	starSvcRouteOutgoingNodeType	OBJECT-TYPE
		SYNTAX  INTEGER {
			source(1),
			transit(2),
			dest(3)
		}
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"Identifier of Star Node in route path"
		::= { starSvcRouteEntry 8 }
 
	starSvcRouteOutgoingModuleId	OBJECT-TYPE
		SYNTAX  INTEGER
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"Slot number of outgoing board for this route"
		::= { starSvcRouteEntry 9 }
 
	starSvcRouteOutgoingPortId  OBJECT-TYPE
		SYNTAX  INTEGER
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"Port number of outgoing port for this route"
		::= { starSvcRouteEntry 10 }
 
	-----------------------------------------------------------------------
	-- Star System PVC Route Table for Route Query (Added on 3/27/98)
	-----------------------------------------------------------------------
	starPVCRouteTable   OBJECT-TYPE
		SYNTAX  SEQUENCE OF PVCRouteEntry
		ACCESS  not-accessible
		STATUS  mandatory
		DESCRIPTION
			"Route table for route query by BBCP"
		::= { starConn 15 }

	starPVCRouteEntry   OBJECT-TYPE
		SYNTAX  PVCRouteEntry
		ACCESS  not-accessible
		STATUS  mandatory
		DESCRIPTION
			""
		INDEX   { starSlotIndex, starPortIndex, starPvcRouteAdjacentNodeType,
				  starPvcRouteSrcGCIDNeId,   starPvcRouteSrcGCIDSlotId,
				  starPvcRouteSrcGCIDPortId, starPvcRouteSrcGCIDIfType, 
				  starPvcRouteSrcGCIDConnId, starPvcRouteSrcGCIDLeafId }
		::= { starPVCRouteTable 1 }
 
	PVCRouteEntry ::=
		SEQUENCE {
			starPvcRouteSrcGCIDNeId			INTEGER,
			starPvcRouteSrcGCIDSlotId		INTEGER,
			starPvcRouteSrcGCIDPortId		INTEGER,
			starPvcRouteSrcGCIDIfType		INTEGER,
			starPvcRouteSrcGCIDConnId		INTEGER,
			starPvcRouteSrcGCIDLeafId		INTEGER,
			starPvcRouteAdjacentNeId		INTEGER,
			starPvcRouteAdjacentSlotId		INTEGER,
			starPvcRouteAdjacentPortId		INTEGER,
			starPvcRouteAdjacentNodeType	INTEGER,
			starPvcRouteLocalNodeType		INTEGER,
			starPvcRouteLocalIfType			INTEGER,
			starPvcRouteLocalConnId			INTEGER,
			starPvcRouteLocalLeafId			INTEGER,
			starPvcRouteQueryDescr			OCTET STRING
		}

	starPvcRouteSrcGCIDNeId  OBJECT-TYPE
		SYNTAX  INTEGER
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"NE # of GCID for route query (BBCP)"
		::= { starPVCRouteEntry 1 }

	starPvcRouteSrcGCIDSlotId  OBJECT-TYPE
		SYNTAX  INTEGER
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"slot # of GCID for route query (BBCP)"
		::= { starPVCRouteEntry 2 }
 
	starPvcRouteSrcGCIDPortId  OBJECT-TYPE
		SYNTAX  INTEGER
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"port # of GCID for route query (BBCP)"
		::= { starPVCRouteEntry 3 }
 
	starPvcRouteSrcGCIDIfType  OBJECT-TYPE
		SYNTAX  INTEGER
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"interface type of GCID for route query (BBCP)"
		::= { starPVCRouteEntry 4 }
 
	starPvcRouteSrcGCIDConnId  OBJECT-TYPE
		SYNTAX  INTEGER
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"connection # of GCID for route query (BBCP)
			 VPI = 16bits & VCI = 16bits for ATM,
			 DLCI = 32bits for FRAME RELAY or,
			 TIMESLOT = 32bits for PDH"
		::= { starPVCRouteEntry 5 }
 
	starPvcRouteSrcGCIDLeafId  OBJECT-TYPE
		SYNTAX  INTEGER
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			""
		::= { starPVCRouteEntry 6 }
 
	starPvcRouteAdjacentNeId	OBJECT-TYPE
		SYNTAX  INTEGER
		ACCESS  read-only
