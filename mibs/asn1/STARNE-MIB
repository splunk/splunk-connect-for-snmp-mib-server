	-----------------------------------------------------------------------
	-- Star System Port DS3 Uni Table 
	-----------------------------------------------------------------------

	starPortDs3UniTable	OBJECT-TYPE
		SYNTAX	SEQUENCE OF PortDs3UniEntry
		ACCESS	not-accessible
		STATUS	mandatory
		DESCRIPTION
			"This Table contains information read from the
			 DS3 Uni Device and is indexed by dsx3CurrentIndex
			 ans is referenced RFC 1407 & 1695"
		::= { starPort 6 }

	starPortDs3UniEntry OBJECT-TYPE
		SYNTAX  PortDs3UniEntry
		ACCESS  not-accessible
		STATUS  mandatory
		DESCRIPTION
			""
		INDEX   { starDsx3CurrentIndex }
		::= { starPortDs3UniTable 1 }
 
	PortDs3UniEntry ::=
		SEQUENCE {
			starDsx3CurrentIndex	INTEGER,
			starPortDs3TxFEBEcount  Gauge,
			starPortDs3RxFEBEcount  Gauge,
			starPortPlcpTxFEBEcount Gauge,
			starPortPlcpRxFEBEcount Gauge,
			starPortRAIstatus	   INTEGER,
			starPortPlcpCVcount	 Gauge,
			starPortPlcpEScount	 Gauge,
			starPortPlcpSEScount	Gauge,
			starPortPlcpEFScount	Gauge,
			starPortPlcpSEFScount   Gauge,
			starPortPlcpUAScount	Gauge,
			starPortDs3LSEScount	Gauge,
			starPortDs3AISScount	Gauge,
			starPortDs3EFScount	 Gauge
		}

	starDsx3CurrentIndex OBJECT-TYPE
		SYNTAX  INTEGER (1..65535)
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"The index value which uniquely identifies the
			DS3/E3 interface to which this entry is
			applicable.  The interface identified by a
			particular value of this index is the same
			interface as identified by the same value an
			dsx3LineIndex object instance."
		::= { starPortDs3UniEntry 1 }
 
	starPortDs3TxFEBEcount  OBJECT-TYPE
		SYNTAX  Gauge
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"The counter associated with the number of
			non-111 FEBE code when F,M,Cp bit error occur
			in transfer mode. Ref: RFC1407"
		::= { starPortDs3UniEntry 2 }
 
	starPortDs3RxFEBEcount  OBJECT-TYPE
		SYNTAX  Gauge
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"The counter associated with the number of
			non-111 FEBE code when F,M,Cp bit error occur
			in receive mode. Ref: RFC1407"
		::= { starPortDs3UniEntry 3 }
 
	starPortPlcpTxFEBEcount OBJECT-TYPE
		SYNTAX  Gauge
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"The counter associated with the number of
			BI/BIP-8 error when transfer mode. Ref: RFC1407"
		::= { starPortDs3UniEntry 4 }

	starPortPlcpRxFEBEcount OBJECT-TYPE
		SYNTAX  Gauge
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"The counter associated with the number of
			FEBE count when receive mode. Ref: RFC1407"
		::= { starPortDs3UniEntry 5 }

	starPortRAIstatus	   OBJECT-TYPE
		SYNTAX  INTEGER
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"The status of RAI bit in GI byte. Ref: RFC1407"
		::= { starPortDs3UniEntry 6 }

	starPortPlcpCVcount	 OBJECT-TYPE
		SYNTAX  Gauge
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"The counter associated with the number of
			BI/BIP-8 error. Ref: RFC1407"
		::= { starPortDs3UniEntry 7 }

	starPortPlcpEScount	 OBJECT-TYPE
		SYNTAX  Gauge
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"The counter associated with the count second of
			more than single CV. Ref: RFC1407"
		::= { starPortDs3UniEntry 8 }
 
	starPortPlcpSEScount	OBJECT-TYPE
		SYNTAX  Gauge
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"The counter associated with the count second of
			more than 15 CVs. Ref: RFC1407"
		::= { starPortDs3UniEntry 9 }
 
	starPortPlcpEFScount	OBJECT-TYPE
		SYNTAX  Gauge
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"The counter associated with the count second of
			more than single PLCP FERR. Ref: RFC1407"
		::= { starPortDs3UniEntry 10 }
 
	starPortPlcpSEFScount   OBJECT-TYPE
		SYNTAX  Gauge
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"The counter associated with the count second of
			more than single SEF. Ref: RFC1407"
		::= { starPortDs3UniEntry 11 }

	starPortPlcpUAScount	OBJECT-TYPE
		SYNTAX  Gauge
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"The counter associated with the count second of
			DS3 LOS, AIS, LOF or PLCP LOF. Ref: RFC1407"
		::= { starPortDs3UniEntry 12 }
 
	starPortDs3LSEScount	OBJECT-TYPE
		SYNTAX  Gauge
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"The counter associated with the count second of
			at least 45 LCVs. Ref: RFC1407"
		::= { starPortDs3UniEntry 13 }
 
	starPortDs3AISScount		OBJECT-TYPE
		SYNTAX  Gauge
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"The counter associated with the count second of
			AIS detection. Ref: RFC1407"
		::= { starPortDs3UniEntry 14 }
 
	starPortDs3EFScount	 OBJECT-TYPE
		SYNTAX  Gauge
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"The counter associated with the count second of
			any errored frame bit. Ref: RFC1407"
		::= { starPortDs3UniEntry 15 }

