	-----------------------------------------------------------------------
	-- Star System PVCM Status Table
	-----------------------------------------------------------------------
	starPvcmStatusTable	OBJECT-TYPE
		SYNTAX  SEQUENCE OF PvcmEntry
		ACCESS  not-accessible
		STATUS  mandatory
		DESCRIPTION
			"" 
		::= { starSlot 14 }

	starPvcmEntry	OBJECT-TYPE
		SYNTAX	PvcmEntry
		ACCESS	not-accessible
		STATUS	mandatory
		DESCRIPTION
			""
		INDEX	{ starSlotIndex, starPortIndex }
		::= { starPvcmStatusTable 1 }

	PvcmEntry ::=
		SEQUENCE {
			starPvcmPtoPOrgConnectionNumber			INTEGER,
			starPvcmPtoPTermConnectionNumber		INTEGER,
			starPvcmPtoMOrgConnectionNumber			INTEGER,
			starPvcmPtoMTermConnectionNumber		INTEGER,
			starPvcmOutOfServiceConnectionNumber	INTEGER,
			starPvcmOptimizedConnectionNumber		INTEGER,
			starPvcmRouteFailCt						INTEGER,
			starPvcmConnectReqFailCt				INTEGER,
			starPvcmOrgConnectedFailCt				INTEGER,
			starPvcmTermConnectedFailCt				INTEGER,
			starPvcmActiveConnectionNumber			INTEGER,
			starPvcmDeletedConnectionNumber			INTEGER,
			starPvcmAddedConnectionNumber			INTEGER,
			starPvcmFailedConnectionNumber			INTEGER,
			starPvcmChangedConnectionNumber			INTEGER,
			starPvcmOrgHoldingConnectionNumber		INTEGER,
			starPvcmTermHoldingConnectionNumber		INTEGER,
			starPvcmInvalidMsgNumber				INTEGER,
			starPvcmTimerExpiredCnt					INTEGER
		}

	starPvcmPtoPOrgConnectionNumber		OBJECT-TYPE
		SYNTAX	INTEGER
		ACCESS	read-only
		STATUS	mandatory
		DESCRIPTION
			"Number of point to point connections that originate
			 on this port"
		::= { starPvcmEntry 1 }

	starPvcmPtoPTermConnectionNumber	OBJECT-TYPE
		SYNTAX	INTEGER
		ACCESS	read-only
		STATUS	mandatory
		DESCRIPTION
			"Number of point to point connections that terminate
			 on this port"
		::= { starPvcmEntry 2 }

	starPvcmPtoMOrgConnectionNumber		OBJECT-TYPE
		SYNTAX	INTEGER
		ACCESS	read-only
		STATUS	mandatory
		DESCRIPTION
			"Number of point to multi-point connections that 
		 	 originate on this port"
		::= { starPvcmEntry 3 }

	starPvcmPtoMTermConnectionNumber		OBJECT-TYPE
		SYNTAX	INTEGER
		ACCESS	read-only
		STATUS	mandatory
		DESCRIPTION
			"Number of point to multi-point connections that 
		 	 terminate on this port"
		::= { starPvcmEntry 4 }

	starPvcmOutOfServiceConnectionNumber		OBJECT-TYPE
		SYNTAX	INTEGER
		ACCESS	read-only
		STATUS	mandatory
		DESCRIPTION
			"Number of connections that have been set out of service
			 by the NMS"
		::= { starPvcmEntry 5 }

