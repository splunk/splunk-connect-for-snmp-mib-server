	-----------------------------------------------------------------------
	-- Star System Loopback Control Table
	-----------------------------------------------------------------------
	starLoopbackCtrTable		OBJECT-TYPE
		SYNTAX  SEQUENCE OF LoopbackCtrEntry
		ACCESS  not-accessible
		STATUS  mandatory
		DESCRIPTION
			"This table contains objects that allow an NMS
			 operator to setup and start a loopback test" 
		::= { starConn 5 }

	starLoopbackCtrEntry	OBJECT-TYPE
		SYNTAX	LoopbackCtrEntry
		ACCESS	not-accessible
		STATUS	mandatory
		DESCRIPTION
			""
		INDEX	{ starSlotIndex }
		::= { starLoopbackCtrTable 1 }

	LoopbackCtrEntry ::=
		SEQUENCE {
			starLoopbackCtrPortIndex			INTEGER,
			starLoopbackCtrVpi					INTEGER,
			starLoopbackCtrVci					INTEGER,
			starLoopbackCtrDeviceId				INTEGER,
			starLoopbackCtrType					INTEGER,
			starLoopbackCtrEndType				INTEGER,
			starLoopbackCtrTxCellNo				INTEGER,
			starLoopbackCtrTimetoWait			INTEGER,
			starLoopbackCtrSetup				INTEGER,
			starLoopbackCtrMethod				INTEGER,
			starLoopbackCtrGeneration			INTEGER,
			starLoopbackCtrInputType			INTEGER,
			starLoopbackCtrPdhNum				INTEGER,
			starLoopbackCtrCellSpeed			INTEGER,
			starLoopbackCtrGCIDNeId				INTEGER,
			starLoopbackCtrGCIDSlotIndex		INTEGER,
			starLoopbackCtrGCIDPortIndex		INTEGER,
			starLoopbackCtrGCIDModuleIfType		INTEGER,
			starLoopbackCtrGCIDVpi				INTEGER,
			starLoopbackCtrGCIDVci				INTEGER,
			starLoopbackCtrGCIDDlci				INTEGER,
			starLoopbackCtrGCIDTimeSlot			INTEGER,
			starLoopbackCtrGCIDLeafNum			INTEGER,
			starLoopbackCtrAssignedSessionNo	INTEGER,
			starLoopbackCtrArea					INTEGER,
			starLoopbackCtrChannelType			INTEGER,
			starLoopbackCtrLastSetupResult		INTEGER,
			starLoopbackCtrPortNum				INTEGER,
			starLoopbackCtrGenCellType			INTEGER
		}

	starLoopbackCtrPortIndex	OBJECT-TYPE
		SYNTAX	INTEGER
		ACCESS	read-write
		STATUS	mandatory
		DESCRIPTION
			"Port number for this loopback request"
		::= { starLoopbackCtrEntry 1 }

	starLoopbackCtrVpi	OBJECT-TYPE
		SYNTAX	INTEGER
		ACCESS	read-write
		STATUS	mandatory
		DESCRIPTION
			"VPI for this loopback request"
		::= { starLoopbackCtrEntry 2 }

	starLoopbackCtrVci	OBJECT-TYPE
		SYNTAX	INTEGER
		ACCESS	read-write
		STATUS	mandatory
		DESCRIPTION
			"VCI for this loopback request"
		::= { starLoopbackCtrEntry 3 }

	starLoopbackCtrDeviceId	OBJECT-TYPE
		SYNTAX	INTEGER
		ACCESS	read-write
		STATUS	mandatory
		DESCRIPTION
			"Device Id for the specified chip type"
		::= { starLoopbackCtrEntry 4 }

	starLoopbackCtrType	OBJECT-TYPE
		SYNTAX	INTEGER {
			oam-f4(1),
			oam-f5(2),
			phyline(3),
			physsu(4)
		}
		ACCESS	read-write
		STATUS	mandatory
		DESCRIPTION
			"Type of loopback, OAM loopback F4, F5 or physical
			 level loopback"
		::= { starLoopbackCtrEntry 5 }

	starLoopbackCtrEndType	OBJECT-TYPE
		SYNTAX	INTEGER {
			segment(1),
			connection-end(2),
			intermediate(3)
		}
		ACCESS	read-write
		STATUS	mandatory
		DESCRIPTION
			"Loopback end type"
		::= { starLoopbackCtrEntry 6 }

	starLoopbackCtrTxCellNo	OBJECT-TYPE
		SYNTAX	INTEGER 
		ACCESS	read-write
		STATUS	mandatory
		DESCRIPTION
			"Cell numbers to send request for loopback test"
		::= { starLoopbackCtrEntry 7 }

	starLoopbackCtrTimetoWait	OBJECT-TYPE
		SYNTAX	INTEGER 
		ACCESS	read-write
		STATUS	mandatory
		DESCRIPTION
			"Interval in seconds to wait for the loopbacked cell
			 to arrive after sending"
		::= { starLoopbackCtrEntry 8 }

	starLoopbackCtrSetup	OBJECT-TYPE
		SYNTAX	INTEGER { 
			initrcvphy(1),
			initrcvatm(2),
			initsender(3),
			finishreceiver(4),
			finishsender(5)
		}
		ACCESS	read-write
		STATUS	mandatory
		DESCRIPTION
			"Setup request for specified command.  All parameters
			 related to each command should be preset before 
			 initiating this action"
		::= { starLoopbackCtrEntry 9 }

	starLoopbackCtrMethod 			OBJECT-TYPE
		SYNTAX	INTEGER 
		ACCESS	read-write
		STATUS	mandatory
		DESCRIPTION
			"Defines how cell is to be loopbacked.  This field is
			 only used for the receiver of the loopback"
		::= { starLoopbackCtrEntry 10 }

	starLoopbackCtrGeneration 		OBJECT-TYPE
		SYNTAX	INTEGER 
		ACCESS	read-write
		STATUS	mandatory
		DESCRIPTION
			"Defines how loopback cell is generated.  This field
			 is only used by the sender of the loopback"
		::= { starLoopbackCtrEntry 11 }

	starLoopbackCtrInputType 		OBJECT-TYPE
