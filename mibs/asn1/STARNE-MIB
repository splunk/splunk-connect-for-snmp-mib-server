		SYNTAX  INTEGER{
			cellPresent (1),
			cellNotPresent (2)
		}
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"Maximum per priority queue depth."
		::= { starRoutPrioQInfoEntry  8 }

	------------------------------------------------------------------------
	-- Star System Route CCB Info Table
	------------------------------------------------------------------------
	starRoutCcbInfoTbl  OBJECT-TYPE
		SYNTAX  SEQUENCE OF  StarRoutCcbInfoEntry
		ACCESS  not-accessible
		STATUS  mandatory
		DESCRIPTION
			"Gives information about Channel control block
			 maintained for each VP, VC." 
		::= { starRout88Info 3 }

	starRoutCcbInfoEntry		OBJECT-TYPE
		SYNTAX  StarRoutCcbInfoEntry
		ACCESS  not-accessible
		STATUS  mandatory
		DESCRIPTION
			"An entry in the routCcbInfoTbl." 
		INDEX { starCcbInfoSlotId, starCcbRoutChipId, starRoutVp, starRoutVc }
		::= { starRoutCcbInfoTbl 1 }

	StarRoutCcbInfoEntry ::=
		SEQUENCE {
			starCcbInfoSlotId		INTEGER,
			starCcbRoutChipId		INTEGER,
			starRoutVp				INTEGER,
			starRoutVc				INTEGER,
			starRoutEnClpDrop		INTEGER,
			starRoutEnAal5Epd		INTEGER,
			starRoutEnEfciMark		INTEGER,
			starRoutEnExtDrop		INTEGER,
			starRoutPti7			INTEGER,
			starRoutPti6			INTEGER,
			starRoutPti5			INTEGER,
			starRoutPti4			INTEGER,
			starRoutCcbQueue		INTEGER,
			starRoutMaxPerVcQd		INTEGER,
			starRoutCongPerVcQd		INTEGER,
			starRoutCurPerVcQd		INTEGER,
			starRoutTags			OCTET STRING ,
			starRoutIsVpTranslated  INTEGER,
			starRoutIsVcTranslated  INTEGER,
			starRoutNewVp			INTEGER,
			starRoutNewVc			INTEGER,
			starRoutCellRxCnt		Counter,
			starRoutCellsDrpdCnt	Counter,
			starRoutCongCellCnt	Counter
		}

	starCcbInfoSlotId	OBJECT-TYPE
		SYNTAX  INTEGER
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"This object indicates the slot id."
		::= { starRoutCcbInfoEntry 1 }

	starCcbRoutChipId   OBJECT-TYPE
		SYNTAX  INTEGER
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"This object indicates the rout chip
			 whose info is needed."
		::= { starRoutCcbInfoEntry  2 }

	starRoutVp  OBJECT-TYPE
		SYNTAX  INTEGER
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			" The virtual path of the call."
		::= { starRoutCcbInfoEntry  3 }

	starRoutVc  OBJECT-TYPE
		SYNTAX  INTEGER
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			" The virtual channel of the call."
		::= { starRoutCcbInfoEntry  4 }

	starRoutEnClpDrop   OBJECT-TYPE
		SYNTAX  INTEGER{
			enablesClpDrop (1),
			disablesClpDrop (2)
		}
		ACCESS  read-write
		STATUS  mandatory
		DESCRIPTION
			"Indicates wheter CLP dropping when either the VC,
			 the priority, or the device is in the congested
			 state is Enable or Disabled."
		::= { starRoutCcbInfoEntry  5 }

	starRoutEnAal5Epd   OBJECT-TYPE
		SYNTAX  INTEGER{
			enablesAal5Discard (1),
			disablesAal5Discard (2)
		}
		ACCESS  read-write
		STATUS  mandatory
		DESCRIPTION
			"Indicates whether the device Enables/Disables
			AAL5 early packed discard when either VC, the
			priority, or the device is in the congested state."
		::= { starRoutCcbInfoEntry  6 }

	starRoutEnEfciMark  OBJECT-TYPE
		SYNTAX  INTEGER{
			enablesEfciMarking (1),
			disablesEfciMarking (2)
		}
		ACCESS  read-write
		STATUS  mandatory
		DESCRIPTION
			"Indicates whether the device Enables/Disables
			EFCI cell marking  when either VC, the
			priority, or the device is in the congested
			state as the cells are dequeued."
		::= { starRoutCcbInfoEntry  7 }

	starRoutEnExtDrop   OBJECT-TYPE
		SYNTAX  INTEGER{
			enablesExtDrop (1),
			disablesExtDrop (2)
		}
		ACCESS  read-write
		STATUS  mandatory
		DESCRIPTION
			"Indicates whether the device enables/disables
			dropping of cells if EXT_DROP pin is asserted."
		::= { starRoutCcbInfoEntry  8 }

	starRoutPti7		OBJECT-TYPE
		SYNTAX  INTEGER{
			putPti (1),
			passPti (2)
		}
		ACCESS  read-write
		STATUS  mandatory
		DESCRIPTION
			"Indicates whether the device puts/passes
			received cells of PTI7 into OAM receive queue."
		::= { starRoutCcbInfoEntry  9 }

	starRoutPti6		OBJECT-TYPE
		SYNTAX  INTEGER{
			putPti (1),
			passPti (2)
		}
		ACCESS  read-write
		STATUS  mandatory
		DESCRIPTION
			"Indicates whether the device puts/passes
			received cells of PTI6 into OAM receive queue."
		::= { starRoutCcbInfoEntry  10 }
 
	starRoutPti5		OBJECT-TYPE
		SYNTAX  INTEGER{
			putPti (1),
			passPti (2)
		}
		ACCESS  read-write
		STATUS  mandatory
		DESCRIPTION
			"Indicates whether the device puts/passes
			received cells of PTI5 into OAM receive queue."
		::= { starRoutCcbInfoEntry  11 }

	starRoutPti4		OBJECT-TYPE
		SYNTAX  INTEGER{
			putPti (1),
			passPti (2)
		}
		ACCESS  read-write
		STATUS  mandatory
		DESCRIPTION
			"Indicates whether the device puts/passes
			received cells of PTI4 into OAM receive queue."
		::= { starRoutCcbInfoEntry  12 }

	starRoutCcbQueue	OBJECT-TYPE
		SYNTAX  INTEGER{
			highPriorityA (1),
			multicastQueue (2),
			pbA (3),
			pbB (4),
			pbC (5),
			highPriorityB (6),
			oamReceiveQ (7)
		}
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"Indicates the queue type in which the cells of
			the given VP, VC will be placed."
		::= { starRoutCcbInfoEntry  13 }

	starRoutMaxPerVcQd  OBJECT-TYPE
		SYNTAX  INTEGER
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"This object gives the per VC upper limit to
			the depth of the queue."
		::= { starRoutCcbInfoEntry  14 }

	starRoutCongPerVcQd OBJECT-TYPE
		SYNTAX  INTEGER
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"This object indicates the per VC queue depth
			above which the enabled congestion management
			actions are taken."
		::= { starRoutCcbInfoEntry  15 }

	starRoutCurPerVcQd  OBJECT-TYPE
		SYNTAX  INTEGER
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"current per VC queue depth."
		::= { starRoutCcbInfoEntry 16 }

	starRoutTags	OBJECT-TYPE
		SYNTAX  OCTET STRING ( SIZE (0..4 ) )
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"Tag information inserted into SE_D_OUT immediately
			after the QUEUE field."
		::= { starRoutCcbInfoEntry 17 }

	starRoutIsVpTranslated	OBJECT-TYPE
		SYNTAX  INTEGER {
			translateVP (1) ,
			doNotTranslateVP (2)
		}
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"Indicates whether VP will be translated
				1 implies VP-is-translated
				2 implies VP-is-not-translated."
		::= { starRoutCcbInfoEntry 18 }

	starRoutIsVcTranslated	OBJECT-TYPE
		SYNTAX  INTEGER  {
			translateVC (1) ,
			doNotTranslateVC (2)
		}
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"Indicates whether VC will be translated
				1 implies VC-is-translated
				2 implies VC-is-not-translated."
		::= { starRoutCcbInfoEntry 19 }

	starRoutNewVp   OBJECT-TYPE
		SYNTAX  INTEGER
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"The New VPI value to which the cells
			 are translated."
		::= { starRoutCcbInfoEntry 20 }

	starRoutNewVc   OBJECT-TYPE
		SYNTAX  INTEGER
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"The New VCI value to which the cells
