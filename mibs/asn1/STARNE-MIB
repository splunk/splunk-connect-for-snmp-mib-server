	------------------------------------------------------------------------
	-- Star System Route88 Chip Subgroup
	------------------------------------------------------------------------
	starRout88Info			OBJECT IDENTIFIER ::={ starSlot 18 }

	------------------------------------------------------------------------
	-- Star System Route88 Chip Info Table
	------------------------------------------------------------------------
	starRoutInfoTbl	 OBJECT-TYPE
		SYNTAX  SEQUENCE OF StarRoutInfoEntry
		ACCESS  not-accessible
		STATUS  mandatory
		DESCRIPTION
			"Gives information about the registers in 
				the rout chip." 
		::= { starRout88Info 1 }

	starRoutInfoEntry   OBJECT-TYPE
		SYNTAX  StarRoutInfoEntry
		ACCESS  not-accessible
		STATUS  mandatory
		DESCRIPTION
			"An entry in the RoutInfoTbl."
		INDEX { starSlotIdRoutInfo, starRoutChipId }
		::= { starRoutInfoTbl 1 }

	StarRoutInfoEntry ::=
		SEQUENCE {
			starSlotIdRoutInfo			INTEGER,
			starRoutChipId				INTEGER,
			starRoutCurrentState		INTEGER,
			starRoutChipVersion			INTEGER,
			starRoutSramConfig			INTEGER,
			starRoutBckPressureDly		INTEGER,
			starRoutNumVpi				INTEGER,
			starRoutConnectToSar		INTEGER,
			starRoutWatchDogConfig		INTEGER,
			starRoutEmptyQstate			INTEGER,
			starRoutTxMarkedCellCnt		Counter,
			starRoutRxMarkedCellCnt		Counter,
			starRoutTxParityFailCnt		Counter,
			starRoutBpIgnoredCnt		Counter,
			starRoutLiveFailCnt			Counter,
			starRoutWdFailCnt			Counter,
			starRoutEmptyQCongQd		INTEGER,
			starRoutEmptyQCurQd			INTEGER,
			starRoutRatioAOrder1		INTEGER,
			starRoutRatioBOrder1		INTEGER,
			starRoutRatioAOrder2		INTEGER,
			starRoutRatioBOrder2		INTEGER
		}

	starSlotIdRoutInfo	OBJECT-TYPE
		SYNTAX  INTEGER
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"This object indicates the slot id."
		::= { starRoutInfoEntry 1 }

	starRoutChipId	OBJECT-TYPE
		SYNTAX  INTEGER
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"This object indicates the rout chip
			 whose info is needed."
		::= { starRoutInfoEntry  2 }

	starRoutCurrentState		OBJECT-TYPE
		SYNTAX  INTEGER {
			init (1) ,
			operational (2)
		}
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"This object describes the operational
			 state of rout chip."
		::= { starRoutInfoEntry 3 }

	starRoutChipVersion OBJECT-TYPE
		SYNTAX  INTEGER
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"This object indicates the version 
			number of the rout chip." 
		::= { starRoutInfoEntry 4 }

	starRoutSramConfig  OBJECT-TYPE
		SYNTAX  INTEGER {
			ram2x32x8 (1) ,
			ram2x128x8 (2) ,
			ram4x128x8 (3)
		}
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"Indicates how the SRAM is configured for the
			 rout chip: it can be two 32K * 8 SRAM,
			 two 128K * 8 SRAM or four 128 * 8 SRAM." 
		::= { starRoutInfoEntry 5 }

	starRoutBckPressureDly	OBJECT-TYPE
		SYNTAX  INTEGER  {
			noDelay (1) ,
			clk2Delay (2) ,
			clk4Delay (3)
		}
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"This object indicates whether there is no
			 back pressure delay, two SE_CLK back pressure
			 delay or four SE_CLK back pressure delay."
		::= { starRoutInfoEntry 6 }

	starRoutNumVpi	OBJECT-TYPE
		SYNTAX  INTEGER
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"This object indicates the no of VPI, VCI bits
			 that will be used in the index to the
			 channel control block." 
		::= { starRoutInfoEntry 7 }

	starRoutConnectToSar		OBJECT-TYPE
		SYNTAX  INTEGER {
			connectPHY (1) ,
			connectSAR (2)
		}
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"This object indicates whether the connection
			 is to the PHY layer or SAR layer."
		::= { starRoutInfoEntry 8 }

	starRoutWatchDogConfig	OBJECT-TYPE
		SYNTAX  INTEGER
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"This object indicates the number of cells times
			 into(*) 32 which the UTOPIA TX_FULL
			 must be deasserted at least once."
		::= { starRoutInfoEntry 9 }

	starRoutEmptyQstate		 OBJECT-TYPE
		SYNTAX  INTEGER {
			inCongestion (1),
			notInCongestion (2)
		}
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"This object indicates whether the empty queue
			 in the rout device is congested or not."
		::= { starRoutInfoEntry 10 }

	starRoutTxMarkedCellCnt	 OBJECT-TYPE
		SYNTAX  Counter
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"This object indicates the number of cells
			 modulo 16 from the switch fabric which had
			 Tag(9,1) set to 1."
		::= { starRoutInfoEntry 11 }

	starRoutRxMarkedCellCnt	 OBJECT-TYPE
		SYNTAX  Counter
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"This object indicates the number of cells
			 modulo 16 to the switch fabric which had
			 Tag(9,1) set to 1."
		::= { starRoutInfoEntry  12 }

	starRoutTxParityFailCnt	 OBJECT-TYPE
		SYNTAX  Counter
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"This object indicates the number of times
			the transmit parity was bad."
		::= { starRoutInfoEntry  13 }

	starRoutBpIgnoredCnt	 OBJECT-TYPE
		SYNTAX  Counter
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"This object indicates the number of times
			the back pressure was ignored."
		::= { starRoutInfoEntry  14 }

	starRoutLiveFailCnt	 OBJECT-TYPE
		SYNTAX  Counter
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"This object indicates the
			back pressure live fail count."
		::= { starRoutInfoEntry  15 }

	starRoutWdFailCnt	 OBJECT-TYPE
		SYNTAX  Counter
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"This object indicates the number of times
			the output watchdog timer expired."
		::= { starRoutInfoEntry  16 }

	starRoutEmptyQCongQd		OBJECT-TYPE
		SYNTAX   INTEGER
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"This object indicates the number of cell
			 buffers below which the device 
			 will enter the empty congestion state."
		::= { starRoutInfoEntry  17 }

	starRoutEmptyQCurQd		 OBJECT-TYPE
		SYNTAX  INTEGER
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"This object indicates the current empty
			 queue depth in the rout."
		::= { starRoutInfoEntry  18 }

	starRoutRatioAOrder1		 OBJECT-TYPE
		SYNTAX  INTEGER
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			" This object specifies the num of eighths
			of the priority Queue in which Queue A is 
			served first."
		::= { starRoutInfoEntry  19 }

	starRoutRatioBOrder1		 OBJECT-TYPE
		SYNTAX  INTEGER
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			" This object specifies the num of eighths
			of the priority Queue in which Queue B is 
			served first."
		::= { starRoutInfoEntry  20 }

	starRoutRatioAOrder2		 OBJECT-TYPE
		SYNTAX  INTEGER
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			" This object specifies the num of eighths
			of the priority Queue in which Queue A is 
			served second."
		::= { starRoutInfoEntry  21 }

	starRoutRatioBOrder2		 OBJECT-TYPE
		SYNTAX  INTEGER
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			" This object specifies the num of eighths
			of the priority Queue in which Queue B is 
			served second."
		::= { starRoutInfoEntry 22 }

