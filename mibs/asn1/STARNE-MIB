	-- Star System SSU Threshold Info Table
	-----------------------------------------------------------------------
	starSSUPmThresholdInfoTbl   OBJECT-TYPE
		SYNTAX  SEQUENCE OF  StarSSUPmThresholdInfoEntry
		ACCESS  not-accessible
		STATUS  mandatory
		DESCRIPTION
			"Load information about the SSU Performance manager
			 Thresholds ."
		::= { starSlotSSUInfo 7 }

	starSSUPmThresholdInfoEntry OBJECT-TYPE
		SYNTAX  StarSSUPmThresholdInfoEntry
		ACCESS  not-accessible
		STATUS  mandatory
		DESCRIPTION
			"An entry in the ssuPmThresholdInfoTbl."
		INDEX { starSSUPmThresholdSlotId }
		::= { starSSUPmThresholdInfoTbl 1 }

	StarSSUPmThresholdInfoEntry ::=
		SEQUENCE {
			starSSUPmThresholdSlotId					INTEGER,
			starSSUSwitchMcGroupThreshold		 		INTEGER,
			starSSUSwitchMcBitsPerMcgThreshold			INTEGER,
			starSSUSwitchTotalMcBitsThreshold			INTEGER,
			starSSUCrsrReqFailedThreshold				INTEGER,
			starSSUCrsrReqNumThreshold					INTEGER,
			starSSUCrsrReqFailedMcgExceededThreshold	INTEGER,
			starSSUCrsrReconcileFailThreshold			INTEGER,
			starSSUSiuLoadCapacityThreshold				INTEGER
		}

	starSSUPmThresholdSlotId  OBJECT-TYPE
		SYNTAX  INTEGER
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"This object specifies the Slot id of the SSU."
		::= { starSSUPmThresholdInfoEntry 1 }

	starSSUSwitchMcGroupThreshold  OBJECT-TYPE
		SYNTAX  INTEGER
		ACCESS  read-write
		STATUS  mandatory
		DESCRIPTION
			"This object specifies threshold for the number
			of multicast groups."
		::= { starSSUPmThresholdInfoEntry 2 }

	starSSUSwitchMcBitsPerMcgThreshold  OBJECT-TYPE
		SYNTAX  INTEGER
		ACCESS  read-write
		STATUS  mandatory
		DESCRIPTION
			"The threshold for multicast bits in a single
			multicast group"
		::= { starSSUPmThresholdInfoEntry 3 }

	starSSUSwitchTotalMcBitsThreshold	OBJECT-TYPE
		SYNTAX  INTEGER
		ACCESS  read-write
		STATUS  mandatory
		DESCRIPTION
			"This object specifies the threshold for the 
			 total num of multicast bits."
		::= { starSSUPmThresholdInfoEntry 4 }

	starSSUCrsrReqFailedThreshold	OBJECT-TYPE
		SYNTAX  INTEGER
		ACCESS  read-write
		STATUS  mandatory
		DESCRIPTION
			"This object specifies the threshold to flag a
			CRSR failure."
		::= { starSSUPmThresholdInfoEntry 5 }

	starSSUCrsrReqNumThreshold   OBJECT-TYPE
		SYNTAX  INTEGER
		ACCESS  read-write
		STATUS  mandatory
		DESCRIPTION
			"This object specifies the threshold for CRSR
			requests beyon which FM is notified."
		::= { starSSUPmThresholdInfoEntry 6 }

	starSSUCrsrReqFailedMcgExceededThreshold   OBJECT-TYPE
		SYNTAX  INTEGER
		ACCESS  read-write
		STATUS  mandatory
		DESCRIPTION
			"This object specifies the threshold at which the
			Multicast group inputs are unavailable and the CRSR 
			has failed to perform its duty."
		::= { starSSUPmThresholdInfoEntry 7 }

	starSSUCrsrReconcileFailThreshold  OBJECT-TYPE
		SYNTAX  INTEGER
		ACCESS  read-write
		STATUS  mandatory
		DESCRIPTION
			"This object specifies the threshold beyond which
			the CRSR reconcile fail is raised."
			::= { starSSUPmThresholdInfoEntry 8 }

	starSSUSiuLoadCapacityThreshold  OBJECT-TYPE
		SYNTAX  INTEGER
		ACCESS  read-write
		STATUS  mandatory
		DESCRIPTION
			"This object specifies the threshold till
			which the SSU accepts the load." 
			::= { starSSUPmThresholdInfoEntry 9 }

	-----------------------------------------------------------------------
	-- Star System Clock Group
	-----------------------------------------------------------------------
	starSysClockInfo		OBJECT IDENTIFIER ::= { starSlot 5 }

	-----------------------------------------------------------------------
	-- Star System Clock Table
	-----------------------------------------------------------------------
	starSysClockTbl	 OBJECT-TYPE
		SYNTAX  SEQUENCE OF SysClockInfoEntry
		ACCESS  not-accessible
		STATUS  mandatory
		DESCRIPTION
			"the table maintains  the clock source of the SCU
			module indexed by Slot #."
		::= { starSysClockInfo 1 }
 
	starSysClockInfoEntry	 OBJECT-TYPE
		SYNTAX  SysClockInfoEntry
		ACCESS  not-accessible
		STATUS  mandatory
		DESCRIPTION
			""
		INDEX { starScuInfoSlotId }
		::= { starSysClockTbl 1 }
 
	SysClockInfoEntry ::=
		SEQUENCE {
			starScuInfoSlotId		INTEGER ,
			starClkSource			INTEGER ,
			starClkPrevSource		INTEGER ,
			starClkSourceLine		INTEGER ,
			starClkPrevSourceLine	INTEGER ,
			starClkStatus			INTEGER ,
			starClkPLLMode			INTEGER ,
			starClkMasterStatus		INTEGER ,
			starClkPriority			INTEGER
		}

	starScuInfoSlotId	 OBJECT-TYPE
		SYNTAX  INTEGER
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"This object indicates the slot id."
		::= { starSysClockInfoEntry 1 }
 
	starClkSource   OBJECT-TYPE
		SYNTAX  INTEGER {
			init (0) ,
			bits (1),
			extline (2),
			holdover (3),
			freerun (4)
		}
		ACCESS  read-write
		STATUS  mandatory
		DESCRIPTION
			"This object indicates  the source of the clock."
		::= { starSysClockInfoEntry 2 }
 
	starClkPrevSource   OBJECT-TYPE
		SYNTAX  INTEGER {
			init (0),
			bits (1),
			extline (2),
			holdover (3),
			freerun (4)
		}
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"This object indicates  the previous source of 
			the clock."
		::= { starSysClockInfoEntry 3 }
 
	starClkSourceLine   OBJECT-TYPE
		SYNTAX  INTEGER
		ACCESS  read-write
		STATUS  mandatory
		DESCRIPTION
			"This object indicates  the SIU number 1..9
			of the current external clock source."
		::= { starSysClockInfoEntry 4 }
 
	starClkPrevSourceLine   OBJECT-TYPE
		SYNTAX  INTEGER
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"This object indicates  the SIU number 1..9
			of the previous external clock source."
		::= { starSysClockInfoEntry 5 }
 
	starClkStatus   OBJECT-TYPE
		SYNTAX  INTEGER {
			normal (1),
			fault (2)
		}
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"This object indicates  the status of the 
			current clock source. The clock source
			number must be 1-9: SIU#1-#9 for line
			clock source, or 10: for BITS"
		::= { starSysClockInfoEntry 6 }

	starClkPLLMode   OBJECT-TYPE
		SYNTAX  INTEGER {
			fast (1),
			mid (2),
			normal (3),
			holdover (4),
			freerun (5)
		}
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"This object indicates  the port number of the
			current clock source."
		::= { starSysClockInfoEntry 7 }

	starClkMasterStatus   OBJECT-TYPE
		SYNTAX  INTEGER {
			master (1),
			slave (2)
		}
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"This object indicates  the master/slave
			status of the current clock source."
		::= { starSysClockInfoEntry 8 }

	starClkPriority   OBJECT-TYPE
		SYNTAX  INTEGER
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"This object indicates  the external line
			priority of the current clock source"
		::= { starSysClockInfoEntry 9 }

