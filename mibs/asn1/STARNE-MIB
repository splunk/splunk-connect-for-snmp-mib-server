	 frlPvcEndptInfoDlci			OBJECT-TYPE
		SYNTAX  INTEGER
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"This object indicates the Dlci ."
		::= {  frlPvcEndptInfoEntry  3 }

	 frPvcEndptInfoTimeStamp	OBJECT-TYPE
		SYNTAX  INTEGER
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"This object indicates time slot ."
		::= {  frlPvcEndptInfoEntry  4 }

	frPvcMarkedDe				OBJECT-TYPE
		SYNTAX  INTEGER
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"This object indicates the num of frames received
				with the DE bit set ."
		::= {  frlPvcEndptInfoEntry  5 }

	frPvcEndptOperStatus			OBJECT-TYPE
		SYNTAX  INTEGER
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"This object indicates if the PVC is active or not."
		::= {  frlPvcEndptInfoEntry  6 }

	frPvcEndptNoRecvFECN			OBJECT-TYPE
		SYNTAX  INTEGER
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"This object indicates number of received 
			 frames with FECN bit set for this dlci."
		::= {  frlPvcEndptInfoEntry  7 }

	frPvcEndptNoTxFECN			OBJECT-TYPE
		SYNTAX  INTEGER
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"This object indicates number of transmitted 
			 frames with FECN bit set for this dlci."
		::= {  frlPvcEndptInfoEntry  8 }

	frPvcEndptNoRecvBECN			OBJECT-TYPE
		SYNTAX  INTEGER
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"This object indicates number of received 
			 frames with BECN bit set for this dlci."
		::= {  frlPvcEndptInfoEntry  9 }

	frPvcEndptNoTxBECN			OBJECT-TYPE
		SYNTAX  INTEGER
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"This object indicates number of transmitted 
			 frames with BECN bit set for this dlci."
		::= {  frlPvcEndptInfoEntry  10 }

	frPvcEndptNoTxDE			OBJECT-TYPE
		SYNTAX  INTEGER
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"This object indicates number of transmitted 
			 frames with DE bit set for this dlci."
		::= {  frlPvcEndptInfoEntry  11 }

	frPvcEndptNoDiscardWithDE			OBJECT-TYPE
		SYNTAX  INTEGER
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"This object indicates number of frames 
			 discarded with DE bit set for this dlci."
		::= {  frlPvcEndptInfoEntry  12 }

	frPvcEndptNoDiscardWoutDE			OBJECT-TYPE
		SYNTAX  INTEGER
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"This object indicates number of frames 
			 discarded with out DE bit set for this dlci."
		::= {  frlPvcEndptInfoEntry  13 }

	frPvcEndptInDEFrames			OBJECT-TYPE
		SYNTAX  INTEGER
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"From RFC1604"
		::= {  frlPvcEndptInfoEntry  14 }

	-----------------------------------------------------------------------
	-- Star FIU SSCS Info Subgroup
	-----------------------------------------------------------------------
	starFiuSscsInfo		 OBJECT IDENTIFIER
		::= { starFiuModuleInfo 2 }

	-----------------------------------------------------------------------
	-- Star FIU SSCS Interface Info Table
	-----------------------------------------------------------------------
	starFiuSscsIfInfoTbl	OBJECT-TYPE
		SYNTAX  SEQUENCE OF StarFiuSscsIfInfoEntry
		ACCESS  not-accessible
		STATUS  mandatory
		DESCRIPTION
			"This table provides information pertaining to the
			Interface of frame relay service specific convergence
			subfunction."
		::= { starFiuSscsInfo 1 }

	starFiuSscsIfInfoEntry	 OBJECT-TYPE
		SYNTAX  StarFiuSscsIfInfoEntry
		ACCESS  not-accessible
		STATUS  mandatory
		DESCRIPTION
			"An entry into FiuSscsIfInfoTbl."
		INDEX { starFiuIfInfoSlotId, starFiuIfInfoPortId }
		::= { starFiuSscsIfInfoTbl 1 }

	StarFiuSscsIfInfoEntry ::=
		SEQUENCE {
			starFiuIfInfoSlotId			INTEGER ,
			starFiuIfInfoPortId			INTEGER ,
			starFiuFrcsIfAdminStatus	INTEGER ,
			starFiuFrcsIfOperStatus		INTEGER ,
			starFiuFrcsIfLastChange		INTEGER ,
			starFiuFrcsIfInOctets		INTEGER ,
			starFiuFrcsIfInUcastPkts	INTEGER ,
			starFiuFrcsIfInDiscards		INTEGER ,
			starFiuFrcsIfInErrors		INTEGER ,
			starFiuFrcsIfOutOctets		INTEGER ,
			starFiuFrcsIfOutUcastPkts	INTEGER ,
			starFiuFrcsIfOutDiscards	INTEGER ,
			starFiuFrcsIfOutErrors		INTEGER 
		}

	starFiuIfInfoSlotId	 OBJECT-TYPE
		SYNTAX  INTEGER
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"This object indicates the slot id."
		::= { starFiuSscsIfInfoEntry 1 }

	starFiuIfInfoPortId	 OBJECT-TYPE
		SYNTAX  INTEGER
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"This object indicates the port id."
		::= { starFiuSscsIfInfoEntry 2 }

	starFiuFrcsIfAdminStatus  OBJECT-TYPE
		SYNTAX  INTEGER {
			defined (1),
			undefined (2),
			out-of-service (3)
		}
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"This object describes the administrative
			 status of Fiu card."
		::= { starFiuSscsIfInfoEntry 3 }

	starFiuFrcsIfOperStatus	 OBJECT-TYPE
		SYNTAX  INTEGER {
			up   (1),
			down (2)
		}
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
		"Indicates the operational status of the active Fiu card."
		::= { starFiuSscsIfInfoEntry 4 }

	starFiuFrcsIfLastChange	OBJECT-TYPE
		SYNTAX  INTEGER
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"Indicates the elapsed  time since 
			last oper_status change in timeticks ." 
		::= { starFiuSscsIfInfoEntry 5 }

	starFiuFrcsIfInOctets	OBJECT-TYPE
		SYNTAX  INTEGER
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"Indicates the number of received octets." 
		::= { starFiuSscsIfInfoEntry 6 }

	starFiuFrcsIfInUcastPkts	OBJECT-TYPE
		SYNTAX  INTEGER
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"Indicates the number of successfully received frames."
		::= { starFiuSscsIfInfoEntry 7 }

	starFiuFrcsIfInDiscards	OBJECT-TYPE
		SYNTAX  INTEGER 
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			" Indicates the number of 
			discarded frames due to congestion or policing."
		::= { starFiuSscsIfInfoEntry 8 }

	starFiuFrcsIfInErrors	OBJECT-TYPE
		SYNTAX  INTEGER 
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"Indicates the number of received frames discarded 
			due to the following errors - invalid DLCI."
		::= { starFiuSscsIfInfoEntry 9 }

	starFiuFrcsIfOutOctets	OBJECT-TYPE
		SYNTAX  INTEGER 
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"Indicates the number of transmitted octets."
		::= { starFiuSscsIfInfoEntry 10 }

	starFiuFrcsIfOutUcastPkts	OBJECT-TYPE
		SYNTAX  INTEGER 
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
		"Indicates the number of successfully transmitted frames."
		::= { starFiuSscsIfInfoEntry 11 }

	starFiuFrcsIfOutDiscards	OBJECT-TYPE
		SYNTAX  INTEGER 
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
		"Indicates the number of discarded frames due to congestion."
		::= { starFiuSscsIfInfoEntry 12 }

	starFiuFrcsIfOutErrors	OBJECT-TYPE
		SYNTAX  INTEGER 
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"Indicates the number of transmit frames 
			discarded due to the following errors-invalid 
			DLCI,incorrect ATM header."
		::= { starFiuSscsIfInfoEntry 13 }

	-----------------------------------------------------------------------
	-- Star FIU SSCS PVC Info Table
	-----------------------------------------------------------------------
	starFiuSscsPvcInfoTbl		 OBJECT-TYPE
		SYNTAX  SEQUENCE OF StarFiuSscsPvcInfoEntry
		ACCESS  not-accessible
		STATUS  mandatory
		DESCRIPTION
			"This table provides the information about the
			Fr-SSCS pvc, ie., on a per DLCI basis."
		::= { starFiuSscsInfo  2 }

	starFiuSscsPvcInfoEntry	 OBJECT-TYPE
		SYNTAX  StarFiuSscsPvcInfoEntry
		ACCESS  not-accessible
		STATUS  mandatory
		DESCRIPTION
			"An entry into Fiu PVC table for SSCS."
		INDEX { starFiuPvcInfoSlotId, starFiuPvcInfoPortId ,
				starFiuPvcInfoDlci }
		::= { starFiuSscsPvcInfoTbl 1 }

	StarFiuSscsPvcInfoEntry ::=
		SEQUENCE {
			starFiuPvcInfoSlotId				INTEGER, 
			starFiuPvcInfoPortId				INTEGER,
			starFiuPvcInfoDlci					INTEGER,
			starFiuFrcsPvcEndptInFrames 		INTEGER,
			starFiuFrcsPvcEndptOutFrames		INTEGER,
			starFiuFrcsPvcEndptInOctets			INTEGER,
			starFiuFrcsPvcEndptOutOctets		INTEGER,
			starFiuFrcsPvcConnectAdminStatus 	INTEGER,
			starFiuFrcsPvcEndptOperStatus		INTEGER,
			starFiuFrcsPvcEndptLastChange		INTEGER,
			starFiuFrcsPvcTxVp					INTEGER,
			starFiuFrcsPvcTxVc					INTEGER,
			starFiuFrcsPvcRxVp					INTEGER,
			starFiuFrcsPvcRxVc					INTEGER,
			starFiuFrcsPvcEndptOutOamCells		INTEGER,	
			starFiuFrcsPvcEndptInOamCells		INTEGER,	
			starFiuFrcsPvcEndptOutRawCells		INTEGER,	
			starFiuFrcsPvcEndptInRawCells		INTEGER		

		}

	starFiuPvcInfoSlotId	OBJECT-TYPE
		SYNTAX  INTEGER
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"This object indicates the slot id."
		::= { starFiuSscsPvcInfoEntry 1 }

