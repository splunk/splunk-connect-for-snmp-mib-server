		DESCRIPTION
			""
		INDEX	{ starSlotIndex, starPmCtrUpcId, starPmCtrLastAssignedSession}
		::= { starPmTable 1 }

	PmEntry ::=
		SEQUENCE {
			starPmTotalCellClp0Count		INTEGER,
			starPmTotalCellClp01Count		INTEGER,
			starPmLostCellClp0Count			INTEGER,
			starPmLostCellClp01Count		INTEGER,
			starPmErroredCellCount			INTEGER,
			starPmMisinsertedCellCount		INTEGER,
			starPmSeverelyErroredBlockCount	INTEGER
		}

	starPmTotalCellClp0Count OBJECT-TYPE
		SYNTAX	INTEGER
		ACCESS	read-only
		STATUS	mandatory
		DESCRIPTION
			"Total number of CLPO cell count for this PM Session"
		::= { starPmEntry 1 }

	starPmTotalCellClp01Count OBJECT-TYPE
		SYNTAX	INTEGER
		ACCESS	read-only
		STATUS	mandatory
		DESCRIPTION
			"Total number of CLPO1 cell count for this PM Session"
		::= { starPmEntry 2 }

	starPmLostCellClp0Count	OBJECT-TYPE
		SYNTAX	INTEGER
		ACCESS	read-only
		STATUS	mandatory
		DESCRIPTION
			"Number of CLP0 lost cells for this PM session"
		::= { starPmEntry 3 }

	starPmLostCellClp01Count	OBJECT-TYPE
		SYNTAX	INTEGER
		ACCESS	read-only
		STATUS	mandatory
		DESCRIPTION
			"Number of CLP1 lost cells for this PM session"
		::= { starPmEntry 4 }

	starPmErroredCellCount	OBJECT-TYPE
		SYNTAX	INTEGER
		ACCESS	read-only
		STATUS	mandatory
		DESCRIPTION
			"Number of errored cells for this PM session"
		::= { starPmEntry 5 }

	starPmMisinsertedCellCount	OBJECT-TYPE
		SYNTAX	INTEGER
		ACCESS	read-only
		STATUS	mandatory
		DESCRIPTION
			"Number of misinserted cells for this PM session"
		::= { starPmEntry 6 }

	starPmSeverelyErroredBlockCount	OBJECT-TYPE
		SYNTAX	INTEGER
		ACCESS	read-only
		STATUS	mandatory
		DESCRIPTION
			"Number of severely errored blocks for this PM session"
		::= { starPmEntry 7 }

