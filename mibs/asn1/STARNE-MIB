	-- Star System Backbone Link Table (Added on 5/6/96)
	-----------------------------------------------------------------------
	starBackboneLinkTable	OBJECT-TYPE
		SYNTAX  SEQUENCE OF BackboneLinkEntry
		ACCESS  not-accessible
		STATUS  mandatory
		DESCRIPTION
			"This table contains information for BackBone links and
			 is indexed by a Backbone Link Id" 
		::= { starBackbone 1 }

	starBackboneLinkEntry	OBJECT-TYPE
		SYNTAX	BackboneLinkEntry
		ACCESS	not-accessible
		STATUS	mandatory
		DESCRIPTION
			""
		INDEX	{ starSlotIndex, starPortIndex }
		::= { starBackboneLinkTable 1 }

	BackboneLinkEntry ::=
		SEQUENCE {
			starBackboneLinkStatus		INTEGER,
			starBackboneLinkDestNodeId	INTEGER,
			starBackboneLinkDestSlotId	INTEGER,
			starBackboneLinkDestPortId	INTEGER,
			starBackboneLinkIfType		INTEGER,
			starBackboneLinkConnStatus	INTEGER
		}

	starBackboneLinkStatus	OBJECT-TYPE
		SYNTAX	INTEGER
		ACCESS	read-only
		STATUS	mandatory
		DESCRIPTION
			"Status of this Backbone Link"
		::= { starBackboneLinkEntry 1 }

	starBackboneLinkDestNodeId	OBJECT-TYPE
		SYNTAX	INTEGER
		ACCESS	read-only
		STATUS	mandatory
		DESCRIPTION
			"Destination Node for this Backbone Link"
		::= { starBackboneLinkEntry 2 }

	starBackboneLinkDestSlotId	OBJECT-TYPE
		SYNTAX	INTEGER
		ACCESS	read-only
		STATUS	mandatory
		DESCRIPTION
			"Destination Slot number of this Backbone Link Link"
		::= { starBackboneLinkEntry 3 }

	starBackboneLinkDestPortId	OBJECT-TYPE
		SYNTAX	INTEGER
		ACCESS	read-only
		STATUS	mandatory
		DESCRIPTION
			"Destination Port number of this Backbone Link Link"
		::= { starBackboneLinkEntry 4 }

	starBackboneLinkIfType	OBJECT-TYPE
		SYNTAX	INTEGER
		ACCESS	read-only
		STATUS	mandatory
		DESCRIPTION
			"Interface port type of this Backbone Link Link"
		::= { starBackboneLinkEntry 5 }

 	starBackboneLinkConnStatus  OBJECT-TYPE
 		SYNTAX  INTEGER
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"Status of this Backbone Link Connection for Warning"
		::= { starBackboneLinkEntry 6 }

