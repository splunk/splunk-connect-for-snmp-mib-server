	-----------------------------------------------------------------------
	-- Star Port DS1ATM CDB chip Table
	-----------------------------------------------------------------------
	starPortDS1ATMTable	OBJECT-TYPE
		SYNTAX SEQUENCE OF PortDS1ATMEntry
		ACCESS not-accessible
		STATUS mandatory
		DESCRIPTION
			"This table contains DS1 ATM chip  peformance data 
			 read from ATM chip of AIU T1, AIU E1 interface"
		::= { starPort 9 }

	starPortDS1ATMEntry	OBJECT-TYPE
		SYNTAX  PortDS1ATMEntry
		ACCESS  not-accessible
		STATUS  mandatory
		DESCRIPTION  "	"
		INDEX { starSlotIndex, starPortDS1ATMIndex }
		::= { starPortDS1ATMTable 1 }

	PortDS1ATMEntry ::=
		SEQUENCE {
			starPortDS1ATMIndex					INTEGER,
			starPortDS1ATMHecCnt				INTEGER,
			starPortDS1ATMDiscardCellCnt		INTEGER,
			starPortDS1ATMErrCorrectCellCnt		INTEGER,
			starPortDS1ATMRxBusyCellCnt			INTEGER,
			starPortDS1ATMTxBusyCellCnt			INTEGER
		}

	starPortDS1ATMIndex   OBJECT-TYPE
		SYNTAX	INTEGER
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION  "The chip number for CDB chips. Each interface
				 has one CDB chip."
		::= { starPortDS1ATMEntry 1 }

	starPortDS1ATMHecCnt   OBJECT-TYPE
		SYNTAX	INTEGER
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
				 "Header Error Check(HEC) counter is incremented
				when thereis an error in the HEC value indicate
				either single or multiple errors in ATM cell
				heaer"
		::= { starPortDS1ATMEntry 2 }

	starPortDS1ATMDiscardCellCnt  OBJECT-TYPE
		SYNTAX	INTEGER
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION  " This counter is incremented when the 3-cell
			output FIFO of the CDB is forced to overflow."
		::= { starPortDS1ATMEntry 3 }

	starPortDS1ATMErrCorrectCellCnt  OBJECT-TYPE
		SYNTAX	INTEGER
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION  "This counter is incremented for every ATM cell 
			header this is corrected for a singel bit error"
		::= { starPortDS1ATMEntry 4 }

	starPortDS1ATMRxBusyCellCnt OBJECT-TYPE
		SYNTAX	INTEGER
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION  "The counter is increment for each complete
			received busy cell."
		::= { starPortDS1ATMEntry 5 }

	starPortDS1ATMTxBusyCellCnt  OBJECT-TYPE
		SYNTAX	INTEGER
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION  "The counter is inceremented for each complete
			 tranmitted busy cell."
		::= { starPortDS1ATMEntry 6 }

	-----------------------------------------------------------------------
	-- Star Port Munich32 chip Table
	-----------------------------------------------------------------------
	starPortM32Table 	OBJECT-TYPE
		SYNTAX	SEQUENCE OF PortM32Entry
		ACCESS	not-accessible
		STATUS	mandatory
		DESCRIPTION "This table contains the performance monitoring
			data for Siemens Munich32 chip (Multichannel
			Network Interface Controlloer for HDLC) for channelized 
			FIU board."
