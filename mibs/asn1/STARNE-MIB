		SYNTAX	INTEGER
		ACCESS	read-only
		STATUS	mandatory
		DESCRIPTION
			"Number of connections that failed after being setup
			 on the terminating module"
		::= { starPvcmEntry 10 }

	starPvcmActiveConnectionNumber		OBJECT-TYPE
		SYNTAX	INTEGER
		ACCESS	read-only
		STATUS	mandatory
		DESCRIPTION
			"Number of connections currently being processed"
		::= { starPvcmEntry 11 }

	starPvcmDeletedConnectionNumber		OBJECT-TYPE
		SYNTAX	INTEGER
		ACCESS	read-only
		STATUS	mandatory
		DESCRIPTION
			"Number of connections currently being queued for
			 deletion"
		::= { starPvcmEntry 12 }

	starPvcmAddedConnectionNumber		OBJECT-TYPE
		SYNTAX	INTEGER
		ACCESS	read-only
		STATUS	mandatory
		DESCRIPTION
			"Number of connections currently being queued for
			 addition"
		::= { starPvcmEntry 13 }

	starPvcmFailedConnectionNumber		OBJECT-TYPE
		SYNTAX	INTEGER
		ACCESS	read-only
		STATUS	mandatory
		DESCRIPTION
			"Number of connections that have failed, currently
			 queued for processing"
		::= { starPvcmEntry 14 }

	starPvcmChangedConnectionNumber		OBJECT-TYPE
		SYNTAX	INTEGER
		ACCESS	read-only
		STATUS	mandatory
		DESCRIPTION
			"Number of connections that have changed, currently
			 queued for processing"
		::= { starPvcmEntry 15 }

	starPvcmOrgHoldingConnectionNumber		OBJECT-TYPE
		SYNTAX	INTEGER
		ACCESS	read-only
		STATUS	mandatory
		DESCRIPTION
			"Number of connections that have failed repeatedly,
			 now in the holding queue"
		::= { starPvcmEntry 16 }

	starPvcmTermHoldingConnectionNumber		OBJECT-TYPE
		SYNTAX	INTEGER
		ACCESS	read-only
		STATUS	mandatory
		DESCRIPTION
			"Number of terminating connections that are being held,
			 awaiting re-connection, or deletion if re-connection
			 is not forthcoming"
		::= { starPvcmEntry 17 }

	 starPvcmInvalidMsgNumber			OBJECT-TYPE
		SYNTAX	INTEGER
		ACCESS	read-only
		STATUS	mandatory
		DESCRIPTION
			"Number of invalid messages received by PVCM."
		::= { starPvcmEntry 18 }

	 starPvcmTimerExpiredCnt			OBJECT-TYPE
		SYNTAX	INTEGER
		ACCESS	read-only
		STATUS	mandatory
		DESCRIPTION
			"Number of times the timer expired ."
		::= { starPvcmEntry 19 }
