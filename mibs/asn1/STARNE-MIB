	-----------------------------------------------------------------------
	-- Star RFC1604 Extended F/R Port Table
	-----------------------------------------------------------------------
	frlPortInfoTbl	OBJECT-TYPE
		SYNTAX  SEQUENCE OF FrlPortInfoEntry
		ACCESS  not-accessible
		STATUS  mandatory
		DESCRIPTION
			"This table is an extension to the Port table 
			in RFC1604, viz the frame relay mib."
		::= { starFrlExtnModuleInfo 1 }

	frlPortInfoEntry	 OBJECT-TYPE
		SYNTAX  FrlPortInfoEntry
		ACCESS  not-accessible
		STATUS  mandatory
		DESCRIPTION
			"An entry into FrlPortInfoTbl."
		INDEX { frlPortInfoSlotId, frlPortInfoPortId }
		::= { frlPortInfoTbl 1 }

	FrlPortInfoEntry ::=
		SEQUENCE {
			frlPortInfoSlotId	INTEGER ,
			frlPortInfoPortId	INTEGER ,
			frlPortSigMode		INTEGER ,
			frlPortActiveDlci	INTEGER 
		}

	frlPortInfoSlotId		 OBJECT-TYPE
		SYNTAX  INTEGER
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"This object indicates the slot id."
		::= { frlPortInfoEntry 1 }

	frlPortInfoPortId		 OBJECT-TYPE
		SYNTAX  INTEGER
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"This object indicates the port id."
		::= { frlPortInfoEntry 2 }

	frlPortSigMode			OBJECT-TYPE
		SYNTAX  INTEGER (1..4)
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"This object indicates if the port is emulating
				user,network,bi-directional,bidir with sync."

		::= { frlPortInfoEntry 3 }

	frlPortActiveDlci		OBJECT-TYPE
		SYNTAX  INTEGER
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"This object indicates  the num of active DLCi's
				on the port."
		::= { frlPortInfoEntry 4 }

	-----------------------------------------------------------------------
	-- Star RFC1604 Extended F/R Endpoint Info Table
	-----------------------------------------------------------------------
	frlPvcEndptInfoTbl	OBJECT-TYPE
		SYNTAX  SEQUENCE OF FrlPvcEndptInfoEntry
		ACCESS  not-accessible
		STATUS  mandatory
		DESCRIPTION
			"This table ia an extension for the Frame relay 
				PVC endpt table of Rfc1604, viz the
				Frame relay mib."
		::= { starFrlExtnModuleInfo 2 }

	 frlPvcEndptInfoEntry		OBJECT-TYPE
		SYNTAX  FrlPvcEndptInfoEntry
		ACCESS  not-accessible
		STATUS  mandatory
		DESCRIPTION
			"An entry into FrlPvcEndptTbl."
		INDEX { frlPvcEndptInfoSlotId, frlPvcEndptInfoPortId,
				frlPvcEndptInfoDlci }
		::= { frlPvcEndptInfoTbl 1 }

	FrlPvcEndptInfoEntry ::=
		SEQUENCE {
			frlPvcEndptInfoSlotId		INTEGER,
			frlPvcEndptInfoPortId		INTEGER,
			frlPvcEndptInfoDlci			INTEGER,
			frPvcEndptInfoTimeStamp 	INTEGER,
			frPvcMarkedDe				INTEGER,
			frPvcEndptOperStatus		INTEGER,
			frPvcEndptNoRecvFECN		INTEGER,
			frPvcEndptNoTxFECN			INTEGER,
			frPvcEndptNoRecvBECN		INTEGER,
			frPvcEndptNoTxBECN			INTEGER,
			frPvcEndptNoTxDE			INTEGER,
			frPvcEndptNoDiscardWithDE	INTEGER,
			frPvcEndptNoDiscardWoutDE	INTEGER, 
			frPvcEndptInDEFrames		INTEGER
		}

	 frlPvcEndptInfoSlotId		 OBJECT-TYPE
		SYNTAX  INTEGER
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"This object indicates the slot id."
		::= { frlPvcEndptInfoEntry 1 }

	 frlPvcEndptInfoPortId		 OBJECT-TYPE
		SYNTAX  INTEGER
		ACCESS  read-only
