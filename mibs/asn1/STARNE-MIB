		::= { starPortEntry 23 }

	-----------------------------------------------------------------------
	-- Star System Port DS1 Framer Table
	-----------------------------------------------------------------------
	starPortDS1FramerTable	OBJECT-TYPE
		SYNTAX  SEQUENCE OF PortDS1FramerEntry
		ACCESS  not-accessible
		STATUS  mandatory
		DESCRIPTION
			"This table contains information read from the
			 Brooktree T1 Framer chip and is indexed by Slot number
			 and Device Index" 
		::= { starPort 2 }

	starPortDS1FramerEntry	OBJECT-TYPE
		SYNTAX	PortDS1FramerEntry
		ACCESS	not-accessible
		STATUS	mandatory
		DESCRIPTION
			""
		INDEX	{ starSlotIndex, starPortDS1FramerIndex }
		::= { starPortDS1FramerTable 1 }

	PortDS1FramerEntry ::=
		SEQUENCE {
			starPortDS1FramerIndex				INTEGER,
			starPortDS1FramerRevision			INTEGER,
			starPortDS1FramerFrameMode			INTEGER,
			starPortDS1FramerLineCoding			INTEGER,
			starPortDS1FramerBPVCounter			INTEGER,
			starPortDS1FramerFBErrorCounter		INTEGER,
			starPortDS1FramerCRCErrorCounter	INTEGER,
			starPortDS1FramerCOFACounter		INTEGER
		}

	starPortDS1FramerIndex	OBJECT-TYPE
		SYNTAX	INTEGER
		ACCESS	read-only
		STATUS	mandatory
		DESCRIPTION
			"Device index of DS1 Framer chip"
		::= { starPortDS1FramerEntry 1 }

	starPortDS1FramerRevision	OBJECT-TYPE
		SYNTAX	INTEGER
		ACCESS	read-only
		STATUS	mandatory
		DESCRIPTION
			"Device revision number"
		::= { starPortDS1FramerEntry 2 }

	starPortDS1FramerFrameMode	OBJECT-TYPE
		SYNTAX	INTEGER {
			sfnormal(1),
			sft1dm(2),
			esfnormal(3),
			esfzbtis(4)
		}
		ACCESS	read-only
		STATUS	mandatory
		DESCRIPTION
			"DS1 Framer frame mode"
		::= { starPortDS1FramerEntry 3 }

	starPortDS1FramerLineCoding	OBJECT-TYPE
		SYNTAX	INTEGER {
			ami(1),
			mbzs(2),
			b8zstxrx(3)
		}
		ACCESS	read-only
		STATUS	mandatory
		DESCRIPTION
			"DS1 Framer line coding type"
		::= { starPortDS1FramerEntry 4 }

	starPortDS1FramerBPVCounter	OBJECT-TYPE
		SYNTAX	INTEGER
		ACCESS	read-only
		STATUS	mandatory
		DESCRIPTION
			"Number of bipolar violations"
		::= { starPortDS1FramerEntry 5 }

	starPortDS1FramerFBErrorCounter	OBJECT-TYPE
		SYNTAX	INTEGER
		ACCESS	read-only
		STATUS	mandatory
		DESCRIPTION
			"Number of frame bit errors"
		::= { starPortDS1FramerEntry 6 }

	starPortDS1FramerCRCErrorCounter	OBJECT-TYPE
		SYNTAX	INTEGER
		ACCESS	read-only
		STATUS	mandatory
		DESCRIPTION
			"Number of CRC errors"
		::= { starPortDS1FramerEntry 7 }

	starPortDS1FramerCOFACounter	OBJECT-TYPE
		SYNTAX	INTEGER
		ACCESS	read-only
		STATUS	mandatory
		DESCRIPTION
			"Number of change-of-frame-alignments"
		::= { starPortDS1FramerEntry 8 }

