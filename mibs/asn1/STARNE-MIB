			starPvcOutFixedCapa			INTEGER,
			starPvcInVariableCapa		INTEGER,
			starPvcOutVariableCapa		INTEGER,
			starSvcInFixedCapa			INTEGER,
			starSvcOutFixedCapa			INTEGER,
			starSvcInVariableCapa		INTEGER,
			starSvcOutVariableCapa		INTEGER 
		}

