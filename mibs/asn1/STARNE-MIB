		::= { starClkDistribStatusEntry 8 }

	starClkDistribStatusSiu8	 OBJECT-TYPE
		SYNTAX  INTEGER {
			eightKHZ (1),
			nintennMHZ (2)
		}
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"It indicates the distribute status of clock on slot14"
		::= { starClkDistribStatusEntry 9 }

	starClkDistribStatusSiu9	 OBJECT-TYPE
		SYNTAX  INTEGER {
			eightKHZ (1),
			nintennMHZ (2)
		}
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"It indicates the distribute status of clock on slot 15"
		::= { starClkDistribStatusEntry 10 }

	-----------------------------------------------------------------------
	-- Star System Switch88 Group
	-----------------------------------------------------------------------
	starSwitch88Info			OBJECT IDENTIFIER ::={ starSlot 6 }

	-----------------------------------------------------------------------
	-- Star System Switch Info Table
	-----------------------------------------------------------------------
	starSwitchInfoTbl   OBJECT-TYPE
		SYNTAX  SEQUENCE OF  StarSwitchInfoEntry
		ACCESS  not-accessible
		STATUS  mandatory
		DESCRIPTION
			"The table gives register values of the chips found on
			the SSU board."
		::= { starSwitch88Info 1 }

	starSwitchInfoEntry OBJECT-TYPE
		SYNTAX  StarSwitchInfoEntry
		ACCESS  not-accessible
		STATUS  mandatory
		DESCRIPTION
			"An entry in switchInfoTbl."
		INDEX { starSwitchInfoSlotId, starSwitchFabricRow,
				starSwitchFabricColumn }
		::= { starSwitchInfoTbl 1 }

	StarSwitchInfoEntry ::=
		SEQUENCE {
			starSwitchInfoSlotId		INTEGER,
			starSwitchFabricRow			INTEGER,
			starSwitchFabricColumn		INTEGER,
			starSwitchCurrentState		INTEGER,
			starSwitchChipVersion		INTEGER,
			starSwitchFabricLevel		INTEGER,
			starSwitchBckPressureDly	INTEGER,
			starSwitchAggrIp			INTEGER,
			starSwitchAggrOp			INTEGER,
			starSwitchStrictPriority	INTEGER,
			starSwitchInMarkedCellCnt	Counter,
			starSwitchOutMarkedCellCnt	Counter,
			starSwitchRatioAOrder1		INTEGER,
			starSwitchRatioAOrder2		INTEGER,
			starSwitchRatioBOrder1		INTEGER,
			starSwitchRatioBOrder2		INTEGER
		}

	starSwitchInfoSlotId  OBJECT-TYPE
		SYNTAX  INTEGER
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"This object indicates the slot id."
		::= { starSwitchInfoEntry 1 }

	starSwitchFabricRow	OBJECT-TYPE
		SYNTAX  INTEGER
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"This object indicates the switch chip
			 in the corresponding row."
		::= { starSwitchInfoEntry 2 }

	starSwitchFabricColumn	OBJECT-TYPE
		SYNTAX  INTEGER
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"This object indicates the switch chip
			 in the corresponding column." 
		::= { starSwitchInfoEntry 3 }

	starSwitchCurrentState	OBJECT-TYPE
		SYNTAX  INTEGER {
			init (1) ,
			operational (2)
		}
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"This object describes the operational
			 state of switch chip."
		::= { starSwitchInfoEntry 4 }

	starSwitchChipVersion	OBJECT-TYPE
		SYNTAX  INTEGER
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"This object indicates the version
			 of the switch chip."
		::= { starSwitchInfoEntry 5 }

	starSwitchFabricLevel	OBJECT-TYPE
		SYNTAX  INTEGER
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"The stage in which the particular cell is
			 placed in the switch fabric.
			 Value is between 1 and 9."
		::= { starSwitchInfoEntry 6 }

	starSwitchBckPressureDly	OBJECT-TYPE
		SYNTAX  INTEGER {
			noDelay (1),
			clk2Delay (2),
			clk4Delay (3)
		}
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"This object indicates that back pressure delay
			 that is to be introduced."
		::= { starSwitchInfoEntry 7 }

	starSwitchAggrIp	OBJECT-TYPE
		SYNTAX  INTEGER
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"This object indicates whether inputs are
			 grouped into 4(620 Mbps) or separately(155 Mbps)."
		::= { starSwitchInfoEntry 8 }

	starSwitchAggrOp	OBJECT-TYPE
		SYNTAX  INTEGER
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"This object indicates whether outputs are
			 grouped into 4(620 Mbps) or separately(155 Mbps)."
		::= { starSwitchInfoEntry 9 }

	starSwitchStrictPriority OBJECT-TYPE
		SYNTAX  INTEGER
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"This var indicates whether back-pressure is inserted
			 on high priority traffic when count of empty cells
			 is 0/8 "
		::= { starSwitchInfoEntry 10 }

	starSwitchInMarkedCellCnt   OBJECT-TYPE
		SYNTAX  Counter
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"This object indicates the number of cells
			 modulo 16 on an input which had Tag(9,1) set to 1."
		::= { starSwitchInfoEntry 11 }

	starSwitchOutMarkedCellCnt  OBJECT-TYPE
		SYNTAX  Counter
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"This object indicates the number of cells
			 modulo 16 on an output which had Tag(9,1) set to 1."
		::= { starSwitchInfoEntry 12 }

	starSwitchRatioAOrder1	OBJECT-TYPE
		SYNTAX  INTEGER
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"This object indicates the number of times
			 queue A is serviced first."
		::= { starSwitchInfoEntry 13 }

	starSwitchRatioAOrder2	OBJECT-TYPE
		SYNTAX  INTEGER
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"This object indicates the number of times
			 queue A is serviced second."
		::= { starSwitchInfoEntry  14 }

	starSwitchRatioBOrder1	OBJECT-TYPE
		SYNTAX  INTEGER
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"This object indicates the number of times
			 queue B is serviced first."
		::= { starSwitchInfoEntry  15 }

	starSwitchRatioBOrder2	OBJECT-TYPE
		SYNTAX  INTEGER
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"This object indicates the number of times
			 queue B is serviced second."
		::= { starSwitchInfoEntry  16 }

