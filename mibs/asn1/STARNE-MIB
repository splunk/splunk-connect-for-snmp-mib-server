	-- Star System Route88 Priority Queue Info Table
	------------------------------------------------------------------------
	starRoutPrioQInfoTbl		OBJECT-TYPE
		SYNTAX  SEQUENCE OF  StarRoutPrioQInfoEntry
		ACCESS  not-accessible
		STATUS  mandatory
		DESCRIPTION
			"Gives information about the seven priority queues."
		::= { starRout88Info 2 }

	starRoutPrioQInfoEntry	OBJECT-TYPE
		SYNTAX  StarRoutPrioQInfoEntry
		ACCESS  not-accessible
		STATUS  mandatory
		DESCRIPTION
			"An entry in the RoutPrioQInfoTbl." 
		INDEX { starSlotIdPrioQInfo, starPrioQRoutChipId, starRoutPrioQType }
		::= { starRoutPrioQInfoTbl 1 }

	StarRoutPrioQInfoEntry ::=
		SEQUENCE {
			starSlotIdPrioQInfo			INTEGER,
			starPrioQRoutChipId			INTEGER,
			starRoutPrioQType			INTEGER,
			starRoutPerPrioMaximumQd	INTEGER,
			starRoutPerPrioCongestionQd INTEGER,
			starRoutPerPrioCurrentQd	INTEGER,
			starRoutIsQCongested		INTEGER,
			starRoutIsCellInQueue		INTEGER
		}

	starSlotIdPrioQInfo	OBJECT-TYPE
		SYNTAX  INTEGER
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"This object indicates the slot id."
		::= { starRoutPrioQInfoEntry 1 }

	starPrioQRoutChipId	OBJECT-TYPE
		SYNTAX  INTEGER
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"This object indicates the rout chip whose
			 info is needed." 
		::= { starRoutPrioQInfoEntry  2 }

	starRoutPrioQType   OBJECT-TYPE
		SYNTAX  INTEGER{
			highPriorityA (1),
			multicastQueue (2),
			pba (3),
			pbb (4),
			pbc (5),
			highPriorityB (6),
			oamReceiveQ (7)
		}
		ACCESS  read-only
		STATUS		mandatory
		DESCRIPTION
			"The value indicates the queue type whose info
			 is to be retrived." 
		::= { starRoutPrioQInfoEntry  3 }

	starRoutPerPrioMaximumQd	OBJECT-TYPE
		SYNTAX  INTEGER
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"Maximum per priority queue depth."
		::= { starRoutPrioQInfoEntry  4 }

	starRoutPerPrioCongestionQd	 OBJECT-TYPE
		SYNTAX  INTEGER
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"Congestion per priority queue depth."
		::= { starRoutPrioQInfoEntry  5 }

	starRoutPerPrioCurrentQd		OBJECT-TYPE
		SYNTAX  INTEGER
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"Current per priority queue depth."
		::= { starRoutPrioQInfoEntry  6 }

	starRoutIsQCongested		OBJECT-TYPE
		SYNTAX  INTEGER{
			qCongested (1),
			qNotCongested (2)
		}
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"Maximum per priority queue depth."
		::= { starRoutPrioQInfoEntry  7 }

