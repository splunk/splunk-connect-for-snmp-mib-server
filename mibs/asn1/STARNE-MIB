		SEQUENCE {
			starCasStatusPortSlotId				INTEGER,
			starCasStatusPortPortId				INTEGER,
			starCasStatusPortConfigured			INTEGER,
			starCasStatusPortCasSamplePeriod	INTEGER,
			starCasStatusPortCasSampleNum		INTEGER
		}

	starCasStatusPortSlotId	OBJECT-TYPE
		SYNTAX  INTEGER
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"This object indicates the slot id."
		::= { starCasStatusPortInfoEntry 1 }

