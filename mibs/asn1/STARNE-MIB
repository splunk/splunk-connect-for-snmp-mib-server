	----------------------------------------------------------------------
	-- Star System Port Statistics Table
	----------------------------------------------------------------------
	starPortCellStatsTable	OBJECT-TYPE
		SYNTAX  SEQUENCE OF PortCellStatsEntry
		ACCESS  not-accessible
		STATUS  mandatory
		DESCRIPTION
			"This table contains statistics gathered on a specific
			 port and is indexed by Slot number, Port number "
		::= { starConn 11 }
 
	starPortCellStatsEntry  OBJECT-TYPE
		SYNTAX  PortCellStatsEntry
		ACCESS  not-accessible
		STATUS  mandatory
		DESCRIPTION
			""
		INDEX   { starSlotIndex, starPortIndex }
		::= { starPortCellStatsTable 1 }
 
	PortCellStatsEntry ::=
		SEQUENCE {
			starPortTimeStamp			Counter,
			starPortRtCellsRxCount		Counter,
			starPortRtCellsDroppedCount	Counter,
			starPortRtCellsCongCount	Counter,
			starPortUpcCellsCLP0		Counter,
			starPortUpcTotalCells		Counter,
			starPortUpcGcra0ViolCount	Counter,
			starPortUpcGcra1ViolCount	Counter,
			starPortUpcTotalOAMCells	Counter
		}

	starPortTimeStamp OBJECT-TYPE
		SYNTAX  Counter
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"Time Stamp in Seconds"
		::= { starPortCellStatsEntry 1 }
 
	starPortRtCellsRxCount  OBJECT-TYPE
		SYNTAX  Counter
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"Number of cells received in RT level for this port"
		::= { starPortCellStatsEntry 2 }
 
	starPortRtCellsDroppedCount OBJECT-TYPE
		SYNTAX  Counter
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"Number of cells dropped in RT level for this VP/VC"
		::= { starPortCellStatsEntry 3 }
 
	starPortRtCellsCongCount OBJECT-TYPE
		SYNTAX  Counter
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"Number of cells congestion in RT level for this VP/VC"
		::= { starPortCellStatsEntry 4 }
 
	starPortUpcCellsCLP0 OBJECT-TYPE
		SYNTAX  Counter
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"Number of cells with CLP0 in UPC level for this VP/VC"
		::= { starPortCellStatsEntry 5 }
 
	starPortUpcTotalCells OBJECT-TYPE
		SYNTAX  Counter
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"Number of total cells in UPC level for this VP/VC"
		::= { starPortCellStatsEntry 6 }
 
	starPortUpcGcra0ViolCount OBJECT-TYPE
		SYNTAX  Counter
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"Number of total GCRA0 violation cells in UPC level for this VP/VC"
		::= { starPortCellStatsEntry 7 }
 
	starPortUpcGcra1ViolCount OBJECT-TYPE
		SYNTAX  Counter
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"Number of total GCRA0 violation cells in UPC level for this VP/VC"
		::= { starPortCellStatsEntry 8 }
 
 
	starPortUpcTotalOAMCells OBJECT-TYPE
		SYNTAX  Counter
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"Number of total OAM cells in UPC level for this VP/VC"
		::= { starPortCellStatsEntry 9 }

