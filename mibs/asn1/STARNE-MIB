		SYNTAX	INTEGER 
		ACCESS	read-write
		STATUS	mandatory
		DESCRIPTION
			"Lear number from GCID when GCID Input type is selected"
		::= { starLoopbackCtrEntry 23 }

	starLoopbackCtrAssignedSessionNo	OBJECT-TYPE
		SYNTAX	INTEGER 
		ACCESS	read-write
		STATUS	mandatory
		DESCRIPTION
			"NMS Assigned session number for this loopback request"
		::= { starLoopbackCtrEntry 24 }

	starLoopbackCtrArea 		OBJECT-TYPE
		SYNTAX	INTEGER {
			intraloop(1),
			interloop(2),
			remoteloop(3)
		}
		ACCESS	read-write
		STATUS	mandatory
		DESCRIPTION
			"Method for loopback test"
		::= { starLoopbackCtrEntry 25 }

	starLoopbackCtrChannelType 		OBJECT-TYPE
		SYNTAX	INTEGER {
			userchan(1),
			looptestchan(2)
		}
		ACCESS	read-write
		STATUS	mandatory
		DESCRIPTION
			"Defines the channel to be used for loopback test, 
			 either a User channel or a predefined test channel"
		::= { starLoopbackCtrEntry 26 }

	starLoopbackCtrLastSetupResult	OBJECT-TYPE
		SYNTAX	INTEGER {
			success(1),
			fail(2)
		}
		ACCESS	read-only
		STATUS	mandatory
		DESCRIPTION
			"Success or failure of last setup request"
		::= { starLoopbackCtrEntry 27 }

	starLoopbackCtrPortNum 		OBJECT-TYPE
		SYNTAX	INTEGER 
		ACCESS	read-write
		STATUS	mandatory
		DESCRIPTION
			"port number of loopback test"
		::= { starLoopbackCtrEntry 28 }

	starLoopbackCtrGenCellType	OBJECT-TYPE
		SYNTAX	INTEGER {
			segment(1),
			connection-end(2)
		}
		ACCESS	read-write
		STATUS	mandatory
		DESCRIPTION
			"type of cell to be generated"
		::= { starLoopbackCtrEntry 29 }

	-----------------------------------------------------------------------
