	-----------------------------------------------------------------------
	-- Star System Port Table
	-----------------------------------------------------------------------
	starPortTable		OBJECT-TYPE
		SYNTAX  SEQUENCE OF PortEntry
		ACCESS  not-accessible
		STATUS  mandatory
		DESCRIPTION
			"This table contains port information for all ports
			 in the Star Node and is indexed by Slot number and
			 Port number" 
		::= { starPort 1 }

	starPortEntry	OBJECT-TYPE
		SYNTAX	PortEntry
		ACCESS	not-accessible
		STATUS	mandatory
		DESCRIPTION
			""
		INDEX	{ starSlotIndex, starPortIndex }
		::= { starPortTable 1 }

	PortEntry ::=
		SEQUENCE {
			starPortIndex					INTEGER,
			starPortIfIndex					INTEGER,
			starPortType					INTEGER,
			starPortConfig					INTEGER,
			starPortDescr					DisplayString,
			starPortAdminStatus				INTEGER,
			starPortAtmAddress				OCTET STRING,
			starPortLastChanged				TimeTicks,
			starPortAlarmStatus				INTEGER,
			starPortRemoteAtmAddress		OCTET STRING,
			starPortOperStatus				INTEGER,
			starPortIFType					INTEGER,
			starPortMaxVPINumber			INTEGER,
			starPortUsedVPINumber			INTEGER,
			starPortMaxVCINumber			INTEGER,
			starPortUsedVCINumber			INTEGER,
			starPortMaxFixedCapacity		INTEGER,
			starPortMaxVariableCapacity		INTEGER,
			starPortUsedFwdFixedCapacity	INTEGER,
			starPortUsedFwdVariableCapacity	INTEGER,
			starPortUsedBwdFixedCapacity	INTEGER,
			starPortUsedBwdVariableCapacity	INTEGER,
			starPortAvailableVpi			INTEGER 
		}

	starPortIndex	OBJECT-TYPE
		SYNTAX	INTEGER
		ACCESS	read-only
		STATUS	mandatory
		DESCRIPTION
			"Port number"
		::= { starPortEntry 1 }

	starPortIfIndex	OBJECT-TYPE
		SYNTAX	INTEGER
		ACCESS	read-only
		STATUS	mandatory
		DESCRIPTION
			"This object contains the ifIndex value related to
			 this port.  The formula used to compute is
			 SlotNumber * 100 + PortNumber"
		::= { starPortEntry 2 }

	starPortType	OBJECT-TYPE
		SYNTAX	INTEGER
		ACCESS	read-only
		STATUS	mandatory
		DESCRIPTION
			"Contains the same value as ifType for this port"
		::= { starPortEntry 3 }

	starPortConfig	OBJECT-TYPE
		SYNTAX	INTEGER
		ACCESS	read-only
		STATUS	mandatory
		DESCRIPTION
			"Contains the configuration for this port"
		::= { starPortEntry 4 }

	starPortDescr	OBJECT-TYPE
		SYNTAX	DisplayString
		ACCESS	read-only
		STATUS	mandatory
		DESCRIPTION
			"Character description for this port"
		::= { starPortEntry 5 }

	starPortAdminStatus	OBJECT-TYPE
		SYNTAX	INTEGER
		ACCESS	read-only
		STATUS	mandatory
		DESCRIPTION
			"Administrative status for this port, can only be set
			 by operator doing a configuration file download with
			 a new value for port status"
		::= { starPortEntry 6 }

	starPortAtmAddress	OBJECT-TYPE
		SYNTAX	OCTET STRING
		ACCESS	read-write
		STATUS	mandatory
		DESCRIPTION
			"Atm address used for SVC's only"
		::= { starPortEntry 7 }

	starPortLastChanged	OBJECT-TYPE
		SYNTAX	TimeTicks
		ACCESS	read-only
		STATUS	mandatory
		DESCRIPTION
			"Time this port was last changed or came into service"
		::= { starPortEntry 8 }

	starPortAlarmStatus	OBJECT-TYPE
		SYNTAX	INTEGER
		ACCESS	read-only
		STATUS	mandatory
		DESCRIPTION
			"Last alarm on this port"
		::= { starPortEntry 9 }

	starPortRemoteAtmAddress	OBJECT-TYPE
		SYNTAX	OCTET STRING
		ACCESS	read-write
		STATUS	mandatory
		DESCRIPTION
			"Remote Atm address used for SVC's only"
		::= { starPortEntry 10 }

	starPortOperStatus	OBJECT-TYPE
		SYNTAX	INTEGER {
			in-service(1),
			out-of-service(2)
		}
		ACCESS	read-only
		STATUS	mandatory
		DESCRIPTION
			"Operational status of this port"
		::= { starPortEntry 11 }

	starPortIFType	OBJECT-TYPE
		SYNTAX	INTEGER
		ACCESS	read-only
		STATUS	mandatory
		DESCRIPTION
			"Contains same value as Board type"
		::= { starPortEntry 12 }

	starPortMaxVPINumber	OBJECT-TYPE
		SYNTAX	INTEGER
		ACCESS	read-only
		STATUS	mandatory
		DESCRIPTION
			"Maximum VPI values for this port"
		::= { starPortEntry 13 }

	starPortUsedVPINumber	OBJECT-TYPE
