	-----------------------------------------------------------------------
	-- Star Port HDLC Configuration Information Table
	-----------------------------------------------------------------------
	starPortHdlcConfigTable	OBJECT-TYPE
		SYNTAX  SEQUENCE OF PortHdlcConfigEntry
		ACCESS  not-accessible
		STATUS  mandatory
		DESCRIPTION
			"This table contains information read from the
			Hdlc Device and is indexed by Slot number
			and Device Index" 
		::= { starPort 11 }

	starPortHdlcConfigEntry	OBJECT-TYPE
		SYNTAX	PortHdlcConfigEntry
		ACCESS	not-accessible
		STATUS	mandatory
		DESCRIPTION
			""
		INDEX	{ starSlotIndex, starPortHdlcIndex }
		::= { starPortHdlcConfigTable 1 }

	PortHdlcConfigEntry ::=
		SEQUENCE {
			starPortHdlcIndex			INTEGER,
			starPortHdlcLineSpeed		INTEGER,
			starPortHdlcLineEncoding	INTEGER,
			starPortHdlcCrcMode			INTEGER,
			starPortHdlcDteDce			INTEGER,
			starPortHdlcClockMode		INTEGER
		}

	starPortHdlcIndex	OBJECT-TYPE
		SYNTAX	INTEGER
		ACCESS	read-only
		STATUS	mandatory
		DESCRIPTION
			"Device index of Hdlc chip"
		::= { starPortHdlcConfigEntry 1 }

	starPortHdlcLineSpeed	OBJECT-TYPE
		SYNTAX	INTEGER {
			t1(1)
		}
		ACCESS	read-only
		STATUS	mandatory
		DESCRIPTION
			"Hdlc chip line speed"
		::= { starPortHdlcConfigEntry 2 }

	starPortHdlcLineEncoding	OBJECT-TYPE
		SYNTAX	INTEGER {
			nrz(1),
			nrzi(2)
		}
		ACCESS	read-only
		STATUS	mandatory
		DESCRIPTION
			"Hdlc line encoding mode"
		::= { starPortHdlcConfigEntry 3 }

	starPortHdlcCrcMode	OBJECT-TYPE
		SYNTAX	INTEGER {
			crc16(1),
			crcccitt(2)
		}
		ACCESS	read-only
		STATUS	mandatory
		DESCRIPTION
			"Hdlc CRC mode"
		::= { starPortHdlcConfigEntry 4 }

	starPortHdlcDteDce	OBJECT-TYPE
		SYNTAX	INTEGER {
			dte(1),
			dce(2)
		}
		ACCESS	read-only
		STATUS	mandatory
		DESCRIPTION
			"Hdlc Dte/Dce type"
		::= { starPortHdlcConfigEntry 5 }

	starPortHdlcClockMode	OBJECT-TYPE
		SYNTAX	INTEGER {
			internal(1),
			external(2)
		}
		ACCESS	read-only
		STATUS	mandatory
		DESCRIPTION
			"Clock mode"
		::= { starPortHdlcConfigEntry 6 }

