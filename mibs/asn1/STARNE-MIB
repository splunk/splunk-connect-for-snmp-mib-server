		INDEX	{ starBbcpSlotIndex, starBbcpPortIndex, starBbcpGCIDNeId,
				  starBbcpGCIDSlotId, starBbcpGCIDPortId, starBbcpGCIDIfType,
				  starBbcpGCIDConnId, starBbcpGCIDLeafId }
		::= { starSlotBbcpXConnectTable 1 }

