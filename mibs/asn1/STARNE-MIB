		::= { starPvcmEntry 6 }

	starPvcmRouteFailCt		OBJECT-TYPE
		SYNTAX	INTEGER
		ACCESS	read-only
		STATUS	mandatory
		DESCRIPTION
			"Number of connections that failed because a route 
			 could not be found"
		::= { starPvcmEntry 7 }

	starPvcmConnectReqFailCt		OBJECT-TYPE
		SYNTAX	INTEGER
		ACCESS	read-only
		STATUS	mandatory
		DESCRIPTION
			"Number of connections that failed during setup"
		::= { starPvcmEntry 8 }

	starPvcmOrgConnectedFailCt		OBJECT-TYPE
		SYNTAX	INTEGER
		ACCESS	read-only
		STATUS	mandatory
		DESCRIPTION
			"Number of connections that failed after being 
			 completely setup"
		::= { starPvcmEntry 9 }

