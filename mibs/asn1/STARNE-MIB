	-----------------------------------------------------------------------
	-- Star System Fault Manager Group
	-----------------------------------------------------------------------
	starFaultManager	OBJECT IDENTIFIER ::= { starSystem 5 }

	starFmSendTrapThreshold	OBJECT-TYPE
		SYNTAX	INTEGER {
			critical(1),
			major(2),
			minor(3),
			cleared(4),
			all(5)
		}
		ACCESS	read-write
		STATUS	mandatory
		DESCRIPTION
			"Threshold level of alarms to be sent as traps to the
			 NMS Manager"
		::= { starFaultManager 1 }

	starFmLogNotify	OBJECT-TYPE
		SYNTAX	INTEGER
		ACCESS	write-only
		STATUS	mandatory
		DESCRIPTION
			"A notification for log file received from NMS
			 to STAR NE"
		::= { starFaultManager 2 }

	starFmLogFileInterval	OBJECT-TYPE
		SYNTAX	INTEGER
		ACCESS	read-write
		STATUS	mandatory
		DESCRIPTION
			"A log file upload interval time to NMS
			 from STAR NE"
		::= { starFaultManager 3 }

	-----------------------------------------------------------------------
	-- Star System Slot Status Table
	-----------------------------------------------------------------------
	starSysSlotStatusTable	OBJECT-TYPE
		SYNTAX  SEQUENCE OF SysSSDEntry
		ACCESS  not-accessible
		STATUS  mandatory
		DESCRIPTION
			"This table contains status information for all boards
			 in the STAR NE."
		::= { starSystem 6 }

	starSysSlotStatusEntry	OBJECT-TYPE
		SYNTAX	SysSSDEntry
		ACCESS	not-accessible
		STATUS	mandatory
		DESCRIPTION
			""
		INDEX	{ starSSDIndex }
		::= { starSysSlotStatusTable 1 }

	SysSSDEntry ::=
		SEQUENCE {
			starSSDIndex			INTEGER,
			starSSDBdEquipStatus	INTEGER,
			starSSDModuleType		INTEGER,
			starSSDInterfaceType	INTEGER,
			starSSDOperStatus		INTEGER,
			starSSDCpuFailStatus	INTEGER,
			starSSDPwrFailStatus	INTEGER,
			starSSDRealStatus		INTEGER,
			starSiuRednConfigStatus	INTEGER,
			starSiuRednOperStatus	INTEGER,
			starSiuRednSwitchOver	INTEGER
		}

	starSSDIndex	OBJECT-TYPE
		SYNTAX	INTEGER (1..15)
		ACCESS	read-only
		STATUS	mandatory
		DESCRIPTION
			"This object value will be slot #. This object
			 will be used as index of current table."
		::= { starSysSlotStatusEntry 1 }

	starSSDBdEquipStatus	OBJECT-TYPE
		SYNTAX	INTEGER {
			noEquip (1),
			equip(2)
		}
		ACCESS	read-only
		STATUS	mandatory
		DESCRIPTION
			"Specifies whether a board is equiped or not
			 in current slot."
		::= { starSysSlotStatusEntry 2 }

	starSSDModuleType	OBJECT-TYPE
		SYNTAX	INTEGER
		ACCESS	read-only
		STATUS	mandatory
		DESCRIPTION
			"Specifies type of board in this slot, AIU, 
			 PIU, etc"
		::= { starSysSlotStatusEntry 3 }

	starSSDInterfaceType	OBJECT-TYPE
		SYNTAX	INTEGER
		ACCESS	read-only
		STATUS	mandatory
		DESCRIPTION
			"Specifies interface of board in this slot, T1, 
			 E3, etc"
		::= { starSysSlotStatusEntry 4 }

	starSSDOperStatus	OBJECT-TYPE
		SYNTAX	INTEGER {
			delete(1),
			install(2),
			active(3),
			deactive(4)
		}
		ACCESS	read-only
		STATUS	mandatory
		DESCRIPTION
			"This object contains operation status information
			 of the board in indexed slot."
		::= { starSysSlotStatusEntry 5 }

	starSSDCpuFailStatus	OBJECT-TYPE
		SYNTAX	INTEGER {
			failed (1),
			normal(2)
		}
		ACCESS	read-only
		STATUS	mandatory
		DESCRIPTION
			"contains CPU fail status of the board in the
			 indexed module"
		::= { starSysSlotStatusEntry 6 }

	starSSDPwrFailStatus	OBJECT-TYPE
		SYNTAX	INTEGER {
			failed (1),
			normal(2)
		}
		ACCESS	read-only
		STATUS	mandatory
		DESCRIPTION
			"contains power supply fail status of the board in the 
			indexed module"
		::= { starSysSlotStatusEntry 7 }

	starSSDRealStatus	OBJECT-TYPE
		SYNTAX	INTEGER 
		ACCESS	read-only
		STATUS	mandatory
		DESCRIPTION
			"SSD Real Status information from SPU's ssd structure"
		::= { starSysSlotStatusEntry 8 }

	starSiuRednConfigStatus	OBJECT-TYPE
		SYNTAX	INTEGER {
			standalone  (0),
			primary     (1),
			secondary   (2),
			undefined   (3),
			maxRedund   (4)
		}
		ACCESS	read-only
		STATUS	mandatory
		DESCRIPTION
			"SIU Redundency config information from IRCM"
		::= { starSysSlotStatusEntry 9 }

	starSiuRednOperStatus	OBJECT-TYPE
		SYNTAX	INTEGER	{
			standalone  (0),
			active      (1),
			standby     (2),
			unknown     (3),
			failedRole  (4),
			maxRedund   (5)
		}
		ACCESS	read-only
		STATUS	mandatory
		DESCRIPTION
			"SIU Redundency current operational infomation from IRCM"
		::= { starSysSlotStatusEntry 10 }


	starSiuRednSwitchOver	OBJECT-TYPE
		SYNTAX	INTEGER 
		ACCESS	write-only
		STATUS	mandatory
		DESCRIPTION
			"SIU Redundency Switchover request from NMS"
		::= { starSysSlotStatusEntry 11 }

	-----------------------------------------------------------------------
	-- Star System Fault Control Table
	-----------------------------------------------------------------------
	starFaultControlTable	OBJECT-TYPE
			SYNTAX  SEQUENCE OF StarFaultControlEntry
			ACCESS  not-accessible
			STATUS  mandatory
			DESCRIPTION
			"This table includes the objects for controlling
			 the threshold value of a fault."
		::= { starSystem 7 }

	starFaultControlEntry	OBJECT-TYPE
		SYNTAX	StarFaultControlEntry
		ACCESS	not-accessible
		STATUS	mandatory
		DESCRIPTION
			""
		INDEX	{ starFaultSlotId, starFaultGroupId, starFaultSubId }
		::= { starFaultControlTable 1 }

	StarFaultControlEntry ::=
		SEQUENCE {
			starFaultSlotId			INTEGER,
			starFaultGroupId		INTEGER,
			starFaultSubId			INTEGER,
			starSeverity			INTEGER,
			starFaultThresholdType	INTEGER,
			starFaultHappenValue	INTEGER,
			starFaultClearValue		INTEGER
		}

	starFaultSlotId	OBJECT-TYPE
		SYNTAX	INTEGER
		ACCESS	read-only
		STATUS	mandatory
		DESCRIPTION
			"The 1st index of current table"
		::= { starFaultControlEntry 1 }

	starFaultGroupId	OBJECT-TYPE
		SYNTAX	INTEGER
		ACCESS	read-only
		STATUS	mandatory
		DESCRIPTION
			"The 2nd index of current table"
		::= { starFaultControlEntry 2 }

	starFaultSubId	OBJECT-TYPE
		SYNTAX	INTEGER
		ACCESS	read-only
		STATUS	mandatory
		DESCRIPTION
			"The 3rd index of current table"
		::= { starFaultControlEntry 3 }

	starSeverity	OBJECT-TYPE
		SYNTAX	INTEGER
		ACCESS	read-write
		STATUS	mandatory
		DESCRIPTION
			""
		::= { starFaultControlEntry 4 }

	starFaultThresholdType	OBJECT-TYPE
		SYNTAX	INTEGER
		ACCESS	read-write
		STATUS	mandatory
		DESCRIPTION
			""
		::= { starFaultControlEntry 5 }

	starFaultHappenValue	OBJECT-TYPE
		SYNTAX	INTEGER
		ACCESS	read-write
		STATUS	mandatory
		DESCRIPTION
			""
		::= { starFaultControlEntry 6 }

