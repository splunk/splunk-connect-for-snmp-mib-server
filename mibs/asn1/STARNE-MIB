	-----------------------------------------------------------------------
	-- Star System Link State Generation Table
	-----------------------------------------------------------------------
	starSlotLsgnStatusTable		OBJECT-TYPE
		SYNTAX  SEQUENCE OF LsgnEntry
		ACCESS  not-accessible
		STATUS  mandatory
		DESCRIPTION
			"" 
		::= { starSlot 10 }

	starLsgnEntry	OBJECT-TYPE
		SYNTAX	LsgnEntry
		ACCESS	not-accessible
		STATUS	mandatory
		DESCRIPTION
			""
		INDEX	{ starSlotIndex, starPortIndex }
		::= { starSlotLsgnStatusTable 1 }

	LsgnEntry ::=
		SEQUENCE {
			starLsgnDownCt					INTEGER,
			starLsgnUpCt					INTEGER,
			starLsgnTrafficLoadChangedCt	INTEGER,
			starLsgnPeriodMsgCt				INTEGER,
			starLsgnLinkOperStatus			INTEGER
		}

	starLsgnDownCt		OBJECT-TYPE
		SYNTAX	INTEGER
		ACCESS	read-only
		STATUS	mandatory
		DESCRIPTION
			"Number of times a Link Down Message was sent for the
			 link"
		::= { starLsgnEntry 1 }

	starLsgnUpCt		OBJECT-TYPE
		SYNTAX	INTEGER
		ACCESS	read-only
		STATUS	mandatory
		DESCRIPTION
			"Number of times a Link Up Message was sent for the
			 link"
		::= { starLsgnEntry 2 }

	starLsgnTrafficLoadChangedCt	OBJECT-TYPE
		SYNTAX	INTEGER
		ACCESS	read-only
		STATUS	mandatory
		DESCRIPTION
			"Number of times the link's traffic load changed"
		::= { starLsgnEntry 3 }

	starLsgnPeriodMsgCt		OBJECT-TYPE
		SYNTAX	INTEGER
		ACCESS	read-only
		STATUS	mandatory
		DESCRIPTION
			"Number of times a Periodic Link Message was sent"
		::= { starLsgnEntry 4 }

	starLsgnLinkOperStatus		OBJECT-TYPE
		SYNTAX	INTEGER
		ACCESS	read-only
		STATUS	mandatory
		DESCRIPTION
			"Current status of the link"
		::= { starLsgnEntry 5 }

	-----------------------------------------------------------------------
	-- Star System Link Status Update Table in RLSP
	-----------------------------------------------------------------------
	starSlotRlspStatusTable		OBJECT-TYPE
		SYNTAX  SEQUENCE OF RlspEntry
		ACCESS  not-accessible
		STATUS  mandatory
		DESCRIPTION
			"" 
		::= { starSlot 11 }

	starRlspEntry	OBJECT-TYPE
		SYNTAX	RlspEntry
		ACCESS	not-accessible
		STATUS	mandatory
		DESCRIPTION
			""
		INDEX	{ starSlotIndex }
		::= { starSlotRlspStatusTable 1 }

	RlspEntry ::=
		SEQUENCE {
			starRlspForwardedCt		INTEGER,
			starRlspDiscardedCt		INTEGER,
			starRlspInvalidCt		INTEGER,
			starRlspMismatchCt		INTEGER,
			starRlspNoRoutesCt		INTEGER
		}

	starRlspForwardedCt		OBJECT-TYPE
		SYNTAX	INTEGER
		ACCESS	read-only
		STATUS	mandatory
		DESCRIPTION
			"Number of received LSU's (Link Status Updates) that
			 are forwarded"
		::= { starRlspEntry 1 }

	starRlspDiscardedCt		OBJECT-TYPE
		SYNTAX	INTEGER
		ACCESS	read-only
		STATUS	mandatory
		DESCRIPTION
			"Number of received LSU's (Link Status Updates) that
			 are discarded"
		::= { starRlspEntry 2 }

	starRlspInvalidCt		OBJECT-TYPE
		SYNTAX	INTEGER
		ACCESS	read-only
		STATUS	mandatory
		DESCRIPTION
			"Number of messages received that indicate that a route
			 specified by the routing software is incorrect"
		::= { starRlspEntry 3 }

	starRlspMismatchCt		OBJECT-TYPE
		SYNTAX	INTEGER
		ACCESS	read-only
		STATUS	mandatory
		DESCRIPTION
			"Number of received LSU (Link Status Update) messages
			 that did not match locally stored Link State Table 
			 Entries"
		::= { starRlspEntry 4 }

	starRlspNoRoutesCt		OBJECT-TYPE
		SYNTAX	INTEGER
		ACCESS	read-only
		STATUS	mandatory
		DESCRIPTION
			"Number of route requests or route validates for which
			 no route with a valid cost was found"
		::= { starRlspEntry 5 }

	-----------------------------------------------------------------------
	-- Star System Backbone Connection Protocol Table
	-----------------------------------------------------------------------
	starSlotBbcpStatusTable		OBJECT-TYPE
		SYNTAX  SEQUENCE OF BbcpEntry
		ACCESS  not-accessible
		STATUS  mandatory
		DESCRIPTION
			"" 
		::= { starSlot 12 }

	starBbcpEntry	OBJECT-TYPE
		SYNTAX	BbcpEntry
		ACCESS	not-accessible
		STATUS	mandatory
		DESCRIPTION
			""
		INDEX	{ starSlotIndex, starPortIndex }
		::= { starSlotBbcpStatusTable 1 }

	BbcpEntry ::=
		SEQUENCE {
			starBbcpPtoPInConnections					INTEGER,
			starBbcpPtoPOutConnections					INTEGER,
			starBbcpPtoMInConnections					INTEGER,
			starBbcpPtoMOutConnections					INTEGER,
			starBbcpInConnectionLinkFailCt				INTEGER,
			starBbcpInConnectionVcciFailCt				INTEGER,
			starBbcpInConnectionNoSwitchCapacityCt		INTEGER,
			starBbcpInConnectionNoBBCapacityCt			INTEGER,
			starBbcpInConnectionTimeoutCt				INTEGER,
			starBbcpInConnectionChipErrorCt				INTEGER,
			starBbcpInConnectionPreemptFailCt			INTEGER,
			starBbcpInConnectionModuleFailCt			INTEGER,
			starBbcpInConnectionVcciCollisionFailCt		INTEGER,
			starBbcpOutConnectionLinkFailCt				INTEGER,
			starBbcpOutConnectionVcciInvalidCt			INTEGER,
			starBbcpOutConnectionNoSwitchCapacityCt		INTEGER,
			starBbcpOutConnectionNoBBCapacityCt			INTEGER,
			starBbcpOutConnectionTimeoutCt				INTEGER,
			starBbcpOutConnectionChipErrorCt			INTEGER,
			starBbcpOutConnectionPreemptFailCt			INTEGER,
			starBbcpOutConnectionModuleFailCt			INTEGER,
			starBbcpOutConnectionVcciCollisionFailCt	INTEGER,
			starBbcpInConnectionClearLinkFailCt			INTEGER,
			starBbcpInConnectionClearPreemptionCt		INTEGER,
			starBbcpInConnectionClearModuleFailCt		INTEGER,
			starBbcpInConnectionClearNormalCt			INTEGER,
			starBbcpOutConnectionClearLinkFailCt		INTEGER,
			starBbcpOutConnectionClearPreemptionCt		INTEGER,
			starBbcpOutConnectionClearModuleFailCt		INTEGER,
			starBbcpOutConnectionClearNormalCt			INTEGER,
			starBbcpInvalidMsgCt						INTEGER,
			starBbcpTimerExpiredCt						INTEGER,
			starBbcpInvalidEventCt						INTEGER,
			starBbcpAvailableFixedCapacity				INTEGER,
			starBbcpAvailableVariableCapacity			INTEGER
		}

	starBbcpPtoPInConnections	OBJECT-TYPE
		SYNTAX	INTEGER
		ACCESS	read-only
		STATUS	mandatory
		DESCRIPTION
			"Number of active incoming point to point connections"
		::= { starBbcpEntry 1 }

	starBbcpPtoPOutConnections	OBJECT-TYPE
		SYNTAX	INTEGER
		ACCESS	read-only
		STATUS	mandatory
		DESCRIPTION
			"Number of active outgoing point to point connections"
		::= { starBbcpEntry 2 }

	starBbcpPtoMInConnections	OBJECT-TYPE
		SYNTAX	INTEGER
		ACCESS	read-only
		STATUS	mandatory
		DESCRIPTION
			"Number of active incoming point to multi-point 
			 connections"
		::= { starBbcpEntry 3 }

	starBbcpPtoMOutConnections	OBJECT-TYPE
		SYNTAX	INTEGER
		ACCESS	read-only
		STATUS	mandatory
		DESCRIPTION
			"Number of active outgoing point to multi-point 
			 connections"
		::= { starBbcpEntry 4 }

	starBbcpInConnectionLinkFailCt	OBJECT-TYPE
		SYNTAX	INTEGER
		ACCESS	read-only
		STATUS	mandatory
		DESCRIPTION
			"Number of incoming connection requests that were
			 rejected because a backbone link on the module was 
			 down"
		::= { starBbcpEntry 5 }

	starBbcpInConnectionVcciFailCt	OBJECT-TYPE
		SYNTAX	INTEGER
		ACCESS	read-only
		STATUS	mandatory
		DESCRIPTION
			"Number of incoming connection requests that were
			 rejected because Vpi, Vci in the connection is 
			 invalid"
		::= { starBbcpEntry 6 }

	starBbcpInConnectionNoSwitchCapacityCt	OBJECT-TYPE
		SYNTAX	INTEGER
		ACCESS	read-only
		STATUS	mandatory
		DESCRIPTION
			"Number of incoming connection requests that were
			 rejected because the switch did not have the capacity
			 to support the call"
		::= { starBbcpEntry 7 }

	starBbcpInConnectionNoBBCapacityCt	OBJECT-TYPE
		SYNTAX	INTEGER
		ACCESS	read-only
		STATUS	mandatory
		DESCRIPTION
			"Number of incoming connection requests that were
			 rejected because the backbone link did not have the
			 capacity to support the call"
		::= { starBbcpEntry 8 }

	starBbcpInConnectionTimeoutCt	OBJECT-TYPE
		SYNTAX	INTEGER
		ACCESS	read-only
		STATUS	mandatory
		DESCRIPTION
			"Number of incoming connection requests that were
			 rejected because the BBCP did not receive a connection
			 proceeding message within the specified time"
		::= { starBbcpEntry 9 }

	starBbcpInConnectionChipErrorCt	OBJECT-TYPE
		SYNTAX	INTEGER
		ACCESS	read-only
		STATUS	mandatory
		DESCRIPTION
			"Number of incoming connection requests that were
			 rejected because a chip could not be programmed"
		::= { starBbcpEntry 10 }

	starBbcpInConnectionPreemptFailCt	OBJECT-TYPE
		SYNTAX	INTEGER
		ACCESS	read-only
		STATUS	mandatory
		DESCRIPTION
			"Number of incoming connection requests that were
