        unbundling(16),
        complete(17),
        abort(18),
        remotePatchRemove(19),
        remoteWriteMem(20),
        eraseMem(21),
        remoteEraseMem(22),
        preloadCams(23),
        localInstallImage(24),
        remoteInstallImage(25),
        localPreloadRsm(26),
        remotePreloadRsm(27),
        remoteCommitRsm(28),
        copyImage(29),
        remoteFirmwareUpdateRsm(30),
        patchApplyCheck(31),
        waitSoftswitch(32),
        waitSpareCamsReset(33),
        waitCamsReset(34)
    }
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION 
        " Status object for E6000 reload "
