-- QWES.com Ethernet group
-- This group contain the MIB objects used to configure the Ethernet Interface.
-- Ethernet group { oem 11 }

	qethernetCfgTable	OBJECT-TYPE
		SYNTAX		   	SEQUENCE OF QethernetCfgEntry
		ACCESS			not-accessible
		STATUS			mandatory
		DESCRIPTION		"A list of Ethernet Configuration
					entries.  The maximum number is based
					on the maximum interfaces."
		::= {qethernet 1}

   	qethernetCfgEntry 	OBJECT-TYPE
		SYNTAX			QethernetCfgEntry
		ACCESS			not-accessible
		STATUS			mandatory
		DESCRIPTION		"The Ethernet configuration information
					for each Ethernet interface."
		INDEX   { ifIndex }
   		::= { qethernetCfgTable 1 }

	QethernetCfgEntry ::=
		SEQUENCE {
		        qethernetMacAddress
		            PhysAddress,
		        qethernetSpeedMode
		            INTEGER,
		        qethernetEncapsulation
		            INTEGER,
			qethernetAutoSense
			    INTEGER,
			qethernetName
			    DisplayString
			}

	qethernetMacAddress	 OBJECT-TYPE
		SYNTAX			PhysAddress
		ACCESS			read-write
		STATUS			mandatory
		DESCRIPTION		"The ethernet's physical address.  An
					six byte hexidecimal identifier, which
					identifies the interface on the
					physical media."
		::= { qethernetCfgEntry 1 }

	qethernetSpeedMode 	OBJECT-TYPE
		SYNTAX			INTEGER {
						full-duplex-100-mbps (1),
						full-duplex-10-mbps (2),
						half-duplex-100-mbps (3),
						half-duplex-10-mbps (4)
						}
		ACCESS			read-write
		STATUS			mandatory
		DESCRIPTION		"The Ethernet chipset has five
					available modes: (1) 
					100-MBPS-FULL-DUPLEX - sets ethernet
					chipset to 100 mbps, full duplex mode;
					(2) 10-MBPS-FULL-DUPLEX - sets ethernet
					chipset to 10 mbps, full duplex mode;
					(3) 100-MBPS-HALF-DUPLEX - sets
					ethernet chipset to 100 mbps, half
					duplex mode; (4) 10-MBPS-HALF-DUPLEX -
					sets ethernet chipset to 10 mbps, half
					duplex mode."
		::= { qethernetCfgEntry 2 }

	qethernetEncapsulation 	OBJECT-TYPE
		SYNTAX			INTEGER {
						etherv2encap  (1),
						ieee8023encap (2)
						}
		ACCESS			read-write
		STATUS			mandatory
		DESCRIPTION		"The flag which sets the ethernets
					encapsulation type.  1 = Ethernet
					Version 2 encapsulation; 2 = IEEE 802.3
					encapsulation."
		::= { qethernetCfgEntry 3 }

	qethernetAutoSense	OBJECT-TYPE
        	SYNTAX 			INTEGER{ yes (1), no (2) }
        	ACCESS			read-write
		STATUS			mandatory
		DESCRIPTION		"This object is used to negotiate the
					speed/duplex.  The default is set to
					YES (1) - which negotiates from the
					configured speed/duplex
					(qethernetSpeedMode) or lower.  When
					set to NO (2) - this forces the
					configured speed/duplex mode and will
					not establish a link unless the other
					end complies."
		::= { qethernetCfgEntry 4 }

	qethernetName		OBJECT-TYPE
        	SYNTAX 			DisplayString (SIZE (0..255))
        	ACCESS			read-write
		STATUS			mandatory
		DESCRIPTION		"This object is used to???"
		::= { qethernetCfgEntry 5 }

	qethernetCurrentSpeedMode 	OBJECT-TYPE
		SYNTAX			INTEGER {
						full-duplex-100-mbps (1),
						full-duplex-10-mbps (2),
						half-duplex-100-mbps (3),
						half-duplex-10-mbps (4),
						no-link (5)
						}
		ACCESS			read-only
		STATUS			mandatory
		DESCRIPTION		"The Ethernet chipset has four
					available modes or operation: (1) 
					100-MBPS-FULL-DUPLEX - sets ethernet
					chipset to 100 mbps, full duplex mode;
					(2) 10-MBPS-FULL-DUPLEX - sets ethernet
					chipset to 10 mbps, full duplex mode;
					(3) 100-MBPS-HALF-DUPLEX - sets
					ethernet chipset to 100 mbps, half
					duplex mode; (4) 10-MBPS-HALF-DUPLEX -
					sets ethernet chipset to 10 mbps, half
					duplex mode.  This object will provide
					the current speed/duplex achieved or
					'No Link'."
		::= { qethernetCfgEntry 6 }


-- QWES.com DS1/E1 group
-- This group contains the MIB objects used to manage the DS1/E1 Interfaces.
-- DS1/E1 group { oem 12 }

	qds1AutoClockSourceSearching	OBJECT-TYPE
		SYNTAX			INTEGER	{ yes (1), no (2) }
		ACCESS			read-write
		STATUS			mandatory
		DESCRIPTION		"Selecting (1) YES will allow the
					Policy Point unit to automatically
					select the next available SPAN to
					provide clock if the Primary Clock
					Source fails.  Selecting (2) NO
					will instruct the unit to ignore all
					clock source ports except the primary
					source clock port."
		::= { qds1-e1 1 }

	qds1PrimaryClockSource	OBJECT-TYPE
		SYNTAX			INTEGER	{
					 	span-1 (2),
					 	span-2 (3),
					 	span-3 (4),
					 	span-4 (5),
					 	span-5 (6),
					 	span-6 (7)
						}
		ACCESS			read-write
		STATUS			mandatory
		DESCRIPTION		"Identifies the unit's primary clock
					source.  The first SPAN port - (2)
					SPAN-1; the second SPAN port - (3)
					SPAN-2; the third SPAN port  - (4)
					SPAN-3; the fourth SPAN port - (5)
					SPAN-4; the fifth SPAN port  - (6)
					SPAN-5; the sixth SPAN port  - (7)
					SPAN-6."
		::= { qds1-e1 2 }

	qds1PrimaryClockSourceAction	OBJECT-TYPE
		SYNTAX			INTEGER	{
					 	use-current-source (1),
					 	restore-to-primary-source (2)
						}
		ACCESS			read-write
		STATUS			mandatory
		DESCRIPTION		"Instructs the Span unit to (1)
					USE-CURRENT-SOURCE - stay with the
					current source clock, or (2)
					RETURN-TO-PRIMARY-SOURCE - restore the
					clock to the primary clock source."
		::= { qds1-e1 3 }

-- DS1/E1 Span Configuration Table
	qds1SpanCfgTable	OBJECT-TYPE
		SYNTAX		   	SEQUENCE OF Qds1SpanCfgEntry
		ACCESS			not-accessible
		STATUS			mandatory
		DESCRIPTION		"A list of Span Configuration entries.
					The maximum number is based on the
					maximum interfaces."
		::= {qds1-e1 4}

   	qds1SpanCfgEntry 	OBJECT-TYPE
		SYNTAX			Qds1SpanCfgEntry
		ACCESS			not-accessible
		STATUS			mandatory
		DESCRIPTION		"The DS1/E1 configuration information
					for each DS1/E1 interface."
		INDEX   { qds1LineCfgIndex }
   		::= { qds1SpanCfgTable 1 }

	Qds1SpanCfgEntry ::=
		SEQUENCE {
		        qds1LineCfgIndex
		            INTEGER,
		        qds1LineBuildOut
		            INTEGER,
		        qds1ClockSourceEligibility
		            INTEGER,
		        qds1ClockMode
		            INTEGER,
		        qds1PerformanceMonitor
		            INTEGER,
		        qds1Scrambler
		            INTEGER
			}

	qds1LineCfgIndex	OBJECT-TYPE
		SYNTAX			INTEGER
		ACCESS			read-only
		STATUS			mandatory
		DESCRIPTION		"A unique value which identifies a
					DS1/E1 interface.  This value 'should'
					be made equal to the ifIndex, however,
					this may not be the case, but will have
					an internal association with the
					ifEntry."
		::= { qds1SpanCfgEntry 1 }

	qds1LineBuildOut	OBJECT-TYPE
		SYNTAX			INTEGER{
					       	line-build-out-133-ft (1),
					       	line-build-out-266-ft (2),
					       	line-build-out-399-ft (3),
					       	line-build-out-533-ft (4),
					       	line-build-out-655-ft (5),
					       	line-build-out-n7-5db (6),
					       	line-build-out-n15db (7),
					       	line-build-out-n22-5db (8),
							not-applicable			(9)
					       }
		ACCESS			read-write
		STATUS			mandatory
		DESCRIPTION		"The Line Build Out specification for
					the DS1/E1 interface.  NOTE: (1)
					represents not only the
					LINE-BUILD-OUT-133-FT, but also 0 db
					CSU."
		::= { qds1SpanCfgEntry 2 }

	qds1ClockSourceEligibility	OBJECT-TYPE
		SYNTAX			INTEGER	{ 
						eligible (1),
						ineligible (2)
						}
		ACCESS			read-write
		STATUS			mandatory
		DESCRIPTION		"Identifies whether the DS1/E1
					interface is eligible to be the source
					clock or not."
		::= { qds1SpanCfgEntry 3 }

	qds1ClockMode			OBJECT-TYPE
		SYNTAX			INTEGER	{
						 slavetonetwork (1),
						 providemasterclock (2)
						 }
		ACCESS			read-write
		STATUS			mandatory
		DESCRIPTION		"Identifies the clock's mode of
					operation.  Setting the clock mode to
					(1) will allow the interface to act as
					the slave to the network clock, and
					setting the mode to (2) will allow the
					unit's interface to provide the master
					clock."
		::= { qds1SpanCfgEntry 4 }

	qds1PerformanceMonitor		OBJECT-TYPE
		SYNTAX			INTEGER	{ enable (1), disable (2) }
		ACCESS			read-write
		STATUS			optional
		DESCRIPTION		"Indicates whether or not performance
               				monitoring will be transmitted on this
					interface. ENABLE (1) will transmit the
					performance report according to the ANSI
					T1.403-1995 standard.  The performance
					report contains performance information
					for each of the four previous one-second
					intervals."
		::= { qds1SpanCfgEntry 5 }

	qds1Scrambler			OBJECT-TYPE
		SYNTAX			INTEGER	{ enable (1), disable (2) }
		ACCESS			read-write
		STATUS			optional
		DESCRIPTION		"Indicates whether or not scrambling
               				of data is being generated on this
					interface. ENABLE (1) will transmit the
					scrambled data according to the ITU-T
					0.191 Recommendation; DISABLE (2) will
					not scramble the data.  NOTE: A T1
					interface will NOT support scrambling,
					and therefore will be DISABLED."
		::= { qds1SpanCfgEntry 6 }

-- DS1/E1 Span Loopback Command/Configuration Table
	qds1SpanLoopBackCmdTable	OBJECT-TYPE
		SYNTAX		   	SEQUENCE OF Qds1SpanLoopBackCmdEntry
		ACCESS			not-accessible
		STATUS			mandatory
		DESCRIPTION		"A list of Span Loopback Configuration
					entries.  The maximum number is based
					on the maximum DS1/E1 interfaces."
		::= {qds1-e1 5}

   	qds1SpanLoopBackCmdEntry	OBJECT-TYPE
		SYNTAX			Qds1SpanLoopBackCmdEntry
		ACCESS			not-accessible
		STATUS			mandatory
		DESCRIPTION		"The DS1/E1 loopback configuration
					information for each DS1/E1 interface."
		INDEX   { qds1LineLoopbackIndex }
   		::= { qds1SpanLoopBackCmdTable 1 }

	Qds1SpanLoopBackCmdEntry ::=
		SEQUENCE {
		        qds1LineLoopbackIndex
		            INTEGER,
		        qds1LoopbackCommand
		            INTEGER,
		        qds1LoopbackDuration
		            INTEGER
			}

	qds1LineLoopbackIndex	OBJECT-TYPE
		SYNTAX			INTEGER
		ACCESS			read-only
		STATUS			mandatory
		DESCRIPTION		"A unique value which identifies DS1/E1
					interface.  This value should be made
					equal to the ifIndex."
		::= { qds1SpanLoopBackCmdEntry 1 }

	qds1LoopbackCommand	OBJECT-TYPE
		SYNTAX			INTEGER {
						activate (1),
						deactivate (2)
						}
		ACCESS			read-write
		STATUS			mandatory
		DESCRIPTION		"This object is used to (1) ACTIVATE
					the configured loopback on the selected
					interface (qds1LineIndex) of the
					specified entity.  If the dsx1OtherLoop
					(4) has been configured (see rfc2495 -
					dsx1LoopbackConfig), the supported
					loopback is the Local Port Loopback
					(LPL).  The Local Port Loopback is used
					to validate the ability to send and
					receive data to the local DS1/E1
					interface.  LPL will use OAM flow to
					verify data transfer.  Normal OAM flows
					are discontinued and an F4 OAM segment
					flow is automatically started on VPI=0,
					VCI=3 with a repeat timer of 1 second.
					When the loopback is set to (2)
					DEACTIVATE or comes to completion, the
					previous OAM configuration flows are
					restored."
						
		::= { qds1SpanLoopBackCmdEntry 2 }

	qds1LoopbackDuration	OBJECT-TYPE
		SYNTAX			INTEGER	(1..3600)
		ACCESS			read-write
		STATUS			mandatory
		DESCRIPTION		"The value indicates the duration (in
					SECONDS) that the Loopback will be in
					process."
		::= { qds1SpanLoopBackCmdEntry 3 }

-- DS1/E1 Span Loopback Statisics Table
	qds1SpanLoopBackStatsTable	OBJECT-TYPE
		SYNTAX		   	SEQUENCE OF Qds1SpanLoopBackStatsEntry
		ACCESS			not-accessible
		STATUS			mandatory
		DESCRIPTION		"A list of Span Loopback Statistics for
					each entries.  The maximum number is
					based on the maximum DS1/E1
					interfaces."
		::= {qds1-e1 6}

   	qds1SpanLoopBackStatsEntry	OBJECT-TYPE
		SYNTAX			Qds1SpanLoopBackStatsEntry
		ACCESS			not-accessible
		STATUS			mandatory
		DESCRIPTION		"The DS1/E1 loopback statistics
					information foreach DS1/E1 interface."
		INDEX   { qds1LineLoopbackStatIndex }
   		::= { qds1SpanLoopBackStatsTable 1 }

	Qds1SpanLoopBackStatsEntry ::=
		SEQUENCE {
		        qds1LineLoopbackStatIndex
		            INTEGER,
		        qds1OamSegmentSent
		            Counter,
		        qds1OamSegmentReceived
		            Counter,
		        qds1OamSegmentFailures
		            Counter,
				qds10amRunnigTime
					Counter
			}

	qds1LineLoopbackStatIndex	OBJECT-TYPE
		SYNTAX			INTEGER
		ACCESS			read-only
		STATUS			mandatory
		DESCRIPTION		"A unique value which identifies DS1/E1
					interface.  This value should be made
					equal to the ifIndex."
		::= { qds1SpanLoopBackStatsEntry 1 }

	qds1OamSegmentSent	OBJECT-TYPE
		SYNTAX			Counter
		ACCESS			read-only
		STATUS			mandatory
		DESCRIPTION		"The total number of segment OAM
					messages transmitted on the DS1/E1
					interface in question for the duration
					of the test." 
		::= { qds1SpanLoopBackStatsEntry 2 }

	qds1OamSegmentReceived	OBJECT-TYPE
		SYNTAX			Counter
		ACCESS			read-only
		STATUS			mandatory
		DESCRIPTION		"The total number of segment OAM
					messages received on the DS1/E1
					interface in question for the duration
					of the test." 
		::= { qds1SpanLoopBackStatsEntry 3 }

	qds1OamSegmentFailures	OBJECT-TYPE
		SYNTAX			Counter
		ACCESS			read-only
		STATUS			mandatory
		DESCRIPTION		"The total number of segment OAM
					messages timed out on the DS1/E1
					interface in question for the duration
					of the test." 
		::= { qds1SpanLoopBackStatsEntry 4 }

	qds10amRunnigTime	OBJECT-TYPE
		SYNTAX			Counter
		ACCESS			read-only
		STATUS			mandatory
		DESCRIPTION		"This is running time for the loop back." 
		::= { qds1SpanLoopBackStatsEntry 5 }

-- QWES.com Configuration Manager group
-- This group contains the MIB objects used to manage the Configuration
-- Manager.
-- Configuration Manager group { oem 13 }

	qcmSystemFile		OBJECT-TYPE
		SYNTAX			DisplayString (SIZE (0..20))
		ACCESS			read-write
		STATUS			mandatory
		DESCRIPTION				"The name of the configuration file to edit."
		::= { qconfigManager 1 }

	qcmConfigMaxEntries	OBJECT-TYPE
		SYNTAX			Counter
		ACCESS			read-only
		STATUS			mandatory
		DESCRIPTION		"The number of Configuration entries."
		::= { qconfigManager 2 }

-- Config Table
   	qcmConfigTable		 OBJECT-TYPE
		SYNTAX		 	SEQUENCE OF QcmConfigEntry
		ACCESS		 	not-accessible
		STATUS		 	mandatory
		DESCRIPTION				"A list of configuration file entries. The number of
							entries is given by the value of qcmConfigMaxEntries."
		::= { qconfigManager 3 }

   	qcmConfigEntry		 OBJECT-TYPE
		SYNTAX		 	QcmConfigEntry
		ACCESS		 	not-accessible
		STATUS		 	mandatory
		INDEX		 	{qcmTableIndex}
   		::= { qcmConfigTable 1 }

	QcmConfigEntry ::=
		SEQUENCE {
		        qcmTableIndex
		            INTEGER,
		        qcmCurrentConfig
		            INTEGER,
		        qcmActionOnConfig
		            INTEGER,
		        qcmFileAlias
		            DisplayString,
		        qcmRelease
		            DisplayString,
		        qcmConfigFile
		            DisplayString,
		        qcmBootSequence
		            DisplayString,
		        qcmCreateDateTime
		            DisplayString,
		        qcmOperSchedDateTime
		            DisplayString
		  }

	qcmTableIndex 		 OBJECT-TYPE
		SYNTAX		 	INTEGER (1..65535)
		ACCESS		 	read-write
		STATUS		 	mandatory
		DESCRIPTION	 	"A unique value for each config file."
		::= { qcmConfigEntry 1 }

	qcmCurrentConfig	 OBJECT-TYPE
		SYNTAX					INTEGER {
					       	no(0),
					       	yes(1)
					       	}
		ACCESS			read-write
		STATUS			mandatory
		DESCRIPTION				"Flag that indicates if this record is the current operational configuration."
		::= { qcmConfigEntry 2 }

	qcmActionOnConfig 	OBJECT-TYPE
		SYNTAX					INTEGER {
					       	none(0),
					       	delete(1),
					       	activatenow(2),
					       	activatelater(3),
									save(4),
									saveas(5)
								}
		ACCESS					read-write
		STATUS					mandatory
		DESCRIPTION				"Operations that can be performed on the current table record."
		::= { qcmConfigEntry 3 }

	qcmFileAlias		OBJECT-TYPE
		SYNTAX			DisplayString (SIZE (0..20))
		ACCESS			read-write
		STATUS			mandatory
		DESCRIPTION				"The file alias name of the 'release' / 'configuration file' pair."
		::= { qcmConfigEntry 4 }

	qcmRelease		OBJECT-TYPE
		SYNTAX			DisplayString (SIZE (0..20))
		ACCESS			read-write
		STATUS			mandatory
		DESCRIPTION		"The name of the release file (executable)."
		::= { qcmConfigEntry 5 }

	qcmConfigFile		OBJECT-TYPE
		SYNTAX			DisplayString (SIZE (0..20))
		ACCESS			read-write
		STATUS			mandatory
		DESCRIPTION		"The name of the configuration file."
		::= { qcmConfigEntry 6 }

	qcmBootSequence		OBJECT-TYPE
		SYNTAX			DisplayString (SIZE (0..20))
		ACCESS			read-write
		STATUS			mandatory
		DESCRIPTION		"The boot sequence."
		::= { qcmConfigEntry 7 }

	qcmCreateDateTime	OBJECT-TYPE
		SYNTAX			DisplayString (SIZE (0..30))
		ACCESS			read-write
		STATUS			mandatory
		DESCRIPTION				"The creation date and time of the configuration file."
		::= { qcmConfigEntry 8 }

	qcmOperSchedDateTime	OBJECT-TYPE
		SYNTAX			DisplayString (SIZE (0..30))
		ACCESS			read-write
		STATUS			mandatory
		DESCRIPTION				"The date and time the configuration file became operational."
		::= { qcmConfigEntry 9 }
-- End of Config Table

-- Release Table
   	qcmReleaseTable		 OBJECT-TYPE
		SYNTAX		 	SEQUENCE OF QcmReleaseEntry
		ACCESS		 	not-accessible
		STATUS		 	mandatory
		DESCRIPTION	 	"A list of Release file entries."
		::= { qconfigManager 4 }

   	qcmReleaseEntry		 OBJECT-TYPE
		SYNTAX		 	QcmReleaseEntry
		ACCESS		 	not-accessible
		STATUS		 	mandatory
		INDEX		 	{qcmReleaseIndex}
   		::= { qcmReleaseTable 1 }

	QcmReleaseEntry ::=
		SEQUENCE {
		        qcmReleaseIndex
		            INTEGER,
		        qcmReleaseName
		            DisplayString,
		        qcmReleaseVersion
		            INTEGER
		  }

	qcmReleaseIndex 	 OBJECT-TYPE
		SYNTAX		 	INTEGER (1..65535)
		ACCESS		 	read-only
		STATUS		 	mandatory
		DESCRIPTION	 	"A unique value for each Release file."
		::= { qcmReleaseEntry 1 }

	qcmReleaseName 		OBJECT-TYPE
		SYNTAX			DisplayString (SIZE (1..20))
		ACCESS			read-only
		STATUS			mandatory
		DESCRIPTION		"The name of a release file."
		::= { qcmReleaseEntry 2 }

	qcmReleaseVersion	OBJECT-TYPE
		SYNTAX			INTEGER (1..4294967295)
		ACCESS			read-only
		STATUS			mandatory
		DESCRIPTION		"The version of the release file."
		::= { qcmReleaseEntry 3 }
-- End of Release Table

-- Config Version Table
   	qcmConfigVerTable 	OBJECT-TYPE
		SYNTAX			SEQUENCE OF QcmConfigVerEntry
		ACCESS			not-accessible
		STATUS			mandatory
		DESCRIPTION		"A list of Config file entries."
		::= { qconfigManager 5 }

   	qcmConfigVerEntry   	 OBJECT-TYPE
		SYNTAX		 	QcmConfigVerEntry
		ACCESS		 	not-accessible
		STATUS		 	mandatory
		INDEX		 	{qcmConfigVerIndex}
   		::= { qcmConfigVerTable 1 }

	QcmConfigVerEntry ::=
		SEQUENCE {
		        qcmConfigVerIndex
		            INTEGER,
		        qcmConfigName
		            DisplayString,
		        qcmConfigVersion
		            INTEGER,
		        qcmConfigWorkWithMin
		            INTEGER,
		        qcmConfigWorkWithMax
		            INTEGER
		  }

	qcmConfigVerIndex	 OBJECT-TYPE
		SYNTAX		 	INTEGER (1..65535)
		ACCESS		 	read-only
		STATUS		 	mandatory
		DESCRIPTION	 	"A unique value for each Config file."
		::= { qcmConfigVerEntry 1 }

	qcmConfigName 		 OBJECT-TYPE
		SYNTAX		 	DisplayString (SIZE (1..20))
		ACCESS		 	read-only
		STATUS		 	mandatory
		DESCRIPTION	 	"The name of a Config file."
		::= { qcmConfigVerEntry 2 }

	qcmConfigVersion	 OBJECT-TYPE
		SYNTAX		 	INTEGER (1..4294967295)
		ACCESS		 	read-only
		STATUS		 	mandatory
		DESCRIPTION	 	"The version of the Config file."
		::= { qcmConfigVerEntry 3 }

	qcmConfigWorkWithMin	 OBJECT-TYPE
		SYNTAX		 	INTEGER (1..4294967295)
		ACCESS		 	read-only
		STATUS		 	mandatory
		DESCRIPTION				"The minimum version of a Release file the Config file is compatable with."
		::= { qcmConfigVerEntry 4 }

	qcmConfigWorkWithMax	 OBJECT-TYPE
		SYNTAX		 	INTEGER (1..4294967295)
		ACCESS		 	read-only
		STATUS		 	mandatory
		DESCRIPTION				"The maximum version of a Release file the Config file is compatable with."
		::= { qcmConfigVerEntry 5 }
-- End of Config Version Table

	qcmWhichFile		OBJECT-TYPE
		SYNTAX					INTEGER
								{
					       	operational(0),
					       	base(1),
					       	new(2),
					       	default(3)
					       }
		ACCESS			read-write
		STATUS			mandatory
		DESCRIPTION				"Which file to do SNMP GETs and SETS on."
		::= { qconfigManager 6 }

	qcmUserBaseCfgFile	OBJECT-TYPE
		SYNTAX			DisplayString (SIZE (0..20))
		ACCESS			read-write
		STATUS			mandatory
		DESCRIPTION				"The name of the configuration file to edit."
		::= { qconfigManager 7 }

	qcmUserNewCfgFile 	OBJECT-TYPE
		SYNTAX			DisplayString (SIZE (0..20))
		ACCESS			read-write
		STATUS			mandatory
		DESCRIPTION				"The name of the new configuration file."
		::= { qconfigManager 8 }

	qcmUserFileAlias				OBJECT-TYPE
		SYNTAX					DisplayString (SIZE (0..20))
		ACCESS					read-write
		STATUS					mandatory
		DESCRIPTION				"The file alias of the system file."
		::= { qconfigManager 9 }

	qcmUserRelease 				OBJECT-TYPE
		SYNTAX					DisplayString (SIZE (0..20))
		ACCESS					read-write
		STATUS					mandatory
		DESCRIPTION				"The name of the release file."
		::= { qconfigManager 10 }

	qcmUserBootSequence			OBJECT-TYPE
		SYNTAX					DisplayString (SIZE (0..40))
		ACCESS					read-write
		STATUS					mandatory
		DESCRIPTION				"The boot sequence."
		::= { qconfigManager 11 }

	qcmUserDateTime				OBJECT-TYPE
		SYNTAX					DisplayString (SIZE (0..30))
		ACCESS					read-write
		STATUS					mandatory
		DESCRIPTION				"The date and time LDF is be scheduled."
		::= { qconfigManager 12 }

	qcmStartConfig 		OBJECT-TYPE
		SYNTAX			INTEGER {
									no(0),		-- do not start configuration
						yes(1) -- start configuration
						}
		ACCESS			read-write
		STATUS			mandatory
		DESCRIPTION		"Start the configuration process."
		::= { qconfigManager 13 }

	qcmStopConfig 		OBJECT-TYPE
		SYNTAX			INTEGER {
									no(0),		-- do not stop configuration
									yes(1)		-- stop and save configuration
								}
		ACCESS	   				read-write
		STATUS	   				mandatory
		DESCRIPTION				"Stop and Save the current configuration process."
		::= { qconfigManager 14 }

	qcmStartViewConfig 			OBJECT-TYPE
		SYNTAX					INTEGER {
									no(0),		-- do not start viewing a configuration file
									yes(1) 		-- start viewing a configuration file
								}
		ACCESS					read-write
		STATUS					mandatory
		DESCRIPTION				"Start viewing a configuration file."
		::= { qconfigManager 15 }

	qcmStopViewConfig 			OBJECT-TYPE
		SYNTAX					INTEGER {
									no(0),		-- do not stop viewing configuration file
									yes(1)		-- stop viewing configuration file
								}
		ACCESS					read-write
		STATUS					mandatory
		DESCRIPTION				"Stop viewing a configuration file."
		::= { qconfigManager 16 }

	qcmCancelConfig 	OBJECT-TYPE
		SYNTAX			INTEGER {
						 no(0),	-- do not start configuration
						 yes(1)	-- start configuration
						}
		ACCESS			read-write
		STATUS			mandatory
		DESCRIPTION				"Cancel the current configuration process."
		::= { qconfigManager 17 }


-- Port Group { oem 15 }
-- QWES.com LP table { qportManagement 1 }
--
-- This group contains the definitions for all MIB objects used to
-- configure an Logical Port (LP).

-- qwesLpTable table
   	qwesLpTable		OBJECT-TYPE
		SYNTAX			SEQUENCE OF QwesLpEntry
		ACCESS			not-accessible
		STATUS			mandatory
		DESCRIPTION		"A list of Logical Port entries."
		::= { qportManagement 1 }

   	qwesLpEntry 		OBJECT-TYPE
		SYNTAX			QwesLpEntry
		ACCESS			not-accessible
		STATUS			mandatory
		DESCRIPTION		"One entry in the table."
		INDEX   { ifIndex }
   		::= { qwesLpTable 1 }

	QwesLpEntry ::=
		SEQUENCE {
		        qwesLpName	 	DisplayString,
		        qwesLpAlias 		DisplayString,
		        qwesLpPhysicalInterface	INTEGER,
		        qwesLpDataLinkInterface	INTEGER,
		        qwesLpTrapStatus	INTEGER,
				qwesLpAdminStatus	INTEGER,
				qwesLpRowStatus	RowStatus
		  }

--
-- Logical Port Entry fields
--
   qwesLpName OBJECT-TYPE
       SYNTAX      DisplayString (SIZE (0..255))
       MAX-ACCESS  read-write
       STATUS      current
       DESCRIPTION
               "Display the name for the Logical Port."
       ::= { qwesLpEntry 1 }

   qwesLpAlias OBJECT-TYPE
       SYNTAX      DisplayString (SIZE (0..255))
       MAX-ACCESS  read-write
       STATUS      current
       DESCRIPTION
               "Enter an alias for the Logical Port Inteface name that has significant
               	to your organization."
       ::= { qwesLpEntry 2 }

	qwesLpPhysicalInterface OBJECT-TYPE
		SYNTAX			INTEGER (1..16)	-- See
						-- QWES-PHYSICAL-INTERFACE-MIN/MAX
		ACCESS			read-write
		STATUS			mandatory
		DESCRIPTION		"The physical port that this Logical
					Port associated."
		::= { qwesLpEntry 3 }

	qwesLpDataLinkInterface OBJECT-TYPE
		SYNTAX			INTEGER (25..88)-- See
						-- QWES-AAL5-INTERFACE-MIN/MAX
		ACCESS			read-write
		STATUS			mandatory
		DESCRIPTION		"The data link port that this Logical
					Port associated."
		::= { qwesLpEntry 4 }

	qwesLpTrapStatus 	OBJECT-TYPE
        SYNTAX  			INTEGER {
                   				yes(1), 
                   				no(2)
               					}
		ACCESS			read-write
		STATUS			mandatory
		DESCRIPTION		"Indicates whether the Link Up/Link
					Down traps should be generated for this
					interface."
		::= { qwesLpEntry 5 }

	qwesLpAdminStatus 	OBJECT-TYPE
        SYNTAX  			INTEGER {
                   				up(1), 
                   				down(2)
               					}
		ACCESS			read-write
		STATUS			mandatory
		DESCRIPTION		"The desired state of the interface set by management."
		::= { qwesLpEntry 6 }

	qwesLpRowStatus 	OBJECT-TYPE
        SYNTAX  			RowStatus
        ACCESS				read-write
		STATUS			mandatory
		DESCRIPTION		"This object is used to create a new
					row or modify or delete an existing row
					in this table."
		::= { qwesLpEntry 7 }


-- QWES.com LIF table { qportManagement 2 }
--
-- This group contains the definitions for all MIB objects used to
-- configure an Logical Interface (LIF).
-- qwesLifTable table
   	qwesLifTable		OBJECT-TYPE
		SYNTAX			SEQUENCE OF QwesLifEntry
		ACCESS			not-accessible
		STATUS			mandatory
		DESCRIPTION		"A list of Logical Interface entries."
		::= { qportManagement 2 }

   	qwesLifEntry 		OBJECT-TYPE
		SYNTAX			QwesLifEntry
		ACCESS			not-accessible
		STATUS			mandatory
		DESCRIPTION		"One entry in the table."
		INDEX   { ifIndex }
   		::= { qwesLifTable 1 }

	QwesLifEntry ::=
		SEQUENCE {
		        qwesLifName		DisplayString,
		        qwesLifAlias 		DisplayString,
		        qwesLifLpIndex		INTEGER,
		        qwesLifTrapStatus	INTEGER,
				qwesLifAdminStatus	INTEGER,
		        qwesLifRowStatus	RowStatus
		  }

--
-- Logical Inteface Entry fields
--
	qwesLifName		OBJECT-TYPE
		SYNTAX 			DisplayString (SIZE (0..255))
		MAX-ACCESS  		read-write
		STATUS      		current
		DESCRIPTION		"The name for the Logical Port."
		::= { qwesLifEntry 1 }

	qwesLifAlias		OBJECT-TYPE
		SYNTAX      		DisplayString (SIZE (0..255))
       		MAX-ACCESS  read-write
       		STATUS      current
       		DESCRIPTION		"The alias for the logical inteface
					name that has significant to your
					organization."
       		::= { qwesLifEntry 2 }

	qwesLifLpIndex		OBJECT-TYPE
       		SYNTAX      		INTEGER (257..512) -- See 
						-- QWES-LP-INTERFACE-MIN/MAX
		ACCESS			read-write
		STATUS			mandatory
		DESCRIPTION		"The logical port (LP) number that this
					logical interface has an association
					with."
		::= { qwesLifEntry 3 }

	qwesLifTrapStatus 	OBJECT-TYPE
        	SYNTAX  		INTEGER {
                   				yes(1), 
                   				no(2)
               					}
		ACCESS			read-write
		STATUS			mandatory
		DESCRIPTION		"Indicates whether the Link Up/Link
					Down traps should be generated for this
					interface."
		::= { qwesLifEntry 4 }

	qwesLifAdminStatus 	OBJECT-TYPE
        	SYNTAX  		INTEGER {
                   				up(1), 
                   				down(2)
               					}
		ACCESS			read-write
		STATUS			mandatory
		DESCRIPTION		"The desired state of the interface set by management."
		::= { qwesLifEntry 5 }

	qwesLifRowStatus 	OBJECT-TYPE
        	SYNTAX  		RowStatus
        	ACCESS			read-write
		STATUS			mandatory
		DESCRIPTION		"This object is used to create a new
					row or modify or delete an existing row
					in this table."
		::= { qwesLifEntry 6 }


