						enabled (1), 	
						disabled (2) 	
						}
		ACCESS			read-write
		STATUS			mandatory
		DESCRIPTION		"Indicates whether the Link Up/Link
					Down traps should be generated for this
					interface."
		::= { qatmUniEntry 4 }

	qatmUniMaxQueueDelay	OBJECT-TYPE
		SYNTAX			INTEGER
		ACCESS			read-write
		STATUS			mandatory
		DESCRIPTION		"xxxx"
		::= { qatmUniEntry 5 }

	qatmUniMinAllocBuffer	OBJECT-TYPE
