   	qIgmpGroupTable	OBJECT-TYPE
		SYNTAX			SEQUENCE OF QWESGroupIgmpEntry
		MAX-ACCESS		not-accessible
		STATUS			current
		DESCRIPTION		"A list of IGMP Group entries."
		::= { qigmp 8 }

   	qIgmpGroupEntry	    OBJECT-TYPE
		SYNTAX			QWESGroupIgmpEntry
		MAX-ACCESS		not-accessible
		STATUS			current
		DESCRIPTION		"One entry in the table."
		INDEX   { qIgmpGroupLp, qIgmpGroupAddr }
   		::= { qIgmpGroupTable 1 }

	QWESGroupIgmpEntry ::=
