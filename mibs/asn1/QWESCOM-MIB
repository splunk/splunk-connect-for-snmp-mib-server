	qEventSystemDateTime			OBJECT-TYPE
		SYNTAX			DisplayString
		MAX-ACCESS		read-only
		STATUS			mandatory
		DESCRIPTION		"The time of day an event occurs
					 SNMP Community."
		::= { qEventSystemEntry 2 }

	qEventSystemType		OBJECT-TYPE
		SYNTAX			INTEGER {
						fatal(1),
						critical(2),
						mild(3),
						standard-snmp(4),
						informational(5),
						debug(6)
					}
		MAX-ACCESS		read-only
		STATUS			mandatory
		DESCRIPTION		"This value will be used to identify the 
					  Event Type to be displayed"
		::= { qEventSystemEntry 3}

	qEventSystemSubSystem			OBJECT-TYPE
		SYNTAX			INTEGER {
						general-subsystem(1),
						ip-subsystem(2),
