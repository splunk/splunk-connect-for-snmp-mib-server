--
--  qmanagementGroup   { oem 17 }
--
--  Management Group MIB definitions place holder.....  
-- 

--
-- OAM  Group { oem 18 }
-- QWES.com OAM Configuration Group { qOamGroup 1 }
--
-- This group contains the definitions for all MIB objects used to
-- configure the system OAM.

--
-- This group defines OAM group configuration such as "all circuit,
--		all uni1, all uni2, all uni3, all vpl, and all vcl."
-- 
-- qoamGroupConfigTable table
   	qoamGroupConfigTable		OBJECT-TYPE
		SYNTAX			SEQUENCE OF QOAMGROUPCONFIGENTRY
		ACCESS			not-accessible
		STATUS			mandatory
		DESCRIPTION		"A list of OAM group Configurable Parameters entries."
		::= { qOamGroup 1 }

   	qoamGroupConfigEntry 	OBJECT-TYPE
		SYNTAX			QOAMGROUPCONFIGENTRY
		ACCESS			not-accessible
		STATUS			mandatory
		DESCRIPTION		"One entry in the table."
		INDEX   { qoamGroupConfigIndex }
   		::= { qoamGroupConfigTable 1 }

	QOAMGROUPCONFIGENTRY ::=
		SEQUENCE {
		        qoamGroupConfigIndex	 	        INTEGER,
		        qoamGroupConfigAlias	 	        DisplayString,
		        qoamGroupConfigType  	 	        INTEGER,
		        qoamGroupConfigTransmitInterval	 	INTEGER,
		        qoamGroupConfigS2SAlarmThreshold	 	INTEGER,
		        qoamGroupConfigE2EAlarmThreshold	 	INTEGER,
				qoamGroupConfigStatus			 	INTEGER

		  }
--
-- OAM Configuration Entry fields
--
   qoamGroupConfigIndex	OBJECT-TYPE
	   SYNTAX		INTEGER
	   MAX-ACCESS 	read-only
       STATUS       current
       DESCRIPTION
               "The entry index of a particular group.  Currently, there
               	are 6 groups define: 
               			1) All circuit
               			2) All Uni1
               			3) All Uni2,
               			4) All Uni3,
               			5) All Vpl,
               			6) All Vcl."
       ::= { qoamGroupConfigEntry 1 }

   qoamGroupConfigAlias		OBJECT-TYPE
	   SYNTAX		        DisplayString (SIZE (0..255))
       MAX-ACCESS 	        read-only
       STATUS       current
       DESCRIPTION
               "Alias name of a specified circuit or for all configured UNIs,
               1=end-to-end, 2=segment-to-segment, 3=both."
       ::= { qoamGroupConfigEntry 2 }

   qoamGroupConfigType	OBJECT-TYPE
	   SYNTAX		INTEGER {
						segment-2-segment(1),
						end-2-end(2),
						both(3)
					}
       MAX-ACCESS 	read-write
       STATUS       current
       DESCRIPTION
               "The type of OAM to be generated."
       DEFVAL { 3 }
       ::= { qoamGroupConfigEntry 3 }

   qoamGroupConfigTransmitInterval	OBJECT-TYPE
	   SYNTAX		INTEGER (1..65535)	   
	   MAX-ACCESS 	read-write
       STATUS       current
       DESCRIPTION
               "The interval between OAM transmission in seconds."
       DEFVAL { 1 }
       ::= { qoamGroupConfigEntry 4 }

   qoamGroupConfigS2SAlarmThreshold	OBJECT-TYPE
	   SYNTAX		INTEGER (1..65535)	   
	   MAX-ACCESS 	read-write
       STATUS       current
       DESCRIPTION
               "The number of timed out segment-to-segment OAM replies 
               accumulated in sequence before an alarm is generated."
       DEFVAL { 5 }
       ::= { qoamGroupConfigEntry 5 }

   qoamGroupConfigE2EAlarmThreshold	OBJECT-TYPE
	   SYNTAX		INTEGER (1..65535)	   
	   MAX-ACCESS 	read-write
       STATUS       current
       DESCRIPTION
               "The number of timed out end-to-end OAM replies accumulated in 
               	sequence before an alarm is generated."
       DEFVAL { 5 }
       ::= { qoamGroupConfigEntry 6 }

   qoamGroupConfigStatus	OBJECT-TYPE
	   SYNTAX		INTEGER {
						enable(1),
						disable(2)
					}
       MAX-ACCESS 	read-write
       STATUS       current
       DESCRIPTION
               "Enable/disable OAM operation."
       DEFVAL { 1 }
       ::= { qoamGroupConfigEntry 7 }


