						reset-subsystem(14), 
						time-subsystem(15),
						icmp-subsystem(16),
						igmp-subsystem(17),
						ping-subsystem(18),
						snmp-subsystem(19),
						tcp-subsystem(20),
						upd-subsystem(21),
						lp-subsystem(22),
						lif-subsystem(23),
						circuit-subsystem(24),
						arp-subsystem(25)
					}
		MAX-ACCESS		read-only
		STATUS			mandatory
		DESCRIPTION		"This value will be used to identify the 
					 Event Type to be displayed"
		::= { qEventSystemEntry 4}

	qEventSystemID-Number		OBJECT-TYPE
