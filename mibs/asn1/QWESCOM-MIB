	qatmVclBwGranularity 	OBJECT-TYPE
		SYNTAX			INTEGER
		ACCESS			read-write
		STATUS			mandatory
		DESCRIPTION		"The bandwidth granularity can be apply
					to this VCC."
		::= { qatmVclEntry 1 }

	qatmVclMaxSubChannels 	OBJECT-TYPE
		SYNTAX			INTEGER	 (0..65535)
		ACCESS			read-write
		STATUS			mandatory
		DESCRIPTION		"The maximum number of sub-channels
					supported on this Virtual Circuit."
		::= { qatmVclEntry 2 }

	qatmVclMaxQueueDelay 	OBJECT-TYPE
		SYNTAX			INTEGER
		ACCESS			read-write
		STATUS			mandatory
		DESCRIPTION		"The maximum queueing delay in
					milliseconds (msec).  Combining this
					value with the sustained rate will
					determine the maximum buffering
					allowed."
		::= { qatmVclEntry 3 }

	qatmVclReservedBuffer 	OBJECT-TYPE
		SYNTAX			INTEGER
		ACCESS			read-write
		STATUS			mandatory
		DESCRIPTION		"The minimum reserved buffer in bytes
					that this VCC will be allocated to
					use."
		::= { qatmVclEntry 4 }

	qatmVclSharedBuffer	 OBJECT-TYPE
		SYNTAX			INTEGER
		ACCESS			read-write
		STATUS			mandatory
		DESCRIPTION		"The amount of buffering to be assigned
					to a VCC for sharing on a First In
					First Out (FIFO) basis between flows on
					this VCC."
		::= { qatmVclEntry 5 }

-- qatmAal5Table table
   	qatmAal5Table		OBJECT-TYPE
		SYNTAX			SEQUENCE OF QatmAal5Entry
		ACCESS			not-accessible
		STATUS			mandatory
		DESCRIPTION		"The ATM Adaptation Layer 5 (Aal5) table."
		::= { qatm 5 }

   	qatmAal5Entry 		OBJECT-TYPE
		SYNTAX			QatmAal5Entry
		ACCESS			not-accessible
		STATUS			mandatory
		DESCRIPTION		"One entry in the table."
		INDEX  {ifIndex}
   		::= { qatmAal5Table 1 }

	QatmAal5Entry ::=
		SEQUENCE {
		        qatmAal5Uni
		            INTEGER,
		        qatmAal5Aal5Name
		            DisplayString,
		        qatmAal5Aal5Alias
		            DisplayString,
		        qatmAal5AdminStatus
		            INTEGER,
		        qatmAal5TrapStatus
		            INTEGER,
		        qatmAal5MtuSize
		            INTEGER,
		        qatmAal5RowStatus
		            INTEGER
		  }

-- ATM Aal5 Entry fields
	qatmAal5Uni 	OBJECT-TYPE
  	    SYNTAX		INTEGER (1..25)	   
		ACCESS			read-write
		STATUS			mandatory
		DESCRIPTION		"The UNI port alias associates with
						this AAL5 interface."
		::= { qatmAal5Entry 1 }

	qatmAal5Aal5Name 	OBJECT-TYPE
		SYNTAX			DisplayString (SIZE (0..255))
		ACCESS			read-write
		STATUS			mandatory
		DESCRIPTION		"The AAL5 port name."
		::= { qatmAal5Entry 2 }

	qatmAal5Aal5Alias 	OBJECT-TYPE
		SYNTAX			DisplayString (SIZE (0..255))
		ACCESS			read-write
		STATUS			mandatory
		DESCRIPTION		"The alias for this AAL5 interface."
		::= { qatmAal5Entry 3 }

	qatmAal5AdminStatus 	OBJECT-TYPE
		SYNTAX INTEGER{
						up(1),	-- operation enabled
						down(2)	-- operation disabled
					  }
		ACCESS			read-write
		STATUS			mandatory
		DESCRIPTION		"The desired state for this interface."
		::= { qatmAal5Entry 4 }

	qatmAal5TrapStatus 	OBJECT-TYPE
		SYNTAX INTEGER{
						enabled(1),	-- operation enabled
						disabled(2)	-- operation disabled
					  }
		ACCESS			read-write
		STATUS			mandatory
		DESCRIPTION		"Indicates whether the Link Up/Link
						Down traps should be generated for this
						interface."
		::= { qatmAal5Entry 5 }

	qatmAal5MtuSize 	OBJECT-TYPE
  	    SYNTAX		INTEGER (1..65535)	   
		ACCESS			read-write
		STATUS			mandatory
		DESCRIPTION		"Set the largest PDU size for the AAL5 CPCS
						 layer that cen be processed by the AAL5 entity."
		::= { qatmAal5Entry 6 }

	qatmAal5RowStatus 	OBJECT-TYPE
  	    SYNTAX		INTEGER (1..6)	   
		ACCESS			read-write
		STATUS			mandatory
		DESCRIPTION		"Indicate the entry's status. 
						(1..5 = add) 6 = delete."
		::= { qatmAal5Entry 7 }


