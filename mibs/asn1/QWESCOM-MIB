--
-- QWES.com OAM control Group { qOamGroup 3 }
--
-- This group contains the definitions for all MIB objects used to
-- control the system OAM.

-- qoamGroupControlTable table
   	qoamGroupControlTable		OBJECT-TYPE
		SYNTAX			SEQUENCE OF QOAMGROUPCONTROLENTRY
		ACCESS			not-accessible
		STATUS			mandatory
		DESCRIPTION		"A list of OAM Control Parameters entries."
		::= { qOamGroup 3 }

   	qoamGroupControlEntry 	OBJECT-TYPE
		SYNTAX			QOAMGROUPCONTROLENTRY
		ACCESS			not-accessible
		STATUS			mandatory
		DESCRIPTION		"One entry in the table."
		INDEX   { qoamGroupControlIndex }
   		::= { qoamGroupControlTable 1 }

	QOAMGROUPCONTROLENTRY ::=
		SEQUENCE {
		        qoamGroupControlIndex	 	        INTEGER,
		        qoamGroupControlType	 	        INTEGER,
				qoamGroupControlStatus				INTEGER
		  }
--
-- OAM Control Entry fields
--
   qoamGroupControlIndex	OBJECT-TYPE
	   SYNTAX		INTEGER(1..256)
       MAX-ACCESS 	read-write
       STATUS       current
       DESCRIPTION
               "The index for the selected circuit."
       ::= { qoamGroupControlEntry 1 }

   qoamGroupControlType	OBJECT-TYPE
	   SYNTAX		INTEGER {
						segment-2-segment(1),
						end-2-end(2),
						both(3)
					}
       MAX-ACCESS 	read-write
       STATUS       current
       DESCRIPTION
               "The type of OAM to be generated,
                1=end-to-end, 2=segment-to-segment, 3=both."
       DEFVAL { 3 }
       ::= { qoamGroupControlEntry 2 }

   qoamGroupControlStatus	OBJECT-TYPE
	   SYNTAX		INTEGER {
						start(1),
						stop(2),
						restore(3)
					}
       MAX-ACCESS 	read-write
       STATUS       current
       DESCRIPTION
               "Initiate or terminate OAM operation, 1=start and 2=stop"
       DEFVAL { 3 }
       ::= { qoamGroupControlEntry 3 }
--
-- QWES.com OAM control Group { qOamGroup 4 }
--
-- MIBs definition for OAM which is particular to a circuit
--
-- 

-- qoamCircuitControlTable table
   	qoamCircuitControlTable		OBJECT-TYPE
		SYNTAX			SEQUENCE OF QOAMCIRCUITCONTROLENTRY
		ACCESS			not-accessible
		STATUS			mandatory
		DESCRIPTION		"A list of OAM Control Parameters entry."
		::= { qOamGroup 4 }

   	qoamCircuitControlEntry 	OBJECT-TYPE
		SYNTAX			QOAMCIRCUITCONTROLENTRY
		ACCESS			not-accessible
		STATUS			mandatory
		DESCRIPTION		"One entry in the table."
		INDEX   { qoamCircuitControlIndex }
   		::= { qoamCircuitControlTable 1 }

	QOAMCIRCUITCONTROLENTRY ::=
		SEQUENCE {
		        qoamCircuitControlIndex	 	        INTEGER,
		        qoamCircuitControlType	 	        INTEGER,
				qoamCircuitControlStatus				INTEGER
		  }
--
-- OAM Control Entry fields
--
   qoamCircuitControlIndex	OBJECT-TYPE
	   SYNTAX		INTEGER(1..1024)
       MAX-ACCESS 	read-write
       STATUS       current
       DESCRIPTION
               "The index for the selected circuit."
       ::= { qoamCircuitControlEntry 1 }

   qoamCircuitControlType	OBJECT-TYPE
	   SYNTAX		INTEGER {
						segment-2-segment(1),
						end-2-end(2),
						both(3)
					}
       MAX-ACCESS 	read-write
       STATUS       current
       DESCRIPTION
               "The type of OAM to be generated,
                1=end-to-end, 2=segment-to-segment, 3=both."
       DEFVAL { 3 }
       ::= { qoamCircuitControlEntry 2 }

   qoamCircuitControlStatus	OBJECT-TYPE
	   SYNTAX		INTEGER {
						start(1),
						stop(2),
						restore(3)
					}
       MAX-ACCESS 	read-write
       STATUS       current
       DESCRIPTION
               "Initiate or terminate OAM operation, 1=start and 2=stop"
       DEFVAL { 3 }
       ::= { qoamCircuitControlEntry 3 }

--
-- QWES.com OAM Statistics Group { qOamGroup 3 }
--
-- This group contains the definitions for all MIB objects used 
-- for OAM statistics.

-- qoamStatsTable table
   	qoamStatsTable		OBJECT-TYPE
		SYNTAX			SEQUENCE OF QOAMSTATSENTRY
		ACCESS			not-accessible
		STATUS			mandatory
		DESCRIPTION		"A list of OAM statistics entries."
		::= { qOamGroup 5 }

   	qoamStatsEntry 	OBJECT-TYPE
		SYNTAX			QOAMSTATSENTRY
		ACCESS			not-accessible
		STATUS			mandatory
		DESCRIPTION		"One entry in the table."
		INDEX   { qatmCircuitId }
   		::= { qoamStatsTable 1 }

	QOAMSTATSENTRY ::=
		SEQUENCE {
		        qoamStatsCircuit	 	INTEGER,
		        qoamStatsVpi	 	    INTEGER,
		        qoamStatsVci	 	    INTEGER,
		        qoamStatsType	 	    INTEGER,
		        qoamStatsStatus	 	    INTEGER,
				qoamRequestTx	 	    INTEGER,
				qoamRequestRx	 	    INTEGER,
				qoamReplyRx	 	        INTEGER,
				qoamReplyTx	 	        INTEGER,
				qoamTimeouts	 	    INTEGER,
				qoamMinRt	 	        INTEGER,
				qoamMaxRt	 	        INTEGER,
				qoamAveRt	 	        INTEGER,
				qoamOtherInErrs	 	    INTEGER,
				qoamOtherOutErrs	 	INTEGER,
				qoamCrc10InErrs	 	    INTEGER,
				qoamCrc10OutErrs	 	INTEGER
		  }

   qoamStatsCircuit	OBJECT-TYPE
	   SYNTAX		INTEGER
	   MAX-ACCESS 	read-only
       STATUS       current
       DESCRIPTION
               "The circuit id identifies the entry."
       ::= { qoamStatsEntry 1 }

   qoamStatsVpi	OBJECT-TYPE
	   SYNTAX		INTEGER
	   MAX-ACCESS 	read-only
       STATUS       current
       DESCRIPTION
               "The first half of the VPI/VCI pairs associated with the circuit id."
       ::= { qoamStatsEntry 2 }

   qoamStatsVci	OBJECT-TYPE
	   SYNTAX		INTEGER
	   MAX-ACCESS 	read-only
       STATUS       current
       DESCRIPTION
               "The second half of the VPI/VCI pairs associated with the circuit id."
       ::= { qoamStatsEntry 3 }

   qoamStatsType	OBJECT-TYPE
	   SYNTAX		INTEGER {
						f4-segment(1),
						f4-end(2),
						f5-segment(3),
						f5-end(4)
					}
	   MAX-ACCESS 	read-only
       STATUS       current
       DESCRIPTION
               "The type of OAM."
       ::= { qoamStatsEntry 4 }

   qoamStatsStatus	OBJECT-TYPE
	   SYNTAX		INTEGER {
						on(1),
						off(2)
					}
	   MAX-ACCESS 	read-only
       STATUS       current
       DESCRIPTION
               "The current operational status of OAM on this circuit.
                ON=OAMs being transmitted, OFF=OAMs not being transmitted."
       ::= { qoamStatsEntry 5 }

   qoamRequestTx	OBJECT-TYPE
	   SYNTAX		INTEGER
	   MAX-ACCESS 	read-only
       STATUS       current
       DESCRIPTION
               "The total number of OAM requests transmitted."
       ::= { qoamStatsEntry 6 }

   qoamRequestRx	OBJECT-TYPE
	   SYNTAX		INTEGER
	   MAX-ACCESS 	read-only
       STATUS       current
       DESCRIPTION
               "The total number of OAM requests received."
       ::= { qoamStatsEntry 7 }

   qoamReplyTx	OBJECT-TYPE
	   SYNTAX		INTEGER
	   MAX-ACCESS 	read-only
       STATUS       current
       DESCRIPTION
               "The total number of OAM replies transmitted."
       ::= { qoamStatsEntry 8 }

   qoamReplyRx	OBJECT-TYPE
	   SYNTAX		INTEGER
	   MAX-ACCESS 	read-only
       STATUS       current
       DESCRIPTION
               "The total number of OAM replies received."
       ::= { qoamStatsEntry 9 }

   qoamTimeouts	OBJECT-TYPE
	   SYNTAX		INTEGER
	   MAX-ACCESS 	read-only
       STATUS       current
       DESCRIPTION
               "The total number of OAM replies that were not received
                before timeout."
       ::= { qoamStatsEntry 10 }

   qoamMinRt	OBJECT-TYPE
	   SYNTAX		INTEGER
	   MAX-ACCESS 	read-only
       STATUS       current
       DESCRIPTION
               "The minimum round trip time in seconds for receiving
                OAM replies since the counters were last reset."
       ::= { qoamStatsEntry 11 }

   qoamMaxRt	OBJECT-TYPE
	   SYNTAX		INTEGER
	   MAX-ACCESS 	read-only
       STATUS       current
       DESCRIPTION
               "The maximun round trip time in seconds for receiving
                OAM replies since the counters were last reset."
       ::= { qoamStatsEntry 12 }

   qoamAveRt	OBJECT-TYPE
	   SYNTAX		INTEGER
	   MAX-ACCESS 	read-only
       STATUS       current
       DESCRIPTION
               "The average round trip time in seconds for receiving
                OAM replies since the counters were last reset."
       ::= { qoamStatsEntry 13 }

   qoamOtherInErrs	OBJECT-TYPE
	   SYNTAX		INTEGER
	   MAX-ACCESS 	read-only
       STATUS       current
       DESCRIPTION
               "The number of OAM received but discarded due to errors."
       ::= { qoamStatsEntry 14 }

   qoamOtherOutErrs	OBJECT-TYPE
	   SYNTAX		INTEGER
	   MAX-ACCESS 	read-only
       STATUS       current
       DESCRIPTION
               "The number of OAM can't transmitted due to errors."
       ::= { qoamStatsEntry 15 }

   qoamCrc10InErrs	OBJECT-TYPE
	   SYNTAX		INTEGER
	   MAX-ACCESS 	read-only
       STATUS       current
       DESCRIPTION
               "The number of OAM received but discarded due to crc-10 
                checksum error."
       ::= { qoamStatsEntry 16 }

   qoamCrc10OutErrs	OBJECT-TYPE
	   SYNTAX		INTEGER
	   MAX-ACCESS 	read-only
       STATUS       current
       DESCRIPTION
               "The number of OAM can't transmitted due to crc-10
                checksum error."
       ::= { qoamStatsEntry 17 }

END
