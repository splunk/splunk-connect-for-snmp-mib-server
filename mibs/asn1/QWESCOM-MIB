-- qcraftPort group { oem 6 }
-- This group contains the MIB objects related to the craft port.

	qcpStatus OBJECT-TYPE
		SYNTAX		INTEGER
					{
					down(0),
					up(1)
				       }
		ACCESS		read-only
		STATUS		mandatory
		DESCRIPTION	""
                ::= { qcraftPort 1 }

	qcpBaudRate OBJECT-TYPE
		SYNTAX		INTEGER
					{
				       	br-1200(0),
				       	br-2400(1),
				       	br-4800(2),
				       	br-9600(3),
				       	br-14400(4),
				       	br-19200(5),
				       	br-28800(6),
				       	br-38400(7),
				       	br-57600(8),
				       	br-115200(9)
				       }
		ACCESS		read-write
		STATUS		mandatory
		DESCRIPTION	"The craft port command baud rate."
		::= { qcraftPort 2 }

	qcpStopBitLength OBJECT-TYPE
		SYNTAX		INTEGER
					{
				       	sb-1(0),
				       	sb-2(1)
				       }
		ACCESS		read-write
		STATUS		mandatory
		DESCRIPTION	""
                ::= { qcraftPort 3 }

	qcpParity OBJECT-TYPE
		SYNTAX		INTEGER
					{
				       	even(0),
				       	odd(1),
				       	none(2)
				       }
		ACCESS		read-write
		STATUS		mandatory
		DESCRIPTION	""
                ::= { qcraftPort 4 }

	qcpDataBits OBJECT-TYPE
		SYNTAX		INTEGER
					{
				       	db-7(0),
				       	db-8(1)
				       }
		ACCESS		read-write
		STATUS		mandatory
		DESCRIPTION	""
                ::= { qcraftPort 5 }

	qcpCharsSent OBJECT-TYPE
		SYNTAX		INTEGER
		ACCESS		read-write
		STATUS		mandatory
		DESCRIPTION	""
                ::= { qcraftPort 6 }

	qcpCharsReceived OBJECT-TYPE
		SYNTAX		INTEGER
		ACCESS		read-write
		STATUS		mandatory
		DESCRIPTION	""
                ::= { qcraftPort 7 }

--
--	qeventGroup { oem 8 }
--  This group contains the MIBs for the event logging.

	qEventConfigTable			OBJECT-TYPE
		SYNTAX			SEQUENCE OF QEventConfigEntry
		MAX-ACCESS		not-accessible
		STATUS			mandatory
		DESCRIPTION		"A list of Event System Configuration
 					Entries used for viewing / changing event
 					options"
		::= { qeventGroup 1 }				  

	qEventConfigEntry			OBJECT-TYPE
		SYNTAX			QEventConfigEntry
		MAX-ACCESS		not-accessible
		STATUS			mandatory
		DESCRIPTION		"One entry in the table"
		INDEX   { qEventConfigIndex }
		::= { qEventConfigTable 1 }

	QEventConfigEntry	 ::=	
		SEQUENCE	{
				qEventConfigIndex
					INTEGER,
				qEventSeverityType
					INTEGER,
				qLogFileType
					INTEGER,
				qLogOption
					INTEGER,
				qDisplayOption 
					INTEGER,
				qTrapOption
					INTEGER
			 	}

--  EVENT Configuration Entry fields
--
	qEventConfigIndex		OBJECT-TYPE
		SYNTAX			INTEGER (1..6)
		MAX-ACCESS		read-only
		STATUS			mandatory       
		DESCRIPTION		"A unique value for each event that has
 					  been logged into one of the Event Logs"
		::= { qEventConfigEntry 1 }

	qEventSeverityType 		OBJECT-TYPE
		SYNTAX			INTEGER {
						fatal(1),
						critical(2),
				       		mild(3),
						standard-snmp(4),
						informational(5),
						debug(6)
					}

		MAX-ACCESS		read-only
		STATUS			mandatory
		DESCRIPTION		"A unique value which identifies each
					Event Type (Severity)."
		DEFVAL { 4 }
		::= { qEventConfigEntry 2 }


	qLogFileType 			OBJECT-TYPE
		SYNTAX			INTEGER {
						alarm(1),
						system(2),
						none(3)
					}

		MAX-ACCESS		read-write
		STATUS			mandatory
		DESCRIPTION		"A unique value which allows the
					selection of the type of Log file
					to Log events into."
		DEFVAL { 2 }
		::= { qEventConfigEntry 3 }


	qLogOption 				OBJECT-TYPE
		SYNTAX			INTEGER {
						enable(1),
						disable(2)
					}

		MAX-ACCESS		read-write
		STATUS			mandatory
		DESCRIPTION		"A flag which enables(1)/disables(2) the 
					 logging feature of the Event Subsystem"
		DEFVAL { 2 }
		::= { qEventConfigEntry 4 }

	qDisplayOption 			OBJECT-TYPE
		SYNTAX			INTEGER {
						enable(1),
						disable(2)
					}
		MAX-ACCESS		read-write
		STATUS			mandatory
		DESCRIPTION		"A flag which enables(1)/disables(2) the 
					 display feature of the Event Subsystem"
		DEFVAL { 2 }
		::= { qEventConfigEntry 5 }



	qTrapOption 			OBJECT-TYPE
		SYNTAX			INTEGER {
						enable(1),
						disable(2)
					}
		MAX-ACCESS		read-write
		STATUS			mandatory
		DESCRIPTION			"A flag which enables(1)/disables(2) the 
					Send Trap feature of the Event Subsystem"
		DEFVAL { 2 }
		::= { qEventConfigEntry 6 }

