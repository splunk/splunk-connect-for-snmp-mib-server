		SYNTAX			IpAddress
		ACCESS			read-write
		STATUS			mandatory
		DESCRIPTION		"The IP address associated with this IGMP entry."
		::= { qIgmpStaticEntry 1 }

   qIgmpLp			OBJECT-TYPE
       SYNTAX		INTEGER
       MAX-ACCESS	read-write
       STATUS		current
       DESCRIPTION
               "The logical port number associated with this IP port."
       ::= { qIgmpStaticEntry 2 }

   qIgmpType    OBJECT-TYPE
	   SYNTAX	INTEGER {
				add-activate(1),
				add-not-activate(2),
				delete(3)
				}
		MAX-ACCESS  read-write
       	STATUS      current
       	DESCRIPTION
               "Defines the IGMP entry current status. Setting this entry to invalid(2)
               has the effect of invalidating the correspding entry in the IGMP table.
               Setting this entry to valid(1) has the effect of installing the entry
               in the IGMP table and make it active."
       ::= { qIgmpStaticEntry 3 }

-- QWES.com IGMP Group
--
-- This group contains the definitions for all MIB objects used to
-- configure an IGMP group.

