		ACCESS			read-write
		STATUS			mandatory
		DESCRIPTION		"set to 1 to start TFTP download. 0 = ready for next download."
		::= { qmanagementGroup 6 }

 	qTftpOperStatus OBJECT-TYPE
		SYNTAX			INTEGER
		ACCESS			read-only
		STATUS			mandatory
		DESCRIPTION		"0=idle, 1=reading, 2=server IP error, 3=server filename error, 4 = done"
		::= { qmanagementGroup 7 }

 	qTftpRxCount OBJECT-TYPE
		SYNTAX			INTEGER
		ACCESS			read-only
		STATUS			mandatory
		DESCRIPTION		"Number of blocks received"
		::= { qmanagementGroup 8 }

	qTimeZone			OBJECT-TYPE
		SYNTAX     		INTEGER {
