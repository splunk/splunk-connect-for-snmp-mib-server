-- qatmVplTable table
   	qatmVplTable		OBJECT-TYPE
		SYNTAX			SEQUENCE OF QatmVplEntry
		ACCESS			not-accessible
		STATUS			mandatory
		DESCRIPTION		"The Virtual Path Link (VPL) table."
		::= { qatm 3 }

   	qatmVplEntry 		OBJECT-TYPE
		SYNTAX			QatmVplEntry
		ACCESS			not-accessible
		STATUS			mandatory
		DESCRIPTION		"One entry in the table."
--		INDEX  {ifIndex, atmVplVpi }
		INDEX  {ifIndex, ifIndex }
   		::= { qatmVplTable 1 }

	QatmVplEntry ::=
		SEQUENCE {
		        qatmVplMaxVcc
		            INTEGER,
		        qatmVplLowestVci
		            INTEGER,
		        qatmVplMaxQueueDelay
		            INTEGER,
		        qatmVplReservedBuffer
		            INTEGER,
			qatmVplBwGranularity
		            INTEGER,
			qatmVplMtu
		            INTEGER,
		        qatmVplSharedBuffer
		            INTEGER
}

-- ATM VPL Entry fields
	qatmVplMaxVcc		OBJECT-TYPE
		SYNTAX			INTEGER (0..1024)
		ACCESS			read-write
		STATUS			mandatory
		DESCRIPTION		"The maximum number of VCCs initialized
					and active for this VPC.  If the value
					is 0, then this VPC is only for
					management purposes and is not assigned
					to a scheduler."
		::= { qatmVplEntry 1 }

	qatmVplLowestVci	OBJECT-TYPE
		SYNTAX			INTEGER	(0..65535)
		ACCESS			read-write
		STATUS			mandatory
		DESCRIPTION		"The start of a contiguous and
					increasing VCI range whose maximum is
					given by the maximum VCCs allowed."
		::= { qatmVplEntry 2 }

	qatmVplMaxQueueDelay	OBJECT-TYPE
		SYNTAX		 	INTEGER
		ACCESS		 	read-write
		STATUS		 	mandatory
		DESCRIPTION	 	"The maximum queueing delay in
					milliseconds (msec).  Combining this
					value with the sustained rate will
					determine the maximum buffering
					allowed."
		::= { qatmVplEntry 3 }

	qatmVplReservedBuffer	OBJECT-TYPE
		SYNTAX			INTEGER
		ACCESS			read-write
		STATUS			mandatory
		DESCRIPTION		"The minimum amount of buffering that
					this VCC will be allocated to use."
		::= { qatmVplEntry 4 }

	qatmVplBwGranularity	OBJECT-TYPE
		SYNTAX			INTEGER
		ACCESS			read-write
		STATUS			mandatory
		DESCRIPTION		"The bandwidth granularity can be apply
					to this VPC."
		::= { qatmVplEntry 5 }

	qatmVplMtu		OBJECT-TYPE
		SYNTAX			INTEGER
		ACCESS			read-write
		STATUS			mandatory
		DESCRIPTION		"The MTU size can be apply to this
					VPC."
		::= { qatmVplEntry 6 }

	qatmVplSharedBuffer	OBJECT-TYPE
		SYNTAX			INTEGER
		ACCESS			read-write
		STATUS			mandatory
		DESCRIPTION		"The amount of buffering to be assigned
					to a VPC for sharing on a First In
					First Out (FIFO) basis between flows on
					this VPC."
		::= { qatmVplEntry 7 }

