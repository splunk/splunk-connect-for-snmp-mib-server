QWESCOM-MIB DEFINITIONS ::= BEGIN

    IMPORTS
	Counter, enterprises
	    FROM RFC1155-SMI
	IpAddress
	    FROM RFC1155-SMI
	TimeTicks
	    FROM RFC1155-SMI
	OBJECT-TYPE
	    FROM RFC-1212
	RowStatus
	    FROM SNMPv2-TC;
-- el10049    atmVplVpi,atmVclVci 
-- el100499	    FROM RFC2515-MIB;


	-- If any of the MIB compilers have problems with the DisplayString
	-- or PhysAddress, uncomment the next two lines.

       DisplayString		::=	OCTET STRING
       PhysAddress		::=	OCTET STRING
       TimeDateString		::=	INTEGER
       Boolean			::=	INTEGER
	--
	-- The QWESCOM enterprise OID.
	--

    qwescom			OBJECT IDENTIFIER ::= {	 enterprises 3662 }


	--
	-- Groups in the QWESCOM Enterprise mib
	--


    oemTree			OBJECT IDENTIFIER ::= { qwescom 3662 }

    oem				OBJECT IDENTIFIER ::= { oemTree 3662 }

    qproprietaryMib2		OBJECT IDENTIFIER ::= { oem 1 }

    qproducts			OBJECT IDENTIFIER ::= { oem 2 }

    qslots			OBJECT IDENTIFIER ::= { oem 3 }

    qFlowClass			OBJECT IDENTIFIER ::= { oem 4 }

    qatm                        OBJECT IDENTIFIER ::= { oem 5 }

    qcraftPort			OBJECT IDENTIFIER ::= { oem 6 }

    qeventGroup			OBJECT IDENTIFIER ::= { oem 8 }

    qextmib			OBJECT IDENTIFIER ::= { oem 9 }

    qmanufactRecord		OBJECT IDENTIFIER ::= { oem 10 }

    qethernet		        OBJECT IDENTIFIER ::= { oem 11 }

    qds1-e1		        OBJECT IDENTIFIER ::= { oem 12 }

    qconfigManager		OBJECT IDENTIFIER ::= { oem 13 }

    qportManagement		OBJECT IDENTIFIER ::= { oem 15 }

    qip                     OBJECT IDENTIFIER ::= { oem 16 }

    qmanagementGroup            OBJECT IDENTIFIER ::= { oem 17 }

    qOamGroup                   OBJECT IDENTIFIER ::= { oem 18 }

    qmiscGroup                  OBJECT IDENTIFIER ::= { oem 19 }

-- QWES.com qproprietaryMib2 { oem 1 }
--
-- This group contains the MIB variables that correspond to the
-- standard Internet Approved Managed Object MIBS (i.e. RFC-1213
-- RFC 1406, RFC-1695, RFC-2358, etc.).

--    qsystem	        OBJECT IDENTIFIER ::= {qproprietaryMib2  1}
--    qinterfaces	OBJECT IDENTIFIER ::= {qproprietaryMib2  2}
--    qat		OBJECT IDENTIFIER ::= {qproprietaryMib2  3}
--    qip		OBJECT IDENTIFIER ::= {qproprietaryMib2  4}
    qsnmp		OBJECT IDENTIFIER ::= {qproprietaryMib2 11}
    qigmp		OBJECT IDENTIFIER ::= {qip 7}

-- SNMP Community Table
   	qsnmpCommunityTable 		OBJECT-TYPE
		SYNTAX		   	SEQUENCE OF QsnmpCommunityEntry
		ACCESS			not-accessible
		STATUS			mandatory
		DESCRIPTION		"A list of SNMP Community entries.  The
					maximum number of SNMP Communities are
					five."
		::= { qsnmp 1 }

   	qsnmpCommunityEntry 		OBJECT-TYPE
		SYNTAX			QsnmpCommunityEntry
		ACCESS			not-accessible
		STATUS			mandatory
		DESCRIPTION		"One entry in the table."
	        INDEX   { qsnmpCommunityIndex }
   		::= { qsnmpCommunityTable 1 }

	QsnmpCommunityEntry ::=
		SEQUENCE {
		        qsnmpCommunityIndex
		            INTEGER,
		        qsnmpCommunityRights
		            INTEGER,
		        qsnmpCommunityName
		            DisplayString
			}

-- SNMP Community Entry fields
	qsnmpCommunityIndex 	OBJECT-TYPE
		SYNTAX			INTEGER (0..4)
		ACCESS			read-only
		STATUS			mandatory
		DESCRIPTION		"A unique value which identifies each
					SNMP Community."
		::= { qsnmpCommunityEntry 1 }

	qsnmpCommunityRights 	OBJECT-TYPE
		SYNTAX			INTEGER {
						readonly  (0),
						readwrite (1)
						}
		ACCESS			read-only
		STATUS			optional
                DESCRIPTION             "The first and third
					qsnmpCommunityIndex have read-write
                                        permission while the others have
					read-only privilege. Values 0 =
					ReadOnly, and 1 = ReadWrite"
                ::= { qsnmpCommunityEntry 2 }

	qsnmpCommunityName 	OBJECT-TYPE
		SYNTAX			DisplayString (SIZE (0..255))
		ACCESS			read-write
		STATUS			mandatory
		DESCRIPTION		"The SNMP Community Name used to access
					the unit with the associated permission
					from the qsnmpCommunityRights."
		::= { qsnmpCommunityEntry 3 }

-- SNMP NMS Table
   	qsnmpNMSTable 		OBJECT-TYPE
		SYNTAX		   	SEQUENCE OF QsnmpNMSEntry
		ACCESS			not-accessible
		STATUS			mandatory
		DESCRIPTION		"A list of SNMP Network Management
					System (NMS) entries."
		::= { qsnmp 2 }

   	qsnmpNMSEntry 		OBJECT-TYPE
		SYNTAX			QsnmpNMSEntry
		ACCESS			not-accessible
		STATUS			mandatory
		DESCRIPTION		"One entry in the table."
	        INDEX   { qsnmpNMSIndex }
   		::= { qsnmpNMSTable 1 }

	QsnmpNMSEntry ::=
		SEQUENCE {
		        qsnmpNMSIndex
		            INTEGER,
		        qsnmpNMSIpAddress
		            IpAddress
			}

-- SNMP NMS Address Entry fields
	qsnmpNMSIndex 		OBJECT-TYPE
		SYNTAX			INTEGER (0..4)
		ACCESS			read-only
		STATUS			mandatory
		DESCRIPTION		"A unique value which identifies each
					NMS."
		::= { qsnmpNMSEntry 1 }

	qsnmpNMSIpAddress 	OBJECT-TYPE
		SYNTAX			IpAddress
		ACCESS			read-write
		STATUS			mandatory
		DESCRIPTION		"The IP Address of a Network Management
					System, which is allowed to access MIB
					variables."
		::= { qsnmpNMSEntry 2 }

-- SNMP Trap IP Address Table
   	qsnmpTrapTable 		OBJECT-TYPE
		SYNTAX		   	SEQUENCE OF QsnmpTrapEntry
		ACCESS			not-accessible
		STATUS			mandatory
		DESCRIPTION		"A list of IP Address entries
					associated with traps."
		::= { qsnmp 3 }

   	qsnmpTrapEntry 		OBJECT-TYPE
		SYNTAX			QsnmpTrapEntry
		ACCESS			not-accessible
		STATUS			mandatory
		DESCRIPTION		"One entry in the table."
	        INDEX   { qsnmpTrapIndex }
   		::= { qsnmpTrapTable 1 }

	QsnmpTrapEntry ::=
		SEQUENCE {
		        qsnmpTrapIndex
		            INTEGER,
		        qsnmpTrapIpAddress
		            IpAddress
			}

-- SNMP Trap IP Address Entry fields
	qsnmpTrapIndex 		OBJECT-TYPE
		SYNTAX			INTEGER (0..4)
		ACCESS			read-only
		STATUS			mandatory
		DESCRIPTION		"A unique value which identifies each
					IP Address."
		::= { qsnmpTrapEntry 1 }

	qsnmpTrapIpAddress 	OBJECT-TYPE
		SYNTAX			IpAddress
		ACCESS			read-write
		STATUS			mandatory
		DESCRIPTION		"The IP Address of an IP entity, which
					is allowed to receive SNMP Trap
					events."
		::= { qsnmpTrapEntry 2 }

	qsnmpForwardTrap 	OBJECT-TYPE
		SYNTAX			INTEGER {
						enable (1),
						disable (2)
						}
		ACCESS			read-write
		STATUS			mandatory
		DESCRIPTION		"The flag which enables(1)/disables(2)
					SNMP Trap events to be transmitted."
		::= { qsnmp 4 }

-- QWES.com qproducts { oem 2 }
--
-- Each QWES.com product family is Assigned an Object Identifier.
-- This is the identifier stored in the MIB-II system group
-- item sysObjectID to identify the product to a network manager.
-- If product family specific items need to be defined they
-- should be defined in the appropriate product family subtree


-- QWES.com qslot { oem 3 }
--


-- QWES.com qFlowClass { oem 4 }
--
-- This group contains the definitions for all MIB objects used to
-- manage all traffic flow through Flow Classification, etc.  Chris
-- will expand.

-- ClassDefinition entry count

	qClassDefinitionMaxEntries 	OBJECT-TYPE
		SYNTAX			Counter
		ACCESS			read-only
		STATUS			mandatory
		DESCRIPTION		"The number of Class Definition
					entries."
		::= { qFlowClass 1 }

-- ClassDefinition table

   	qClassDefinitionTable	OBJECT-TYPE
		SYNTAX		   	SEQUENCE OF QClassDefinitionEntry
		ACCESS			not-accessible
		STATUS			mandatory
		DESCRIPTION		"A list of Class Definition entries.
					The number of entries is given by the
					value of qClassDefinitionMaxEntries."
		::= { qFlowClass 2 }

   	qClassDefinitionEntry 	OBJECT-TYPE
		SYNTAX			QClassDefinitionEntry
		ACCESS			not-accessible
		STATUS			mandatory
		DESCRIPTION		"One entry in the table."
	        INDEX   { qClassDefinitionIndex }
   		::= { qClassDefinitionTable 1 }

	QClassDefinitionEntry ::=
		SEQUENCE {
		        qClassDefinitionIndex
		            INTEGER,
		        qClassDefinitionAlias
		            DisplayString,
		        qClassDefinitionParentClassID
		            INTEGER,
		        qClassDefinitionFlowType
		            INTEGER,
		        qClassDefinitionSourceIPMask
		            OCTET STRING,
		        qClassDefinitionDestIPMask
		            IpAddress,
		        qClassDefinitionSourceIPLowerBound
		            IpAddress,
		        qClassDefinitionSourceIPUpperBound
		            IpAddress,
		        qClassDefinitionDestIPLowerBound
		            IpAddress,
		        qClassDefinitionDestIPUpperBound
		            IpAddress,
		        qClassDefinitionDSMask
		            INTEGER,
		        qClassDefinitionDSLowerBound
		            INTEGER,
		        qClassDefinitionDSUpperBound
		            INTEGER,
		        qClassDefinitionWellKnownApplication
		            INTEGER,
		        qClassDefinitionProtocol
		            INTEGER,
		        qClassDefinitionSourcePortLowerBound
		            INTEGER,
		        qClassDefinitionSourcePortUpperBound
		            INTEGER,
		        qClassDefinitionDestPortLowerBound
		            INTEGER,
		        qClassDefinitionDestPortUpperBound
		            INTEGER,
                        qClassDefinitionFlowAgingTime
		            INTEGER
		  }

-- Class Definition Entry fields
	qClassDefinitionIndex 	OBJECT-TYPE
		SYNTAX			INTEGER (2..1024)
		ACCESS			read-write
		STATUS			mandatory
		DESCRIPTION		"A unique value for each class
					definition. Values 0 and 1 are reserved
					for internal use."
		::= { qClassDefinitionEntry 1 }

	qClassDefinitionAlias 	OBJECT-TYPE
		SYNTAX			DisplayString (SIZE (0..255))
		ACCESS			read-write
		STATUS			optional
		DESCRIPTION		"A textual string containing user
					defined information about the Class
					Definition."
		::= { qClassDefinitionEntry 2 }

	qClassDefinitionParentClassID 	OBJECT-TYPE
		SYNTAX			INTEGER (0..1024)
		ACCESS			read-write
		STATUS			optional
		DESCRIPTION		"The qClassDefinitionIndex value of the
					parent for this Class Definition."
		::= { qClassDefinitionEntry 3 }

	qClassDefinitionFlowType 	OBJECT-TYPE
		SYNTAX			INTEGER {
						normal (1),
						dsingres (2),
						dsegress (3),
						dsnode (4),
						invalid (5)
						}
		ACCESS			read-write
		STATUS			mandatory
		DESCRIPTION		"The type of Class Definition being
					created.  INVALID causes the current
					Class Definition Entry to be removed."
		::= { qClassDefinitionEntry 4 }

	qClassDefinitionSourceIPMask 	OBJECT-TYPE
		SYNTAX			IpAddress
		ACCESS			read-write
		STATUS			optional
		DESCRIPTION		"The IP address mask for the source."
		::= { qClassDefinitionEntry 5 }

	qClassDefinitionSourceIPLowerBound 	OBJECT-TYPE
		SYNTAX			IpAddress
		ACCESS			read-write
		STATUS			optional
		DESCRIPTION		"The first source IP address which
					matches the class (after masking)."
		::= { qClassDefinitionEntry 6 }

	qClassDefinitionSourceIPUpperBound 	OBJECT-TYPE
		SYNTAX			IpAddress
		ACCESS			read-write
		STATUS			optional
		DESCRIPTION		"The last source IP address which
					matches the class (after masking)."
		::= { qClassDefinitionEntry 7 }

	qClassDefinitionDestIPMask 	OBJECT-TYPE
		SYNTAX			IpAddress
		ACCESS			read-write
		STATUS			optional
		DESCRIPTION		"The IP address mask for the
					destination."
		::= { qClassDefinitionEntry 8 }

	qClassDefinitionDestIPLowerBound 	OBJECT-TYPE
		SYNTAX			IpAddress
		ACCESS			read-write
		STATUS			optional
		DESCRIPTION		"The first destination IP address which
					matches the class (after masking)."
		::= { qClassDefinitionEntry 9 }

	qClassDefinitionDestIPUpperBound 	OBJECT-TYPE
		SYNTAX			IpAddress
		ACCESS			read-write
		STATUS			optional
		DESCRIPTION		"The last destination IP address which
					matches the class (after masking)."
		::= { qClassDefinitionEntry 10 }

	qClassDefinitionDSMask	OBJECT-TYPE
		SYNTAX			INTEGER (0..255)
		ACCESS			read-write
		STATUS			optional
		DESCRIPTION		"The DS/TOS byte mask value."
		::= { qClassDefinitionEntry 11 }

	qClassDefinitionDSLowerBound 	OBJECT-TYPE
		SYNTAX			INTEGER (0..255)
		ACCESS			read-write
		STATUS			optional
		DESCRIPTION		"The first DS/TOS byte value which
					matches the class (after masking)."
		::= { qClassDefinitionEntry 12 }

	qClassDefinitionDSUpperBound 	OBJECT-TYPE
		SYNTAX			INTEGER (0..255)
		ACCESS			read-write
		STATUS			optional
		DESCRIPTION		"The last DS/TOS byte value which
					matches the class (after masking)."
		::= { qClassDefinitionEntry 13 }

	qClassDefinitionWellKnownApplication OBJECT-TYPE
		SYNTAX			INTEGER {
						unspecified (0), -- none of the following:
								 -- following
						realaudio (1),
						netmeeting (2),
						echo (3),
						discard (4),
						rip (5),
						daytime (5),
						quoteofday (6),
						msp (7),
						ftp (8),
						telnet (9),
						smtp (10),
						msg (11),
						time (12),
						rap (13),
						rlp (14),
						whois (15),
						login (16),
						remotemail (17),
						xns (18),
						dns (19),
						sql (20),
						bootp (21),
						tftp (22),
						gopher (23),
						finger (24),
						http (25),
						kerberos (26),
						npp (27),
						hostname (28),
						isotsap (29),
						remotetelnet (30),
						sna (31),
						pop3 (32),
						rpc (33),
						authentication (34),
						audionews (35),
						sftp (36),
						uucp (37),
						networktime (38),
						pwdgen (39),
						ciscofna (40),
						ciscotna (41),
						ciscosys (42),
						netbios (43),
						news (44),
						management (45),
						send (46),
						appletalk (47),
						ipx (48),
						performancedata (49),
						ldap (50),
						novell (51),
						microsoft (52),
						doom (53),
						lotusnotes (54),
						audiomail (55),
						videomail (56),
						autodesk (57),
						ibm (58),
						vlsi (59),
						oracle (60),
						xwindows (61)
			               }
		ACCESS			read-write
		STATUS			mandatory
		DESCRIPTION		"The type of application to match."
		::= { qClassDefinitionEntry 14 }

	qClassDefinitionProtocol OBJECT-TYPE
		SYNTAX			INTEGER {
						all(0), -- any protocol matches
						udptcp(1), -- only udp and tcp
							   -- match
						udp(2),
						tcp(3),
						icmp(4),
						arp(5),
						rsvp(6),
						egp(7),
						ipinip(8),
						rdp(9),
						isotp4(10),
						idrp(11),
						pvp(12),
						isoip(13),
						vines(14),
						ttp(15),
						nsfnetigp(16),
						igrp(17),
						ospfigp(18)
			               }
		ACCESS			read-write
		STATUS			mandatory
		DESCRIPTION		"The IP protocol which matches the
					class."
		::= { qClassDefinitionEntry 15 }

	qClassDefinitionSourcePortLowerBound OBJECT-TYPE
		SYNTAX			INTEGER (0..65535)
		ACCESS			read-write
		STATUS			optional
		DESCRIPTION		"The first source UDP/TCP port which
					matches the class.  Only valid if
					qClassDefinitionWellKnownApplication =
					UNSPECIFIED, and
					qClassDefinitionProtocol = (ALL,
					UDPTCP, UDP, or TCP)."
		::= { qClassDefinitionEntry 16 }

	qClassDefinitionSourcePortUpperBound OBJECT-TYPE
		SYNTAX			INTEGER (0..65535)
		ACCESS			read-write
		STATUS			optional
		DESCRIPTION		"The last source UDP/TCP port which
					matches the class.  Only valid if
					qClassDefinitionWellKnownApplication =
					UNSPECIFIED, and
					qClassDefinitionProtocol = (ALL,
					UDPTCP, UDP, or TCP)."
		::= { qClassDefinitionEntry 17 }

	qClassDefinitionDestPortLowerBound OBJECT-TYPE
		SYNTAX			INTEGER (0..65535)
		ACCESS			read-write
		STATUS			optional
		DESCRIPTION		"The first dest UDP/TCP port which
					matches the class.  Only valid if
					qClassDefinitionWellKnownApplication =
					UNSPECIFIED, and
					qClassDefinitionProtocol = (ALL,
					UDPTCP, UDP, or TCP)."
		::= { qClassDefinitionEntry 18 }

	qClassDefinitionDestPortUpperBound OBJECT-TYPE
		SYNTAX			INTEGER (0..65535)
		ACCESS			read-write
		STATUS			optional
		DESCRIPTION		"The last dest UDP/TCP port which
					matches the class.  Only valid if
					qClassDefinitionWellKnownApplication =
					UNSPECIFIED, and
					qClassDefinitionProtocol = (ALL,
					UDPTCP, UDP, or TCP)."
		::= { qClassDefinitionEntry 19 }

        qClassDefinitionFlowAgingTime OBJECT-TYPE
		SYNTAX			INTEGER (0..65535)
		ACCESS			read-write
		STATUS			mandatory
		DESCRIPTION		"The number of seconds to use as the
					aging time for LIF related flow
					information."
		::= { qClassDefinitionEntry 20 }

-- qPolicyDefinitionMaxEntries entry count

	qPolicyDefinitionMaxEntries  OBJECT-TYPE
                SYNTAX			INTEGER (0..65535)
                ACCESS			read-write
		STATUS			mandatory
		DESCRIPTION		"The number of Policy Definition
					entries."

		::= { qFlowClass 3 }

-- qPolicyDefinition table

   	qPolicyDefinitionTable 		OBJECT-TYPE
		SYNTAX		   	SEQUENCE OF QPolicyDefinitionEntry
		ACCESS			not-accessible
		STATUS			mandatory
		DESCRIPTION		"A list of Policy Definition entries.
					The number of entries is given by the
					value of qPolicyDefinitionMaxEntries."
		::= { qFlowClass 4 }

   	qPolicyDefinitionEntry 		OBJECT-TYPE
		SYNTAX			QPolicyDefinitionEntry
		ACCESS			not-accessible
		STATUS			mandatory
	        INDEX   { qPolicyDefinitionIndex }
   		::= { qPolicyDefinitionTable 1 }

	QPolicyDefinitionEntry ::=
		SEQUENCE {
		        qPolicyDefinitionIndex
		            INTEGER,
		        qPolicyDefinitionAlias
		            DisplayString,
		        qPolicyDefinitionType
		            INTEGER,
		        qPolicyDefinitionLP
		            INTEGER,
		        qPolicyDefinitionStartTime
		            TimeDateString,
		        qPolicyDefinitionEndTime
		            TimeDateString,
		        qPolicyDefinitionDayofWeek
		            INTEGER,
		        qPolicyDefinitionPipeID
		            INTEGER,
		        qPolicyDefinitionClassID
		            INTEGER,
		        qPolicyDefinitionChannelized
		            INTEGER,
		        qPolicyDefinitionOutboundDSValue
		            INTEGER,
		        qPolicyDefinitionPriority
		            INTEGER,
		        qPolicyDefinitionEvent
		            INTEGER
		  }

-- Policy Definition Entry fields
	qPolicyDefinitionIndex 	OBJECT-TYPE
		SYNTAX			INTEGER (2..1024)
		ACCESS			read-write
		STATUS			mandatory
		DESCRIPTION		"A unique value for each policy
					definition. Values 0 and 1 are
					reserved for internal use."
		::= { qPolicyDefinitionEntry 1 }

	qPolicyDefinitionAlias 	OBJECT-TYPE
		SYNTAX			DisplayString (SIZE (0..255))
		ACCESS			read-write
		STATUS			optional
		DESCRIPTION		"A textual string containing user
					defined information about the Policy
					Definition."
		::= { qPolicyDefinitionEntry 2 }

	qPolicyDefinitionType 	OBJECT-TYPE
		SYNTAX			INTEGER {
						normal (1), -- normal policy
						dsingress (2), -- diffserv
							       -- ingress node
						dsnode (3), -- diffserv node
						dsegress (4), -- diffserv egree
							      -- node
						discard (5), -- discard policy
							     -- ... all pdus go
							     -- into the bit
							     -- bucket
						delete (6) -- delete policy
							   -- descriptor
						}
		ACCESS			read-write
		STATUS			optional
		DESCRIPTION		"The type of Policy Definition. DELETE
					causes the current definition to be
					removed."
		::= { qPolicyDefinitionEntry 3 }

        qPolicyDefinitionLP     OBJECT-TYPE
		SYNTAX			INTEGER (0..1024)
		ACCESS			read-write
		STATUS			optional
		DESCRIPTION		"The logical interface this policy
					relates to."
		::= { qPolicyDefinitionEntry 4 }

	qPolicyDefinitionStartTime 	OBJECT-TYPE
		SYNTAX			TimeDateString
		ACCESS			read-write
		STATUS			optional
		DESCRIPTION		"The time of day this policy takes
					effect. NULL = always in effect."
		::= { qPolicyDefinitionEntry 5 }

	qPolicyDefinitionEndTime 	OBJECT-TYPE
		SYNTAX			TimeDateString
		ACCESS			read-write
		STATUS			optional
		DESCRIPTION		"The time of day this policy stops
					taking effect. NULL = always in
					effect."
		::= { qPolicyDefinitionEntry 6 }

	qPolicyDefinitionDayofWeek 	OBJECT-TYPE
		SYNTAX			INTEGER (0..255)
		ACCESS			read-write
		STATUS			optional
		DESCRIPTION		"Bit flags which indicate the day of
					week. Bit 0 = Monday, 1 = Tuesday,
					2 = Weds, 4 = Thurs, 8 = Fri, 16 = Sat,
					32 = Sun, 64 = Holidays."
		::= { qPolicyDefinitionEntry 7 }

	qPolicyDefinitionPipeID 	OBJECT-TYPE
		SYNTAX			INTEGER (0..1024)
		ACCESS			read-write
		STATUS			mandatory
		DESCRIPTION		"The pipe definition this policy
					references."
		::= { qPolicyDefinitionEntry 8 }

	qPolicyDefinitionClassID 	OBJECT-TYPE
		SYNTAX			INTEGER (0..1024)
		ACCESS			read-write
		STATUS			mandatory
		DESCRIPTION		"The class definition this policy
					references."
		::= { qPolicyDefinitionEntry 9 }

	qPolicyDefinitionChannelized 	OBJECT-TYPE
		SYNTAX			INTEGER (0..65535)
		ACCESS			read-write
		STATUS			optional
		DESCRIPTION		"Bit flags indicating which Classing range is used to create
					unique channels. 0 = off, 1 = Source IP, 2 = Dest IP
					4 = Source Port, 8 = Dest Port, 16 = Protocol, 32 = DS.
					The maximum number of channels (1-255) is set into the upper
					byte of this value. If a channel is selected, the default 
					maximum is 16."
		::= { qPolicyDefinitionEntry 10 }

	qPolicyDefinitionOutboundDSValue 	OBJECT-TYPE
		SYNTAX			INTEGER (0..256)
		ACCESS			read-write
		STATUS			optional
		DESCRIPTION		"The DS/TOS byte value to set in
					outbound PDUs. A value of 256 indicates
					no replacement."
		::= { qPolicyDefinitionEntry 11 }

	qPolicyDefinitionPriority 	OBJECT-TYPE
		SYNTAX			INTEGER (0..255)
		ACCESS			read-write
		STATUS			optional
		DESCRIPTION		"The relative priority of this policy."
		::= { qPolicyDefinitionEntry 12 }

	qPolicyDefinitionEvent OBJECT-TYPE
		SYNTAX			INTEGER {
						static (1), -- the policy is
							    -- always in effect.
						time (2), -- time of day.
						reducedbw (3), -- reduced
							       -- bandwidth on
							       -- lif/vcc.
						increasedbw (4) -- increased
								-- bandwidth on
								-- lif/vcc.
			               		}
		ACCESS			read-write
		STATUS			mandatory
		DESCRIPTION		"The type of event which causes this
					policy to come into effect."
		::= { qPolicyDefinitionEntry 13 }

-- qPipeDefinitionMaxEntries entry count

	qPipeDefinitionMaxEntries  OBJECT-TYPE
		SYNTAX			Counter
		ACCESS			read-only
		STATUS		   	mandatory
		DESCRIPTION		"The number of Pipe Definition
					entries."

		::= { qFlowClass 5 }

-- qPipeDefinition table

   	qPipeDefinitionTable 		OBJECT-TYPE
		SYNTAX		   	SEQUENCE OF QPipeDefinitionEntry
		ACCESS			not-accessible
		STATUS			mandatory
		DESCRIPTION		"A list of Pipe Definition entries. The
					number of entries is given by the value
					of qPipeDefinitionMaxEntries."
		::= { qFlowClass 6 }

   	qPipeDefinitionEntry 		OBJECT-TYPE
		SYNTAX				QPipeDefinitionEntry
		ACCESS				not-accessible
		STATUS				mandatory
	        INDEX   { qPipeDefinitionIndex }
   		::= { qPipeDefinitionTable 1 }

	QPipeDefinitionEntry ::=
		SEQUENCE {
		        qPipeDefinitionIndex
		            INTEGER,
		        qPipeDefinitionAlias
		            DisplayString,
		        qPipeDefinitionType
		            INTEGER,
		        qPipeDefinitionCircuitID
		            INTEGER,
		        qPipeDefinitionBandwidth
		            INTEGER,
		        qPipeDefinitionPeak
		            INTEGER,
		        qPipeDefinitionMBS
		            INTEGER,
			qPipeDefinitionBufferAllocation
			    INTEGER,
		        qPipeDefinitionDelay
		            INTEGER,
			qPipeDefinitionPhyIdx
			    INTEGER,
			qPipeDefinitionPhyType
			    INTEGER
		  }

-- Pipe Definition Entry fields
	qPipeDefinitionIndex 	OBJECT-TYPE
		SYNTAX			INTEGER (2..1024)
		ACCESS			read-write
		STATUS			mandatory
		DESCRIPTION		"A unique value for each pipe
					definition. Values 0 and 1 are reserved
					for internal use."
		::= { qPipeDefinitionEntry 1 }

	qPipeDefinitionAlias 	OBJECT-TYPE
		SYNTAX			DisplayString (SIZE (0..255))
		ACCESS			read-write
		STATUS			optional
		DESCRIPTION		"A textual string containing user
					defined information about the Pipe
					Definition."
		::= { qPipeDefinitionEntry 2 }

	qPipeDefinitionType 	OBJECT-TYPE
		SYNTAX			INTEGER {
						valid (1), -- valid
						delete (2) -- delete pipe
							   -- definition
						}
		ACCESS			read-write
		STATUS			optional
		DESCRIPTION		"The type of Pipe Definition. DELETE
					causes the current definition to be
					removed."
		::= { qPipeDefinitionEntry 3 }

	qPipeDefinitionCircuitID OBJECT-TYPE
		SYNTAX			INTEGER (0..1024)
		ACCESS			read-write
		STATUS			optional
		DESCRIPTION		"The WAN circuit this pipe relates to."
		::= { qPipeDefinitionEntry 4 }

	qPipeDefinitionBandwidth OBJECT-TYPE
		SYNTAX			INTEGER
		ACCESS			read-write
		STATUS			optional
		DESCRIPTION		"The sustained bandwidth (bits/sec) of
					this pipe."
		::= { qPipeDefinitionEntry 5 }

	qPipeDefinitionPeak 	OBJECT-TYPE
		SYNTAX			INTEGER
		ACCESS			read-only
		STATUS			optional
		DESCRIPTION		"Peak bandwidth of this pipe
					(bits/sec).  Write-acces not supported
					in Release 1."
		::= { qPipeDefinitionEntry 6 }

	qPipeDefinitionMBS 	OBJECT-TYPE
		SYNTAX			INTEGER
		ACCESS			read-only
		STATUS			mandatory
		DESCRIPTION		"The maximum burst size (bytes).
					Write-acces not supported in Release
					1."
		::= { qPipeDefinitionEntry 7 }

        qPipeDefinitionBufferAllocation     OBJECT-TYPE
		SYNTAX			INTEGER
		ACCESS			read-write
		STATUS			mandatory
		DESCRIPTION		"The maximum queuing delay for this
					pipe."
		::= { qPipeDefinitionEntry 8 }

        qPipeDefinitionDelay     OBJECT-TYPE
		SYNTAX			INTEGER
		ACCESS			read-write
		STATUS			mandatory
		DESCRIPTION		"The maximum queuing delay for this
					pipe."
                ::= { qPipeDefinitionEntry 9 }

        qPipeDefinitionPhyIdx    OBJECT-TYPE
		SYNTAX			INTEGER
		ACCESS			read-only
		STATUS			mandatory
		DESCRIPTION		"The physical index for the port."
		::= { qPipeDefinitionEntry 10 }

	qPipeDefinitionPhyType 	OBJECT-TYPE
		SYNTAX			INTEGER
		ACCESS			read-only
		STATUS			mandatory
		DESCRIPTION		"The physical type for the port."
		::= { qPipeDefinitionEntry 11 }

-- Class Statistics table

   	qClassStatisticsTable 	OBJECT-TYPE
		SYNTAX		   	SEQUENCE OF QClassStatisticsEntry
		ACCESS			not-accessible
		STATUS			mandatory
		DESCRIPTION		"A list of Class Statistics entries.
					The number of entries is given by the
					value of qClassDefinitionMaxEntries."
		::= { qFlowClass 7 }

   	qClassStatisticsEntry 	OBJECT-TYPE
		SYNTAX			QClassStatisticsEntry
		ACCESS			not-accessible
		STATUS			mandatory
	        INDEX   { qClassStatisticsIndex }
   		::= { qClassStatisticsTable 1 }

	QClassStatisticsEntry ::=
		SEQUENCE {
		        qClassStatisticsIndex
		            INTEGER,
		        qClassStatisticsAlias
		            DisplayString,
		        qClassStatisticsTransmittedCount
		            Counter,
		        qClassStatisticsDiscardCount
		            Counter,
		        qClassStatisticsFallbackCount
		            Counter
		  }

-- Class Statistics Entry fields
	qClassStatisticsIndex 	OBJECT-TYPE
		SYNTAX			INTEGER (0..1024)
		ACCESS			read-only
		STATUS			mandatory
		DESCRIPTION		"A unique value for each class stats
					entry. These match their corresponding
					Class Definition entry index."
		::= { qClassStatisticsEntry 1 }

	qClassStatisticsAlias 	OBJECT-TYPE
		SYNTAX			DisplayString
		ACCESS			read-only
		STATUS			mandatory
		DESCRIPTION		"A textual string containing user
					defined information about the Class
					Definition this stats entry relates
					to."
		::= { qClassStatisticsEntry 2 }

	qClassStatisticsTransmittedCount OBJECT-TYPE
		SYNTAX			Counter
		ACCESS			read-only
		STATUS			mandatory
		DESCRIPTION		"The number of bytes transmitted."
		::= { qClassStatisticsEntry 3 }

	qClassStatisticsDiscardCount OBJECT-TYPE
		SYNTAX			Counter
		ACCESS			read-only
		STATUS			mandatory
		DESCRIPTION		"The number of bytes discarded."
		::= { qClassStatisticsEntry 4 }

	qClassStatisticsFallbackCount	OBJECT-TYPE
		SYNTAX			Counter
		ACCESS			read-only
		STATUS			mandatory
		DESCRIPTION		"The number of bytes transmitted on the
					'all others' flow."
		::= { qClassStatisticsEntry 5 }


-- qatm group { oem 5 }
-- QWES.com atm circuit table { qatm 1 }
--
-- This group contains the definitions for all MIB objects used to
-- configure ATM circuits.

-- qatmCircuitTable table
   	qatmCircuitTable	OBJECT-TYPE
		SYNTAX			SEQUENCE OF QatmCircuitEntry
		ACCESS			not-accessible
		STATUS			mandatory
		DESCRIPTION		"A list of ATM circuit entries."
		::= { qatm 1 }

   	qatmCircuitEntry 	OBJECT-TYPE
		SYNTAX			QatmCircuitEntry
		ACCESS			not-accessible
		STATUS			mandatory
		DESCRIPTION		"One entry in the table."
	        INDEX   { qatmCircuitId }
   		::= { qatmCircuitTable 1 }

	QatmCircuitEntry ::=
		SEQUENCE {
		        qatmCircuitId
		            INTEGER,
		        qatmCircuitAlias
		            DisplayString,
		        qatmCircuitUNI
		            INTEGER,
		        qatmCircuitLogicalInterface
		            INTEGER,
		        qatmCircuitVpi
		            INTEGER,
		        qatmCircuitFirstVci
		            INTEGER,
				qatmCircuitMode
		            INTEGER,
				qatmCircuitStatus
		            INTEGER
		  }

-- ATM Circuit Entry fields
	qatmCircuitId 		OBJECT-TYPE
		SYNTAX			INTEGER (1..1024)
		ACCESS			read-write
		STATUS			mandatory
		DESCRIPTION		"A unique value for this circuit
					definition."
		::= { qatmCircuitEntry 1 }


	qatmCircuitAlias 		OBJECT-TYPE
		SYNTAX			DisplayString (SIZE (0..52))
		ACCESS			read-write
		STATUS			mandatory
		DESCRIPTION		"A unique alias for this circuit
					definition."
		::= { qatmCircuitEntry 2 }

	qatmCircuitUNI 	OBJECT-TYPE
		SYNTAX			INTEGER (1..1024)
		ACCESS			read-write
		STATUS			mandatory
		DESCRIPTION		"The UNI interface number for this
					circuit."
		::= { qatmCircuitEntry 3 }

	qatmCircuitLogicalInterface 	OBJECT-TYPE
		SYNTAX			INTEGER (2..1024)
		ACCESS			read-write
		STATUS			mandatory
		DESCRIPTION		"The LIF to assign this Circuit to. LIF
					1 is reserved as an Ethernet
					identifier."
		::= { qatmCircuitEntry 4 }

	qatmCircuitVpi 	OBJECT-TYPE
		SYNTAX			INTEGER (0..255)
		ACCESS			read-write
		STATUS			mandatory
		DESCRIPTION		"The Virtual Path Identifier for this
					circuit."
		::= { qatmCircuitEntry 5 }

	qatmCircuitFirstVci 	OBJECT-TYPE
		SYNTAX			INTEGER (0..255)
		ACCESS			read-write
		STATUS			mandatory
		DESCRIPTION		"The Virtual Path Identifier for this
					circuit."
		::= { qatmCircuitEntry 6 }

				
	qatmCircuitMode 	OBJECT-TYPE
		SYNTAX			INTEGER {
						cell (1),
						packet (2)
						}
		ACCESS			read-write
		STATUS			mandatory
		DESCRIPTION		"Determining the multiplexing mode for this connection."
		::= { qatmCircuitEntry 7 }

	qatmCircuitStatus 	OBJECT-TYPE
		SYNTAX			INTEGER {
						valid (1), -- the circuit is
							   -- valid
						deltete (2) -- delete the
							    -- circuit entry
						}
		ACCESS			read-write
		STATUS			mandatory
		DESCRIPTION		"Setting a value of 1 adds/modifies an
					entry. Setting a value of 2 deletes a
					current entry."
		::= { qatmCircuitEntry 8 }


-- ATM UNI table
   	qatmUniTable		OBJECT-TYPE
		SYNTAX			SEQUENCE OF QatmUniEntry
		ACCESS			not-accessible
		STATUS			mandatory
		DESCRIPTION		"This table contains the QWES.com
					proprietary configuration parameters
					for an ATM local Interface (UNI)."
		::= { qatm 2 }

   	qatmUniEntry 		OBJECT-TYPE
		SYNTAX			QatmUniEntry
		ACCESS			not-accessible
		STATUS			mandatory
		DESCRIPTION		"This list contains the QWES.com
					proprietary configuration
					parameters and state variables and is
					indexed by ifIndex values ATM
					interface."
		INDEX  {ifIndex}
   		::= { qatmUniTable 1 }

	QatmUniEntry ::=
		SEQUENCE {
		        qatmSpanPort
		            INTEGER,
		        qatmUniName
		            DisplayString,
		        qatmUniAlias
		            DisplayString,
		        qatmUniTrapStatus
		            INTEGER,
		        qatmUniMaxQueueDelay
		            INTEGER,
		        qatmUniMinAllocBuffer
		            INTEGER,
		        qatmUniSharedBuffer
		            INTEGER,
		        qatmUniLinkBandwidth
		            INTEGER,
		        qatmUniLinkDelay
		            INTEGER,
		        qatmUniBandwidthGranularity
		            INTEGER,
		        qatmUniIlmiStatus
		            INTEGER,
		        qatmUniRowStatus
		            INTEGER
		  }

