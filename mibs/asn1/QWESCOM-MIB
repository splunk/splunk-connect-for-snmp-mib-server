-- QWES.com qextmib group { oem 9 }
-- This group contains the MIB objects provided by Phase2:
-- These groups are the extensions mib defined in qextmib
-- to be relative to an enterprise specific base.

    qipExt	OBJECT IDENTIFIER ::= {qextmib 1}
    qarpExt	OBJECT IDENTIFIER ::= {qextmib 2}
    qripExt	OBJECT IDENTIFIER ::= {qextmib 3}

-- the IP group

qipCircExtTable OBJECT-TYPE
	SYNTAX SEQUENCE OF QipCircExtEntry
	ACCESS not-accessible
	STATUS mandatory
        DESCRIPTION "Table of IP circuit parameters"
	::= {qipExt 1}

qipCircExtEntry OBJECT-TYPE
	SYNTAX QipCircExtEntry
	ACCESS not-accessible
	STATUS mandatory
	DESCRIPTION "A circuits IP parameters"
	INDEX {qipCircExtIndex}
	::= {qipCircExtTable 1}

QipCircExtEntry ::=
	SEQUENCE {
		qipCircExtIndex
			INTEGER,
		qipCircExtIfIndex
			INTEGER,
		qipCircExtAdminState
			INTEGER,
		qipCircExtOperState
			INTEGER
		}

qipCircExtIndex OBJECT-TYPE
	SYNTAX INTEGER
	ACCESS read-only
	STATUS mandatory
	DESCRIPTION "An index value which uniquely identifies
		this circuit within the router. This may or may
		not be related to the physical interface index."
	::= {qipCircExtEntry 1}

qipCircExtIfIndex OBJECT-TYPE
	SYNTAX INTEGER
	ACCESS read-only
	STATUS mandatory
	DESCRIPTION "An index value which uniquely identifies
		the local physical interface which this circuit
		uses. This value identifies the same interface
		identified by ifIndex in MIB-2."
	::= {qipCircExtEntry 2}

qipCircExtAdminState OBJECT-TYPE
	SYNTAX INTEGER{
			disabled(1),	-- operation disabled
			enabled(2)	-- operation enabled
		  }
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION "Enable or disable the operation of IP
		on the specified interface."
	::= {qipCircExtEntry 3}

qipCircExtOperState OBJECT-TYPE
	SYNTAX INTEGER{
			off(1),	-- circuit not up
			on(2)	-- circuit up
		  }
	ACCESS read-only
	STATUS mandatory
	DESCRIPTION "The operational state of the circuit. May
		or may not be the same as administrative state."
	::= {qipCircExtEntry 4}

-- the ARP group

qarpExtTtl OBJECT-TYPE
	SYNTAX INTEGER
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION "The lifetime of an ARP entry within the
		router."
	::= {qarpExt 1}

qarpCircExtTable OBJECT-TYPE
	SYNTAX SEQUENCE OF QarpCircExtEntry
	ACCESS not-accessible
	STATUS mandatory
	DESCRIPTION "Table of ARP circuit parameters"
	::= {qarpExt 2}

qarpCircExtEntry OBJECT-TYPE
	SYNTAX QarpCircExtEntry
	ACCESS not-accessible
	STATUS mandatory
	DESCRIPTION "A circuits ARP parameters"
	INDEX {qarpCircExtIndex}
	::= {qarpCircExtTable 1}

QarpCircExtEntry ::=
	SEQUENCE {
		qarpCircExtIndex
			INTEGER,
		qarpCircExtIfIndex
			INTEGER,
		qarpCircExtDoProxy
			INTEGER,
		qarpCircExtDoResp
			INTEGER
	}

qarpCircExtIndex OBJECT-TYPE
	SYNTAX INTEGER
	ACCESS read-only
	STATUS mandatory
	DESCRIPTION "An index value which uniquely identifies
		this circuit within the router. This may or may
		not be related to the physical interface index."
	::= {qarpCircExtEntry 1}

qarpCircExtIfIndex OBJECT-TYPE
	SYNTAX INTEGER
	ACCESS read-only
	STATUS mandatory
	DESCRIPTION "An index value which uniquely identifies
		the local physical interface which this circuit
		uses. This value identifies the same interface
		identified by ifIndex in MIB-2."
	::= {qarpCircExtEntry 2}

qarpCircExtDoProxy OBJECT-TYPE
	SYNTAX INTEGER{
			disabled(1),	-- operation disabled
			enabled(2)	-- operation enabled
		  }
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION "Enable or disable ARP proxy responses
		on the specified interface."
	::= {qarpCircExtEntry 3}

qarpCircExtDoResp OBJECT-TYPE
	SYNTAX INTEGER{
			disabled(1),	-- operation disabled
			enabled(2)	-- operation enabled
		  }
	ACCESS read-only
	STATUS mandatory
	DESCRIPTION "Enable or disable the sending of ARP
		responses on specified circuit."
	::= {qarpCircExtEntry 4}

-- the RIP group

qripExtAdminState OBJECT-TYPE
	SYNTAX INTEGER{
			disabled(1),	-- operation disabled
			enabled(2)	-- operation enabled
		  }
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION "Enable or disable the operation of RIP
		on all interfaces."
	::= {qripExt 1}

qripExtUpdateTime OBJECT-TYPE
	SYNTAX INTEGER
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION "The time between transmission of RIP
		updates."
	::= {qripExt 2}

qripCircExtTable OBJECT-TYPE
	SYNTAX SEQUENCE OF QripCircExtEntry
	ACCESS not-accessible
	STATUS mandatory
	DESCRIPTION "Table of RIP circuit parameters"
	::= {qripExt 3}

qripCircExtEntry OBJECT-TYPE
	SYNTAX QripCircExtEntry
	ACCESS not-accessible
	STATUS mandatory
	DESCRIPTION "A circuits RIP parameters"
	INDEX {qripCircExtIndex}
	::= {qripCircExtTable 1}

QripCircExtEntry ::=
	SEQUENCE {
		qripCircExtIndex
			INTEGER,
		qripCircExtIfIndex
			INTEGER,
		qripCircExtTalk
			INTEGER,
		qripCircExtListen
			INTEGER,
		qripCircExtPoison
			INTEGER
	}

qripCircExtIndex OBJECT-TYPE
	SYNTAX INTEGER
	ACCESS read-only
	STATUS mandatory
	DESCRIPTION "An index value which uniquely identifies
		this circuit within the router. This may or may
		not be related to the physical interface index."
	::= {qripCircExtEntry 1}

qripCircExtIfIndex OBJECT-TYPE
	SYNTAX INTEGER
	ACCESS read-only
	STATUS mandatory
	DESCRIPTION "An index value which uniquely identifies
		the local physical interface which this circuit
		uses. This value identifies the same interface
		identified by ifIndex in MIB-2."
	::= {qripCircExtEntry 2}

qripCircExtTalk OBJECT-TYPE
	SYNTAX INTEGER{
			disabled(1),	-- operation disabled
			enabled(2)	-- operation enabled
		  }
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION "Enable or disable RIP transmission
		on the specified interface."
	::= {qripCircExtEntry 3}

qripCircExtListen OBJECT-TYPE
	SYNTAX INTEGER{
			disabled(1),	-- operation disabled
			enabled(2)	-- operation enabled
		  }
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION "Enable or disable RIP reception
		on the specified interface."
	::= {qripCircExtEntry 4}

qripCircExtPoison OBJECT-TYPE
	SYNTAX INTEGER{
			disabled(1),	-- operation disabled
			enabled(2)	-- operation enabled
		  }
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION "Enable or disable operation of poison
		reverse on the specified interface. If poison
		is disabled, split-horzon operation is enabled."
	::= {qripCircExtEntry 5}


