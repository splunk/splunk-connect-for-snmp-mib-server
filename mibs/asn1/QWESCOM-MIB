							new-caledonia(26),
							auckland(27)
						}
		ACCESS			read-write
		STATUS			mandatory
		DESCRIPTION		"Current time zone of the unit."
		::= { qmanagementGroup 9 }

-- QWES.com IP Port Group { qip 1 }
--
-- This group contains the definitions for all MIB objects used to
-- configure an IP Port.

-- qipPortTable table
   	qipPortTable		OBJECT-TYPE
		SYNTAX			SEQUENCE OF QWESIPENTRY
		ACCESS			not-accessible
		STATUS			mandatory
		DESCRIPTION		"A list of IP Port entries."
		::= { qip 1 }

   	qipPortEntry 		OBJECT-TYPE
		SYNTAX			QWESIPENTRY
		ACCESS			not-accessible
		STATUS			mandatory
		DESCRIPTION		"One entry in the table."
		INDEX   { qIpLp }
   		::= { qipPortTable 1 }

	QWESIPENTRY ::=
		SEQUENCE {
		        qIpLp	 	INTEGER,
		        qIpAddr     IpAddress,
		        qIpSubnet	IpAddress,
				qIpMulticast INTEGER,
				qIpRipStatus INTEGER,
				qIpArpTimeout INTEGER,
				qIpEncap     INTEGER
		  }
--
-- IP Port Entry fields
--
   qIpLp           OBJECT-TYPE
       SYNTAX      INTEGER
       MAX-ACCESS  read-write
       STATUS      current
       DESCRIPTION
               "The logical port number associated with this IP port."
       ::= { qipPortEntry 1 }

	qIpAddr 	        OBJECT-TYPE
		SYNTAX			IpAddress
		ACCESS			read-write
		STATUS			mandatory
		DESCRIPTION		"The IP address of this IP Port."
		::= { qipPortEntry 2 }

	qIpSubnet 	        OBJECT-TYPE
		SYNTAX			IpAddress
		ACCESS			read-write
		STATUS			mandatory
		DESCRIPTION		"The IP subnet ddress of this IP Port."
		::= { qipPortEntry 3 }

   qIpMulticast    OBJECT-TYPE
		SYNTAX     INTEGER {
					enable(1),
					disable(2)
					}
       MAX-ACCESS  read-write
       STATUS      current
       DESCRIPTION
               "Indicate whether or not multicast will be transmit on this interface."
       ::= { qipPortEntry 4 }

   qIpRipStatus    OBJECT-TYPE
		SYNTAX     INTEGER {
					enable(1),
					disable(2)
					}
       MAX-ACCESS  read-write
       STATUS      current
       DESCRIPTION
               "Indicate the RIP status."
       ::= { qipPortEntry 5 }

   qIpArpTimeout    OBJECT-TYPE
		SYNTAX      INTEGER
		MAX-ACCESS  read-write
       STATUS      current
       DESCRIPTION
               "The elapsed time of seconds before an ARP entry can be removed from the ARP table."
       ::= { qipPortEntry 6 }

   qIpEncap    OBJECT-TYPE
		SYNTAX     INTEGER {
					standard(1),
					hp(2)
					}
       MAX-ACCESS  read-write
       STATUS      current
       DESCRIPTION
               "Indicate the encapsulation type."
       ::= { qipPortEntry 7 }


-- Ip/Rip Port Group { oem 15 }
-- QWES.com RIP Port Group { qip 2 }
--
-- This group contains the definitions for all MIB objects used to
-- configure an RIP Port.

-- qRipPortTable table
   	qRipPortTable		OBJECT-TYPE
		SYNTAX			SEQUENCE OF QWESRIPENTRY
		ACCESS			not-accessible
		STATUS			mandatory
		DESCRIPTION		"A list of IP Port entries."
		::= { qip 2 }

   	qRipPortEntry 		OBJECT-TYPE
		SYNTAX			QWESRIPENTRY
		ACCESS			not-accessible
		STATUS			mandatory
		DESCRIPTION		"One entry in the table."
		INDEX   { qRipIpAddr }
   		::= { qRipPortTable 1 }

	QWESRIPENTRY ::=
		SEQUENCE {
                                qRipIpAddr                      IpAddress,        
                                qMode                           INTEGER,        
                                qAuthentication                 DisplayString,
				qSplitHorizon           	INTEGER,        
				qAdvertiseStaticRoutes		INTEGER,		
				qAdvertiseIntf			INTEGER,        
				qBroadcastType			INTEGER,        
				qHoldDownInterval		INTEGER,	    
				qUpdatesSend			INTEGER,        
				qUpdatesReceive			INTEGER,        
				qDefaultRouteMetric		INTEGER	    
                  }
--
-- RIP Port Entry fields
--
   qRipIpAddr              OBJECT-TYPE
        SYNTAX                  IpAddress
        ACCESS                  read-write
        STATUS                  mandatory
        DESCRIPTION             "The IP address associated with this RIP Port."
        ::= { qRipPortEntry 1 }

   qMode           OBJECT-TYPE
		SYNTAX     INTEGER {
                                        none (1),
                                        password (2),
                                        md5 (3)
					}
       MAX-ACCESS  read-write
       STATUS      current
       DESCRIPTION
               "Selects what type of rip authentication to be used on the selected port."
       DEFVAL { 1 }
       ::= { qRipPortEntry 2 }

   qAuthentication              OBJECT-TYPE
        SYNTAX                  DisplayString (SIZE (0..16))
        ACCESS                  read-write
        STATUS                  current
        DESCRIPTION             "This is the password to be used, when password type is selected for above authentication."
        ::= { qRipPortEntry 3 }

   qSplitHorizon   OBJECT-TYPE
		SYNTAX     INTEGER {
					poisoned-reverse(1),
					simple(2),
					disable (3)
					}
       MAX-ACCESS  read-write
       STATUS      current
       DESCRIPTION
               "."
       DEFVAL { 3 }
       ::= { qRipPortEntry 4 }

   qAdvertiseStaticRoutes   OBJECT-TYPE
		SYNTAX     INTEGER {
					yes(1),
					no(2)
					}
       MAX-ACCESS  read-write
       STATUS      current
       DESCRIPTION
               "."
       DEFVAL { 1 }
       ::= { qRipPortEntry 5 }

   qAdvertiseIntf   OBJECT-TYPE
		SYNTAX     INTEGER {
					yes(1),
					no(2)
					}
       MAX-ACCESS  read-write
       STATUS      current
       DESCRIPTION
               "."
       DEFVAL { 1 }
       ::= { qRipPortEntry 6 }

   qBroadcastType   OBJECT-TYPE
		SYNTAX     INTEGER {
					local(1),
					direct-with-ones(2),
					direct-with-zeros(3)
					}
       MAX-ACCESS  read-write
       STATUS      current
       DESCRIPTION
               "."
       DEFVAL { 1 }
       ::= { qRipPortEntry 7 }

   qHoldDownInterval   OBJECT-TYPE
	   SYNTAX     INTEGER(1..65535)
	   MAX-ACCESS  read-write
       STATUS      current
       DESCRIPTION
               "."
       DEFVAL { 60 }
       ::= { qRipPortEntry 8 }

   qUpdatesSend   OBJECT-TYPE
		SYNTAX     INTEGER {
					silent(1),
					version-1(2),
					rip-1-compatible(3),
					version-2(4),
					version-1-demand(5),
					version-2-demand(6)
					}
	   MAX-ACCESS  read-write
       STATUS      current
       DESCRIPTION
           "What the router sends on this interface.
           ripVersion1 implies sending RIP updates compliant
           with  RFC  1058.   rip1Compatible implies
           broadcasting RIP-2 updates using RFC 1058 route
           subsumption rules.  ripVersion2 implies
           multicasting RIP-2 updates.  ripV1Demand indicates
           the use of Demand RIP on a WAN interface under RIP
           Version 1 rules.  ripV2Demand indicates the use of
           Demand RIP on a WAN interface under Version 2 rules."
       DEFVAL { 2 }
       ::= { qRipPortEntry 9 }

   qUpdatesReceive   OBJECT-TYPE
		SYNTAX     INTEGER {
					rip1(1),
					rip2(2),
					rip1-or-rip2(3),
					none(3)
					}
	   MAX-ACCESS  read-write
       STATUS      current
       DESCRIPTION
           "This indicates which version of RIP updates
           are to be accepted.  Note that rip2 and
           rip1OrRip2 implies reception of multicast
           packets."
       DEFVAL { 3 }
       ::= { qRipPortEntry 10 }

   qDefaultRouteMetric   OBJECT-TYPE
		SYNTAX     INTEGER(1..15)
		MAX-ACCESS  read-write
       STATUS      current
       DESCRIPTION
           "This variable indicates the metric that is to
           be used for the default route entry in RIP updates
           originated on this interface.  A value of zero
           indicates that no default route should be
           originated; in this case, a default route via
           another router may be propagated."
       DEFVAL { 1 }
       ::= { qRipPortEntry 11 }


-- Ip Static Arp Group { oem 15 }
-- QWES.com Static ARP Group { qip 3 }
--
-- This group contains the definitions for all MIB objects used to
-- configure an static ARP entry.

-- qStaticArpTable table
   	qStaticArpTable		OBJECT-TYPE
		SYNTAX			SEQUENCE OF QWESSTATICARPENTRY
		ACCESS			not-accessible
		STATUS			mandatory
		DESCRIPTION		"A list of Static ARP entries."
		::= { qip 3 }

   	qStaticArpEntry 		OBJECT-TYPE
		SYNTAX			QWESSTATICARPENTRY
		ACCESS			not-accessible
		STATUS			mandatory
		DESCRIPTION		"One entry in the table."
		INDEX   { qStaticArpAddr }
   		::= { qStaticArpTable 1 }

	QWESSTATICARPENTRY ::=
		SEQUENCE {
		        qStaticArpAddr    IpAddress,        
				qLif			  INTEGER,        
				qPhysType		  INTEGER,        
				qMacAddr		  PhysAddress,
				qType			  INTEGER
		  }
--
-- Static Arp Entry fields
--
	qStaticArpAddr 	        OBJECT-TYPE
		SYNTAX			IpAddress
		ACCESS			read-write
		STATUS			mandatory
		DESCRIPTION		"The IP address associated with this ARP entry."
		::= { qStaticArpEntry 1 }

   qLif           OBJECT-TYPE
		SYNTAX     INTEGER(513..1536)
		MAX-ACCESS  read-write
       	STATUS      mandatory
       	DESCRIPTION
               "The Logical Interface which uniquely identifies the local 
                interface which this ARP entry can be reached."
       ::= { qStaticArpEntry 2 }

   qPhysType   OBJECT-TYPE
	   SYNTAX	INTEGER {
				ethernet(1),
				atm-and-others(2)
				}
		MAX-ACCESS  read-write
       	STATUS      mandatory
       	DESCRIPTION
               "The physical interface type associated with this ARP entry."
       ::= { qStaticArpEntry 3 }

   qMacAddr   OBJECT-TYPE
	   SYNTAX      PhysAddress
	   MAX-ACCESS  read-write
       STATUS      mandatory
       DESCRIPTION
               "The physical address associated with this ARP entry."
       ::= { qStaticArpEntry 4 }

   qType   OBJECT-TYPE
	   SYNTAX	INTEGER {
				add-activate(1),
				add-not-activate(2),
				delete(3)
				}
		MAX-ACCESS  read-write
       	STATUS      mandatory
       	DESCRIPTION
               "Defines the ARP entry current status. Setting this entry to invalid(2)
               has the effect of invalidating the correspding entry in the ARP table.
               Setting this entry to valid(1) has the effect of installing the entry
               in the ARP table and make it active."
       ::= { qStaticArpEntry 5 }

--
-- IP Node Parameters
--
      qipTtl OBJECT-TYPE
          SYNTAX  INTEGER
          ACCESS  read-write
          STATUS  mandatory
          DESCRIPTION
              "The time-to-live (in seconds) that a packet can live in  
              the network."
	      DEFVAL { 60 }
          ::= { qip 4 }

      qipRoutePriority OBJECT-TYPE
          SYNTAX  INTEGER {
					rip(1),
					static(2)
				}
          ACCESS  read-write
          STATUS  mandatory
          DESCRIPTION
              "The route priority indicates routing entry precedence in the system.
               If RIP is selected, then all routing entries learned by RIP will have
               higher priority then other entries.  Also, if STATIC is selected, then
               all routing entries which are static defined will have higher priority
               then other entries."
	      DEFVAL { 1 }
          ::= { qip 5 }

      qipRipStatus OBJECT-TYPE
          SYNTAX  INTEGER {
					enable(1),
					disable(2)
				}
          ACCESS  read-write
          STATUS  mandatory
          DESCRIPTION
              "The RIP system flag."
	      DEFVAL { 1 }
          ::= { qip 6 }

      qigmpInMsgs OBJECT-TYPE
          SYNTAX  INTEGER 
          ACCESS  read-only
          STATUS  mandatory
          DESCRIPTION
              "Messages received by the system."
          ::= { qigmp 1 }

      qigmpOutMsgs OBJECT-TYPE
          SYNTAX  INTEGER 
          ACCESS  read-only
          STATUS  mandatory
          DESCRIPTION
              "Messages transmitted  by the system."
          ::= { qigmp 2 }

      qigmpInErrors OBJECT-TYPE
          SYNTAX  INTEGER 
          ACCESS  read-only
          STATUS  mandatory
          DESCRIPTION
              "Error messages received  by the system."
          ::= { qigmp 3 }

      qigmpInReports OBJECT-TYPE
          SYNTAX  INTEGER 
          ACCESS  read-only
          STATUS  mandatory
          DESCRIPTION
              "Reported messages received  by the system."
          ::= { qigmp 4 }

      qigmpInQueries OBJECT-TYPE
          SYNTAX  INTEGER 
          ACCESS  read-only
          STATUS  mandatory
          DESCRIPTION
              "Querries received  by the system."
          ::= { qigmp 5 }

      qigmpInUnknownType OBJECT-TYPE
          SYNTAX  INTEGER 
          ACCESS  read-only
          STATUS  mandatory
          DESCRIPTION
              "Unkown messages received  by the system."
          ::= { qigmp 6 }

-- QWES.com Static IGMP Group
--
-- This group contains the definitions for all MIB objects used to
-- configure an static IGMP entry.

-- qIgmpStaticTable table
   	qIgmpStaticTable	OBJECT-TYPE
		SYNTAX			SEQUENCE OF QWESSTATICIGMPENTRY
		ACCESS			not-accessible
		STATUS			mandatory
		DESCRIPTION		"A list of Static IGMP entries."
		::= { qigmp 7 }

   	qIgmpStaticEntry	OBJECT-TYPE
		SYNTAX			QWESSTATICIGMPENTRY
		ACCESS			not-accessible
		STATUS			mandatory
		DESCRIPTION		"One entry in the table."
		INDEX   { qIgmpStaticAddr }
   		::= { qIgmpStaticTable 1 }

	QWESSTATICIGMPENTRY ::=
		SEQUENCE {
		        qIgmpStaticAddr    IpAddress,        
