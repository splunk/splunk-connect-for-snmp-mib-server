   	qwesAtmArpEntry 		OBJECT-TYPE
		SYNTAX			QWESLIFENTRY
		ACCESS			not-accessible
		STATUS			mandatory
		DESCRIPTION		"One entry in the table."
		INDEX   { ifIndex }
   		::= { qwesAtmArpTable 1 }

	QWESLIFENTRY ::=
                SEQUENCE { qwesAtmArpLifIndex      INTEGER }

	qwesAtmArpLifIndex		OBJECT-TYPE
       	SYNTAX      	INTEGER
       	ACCESS			read-only
		STATUS			mandatory
		DESCRIPTION		"The logical interfacne (LIF) number that this
						 ATM ARP entry has an association with."
		::= { qwesAtmArpEntry 1 }


