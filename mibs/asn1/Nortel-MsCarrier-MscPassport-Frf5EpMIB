frf5EpMIB OBJECT IDENTIFIER ::= { mscPassportMIBs 121 }










-- McsMgr Frf5EndPointGroup/n
-- The Frf5EpG component contains FRF.5 End Points that share a 
-- common destination NSAP address, and a list of address prefixes.

mscMcsMgrFrf5EpG OBJECT IDENTIFIER ::= { mscMcsMgr 14 }



mscMcsMgrFrf5EpGRowStatusTable OBJECT-TYPE
   SYNTAX     SEQUENCE OF MscMcsMgrFrf5EpGRowStatusEntry
   ACCESS     not-accessible
   STATUS     mandatory
   DESCRIPTION
     "This entry controls the addition and deletion of 
      mscMcsMgrFrf5EpG components."
::= { mscMcsMgrFrf5EpG 1 }

mscMcsMgrFrf5EpGRowStatusEntry OBJECT-TYPE
   SYNTAX     MscMcsMgrFrf5EpGRowStatusEntry
   ACCESS     not-accessible
   STATUS     mandatory
   DESCRIPTION
     "A single entry in the table represents a single 
      mscMcsMgrFrf5EpG component."
   INDEX {
      mscMcsMgrIndex,
      mscMcsMgrFrf5EpGIndex }
::= { mscMcsMgrFrf5EpGRowStatusTable 1 }

