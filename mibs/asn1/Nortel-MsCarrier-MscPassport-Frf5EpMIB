MscMcsMgrFrf5EpGAddrProvEntry ::= SEQUENCE {
   mscMcsMgrFrf5EpGAddrRemoteAddress
      AsciiString,
   mscMcsMgrFrf5EpGAddrCommentText
      AsciiString }

mscMcsMgrFrf5EpGAddrRemoteAddress OBJECT-TYPE
   SYNTAX     AsciiString (SIZE (0..44))
   ACCESS     read-write
   STATUS     mandatory
   DESCRIPTION
     "This attribute specifies the ATM End System Address (AESA) in 
      the ISO NSAP format that is used by each EndPoint under this 
      Frf5EpG to establish a VCC. For example, NSAP ATM address 
      '45000006138292323F000000000020480D002900'."
::= { mscMcsMgrFrf5EpGAddrProvEntry 1 }

mscMcsMgrFrf5EpGAddrCommentText OBJECT-TYPE
   SYNTAX     AsciiString (SIZE (0..750))
   ACCESS     read-write
   STATUS     mandatory
   DESCRIPTION
     "This optional attribute contains additional textual information to be 
      added by the network operator."
   DEFVAL     { ''H }  -- ""
::= { mscMcsMgrFrf5EpGAddrProvEntry 2 }

mscMcsMgrFrf5EpGAddrAddrPreTable OBJECT-TYPE
   SYNTAX     SEQUENCE OF MscMcsMgrFrf5EpGAddrAddrPreEntry
   ACCESS     not-accessible
   STATUS     mandatory
   DESCRIPTION
     "This attribute specifies the list of prefixes directed to the EndPoints 
      under this Frf5EndPointGroup. The individual end point is selected 
      for each subconnection on the basis of its 
      supportedTransferPriorities.
      For example, E.164 prefix 'e.613722', X.121 prefix 'x.302'."
::= { mscMcsMgrFrf5EpGAddr 362 }

mscMcsMgrFrf5EpGAddrAddrPreEntry OBJECT-TYPE
   SYNTAX     MscMcsMgrFrf5EpGAddrAddrPreEntry
   ACCESS     not-accessible
   STATUS     mandatory
   DESCRIPTION
     "An entry in the mscMcsMgrFrf5EpGAddrAddrPreTable."
   INDEX {
      mscMcsMgrIndex,
      mscMcsMgrFrf5EpGIndex,
      mscMcsMgrFrf5EpGAddrIndex,
      mscMcsMgrFrf5EpGAddrAddrPreValue }
::= { mscMcsMgrFrf5EpGAddrAddrPreTable 1 }

MscMcsMgrFrf5EpGAddrAddrPreEntry ::= SEQUENCE {
   mscMcsMgrFrf5EpGAddrAddrPreValue
      AsciiString,
   mscMcsMgrFrf5EpGAddrAddrPreRowStatus
      RowStatus }

mscMcsMgrFrf5EpGAddrAddrPreValue OBJECT-TYPE
   SYNTAX     AsciiString (SIZE (3..17))
   ACCESS     read-write
   STATUS     mandatory
   DESCRIPTION
     "This variable represents both the value and the index for the
      mscMcsMgrFrf5EpGAddrAddrPreTable."
::= { mscMcsMgrFrf5EpGAddrAddrPreEntry 1 }

