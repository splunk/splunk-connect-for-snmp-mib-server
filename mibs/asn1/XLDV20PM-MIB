                   xldv20pmImaGroupCurrentElapsedTime              INTEGER,
                   xldv20pmImaGroupCurrentThresholdLimitIndex      INTEGER,
                   xldv20pmImaGroupCurrentUnavailSecs              Gauge,
                   xldv20pmImaGroupCurrentNeNumFailures            Gauge
                   }

     xldv20pmImaGroupCurrentIntervalType OBJECT-TYPE
     -- info-VALUE:     min15(1), day(2)
