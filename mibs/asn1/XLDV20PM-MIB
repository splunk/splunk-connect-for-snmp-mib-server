     xldv20pmS1CurrentTable OBJECT-TYPE
     -- info-IMPL:      V2.0-> 
        SYNTAX        SEQUENCE OF Xldv20pmS1CurrentEntry
        ACCESS        not-accessible
        STATUS        mandatory
        DESCRIPTION
             " This table contains the entries for the current 15 minute interval
               or the current day for STM1 near-end and far-end PPTPs.
               Note: Day PM collection uses the 15min  interval results . "
        --DEFAULT test-function-async     fsnPmS1cEntryTest
        --DEFAULT next-function-async     fsnPmS1cEntryNext
        --DEFAULT set-function-async      fsnPmS1cEntrySet
        --DEFAULT get-function-async      fsnPmS1cEntryGet
        ::= { xldv20pmPerformanceMonitoring 8 }

     xldv20pmS1CurrentEntry OBJECT-TYPE
     -- info-IMPL:      V2.0-> 
     -- info-VERCTRL:   -
        SYNTAX        Xldv20pmS1CurrentEntry
        ACCESS        not-accessible
        STATUS        mandatory
        DESCRIPTION
             " An entry describing the Stm1 Current Data Table . "
        INDEX { xldv20IfIndex,
                xldv20pmS1cIntervalType,
                xldv20pmS1cOAMLevel }
        ::= { xldv20pmS1CurrentTable 1 }

     Xldv20pmS1CurrentEntry ::=
        SEQUENCE { xldv20pmS1cIntervalType         Xldv20pmIntervalType,
                   xldv20pmS1cOAMLevel                 INTEGER,
                   xldv20pmS1cAdminState           Xldv20AdminState,
                   xldv20pmS1cSuspectFlag          Xldv20pmSuspectType,
                   xldv20pmS1cHistEntrySize            INTEGER,
                   xldv20pmS1cNrHistEntriesAvailable   INTEGER,
                   xldv20pmS1cElapsedTime              INTEGER,
                   xldv20pmS1cThresholdLimitIndex      INTEGER,
                   xldv20pmS1cNeErroredSeconds         Counter,
                   xldv20pmS1cFeErroredSeconds         Counter,
                   xldv20pmS1cNeSeverelyErroredSeconds Counter,
                   xldv20pmS1cFeSeverelyErroredSeconds Counter,
                   xldv20pmS1cNeBackgroundBlockErrors  Counter,
                   xldv20pmS1cFeBackgroundBlockErrors  Counter,
                   xldv20pmS1cNeUnavailableSeconds     Counter,
                   xldv20pmS1cFeUnavailableSeconds     Counter,
                   xldv20pmS1cNeOfsSeconds             Counter,
                   xldv20pmS1cNeAisSeconds             Counter,
                   xldv20pmS1cNeLosSeconds             Counter }

     xldv20pmS1cIntervalType OBJECT-TYPE
     -- info-VALUE:     min15(1), day(2)
     -- info-ROU:       GET  db
     -- info-IMPL:      V2.0-> 
     -- info-VERCTRL:   -
        SYNTAX        Xldv20pmIntervalType
        ACCESS        read-only
        STATUS        mandatory
        DESCRIPTION
             " This type attribute is used to select between the
               15 min  or the day accumulation types. "
        ::= { xldv20pmS1CurrentEntry 1 }

     xldv20pmS1cOAMLevel OBJECT-TYPE
     -- info-VALUE:     1..3
     -- info-ROU:       GET db
     -- info-IMPL:      V2.0-> 
     -- info-VERCTRL:   -
        SYNTAX        INTEGER (1..3)
        ACCESS        read-only
        STATUS        mandatory
        DESCRIPTION
             " This attribute is used to select between the
               3 OAM levels f1 (1), f2 (2) and f3 (3) . "
        ::= { xldv20pmS1CurrentEntry 2 }

     xldv20pmS1cAdminState OBJECT-TYPE
     -- info-VALUE:     unlocked(1), locked(2)
     -- info-ROU:       GET db  SET pm
     -- info-IMPL:      V2.0-> 
     -- info-VERCTRL:   -
        SYNTAX        Xldv20AdminState
        ACCESS        read-write
        STATUS        mandatory
        DESCRIPTION
             " This read-write attribute is used to activate (unlock)
               and deactivate (lock) the functions (incl. collection)
               performed by this managed entity.
               Note: the 15min  collection object must be activated,
                     before day collection can operate. "
        ::= { xldv20pmS1CurrentEntry 3 }

     xldv20pmS1cSuspectFlag OBJECT-TYPE
     -- info-VALUE:     reliable(1), suspect(2)
     -- info-ROU:       GET db
     -- info-IMPL:      V2.0-> 
     -- info-VERCTRL:   -
        SYNTAX        Xldv20pmSuspectType
        ACCESS        read-only
        STATUS        mandatory
        DESCRIPTION
             " This read-only attribute will be set to indicate that
               troubles arose during an interval and that the counter
               values are suspect and incomplete. "
        ::= { xldv20pmS1CurrentEntry 4 }

     xldv20pmS1cHistEntrySize OBJECT-TYPE
     -- info-VALUE:     1..96
     -- info-ROU:       GET db
     -- info-IMPL:      V2.0-> 
     -- info-VERCTRL:   -
        SYNTAX        INTEGER (1..96)
        ACCESS        read-only
        STATUS        mandatory
        DESCRIPTION
             " This read-only attribute indicates the configured history size.
               The default value is 1 and means that at least one
               history entry is provided. "
        ::= { xldv20pmS1CurrentEntry 5 }

     xldv20pmS1cNrHistEntriesAvailable OBJECT-TYPE
     -- info-VALUE:     0..96
     -- info-ROU:       GET db
     -- info-IMPL:      V2.0-> 
     -- info-VERCTRL:   -
        SYNTAX        INTEGER (0..96)
        ACCESS        read-only
        STATUS        mandatory
        DESCRIPTION
             " This read-only attribute indicates how many history entries
               are already available.
               The range of values is between 0 and xldv20pmS1cHistEntrySize,
               thereby 0 means that this entry is not valid . "
        ::= { xldv20pmS1CurrentEntry 6 }

     xldv20pmS1cElapsedTime OBJECT-TYPE
     -- info-VALUE:     0..900 (sec) for min15,
     -- info-VALUE:     0..1440 (min) for day
     -- info-ROU:       GET db
     -- info-IMPL:      V2.0-> 
     -- info-VERCTRL:   -
        SYNTAX        INTEGER(0..1440)
        ACCESS        read-only
        STATUS        mandatory
        DESCRIPTION
             " This read-only attribute represents the difference
               between the current time and the start of the present
               summary interval.
	       Units are seconds when 15 minute interval is selected 
	       or minutes for day interval.
               (Note: accurate to the sample time interval). "
        ::= { xldv20pmS1CurrentEntry 7 }

     xldv20pmS1cThresholdLimitIndex OBJECT-TYPE
     -- info-VALUE:     1..30
     -- info-ROU:       GET db  SET pm
     -- info-IMPL:      V2.0-> 
     -- info-VERCTRL:   -
        SYNTAX        INTEGER (1..30)
        ACCESS        read-write
        STATUS        mandatory
        DESCRIPTION
             " This read-write attribute provides an index to the
               ThresholdLimitTable managed entity that contains the
               threshold limits for the performance parameters reported
               by this managed entity.
               When the value of this attribute is set to 1, default
               threshold limits shall be used. These default values are
               not part of the threshold limit table but are hard coded
               in the application SW of PM. "
        ::= { xldv20pmS1CurrentEntry 8 }

     xldv20pmS1cNeErroredSeconds OBJECT-TYPE
