             " An entry describing the STS3 near-end and far-end
               Current Data Table . "
        INDEX { xldv20IfIndex,
                xldv20pmS3cIntervalType,
                xldv20pmS3cOAMLevel }
        ::= { xldv20pmS3CurrentTable 1 }

     Xldv20pmS3CurrentEntry ::=
        SEQUENCE { xldv20pmS3cIntervalType            Xldv20pmIntervalType,
                   xldv20pmS3cOAMLevel                       INTEGER,
                   xldv20pmS3cAdminState              Xldv20AdminState,
                   xldv20pmS3cSuspectFlag             Xldv20pmSuspectType,
                   xldv20pmS3cHistEntrySize                  INTEGER,
                   xldv20pmS3cNrHistEntriesAvailable         INTEGER,
                   xldv20pmS3cElapsedTime                    INTEGER,
                   xldv20pmS3cThresholdLimitIndex            INTEGER,
                   xldv20pmS3cNeErroredSeconds               Counter,
                   xldv20pmS3cFeErroredSeconds               Counter,
                   xldv20pmS3cNeSeverelyErroredSeconds       Counter,
                   xldv20pmS3cFeSeverelyErroredSeconds       Counter,
                   xldv20pmS3cNeCodeViolations               Counter,
                   xldv20pmS3cFeCodeViolations               Counter,
                   xldv20pmS3cNeUnavailableSeconds           Counter,
                   xldv20pmS3cFeUnavailableSeconds           Counter,
                   xldv20pmS3cNeSeverelyErroredFrameSeconds  Counter }

     xldv20pmS3cIntervalType OBJECT-TYPE
     -- info-VALUE:     min15(1), day(2)
     -- info-ROU:       GET db
     -- info-IMPL:      V2.0-> 
     -- info-VERCTRL:   -
        SYNTAX        Xldv20pmIntervalType
        ACCESS        read-only
        STATUS        mandatory
        DESCRIPTION
             " This type attribute is used to select between the
               15 min  or the day accumulation types. "
        ::= { xldv20pmS3CurrentEntry 1 }

     xldv20pmS3cOAMLevel OBJECT-TYPE
     -- info-VALUE:     1..3
     -- info-ROU:       GET db
     -- info-IMPL:      V2.0-> 
     -- info-VERCTRL:   -
        SYNTAX        INTEGER (1..3)
        ACCESS        read-only
        STATUS        mandatory
        DESCRIPTION
             " This attribute is used to select between the
               3 OAM levels f1 (1), f2 (2) and f3 (3) . "
        ::= { xldv20pmS3CurrentEntry 2 }

     xldv20pmS3cAdminState OBJECT-TYPE
     -- info-VALUE:     unlocked(1), locked(2)
     -- info-ROU:       GET  db SET pm
     -- info-IMPL:      V2.0-> 
     -- info-VERCTRL:   -
        SYNTAX        Xldv20AdminState
        ACCESS        read-write
        STATUS        mandatory
        DESCRIPTION
             " This read-write attribute is used to activate (unlock)
               and deactivate (lock) the functions (incl. collection)
               performed by this managed entity.
               Note: the 15min  collection object must be activated,
                     before day collection can operate. "
        ::= { xldv20pmS3CurrentEntry 3 }

     xldv20pmS3cSuspectFlag OBJECT-TYPE
     -- info-VALUE:     reliable(1), suspect(2)
     -- info-ROU:       GET db
     -- info-IMPL:      V2.0-> 
     -- info-VERCTRL:   -
        SYNTAX        Xldv20pmSuspectType
        ACCESS        read-only
        STATUS        mandatory
        DESCRIPTION
             " This read-only attribute will be set to indicate that
               troubles arose during an interval and that the counter
               values are suspect and incomplete. "
        ::= { xldv20pmS3CurrentEntry 4 }

     xldv20pmS3cHistEntrySize OBJECT-TYPE
     -- info-VALUE:     1..96
     -- info-ROU:       GET db
     -- info-IMPL:      V2.0-> 
     -- info-VERCTRL:   -
        SYNTAX        INTEGER (1..96)
        ACCESS        read-only
        STATUS        mandatory
        DESCRIPTION
             " This read-only attribute indicates the number of 15min 
               history entries configured at the particular interface.
               The default value is 1 and means that at least one
               history entry is provided . "
        ::= { xldv20pmS3CurrentEntry 5 }

     xldv20pmS3cNrHistEntriesAvailable OBJECT-TYPE
     -- info-VALUE:     0..96
     -- info-ROU:       GET db
     -- info-IMPL:      V2.0-> 
     -- info-VERCTRL:   -
        SYNTAX        INTEGER (0..96)
        ACCESS        read-only
        STATUS        mandatory
        DESCRIPTION
             " This read-only attribute indicates how many history entries
               are already available.
               The range of values is between 0 and xldv20S3cHistEntrySize,
               thereby 0 means that this entry is not valid . "
        ::= { xldv20pmS3CurrentEntry 6 }

     xldv20pmS3cElapsedTime OBJECT-TYPE
     -- info-VALUE:     0..900 (sec) for min15
     -- info-VALUE:     0..1440 (min) for day
     -- info-ROU:       GET db
     -- info-IMPL:      V2.0-> 
     -- info-VERCTRL:   -
        SYNTAX        INTEGER (0..1440)
        ACCESS        read-only
        STATUS        mandatory
        DESCRIPTION
             " This read-only attribute represents the difference
               between the current time and the start of the present
               summary interval.
	       Units are seconds when 15 minute interval is selected 
	       or minutes for day interval.
               (Note: accurate to the sample time interval). "
        ::= { xldv20pmS3CurrentEntry 7 }

     xldv20pmS3cThresholdLimitIndex OBJECT-TYPE
     -- info-VALUE:     1..30
     -- info-ROU:       GET db  SET pm
     -- info-IMPL:      V2.0-> 
     -- info-VERCTRL:   -
        SYNTAX        INTEGER (1..30)
        ACCESS        read-write
        STATUS        mandatory
        DESCRIPTION
             " This read-write attribute provides an index to the
               ThresholdLimitTable managed entity that contains the
               threshold limits for the performance parameters reported
               by this managed entity.
               When the value of this attribute is set to 1, default
               threshold limits shall be used
               The range of values is [1,30]. "
        ::= { xldv20pmS3CurrentEntry 8 }

