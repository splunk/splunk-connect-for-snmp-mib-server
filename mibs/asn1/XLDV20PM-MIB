     -- --------------------------------------------------------------
     --           PMo  D S 3  H i s t o r y  D a t a  Table
     -- --------------------------------------------------------------

     xldv20pmD3HistoryTable OBJECT-TYPE
     -- info-IMPL:      V2.0-> 
     -- info-VERCTRL:   -
        SYNTAX        SEQUENCE OF Xldv20pmD3HistoryEntry
        ACCESS        not-accessible
        STATUS        mandatory
        DESCRIPTION
             " This table contains the entries for the PMo history
               objects at the near-end and far-end DS3 interfaces.
               It contains results that are overtaken from the
               D3CurrentTable PM-object at the end of a 15 min /day
               interval . "
        --DEFAULT next-function-async     fsnPmD3hEntryNext
        --DEFAULT get-function-async      fsnPmD3hEntryGet
        ::= { xldv20pmPerformanceMonitoring 7 }

     xldv20pmD3HistoryEntry OBJECT-TYPE
     -- info-IMPL:      V2.0-> 
     -- info-VERCTRL:   -
        SYNTAX        Xldv20pmD3HistoryEntry
        ACCESS        not-accessible
        STATUS        mandatory
        DESCRIPTION
             " An entry describing the DS3 near-end and far-end History
               Data Table . "
        INDEX { xldv20IfIndex,
                xldv20pmD3hIntervalType,
                xldv20pmD3hOAMLevel,
                xldv20pmD3hHistEntryNr }
        ::= { xldv20pmD3HistoryTable 1 }

     Xldv20pmD3HistoryEntry ::=
        SEQUENCE { xldv20pmD3hIntervalType         Xldv20pmIntervalType,
                   xldv20pmD3hOAMLevel                   INTEGER,
                   xldv20pmD3hHistEntryNr                INTEGER,
                   xldv20pmD3hSuspectFlag          Xldv20pmSuspectType,
                   xldv20pmD3hPeriodEndTime              INTEGER,
                   xldv20pmD3hNeErroredSecondsCP           Counter,
                   xldv20pmD3hFeErroredSecondsCP           Counter,
                   xldv20pmD3hNeErroredSecondsPLCP         Counter,
                   xldv20pmD3hFeErroredSecondsPLCP         Counter,
                   xldv20pmD3hNeSeverelyErroredSecondsCP   Counter,
                   xldv20pmD3hFeSeverelyErroredSecondsCP   Counter,
                   xldv20pmD3hNeSeverelyErroredSecondsPLCP Counter,
                   xldv20pmD3hFeSeverelyErroredSecondsPLCP Counter,
                   xldv20pmD3hNeCodeViolationsP            Counter,
                   xldv20pmD3hNeCodeViolationsCP           Counter,
                   xldv20pmD3hFeCodeViolationsCP           Counter,
                   xldv20pmD3hNeCodeViolationsPLCP         Counter,
                   xldv20pmD3hFeCodeViolationsPLCP         Counter,
                   xldv20pmD3hNeUnavailableSeconds         Counter,
                   xldv20pmD3hFeUnavailableSeconds         Counter,
                   xldv20pmD3hNeUnavailableSecondsPLCP     Counter,
                   xldv20pmD3hFeUnavailableSecondsPLCP     Counter,
                   xldv20pmD3hNeSas                        Counter,
                   xldv20pmD3hFeSas                        Counter }

     xldv20pmD3hIntervalType OBJECT-TYPE
     -- info-VALUE:     min15(1), day(2)
     -- info-IMPL:      V2.0-> 
     -- info-VERCTRL:   -
        SYNTAX        Xldv20pmIntervalType
        ACCESS        read-only
        STATUS        mandatory
        DESCRIPTION
             " This type attribute is used to select between the
               15 min  or the day accumulation types. "
        ::= { xldv20pmD3HistoryEntry 1 }

     xldv20pmD3hOAMLevel OBJECT-TYPE
     -- info-VALUE:     1..3
     -- info-IMPL:      V2.0-> 
     -- info-VERCTRL:   -
