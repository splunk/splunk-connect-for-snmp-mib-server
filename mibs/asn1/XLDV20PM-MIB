     -- ==============================================================
     --       P e r f o r m a n c e   M o n i t o r i n g  (PMo)
     --                         g r o u p
     -- ==============================================================

     -- ------------------------------------------------------------------
     --  PMo o n  p h y s i c a l  l a y e r
     -- ------------------------------------------------------------------
     --
     -- Description of the PMo Events of the PM common group.
     -- To the common group belong those type of TLM interfaces
     -- which have the same performance monitoring counters.
     -- These are the interface types : adc, adr, vdc, vdr, e3n, e3f.
     --
     -- Errored Seconds (ES)
     -- An Errored Second is a  one second interval containing one or
     -- more crc-i or crc-ni anomalies, or one or more loss of signal
     -- or severely errored frame (SEF) defects applicable to adc/adr
     -- interfaces
     --
     -- Severely Errored Seconds (SEF)
     -- A one second period in which at least 30% of the blocks are in
     -- error or as a one second period which contains a Severely
     -- Disturbed Period . SDP is usually estimated as the occurrence
     -- of a network defect, such as loss of signal, loss of frame,
     -- or other alarm indicators
     --
     -- Background Block Error (BBE)
     -- A Background Block Error is an errored-block not occurring as
     -- part of an SES
     --
     -- Unavailable Seconds (UAS)
     -- A period of unavailable time begins at the onset of 10 consecu-
     -- tive SES events. These ten seconds are considered to be part of
     -- unavailable time. A new period of available time begins at the
     -- onset of ten consecutive non-SES events. These 10 seconds are
     -- considered to be part of available time.
     -- Note: during Unavailable Seconds (UAS), performance parameters
     --       are not counted, exception is the UAS themselves, they
     --       are counted further.



     -- --------------------------------------------------------------
     --               PMo Common Current Data Table
     -- --------------------------------------------------------------

     xldv20pmCommonCurrentTable OBJECT-TYPE
     -- info-IMPL:      V2.0-> 
     -- info-VERCTRL:   -
        SYNTAX        SEQUENCE OF Xldv20pmCommonCurrentEntry
        ACCESS        not-accessible
        STATUS        mandatory
        DESCRIPTION
             " This table contains the entries for the current 15 minute interval
               or the current day of PMo objects of the common group.
               (adc, adr, e3n and e3f) . "
        --DEFAULT test-function-async     fsnPmCcEntryTest
        --DEFAULT next-function-async     fsnPmCcEntryNext
        --DEFAULT set-function-async      fsnPmCcEntrySet
        --DEFAULT get-function-async      fsnPmCcEntryGet
        ::= { xldv20pmPerformanceMonitoring 2 }

     xldv20pmCommonCurrentEntry OBJECT-TYPE
     -- info-IMPL:      V2.0-> 
     -- info-VERCTRL:   -
        SYNTAX        Xldv20pmCommonCurrentEntry
        ACCESS        not-accessible
        STATUS        mandatory
        DESCRIPTION
             " An entry describing the Common Current Data Table . "
        INDEX { xldv20IfIndex,
                xldv20pmCcIntervalType,
                xldv20pmCcOAMLevel }
        ::= { xldv20pmCommonCurrentTable 1 }

     Xldv20pmCommonCurrentEntry ::=
        SEQUENCE { xldv20pmCcIntervalType         Xldv20pmIntervalType,
                   xldv20pmCcOAMLevel                    INTEGER,
                   xldv20pmCcAdminState           Xldv20AdminState,
                   xldv20pmCcSuspectFlag          Xldv20pmSuspectType,
                   xldv20pmCcHistEntrySize               INTEGER,
                   xldv20pmCcNrHistEntriesAvailable      INTEGER,
                   xldv20pmCcElapsedTime                 INTEGER,
                   xldv20pmCcThresholdLimitIndex         INTEGER,
                   xldv20pmCcNeErroredSeconds            Counter,
                   xldv20pmCcFeErroredSeconds            Counter,
                   xldv20pmCcNeSeverelyErroredSeconds    Counter,
                   xldv20pmCcFeSeverelyErroredSeconds    Counter,
                   xldv20pmCcNeBackgroundBlockErrors     Counter,
                   xldv20pmCcFeBackgroundBlockErrors     Counter,
                   xldv20pmCcNeUnavailableSeconds        Counter,
                   xldv20pmCcFeUnavailableSeconds        Counter }

     xldv20pmCcIntervalType OBJECT-TYPE
     -- info-VALUE:     min15(1), day(2)
     -- info-ROU:       GET  db
     -- info-IMPL:      V2.0-> 
     -- info-VERCTRL:   -
        SYNTAX        Xldv20pmIntervalType
        ACCESS        read-only
        STATUS        mandatory
        DESCRIPTION
             " This attribute is used to select between the
               15 min  or the day accumulation types. "
        ::= { xldv20pmCommonCurrentEntry 1 }

     xldv20pmCcOAMLevel OBJECT-TYPE
     -- info-VALUE:     1..3
     -- info-ROU:       GET db
     -- info-IMPL:      V2.0-> 
     -- info-VERCTRL:   -
        SYNTAX        INTEGER (1..3)
        ACCESS        read-only
        STATUS        mandatory
        DESCRIPTION
             " This attribute is used to select between the
               3 OAM levels f1 (1), f2 (2) and f3 (3) . "
        ::= { xldv20pmCommonCurrentEntry 2 }

     xldv20pmCcAdminState OBJECT-TYPE
     -- info-VALUE:     unlocked(1), locked(2)
     -- info-ROU:       GET db  SET pm
     -- info-IMPL:      V2.0-> 
     -- info-VERCTRL:   -
        SYNTAX        Xldv20AdminState
        ACCESS        read-write
        STATUS        mandatory
        DESCRIPTION
             " This read-write attribute is used to activate (unlock)
               and deactivate (lock) the functions (incl. collection)
               performed by this managed entity.
               Note: the 15min  collection object must be activated,
                     before day collection can operate. "
        ::= { xldv20pmCommonCurrentEntry 3 }

     xldv20pmCcSuspectFlag OBJECT-TYPE
     -- info-VALUE:     reliable(1), suspect(2)
     -- info-ROU:       GET db
     -- info-IMPL:      V2.0-> 
     -- info-VERCTRL:   -
        SYNTAX        Xldv20pmSuspectType
        ACCESS        read-only
        STATUS        mandatory
        DESCRIPTION
             " This read-only attribute will be set to indicate that
               troubles arose during an interval and that the counter
               values are suspect and incomplete. "
        ::= { xldv20pmCommonCurrentEntry 4 }

     xldv20pmCcHistEntrySize OBJECT-TYPE
     -- info-VALUE:     1..96
     -- info-ROU:       GET db
     -- info-IMPL:      V2.0-> 
     -- info-VERCTRL:   -
        SYNTAX        INTEGER (1..96)
        ACCESS        read-only
        STATUS        mandatory
        DESCRIPTION
             " The number of 15min history entries configured at the particular interface. This
               attribute is used for history data collection.
               The default value is 1 and means at least one history entry
               is desired . "
        ::= { xldv20pmCommonCurrentEntry 5 }

     xldv20pmCcNrHistEntriesAvailable OBJECT-TYPE
     -- info-VALUE:     0..96
     -- info-ROU:       GET db
     -- info-IMPL:      V2.0-> 
     -- info-VERCTRL:   -
        SYNTAX        INTEGER (0..96)
        ACCESS        read-only
        STATUS        mandatory
        DESCRIPTION
             " This read-only attribute indicates how many HistEntries
               are already available.
               The range of values is between 0 and xldv20pmCcHistEntrySize,
               thereby 0 means that this entry is not valid . "
        ::= { xldv20pmCommonCurrentEntry 6 }

     xldv20pmCcElapsedTime OBJECT-TYPE
     -- info-VALUE:     0..900 (sec) for min15,
     -- info-VALUE:     0..1440 (min) for day
     -- info-ROU:       GET db
     -- info-IMPL:      V2.0-> 
     -- info-VERCTRL:   -
        SYNTAX        INTEGER(0..1440)
        ACCESS        read-only
        STATUS        mandatory
