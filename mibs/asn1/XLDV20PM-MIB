             " An entry describing the DS3 near-end and far-end Current
               Data Table . "
        INDEX { xldv20IfIndex,
                xldv20pmD3cIntervalType,
                xldv20pmD3cOAMLevel }
        ::= { xldv20pmD3CurrentTable 1 }

     Xldv20pmD3CurrentEntry ::=
        SEQUENCE {xldv20pmD3cIntervalType          Xldv20pmIntervalType,
                  xldv20pmD3cOAMLevel                     INTEGER,
                  xldv20pmD3cAdminState            Xldv20AdminState,
                  xldv20pmD3cSuspectFlag           Xldv20pmSuspectType,
                  xldv20pmD3cHistEntrySize                INTEGER,
                  xldv20pmD3cNrHistEntriesAvailable       INTEGER,
                  xldv20pmD3cElapsedTime                  INTEGER,
                  xldv20pmD3cThresholdLimitIndex          INTEGER,
                  xldv20pmD3cNeErroredSecondsCP           Counter,
                  xldv20pmD3cFeErroredSecondsCP           Counter,
                  xldv20pmD3cNeErroredSecondsPLCP         Counter,
                  xldv20pmD3cFeErroredSecondsPLCP         Counter,
                  xldv20pmD3cNeSeverelyErroredSecondsCP   Counter,
                  xldv20pmD3cFeSeverelyErroredSecondsCP   Counter,
                  xldv20pmD3cNeSeverelyErroredSecondsPLCP Counter,
                  xldv20pmD3cFeSeverelyErroredSecondsPLCP Counter,
                  xldv20pmD3cNeCodeViolationsP            Counter,
                  xldv20pmD3cNeCodeViolationsCP           Counter,
                  xldv20pmD3cFeCodeViolationsCP           Counter,
                  xldv20pmD3cNeCodeViolationsPLCP         Counter,
                  xldv20pmD3cFeCodeViolationsPLCP         Counter,
                  xldv20pmD3cNeUnavailableSeconds         Counter,
                  xldv20pmD3cFeUnavailableSeconds         Counter,
                  xldv20pmD3cNeUnavailableSecondsPLCP     Counter,
                  xldv20pmD3cFeUnavailableSecondsPLCP     Counter,
                  xldv20pmD3cNeSas                        Counter,
                  xldv20pmD3cFeSas                        Counter }

     xldv20pmD3cIntervalType OBJECT-TYPE
     -- info-VALUE:     min15(1), day(2)
