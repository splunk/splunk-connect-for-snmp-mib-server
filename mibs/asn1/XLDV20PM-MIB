             " This table contains the enpoints for PMo on Atm layer. "
        --DEFAULT next-function-async     fsnPmAeEntryNext
        --DEFAULT get-function-async      fsnPmAeEntryGet
        ::= { xldv20pmPerformanceMonitoring 11 }

     xldv20pmAtmEndpointsEntry OBJECT-TYPE
     -- info-IMPL:      V2.0-> 
     -- info-VERCTRL:   -
