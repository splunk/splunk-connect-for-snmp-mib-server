     -- ==============================================================
     --         P r o t o c o l   M o n i t o r i n g  (PrM)
     --                         g r o u p
     -- ==============================================================

     -- --------------------------------------------------------------
     --    PrM  T C - A d a p t o r  C u r r e n t  D a t a  Table
     -- --------------------------------------------------------------

     xldv20pmTcaPrMCurrentTable OBJECT-TYPE
     -- info-IMPL:      V2.0-> 
     -- info-VERCTRL:   -
        SYNTAX        SEQUENCE OF Xldv20pmTcaPrMCurrentEntry
        ACCESS        not-accessible
        STATUS        mandatory
        DESCRIPTION
             " This table contains the entries for the PrM object at
               the following tca layer locations:
                - STM1 interface (in downstream direction)
                - STS3-c interface (in downstream direction)
                - E3 interface (in downstream direction)
                - DS3 interface (in downstream direction)
                - SU_AD16 interfaces (in upstream stream direction).
               PrM also possible at IMA groups.
               It includes attributes for performance parameters,
               collection interval is 15 min /one day . "
        --DEFAULT test-function-async     fsnTcEntryTest
        --DEFAULT next-function-async     fsnTcEntryNext
        --DEFAULT set-function-async      fsnTcEntrySet
        --DEFAULT get-function-async      fsnTcEntryGet
        ::= { xldv20pmProtocolMonitoring 1 }

     xldv20pmTcaPrMCurrentEntry OBJECT-TYPE
     -- info-IMPL:      V2.0-> 
     -- info-VERCTRL:   -
        SYNTAX        Xldv20pmTcaPrMCurrentEntry
        ACCESS        not-accessible
        STATUS        mandatory
        DESCRIPTION
             " An entry describing the TC-Adaptor Current Data Table
               . "
        INDEX { xldv20IfIndex,
                xldv20pmTcIntervalType }
        ::= { xldv20pmTcaPrMCurrentTable 1 }

     Xldv20pmTcaPrMCurrentEntry ::=
        SEQUENCE { xldv20pmTcIntervalType     Xldv20pmIntervalType,
                   xldv20pmTcAdminState       Xldv20AdminState,
                   xldv20pmTcSuspectFlag      Xldv20pmSuspectType,
                   xldv20pmTcHistEntrySize            INTEGER,
                   xldv20pmTcNrHistEntriesAvailable   INTEGER,
                   xldv20pmTcElapsedTime              INTEGER,
                   xldv20pmTcThresholdLimitIndex      INTEGER,
                   xldv20pmTcDiscardedCells           Counter }

     xldv20pmTcIntervalType OBJECT-TYPE
     -- info-VALUE:     min15(1), day(2)
     -- info-ROU:       GET db
     -- info-IMPL:      V2.0-> 
     -- info-VERCTRL:   -
        SYNTAX        Xldv20pmIntervalType
        ACCESS        read-only
        STATUS        mandatory
        DESCRIPTION
             " This nac type attribute is used to select between the
               15 min  or the day accumulation types. "
        ::= { xldv20pmTcaPrMCurrentEntry 1 }

     xldv20pmTcAdminState OBJECT-TYPE
     -- info-VALUE:     unlocked(1), locked(2)
     -- info-ROU:       GET db  SET pm
     -- info-IMPL:      V2.0-> 
     -- info-VERCTRL:   -
        SYNTAX        Xldv20AdminState
        ACCESS        read-write
        STATUS        mandatory
        DESCRIPTION
             " This read-write attribute is used to activate (unlock)
               and deactivate (lock) the functions (incl. collection)
               performed by this managed entity.
               Note: the 15min  collection object must be activated,
                     before day collection can operate. "
        ::= { xldv20pmTcaPrMCurrentEntry 2 }

     xldv20pmTcSuspectFlag OBJECT-TYPE
     -- info-VALUE:     reliable(1), suspect(2)
     -- info-ROU:       GET db
     -- info-IMPL:      V2.0-> 
     -- info-VERCTRL:   -
        SYNTAX        Xldv20pmSuspectType
        ACCESS        read-only
        STATUS        mandatory
        DESCRIPTION
             " This read-only attribute will be set to indicate that
               troubles arose during an interval and that the counter
               values are suspect and incomplete. "
        ::= { xldv20pmTcaPrMCurrentEntry 3 }

     xldv20pmTcHistEntrySize OBJECT-TYPE
     -- info-VALUE:     1..96
     -- info-ROU:       GET db
     -- info-IMPL:      V2.0-> 
     -- info-VERCTRL:   -
        SYNTAX        INTEGER (1..96)
        ACCESS        read-only
        STATUS        mandatory
        DESCRIPTION
             " This read-only attribute indicates the number of 15min 
               history entries configured for the particular interface.
               The default value is 1 and means that at least one 
               history entry is provided. "
        ::= { xldv20pmTcaPrMCurrentEntry 4 }

     xldv20pmTcNrHistEntriesAvailable OBJECT-TYPE
     -- info-VALUE:     0..96
     -- info-ROU:       GET db
     -- info-IMPL:      V2.0-> 
     -- info-VERCTRL:   -
        SYNTAX        INTEGER (0..96)
        ACCESS        read-only
        STATUS        mandatory
        DESCRIPTION
             " This read-only attribute indicates how many HistEntries
               are already available.
               The range of values is between 0 and xldv20pmTcHistEntrySize,
               thereby 0 means that this entry is not valid . "
        ::= { xldv20pmTcaPrMCurrentEntry 5 }

     xldv20pmTcElapsedTime OBJECT-TYPE
     -- info-VALUE:     0..900 (sec) for min15,
     -- info-VALUE:     0..1440 (min) for day
     -- info-ROU:       GET db
     -- info-IMPL:      V2.0-> 
     -- info-VERCTRL:   -
        SYNTAX        INTEGER (0..1440)
        ACCESS        read-only
        STATUS        mandatory
        DESCRIPTION
             " This read-only attribute represents the difference
               between the current time and the start of the present
               summary interval.
	       Units are seconds when 15 minute interval is selected 
	       or minutes for day interval.
               (Note: accurate to the sample time interval). "
        ::= { xldv20pmTcaPrMCurrentEntry 6 }

     xldv20pmTcThresholdLimitIndex OBJECT-TYPE
     -- info-VALUE:     1..30
     -- info-ROU:       GET db  SET pm
     -- info-IMPL:      V2.0-> 
     -- info-VERCTRL:   -
        SYNTAX        INTEGER (1..30)
        ACCESS        read-write
        STATUS        mandatory
        DESCRIPTION
             " This read-write attribute provides an index to the
               ThresholdLimitTable managed entity that contains the
               threshold limits for the performance parameters reported
               by this managed entity.
               When the value of this attribute is set to 1, default
               threshold limits shall be used. These default values are
               not part of the threshold limit table but are hard coded
               in the application SW of PM. "
        ::= { xldv20pmTcaPrMCurrentEntry 7 }

     xldv20pmTcDiscardedCells OBJECT-TYPE
     -- info-VALUE:     -
     -- info-ROU:       GET db
     -- info-IMPL:      V2.0-> 
     -- info-VERCTRL:   -
        SYNTAX        Counter
        ACCESS        read-only
        STATUS        mandatory
        DESCRIPTION
             " This read-only attribute represents a counter of the
               number of ATM cells discarded at this interface, due to
               a HEC violation, since the start of this interval . "
        ::= { xldv20pmTcaPrMCurrentEntry 8 }


     -- --------------------------------------------------------------
     --    PrM  T C - A d a p t o r  H i s t o r y  D a t a  Table
     -- --------------------------------------------------------------

