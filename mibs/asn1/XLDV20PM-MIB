        ::= { xldv20pmImaGroupCurrentEntry 9 }

     -- --------------------------------------------------------------
     --          PMo  I M A Group  H i s t o r y  subtree
     -- --------------------------------------------------------------

     -- This section combines the 
     -- the IMA Group Interval Statistics subtree and
     -- the IMA Group Total Statistics subtree.
     -- It consists of a single table:
     -- the IMA Group History Table.  
     -- 
     -- Selection the xldv20pmImaGroupHistoryIntervalType min15 :
     -- the entries contain various statistics
     -- collected by each IMA Group over the previous 24 hours of operation.
     --
     -- Selection the xldv20pmImaGroupHistoryIntervalType day :
     -- The entries contain the cumulative sum
     -- of the various statistics collected by each IMA Group for the 24 hour
     -- period preceding the current interval.
     --

     xldv20pmImaGroupHistoryTable OBJECT-TYPE
     -- info-IMPL:      V2.0-> 
     -- info-VERCTRL:   -
        SYNTAX        SEQUENCE OF Xldv20pmImaGroupHistoryEntry
        ACCESS        not-accessible
        STATUS        mandatory
        DESCRIPTION
             " This table contains the entries for the 15 min /one
             day-history data collection results for PMo objects 
             of  IMA Group interfaces. "
        --DEFAULT test-function-async     fsnPmIghEntryTest
        --DEFAULT next-function-async     fsnPmIghEntryNext
        --DEFAULT set-function-async      fsnPmIghEntrySet
        --DEFAULT get-function-async      fsnPmIghEntryGet
        ::= { xldv20pmPerformanceMonitoring 15 }

     xldv20pmImaGroupHistoryEntry OBJECT-TYPE
     -- info-IMPL:      V2.0-> 
     -- info-VERCTRL:   -
        SYNTAX        Xldv20pmImaGroupHistoryEntry
        ACCESS        not-accessible
        STATUS        mandatory
        DESCRIPTION
             " An entry describing the History Data Table for PMo 
             IMA groups. "
        INDEX { xldv20IfIndex,
                xldv20pmImaGroupHistoryIntervalType,
                xldv20pmImaGroupHistoryHistEntryNr }
        ::= { xldv20pmImaGroupHistoryTable 1 }

     Xldv20pmImaGroupHistoryEntry ::=
        SEQUENCE { xldv20pmImaGroupHistoryIntervalType    Xldv20pmIntervalType,
                   xldv20pmImaGroupHistoryHistEntryNr           INTEGER,
                   xldv20pmImaGroupHistorySuspectFlag     Xldv20pmSuspectType,
                   xldv20pmImaGroupHistoryPeriodEndTime         INTEGER,
                   xldv20pmImaGroupHistoryUnavailSecs           Gauge,
                   xldv20pmImaGroupHistoryNeNumFailures         Gauge
                 }

     xldv20pmImaGroupHistoryIntervalType OBJECT-TYPE
     -- info-VALUE:     min15(1), day(2)
     -- info-ROU:       GET db
     -- info-IMPL:      V2.0-> 
     -- info-VERCTRL:   -
        SYNTAX        Xldv20pmIntervalType
        ACCESS        read-only
        STATUS        mandatory
        DESCRIPTION
             " This type attribute is used to select between the
               15 min  or the day accumulation types. "
        ::= { xldv20pmImaGroupHistoryEntry 1 }

     xldv20pmImaGroupHistoryHistEntryNr OBJECT-TYPE
     -- info-VALUE:     1..96
     -- info-ROU:       GET db
     -- info-IMPL:      V2.0-> 
     -- info-VERCTRL:   -
        SYNTAX        INTEGER (1..96)
        ACCESS        read-only
        STATUS        mandatory
        DESCRIPTION
             " This attribute is used as index to address a history entry.
             Entry 1 is the most recent, entry 96 is 24 hours ago.
             The range of values is between 1 and xldv20pmImaGroupCurrentHistEntrySize . " 
        ::= { xldv20pmImaGroupHistoryEntry 2 }

     xldv20pmImaGroupHistorySuspectFlag OBJECT-TYPE
     -- info-VALUE:     reliable(1), suspect(2)
     -- info-ROU:       GET db
     -- info-IMPL:      V2.0-> 
     -- info-VERCTRL:   -
        SYNTAX        Xldv20pmSuspectType
        ACCESS        read-only
        STATUS        mandatory
        DESCRIPTION
             " This read-only attribute will be set to indicate that
               troubles arose during an interval and that the counter
               values are suspect and incomplete. "
        ::= { xldv20pmImaGroupHistoryEntry 3 }

     xldv20pmImaGroupHistoryPeriodEndTime OBJECT-TYPE
     -- info-VALUE:     0..2147483647
     -- info-ROU:       GET  db
     -- info-IMPL:      V2.0-> 
     -- info-VERCTRL:   -
        SYNTAX        INTEGER(0..2147483647)
        ACCESS        read-only
        STATUS        mandatory
        DESCRIPTION
             " Date/time at the end of the selected interval. "
        ::= { xldv20pmImaGroupHistoryEntry 4 }

     xldv20pmImaGroupHistoryUnavailSecs OBJECT-TYPE
     -- info-VALUE:     -
     -- info-ROU:       GET db
     -- info-IMPL:      V2.0-> 
     -- info-VERCTRL:   -
        SYNTAX        Gauge
        ACCESS        read-only
        STATUS        mandatory
        DESCRIPTION
        " The counts of second intervals where the IMA group
          traffic state machine is down in the current
          15 min /day interval. "
        ::= { xldv20pmImaGroupHistoryEntry 5 }
     
     xldv20pmImaGroupHistoryNeNumFailures OBJECT-TYPE
     -- info-VALUE:     -
     -- info-ROU:       GET db
     -- info-IMPL:      V2.0-> 
     -- info-VERCTRL:   -
        SYNTAX        Gauge
        ACCESS        read-only
        STATUS        mandatory
        DESCRIPTION
        " The number of times a near-end group failure
          (config-Aborted, unsufficient-Links) has been reported 
          for a previous 15 min interval or the previous day. "
        ::= { xldv20pmImaGroupHistoryEntry 6 }

