             " An entry of xldv20pmAtmEndpointsTable. "
        INDEX { xldv20pmAoIndex,
                xldv20pmAeEndpointType }
        ::= { xldv20pmAtmEndpointsTable 1 }

     Xldv20pmAtmEndpointsEntry ::=
        SEQUENCE { xldv20pmAeEndpointType         Xldv20pmEndpointType,
                   xldv20pmAeConnectionIndex          INTEGER,
                   xldv20pmAeConnectionIndexType  Xldv20pmConnectionIndexType,
                   xldv20pmAeRole                 Xldv20pmRoleType,
                   xldv20pmAeFlowDirection        Xldv20FlowDirection }

     xldv20pmAeEndpointType OBJECT-TYPE
     -- info-VALUE:     source(1), sink(2)
     -- info-ROU:       GET  db
     -- info-IMPL:      V2.0-> 
     -- info-VERCTRL:   -
        SYNTAX        Xldv20pmEndpointType
        ACCESS        read-only
        STATUS        mandatory
        DESCRIPTION
             " Identifies the type of endpoint. "
        ::= { xldv20pmAtmEndpointsEntry 1 }
     
     xldv20pmAeConnectionIndex OBJECT-TYPE
     -- info-VALUE:     1..2048
     -- info-ROU:       GET  db
     -- info-IMPL:      V2.0-> 
     -- info-VERCTRL:   -
        SYNTAX        INTEGER (1..2048)
        ACCESS        read-only
        STATUS        mandatory
        DESCRIPTION
             " This attribute identifies the Vpl or Vcl cross connection
             where an endpoint object should be added. "
        ::= { xldv20pmAtmEndpointsEntry 2 }

    xldv20pmAeConnectionIndexType OBJECT-TYPE
     -- info-VALUE:     vplUni(1),vplNni(2),vclUni(3),vclNni(4)
     -- info-ROU:       GET db
     -- info-IMPL:      V2.0-> 
     -- info-VERCTRL:   -
        SYNTAX        Xldv20pmConnectionIndexType
        ACCESS        read-only
        STATUS        mandatory
        DESCRIPTION
             " This attribute identifies the type of connection index
             and the termination point where an endpoint should be added. "
        ::= { xldv20pmAtmEndpointsEntry 3 }
    
    xldv20pmAeRole OBJECT-TYPE
     -- info-VALUE:     analyseWithBackwardReporting(1),
     -- info-VALUE:     analyseWithoutBackwardReporting(2),
     -- info-VALUE:     generateWithCollect(3), generateWithoutCollect(4)}
     -- info-ROU:       GET db
     -- info-IMPL:      V2.0-> 
     -- info-VERCTRL:   -
        SYNTAX        Xldv20pmRoleType
        ACCESS        read-only
        STATUS        mandatory
        DESCRIPTION
             " The role of endpoint.
             'analyseWithBackwardReporting' (sink) means extracting
             forward monitoring (FM) OAM cells, sending back 
             backward reporting OAM cells and no data collection
             for current and history table.  
             'analyseWithoutBackwardReporting' (sink) means extracting
             FM cells and also collecting data.  
             'generateWithCollect' (source) means inserting FM cells,
             extracting backward reporting cells if they arrive 
             and collect data for current and history table.  
             'generateWithoutCollect' (source) means inserting FM cells
             but without collecting data.
             The CMU, the Ethernet part of the Twin NT can be 
             inserting and extracting points.  "
        ::= { xldv20pmAtmEndpointsEntry 4 }

    xldv20pmAeFlowDirection OBJECT-TYPE
     -- info-VALUE:     inwards(2), outwards(3)
     -- info-ROU:       GET db
     -- info-IMPL:      V2.0-> 
     -- info-VERCTRL:   -
        SYNTAX        Xldv20FlowDirection
        ACCESS        read-only
        STATUS        mandatory
        DESCRIPTION
             " This attribute identifies the flow direction.
               inwards means flow direction from outside (external interface)
               to cross connection.
               outwards means flow direction from inside to external interface. "
        ::= { xldv20pmAtmEndpointsEntry 5 }

