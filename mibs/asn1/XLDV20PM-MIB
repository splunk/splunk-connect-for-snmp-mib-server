     -- --------------------------------------------------------------
     --          PMo  The IMA Group Current Statistics subtree
     -- --------------------------------------------------------------
     --
     --
     -- The IMA Group Current Statistics subtree consists of a single table:
     -- the IMA Group Current Table. 
     -- 
     -- Selection of xldv20pmImaGroupIntervalType min15 :
     -- The entries contain the cumulative sum
     -- collected by each IMA group for the current 15 minute interval.
     -- 
     -- Selection of xldv20pmImaGroupIntervalType day  :
     -- The entries contain the cumulative sum
     -- of the various statistics collected by each IMA group for the 24 hour
     -- period preceding the current interval
     --

     xldv20pmImaGroupCurrentTable OBJECT-TYPE
     -- info-IMPL:      V2.0-> 
        SYNTAX        SEQUENCE OF Xldv20pmImaGroupCurrentEntry
        ACCESS        not-accessible
        STATUS        mandatory
        DESCRIPTION
             " This table contains the entries for the PMo objects
               for the current data of Pmo monitoring objects
               for IMA groups . 
               Collection interval is the current 15 minute and one the
               current day . "
        --DEFAULT test-function-async     fsnPmIgcEntryTest
        --DEFAULT next-function-async     fsnPmIgcEntryNext
        --DEFAULT set-function-async      fsnPmIgcEntrySet
        --DEFAULT get-function-async      fsnPmIgcEntryGet
        ::= { xldv20pmPerformanceMonitoring 14 }

     xldv20pmImaGroupCurrentEntry OBJECT-TYPE
     -- info-IMPL:      V2.0-> 
     -- info-VERCTRL:   -
        SYNTAX        Xldv20pmImaGroupCurrentEntry
        ACCESS        not-accessible
        STATUS        mandatory
        DESCRIPTION
             " An entry describing the current PMo data for ImaGroups. "
        INDEX { xldv20IfIndex,
                xldv20pmImaGroupCurrentIntervalType }
        ::= { xldv20pmImaGroupCurrentTable 1 }

     Xldv20pmImaGroupCurrentEntry ::=
        SEQUENCE { xldv20pmImaGroupCurrentIntervalType         Xldv20pmIntervalType,
                   xldv20pmImaGroupCurrentAdminState           Xldv20AdminState,
