     xldv20pmAtmObjectsTable OBJECT-TYPE
     -- info-IMPL:      V2.0-> 
     -- info-VERCTRL:   -
        SYNTAX        SEQUENCE OF Xldv20pmAtmObjectsEntry
        ACCESS        not-accessible
        STATUS        mandatory
        DESCRIPTION
             " This table contains the entries for the PMo objects 
               on ATM layer. Maximum of monitored connections is 128. "
        --DEFAULT test-function-async     fsnPmAoEntryTest
        --DEFAULT next-function-async     fsnPmAoEntryNext
        --DEFAULT set-function-async      fsnPmAoEntrySet
        --DEFAULT get-function-async      fsnPmAoEntryGet
        ::= { xldv20pmPerformanceMonitoring 10 }

     xldv20pmAtmObjectsEntry OBJECT-TYPE
     -- info-IMPL:      V2.0-> 
     -- info-VERCTRL:   -
