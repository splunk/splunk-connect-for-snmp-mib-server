        DESCRIPTION
             " This table contains the entries for the current 15 minute interval
               or the current day for STS3c near-end and far-end PPTPs. "
        --DEFAULT test-function-async     fsnPmS3cEntryTest
        --DEFAULT next-function-async     fsnPmS3cEntryNext
        --DEFAULT set-function-async      fsnPmS3cEntrySet
        --DEFAULT get-function-async      fsnPmS3cEntryGet
        ::= { xldv20pmPerformanceMonitoring 4 }

     xldv20pmS3CurrentEntry OBJECT-TYPE
     -- info-IMPL:      V2.0-> 
