             " Identifies an entry of the xldv20pmAtmObjectsTable. "
        ::= { xldv20pmAtmObjectsEntry 1 }
    
    xldv20pmAoFlowType OBJECT-TYPE
     -- info-VALUE:     f4Segment(1), f4EndToEnd(2)
     -- info-VALUE:     f5Segment(3), f5EndToEnd(4)
