     -- --------------------------------------------------------------
     --           Thresholding Table for PM on ATM layer
     -- --------------------------------------------------------------

     xldv20pmAtmThresholdLimitTable OBJECT-TYPE
     -- info-IMPL:      V2.0-> 
     -- info-VERCTRL:   -
        SYNTAX        SEQUENCE OF Xldv20pmAtmThresholdLimitEntry
        ACCESS        not-accessible
        STATUS        mandatory
        DESCRIPTION
             " The threshold limit table for all interface. 
               This table associates threshold limit index/PM counter
               pairs with threshold limit values for a 15 minutes
               interval. Entries with same threshold limit index
               belong to the same threshold limit profile.
               A crossing of a threshold limit leads to a 
               threshold crossing alert. "
        --DEFAULT test-function-async     fsnPmAtlEntryTest
        --DEFAULT next-function-async     fsnPmAtlEntryNext
        --DEFAULT set-function-async      fsnPmAtlEntrySet
        --DEFAULT get-function-async      fsnPmAtlEntryGet
        ::= { xldv20pmThresholding 2 }

     xldv20pmAtmThresholdLimitEntry OBJECT-TYPE
     -- info-IMPL:      V2.0-> 
     -- info-VERCTRL:   -
        SYNTAX        Xldv20pmAtmThresholdLimitEntry
        ACCESS        not-accessible
        STATUS        mandatory
        DESCRIPTION
             " An entry describing the threshold limit information . "
        INDEX { xldv20pmAtlThresholdLimitIndex }
        ::= { xldv20pmAtmThresholdLimitTable 1 }

     Xldv20pmAtmThresholdLimitEntry ::=
        SEQUENCE { xldv20pmAtlThresholdLimitIndex      INTEGER,
                   xldv20pmAtlTransmittedUserCells     Unsigned32,
                   xldv20pmAtlLostUserCells            Unsigned32,
                   xldv20pmAtlMisinsertedUserCells     Unsigned32
		   }

     xldv20pmAtlThresholdLimitIndex OBJECT-TYPE
     -- info-VALUE:     1..30
     -- info-ROU:       GET  db
     -- info-IMPL:      V2.0-> 
     -- info-VERCTRL:   -
        SYNTAX        INTEGER (1..30)
        ACCESS        read-only
        STATUS        mandatory
        DESCRIPTION
             " This read-only attribute selects a profile out of the
               threshold limit table of PM on ATM layer. "
        ::= { xldv20pmAtmThresholdLimitEntry 1 }

     xldv20pmAtlTransmittedUserCells OBJECT-TYPE
     -- info-VALUE:     
     -- info-ROU:       GET db SET pm
     -- info-IMPL:      V2.0-> 
     -- info-VERCTRL:   
        SYNTAX        Unsigned32
        ACCESS        read-write
        STATUS        mandatory
        DESCRIPTION
             " This read-write attribute selects the threshold limit value
               of the counter in the current threshold limit profile. "
        ::= { xldv20pmAtmThresholdLimitEntry 2 }

     xldv20pmAtlLostUserCells OBJECT-TYPE
     -- info-VALUE:     
     -- info-ROU:       GET db SET pm
     -- info-IMPL:      V2.0-> 
     -- info-VERCTRL:   
        SYNTAX        Unsigned32
        ACCESS        read-write
        STATUS        mandatory
        DESCRIPTION
             " This read-write attribute selects the threshold limit value
               of the counter in the current threshold limit profile. "
        ::= { xldv20pmAtmThresholdLimitEntry 3 }

     xldv20pmAtlMisinsertedUserCells OBJECT-TYPE
     -- info-VALUE:     
     -- info-ROU:       GET db SET pm
     -- info-IMPL:      V2.0-> 
     -- info-VERCTRL:   
        SYNTAX        Unsigned32
        ACCESS        read-write
        STATUS        mandatory
        DESCRIPTION
             " This read-write attribute selects the threshold limit value
               of the counter in the current threshold limit profile. "
        ::= { xldv20pmAtmThresholdLimitEntry 4 }

