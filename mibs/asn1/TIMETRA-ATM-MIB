tAtmObjs               OBJECT IDENTIFIER ::= { tmnxSRObjs 27 }
tAtmMIBConformance     OBJECT IDENTIFIER ::= { tmnxSRConfs 27 }
tAtmMdaObjs            OBJECT IDENTIFIER ::= { tAtmObjs 1 }
tAtmIntfObjs           OBJECT IDENTIFIER ::= { tAtmObjs 2 }
tAtmVclObjs            OBJECT IDENTIFIER ::= { tAtmObjs 3 }
tAal5VccObjs           OBJECT IDENTIFIER ::= { tAtmObjs 4 }
tAtmTrafficDescObjs    OBJECT IDENTIFIER ::= { tAtmObjs 5 }
tAtmVplObjs            OBJECT IDENTIFIER ::= { tAtmObjs 6 }
tAtmVtlObjs            OBJECT IDENTIFIER ::= { tAtmObjs 7 }
tAtmCellVclObjs        OBJECT IDENTIFIER ::= { tAtmObjs 8 }
tAtmSystemObjs         OBJECT IDENTIFIER ::= { tAtmObjs 9 }
tAtmIlmiLinkObjs       OBJECT IDENTIFIER ::= { tAtmObjs 10 }
tAtmTCSublayerObjs     OBJECT IDENTIFIER ::= { tAtmObjs 11 }
tAtmCpObjs             OBJECT IDENTIFIER ::= { tAtmObjs 12 }

