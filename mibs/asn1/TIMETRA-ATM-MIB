TIMETRA-ATM-MIB DEFINITIONS ::= BEGIN

IMPORTS
        Integer32, MODULE-IDENTITY,
        OBJECT-TYPE, Counter32, Counter64,
        Unsigned32, NOTIFICATION-TYPE                FROM SNMPv2-SMI

        RowStatus, TimeStamp, TEXTUAL-CONVENTION     FROM SNMPv2-TC

        MODULE-COMPLIANCE, OBJECT-GROUP, 
        NOTIFICATION-GROUP                           FROM SNMPv2-CONF

        ifIndex                                      FROM IF-MIB

        atmInterfaceConfEntry, atmVclEntry, 
        atmVclVpi, atmVclVci, aal5VccEntry,
        atmVplEntry, atmVplVpi,
        atmInterfaceTCAlarmState,
        atmInterfaceDs3PlcpAlarmState,
        atmInterfaceCurrentMaxVpiBits,
        atmInterfaceCurrentMaxVciBits,
        atmInterfaceMaxActiveVpiBits,
        atmInterfaceMaxActiveVciBits,
        atmInterfaceConfVccs,
        atmInterfaceConfVpcs,
        atmInterfaceTCEntry                          FROM ATM-MIB

        AtmVpIdentifier, AtmVcIdentifier,
        AtmConnKind, AtmConnCastType, 
        AtmTrafficDescrParamIndex, 
        AtmVorXAdminStatus, AtmVorXOperStatus,
        AtmVorXLastChange                            FROM ATM-TC-MIB

        timetraSRMIBModules, tmnxSRObjs,
        tmnxSRNotifyPrefix, tmnxSRConfs              FROM TIMETRA-GLOBAL-MIB

        tmnxChassisIndex, tmnxCardSlotNum,
        tmnxMDASlotNum                               FROM TIMETRA-CHASSIS-MIB

        tmnxConnProfId                               FROM TIMETRA-CONN-PROF-MIB;

timetraATMMIBModule MODULE-IDENTITY
        LAST-UPDATED "201102010000Z"
        ORGANIZATION "Alcatel-Lucent"
        CONTACT-INFO
            "Alcatel-Lucent SROS Support
             Web: http://support.alcatel-lucent.com "
        DESCRIPTION
        "This document is the SNMP MIB module to manage and provision ATM 
        functionality for the Alcatel-Lucent SROS device. This includes 
        extensions to the AToM MIB defined in RFC 2515 to allow statistics 
        retrieval as well as extensions to the ILMI link attributes. 

        Copyright 2003-2011 Alcatel-Lucent. All rights reserved.
