    SYNTAX  INTEGER
    {
    other(1),
    no(2),
    yes(3)
    }
    ACCESS  read-write
    STATUS  mandatory
    DESCRIPTION
        "Determines if PAP is allowed to be negotiated by the peer."
    ::= { ipadPPPCfgTableEntry 14 }

ipadPPPCfgAllowCHAP    OBJECT-TYPE
    SYNTAX  INTEGER
    {
    other(1),
