				sheTssmLastLinkUpLogAtucTxRateThreshold
					Gauge32,
				sheTssmLastLinkUpLogAturTxRateThreshold
					Gauge32,
				sheTssmLastLinkUpLogAtucChanInterleaveDelay
					Gauge32,
				sheTssmLastLinkUpLogAturChanInterleaveDelay
					Gauge32,
				sheTssmLastLinkUpLogAtucTargetSnrMgn
					INTEGER,
				sheTssmLastLinkUpLogAturTargetSnrMgn
					INTEGER,
				sheTssmLastLinkUpLogConfiguredRateDown
					Unsigned32,
				sheTssmLastLinkUpLogConfiguredRateUp
					Unsigned32
			 }

		sheTssmLastLinkUpLogLinkTime OBJECT-TYPE
			SYNTAX DateAndTime
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"The time when the link up happened last time on this port."
			::= { sheTssmLastLinkUpLogEntry 1 }

		
		sheTssmLastLinkUpLogAtucTxRate OBJECT-TYPE
			SYNTAX Gauge32 (1..32736)
			UNITS "kbps"
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"The stable downstream transmit rate on the channel.
				It is the value of sheAdslAtucChanCurrTxRate when the link up is stable."
			::= { sheTssmLastLinkUpLogEntry 2 }

		
		sheTssmLastLinkUpLogAturTxRate OBJECT-TYPE
			SYNTAX Gauge32 (1..32736)
			UNITS "kbps"
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"The stable upstream transmit rate on the channel. It is the
				value of sheAdslAturChanCurrTxRate when the link up is stable."
			::= { sheTssmLastLinkUpLogEntry 3 }

		
		sheTssmLastLinkUpLogAtucSnrMgn OBJECT-TYPE
			SYNTAX INTEGER (-640..640)
			UNITS "tenth dB"
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"Noise Margin as seen by this ATU with respect to its
				received signal in tenth dB. It is the value of
				sheAdslAtucPhysCurrSnrMgn when the link up is stable."
			::= { sheTssmLastLinkUpLogEntry 4 }

		
		sheTssmLastLinkUpLogAturSnrMgn OBJECT-TYPE
			SYNTAX INTEGER (-640..640)
			UNITS "tenth dB"
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"Noise Margin as seen by this ATU with respect to its
				received signal in tenth dB. It is the value of
				sheAdslAturPhysCurrSnrMgn when the link up is stable."
			::= { sheTssmLastLinkUpLogEntry 5 }

		
		sheTssmLastLinkUpLogAtucAtn OBJECT-TYPE
			SYNTAX Gauge32 (0..630)
			UNITS "tenth dB"
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"Measured difference in the total power transmitted by
				the peer ATU and the total power received by this ATU.
				It is the value of sheAdslAtucPhysCurrAtn when the link up is stable."
			::= { sheTssmLastLinkUpLogEntry 6 }

		
		sheTssmLastLinkUpLogAturAtn OBJECT-TYPE
			SYNTAX Gauge32 (0..630)
			UNITS "tenth dB"
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"Measured difference in the total power transmitted by
				the peer ATU and the total power received by this ATU.
				It is the value of sheAdslAturPhysCurrAtn when the link up is stable."
			::= { sheTssmLastLinkUpLogEntry 7 }

		
		sheTssmLastLinkUpLogAtucAttainableRate OBJECT-TYPE
			SYNTAX Gauge32
			UNITS "kbps"
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"Indicates the maximum attainable data rate by the ATU.
				It is the value of sheAdslAtucPhysCurrAttainableRate
				when the link up is stable."
			::= { sheTssmLastLinkUpLogEntry 8 }

		
		sheTssmLastLinkUpLogAturAttainableRate OBJECT-TYPE
			SYNTAX Gauge32
			UNITS "kbps"
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"Indicates the maximum attainable data rate by the ATU.
				It is the value of sheAdslAturPhysCurrAttainableRate
				when the link up is stable."
			::= { sheTssmLastLinkUpLogEntry 9 }

		
		sheTssmLastLinkUpLogAtucTxRateThreshold OBJECT-TYPE
			SYNTAX Gauge32 (1..32736)
			UNITS "kbps"
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"The configured threshold of downstream transmit rate on the channel."
			::= { sheTssmLastLinkUpLogEntry 10 }

		
		sheTssmLastLinkUpLogAturTxRateThreshold OBJECT-TYPE
			SYNTAX Gauge32 (1..32736)
			UNITS "kbps"
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"The configured threshold of upstream transmit rate on the channel."
			::= { sheTssmLastLinkUpLogEntry 11 }

		
		sheTssmLastLinkUpLogAtucChanInterleaveDelay OBJECT-TYPE
			SYNTAX Gauge32
			UNITS "milli-seconds"
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"Interleave Delay for this channel. It is the value of
				sheAdslAtucChanInterleaveDelay when the link up is stable."
			::= { sheTssmLastLinkUpLogEntry 12 }

		
		sheTssmLastLinkUpLogAturChanInterleaveDelay OBJECT-TYPE
			SYNTAX Gauge32
			UNITS "milli-seconds"
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"Interleave Delay for this channel. It is the value of
				sheAdslAturChanInterleaveDelay when the link up is stable."
			::= { sheTssmLastLinkUpLogEntry 13 }

		
		sheTssmLastLinkUpLogAtucTargetSnrMgn OBJECT-TYPE
			SYNTAX INTEGER (0..310)
			UNITS "tenth dB"
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"Configured Target Signal/Noise Margin on this line.
				It is the value of sheAdslLineConfProfileAtucConfTargetSnrMgn
				when the link up is stable."
			::= { sheTssmLastLinkUpLogEntry 14 }

		
		sheTssmLastLinkUpLogAturTargetSnrMgn OBJECT-TYPE
			SYNTAX INTEGER (0..310)
			UNITS "tenth dB"
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"Configured Target Signal/Noise Margin on this line.
				It is the value of sheAdslLineConfProfileAturConfTargetSnrMgn
				when the link up is stable."
			::= { sheTssmLastLinkUpLogEntry 15 }

		
		sheTssmLastLinkUpLogConfiguredRateDown OBJECT-TYPE
			SYNTAX Unsigned32 (32..32736)
			UNITS "kbps"
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"Configured downstream transmit rate for the channel on this line, in kbps.
				It is the value of sheAdslLineConfProfileAtucChanConfFastMaxTxRate or
				sheAdslLineConfProfileAtucChanConfInterleaveMaxTxRate, depending on
				the mode of the channel is fast or interleave.
				"
			::= { sheTssmLastLinkUpLogEntry 16 }

		
		sheTssmLastLinkUpLogConfiguredRateUp OBJECT-TYPE
			SYNTAX Unsigned32 (32..32736)
			UNITS "kbps"
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"Configured upstream transmit rate for the channel on this line, in kbps.
				It is the value of sheAdslLineConfProfileAturChanConfFastMaxTxRate or
				sheAdslLineConfProfileAturChanConfInterleaveMaxTxRate, depending on
				the mode of the channel is fast or interleave."
			::= { sheTssmLastLinkUpLogEntry 17 }

		
		sheTssmLastLinkDownLogTable OBJECT-TYPE
			SYNTAX SEQUENCE OF SheTssmLastLinkDownLogEntry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"Each entry in this table contains the ADSL line parameters of a
				port for last link down."
			::= { sheTssmInfo 7 }

		
		sheTssmLastLinkDownLogEntry OBJECT-TYPE
			SYNTAX SheTssmLastLinkDownLogEntry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"An entry in sheTssmLastLinkDownLogTable."
			INDEX { ifIndex }
			::= { sheTssmLastLinkDownLogTable 1 }

		
		SheTssmLastLinkDownLogEntry ::=
			SEQUENCE { 
				sheTssmLastLinkDownLogLinkTime
					DateAndTime
			 }

		sheTssmLastLinkDownLogLinkTime OBJECT-TYPE
			SYNTAX DateAndTime
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"The time when the link down happened last time on this port."
			::= { sheTssmLastLinkDownLogEntry 1 }

		
--  -----------------------------------------------------------------
-- Trap definitions
-- -----------------------------------------------------------------
		sheTssmNotifications OBJECT IDENTIFIER ::= { sheTssmMib 2 }

		
		sheTssmEvents OBJECT IDENTIFIER ::= { sheTssmNotifications 1 }

		
		sheTssmControlResult NOTIFICATION-TYPE
			OBJECTS { sheTssmControlTimeStamp, sheTssmControlRequest, sheTssmControlReqResult }
			STATUS current
			DESCRIPTION 
				"Notification sent at the end of long running command.
				The possible values of sheTssmControlReqResult are:
				    wrongIpAddress(2),      -- IP address of FTP servre is incorrect
				    wrongUserPassword(3),   -- The user name or password is wrong
				    permissionDenied(4),    -- The user name has no permission to create dir/file
				    commandSucceeded(37)
				"
			::= { sheTssmEvents 1 }

		
		sheTssmUploadFailedTrap NOTIFICATION-TYPE
			OBJECTS { sheTssmLastUploadStatus, sheTssmLastUploadTimeStamp, sheTssmLastUploadFileName, sheTssmLastUploadErrorString }
			STATUS current
			DESCRIPTION 
				"Indicates that an FTP upload failure occurred."
			::= { sheTssmEvents 2 }

		
--  the criteria
-- link up time
-- stable downstream rate
-- stable upstream rate
-- downstream noise margin
-- upstream noise margin
-- downstream attenuation
-- upstream attenuation
-- downstream maximum attainable rate
-- upstream maximum attainable rate
-- configured rate
-- configured rate
-- interleave delay
-- interleave delay
-- target noise margin
-- target noise margin
-- alarm threshold rate
-- alarm threshold rate
		sheTssmMissTargetRateTrap NOTIFICATION-TYPE
			OBJECTS { ifIndex, sheTssmMissTargetRateCriteria, sheTssmLastLinkUpLogLinkTime, sheTssmLastLinkUpLogAtucTxRate, sheTssmLastLinkUpLogAturTxRate, 
				sheTssmLastLinkUpLogAtucSnrMgn, sheTssmLastLinkUpLogAturSnrMgn, sheTssmLastLinkUpLogAtucAtn, sheTssmLastLinkUpLogAturAtn, sheTssmLastLinkUpLogAtucAttainableRate, 
				sheTssmLastLinkUpLogAturAttainableRate, sheTssmLastLinkUpLogConfiguredRateDown, sheTssmLastLinkUpLogConfiguredRateUp, sheTssmLastLinkUpLogAtucChanInterleaveDelay, sheTssmLastLinkUpLogAturChanInterleaveDelay, 
				sheTssmLastLinkUpLogAtucTargetSnrMgn, sheTssmLastLinkUpLogAturTargetSnrMgn, sheTssmLastLinkUpLogAtucTxRateThreshold, sheTssmLastLinkUpLogAturTxRateThreshold }
			STATUS current
			DESCRIPTION 
				"Indicates that the stable link rate does not meet the target rate on
				the ADSL port specified by ifIndex. The EM will show an alarm on this port.
				The corresponding parameters are included in the trap."
			::= { sheTssmEvents 3 }

		
--  the criteria
-- link up time
-- stable downstream rate
-- stable upstream rate
-- downstream noise margin
-- upstream noise margin
-- downstream attenuation
-- upstream attenuation
-- downstream maximum attainable rate
-- upstream maximum attainable rate
-- configured rate
-- configured rate
-- interleave delay
-- interleave delay
-- target noise margin
-- target noise margin
-- alarm threshold rate
-- alarm threshold rate
		sheTssmReachTargetRateTrap NOTIFICATION-TYPE
			OBJECTS { ifIndex, sheTssmMissTargetRateCriteria, sheTssmLastLinkUpLogLinkTime, sheTssmLastLinkUpLogAtucTxRate, sheTssmLastLinkUpLogAturTxRate, 
				sheTssmLastLinkUpLogAtucSnrMgn, sheTssmLastLinkUpLogAturSnrMgn, sheTssmLastLinkUpLogAtucAtn, sheTssmLastLinkUpLogAturAtn, sheTssmLastLinkUpLogAtucAttainableRate, 
				sheTssmLastLinkUpLogAturAttainableRate, sheTssmLastLinkUpLogConfiguredRateDown, sheTssmLastLinkUpLogConfiguredRateUp, sheTssmLastLinkUpLogAtucChanInterleaveDelay, sheTssmLastLinkUpLogAturChanInterleaveDelay, 
				sheTssmLastLinkUpLogAtucTargetSnrMgn, sheTssmLastLinkUpLogAturTargetSnrMgn, sheTssmLastLinkUpLogAtucTxRateThreshold, sheTssmLastLinkUpLogAturTxRateThreshold }
			STATUS current
			DESCRIPTION 
				"If at last time, the ADSL port that specified by ifIndex missed the target
				rate, and the sheTssmMissTargetRateTrap was sent, but it met the target rate
				at this link up, this trap is sent. The EM can clear the alarm then.
				The corresponding parameters are included in the trap."
			::= { sheTssmEvents 4 }

		
		sheTssmMibConformance OBJECT IDENTIFIER ::= { sheTssmMib 100 }

		
		sheTssmMibCompliances OBJECT IDENTIFIER ::= { sheTssmMibConformance 1 }

		
		sheTssmMibCompliance MODULE-COMPLIANCE
			STATUS current
			DESCRIPTION 
				"Description."
			MODULE -- this module
				MANDATORY-GROUPS { sheTssmMibGroup, sheTssmMibControlGroup, sheTssmMibNotificationGroup }
			::= { sheTssmMibCompliances 1 }

		
		sheTssmMibGroups OBJECT IDENTIFIER ::= { sheTssmMibConformance 2 }

		
		sheTssmMibGroup OBJECT-GROUP
			OBJECTS { sheTssmFtpRootDir, sheTssmFtpIp, sheTssmFtpUser, sheTssmFtpPassword, sheTssmUploadInterval, 
				sheTssmUploadEnabled, sheTssmLogEnabled, sheTssmMissTargetRateCriteria, sheTssmLastUploadStatus, sheTssmLastUploadTimeStamp, 
				sheTssmLastUploadFileName, sheTssmLastUploadErrorString, sheTssmPortTrapEnabled, sheTssmPortThresholdUnit, sheTssmPortThresholdRateDownKbps, 
				sheTssmPortThresholdRateUpKbps, sheTssmPortThresholdRateDownRatio, sheTssmPortThresholdRateUpRatio, sheTssmLastLinkUpLogLinkTime, sheTssmLastLinkUpLogAtucTxRate, 
				sheTssmLastLinkUpLogAturTxRate, sheTssmLastLinkUpLogAtucSnrMgn, sheTssmLastLinkUpLogAturSnrMgn, sheTssmLastLinkUpLogAtucAtn, sheTssmLastLinkUpLogAturAtn, 
				sheTssmLastLinkUpLogAtucAttainableRate, sheTssmLastLinkUpLogAturAttainableRate, sheTssmLastLinkUpLogAtucTxRateThreshold, sheTssmLastLinkUpLogAturTxRateThreshold, sheTssmLastLinkUpLogAtucChanInterleaveDelay, 
				sheTssmLastLinkUpLogAturChanInterleaveDelay, sheTssmLastLinkUpLogAtucTargetSnrMgn, sheTssmLastLinkUpLogAturTargetSnrMgn, sheTssmLastLinkUpLogConfiguredRateDown, sheTssmLastLinkUpLogConfiguredRateUp, 
				sheTssmLastLinkDownLogLinkTime }
			STATUS current
			DESCRIPTION 
				"Description."
			::= { sheTssmMibGroups 1 }

		
		sheTssmMibControlGroup OBJECT-GROUP
			OBJECTS { sheTssmControlRequest, sheTssmControlStatus, sheTssmControlTimer, sheTssmControlReqResult, sheTssmControlTimeStamp, 
				sheTssmControlFtpRootDir, sheTssmControlFtpIp, sheTssmControlFtpUser, sheTssmControlFtpPassword, sheTssmControlUploadInterval, 
				sheTssmControlUploadEnabled, sheTssmControlLogEnabled, sheTssmControlMissTargetRateCriteria, sheTssmPortControlRequest, sheTssmPortControlStatus, 
				sheTssmPortControlTimer, sheTssmPortControlReqResult, sheTssmPortControlTimeStamp, sheTssmPortControlType, sheTssmPortControlRange, 
				sheTssmPortControlIndex, sheTssmPortControlTrapEnabled, sheTssmPortControlThresholdUnit, sheTssmPortControlThresholdRateDownKbps, sheTssmPortControlThresholdRateUpKbps, 
				sheTssmPortControlThresholdRateDownRatio, sheTssmPortControlThresholdRateUpRatio }
			STATUS current
			DESCRIPTION 
				"Description."
			::= { sheTssmMibGroups 2 }

		
		sheTssmMibNotificationGroup NOTIFICATION-GROUP
			NOTIFICATIONS { sheTssmControlResult, sheTssmUploadFailedTrap, sheTssmMissTargetRateTrap, sheTssmReachTargetRateTrap }
			STATUS current
			DESCRIPTION 
				"Description."
			::= { sheTssmMibGroups 3 }

		
	
	END

