    SYNTAX          INTEGER  {
                        tdm(1),
                        voip(2)
                    }
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION
        "This object indicates the call type of the active call leg.
        tdm(1): TDM call leg.
        voip (2): VoIP call leg." 
    ::= { cmqCommonCallActiveNREntry 3 }

