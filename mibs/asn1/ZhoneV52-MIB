        -- 1.3.6.1.4.1.5504.4.3.2.6.1.10
        v52UserPortRowStatus OBJECT-TYPE
            SYNTAX ZhoneRowStatus
            MAX-ACCESS read-create
            STATUS current
            DESCRIPTION
                "Used to control row creation and deletion. 
                
                A row in this table is created using a single 
