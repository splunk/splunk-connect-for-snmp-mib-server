                              DESCRIPTION
                              "High Voltage Shutdown Alarm"
                              ::= 5
    vpwrTrapBDAlarm   TRAP-TYPE
       ENTERPRISE      vpwrDcPowerTraps
        VARIABLES { vpwrTrapsMsgString }
                              DESCRIPTION
                              "Battery on Discharge Alarm"
                              ::= 6
