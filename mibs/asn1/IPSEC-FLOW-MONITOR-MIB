                   -- ipSecTunIkeTunnelAlive,
                   ipSecTunLocalAddr,
                   ipSecTunRemoteAddr,
                   -- ipSecTunKeyType,
                   ipSecTunEncapMode,
                   ipSecTunLifeSize,
                   ipSecTunLifeTime,
