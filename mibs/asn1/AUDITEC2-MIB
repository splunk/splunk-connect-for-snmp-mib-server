              ::= { scenarioBN 4 }


   sceBNState OBJECT-TYPE
              SYNTAX  INTEGER 
                  {
                  init(1),       -- At least one execution since group start 
                  outOfRange(2), -- Outside of range (user scenario only)                      
                      suspended(4),  -- The scenario is suspended
                      unavailable(8) -- Last executuon unavailable
                  }
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION "Last scenario state. State is a combination of values"
              ::= { scenarioBN 5 }


