   ordPrev5ErrCode OBJECT-TYPE
              SYNTAX  DisplayString (SIZE (1..65))
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION "Error code for order execution (for sample = current - 5)"
              ::= { order 43 }                          
 
   -- the order list accessible by index (text orders)
 
   grpOrdTxtList OBJECT-TYPE
              SYNTAX  SEQUENCE OF OrderTxt
              ACCESS  not-accessible
              STATUS  mandatory
              DESCRIPTION "List of text orders"
              ::= { group 7 }
              
   orderTxt OBJECT-TYPE
              SYNTAX  OrderTxt
              ACCESS  not-accessible
              STATUS  mandatory
              DESCRIPTION "Entry of list of text order"
              INDEX   { otxQueIndex, otxSceIndex, otxIndex }
              ::= { grpOrdTxtList 1 }


   OrderTxt ::= SEQUENCE {
              otxIndex             INTEGER,
              otxSceIndex          INTEGER,
              otxQueIndex          INTEGER,
              otxName              DisplayString,
              otxNbSamples         Counter,
              otxNbUnavail                 Counter,
              otxTime              TimeTicks,
              otxValue             DisplayString,
              otxErrCode           DisplayString,
              otxPrev1Time         TimeTicks,
              otxPrev1Value        DisplayString,
              otxPrev1ErrCode      DisplayString,
              otxPrev2Time         TimeTicks,
              otxPrev2Value        DisplayString,
              otxPrev2ErrCode      DisplayString,
              otxPrev3Time         TimeTicks,
              otxPrev3Value        DisplayString,
              otxPrev3ErrCode      DisplayString,
              otxPrev4Time         TimeTicks,
              otxPrev4Value        DisplayString,
              otxPrev4ErrCode      DisplayString,
              otxPrev5Time         TimeTicks,
              otxPrev5Value        DisplayString,
              otxPrev5ErrCode      DisplayString
              }
              
   otxIndex OBJECT-TYPE
              SYNTAX  INTEGER
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION "Order index"
              ::= { orderTxt 1 }
   
   otxSceIndex OBJECT-TYPE
              SYNTAX  INTEGER
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION "Sce index"
              ::= { orderTxt 2 }


   otxQueIndex OBJECT-TYPE
              SYNTAX  INTEGER
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION "Que Index"
              ::= { orderTxt 3 }


   otxName OBJECT-TYPE
              SYNTAX  DisplayString (SIZE (1..65))
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION "Order name"
              ::= { orderTxt 4 }


   otxNbSamples OBJECT-TYPE
              SYNTAX  Counter
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION "Number of response time samples or numerical samples"
              ::= { orderTxt 5 }


   otxNbUnavail OBJECT-TYPE
              SYNTAX  Counter
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION "Number of unavailabilities for this order"
              ::= { orderTxt 6 }                                        


   otxTime OBJECT-TYPE
              SYNTAX  TimeTicks
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION "Date of order execution. The date is written as the number of seconds since 1980, january the 1st"
