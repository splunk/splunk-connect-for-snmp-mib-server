                              "Minimum number of database connections."
                      ::= { uplMessengerDescriptionEntry 10 }

              uplmDBConnectionCacheThreadWaits OBJECT-TYPE
                      SYNTAX     Counter
                      ACCESS     read-only
                      STATUS     mandatory
                      DESCRIPTION
                              "Number of times threads had to wait for a database connection."
                      ::= { uplMessengerDescriptionEntry 11 }

              uplmDBConnectionCacheMeanWaitTime OBJECT-TYPE
                      SYNTAX     INTEGER
                      ACCESS     read-only
                      STATUS     mandatory
                      DESCRIPTION
                              "Mean wait time in milliseconds for threads waiting on a database connection."
                      ::= { uplMessengerDescriptionEntry 12 }

              uplmDBConnectionCacheDeviationOfWaitTime OBJECT-TYPE
                      SYNTAX     INTEGER
                      ACCESS     read-only
                      STATUS     mandatory
                      DESCRIPTION
                              "Standard deviation of uplmDBConnectionCacheMeanWaitTime."
                     ::= { uplMessengerDescriptionEntry 13 }
 --ITS240086 end Nathan
