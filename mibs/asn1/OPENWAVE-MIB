                VARIABLES  { upsTrapInfo }
                DESCRIPTION
                       "An event that is generated when the bookmarks
                       service is down "
                ::= 403

        upsBookmarksServiceSlow TRAP-TYPE
                ENTERPRISE { upAdmin }
                VARIABLES  { upsTrapInfo }
                DESCRIPTION
                       "An event that is generated when the bookmarks
                       service is slow "
                ::= 404

