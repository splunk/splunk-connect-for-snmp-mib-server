---v5
	--
    -- upPushProxy 
    --
        upPushProxy OBJECT IDENTIFIER ::= { systems 5 }

    --
    -- Push Proxy PAP   
    --
        
    	upPushPap    OBJECT IDENTIFIER ::= { upPushProxy 1 }
 
        --
        -- upPushPapDescriptionTable
        -- This table describes information identifying UPPushPAP
        --

        upPushPapDescriptionTable OBJECT-TYPE
                SYNTAX     SEQUENCE OF UpPushPapDescriptionEntry
                ACCESS     not-accessible
                STATUS     mandatory
                DESCRIPTION
                        "The table holding UPPushPAP identifying
                         information."
                ::= { upPushPap 1 }

        upPushPapDescriptionEntry OBJECT-TYPE
                SYNTAX     UpPushPapDescriptionEntry
                ACCESS     not-accessible
                STATUS     mandatory
                DESCRIPTION
                        "The entry associated with each UPPushPAP."
                INDEX { uppdPAPIndex }
                ::= { upPushPapDescriptionTable 1 }

        UpPushPapDescriptionEntry ::= SEQUENCE {
        		uppdPAPIndex		           INTEGER,
                uppdPAPIdentifier              INTEGER,
                uppdProcessId                  INTEGER,
                uppdHostName                   DisplayString,
                uppdPublicHTTPPortNumber       INTEGER,
                uppdPublicHTTPSPortNumber      INTEGER,
                uppdPrivateHTTPPortNumber      INTEGER,
                uppdStartupTime                DisplayString
        }
		uppdPAPIndex	   OBJECT-TYPE
                SYNTAX          INTEGER
                ACCESS          read-only
                STATUS          mandatory
                DESCRIPTION
                        "The index for each PAP process in the SNMP table."
                ::= { upPushPapDescriptionEntry 1 }

        uppdPAPIdentifier  OBJECT-TYPE
                SYNTAX          INTEGER
                ACCESS          read-only
                STATUS          mandatory
                DESCRIPTION
                        "The unique identifier for each PAP process."
                ::= { upPushPapDescriptionEntry 2 }

        uppdProcessId OBJECT-TYPE
                SYNTAX     INTEGER
                ACCESS     read-only
                STATUS     mandatory
                DESCRIPTION
                       "The process id of the UPPushPAP."
                ::= { upPushPapDescriptionEntry 3 }

        uppdHostName OBJECT-TYPE
                SYNTAX     DisplayString
                ACCESS     read-only
                STATUS     mandatory
                DESCRIPTION
                        "The host name of the node running UPPushPAP."
                ::= { upPushPapDescriptionEntry 4 }

        uppdPublicHTTPPortNumber       OBJECT-TYPE
                SYNTAX     INTEGER
                ACCESS     read-only
                STATUS     mandatory
                DESCRIPTION
                        "The port number on which the UPPushPAP listens
                         for nonsecure public notification services."
                ::= { upPushPapDescriptionEntry 5 }

        uppdPublicHTTPSPortNumber      OBJECT-TYPE
                SYNTAX     INTEGER
                ACCESS     read-only
                STATUS     mandatory
                DESCRIPTION
                        "The port number on which the UPPushPAP listens
                         for secure public notification services."
                ::= { upPushPapDescriptionEntry 6 }

        uppdPrivateHTTPPortNumber      OBJECT-TYPE
                SYNTAX     INTEGER
                ACCESS     read-only
                STATUS     mandatory
                DESCRIPTION
                        "The port number on which the UPPushPAP listens
                         for nonsecure private notification services."
                ::= { upPushPapDescriptionEntry 7 }

        uppdStartupTime OBJECT-TYPE
                SYNTAX     DisplayString
                ACCESS     read-only
                STATUS     mandatory
                DESCRIPTION
                        "The date and time the UPPushPAP started."
                ::= { upPushPapDescriptionEntry 8 }


        --
        -- Notifications statistics that are held in the UPPushPAP
        --

        upPushPapNtfnStatsTable OBJECT-TYPE
                SYNTAX     SEQUENCE OF UpPushPapNtfnStatsEntry
                ACCESS     not-accessible
                STATUS     mandatory
                DESCRIPTION
                       "The table holding statistics on notification
                                                services provided by UPPushPAP."
                ::= { upPushPap 2 }

        upPushPapNtfnStatsEntry OBJECT-TYPE
                SYNTAX     UpPushPapNtfnStatsEntry
                ACCESS     not-accessible
                STATUS     mandatory
                DESCRIPTION
                        "The entry associated with notifications statistics
                                                for each UPPushPAP."
                INDEX { uppnsPAPIndex }
                ::= { upPushPapNtfnStatsTable 1 }

        UpPushPapNtfnStatsEntry ::= SEQUENCE {
                uppnsPAPIndex		              	   INTEGER,
                uppnsPAPIdentifier              	   INTEGER,
                uppnsPublicHTTPReqReceived             Counter,
                uppnsPrivateHTTPReqReceived            Counter,
                uppnsPublicHTTPSReqReceived            Counter,
                uppnsPublicHTTPReqAccepted	           Counter,
                uppnsPrivateHTTPReqAccepted            Counter,
                uppnsPublicHTTPSReqAccepted            Counter,
                uppnsAvgNtfnsReceivedPerSec            INTEGER,
                uppnsAvgNtfnsAcceptedPerSec            INTEGER
        }

        uppnsPAPIndex  OBJECT-TYPE
                SYNTAX          INTEGER
                ACCESS          read-only
                STATUS          mandatory
                DESCRIPTION
                        "The index for each PAP process in the SNMP table."
                ::= { upPushPapNtfnStatsEntry 1 }
                
        uppnsPAPIdentifier  OBJECT-TYPE
                SYNTAX          INTEGER
                ACCESS          read-only
                STATUS          mandatory
                DESCRIPTION
                        "The unique identifier for each PAP process."
                ::= { upPushPapNtfnStatsEntry 2 }

        uppnsPublicHTTPReqReceived OBJECT-TYPE
                SYNTAX     Counter
                ACCESS     read-only
                STATUS     mandatory
                DESCRIPTION
                        "The number of public HTTP requests the UPPushPAP
                                                 received since it comes up."
                ::= { upPushPapNtfnStatsEntry 3 }

        uppnsPrivateHTTPReqReceived OBJECT-TYPE
                SYNTAX     Counter
                ACCESS     read-only
                STATUS     mandatory
                DESCRIPTION
                        "The number of private HTTP requests the UPPushPAP
                                                 received since it comes up."
                ::= { upPushPapNtfnStatsEntry 4 }

        uppnsPublicHTTPSReqReceived OBJECT-TYPE
                SYNTAX     Counter
                ACCESS     read-only
                STATUS     mandatory
                DESCRIPTION
                        "The number of public HTTP requests the UPPushPAP
                                                 received since it comes up."
                ::= { upPushPapNtfnStatsEntry 5 }

        uppnsPublicHTTPReqAccepted OBJECT-TYPE
                SYNTAX     Counter
                ACCESS     read-only
                STATUS     mandatory
                DESCRIPTION
                        "The number of public HTTP requests the UPPushPAP
                                                 successfully accepted since it comes up."
                ::= { upPushPapNtfnStatsEntry 6 }

        uppnsPrivateHTTPReqAccepted OBJECT-TYPE
                SYNTAX     Counter
                ACCESS     read-only
                STATUS     mandatory
                DESCRIPTION
                        "The number of private HTTP requests the UPPushPAP
                                                 successfully accepted since it comes up."
                ::= { upPushPapNtfnStatsEntry 7 }

        uppnsPublicHTTPSReqAccepted OBJECT-TYPE
                SYNTAX     Counter
                ACCESS     read-only
                STATUS     mandatory
                DESCRIPTION
                        "The number of public HTTPS requests the UPPushPAP
                                                 successfully accepted since it comes up."
                ::= { upPushPapNtfnStatsEntry 8 }

        uppnsAvgNtfnsReceivedPerSec OBJECT-TYPE
                SYNTAX     INTEGER
                ACCESS     read-only
                STATUS     mandatory
                DESCRIPTION
                        "The average number of notifications received
                                                 per second by the UPPushPAP."
                ::= { upPushPapNtfnStatsEntry 9 }
                
        uppnsAvgNtfnsAcceptedPerSec OBJECT-TYPE
                SYNTAX     INTEGER
                ACCESS     read-only
                STATUS     mandatory
                DESCRIPTION
                        "The average number of notifications accepted
                                                 per second by the UPPushPAP."
                ::= { upPushPapNtfnStatsEntry 10 }

        --
        -- upPushPapForwardedNtfnsTable
        -- This table holds statistics on the notifications
        -- forwarded to both PPG and Messenger processes by the UPPushPAP
        --

        upPushPapForwardedNtfnsTable OBJECT-TYPE
                SYNTAX     SEQUENCE OF UpPushPapForwardedNtfnsEntry
                ACCESS     not-accessible
                STATUS     mandatory
                DESCRIPTION
                       "The table holding statistics on the notification
                                                forwarded by UPPushPAP."
                ::= { upPushPap 3 }

        upPushPapForwardedNtfnsEntry OBJECT-TYPE
                SYNTAX     UpPushPapForwardedNtfnsEntry
                ACCESS     not-accessible
                STATUS     mandatory
                DESCRIPTION
                        "The entry associated with the forwarded notifications 
                                                 statistics for each UPPushPAP."
                INDEX { uppfnPAPIndex }
                ::= { upPushPapForwardedNtfnsTable 1 }

        UpPushPapForwardedNtfnsEntry ::= SEQUENCE {
                uppfnPAPIndex	                 INTEGER,
                uppfnPAPIdentifier               INTEGER,
                uppfnPPGForwardedNtfns         	 Counter,
                uppfnPPGFailedNtfns            	 Counter,
                uppfnMessengerForwardedNtfns     Counter,
                uppfnMessengerFailedNtfns        Counter
                }

        uppfnPAPIndex  OBJECT-TYPE
                SYNTAX          INTEGER
                ACCESS          read-only
                STATUS          mandatory
                DESCRIPTION
                        "The index for each PAP process in the SNMP table."
                ::= { upPushPapForwardedNtfnsEntry 1 }
                
        uppfnPAPIdentifier  OBJECT-TYPE
                SYNTAX          INTEGER
                ACCESS          read-only
                STATUS          mandatory
                DESCRIPTION
                        "The unique identifier for each PAP process."
                ::= { upPushPapForwardedNtfnsEntry 2 }

        uppfnPPGForwardedNtfns OBJECT-TYPE
                SYNTAX     Counter
                ACCESS     read-only
                STATUS     mandatory
                DESCRIPTION
                        "The number of notifications forwarded to the 
                         UPPushPPG process by the UPPushPAP."
                ::= { upPushPapForwardedNtfnsEntry 3 }

        uppfnPPGFailedNtfns OBJECT-TYPE
                SYNTAX     Counter
                ACCESS     read-only
                STATUS     mandatory
                DESCRIPTION
                        "The number of notifications that failed to be 
                         forwarded to the UPPushPPG process by the UPPushPAP."
                ::= { upPushPapForwardedNtfnsEntry 4 }
                
        uppfnMessengerForwardedNtfns OBJECT-TYPE
                SYNTAX     Counter
                ACCESS     read-only
                STATUS     mandatory
                DESCRIPTION
                        "The number of notifications forwarded to the 
                         UPLMessenger process by the UPPushPAP."
                ::= { upPushPapForwardedNtfnsEntry 5 }
                
        uppfnMessengerFailedNtfns OBJECT-TYPE
                SYNTAX     Counter
                ACCESS     read-only
                STATUS     mandatory
                DESCRIPTION
                        "The number of notifications that failed to be 
                         forwarded to the UPLMessenger process by the UPPushPAP."
                ::= { upPushPapForwardedNtfnsEntry 6 }
        --
        -- upPushPapTraps
        -- This section describes traps originated by UPPushPAP
        --

        upPushPapTrapInfo   OBJECT IDENTIFIER ::= { upPushPap 20 }

        uppTrapInfo OBJECT-TYPE
                SYNTAX     DisplayString
                ACCESS     not-accessible
                STATUS     optional
                DESCRIPTION
                        "Text string that provides additional
                         information about the trap."
                ::= { upPushPapTrapInfo 1 }

        uppStartup TRAP-TYPE
                ENTERPRISE { upPushProxy }
                VARIABLES { uppdPAPIdentifier,
                            uppdHostName,
                            uppdProcessId,
                            uppTrapInfo
                          }
                DESCRIPTION
                       "An event that is generated when the UPPushPAP
                        is started."
                ::= 1000
                
        uppShutDown TRAP-TYPE
                ENTERPRISE { upPushProxy }
                VARIABLES { uppdPAPIdentifier,
                            uppdHostName,
                            uppdProcessId,
                            uppTrapInfo
                          }
                DESCRIPTION
                       "An event that is generated when the UPPushPAP
                        is shutting down. uppTrapInfo gives the 
                        reason for the shutdown."
                ::= 1001

        uppFailToStart TRAP-TYPE
                ENTERPRISE { upPushProxy }
                VARIABLES { uppdPAPIdentifier,
                            uppdHostName,
                            uppdProcessId,
                            uppTrapInfo
                          }
                DESCRIPTION
                       "An event that is generated when the UPPushPAP
                        has failed to start-up. uppTrapInfo gives the 
                        reason for the shutdown."
                ::= 1002

        uppDatabaseConnectionDown TRAP-TYPE
                ENTERPRISE { upPushProxy }
                VARIABLES { uppdPAPIdentifier,
                            uppdHostName,
                            uppdProcessId,
                            uppTrapInfo
                          }
                DESCRIPTION
                       "An event that is generated when the UPPushPAP
                        detects that the database connection is down."
                ::= 1003

        uppInternalErrors TRAP-TYPE
                ENTERPRISE { upPushProxy }
                VARIABLES { uppdPAPIdentifier,
                            uppdHostName,
                            uppdProcessId,
                            uppTrapInfo
                          }
                DESCRIPTION
                       "An event that is generated when the UPPushPAP
                        detects an internal error. uppTrapInfo
                        gives error name."
                ::= 1006

        uppPublicHTTPServiceStarted TRAP-TYPE
                ENTERPRISE { upPushProxy }
                VARIABLES { uppdPAPIdentifier,
                            uppdHostName,
                            uppdProcessId,
                            uppTrapInfo
                          }
                DESCRIPTION
                       "An event that is generated when the UPPushPAP
                        is starting the Public HTTP service. This is
                        sent as a warning because the service lowers
                        the overall security of the system."
                ::= 1007

        uppPublicHTTPServiceDown TRAP-TYPE
                ENTERPRISE { upPushProxy }
                VARIABLES { uppdPAPIdentifier,
                            uppdHostName,
                            uppdProcessId,
                            uppTrapInfo
                          }
                DESCRIPTION
                       "An event that is generated when the UPPushPAP
                        detects the Public HTTP interface is down."
                ::= 1008

        uppPrivateHTTPServiceDown TRAP-TYPE
                ENTERPRISE { upPushProxy }
                VARIABLES { uppdPAPIdentifier,
                            uppdHostName,
                            uppdProcessId,
                            uppTrapInfo
                          }
                DESCRIPTION
                       "An event that is generated when the UPPushPAP
                        detects the Private HTTP interface is down."
                ::= 1009

        uppPublicHTTPSServiceDown TRAP-TYPE
                ENTERPRISE { upPushProxy }
                VARIABLES { uppdPAPIdentifier,
                            uppdHostName,
                            uppdProcessId,
                            uppTrapInfo
                          }
                DESCRIPTION
                       "An event that is generated when the UPPushPAP
                        detects the Public secure HTTP interface is down."
                ::= 1010
                
         uppPPGInterfaceDown TRAP-TYPE
                ENTERPRISE { upPushProxy }
                VARIABLES { uppdPAPIdentifier,
                            uppdHostName,
                            uppdProcessId,
                            uppTrapInfo
                          }
                DESCRIPTION
                       "An event that is generated when the UPPushPAP
                        detects a PPG interface is down. uppTrapInfo gives the 
                        PPG identifier"
                ::= 1011
         
         uppPPGInterfaceUp TRAP-TYPE
                ENTERPRISE { upPushProxy }
                VARIABLES { uppdPAPIdentifier,
                            uppdHostName,
                            uppdProcessId,
                            uppTrapInfo
                          }
                DESCRIPTION
                       "An event that is generated when the UPPushPAP
                        detects a PPG interface is up. uppTrapInfo gives the 
                        PPG identifier"
                ::= 1012
          
          uppPAPSuspended TRAP-TYPE
                ENTERPRISE { upPushProxy }
                VARIABLES { uppdPAPIdentifier,
                            uppdHostName,
                            uppdProcessId,
                            uppTrapInfo
                          }
                DESCRIPTION
                       "An event that is generated when the UPPushPAP
                        process has been suspended."
                ::= 1013
          
          
           uppPAPResumed TRAP-TYPE
                ENTERPRISE { upPushProxy }
                VARIABLES { uppdPAPIdentifier,
                            uppdHostName,
                            uppdProcessId,
                            uppTrapInfo
                          }
                DESCRIPTION
                       "An event that is generated when the UPPushPAP
                        has resumed."
                ::= 1014
        
    --
    -- Push Proxy PPG   
    --
        
    	upPushPpg    OBJECT IDENTIFIER ::= { upPushProxy 2 }
 
        --
        -- upPushPpgDescriptionTable
        -- This table describes information identifying UPPushPPG
        --

        upPushPpgDescriptionTable OBJECT-TYPE
                SYNTAX     SEQUENCE OF UpPushPpgDescriptionEntry
                ACCESS     not-accessible
                STATUS     mandatory
                DESCRIPTION
                        "The table holding UPPushPPG identifying
                         information."
                ::= { upPushPpg 1 }

        upPushPpgDescriptionEntry OBJECT-TYPE
                SYNTAX     UpPushPpgDescriptionEntry
                ACCESS     not-accessible
                STATUS     mandatory
                DESCRIPTION
                        "The entry associated with each UPPushPPG."
                INDEX { uppgdPPGIndex }
                ::= { upPushPpgDescriptionTable 1 }

        UpPushPpgDescriptionEntry ::= SEQUENCE {
        		uppgdPPGIndex	                INTEGER,
        		uppgdPPGIdentifier              INTEGER,
                uppgdProcessId                  INTEGER,
                uppgdHostName                   DisplayString,
                uppgdMsgServerPortNumber        INTEGER,
                uppgdStartupTime                DisplayString
        }
        
        uppgdPPGIndex  OBJECT-TYPE
                SYNTAX          INTEGER
                ACCESS          read-only
                STATUS          mandatory
                DESCRIPTION
                        "The index for each PPG process in the SNMP table."
                ::= { upPushPpgDescriptionEntry 1 }

        uppgdPPGIdentifier  OBJECT-TYPE
                SYNTAX          INTEGER
                ACCESS          read-only
                STATUS          mandatory
                DESCRIPTION
                        "The unique identifier for each PPG process."
                ::= { upPushPpgDescriptionEntry 2 }

        uppgdProcessId OBJECT-TYPE
                SYNTAX     INTEGER
                ACCESS     read-only
                STATUS     mandatory
                DESCRIPTION
                       "The process id of the UPPushPPG."
                ::= { upPushPpgDescriptionEntry 3 }

        uppgdHostName OBJECT-TYPE
                SYNTAX     DisplayString
                ACCESS     read-only
                STATUS     mandatory
                DESCRIPTION
                        "The host name of the node running UPPushPPG."
                ::= { upPushPpgDescriptionEntry 4 }

        uppgdMsgServerPortNumber OBJECT-TYPE
                SYNTAX     INTEGER
                ACCESS     read-only
                STATUS     mandatory
                DESCRIPTION
                        "The port number on which the UPPushPPG connects
                         to the proxy to deliver notifications."
                ::= { upPushPpgDescriptionEntry 5 }

        uppgdStartupTime OBJECT-TYPE
                SYNTAX     DisplayString
                ACCESS     read-only
                STATUS     mandatory
                DESCRIPTION
                        "The date and time the UPPushPPG started."
                ::= { upPushPpgDescriptionEntry 6 }

        --
        -- Notifications statistics that are held in the UPPushPPG
        --

        upPushPpgNtfnStatsTable OBJECT-TYPE
                SYNTAX     SEQUENCE OF UpPushPpgNtfnStatsEntry
                ACCESS     not-accessible
                STATUS     mandatory
                DESCRIPTION
                       "The table holding statistics on notification
                                                services provided by UPPushPPG."
                ::= { upPushPpg 2 }

        upPushPpgNtfnStatsEntry OBJECT-TYPE
                SYNTAX     UpPushPpgNtfnStatsEntry
                ACCESS     not-accessible
                STATUS     mandatory
                DESCRIPTION
                        "The entry associated with notifications statistics
                                                for each UPPushPPG."
                INDEX { uppgnsPPGIndex }
                ::= { upPushPpgNtfnStatsTable 1 }

        UpPushPpgNtfnStatsEntry ::= SEQUENCE {
                uppgnsPPGIndex	                        INTEGER,
                uppgnsPPGIdentifier                     INTEGER,
 				uppgnsTotalNumOfPendingNtfns            Counter,
                uppgnsAvgNtfnsDeliveredPerSec           INTEGER,
                uppgnsAvgNtfnsMarkedUnDelvrPerSec       INTEGER
        }

		uppgnsPPGIndex  OBJECT-TYPE
                SYNTAX     INTEGER
                ACCESS     read-only
                STATUS     mandatory
                DESCRIPTION
                        "The index for each PPG process in the SNMP table."
                ::= { upPushPpgNtfnStatsEntry 1 }

        uppgnsPPGIdentifier  OBJECT-TYPE
                SYNTAX     INTEGER
                ACCESS     read-only
                STATUS     mandatory
                DESCRIPTION
                        "The unique identifier for each PPG process."
                ::= { upPushPpgNtfnStatsEntry 2 }

        uppgnsTotalNumOfPendingNtfns OBJECT-TYPE
                SYNTAX     Counter
                ACCESS     read-only
                STATUS     mandatory
                DESCRIPTION
                         "The total number of pending notifications
                          in the cache as maintained by the UPPushPPG."
                ::= { upPushPpgNtfnStatsEntry 3 }

        uppgnsAvgNtfnsDeliveredPerSec OBJECT-TYPE
                SYNTAX     INTEGER
                ACCESS     read-only
                STATUS     mandatory
                DESCRIPTION
                        "The average number of notifications delivered
                         per second by the UPPushPPG."
                ::= { upPushPpgNtfnStatsEntry 4 }

        uppgnsAvgNtfnsMarkedUnDelvrPerSec OBJECT-TYPE
                SYNTAX     INTEGER
                ACCESS     read-only
                STATUS     mandatory
                DESCRIPTION
                        "The average number of notifications marked
                         undeliverable per second by the UPPushPPG."
                ::= { upPushPpgNtfnStatsEntry 5 }
        --
        -- upPushPpgTraps
        -- This section describes traps originated by UPPushPPG
        --

        upPushPpgTrapInfo   OBJECT IDENTIFIER ::= { upPushPpg 20 }

        uppgTrapInfo OBJECT-TYPE
                SYNTAX     DisplayString
                ACCESS     not-accessible
                STATUS     optional
                DESCRIPTION
                        "Text string that provides additional
                         information about the trap."
                ::= { upPushPpgTrapInfo 1 }

        uppgStartup TRAP-TYPE
                ENTERPRISE { upPushProxy }
                VARIABLES  { uppgdPPGIdentifier,
                             uppgdHostName,
                             uppgdProcessId,
                             uppgTrapInfo
                           }
                DESCRIPTION
                       "An event that is generated when the UPPushPPG
                        is started."
                ::= 1100

        upgFailToStart TRAP-TYPE
                ENTERPRISE { upPushProxy }
                VARIABLES { uppgdPPGIdentifier,
                            uppgdHostName,
                            uppgdProcessId,
                            uppgTrapInfo
                          }
                DESCRIPTION
                       "An event that is generated when the UPPushPPG
                        has failed to start-up. uppTrapInfo gives the 
                        reason for the shutdown."
                ::= 1101

        uppgDatabaseConnectionDown TRAP-TYPE
                ENTERPRISE { upPushProxy }
                VARIABLES { uppgdPPGIdentifier,
                            uppgdHostName,
                            uppgdProcessId,
                            uppgTrapInfo
                          }
                DESCRIPTION
                       "An event that is generated when the UPPushPPG
                        detects that the database connection is down."
                ::= 1102

        uppgInternalErrors TRAP-TYPE
                ENTERPRISE { upPushProxy }
                VARIABLES { uppgdPPGIdentifier,
                            uppgdHostName,
                            uppgdProcessId,
                            uppgTrapInfo
                          }
                DESCRIPTION
                       "An event that is generated when the UPPushPPG
                        detects an internal error. uppgTrapInfo
                        gives error name."
                ::= 1105
        
    --
    -- Services - all non-infrastructure items
    --
                
        services OBJECT IDENTIFIER ::= { openwave 5 }
       
    --
    -- UP.Mail
    --
    
        upMail      OBJECT IDENTIFIER ::= { services 1 }
        
        -- to be supplied
        
END
