   mscAtmIfPnniAddrRowStatus
      RowStatus,
   mscAtmIfPnniAddrComponentName
      DisplayString,
   mscAtmIfPnniAddrStorageType
      StorageType,
   mscAtmIfPnniAddrAddressIndex
      AsciiStringIndex,
   mscAtmIfPnniAddrAddressTypeIndex
      INTEGER }

