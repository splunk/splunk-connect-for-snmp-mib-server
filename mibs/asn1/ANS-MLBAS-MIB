-- *****************************************************************************
-- 
--			MLBAS Service Unit Port Table
--
-- *****************************************************************************

mlbasServiceUnitPortTable OBJECT-TYPE
	SYNTAX	SEQUENCE OF MlbasServiceUnitPortEntry
	ACCESS	not-accessible
	STATUS		mandatory
	DESCRIPTION
		"Service Unit Port table"
	::= { mlbasServiceUnitPort 1 }

mlbasServiceUnitPortEntry OBJECT-TYPE 
	SYNTAX	MlbasServiceUnitPortEntry
	ACCESS	not-accessible
	STATUS		mandatory
	DESCRIPTION
		"Entries in the mlbasServiceUnitPortTable represent ports
		 at Access Terminals provided as Service Unit ports at each
		 Access Terminal."
	INDEX {ansAccessUserPortSystemNode, ansAccessUserPortSubrack,
	       ansAccessUserPortPosition, ansAccessUserPortIndex}
	::= { mlbasServiceUnitPortTable 1 }

MlbasServiceUnitPortEntry ::= SEQUENCE {
	mlbasSUPortPosition		INTEGER,
	mlbasSUPortIndex		INTEGER
}

-- The mlbasServiceUnitPortTable is indexed by a port at the Access Terminal
-- (system node, subrack, position, port) and gives a fast way to obtain
-- the corresponding service unit port

mlbasSUPortPosition	OBJECT-TYPE
	SYNTAX		INTEGER
	ACCESS		read-only
	STATUS		mandatory
	DESCRIPTION
		"Reference to slot at the AT for the SU."
	::= { mlbasServiceUnitPortEntry 1 }

mlbasSUPortIndex	OBJECT-TYPE
	SYNTAX		INTEGER
	ACCESS		read-only
	STATUS		mandatory
	DESCRIPTION
		"Reference to the port at the SU."
	::= { mlbasServiceUnitPortEntry 2 }

END
