ANS-MLBAS-MIB DEFINITIONS ::= BEGIN



IMPORTS
        OBJECT-TYPE
                FROM SNMPv2-SMI
	DisplayString
                FROM RFC1213-MIB
	RowPointer, RowStatus, mlpmpR115, DateAndTime
		FROM ANS-COMMON-MIB
	ansAccessUserPortSystemNode, ansAccessUserPortSubrack,
	ansAccessUserPortPosition, ansAccessUserPortIndex
		FROM ANS-GS-MIB
	ansBoardSystemNodeIndex, ansBoardSubrackIndex, ansBoardPosition,
	ansSlotSystemNodeIndex, ansSlotSubrackIndex, ansSlotPosition
		FROM ANS-EQUIPMENT-MIB;

mlbas			OBJECT IDENTIFIER ::= {mlpmpR115 5}
mlbasRadioNode		OBJECT IDENTIFIER ::= {mlbas 1}
mlbasAccessTerminalSlot	OBJECT IDENTIFIER ::= {mlbas 2}
mlbasAccessTerminal	OBJECT IDENTIFIER ::= {mlbas 3}
mlbasServiceUnit	OBJECT IDENTIFIER ::= {mlbas 4}
mlbasDownLinkPerf15min	OBJECT IDENTIFIER ::= {mlbas 5}
mlbasUpLinkPerf15min	OBJECT IDENTIFIER ::= {mlbas 6}
mlbasDownLinkPerf24hour	OBJECT IDENTIFIER ::= {mlbas 7}
mlbasUpLinkPerf24hour	OBJECT IDENTIFIER ::= {mlbas 8}
mlbasDownLinkLog15min	OBJECT IDENTIFIER ::= {mlbas 9}
mlbasUpLinkLog15min	OBJECT IDENTIFIER ::= {mlbas 10}
mlbasDownLinkLog24hour	OBJECT IDENTIFIER ::= {mlbas 11}
mlbasUpLinkLog24hour	OBJECT IDENTIFIER ::= {mlbas 12}
mlbasServiceUnitPort    OBJECT IDENTIFIER ::= {mlbas 13}



-- *****************************************************************************
-- 
--			MLBAS Radio Node Table
--
-- *****************************************************************************

mlbasRadioNodeTable OBJECT-TYPE 
	SYNTAX	SEQUENCE OF MlbasRadioNodeEntry
	ACCESS	not-accessible
	STATUS	mandatory
	DESCRIPTION
		"Radio node table"
	::= { mlbasRadioNode 1 }

mlbasRadioNodeEntry OBJECT-TYPE
	SYNTAX	MlbasRadioNodeEntry
	ACCESS	not-accessible
	STATUS	mandatory
	DESCRIPTION
		"This entity represents a WBAS radio node which consists of
		 an indoor Control Unit (CU) and an outdoor Radio Unit. The
		 CU consists of MAC and modem and is implemented as an
		 extended board that can be inserted into a subrack slot."
	INDEX {ansBoardSystemNodeIndex, ansBoardSubrackIndex, ansBoardPosition}
	::= { mlbasRadioNodeTable 1 }

MlbasRadioNodeEntry ::= SEQUENCE {
	mlbasRnCUId		DisplayString (SIZE(20)),
	mlbasRnCUSecurity	INTEGER,
	mlbasRnCUBerThreshold	INTEGER,
	mlbasRnCUBwLimit	INTEGER,
	mlbasRnCULoopMode	INTEGER,
	mlbasRnCULoopPoint	INTEGER,
	mlbasRnCUAlarmLevel	INTEGER,
	mlbasRnCUSignedOnAts	INTEGER,
	mlbasRnCUSignedOffAts	INTEGER,
	mlbasRnCUChBw		INTEGER,
	mlbasRnCUHwIdModem	DisplayString (SIZE(41)),
	mlbasRnCUSwIdModem	DisplayString (SIZE(29)),
	mlbasRnRUDownFreq	INTEGER,
	mlbasRnRUUpFreq		INTEGER,
	mlbasRnRUDownFreqMin	INTEGER,
	mlbasRnRUDownFreqMax	INTEGER,
	mlbasRnRUFreqDuplex	INTEGER,
	mlbasRnRUFreqStep	INTEGER,
	mlbasRnRUOutputPower	INTEGER,
	mlbasRnRUOutputPowerMin	INTEGER,
	mlbasRnRUOutputPowerMax	INTEGER,
	mlbasRnRUHwIdRadio	DisplayString (SIZE(41)),
	mlbasRnRUSwIdRadio	DisplayString (SIZE(29)),
	mlbasRnInterfLevel	INTEGER,
	mlbasRnRSSIEXP		INTEGER,
	mlbasRnCUSignOffAts	INTEGER,
	mlbasRnSwitchBoardAction	INTEGER
}

--
--	This table is an extension of the ansBoardTable and uses the index
--	(ansBoardSystemNodeIndex, ansBoardSubrackIndex, ansBoardPosition)
--

mlbasRnCUId	OBJECT-TYPE
	-- TODO: Check if it is ok to have a variable size on the rn id
	SYNTAX	DisplayString (SIZE(20))
	ACCESS	read-write
	STATUS		mandatory
	DESCRIPTION
		"Radio Node Identity"
	::= { mlbasRadioNodeEntry 1 }

mlbasRnCUSecurity	OBJECT-TYPE
	SYNTAX	INTEGER{
			enable(1),
			disable(2)
			}
	ACCESS	read-write
	STATUS		mandatory
	DESCRIPTION
		"Controls scrambling on RNs and ATs"
	::= { mlbasRadioNodeEntry 2 }

mlbasRnCUBerThreshold	OBJECT-TYPE
	SYNTAX	INTEGER{
			ber3(3),
			ber4(4),
			ber5(5),
			ber6(6)
			}
	ACCESS	read-write
	STATUS  mandatory	
	DESCRIPTION
		"Threshold for bit error rate alarms"
	::= { mlbasRadioNodeEntry 3 }

mlbasRnCUBwLimit	OBJECT-TYPE
	SYNTAX	INTEGER (1..100)
	ACCESS	read-write
	STATUS		mandatory
	DESCRIPTION
		"Percentage of the available RN - AT air interface bandwidth
		 that is to be utilized for user-traffic"
	::= { mlbasRadioNodeEntry 4 }

mlbasRnCULoopMode	OBJECT-TYPE
	SYNTAX	INTEGER{
			off(1),
			permanent(2),
			selfTest(3)
			}
	ACCESS	read-write
	STATUS		mandatory
	DESCRIPTION
		"The loop mode"
	::= { mlbasRadioNodeEntry 5 }

mlbasRnCULoopPoint	OBJECT-TYPE
	SYNTAX	INTEGER{
			modem(1),
			radio(2)
			}
	ACCESS	read-write
	STATUS		mandatory
	DESCRIPTION
		"The loop point: Only editable when permanent"
	::= { mlbasRadioNodeEntry 6 }

mlbasRnCUAlarmLevel	OBJECT-TYPE
	SYNTAX	INTEGER{
			allAlarms(1),
			filter(2),
			noAlarms(3)
			}
	ACCESS	read-write
	STATUS		mandatory
	DESCRIPTION
		"Controls the alarm filtering at the RN"
	::= { mlbasRadioNodeEntry 7 }

mlbasRnCUSignedOnAts	OBJECT-TYPE
	SYNTAX		INTEGER
	ACCESS		read-only
	STATUS		mandatory
	DESCRIPTION
		"Number of signed on ATs at the RN"
	::= { mlbasRadioNodeEntry 8 }

mlbasRnCUSignedOffAts	OBJECT-TYPE
	SYNTAX		INTEGER
	ACCESS		read-only
	STATUS		mandatory
	DESCRIPTION
		"Number of signed off ATs at the RN"
	::= { mlbasRadioNodeEntry 9 }

mlbasRnCUChBw	OBJECT-TYPE
	SYNTAX		INTEGER
	ACCESS		read-only
	STATUS		mandatory
	DESCRIPTION
		"Channel Bandwidth [kHz]"
	::= { mlbasRadioNodeEntry 10 }

mlbasRnCUHwIdModem	OBJECT-TYPE
	SYNTAX		DisplayString (SIZE(41))
	ACCESS		read-only
	STATUS		mandatory
	DESCRIPTION
		"The hardware id of the modem"
	::= { mlbasRadioNodeEntry 11 }

mlbasRnCUSwIdModem	OBJECT-TYPE
	SYNTAX		DisplayString (SIZE(29))
	ACCESS		read-only
	STATUS		mandatory
	DESCRIPTION
		"The software id of the modem"
	::= { mlbasRadioNodeEntry 12 }

mlbasRnRUDownFreq	OBJECT-TYPE
	SYNTAX		INTEGER
	ACCESS	read-write
	STATUS		mandatory
	DESCRIPTION
		"Current down link center frequency [kHz]"
	::= { mlbasRadioNodeEntry 13 }

mlbasRnRUUpFreq	OBJECT-TYPE
	SYNTAX		INTEGER
	ACCESS		read-only
	STATUS		mandatory
	DESCRIPTION
		"Current up link center frequency [kHz]"
	::= { mlbasRadioNodeEntry 14 }

mlbasRnRUDownFreqMin	OBJECT-TYPE
	SYNTAX		INTEGER
	ACCESS		read-only
	STATUS		mandatory
	DESCRIPTION
		"Min. down link center frequency in subband [kHz]"
	::= { mlbasRadioNodeEntry 15 }

mlbasRnRUDownFreqMax	OBJECT-TYPE
	SYNTAX		INTEGER
	ACCESS		read-only
	STATUS		mandatory
	DESCRIPTION
		"Max. down link center frequency in subband [kHz]"
	::= { mlbasRadioNodeEntry 16 }

mlbasRnRUFreqDuplex	OBJECT-TYPE
	SYNTAX		INTEGER
	ACCESS		read-only
	STATUS		mandatory
	DESCRIPTION
		"Duplex dist. (mlbasRnRUUpFreq - mlbasRnRUDownFreq) [kHz]"
	::= { mlbasRadioNodeEntry 17 }

mlbasRnRUFreqStep	OBJECT-TYPE
	SYNTAX		INTEGER
	ACCESS		read-only
	STATUS		mandatory
	DESCRIPTION
		"Size of one frequency tuning step [kHz]"
	::= { mlbasRadioNodeEntry 18 }

mlbasRnRUOutputPower	OBJECT-TYPE
	SYNTAX		INTEGER
	ACCESS	read-write
	STATUS		mandatory
	DESCRIPTION
		"Current output power [dbm]"
	::= { mlbasRadioNodeEntry 19 }

mlbasRnRUOutputPowerMin	OBJECT-TYPE
	SYNTAX		INTEGER
	ACCESS		read-only
	STATUS		mandatory
	DESCRIPTION
		"Minimum output power [dbm]"
	::= { mlbasRadioNodeEntry 20 }

mlbasRnRUOutputPowerMax	OBJECT-TYPE
	SYNTAX		INTEGER
	ACCESS		read-only
	STATUS		mandatory
	DESCRIPTION
		"Maximum output power [dbm]"
	::= { mlbasRadioNodeEntry 21 }

mlbasRnRUHwIdRadio	OBJECT-TYPE
	SYNTAX		DisplayString (SIZE(41))
	ACCESS		read-only
	STATUS		mandatory
	DESCRIPTION
		"The hardware id of the radio"
	::= { mlbasRadioNodeEntry 22 }

mlbasRnRUSwIdRadio	OBJECT-TYPE
	SYNTAX		DisplayString (SIZE(29))
	ACCESS		read-only
	STATUS		mandatory
	DESCRIPTION
		"The software id of the radio"
	::= { mlbasRadioNodeEntry 23 }


mlbasRnInterfLevel      OBJECT-TYPE
        SYNTAX          INTEGER{
                         low(1),
                         medium(2),
                         high(3),
                         manual(4)
			}
        ACCESS      read-write
        STATUS          mandatory
        DESCRIPTION
                "Interference levels to set the RSSI expected"
        ::= { mlbasRadioNodeEntry 24 }


mlbasRnRSSIEXP	 	OBJECT-TYPE
	SYNTAX		INTEGER
	ACCESS	read-write
	STATUS		mandatory
	DESCRIPTION
		"Nominal RF Input Power [dbm]"
	::= { mlbasRadioNodeEntry 25 }

mlbasRnCUSignOffAts	OBJECT-TYPE
	SYNTAX	INTEGER{
			normal(1),
			signOff(2)
			}
	ACCESS	read-write
	STATUS		mandatory
	DESCRIPTION
		"Initiates sign off of all ATs at the RN"
	::= { mlbasRadioNodeEntry 26 }

mlbasRnSwitchBoardAction  OBJECT-TYPE
        SYNTAX  INTEGER{
                        none(1),
                        switch-board(2)
                        }
        ACCESS      read-write
        STATUS          mandatory
        DESCRIPTION
                ""
        ::= { mlbasRadioNodeEntry 27 }




-- *****************************************************************************
-- 
--			MLBAS Access Terminal Slot Table
--
-- *****************************************************************************

mlbasAccessTerminalSlotTable OBJECT-TYPE 
	SYNTAX	SEQUENCE OF MlbasAccessTerminalSlotEntry
	ACCESS	not-accessible
	STATUS		mandatory
	DESCRIPTION
		"Access Terminal Slot table"
	::= { mlbasAccessTerminalSlot 1 }

mlbasAccessTerminalSlotEntry OBJECT-TYPE 
	SYNTAX	MlbasAccessTerminalSlotEntry
	ACCESS	not-accessible
	STATUS		mandatory
	DESCRIPTION
		"This entity represents the relation between a Radio Node
		 and an Access Terminal."
	INDEX {ansSlotSystemNodeIndex, ansSlotSubrackIndex, ansSlotPosition}
	::= { mlbasAccessTerminalSlotTable 1 }

MlbasAccessTerminalSlotEntry ::= SEQUENCE {
	mlbasAtSlotTerminalId		DisplayString (SIZE(20)),
	mlbasAtSlotRnPos		INTEGER,
	mlbasAtSlotLocation		DisplayString (SIZE(0..255)),
	mlbasAtSlotSignOnAction		INTEGER,
	mlbasAtSlotSignOnStatus		INTEGER,
	mlbasAtSlotRowStatus		RowStatus
}

--
--	This table is an extension of the ansSlotTable and uses the index
--	(ansSlotSystemNodeIndex, ansSlotSubrackIndex, ansSlotPosition)
--

mlbasAtSlotTerminalId	OBJECT-TYPE
	SYNTAX		DisplayString (SIZE(20))
	ACCESS	read-write -- sec_key
	STATUS		mandatory
	DESCRIPTION
		"Access Terminal Identity, unique within the system"
	::= { mlbasAccessTerminalSlotEntry 1 }

mlbasAtSlotRnPos	OBJECT-TYPE
	SYNTAX		INTEGER
	ACCESS	read-write
	STATUS		mandatory
	DESCRIPTION
		"RN control board pos. within the subrack"
	::= { mlbasAccessTerminalSlotEntry 2 }

mlbasAtSlotLocation	OBJECT-TYPE
	SYNTAX		DisplayString (SIZE(0..255))
	ACCESS	read-write
	STATUS		mandatory
	DESCRIPTION
		"Physical location of the AT"
	::= { mlbasAccessTerminalSlotEntry 3 }

mlbasAtSlotSignOnAction	OBJECT-TYPE
	SYNTAX	INTEGER{
			none(1),
			on(2),
			off(3)
			}
	ACCESS	read-write
	STATUS		mandatory
	DESCRIPTION
		"Initiates sign on for the AT"
	::= { mlbasAccessTerminalSlotEntry 4 }

mlbasAtSlotSignOnStatus	OBJECT-TYPE
	SYNTAX	INTEGER{
			signedOn(1),
			signedOff(2),
			signingOn(3),
			signingOff(4)
			}
	ACCESS 		read-only
	STATUS 		mandatory
	DESCRIPTION
		"Current sign on status for the AT"
	::= { mlbasAccessTerminalSlotEntry 5 }

mlbasAtSlotRowStatus OBJECT-TYPE
	SYNTAX		RowStatus
	ACCESS	read-write
	STATUS		mandatory
	DESCRIPTION
		""
	::= { mlbasAccessTerminalSlotEntry 6 }


-- *****************************************************************************
-- 
--			MLBAS Access Terminal Table
--
-- *****************************************************************************

mlbasAccessTerminalTable OBJECT-TYPE 
	SYNTAX	SEQUENCE OF MlbasAccessTerminalEntry
	ACCESS	not-accessible
	STATUS		mandatory
	DESCRIPTION
		"Access Terminal table"
	::= { mlbasAccessTerminal 1 }

mlbasAccessTerminalEntry OBJECT-TYPE 
	SYNTAX	MlbasAccessTerminalEntry
	ACCESS	not-accessible
	STATUS		mandatory
	DESCRIPTION
		"This entity represents a WBAS Access Terminal in the remote
		 part of the access network located at a customer.
		 Collection of link performance is also controlled via this
		 entity."
	INDEX {ansSlotSystemNodeIndex, ansSlotSubrackIndex, ansSlotPosition}
	::= { mlbasAccessTerminalTable 1 }

MlbasAccessTerminalEntry ::= SEQUENCE {
	mlbasAtCUPerformance		INTEGER,
	mlbasAtCUPerfStart		DateAndTime,
	mlbasAtCUAtmLoop		INTEGER,
	mlbasAtCUBerThreshold		INTEGER,
	mlbasAtCUValid15MinPerf		INTEGER,
	mlbasAtCUInvalid15MinPerf	INTEGER,
	mlbasAtCUValid24HPerf		INTEGER,
	mlbasAtCUInvalid24HPerf		INTEGER,
	mlbasAtCUHwIdModem		DisplayString (SIZE(41)),
	mlbasAtCUSwIdModem		DisplayString (SIZE(29)),
	mlbasAtRUDownFreq		INTEGER,
	mlbasAtRUUpFreq			INTEGER,
	mlbasAtRUOutputPower		INTEGER,
	mlbasAtRUHwIdRadio		DisplayString (SIZE(41)),
	mlbasAtRUSwIdRadio		DisplayString (SIZE(29))
}

--
--	This table has been modifed by qtxoswi
--

mlbasAtCUPerformance	OBJECT-TYPE
	SYNTAX	INTEGER{
			enable(1),
			disable(2)
			}
	ACCESS	read-write
	STATUS		mandatory
	DESCRIPTION
		"Current performance monitoring status"
	::= { mlbasAccessTerminalEntry 1 }

mlbasAtCUPerfStart	OBJECT-TYPE
	SYNTAX		DateAndTime
	ACCESS		read-only
	STATUS		mandatory
	DESCRIPTION
		"Indicates when monitoring was started"
	::= { mlbasAccessTerminalEntry 2 }

mlbasAtCUAtmLoop	OBJECT-TYPE
	SYNTAX	INTEGER{
			on(1),
			off(2)
			}
	ACCESS	read-write
	STATUS		mandatory
	DESCRIPTION
		"Controls ATM traffic loop"
	::= { mlbasAccessTerminalEntry 3 }

mlbasAtCUBerThreshold	OBJECT-TYPE
	SYNTAX	INTEGER{
			ber3(3),
			ber4(4),
			ber5(5),
			ber6(6)
			}
	ACCESS	read-write
	STATUS		mandatory
	DESCRIPTION
		"Threshold for bit error rate alarms"
	::= { mlbasAccessTerminalEntry 4 }

mlbasAtCUValid15MinPerf	OBJECT-TYPE
	SYNTAX		INTEGER
	ACCESS		read-only
	STATUS		mandatory
	DESCRIPTION
		"Number of valid 15 min. periods stored in CP"
	::= { mlbasAccessTerminalEntry 5 }

mlbasAtCUInvalid15MinPerf	OBJECT-TYPE
	SYNTAX			INTEGER
	ACCESS			read-only
	STATUS			mandatory
	DESCRIPTION
		"Number of invalid 15 min. periods stored in CP"
	::= { mlbasAccessTerminalEntry 6 }

mlbasAtCUValid24HPerf	OBJECT-TYPE
	SYNTAX		INTEGER
	ACCESS		read-only
	STATUS		mandatory
	DESCRIPTION
		"Number of valid 24 hour periods stored in CP"
	::= { mlbasAccessTerminalEntry 7 }

mlbasAtCUInvalid24HPerf	OBJECT-TYPE
	SYNTAX		INTEGER
	ACCESS		read-only
	STATUS		mandatory
	DESCRIPTION
		"Number of invalid 24 hour periods stored in CP"
	::= { mlbasAccessTerminalEntry 8 }

mlbasAtCUHwIdModem	OBJECT-TYPE
	SYNTAX		DisplayString (SIZE(41))
	ACCESS		read-only
	STATUS		mandatory
	DESCRIPTION
		"The hardware id of the modem"
	::= { mlbasAccessTerminalEntry 9 }

mlbasAtCUSwIdModem	OBJECT-TYPE
	SYNTAX		DisplayString (SIZE(29))
	ACCESS		read-only
	STATUS		mandatory
	DESCRIPTION
		"The software id of the modem"
	::= { mlbasAccessTerminalEntry 10 }

mlbasAtRUDownFreq	OBJECT-TYPE
	SYNTAX		INTEGER
	ACCESS		read-only
	STATUS		mandatory
	DESCRIPTION
		"Current down link frequency [kHz]"
	::= { mlbasAccessTerminalEntry 11 }

mlbasAtRUUpFreq		OBJECT-TYPE
	SYNTAX		INTEGER
	ACCESS		read-only
	STATUS		mandatory
	DESCRIPTION
		"Current up link frequency [kHz]"
	::= { mlbasAccessTerminalEntry 12 }

mlbasAtRUOutputPower	OBJECT-TYPE
	SYNTAX		INTEGER
	ACCESS		read-only
	STATUS		mandatory
	DESCRIPTION
		"Current output power [dbm]"
	::= { mlbasAccessTerminalEntry 13 }

mlbasAtRUHwIdRadio	OBJECT-TYPE
	SYNTAX		DisplayString (SIZE(41))
	ACCESS		read-only
	STATUS		mandatory
	DESCRIPTION
		"The hardware id of the radio"
	::= { mlbasAccessTerminalEntry 14 }

mlbasAtRUSwIdRadio	OBJECT-TYPE
	SYNTAX		DisplayString (SIZE(29))
	ACCESS		read-only
	STATUS		mandatory
	DESCRIPTION
		"The software id of the radio"
	::= { mlbasAccessTerminalEntry 15 }



-- *****************************************************************************
-- 
--			MLBAS Service (Interface) Unit Table
--
-- *****************************************************************************

mlbasServiceUnitTable OBJECT-TYPE
	SYNTAX	SEQUENCE OF MlbasServiceUnitEntry
	ACCESS	not-accessible
	STATUS		mandatory
	DESCRIPTION
		"Service Interface Unit table"
	::= { mlbasServiceUnit 1 }

mlbasServiceUnitEntry OBJECT-TYPE
	SYNTAX	MlbasServiceUnitEntry
	ACCESS	not-accessible
	STATUS		mandatory
	DESCRIPTION
		"This entity represents a WBAS Service Interface Unit which
		 can be inserted into an Access Terminal as a plug-in card.
		 Both plug-in card and corresponding slot are covered by
		 the entity."
	INDEX {ansBoardSystemNodeIndex, ansBoardSubrackIndex,
	       ansBoardPosition, mlbasSuPos}
	::= { mlbasServiceUnitTable 1 }

MlbasServiceUnitEntry ::= SEQUENCE {
	mlbasSuPos		INTEGER,
	mlbasSuManagedStatus	INTEGER,
	mlbasSuOperState	INTEGER,
	mlbasSuHwId		DisplayString (SIZE(35)),
	mlbasSuType		INTEGER,
	mlbasSuPciId		INTEGER,
	mlbasSuPciVendor	INTEGER,
	mlbasSuPortStartIndex	INTEGER,
	mlbasSuNumOfPorts	INTEGER
}


mlbasSuPos		OBJECT-TYPE
	SYNTAX		INTEGER
	ACCESS		read-only -- key
	STATUS		mandatory
	DESCRIPTION
		"Position within the AT"
	::= { mlbasServiceUnitEntry 1 }

mlbasSuManagedStatus	OBJECT-TYPE
	SYNTAX	INTEGER{
			unmanaged(1),
			empty(2),
			present(3),
			managed(4)
			}
	ACCESS	read-write
	STATUS		mandatory
	DESCRIPTION
		"Defines status of the slot; Only managed and unmanaged is
		 selectable; When setting managed, empty or present is
		 returned"
	::= { mlbasServiceUnitEntry 2 }

mlbasSuOperState	OBJECT-TYPE
	SYNTAX	INTEGER{
			enabled(1),
			disabled(2)
			}
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION
		"Operational state"
	::= { mlbasServiceUnitEntry 3 }

mlbasSuHwId		OBJECT-TYPE
	SYNTAX		DisplayString (SIZE(35))
	ACCESS		read-only
	STATUS		mandatory
	DESCRIPTION
		"Hardware identity; mlbasSuManagedStatus must be set to
		 unmanaged"
	::= { mlbasServiceUnitEntry 4 }

mlbasSuType		OBJECT-TYPE
	SYNTAX	INTEGER{
			any(1),
			ethernet(2),
			e1(3),
			ds1(4),
			atm(5),
			unknown(6)
			}
	ACCESS	read-write
	STATUS		mandatory
	DESCRIPTION
		"Interface type"
	::= { mlbasServiceUnitEntry 5 }

mlbasSuPciId		OBJECT-TYPE
	SYNTAX		INTEGER
	ACCESS		read-only
	STATUS		mandatory
	DESCRIPTION
		"..."
	::= { mlbasServiceUnitEntry 6 }

mlbasSuPciVendor	OBJECT-TYPE
	SYNTAX		INTEGER
	ACCESS		read-only
	STATUS		mandatory
	DESCRIPTION
		"..."
	::= { mlbasServiceUnitEntry 7 }

mlbasSuPortStartIndex	OBJECT-TYPE
	SYNTAX		INTEGER
	ACCESS		read-only
	STATUS		mandatory
	DESCRIPTION
		"Port start index"
	::= { mlbasServiceUnitEntry 8 }

mlbasSuNumOfPorts	OBJECT-TYPE
	SYNTAX		INTEGER
	ACCESS		read-only
	STATUS		mandatory
	DESCRIPTION
		"Number of provided ports on the SU"
	::= { mlbasServiceUnitEntry 9 }



-- *****************************************************************************
-- 
--			MLBAS Down Link Performance 15 min Table
--
-- *****************************************************************************

mlbasDownLinkPerf15minTable OBJECT-TYPE
	SYNTAX	SEQUENCE OF MlbasDownLinkPerf15minEntry
	ACCESS	not-accessible
	STATUS		mandatory
	DESCRIPTION
		"Down Link Performance 15 min Table"
	::= { mlbasDownLinkPerf15min 1 }

mlbasDownLinkPerf15minEntry OBJECT-TYPE 
	SYNTAX	MlbasDownLinkPerf15minEntry
	ACCESS	not-accessible
	STATUS		mandatory
	DESCRIPTION
		"This entity contains performance data collected for the
		 down link."
	INDEX {ansBoardSystemNodeIndex, ansBoardSubrackIndex,
	       ansBoardPosition, mlbasDL15PeriodIndex}
	::= { mlbasDownLinkPerf15minTable 1 }

MlbasDownLinkPerf15minEntry ::= SEQUENCE {
	mlbasDL15PeriodIndex	INTEGER,
	mlbasDL15UpdateTime	DateAndTime,
