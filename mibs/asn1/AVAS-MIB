	::= {avasCTabEntry 4}

avasCState              OBJECT-TYPE
	SYNTAX  INTEGER {
			  created      (1),
			  deleted      (2),
			  ended        (3),
			  abended      (4),
			  free         (5),
			  error        (6),
			  ignored      (7),
			  no-plan      (8),
			  no-submit    (9),
			  skipped     (10),
			  shared      (11),
			  exclusiv    (12)
			}
	ACCESS  read-only
	STATUS  mandatory
	DESCRIPTION "The State of the Condition"
	::= {avasCTabEntry 5}

avasCValue              OBJECT-TYPE
	SYNTAX  INTEGER {
			  created      (1),  -- net job res
			  ended        (2),  -- net job
			  abended      (3),  -- net job
			  ignored      (4),  -- net job
			  free         (5),  --         res
			  error        (6),  --     job res
			  deleted      (7),  --     job
			  no-plan      (8),  --     job
			  no-submit    (9),  --     job
			  skipped     (10),  --     job
			  shared      (11),  --         res
			  exclusiv    (12),  --         res
			  value       (13)   --              val
			}
	ACCESS  read-only
	STATUS  mandatory
	DESCRIPTION "The Value of the Condition"
	::= {avasCTabEntry 6}

avasCCreateBy           OBJECT-TYPE
	SYNTAX  DisplayString
	ACCESS  read-only
	STATUS  mandatory
	DESCRIPTION "The Condition Description was created by ..."
	::= {avasCTabEntry 7}

avasCCreateDate         OBJECT-TYPE
	SYNTAX  DisplayString
	ACCESS  read-only
	STATUS  mandatory
	DESCRIPTION "Date of the Creation of Condition Description"
	::= {avasCTabEntry 8}

