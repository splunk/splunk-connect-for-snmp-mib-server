	SYNTAX  INTEGER {
			  all       (1),
			  free      (2),
			  error     (3),
			  share     (4),
			  exclusiv  (5)      -- weitere DRo ??
			}
	ACCESS  read-write
	STATUS  mandatory
	DESCRIPTION "Flag for restriction in Show Table"
		    -- s. BHB S.113
		    -- kann bei Auswahl angegeben werden
	::= {avasCond 1}

avasCNum                OBJECT-TYPE
	SYNTAX  INTEGER
	ACCESS  read-only
	STATUS  mandatory
	DESCRIPTION "The number of entries in the table avasCTab"
	::= {avasCond 2}

