			}
	ACCESS  read-only
	STATUS  mandatory
	DESCRIPTION "Delay-Solution for this element"
	::= {avasETabEntry 9}

avasELatest             OBJECT-TYPE
	SYNTAX  DisplayString
	ACCESS  read-only
	STATUS  mandatory
	DESCRIPTION "Latest Start/ Latest Occure"  -- time
	::= {avasETabEntry 10}

