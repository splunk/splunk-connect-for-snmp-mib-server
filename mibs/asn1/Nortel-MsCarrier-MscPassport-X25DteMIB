MscX25DtePLcnProvEntry ::= SEQUENCE {
   mscX25DtePLcnEncAddressType
      INTEGER,
   mscX25DtePLcnEncAddress
      AsciiString,
   mscX25DtePLcnProtocolEncType
      INTEGER,
