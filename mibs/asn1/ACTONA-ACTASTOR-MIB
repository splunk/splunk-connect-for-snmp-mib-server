    OBJECTS         {
                        acLogSeverity,
                        acLogMsgText
                    }
    STATUS          current
    DESCRIPTION
        "This trap indicates that the CoreServer has generated a
        log message.  This object supersedes mgrLogsTrap"
   ::= { acNotifications 0 }

