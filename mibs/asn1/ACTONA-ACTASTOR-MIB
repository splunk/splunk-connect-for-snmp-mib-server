    OBJECTS         {
                        acLogSeverity,
                        acLogMsgText
                    }
    STATUS          current
    DESCRIPTION
        "This trap indicates that the EdgeServer has generated a
        log message. This object supersedes esLogsTrap"
   ::= { acNotifications 2 }

