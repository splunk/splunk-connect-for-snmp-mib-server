    SYNTAX Integer32
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
        "Max size of object allowed in partition"
    ::= { ibmcacheserverPartitionEntry 11 }

ibmcacheserverPartitionDynamicDefaultLifetime OBJECT-TYPE
    SYNTAX Integer32
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
        "Default lifetime for dynamic items"
    ::= { ibmcacheserverPartitionEntry 12 }

