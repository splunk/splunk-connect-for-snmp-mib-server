		oPstnCasTabRegComp2Off	INTEGER
	}
	oPstnCasTabDslIndex OBJECT-TYPE
                SYNTAX INTEGER 
                ACCESS read-only
                STATUS mandatory
                DESCRIPTION "PSTN CAS Table Dsl Index."
		::= { accPstnIntCasSupvParamEntry 1 }
	oPstnCasTabSupvModeIn OBJECT-TYPE
                SYNTAX INTEGER {
			none(1),
