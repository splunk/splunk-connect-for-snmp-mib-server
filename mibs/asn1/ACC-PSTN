    accMisc	OBJECT IDENTIFIER ::= { accNewLiberty 5 }
	accDsprmMessageLevel OBJECT-TYPE
	    SYNTAX INTEGER
	    ACCESS read-write
	    STATUS mandatory
	    DESCRIPTION
	       " Level for depth of messages from the DSPRM."
	    ::= { accMisc 1 }
