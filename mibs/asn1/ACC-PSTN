		SYNTAX AccPstnIntCasSupvParamEntry
                ACCESS not-accessible
                STATUS mandatory
                DESCRIPTION "PSTN line supervision parameter-table entry."
		INDEX{ oPstnCasTabDslIndex }
                ::= { accPstnIntCasSupvParamTable 1 } 
	AccPstnIntCasSupvParamEntry ::= SEQUENCE {
