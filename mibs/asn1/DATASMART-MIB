              SYNTAX  IpAddress
              ACCESS  read-write
              STATUS  mandatory
              DESCRIPTION
                      "The IP address for the unit's Ethernet Port."
              ::= { dsMc 7 }

dsMcEIpMask OBJECT-TYPE
              SYNTAX  IpAddress
              ACCESS  read-write
              STATUS  mandatory
              DESCRIPTION
                      "The IP subnet mask for the unit's Ethernet Port."
              ::= { dsMc 8 }

