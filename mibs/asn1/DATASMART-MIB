              SYNTAX  INTEGER (0..99)
              ACCESS  read-write
              STATUS  mandatory
              DESCRIPTION
                      "The number of the current year. 95 is 1995."
              ::= { dsSc 3 }

dsScHour    OBJECT-TYPE
              SYNTAX  INTEGER (0..23)
              ACCESS  read-write
              STATUS  mandatory
              DESCRIPTION
                      "The number of the current hour. 0 is 12:00am."
              ::= { dsSc 4 }

