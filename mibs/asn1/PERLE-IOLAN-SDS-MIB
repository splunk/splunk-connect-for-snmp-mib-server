          OnOffValue,
        ioRelayCfgDescription
          DisplayString,
        ioRelayCfgMode
          INTEGER,
        ioRelayCfgPulseMode
          INTEGER,
        ioRelayCfgPulseCount
          INTEGER,
        ioRelayCfgInactiveSignalWidth
          INTEGER,
        ioRelayCfgActiveSignalWidth
          INTEGER,
        ioRelayCfgDelay
          INTEGER,
        ioRelayCfgFailsafe
          INTEGER
      }

