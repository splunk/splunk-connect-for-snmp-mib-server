        SYNTAX	IpAddress
        ACCESS	read-only
        STATUS	obsolete
        DESCRIPTION
                "(Local) IP address corresponding to
                wfFrSwUsageCircuitNumber of this virtual circuit. "
        ::= { wfFrSwUsageEntry 4 }

