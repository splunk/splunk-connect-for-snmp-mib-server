   SYNTAX     AsciiString (SIZE (0..60))
   ACCESS     read-write
   STATUS     mandatory
   DESCRIPTION
     "Any miscellaneous text about the facility."
::= { mscLpE1AdminInfoEntry 2 }

mscLpE1IfEntryTable OBJECT-TYPE
   SYNTAX     SEQUENCE OF MscLpE1IfEntryEntry
   ACCESS     not-accessible
   STATUS     mandatory
   DESCRIPTION
     "This group contains the provisionable attributes for the ifEntry."
::= { mscLpE1 13 }

mscLpE1IfEntryEntry OBJECT-TYPE
   SYNTAX     MscLpE1IfEntryEntry
   ACCESS     not-accessible
   STATUS     mandatory
   DESCRIPTION
     "An entry in the mscLpE1IfEntryTable."
   INDEX {
      mscLpIndex,
      mscLpE1Index }
