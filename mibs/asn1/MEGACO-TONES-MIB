   -- 
   -- ***************************************************************** 
   megacoTonesMIBNotifications OBJECT IDENTIFIER::= {megacoTonesMIB 2} 
    
    
   megTonesNotifPrefix  OBJECT IDENTIFIER ::= { 
   megacoTonesMIBNotifications 0 } 
   megTonesNotifObjects OBJECT IDENTIFIER ::= { 
   megacoTonesMIBNotifications 1 } 
 
    
