        DESCRIPTION
            "A warning event will be logged when the usage of the quota has
            reached this value."
        ::= { virtualVolumeTitanQuotaDefaultEntry 6 }

    virtualVolumeTitanQuotaDefaultUsageCritical OBJECT-TYPE
        SYNTAX Counter64
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "A severe event will be logged when the usage of the quota has
            reached this value."
        ::= { virtualVolumeTitanQuotaDefaultEntry 7 }

    virtualVolumeTitanQuotaDefaultFileCountLimit OBJECT-TYPE
        SYNTAX Unsigned32 (1..4294967295)
