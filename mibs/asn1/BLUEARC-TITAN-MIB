    virtualVolumeTitanQuotaDefaultFileCountHardLimit OBJECT-TYPE
        SYNTAX INTEGER {
            soft(0),
            hard(1),
            unknown(2)
        }
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "Whether the upper limit for number of files in the quota
            is a hard limit.

             soft(1)    - not a hard limit.
             hard(2)    - a hard limit."
        ::= { virtualVolumeTitanQuotaDefaultEntry 9 }

    virtualVolumeTitanQuotaDefaultFileCountReset OBJECT-TYPE
        SYNTAX Unsigned32 (1..4294967295)
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "When a warning limit for number of files has been reached,
            a warning event will not be logged again until the file count
            in the quota has first dropped back to the reset value."
        ::= { virtualVolumeTitanQuotaDefaultEntry 10 }

    virtualVolumeTitanQuotaDefaultFileCountWarning OBJECT-TYPE
        SYNTAX Unsigned32 (1..4294967295)
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "A warning event will be logged when the file count in the quota has
            reached this value."
        ::= { virtualVolumeTitanQuotaDefaultEntry 11 }

    virtualVolumeTitanQuotaDefaultFileCountCritical OBJECT-TYPE
        SYNTAX Unsigned32 (1..4294967295)
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "A severe event will be logged when the file count in the quota has
            reached this value."
        ::= { virtualVolumeTitanQuotaDefaultEntry 12 }

    virtualVolumeTitanQuotasTable OBJECT-TYPE
        SYNTAX SEQUENCE OF VirtualVolumeTitanQuotasEntry
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION
            "A table containing quotas associated with
            Virtual Volumes. The quotas are indexed by the values
            virtualVolumeTitanSpanId and virtualVolumeTitanTarget in
            virtualVolumeTitanTable, and the quota name."
        ::= { virtualVolumesTitan 7 }

    virtualVolumeTitanQuotasEntry OBJECT-TYPE
        SYNTAX VirtualVolumeTitanQuotasEntry
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION
            "Information about a quota."
        INDEX { virtualVolumeTitanSpanId, virtualVolumeTitanName, virtualVolumeTitanQuotasTarget, virtualVolumeTitanQuotasTargetType }
        ::= { virtualVolumeTitanQuotasTable 1 }

    VirtualVolumeTitanQuotasEntry ::=
        SEQUENCE {
            virtualVolumeTitanQuotasTarget
                DisplayString,
            virtualVolumeTitanQuotasType
                INTEGER,
            virtualVolumeTitanQuotasTargetType
                INTEGER,
            virtualVolumeTitanQuotasUsage
                Counter64,
            virtualVolumeTitanQuotasFileCount
                Unsigned32,
            virtualVolumeTitanQuotasUsageLimit
                Counter64,
            virtualVolumeTitanQuotasUsageHardLimit
                INTEGER,
            virtualVolumeTitanQuotasUsageReset
                Counter64,
            virtualVolumeTitanQuotasUsageWarning
                Counter64,
            virtualVolumeTitanQuotasUsageCritical
                Counter64,
            virtualVolumeTitanQuotasFileCountLimit
                Unsigned32,
            virtualVolumeTitanQuotasFileCountHardLimit
                INTEGER,
            virtualVolumeTitanQuotasFileCountReset
                Unsigned32,
            virtualVolumeTitanQuotasFileCountWarning
                Unsigned32,
            virtualVolumeTitanQuotasFileCountCritical
                Unsigned32
        }

    virtualVolumeTitanQuotasTarget OBJECT-TYPE
        SYNTAX DisplayString
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The target of a quota.
            If the quota is a Virtual Volume quota (i.e. tracking
            usage of the entire Virtual Volume, regardless of the
            user of group) then the target will be 'Virtual Volume
            Quota'.
            A User Quota will have a target comprised of the user's
            domain and user name, in the form <domain>\<user>.
            A Group quota target will consist of the group's domain
            and group name, in the form <domain>\<group>."


        ::= { virtualVolumeTitanQuotasEntry 1 }

    virtualVolumeTitanQuotasType OBJECT-TYPE
        SYNTAX INTEGER {
            default(1),
            explicit(2),
            unknown(3)
        }
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "Indicates whether the quota is a default or explicit quota.

             default(1)  - the quota has been created automatically using
                            the default values for the Virtual Volume.
             explicit(2) - the quota has been created explicitly using
                            user-defined values."
        ::= { virtualVolumeTitanQuotasEntry 2 }

    virtualVolumeTitanQuotasTargetType OBJECT-TYPE
        SYNTAX INTEGER {
            user(1),
            group(2),
            virtualvolume(3),
            unknown(4)
        }
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "Indicates whether the scope of the quota's tracking.

             user(1)            - the quota is tracking a single user's use
                                  of the Virtual Volume.
             group(2)           - the quota is tracking a group's use of the
                                  Virtual Volume.
             virtualvolume(3)   - the quota is tracking the use of the entire
                                  Virtual Volume."
        ::= { virtualVolumeTitanQuotasEntry 3 }

    virtualVolumeTitanQuotasUsage OBJECT-TYPE
        SYNTAX Counter64
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The usage of this quota, in bytes."
        ::= { virtualVolumeTitanQuotasEntry 4 }

    virtualVolumeTitanQuotasFileCount OBJECT-TYPE
        SYNTAX Unsigned32 (1..4294967295)
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The usage of this quota, in number of files."
        ::= { virtualVolumeTitanQuotasEntry 5 }

    virtualVolumeTitanQuotasUsageLimit OBJECT-TYPE
        SYNTAX Counter64
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The upper limit set for usage of space in this quota."
        ::= { virtualVolumeTitanQuotasEntry 6 }

    virtualVolumeTitanQuotasUsageHardLimit OBJECT-TYPE
        SYNTAX INTEGER {
            soft(0),
            hard(1),
            unknown(2)
        }
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "Whether the upper limit for usage on this quota is a hard limit.

             soft(1)    - not a hard limit.
             hard(2)    - a hard limit."
        ::= { virtualVolumeTitanQuotasEntry 7 }

    virtualVolumeTitanQuotasUsageReset OBJECT-TYPE
        SYNTAX Counter64
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "When a warning limit for usage has been reached, a warning event
            will not be logged again until the use of the quota has first
            dropped back to the reset value."
        ::= { virtualVolumeTitanQuotasEntry 8 }

    virtualVolumeTitanQuotasUsageWarning OBJECT-TYPE
        SYNTAX Counter64
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "A warning event will be logged when the usage of a quota has
            reached this value."
        ::= { virtualVolumeTitanQuotasEntry 9 }

    virtualVolumeTitanQuotasUsageCritical OBJECT-TYPE
        SYNTAX Counter64
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "A severe event will be logged when the usage of a quota has
            reached this value."
        ::= { virtualVolumeTitanQuotasEntry 10 }

    virtualVolumeTitanQuotasFileCountLimit OBJECT-TYPE
        SYNTAX Unsigned32 (1..4294967295)
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The upper limit set for the number of files in a quota."
        ::= { virtualVolumeTitanQuotasEntry 11 }

    virtualVolumeTitanQuotasFileCountHardLimit OBJECT-TYPE
        SYNTAX INTEGER {
            soft(0),
            hard(1),
            unknown(2)
        }
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "Whether the upper limit for number of files in a quota
            is a hard limit.

             soft(1)    - not a hard limit.
             hard(2)    - a hard limit."
        ::= { virtualVolumeTitanQuotasEntry 12 }

    virtualVolumeTitanQuotasFileCountReset OBJECT-TYPE
        SYNTAX Unsigned32 (1..4294967295)
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "When a warning limit for number of files has been reached,
            a warning event will not be logged again until the file count
            in a quota has first dropped back to the reset value."
        ::= { virtualVolumeTitanQuotasEntry 13 }

    virtualVolumeTitanQuotasFileCountWarning OBJECT-TYPE
        SYNTAX Unsigned32 (1..4294967295)
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "A warning event will be logged when the file count in a quota has
            reached this value."
        ::= { virtualVolumeTitanQuotasEntry 14 }

    virtualVolumeTitanQuotasFileCountCritical OBJECT-TYPE
