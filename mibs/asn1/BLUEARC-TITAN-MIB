        SYNTAX SEQUENCE OF LagEntry
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION
            "A table of information about link aggregations
            configured."
        ::= { linkAggregation 2 }

    lagEntry OBJECT-TYPE
        SYNTAX LagEntry
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION
            "Information about link aggregations."
        INDEX { lagName }
        ::= { lagTable 1 }

    LagEntry ::=
        SEQUENCE {
            lagName
                DisplayString,
            linkedInterfaces
                DisplayString
        }

    lagName OBJECT-TYPE
        SYNTAX DisplayString
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The name of the Link Aggregation."
        ::= { lagEntry 1 }

    linkedInterfaces OBJECT-TYPE
        SYNTAX DisplayString
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "A list of physical interfaces in the Link Aggregation."
        ::= { lagEntry 2 }


--------------------------------------------------------------------------------
--
-- MgmntTitan / Packages
--
--------------------------------------------------------------------------------

     packageNumber OBJECT-TYPE
        SYNTAX Integer32 (1..2147483647)
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The number of packages in flash."
        ::= { packageMgmnt 1 }

     packageTable OBJECT-TYPE
        SYNTAX SEQUENCE OF PackageEntry
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION
            "A table containing details of the packages available in flash."
        ::= { packageMgmnt 2 }

    packageEntry OBJECT-TYPE
        SYNTAX PackageEntry
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION
            "Information about the packages in flash."
        INDEX { packageName }
        ::= { packageTable 1 }

    PackageEntry ::=
        SEQUENCE {
            packageName
                DisplayString,
            packageDate
                DisplayString,
            packageSize
                Unsigned32,
            packageDefault
                INTEGER,
            packageCurrent
                INTEGER
        }

    packageName OBJECT-TYPE
        SYNTAX DisplayString
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The name of the package."
        ::= { packageEntry 1 }

    packageDate OBJECT-TYPE
        SYNTAX DisplayString
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The date on which the package was uploaded."
        ::= { packageEntry 2 }

    packageSize OBJECT-TYPE
        SYNTAX Unsigned32 (1..4294967295)
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The size of the package in bytes."
        ::= { packageEntry 3 }

    packageDefault OBJECT-TYPE
        SYNTAX INTEGER {
            default(0),
            notdefault(1)
        }
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "Whether the package is the default package. The default
            package is the package which will be loaded when the server
            is rebooted.
             default(1)    - the default package.
             notdefault(2) - not the default package."
        ::= { packageEntry 4 }

    packageCurrent OBJECT-TYPE
        SYNTAX INTEGER {
            current(0),
            notcurrent(1)
        }
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "Whether the package is the current package. The current
            package is the package which was loaded when the server
            was rebooted.
             current(1)    - the current package.
             notcurrent(2) - not the current package."
        ::= { packageEntry 5 }

    packageFreeSpace OBJECT-TYPE
        SYNTAX Unsigned32 (1..4294967295)
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The amount of available flash file-space, in bytes."
        ::= { packageMgmnt 3 }

