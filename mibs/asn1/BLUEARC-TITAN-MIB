    ftpConfigServiceEnabled OBJECT-TYPE
        SYNTAX INTEGER {
            enabled(1),
            disabled(2),
            unknown(3)
        }
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "Indicates whether the FTP service is enabled.

            enabled(1) - the service is enabled
            disabled(2) - the service is disabled
            unknown(3) - the status of the service cannot be
                         determined.

            This object is now obsolete."
        ::= { ftpConfig 1 }

    ftpConfigServiceMaxUsers OBJECT-TYPE
        SYNTAX Unsigned32
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "The maximum number of FTP users the service will allow.

            This object is now obsolete."
        ::= { ftpConfig 2 }

    ftpConfigTimeout OBJECT-TYPE
        SYNTAX Integer32
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "The FTP session timeout in minutes.
             NOTE: this value applies to all FTP sessions.

            This object is now obsolete."
        ::= { ftpConfig 3 }

    ftpConfigNTPasswordEnabled OBJECT-TYPE
        SYNTAX INTEGER {
            enabled(1),
            disabled(2),
            notavailable(3),
            unknown(4)
        }
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "Indicates whether FTP uses NT password authentication.

            enabled(1) - NT security is enabled
            disabled(2) - NT security is disabled
            notavailable(3) - NT security is not available
            unknown(4) - the status of NT security cannot be
                         determined.

            This object is now obsolete."
        ::= { ftpConfig 4 }

    ftpConfigNISPasswordEnabled OBJECT-TYPE
        SYNTAX INTEGER {
            enabled(1),
            disabled(2),
            notavailable(3),
            unknown(4)
        }
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "Indicates whether FTP uses NIS password authentication.

            enabled(1) - NIS security is enabled
            disabled(2) - NIS security is disabled
            notavailable(3) - NIS security is not available
            unknown(4) - the status of NIS security cannot be
                         determined.

            This object is now obsolete."
        ::= { ftpConfig 5 }

    ftpAuditLoggingTable OBJECT-TYPE
        SYNTAX SEQUENCE OF FtpAuditLoggingEntry
        MAX-ACCESS not-accessible
        STATUS obsolete
        DESCRIPTION
            "A table containing the FTP audit logging configuration 
            for the system.

             The number of entries in this table is given by the number of
             EVSs.

            This object is now obsolete."
        ::= { ftpConfig 6 }

    ftpAuditLoggingEntry OBJECT-TYPE
        SYNTAX FtpAuditLoggingEntry
        MAX-ACCESS not-accessible
        STATUS obsolete
        DESCRIPTION
            "FTP audit logging information."
        INDEX { ftpAuditLoggingEVS }
        ::= { ftpAuditLoggingTable 1 }

    FtpAuditLoggingEntry ::=
        SEQUENCE {
            ftpAuditLoggingEVS
                Integer32,
            ftpConfigAuditLogging
                INTEGER,
            ftpConfigAuditLogDeviceId
                Integer32,
            ftpConfigAuditLogDirectory
                DisplayString,
            ftpConfigAuditLogRecordsPerFile
                Unsigned32,
            ftpConfigAuditMaximumLogFiles
                Unsigned32
        }

    ftpAuditLoggingEVS OBJECT-TYPE
        SYNTAX Integer32 (0..255)
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "The ID of the EVS the FTP audit logging
             configuration is associated with.

            This object is now obsolete."
        ::= { ftpAuditLoggingEntry 1 }

    ftpConfigAuditLogging OBJECT-TYPE
        SYNTAX INTEGER {
            enabled(1),
            disabled(2),
            unknown(3)
        }
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "Indicates whether FTP Audit logging is enabled on
            this Cluster Node.

            enabled(1) - logging is enabled
            disabled(2) - logging is disabled
            unknown(3) - the status of logging cannot be
                         determined.

            This object is now obsolete."
        ::= { ftpAuditLoggingEntry 2 }

    ftpConfigAuditLogDeviceId OBJECT-TYPE
        SYNTAX Integer32
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "The Device ID of the volume to which the FTP server
            is to write the audit log.

            This object is now obsolete."
        ::= { ftpAuditLoggingEntry 3 }

    ftpConfigAuditLogDirectory OBJECT-TYPE
        SYNTAX DisplayString
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "The directory to which the FTP server is to write
            the audit log.

            This object is now obsolete."
        ::= { ftpAuditLoggingEntry 4 }

    ftpConfigAuditLogRecordsPerFile OBJECT-TYPE
        SYNTAX Unsigned32
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "The number of records in each audit log file.

            This object is now obsolete."
        ::= { ftpAuditLoggingEntry 5 }

    ftpConfigAuditMaximumLogFiles OBJECT-TYPE
        SYNTAX Unsigned32
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "The maximum number of audit log files.

            This object is now obsolete."
        ::= { ftpAuditLoggingEntry 6 }

