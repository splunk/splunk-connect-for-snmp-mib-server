--
--
--  BlueArc Titan Server Enterprise MIB Module
--
--

BLUEARC-TITAN-MIB DEFINITIONS ::= BEGIN

IMPORTS
    MODULE-IDENTITY, OBJECT-TYPE, Integer32,
    Unsigned32, Counter32, Counter64,
    IpAddress, mib-2, enterprises             FROM SNMPv2-SMI
    DisplayString                             FROM SNMPv2-TC
    MODULE-COMPLIANCE, OBJECT-GROUP           FROM SNMPv2-CONF
    blueArc, blueArcPrivate                   FROM BLUEARC-SERVER-MIB;

blueArcTitan MODULE-IDENTITY
    LAST-UPDATED "200711051200Z"
    ORGANIZATION "BlueArc UK Ltd."
    CONTACT-INFO
                 "Contact:  Central Support

                   Postal:  BlueArc UK Ltd.,
                            Queensgate House,
                            Cookham Road,
                            Bracknell, RG12 1RB,
                            United Kingdom.

                      Tel:  +44(0)1344 408 200
                      Fax:  +44(0)1344 408 202

                      WEB:  http://www.bluearc.com
                    email:  centralsupport@bluearc.com"
    DESCRIPTION
                 "The MIB module for monitoring a BlueArc Titan Server."
    REVISION
                 "200711051200Z"
    DESCRIPTION
                 "Obsolete FTP user and mount point objects, which are
                  no longer to be reported by the agent."
    REVISION
                 "200604241200Z"
    DESCRIPTION
                 "Obsolete NVRAM objects, and re-implement in BLUEARC_SERVER_MIB."
    REVISION
                 "200402201200Z"
    DESCRIPTION
                 "Define objects for reporting FTP configuration, mount points,
                 users and statistics.
                 
                 Obsolete NVRAM objects, and re-implement in a table indexed
                 by Cluster Node."
    REVISION
                 "200311281200Z"
    DESCRIPTION
                 "Obsolete all quota template and quota objects, and
                 re-implement them using Counter64 data types for
                 usage and usage-limit objects."
    REVISION
                 "200306161200Z"
    DESCRIPTION
                 "Initial Revision 16 Jun 2003 12:00 GMT."

::= { blueArcPrivate 2 }

--------------------------------------------------------------------------------
--
-- MIB Tree Structure
--
--------------------------------------------------------------------------------

    blueArcTitanObjs                    OBJECT IDENTIFIER ::= { blueArcTitan 1 }
        systemTitan                     OBJECT IDENTIFIER ::= { blueArcTitanObjs 1 }
        storageTitan                    OBJECT IDENTIFIER ::= { blueArcTitanObjs 2 }
            virtualVolumesTitan         OBJECT IDENTIFIER ::= { storageTitan 1 }
            nvram                       OBJECT IDENTIFIER ::= { storageTitan 2 }
        networkTitan                    OBJECT IDENTIFIER ::= { blueArcTitanObjs 3 }
            ethernetTitan               OBJECT IDENTIFIER ::= { networkTitan 1}
                linkAggregation         OBJECT IDENTIFIER ::= { ethernetTitan 1 }
        mgmntTitan                      OBJECT IDENTIFIER ::= { blueArcTitanObjs 4 }
            packageMgmnt                OBJECT IDENTIFIER ::= { mgmntTitan 1 }
        fileProtocolTitan               OBJECT IDENTIFIER ::= { blueArcTitanObjs 5 }
            ftpTitan                    OBJECT IDENTIFIER ::= { fileProtocolTitan 1}
                ftpConfig               OBJECT IDENTIFIER ::= { ftpTitan 1 }
                ftpMountpoints          OBJECT IDENTIFIER ::= { ftpTitan 2 }
                ftpUser                 OBJECT IDENTIFIER ::= { ftpTitan 3 }
                ftpStatistics           OBJECT IDENTIFIER ::= { ftpTitan 4 }

--------------------------------------------------------------------------------
--
-- StorageTitan / Virtual Volumes
--
--------------------------------------------------------------------------------

    virtualVolumeTitanNumber OBJECT-TYPE
        SYNTAX Integer32 (1..2147483647)
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The number of virtual volumes which are currently accessible.
             NOTE: a virtual volume is only accessible if its associated
                   parent volume is mounted."
        ::= { virtualVolumesTitan 1 }

    virtualVolumeTitanTable OBJECT-TYPE
        SYNTAX SEQUENCE OF VirtualVolumeTitanEntry
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION
            "A table containing information about each virtual volume.

             The number of entries in this table is given by the value of
             virtualVolumeTitanNumber."
        ::= { virtualVolumesTitan 2 }

    virtualVolumeTitanEntry OBJECT-TYPE
        SYNTAX VirtualVolumeTitanEntry
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION
            "Information about a virtual volume."
        INDEX { virtualVolumeTitanSpanId, virtualVolumeTitanName }
        ::= { virtualVolumeTitanTable 1 }

    VirtualVolumeTitanEntry ::=
        SEQUENCE {
            virtualVolumeTitanSpanId
                Unsigned32,
            virtualVolumeTitanName
                DisplayString,
            virtualVolumeTitanPath
                DisplayString,
            virtualVolumeTitanEmailNumber
                Integer32,
            virtualVolumeTitanQuotaNumber
                Integer32
        }

    virtualVolumeTitanSpanId OBJECT-TYPE
        SYNTAX Unsigned32 (1..4294967295)
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The parent device of the virtual volume.
             The device identified by a particular value of this index is
             the span underlying the volume hosting the Virtual Volume."
        ::= { virtualVolumeTitanEntry 1 }

    virtualVolumeTitanName OBJECT-TYPE
        SYNTAX DisplayString
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The name assigned to the virtual volume by a user for
             identification purposes."
        ::= { virtualVolumeTitanEntry 2 }

    virtualVolumeTitanPath OBJECT-TYPE
        SYNTAX DisplayString
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The path to the root of this Virtual Volume."
        ::= { virtualVolumeTitanEntry 3 }

    virtualVolumeTitanEmailNumber OBJECT-TYPE
        SYNTAX Integer32 (1..2147483647)
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The number of email addresses associated with this
            Virtual Volume."
        ::= { virtualVolumeTitanEntry 4 }

    virtualVolumeTitanQuotaNumber OBJECT-TYPE
        SYNTAX Integer32 (1..2147483647)
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The number of quotas associated with this
            Virtual Volume."
        ::= { virtualVolumeTitanEntry 5 }

    virtualVolumeTitanEmailTable OBJECT-TYPE
        SYNTAX SEQUENCE OF VirtualVolumeTitanEmailEntry
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION
            "A table containing the email address associated with
            this Virtual Volume.

             The number of entries in this table is given by the value of
             virtualVolumeTitanEmailNumber, in virtualVolumeTitanTable."
        ::= { virtualVolumesTitan 3 }

    virtualVolumeTitanEmailEntry OBJECT-TYPE
        SYNTAX VirtualVolumeTitanEmailEntry
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION
            "An email address associated with a Virtual Volume,
            which is uniquely identified by virtualVolumeTitanSpanId
            and virtualVolumeTitanName in virtualVolumeTitanTable."
        INDEX { virtualVolumeTitanSpanId, virtualVolumeTitanName, virtualVolumeTitanEmailAddress }
        ::= { virtualVolumeTitanEmailTable 1 }

    VirtualVolumeTitanEmailEntry ::=
        SEQUENCE {
            virtualVolumeTitanEmailAddress
                DisplayString
        }

    virtualVolumeTitanEmailAddress OBJECT-TYPE
        SYNTAX DisplayString
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "An email address associated with a Virtual Volume."
        ::= { virtualVolumeTitanEmailEntry 1 }

    virtualVolumeTitanQuotaTemplateTable OBJECT-TYPE
        SYNTAX SEQUENCE OF VirtualVolumeTitanQuotaTemplateEntry
        MAX-ACCESS not-accessible
        STATUS obsolete
        DESCRIPTION
            "A table containing quota templates associated with
            Virtual Volumes. The template is indexed by the values
            virtualVolumeTitanSpanId and virtualVolumeTitanTarget in
            virtualVolumeTitanTable, and the
            virtualVolumeQuotaTemplateTargetType.

            The two types of quota template are: user quota template,
            and group quota template. The templates define the
            default quota settings which will be used to create a
            Default Quota when a user or group begins to use the Virtual
            Volume. Once the default quota has been created, a user
            or group's quota may be modified, whence it will become an
            Explicit Quota.

            This object is now obsolete."
        ::= { virtualVolumesTitan 4 }

    virtualVolumeTitanQuotaTemplateEntry OBJECT-TYPE
        SYNTAX VirtualVolumeTitanQuotaTemplateEntry
        MAX-ACCESS not-accessible
        STATUS obsolete
        DESCRIPTION
            "A quota template associated with a Virtual Volume.
            The template is indexed by the values virtualVolumeTitanSpanId
            and virtualVolumeTitanTarget in virtualVolumeTitanTable, and 
            the virtualVolumeQuotaTemplateTargetType.

            This object is now obsolete."
        INDEX { virtualVolumeTitanSpanId, virtualVolumeTitanName, virtualVolumeQuotaTemplateTargetType }
        ::= { virtualVolumeTitanQuotaTemplateTable 1 }

    VirtualVolumeTitanQuotaTemplateEntry ::=
        SEQUENCE {
            virtualVolumeQuotaTemplateTargetType
                INTEGER,
            virtualVolumeQuotaTemplateIgnoreWellKnownGroups
                INTEGER,
            virtualVolumeTitanQuotaTemplateUsageLimit
                Unsigned32,
            virtualVolumeTitanQuotaTemplateUsageHardLimit
                INTEGER,
            virtualVolumeTitanQuotaTemplateUsageReset
                Unsigned32,
            virtualVolumeTitanQuotaTemplateUsageWarning
                Unsigned32,
            virtualVolumeTitanQuotaTemplateUsageCritical
                Unsigned32,
            virtualVolumeTitanQuotaTemplateFileCountLimit
                Unsigned32,
            virtualVolumeTitanQuotaTemplateFileCountHardLimit
                INTEGER,
            virtualVolumeTitanQuotaTemplateFileCountReset
                Unsigned32,
            virtualVolumeTitanQuotaTemplateFileCountWarning
                Unsigned32,
            virtualVolumeTitanQuotaTemplateFileCountCritical
                Unsigned32
        }

    virtualVolumeQuotaTemplateTargetType OBJECT-TYPE
        SYNTAX INTEGER {
            user(1),
            group(2)
        }
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "The type of a Quota Template associated with a Virtual
            Volume.

            user(1)     - a Quota Template for a user
            group(2)    - a Quota Template for a group.

            This object is now obsolete."
        ::= { virtualVolumeTitanQuotaTemplateEntry 1 }

    virtualVolumeQuotaTemplateIgnoreWellKnownGroups OBJECT-TYPE
        SYNTAX INTEGER {
            ignore(1),
            include(2),
            notapplicable(3)
        }
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "Should this Quota Template generate a quota for 
            well-known groups. A well-known group would
            be a group to which many or all of the potential
            users belong. It may be desired to ignore such
            a group because a default quota generated would
            quickly reach usage or file-count thresholds, and
            cause events to be logged unnecessarily.

            A value notapplicable(3) will be set if the row in
            the table relates to a User Quota Template.

            This object is now obsolete."
        ::= { virtualVolumeTitanQuotaTemplateEntry 2 }

    virtualVolumeTitanQuotaTemplateUsageLimit OBJECT-TYPE
        SYNTAX Unsigned32 (1..4294967295)
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "The upper limit set for usage of space in the quota.

            This object is now obsolete."
        ::= { virtualVolumeTitanQuotaTemplateEntry 3 }

    virtualVolumeTitanQuotaTemplateUsageHardLimit OBJECT-TYPE
        SYNTAX INTEGER {
            soft(0),
            hard(1)
        }
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "Whether the upper limit for usage on the quota is a hard limit.

             soft(1)    - not a hard limit.
             hard(2)    - a hard limit.

            This object is now obsolete."
        ::= { virtualVolumeTitanQuotaTemplateEntry 4 }

    virtualVolumeTitanQuotaTemplateUsageReset OBJECT-TYPE
        SYNTAX Unsigned32 (1..4294967295)
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "When a warning limit for usage has been reached, a warning event
            will not be logged again until the use of the quota has first
            dropped back to the reset value.

            This object is now obsolete."
        ::= { virtualVolumeTitanQuotaTemplateEntry 5 }

    virtualVolumeTitanQuotaTemplateUsageWarning OBJECT-TYPE
        SYNTAX Unsigned32 (1..4294967295)
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "A warning event will be logged when the usage of the quota has
            reached this value.

            This object is now obsolete."
        ::= { virtualVolumeTitanQuotaTemplateEntry 6 }

    virtualVolumeTitanQuotaTemplateUsageCritical OBJECT-TYPE
        SYNTAX Unsigned32 (1..4294967295)
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "A severe event will be logged when the usage of the quota has
            reached this value.

            This object is now obsolete."
        ::= { virtualVolumeTitanQuotaTemplateEntry 7 }

    virtualVolumeTitanQuotaTemplateFileCountLimit OBJECT-TYPE
        SYNTAX Unsigned32 (1..4294967295)
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "The upper limit set for the number of files in the quota.

            This object is now obsolete."
        ::= { virtualVolumeTitanQuotaTemplateEntry 8 }

    virtualVolumeTitanQuotaTemplateFileCountHardLimit OBJECT-TYPE
        SYNTAX INTEGER {
            soft(0),
            hard(1)
        }
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "Whether the upper limit for number of files in the quota
            is a hard limit.

             soft(1)    - not a hard limit.
             hard(2)    - a hard limit.

            This object is now obsolete."
        ::= { virtualVolumeTitanQuotaTemplateEntry 9 }

    virtualVolumeTitanQuotaTemplateFileCountReset OBJECT-TYPE
        SYNTAX Unsigned32 (1..4294967295)
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "When a warning limit for number of files has been reached,
            a warning event will not be logged again until the file count
            in the quota has first dropped back to the reset value.

            This object is now obsolete."
        ::= { virtualVolumeTitanQuotaTemplateEntry 10 }

    virtualVolumeTitanQuotaTemplateFileCountWarning OBJECT-TYPE
        SYNTAX Unsigned32 (1..4294967295)
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "A warning event will be logged when the file count in the quota has
            reached this value.

            This object is now obsolete."
        ::= { virtualVolumeTitanQuotaTemplateEntry 11 }

    virtualVolumeTitanQuotaTemplateFileCountCritical OBJECT-TYPE
        SYNTAX Unsigned32 (1..4294967295)
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "A severe event will be logged when the file count in the quota has
            reached this value.

            This object is now obsolete."
        ::= { virtualVolumeTitanQuotaTemplateEntry 12 }

    virtualVolumeTitanQuotaTable OBJECT-TYPE
        SYNTAX SEQUENCE OF VirtualVolumeTitanQuotaEntry
        MAX-ACCESS not-accessible
        STATUS obsolete
        DESCRIPTION
            "A table containing quotas associated with
            Virtual Volumes. The quotas are indexed by the values
            virtualVolumeTitanSpanId and virtualVolumeTitanTarget in
            virtualVolumeTitanTable, and the quota name.

            This object is now obsolete."
        ::= { virtualVolumesTitan 5 }

    virtualVolumeTitanQuotaEntry OBJECT-TYPE
        SYNTAX VirtualVolumeTitanQuotaEntry
        MAX-ACCESS not-accessible
        STATUS obsolete
        DESCRIPTION
            "Information about a quota.

            This object is now obsolete."
        INDEX { virtualVolumeTitanSpanId, virtualVolumeTitanName, virtualVolumeTitanQuotaTarget, virtualVolumeTitanQuotaTargetType }
        ::= { virtualVolumeTitanQuotaTable 1 }

    VirtualVolumeTitanQuotaEntry ::=
        SEQUENCE {
            virtualVolumeTitanQuotaTarget
                DisplayString,
            virtualVolumeTitanQuotaType
                INTEGER,
            virtualVolumeTitanQuotaTargetType
                INTEGER,
            virtualVolumeTitanQuotaUsage
                Unsigned32,
            virtualVolumeTitanQuotaFileCount
                Unsigned32,
            virtualVolumeTitanQuotaUsageLimit
                Unsigned32,
            virtualVolumeTitanQuotaUsageHardLimit
                INTEGER,
            virtualVolumeTitanQuotaUsageReset
                Unsigned32,
            virtualVolumeTitanQuotaUsageWarning
                Unsigned32,
            virtualVolumeTitanQuotaUsageCritical
                Unsigned32,
            virtualVolumeTitanQuotaFileCountLimit
                Unsigned32,
            virtualVolumeTitanQuotaFileCountHardLimit
                INTEGER,
            virtualVolumeTitanQuotaFileCountReset
                Unsigned32,
            virtualVolumeTitanQuotaFileCountWarning
                Unsigned32,
            virtualVolumeTitanQuotaFileCountCritical
                Unsigned32
        }

    virtualVolumeTitanQuotaTarget OBJECT-TYPE
        SYNTAX DisplayString
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "The target of a quota.
            If the quota is a Virtual Volume quota (i.e. tracking
            usage of the entire Virtual Volume, regardless of the
            user of group) then the target will be 'Virtual Volume
            Quota'.
            A User Quota will have a target comprised of the user's
            domain and user name, in the form <domain>\<user>.
            A Group quota target will consist of the group's domain
            and group name, in the form <domain>\<group>.

            This object is now obsolete."
        ::= { virtualVolumeTitanQuotaEntry 1 }

    virtualVolumeTitanQuotaType OBJECT-TYPE
        SYNTAX INTEGER {
            default(1),
            explicit(2)
        }
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "Indicates whether the quota is a default or explicit quota.

             default(1)  - the quota has been created automatically using
                            the default values for the Virtual Volume.
             explicit(2) - the quota has been created explicitly using
                            user-defined values.

            This object is now obsolete."
        ::= { virtualVolumeTitanQuotaEntry 2 }

    virtualVolumeTitanQuotaTargetType OBJECT-TYPE
        SYNTAX INTEGER {
            user(1),
            group(2),
            virtualvolume(3)
        }
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "Indicates whether the scope of the quota's tracking.

             user(1)            - the quota is tracking a single user's use
                                  of the Virtual Volume.
             group(2)           - the quota is tracking a group's use of the
                                  Virtual Volume.
             virtualvolume(3)   - the quota is tracking the use of the entire
                                  Virtual Volume.

            This object is now obsolete."
        ::= { virtualVolumeTitanQuotaEntry 3 }

    virtualVolumeTitanQuotaUsage OBJECT-TYPE
        SYNTAX Unsigned32 (1..4294967295)
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "The usage of this quota, in bytes.

            This object is now obsolete."
        ::= { virtualVolumeTitanQuotaEntry 4 }

    virtualVolumeTitanQuotaFileCount OBJECT-TYPE
        SYNTAX Unsigned32 (1..4294967295)
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "The usage of this quota, in number of files.

            This object is now obsolete."
        ::= { virtualVolumeTitanQuotaEntry 5 }

    virtualVolumeTitanQuotaUsageLimit OBJECT-TYPE
        SYNTAX Unsigned32 (1..4294967295)
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "The upper limit set for usage of space in this quota.

            This object is now obsolete."
        ::= { virtualVolumeTitanQuotaEntry 6 }

    virtualVolumeTitanQuotaUsageHardLimit OBJECT-TYPE
        SYNTAX INTEGER {
            soft(0),
            hard(1),
            unknown(3)
        }
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "Whether the upper limit for usage on this quota is a hard limit.

             soft(1)    - not a hard limit.
             hard(2)    - a hard limit.

            This object is now obsolete."
        ::= { virtualVolumeTitanQuotaEntry 7 }

    virtualVolumeTitanQuotaUsageReset OBJECT-TYPE
        SYNTAX Unsigned32 (1..4294967295)
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "When a warning limit for usage has been reached, a warning event
            will not be logged again until the use of the quota has first
            dropped back to the reset value.

            This object is now obsolete."
        ::= { virtualVolumeTitanQuotaEntry 8 }

    virtualVolumeTitanQuotaUsageWarning OBJECT-TYPE
        SYNTAX Unsigned32 (1..4294967295)
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "A warning event will be logged when the usage of a quota has
            reached this value.

            This object is now obsolete."
        ::= { virtualVolumeTitanQuotaEntry 9 }

    virtualVolumeTitanQuotaUsageCritical OBJECT-TYPE
        SYNTAX Unsigned32 (1..4294967295)
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "A severe event will be logged when the usage of a quota has
            reached this value.

            This object is now obsolete."
        ::= { virtualVolumeTitanQuotaEntry 10 }

    virtualVolumeTitanQuotaFileCountLimit OBJECT-TYPE
        SYNTAX Unsigned32 (1..4294967295)
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "The upper limit set for the number of files in a quota.

            This object is now obsolete."
        ::= { virtualVolumeTitanQuotaEntry 11 }

    virtualVolumeTitanQuotaFileCountHardLimit OBJECT-TYPE
        SYNTAX INTEGER {
            soft(0),
            hard(1),
            unknown(2)
        }
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "Whether the upper limit for number of files in a quota
            is a hard limit.

             soft(1)    - not a hard limit.
             hard(2)    - a hard limit.

            This object is now obsolete."
        ::= { virtualVolumeTitanQuotaEntry 12 }

    virtualVolumeTitanQuotaFileCountReset OBJECT-TYPE
        SYNTAX Unsigned32 (1..4294967295)
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "When a warning limit for number of files has been reached,
            a warning event will not be logged again until the file count
            in a quota has first dropped back to the reset value.

