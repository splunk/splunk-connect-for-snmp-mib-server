--------------------------------------------------------------------------------
--
-- StorageTitan / NVRAM
--
--------------------------------------------------------------------------------

    nvramSize OBJECT-TYPE
        SYNTAX Unsigned32 (1..2147483647)
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "The total size of nvram. If no formatted or mounted volume
            is found, or if nvram is not supported, this value will be reported
            as zero.

            This object is obsolete, and is re-implemented in nvramPoolTable."
        ::= { nvram 1 }

    nvramMaximumUsed OBJECT-TYPE
        SYNTAX Unsigned32 (1..2147483647)
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "The maximum amount of nvram used. If no formatted or mounted volume
            is found, or if nvram is not supported, this value will be reported
            as zero.

            This object is obsolete, and is re-implemented in nvramPoolTable."
        ::= { nvram 2 }

    nvramTotalCurrentUsage OBJECT-TYPE
        SYNTAX Unsigned32 (1..2147483647)
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "The amount of nvram currently in use. If no formatted or mounted
            volume is found, or if nvram is not supported, this value will be
            reported as zero.

            This object is obsolete, and is re-implemented in nvramPoolTable."
        ::= { nvram 3 }

    nvramNumber OBJECT-TYPE
        SYNTAX Unsigned32 (1..2147483647)
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "The number of volumes with NVRAM statistics."
        ::= { nvram 4 }

    nvramTable OBJECT-TYPE
        SYNTAX SEQUENCE OF NvramEntry
        MAX-ACCESS not-accessible
        STATUS obsolete
        DESCRIPTION
            "A table of NVRAM data for a given volume."
        ::= { nvram 5 }

    nvramEntry OBJECT-TYPE
        SYNTAX NvramEntry
        MAX-ACCESS not-accessible
        STATUS obsolete
        DESCRIPTION
            "Information about NVRAM on a given volume. The
            volume must be formatted, mounted, and NVRAM must
            be supported."
        INDEX { spanId }
        ::= { nvramTable 1 }

    NvramEntry ::=
        SEQUENCE {
            spanId
                Unsigned32,
            nvramCurrentUsage
                Unsigned32,
            nvramNumberCheckpoints
                Unsigned32,
            nvramNumberActivityCheckpoints
                Unsigned32
        }

    spanId OBJECT-TYPE
        SYNTAX Unsigned32
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "The id of the Span hosting the volume with which the
            NVRAM is associated."
        ::= { nvramEntry 1 }

    nvramCurrentUsage OBJECT-TYPE
        SYNTAX Unsigned32
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "The amount of NVRAM currently in use for this volume."
        ::= { nvramEntry 2 }

   nvramNumberCheckpoints OBJECT-TYPE
        SYNTAX Unsigned32
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "The total number of checkpoints which have occurred for the
            NVRAM assigned to this volume."
        ::= { nvramEntry 3 }

   nvramNumberActivityCheckpoints OBJECT-TYPE
        SYNTAX Unsigned32
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "The number of checkpoints which have occurred for the NVRAM
            assigned to this volume as a result of file system activity
            (i.e. not timed checkpoints."
        ::= { nvramEntry 4 } 
        
   nvramPoolTable OBJECT-TYPE
        SYNTAX SEQUENCE OF NvramPoolEntry
        MAX-ACCESS not-accessible
        STATUS obsolete
        DESCRIPTION
            "A table of NVRAM data for a given Cluster Node."
        ::= { nvram 6 }

    nvramPoolEntry OBJECT-TYPE
        SYNTAX NvramPoolEntry
        MAX-ACCESS not-accessible
        STATUS obsolete
        DESCRIPTION
            "Information about NVRAM on a given Cluster Node."
        INDEX { clusterNodeId }
        ::= { nvramPoolTable 1 }

    NvramPoolEntry ::=
        SEQUENCE {
            clusterNodeId
                Unsigned32,
            nvramPoolSize
                Unsigned32,
            nvramPoolMaximumUsed
                Unsigned32,
            nvramPoolTotalCurrentUsage
                Unsigned32
        }

    clusterNodeId OBJECT-TYPE
        SYNTAX Unsigned32 (0..255)
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "The ID of the cluster node for which these pool
            statistics are reported."
        ::= { nvramPoolEntry 1 }

    nvramPoolSize OBJECT-TYPE
        SYNTAX Unsigned32 (1..2147483647)
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "The total size of nvram. If no formatted or mounted volume
            is found, or if nvram is not supported, this value will be reported
            as zero."
        ::= { nvramPoolEntry 2 }

    nvramPoolMaximumUsed OBJECT-TYPE
        SYNTAX Unsigned32 (1..2147483647)
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "The maximum amount of nvram used. If no formatted or mounted volume
            is found, or if nvram is not supported, this value will be reported
            as zero."
        ::= { nvramPoolEntry 3 }

    nvramPoolTotalCurrentUsage OBJECT-TYPE
        SYNTAX Unsigned32 (1..2147483647)
        MAX-ACCESS read-only
        STATUS obsolete
        DESCRIPTION
            "The amount of nvram currently in use. If no formatted or mounted
            volume is found, or if nvram is not supported, this value will be
            reported as zero."
        ::= { nvramPoolEntry 4 }

