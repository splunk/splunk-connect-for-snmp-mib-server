    xioHubSdServerInfoTable OBJECT-TYPE
                   SYNTAX  SEQUENCE OF XioHubSdServerInfoEntry
                   ACCESS  not-accessible
                   STATUS  mandatory
                   DESCRIPTION
                              "The Server status info."
                   ::= { xioHubStatusData 5 }

            --  *************************************************************
            --  xioHubSdServerInfoTable objects 
            --  *************************************************************

           xioHubSdServerInfoEntry OBJECT-TYPE
                           SYNTAX  XioHubSdServerInfoEntry
                           ACCESS  not-accessible
                           STATUS  mandatory
                           DESCRIPTION
                                      "Server status."
                           INDEX   { xioHubSdServerId }
                           ::= { xioHubSdServerInfoTable 1 }
        
                           XioHubSdServerInfoEntry ::=
                               SEQUENCE {
                                    xioHubSdServerId
                                           INTEGER,
                                    xioHubSdServerStates
                                           INTEGER
                                  }

            xioHubSdServerId  OBJECT-TYPE
                           SYNTAX  INTEGER  
                           ACCESS  read-only
                           STATUS  mandatory
                           DESCRIPTION
                                   "Value indicating specific Server."
                           ::= { xioHubSdServerInfoEntry 1 } 

            xioHubSdServerStates  OBJECT-TYPE
                   SYNTAX  INTEGER {   offline(0),
                                       standby(1),
                                       post0(2),
                                       post1(3),
                                       runningdiagnostics(4),
                                       operational(5),
                                       failed(6),
                                       waitingforfibreconnect(7),
                                       synchronizing(8),
                                       notconnected(9),
                                       unknown(15),
                                       goneoffline(65535)
                                       }
                           ACCESS  read-only
                           STATUS  mandatory
                           DESCRIPTION
                                   "State of the Server containing a Hab."
                           ::= { xioHubSdServerInfoEntry 2 }  



