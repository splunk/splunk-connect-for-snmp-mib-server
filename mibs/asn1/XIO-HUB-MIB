--  **********************************************************************
--  **********************************************************************
--
--  Status Data Group (3)
--
--      Contains Information and Status of HUB components.
--
--      Object Name  (ID) :  'xioHubStatusData'  (3)
--
--      Group designation : 'Sd'
--
--  **********************************************************************
--  **********************************************************************

    xioHubSdEmbedPcState   OBJECT-TYPE
                   SYNTAX  INTEGER {   offline(0),
                                       standby(1),
                                       post0(2),
                                       post1(3),
                                       runningdiagnostics(4),
                                       operational(5),
                                       failed(6),
                                       waitingforfibreconnect(7),
                                       synchronizing(8),
                                       notconnected(9),
                                       unknown(15),
                                       goneoffline(65535)
                                       }
                  ACCESS  read-only
                   STATUS  mandatory
                   DESCRIPTION
                           "Embedded PC State." 
                   ::= { xioHubStatusData 1 }      

    xioHubSdControllerState   OBJECT-TYPE
                   SYNTAX  INTEGER {   offline(0),
                                       standby(1),
                                       post0(2),
                                       post1(3),
                                       runningdiagnostics(4),
                                       operational(5),
                                       failed(6),
                                       waitingforfibreconnect(7),
                                       synchronizing(8),
                                       notconnected(9),
                                       unknown(15),
                                       goneoffline(65535)
                                       }
                   ACCESS  read-only
                   STATUS  mandatory
                   DESCRIPTION
                           "Controller State." 
                   ::= { xioHubStatusData 2 }

    xioHubSdCacheState   OBJECT-TYPE
                   SYNTAX  INTEGER
                   ACCESS  read-only
                   STATUS  mandatory
                   DESCRIPTION
                           "Write Cache State." 
                   ::= { xioHubStatusData 3 }

    xioHubSdHabStatusTable OBJECT-TYPE
                   SYNTAX  SEQUENCE OF XioHubSdHabStatusEntry
                   ACCESS  not-accessible
                   STATUS  mandatory
                   DESCRIPTION
                              "The Hab board status."
                   ::= { xioHubStatusData 4 }

            --  *************************************************************
            --  xioHubSdHabStatusTable objects 
            --  *************************************************************

           xioHubSdHabStatusEntry OBJECT-TYPE
                           SYNTAX  XioHubSdHabStatusEntry
                           ACCESS  not-accessible
                           STATUS  mandatory
                           DESCRIPTION
                                      "Hab status."
                           INDEX   { xioHubSdHabId }
                           ::= { xioHubSdHabStatusTable 1 }
        
                           XioHubSdHabStatusEntry ::=
                               SEQUENCE {
                                    xioHubSdHabId
                                           INTEGER,
                                    xioHubSdHabInstalled
                                           INTEGER,
                                    xioHubSdHubHabState
                                           INTEGER,
                                    xioHubSdServerHabState
                                           INTEGER
                                  }

            xioHubSdHabId  OBJECT-TYPE
                           SYNTAX  INTEGER  
                           ACCESS  read-only
                           STATUS  mandatory
                           DESCRIPTION
                                   "Value indicating specific Hab."
                           ::= { xioHubSdHabStatusEntry 1 } 

            xioHubSdHabInstalled  OBJECT-TYPE
                           SYNTAX  INTEGER  
                           ACCESS  read-only
                           STATUS  mandatory
                           DESCRIPTION
                                   "Flag indicating that Hab is installed."
                           ::= { xioHubSdHabStatusEntry 2 }      

            xioHubSdHubHabState  OBJECT-TYPE
                   SYNTAX  INTEGER {   offline(0),
                                       standby(1),
                                       post0(2),
                                       post1(3),
                                       runningdiagnostics(4),
                                       operational(5),
                                       failed(6),
                                       waitingforfibreconnect(7),
                                       synchronizing(8),
                                       notconnected(9),
                                       unknown(15),
                                       goneoffline(65535)
                                       }
                           ACCESS  read-only
                           STATUS  mandatory
                           DESCRIPTION
                                   "State of Hab within the Hub."
                           ::= { xioHubSdHabStatusEntry 3 }      

            xioHubSdServerHabState  OBJECT-TYPE
                   SYNTAX  INTEGER {   offline(0),
