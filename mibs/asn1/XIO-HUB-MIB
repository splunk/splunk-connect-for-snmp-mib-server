            --  xioHubHiHabInfoTable objects 
            --  *************************************************************

           xioHubSvInfoEntry OBJECT-TYPE
                           SYNTAX  XioHubSvInfoEntry
                           ACCESS  not-accessible
                           STATUS  mandatory
                           DESCRIPTION  "Info on a Server."
                           INDEX   { xioHubSvId }
                           ::= { xioHubSvInfoTable 1 }
        
                           XioHubSvInfoEntry ::=
                               SEQUENCE {
                                    xioHubSvClusterId
                                           INTEGER,
                                    xioHubSvFCalId
                                           INTEGER,
                                    xioHubSvVDiskId
                                           INTEGER,
                                    xioHubSvVDiskLUN
                                           INTEGER,
                                    xioHubSvId
                                           INTEGER,
                                    xioHubSvName
                                           DisplayString
                                  }

            xioHubSvClusterId   OBJECT-TYPE
                         SYNTAX  INTEGER  
                         ACCESS  read-only
                         STATUS  mandatory
                         DESCRIPTION "Cluster index number."
                         ::= { xioHubSvInfoEntry 1 } 

            xioHubSvFCalId   OBJECT-TYPE
                         SYNTAX  INTEGER  
                         ACCESS  read-only
                         STATUS  mandatory
                         DESCRIPTION "Fcal id."
                         ::= { xioHubSvInfoEntry 2 } 

            xioHubSvVDiskId   OBJECT-TYPE
                         SYNTAX  INTEGER  
                         ACCESS  read-only
                         STATUS  mandatory
                         DESCRIPTION "VDisk id."
                         ::= { xioHubSvInfoEntry 3 } 

            xioHubSvVDiskLUN  OBJECT-TYPE
                         SYNTAX  INTEGER
                         ACCESS  read-only
                         STATUS  mandatory
                         DESCRIPTION "LUN value for this vdisk."
                         ::= { xioHubSvInfoEntry 4 } 

            xioHubSvId   OBJECT-TYPE
                         SYNTAX  INTEGER  
                         ACCESS  read-only
                         STATUS  mandatory
                         DESCRIPTION "Server index number."
                         ::= { xioHubSvInfoEntry 5 } 

            xioHubSvName  OBJECT-TYPE
                         SYNTAX  DisplayString  (SIZE (0..8))
                         ACCESS  read-only
                         STATUS  mandatory
                         DESCRIPTION "Server Name."
                         ::= { xioHubSvInfoEntry 6 } 


END
