XIO-HUB-MIB DEFINITIONS  ::= BEGIN

IMPORTS
        enterprises, Counter
                FROM RFC1155-SMI
        OBJECT-TYPE
                FROM RFC-1212
        DisplayString
                FROM RFC1213-MIB
        TRAP-TYPE
                FROM RFC-1215;

            -- textual conventions
            --
            --    DisplayString ::=
            --              OCTET STRING
            -- This data type is used to model textual information taken
            -- from the NVT ASCII character set.  By convention, objects
            -- with this syntax are declared as having
            --
            --              SIZE (0..255)


--  **********************************************************************
--
--  Xiotech MIB Objects
--
--      Xiotech enterprise                   1.3.6.1.4.1.2366
--          xiotech HUB MIB                  1.3.6.1.4.1.2366.1
--              Hub Identification Group     1.3.6.1.4.1.2366.1.1  
--              Environmental Data Group     1.3.6.1.4.1.2366.1.2  
--              Status Data Group            1.3.6.1.4.1.2366.1.3  
--              Physical Devices Group       1.3.6.1.4.1.2366.1.4
--              RAID Devices Group           1.3.6.1.4.1.2366.1.5
--              Virtual Devices Group        1.3.6.1.4.1.2366.1.6
--              Cache Information Group      1.3.6.1.4.1.2366.1.7
--              HAB Information Group        1.3.6.1.4.1.2366.1.8
--              Bus Information Group        1.3.6.1.4.1.2366.1.9
--              Trap Variables Group         1.3.6.1.4.1.2366.1.10
--              Traps Group                  1.3.6.1.4.1.2366.1.20 
--              CPU Loads Information Group  1.3.6.1.4.1.2366.1.30
--              Storage Device List Group    1.3.6.1.4.1.2366.1.40
--              VDisk Perf. Stats Group      1.3.6.1.4.1.2366.1.50 
--              PDisk Perf. Stats Group      1.3.6.1.4.1.2366.1.60 
--              Server Info Group            1.3.6.1.4.1.2366.1.70 
--
--  Object Naming Conventions
--
--      Group Naming Conventions
--
--           cccmmmnnnnnnnnnnnn
--
--          where : ccc      -  3 character company symbol (i.e. 'xio')
--                  mmm      -  3 character MIB identifier ( i.e. 'Hub')
--                  nn...nn  -  1 - 15 character group name
--  
--      Variable Naming Conventions
--
--           cccmmmggg[tt]nnnnnnnnnnnn
--
--          where : ccc      -  3 character company symbol (i.e. 'xio')
--                  mmm      -  3 character MIB identifier ( i.e. 'Hub')
--                  gg       -  2character group designation ( i.e. 'Ev')
--                  nn...nn  -  1 - 13 character variable name
--  
--      Trap Naming Conventions
--
--           cccmmmTRPnnnnnnnnnnnn
--
--          where : ccc      -  3 character company symbol (i.e. 'xio')
--                  mmm      -  3 character MIB identifier ( i.e. 'Hub')
--                  TRP      -  3 characters identifying a TRAP (required)
--                  nn...nn  -  1 - 15 character trap name
--  
--  **********************************************************************

xiotech				OBJECT IDENTIFIER ::= { enterprises 2366 }

--
--      Xiotech HUB  'xioHub' MIB
--
xioHub               OBJECT IDENTIFIER ::= { xiotech 1 }

--
--      HUB MIB Groups        1.3.6.1.4.1.2366.1.x  (x = 1 - 20)
--
xioHubIdentification OBJECT IDENTIFIER ::= { xioHub  1 }
xioHubEnvironment    OBJECT IDENTIFIER ::= { xioHub  2 }
xioHubStatusData     OBJECT IDENTIFIER ::= { xioHub  3 }
xioHubPhysicalDevice OBJECT IDENTIFIER ::= { xioHub  4 }
xioHubRaidDevice     OBJECT IDENTIFIER ::= { xioHub  5 }
xioHubVirtualDevice  OBJECT IDENTIFIER ::= { xioHub  6 }
xioHubCacheInfo      OBJECT IDENTIFIER ::= { xioHub  7 }
xioHubHabInfo        OBJECT IDENTIFIER ::= { xioHub  8 }
xioHubBusInfo        OBJECT IDENTIFIER ::= { xioHub  9 }
xioHubTrapVariables  OBJECT IDENTIFIER ::= { xioHub  10 }
xioHubTraps          OBJECT IDENTIFIER ::= { xioHub  20 }
xioHubLoadInfo       OBJECT IDENTIFIER ::= { xioHub  30 }
xioHubStorageDevList OBJECT IDENTIFIER ::= { xioHub  40 }
xioHubVpInfo         OBJECT IDENTIFIER ::= { xioHub  50 }
xioHubPpInfo         OBJECT IDENTIFIER ::= { xioHub  60 }
xioHubSvInfo         OBJECT IDENTIFIER ::= { xioHub  70 }


--  **********************************************************************
--  **********************************************************************
--
--  Hub Identification Group (1)
--
--      Contains Storage Hub name.
--
--      Object Name  (ID) :  'xioHubIdentification'  (1)
--
--      Group designation : 'Hu'
--
--  **********************************************************************
--  **********************************************************************

    xioHubHuName   OBJECT-TYPE
                   SYNTAX  DisplayString  (SIZE (0..64))
                   ACCESS  read-write
                   STATUS  mandatory
                   DESCRIPTION
                           "Storage Hub Name." 
                   ::= { xioHubIdentification 1 }      

    xioHubHuSerialNum OBJECT-TYPE
                   SYNTAX  INTEGER
                   ACCESS  read-only
                   STATUS  mandatory
                   DESCRIPTION
                           "Storage Hub Serial Number." 
                   ::= { xioHubIdentification 2 }      

    xioHubHuCompat OBJECT-TYPE
                   SYNTAX  INTEGER
                   ACCESS  read-only
                   STATUS  mandatory
                   DESCRIPTION
                           "Compatibility level of Storage Hub." 
                   ::= { xioHubIdentification 3 }      

    xioHubHuTCPIPAddr   OBJECT-TYPE
                   SYNTAX  DisplayString  (SIZE (0..64))
                   ACCESS  read-write
                   STATUS  mandatory
                   DESCRIPTION
                           "Storage Hub TCPIP address." 
                   ::= { xioHubIdentification 4 }      

    xioHubHuHeartbeat  OBJECT-TYPE
                       SYNTAX  INTEGER
                       ACCESS  read-only
                       STATUS  mandatory
                       DESCRIPTION
                           "Storage Hub Heartbeat." 
                      ::= { xioHubIdentification 5 }      

--  **********************************************************************
--  **********************************************************************
--
--  Environmental Data Group (2)
--
--      Contains status and temperature for HUB components.
--
--      Object Name  (ID) :  'xioHubEnvironment'  (2)
--
--      Group designation : 'Ev'
--
--  **********************************************************************
--  **********************************************************************

    xioHubEvUpsAc   OBJECT-TYPE
                   SYNTAX  INTEGER
                   ACCESS  read-only
                   STATUS  mandatory
                   DESCRIPTION
                           "Indicates that the UPS AC input is good." 
                   ::= { xioHubEnvironment 1 }      

    xioHubEvUpsBattLow   OBJECT-TYPE
                   SYNTAX  INTEGER 
                   ACCESS  read-only
                   STATUS  mandatory
                   DESCRIPTION
                           "UPS Battery is good." 
                   ::= { xioHubEnvironment 2 }      

    xioHubEvMainPower   OBJECT-TYPE
                   SYNTAX  INTEGER
                   ACCESS  read-only
                   STATUS  mandatory
                   DESCRIPTION
                           "Main DC Power supply is good." 
                   ::= { xioHubEnvironment 3 }      

    xioHubEvNumSupplies  OBJECT-TYPE
                   SYNTAX  INTEGER
                   ACCESS  read-only
                   STATUS  mandatory
                   DESCRIPTION
                           "Number of DC Power Supplies." 
                   ::= { xioHubEnvironment 4 }      

    xioHubEvNumFrontFans  OBJECT-TYPE
                   SYNTAX  INTEGER
                   ACCESS  read-only
                   STATUS  mandatory
                   DESCRIPTION
                           "Number of Front Fans." 
                   ::= { xioHubEnvironment 5 }      

    xioHubEvNumRearFans  OBJECT-TYPE
                   SYNTAX  INTEGER
                   ACCESS  read-only
                   STATUS  mandatory
                   DESCRIPTION
                           "Number of Rear Fans." 
                   ::= { xioHubEnvironment 6 }      

    xioHubEvMainDcPowerTable OBJECT-TYPE
                   SYNTAX  SEQUENCE OF XioHubEvMainDcPowerEntry
                   ACCESS  not-accessible
                   STATUS  mandatory
                   DESCRIPTION
                              "The status of Main DC Power Supplies."
                   ::= { xioHubEnvironment 7 }

            --  *************************************************************
            --  xioHubEvMainDcPowerTable objects 
            --  *************************************************************

           xioHubEvMainDcPowerEntry OBJECT-TYPE
                           SYNTAX  XioHubEvMainDcPowerEntry
                           ACCESS  not-accessible
                           STATUS  mandatory
                           DESCRIPTION
                                      "Main DC Power Supply status."
                           INDEX   { xioHubEvDcId }
                           ::= { xioHubEvMainDcPowerTable 1 }
        
                           XioHubEvMainDcPowerEntry ::=
                               SEQUENCE {
                                    xioHubEvDcId
                                           INTEGER,
                                    xioHubEvDcExists
                                           INTEGER,
                                    xioHubEvDcGood
                                           INTEGER
                                  }

            xioHubEvDcId   OBJECT-TYPE
                           SYNTAX  INTEGER  (0..12)
                           ACCESS  read-only
                           STATUS  mandatory
                           DESCRIPTION
                                   "Value indicating a specific DC Power Supply."
                           ::= { xioHubEvMainDcPowerEntry 1 }      

            xioHubEvDcExists   OBJECT-TYPE
                           SYNTAX  INTEGER  (0..12)
                           ACCESS  read-only
                           STATUS  mandatory
                           DESCRIPTION
                                   "Flag indicating that DC Power Supply exists."
                           ::= { xioHubEvMainDcPowerEntry 2 }      

            xioHubEvDcGood    OBJECT-TYPE
                           SYNTAX  INTEGER (0..12)
                           ACCESS  read-only
                           STATUS  mandatory
                           DESCRIPTION
                                   "Flag indicating status of DC Power Supply"
                           ::= { xioHubEvMainDcPowerEntry 3 }      


    xioHubEvFrontFansTable OBJECT-TYPE
                   SYNTAX  SEQUENCE OF XioHubEvFrontFansEntry
                   ACCESS  not-accessible
                   STATUS  mandatory
                   DESCRIPTION
                              "The status of Front fans."
                   ::= { xioHubEnvironment 8 }

            --  *************************************************************
            --  xioHubEvFrontFansTable objects 
            --  *************************************************************

           xioHubEvFrontFansEntry OBJECT-TYPE
                           SYNTAX  XioHubEvFrontFansEntry
                           ACCESS  not-accessible
                           STATUS  mandatory
                           DESCRIPTION
                                      "Front Fans status."
                           INDEX   { xioHubEvFrontFansId }
                           ::= { xioHubEvFrontFansTable 1 }
        
                           XioHubEvFrontFansEntry ::=
                               SEQUENCE {
                                    xioHubEvFrontFansId
                                           INTEGER,
                                    xioHubEvFrontFansGood
                                           INTEGER
                                  }

            xioHubEvFrontFansId  OBJECT-TYPE
