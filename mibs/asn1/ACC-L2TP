    accL2tpUdpConnectionEndpointIpAddress    OBJECT-TYPE
        SYNTAX  IpAddress
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION
             "Tunnel Remote Endpoint Ip Address"
	::= { accL2tpUdpConnectionEntry 2 }
    accL2tpUdpConnectionRxPort      OBJECT-TYPE
        SYNTAX  INTEGER	(0..65535)
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION
             "Receive Port"
	::= { accL2tpUdpConnectionEntry 3 }
    accL2tpUdpConnectionTxPort      OBJECT-TYPE
        SYNTAX  INTEGER	(0..65535)
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION
             "Transmit Port"
	::= { accL2tpUdpConnectionEntry 4 }
    accL2tpUdpConnectionAddressChanges  OBJECT-TYPE
        SYNTAX  Counter
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION
             "Number of times User changed his Ip Address"
	::= { accL2tpUdpConnectionEntry 5 }
-- START CFG_TRAP_ENHANCEMENTS
-- L2tp Traps Branch Enterprise
      accL2tpTraps OBJECT IDENTIFIER    ::= { accL2tpTransportUdp 4 }
        accL2tpTrapMsg     OBJECT-TYPE
        SYNTAX          DisplayString
        ACCESS          read-only
        STATUS          mandatory
        DESCRIPTION
                  " Enterprise Trap Message for L2tp"
        ::= { accL2tpTraps 1 }
-- END CFG_TRAP_ENHANCEMENTS 

        accL2tpTunnelDelTrap TRAP-TYPE
        ENTERPRISE   accL2tpTraps 
        VARIABLES  { accL2tpTrapMsg,
                     accL2tpSessionLocalCallId ,
                      accTrapLogSeqNum}
        DESCRIPTION
                "When L2TP resources associated with a tunnel port are
                released and removing any references, if any sessions
                are left., delete action fails. This trap message
                carries this information.
SEVERITY-DEFAULT: Minor 
EVENTTYPE-DEFAULT: 10 
ADDITIONAL-TEXT-VARIABLES: { accTrapLogSeqNum,accL2tpTrapMsg }
PCAUSE-DEFAULT: 46 " 
 
        ::= 1
        accL2tpTrap2 TRAP-TYPE
        ENTERPRISE   accL2tpTraps 
        VARIABLES  { accL2tpTrapMsg,
                     accL2tpUdpTransportTunnelIfIndex ,
                      accTrapLogSeqNum}
        DESCRIPTION
                "While searching the UDP endpoint database to see if
                the EP exists., transport entry can exist without l2tp
                tunnel ports and so tries to create one and generates
                this trap if this creation fails.
SEVERITY-DEFAULT: Minor 
EVENTTYPE-DEFAULT: 10 
ADDITIONAL-TEXT-VARIABLES: { accTrapLogSeqNum,accL2tpTrapMsg }
PCAUSE-DEFAULT: 46 " 
 
        ::= 2
        accL2tpUdpEpTrap TRAP-TYPE
        ENTERPRISE   accL2tpTraps 
        VARIABLES  { accL2tpTrapMsg ,
                      accTrapLogSeqNum}
        DESCRIPTION
                "In the ad hoc L2tp testing, all the tunnels are created
                and brought up. In the process if the L25 UDP end point
                is not found, then this trap is generated. 
SEVERITY-DEFAULT: Minor 
EVENTTYPE-DEFAULT: 10 
ADDITIONAL-TEXT-VARIABLES: { accTrapLogSeqNum,accL2tpTrapMsg }
PCAUSE-DEFAULT: 46 " 
 
        ::= 3
