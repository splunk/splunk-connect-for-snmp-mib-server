-- This table provides aggragete summary information of the performance of the 
-- dictionary cache.  Each user and data request that connects to the database
-- references the data dictionary. The data dictionary cache is an area of the
-- shared pool that contains data dictionary information in memory. These 
-- values are internal but you can influence the amount of resources they get
-- by increasing the value in init.ora for the parameter sharedpoolsize. 
-- The object is to keep as much of the data dictionary information in memory
-- as possible. This is a sumation of each relevent column in v$rowcache for
-- every row.

-- Data Dictionary Aggregate information
      --oramodSgaDddAgg OBJECT IDENTIFIER ::= { oramodSGA 3 }

        oramodSgaDddAggTable OBJECT-TYPE
                SYNTAX SEQUENCE OF OramodSgaDddAggEntry
                ACCESS not-accessible
                STATUS mandatory
                DESCRIPTION
                   "This table describes the data dictionary aggregates."
                ::= { oramodSgaDddAgg 1 }

        oramodSgaDddAggEntry OBJECT-TYPE
                SYNTAX OramodSgaDddAggEntry
                ACCESS not-accessible
                STATUS mandatory
                DESCRIPTION
                   "An entry in the Data Dictionary Aggregate table."
                INDEX { oramodSGAAGGSIDINDX }
                ::= {  oramodSgaDddAggTable 1 }

        OramodSgaDddAggEntry ::= SEQUENCE {
                oramodSGAAGGSIDINDX
                        INTEGER,
                oramodSGAAGGCNT
                        Counter,
                oramodSGAAGGUSGE
                        Counter,
                oramodSGAAGGFIX
                        Counter,
                oramodSGAAGGGET
                        Counter,
                oramodSGAAGGGETMISS
                        Counter,
                oramodSGAAGGSCAN
                        Counter,
                oramodSGAAGGSCANMISS
                        Counter,
                oramodSGAAGGSCANCPLT
                        Counter,
                oramodSGAAGGMODS
                        Counter,
                oramodSGAAGGFLUSH
                        Counter
        }

        oramodSGAAGGSIDINDX OBJECT-TYPE
                SYNTAX INTEGER (1..2147483647)
                ACCESS read-only
                STATUS mandatory
                DESCRIPTION
		   "Sid Index Number."
                ::= { oramodSgaDddAggEntry 1 }

        oramodSGAAGGCNT OBJECT-TYPE
                SYNTAX Counter
                ACCESS read-only
                STATUS mandatory
                DESCRIPTION
		   "Total number of entries in the data dictionary cache
		since instance startup."
                ::= { oramodSgaDddAggEntry 2 }

        oramodSGAAGGUSGE OBJECT-TYPE
                SYNTAX Counter
                ACCESS read-only
                STATUS mandatory
                DESCRIPTION
		   "The total number of cache entries with valid data since
		instance startup."
                ::= { oramodSgaDddAggEntry 3 }

        oramodSGAAGGFIX OBJECT-TYPE
                SYNTAX Counter
                ACCESS read-only
                STATUS mandatory
                DESCRIPTION
		   "The total number of fixed entries in the data dictionary
		cache since instance startup."
                ::= { oramodSgaDddAggEntry 4 }

        oramodSGAAGGGET OBJECT-TYPE
                SYNTAX Counter
                ACCESS read-only
                STATUS mandatory
                DESCRIPTION
		   "The total number of information requests since instance
		startup." 
                ::= { oramodSgaDddAggEntry 5 }

        oramodSGAAGGGETMISS OBJECT-TYPE
                SYNTAX Counter
                ACCESS read-only
                STATUS mandatory
                DESCRIPTION
		   "The total number of informations requests resulting in 
		cache misses since instance startup." 
                ::= { oramodSgaDddAggEntry 6 }

        oramodSGAAGGSCAN OBJECT-TYPE
                SYNTAX Counter
                ACCESS read-only
                STATUS mandatory
                DESCRIPTION
		   "The total number of scan requests since instance startup." 
                ::= { oramodSgaDddAggEntry 7 }

        oramodSGAAGGSCANMISS OBJECT-TYPE
                SYNTAX Counter
                ACCESS read-only
                STATUS mandatory
                DESCRIPTION
		   "The total number of scan requests resulting in misses
		since instance startup." 
                ::= { oramodSgaDddAggEntry 8 }

        oramodSGAAGGSCANCPLT OBJECT-TYPE
                SYNTAX Counter
                ACCESS read-only
                STATUS mandatory
                DESCRIPTION
		  "For subordinate entries, the total number of times 
		list was scanned completely since instance startup."
                ::= { oramodSgaDddAggEntry 9 }

        oramodSGAAGGMODS OBJECT-TYPE
                SYNTAX Counter
                ACCESS read-only
                STATUS mandatory
                DESCRIPTION
		  "The total number of inserts, updates, and deletions
		since instance startup."
                ::= { oramodSgaDddAggEntry 10 }

        oramodSGAAGGFLUSH OBJECT-TYPE
                SYNTAX Counter
                ACCESS read-only
                STATUS mandatory
                DESCRIPTION
		  "The total number of times flushed to disk since instance
		startup."
                ::= { oramodSgaDddAggEntry 11 }

-- This table provides information about SQL and PL/SQL statements that 
-- are stored in the library cache. Oracle stores the execution plan of SQL 
-- statements so that less processing is necessary for subsequent executions.
-- Information is pulled from the view v$librarycache. The library cache is 
-- the most important area of the shared pool to tune. A least recently used
-- algorithm is used to manage the cache. Goals are to hold the 
-- majority of SQL statements and to reduce the number of statements that 
-- have to be reparsed.  Continuous aging and reparsing of statements 
-- degredates perfomance.

      --oramodSgaDlci OBJECT IDENTIFIER ::= { oramodSGA 5 }

        oramodSgaDlciTable OBJECT-TYPE
                SYNTAX SEQUENCE OF OramodSgaDlciEntry
                ACCESS not-accessible
                STATUS mandatory
                DESCRIPTION
                  "This table describes the detailed library cache information."
                ::= { oramodSgaDlci 1 }

        oramodSgaDlciEntry OBJECT-TYPE
                SYNTAX OramodSgaDlciEntry
                ACCESS not-accessible
                STATUS mandatory
                DESCRIPTION
                   "An entry in the Library Cache table."
                INDEX { oramodSGADLCISIDINDX, oramodSGADLCIINDX }
                ::= {  oramodSgaDlciTable 1 }

        OramodSgaDlciEntry ::= SEQUENCE {
      		oramodSGADLCISIDINDX
			INTEGER,
      		oramodSGADLCIINDX
			INTEGER,
	        oramodSGADLCINAME
                        DisplayString,
                oramodSGADLCIGET
                        Counter,
                oramodSGADLCIGETHIT
                        Counter,
                oramodSGADLCIGETHITRT
                        Gauge,
                oramodSGADLCIPIN
                        Counter,
                oramodSGADLCIPINHIT
                        Counter,
                oramodSGADLCIPINHITRT
                        Gauge,
                oramodSGADLCIRELOAD
                        Counter,
                oramodSGADLCIINVALID
                        Counter
        }
        oramodSGADLCISIDINDX OBJECT-TYPE
                SYNTAX INTEGER (1..2147483647)
                ACCESS read-only
                STATUS mandatory
                DESCRIPTION
		   "Sid Index Number."
                ::= { oramodSgaDlciEntry 1 }

        oramodSGADLCIINDX OBJECT-TYPE
                SYNTAX INTEGER (1..2147483647)
                ACCESS read-only
                STATUS mandatory
                DESCRIPTION
		   "Namespace Index Number."
                ::= { oramodSgaDlciEntry 2 }

        oramodSGADLCINAME OBJECT-TYPE
                SYNTAX DisplayString
                ACCESS read-only
                STATUS mandatory
                DESCRIPTION
		   "Library Cache Namespace. (SQL AREA, TABLE/PROCEDURE, BODY, 
		TRIGGER, INDEX, CLUSTER, OBJECT, PIPE)"
                ::= { oramodSgaDlciEntry 3 }

        oramodSGADLCIGET OBJECT-TYPE
                SYNTAX Counter 
                ACCESS read-only
                STATUS mandatory
                DESCRIPTION
		   "The total number of request handles to library namespace
		since instance startup Typical range (0..500,00)." 
                ::= { oramodSgaDlciEntry 4 }

        oramodSGADLCIGETHIT OBJECT-TYPE
                SYNTAX Counter 
                ACCESS read-only
                STATUS mandatory
                DESCRIPTION
		   "The total number of times handles are already allocated
		in the cache. If the handle is not already allocated it is a
		miss." 
                ::= { oramodSgaDlciEntry 5 }

        oramodSGADLCIGETHITRT OBJECT-TYPE
                SYNTAX Gauge
                ACCESS read-only
                STATUS mandatory
                DESCRIPTION
		   "The Get/Hit ratio determines the percentage of parse 
		calls that find a cursor to share. The value is derived from 
		GETHITS divided by GETS. Should be in the high 90s which 
		indicates that most of the handles the system has tried to get
		are cached." 
                ::= { oramodSgaDlciEntry 6 }

        oramodSGADLCIPIN OBJECT-TYPE
                SYNTAX Counter 
                ACCESS read-only
                STATUS mandatory
                DESCRIPTION
		   "The total number of pin requests for objects in the cache
		since instance startup." 
                ::= { oramodSgaDlciEntry 7 }

        oramodSGADLCIPINHIT OBJECT-TYPE
                SYNTAX Counter 
                ACCESS read-only
                STATUS mandatory
                DESCRIPTION
		   "The number of pin hits or executions of objects that 
		are already allocated and initiated in the cache. Otherwise, 
		it is a miss, and the system has to reallocate and initialize
		it with data queried from the database or generate the data." 
                ::= { oramodSgaDlciEntry 8 }

        oramodSGADLCIPINHITRT OBJECT-TYPE
                SYNTAX Gauge
                ACCESS read-only
                STATUS mandatory
                DESCRIPTION
		   "The ratio of PINHITS to PINS. Values close to 100 indicate
		that most of the objects the system has tried to pin and 
		access have been cached." 
                ::= { oramodSgaDlciEntry 9 }

        oramodSGADLCIRELOAD OBJECT-TYPE
                SYNTAX Counter 
                ACCESS read-only
                STATUS mandatory
                DESCRIPTION
		  "Number of times a statement had to be reparsed in order to
		be executed. Statements are reloaded due to aging or 
		invalidation. Should never be more that one percent of PINS.
		 Goal is the have close to zero reloads."
                ::= { oramodSgaDlciEntry 10 }

        oramodSGADLCIINVALID OBJECT-TYPE
                SYNTAX Counter 
                ACCESS read-only
                STATUS mandatory
                DESCRIPTION
		  "The number of times that non-persistant library objects
	 	(like SQL areas) have been invalidated."
                ::= { oramodSgaDlciEntry 11 }

-- Redo Log information from view v$sysstat. Redo Logs are used to perform
-- database recoveries. Oracle stores data that is being changed by 
-- transactions in the redo log buffers. Performance can be improved by 
-- alternating the placement of the redo logs on disk. Since Oracle will 
-- read the prior log to archive it, disk head contention will
-- occur if the logs are on the same disk. By placing them on different disks,
-- one disk can be read while the other is being written to. 

        --oramodRedoLogBuf OBJECT IDENTIFIER ::= { oramodPerf 7 }

        oramodRedoLogBufTable OBJECT-TYPE
                SYNTAX SEQUENCE OF OramodRedoLogBufEntry
                ACCESS not-accessible
                STATUS mandatory
                DESCRIPTION
                  "This table describes the Redo Log information."
                ::= { oramodRedoLogBuf 1 }

        oramodRedoLogBufEntry OBJECT-TYPE
                SYNTAX OramodRedoLogBufEntry
                ACCESS not-accessible
                STATUS mandatory
                DESCRIPTION
                   "An entry in the Redo Log table."
                INDEX { oramodREDOSIDINDX }
                ::= {  oramodRedoLogBufTable 1 }

        OramodRedoLogBufEntry ::= SEQUENCE {
      		oramodREDOSIDINDX
			INTEGER,
		oramodREDOBLKWRT 
			Counter,
		oramodREDOENTRIES 
			Counter,
		oramodREDOSIZE 
			INTEGER,
		oramodREDOSPACERQST 
			Counter,
		oramodREDOSPACEWAIT 
			Counter,
		oramodREDOSYNCHWRT 
			Counter,
		oramodREDOSYNCHTM 
			INTEGER,
		oramodREDOWASTAGE 
			Counter,
		oramodREDORETRIES 
			Counter,
		oramodREDOLASTUPDATE 
                        TimeTicks
	}

        oramodREDOSIDINDX OBJECT-TYPE
                SYNTAX INTEGER (1..2147483647)
                ACCESS read-only
                STATUS mandatory
                DESCRIPTION
		   "Sid Index Number."
                ::= { oramodRedoLogBufEntry 1 }

        oramodREDOBLKWRT OBJECT-TYPE
		SYNTAX Counter
		ACCESS read-only
		STATUS mandatory
		DESCRIPTION
                  "The total number of redo blocks written since instance
		startup."
		::= { oramodRedoLogBufEntry 2 }

        oramodREDOENTRIES OBJECT-TYPE
		SYNTAX Counter
		ACCESS read-only
		STATUS mandatory
		DESCRIPTION
                  "The total number of redo entries in the redo log since
		instance startup."
		::= { oramodRedoLogBufEntry 3 }

        oramodREDOSIZE OBJECT-TYPE
		SYNTAX INTEGER
		ACCESS read-only
		STATUS mandatory
		DESCRIPTION
                  "The size of the redo log."
		::= { oramodRedoLogBufEntry 4 }

        oramodREDOSPACERQST OBJECT-TYPE
		SYNTAX Counter
		ACCESS read-only
		STATUS mandatory
		DESCRIPTION
                  "The number of redo log space requests. Should be close 
		to zero. A nonzero value means the the buffer may be too small. 		Increase the size of the buffer so that processes are not 
		waiting. The INIT.ora parameter LOGBUFFER controls the 
		size of the redo log buffer."
		::= { oramodRedoLogBufEntry 5 }

        oramodREDOSPACEWAIT OBJECT-TYPE
		SYNTAX Counter
		ACCESS read-only
		STATUS mandatory
		DESCRIPTION
