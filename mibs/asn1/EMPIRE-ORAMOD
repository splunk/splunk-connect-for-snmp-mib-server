                INDEX { oramodWaitsSIDINDX, oramodWaitsEVENTINDX }
                ::= {  oramodWaitsTable 1 }

        OramodWaitsEntry ::= SEQUENCE {
          	oramodWaitsSIDINDX
                        INTEGER,
          	oramodWaitsEVENTINDX
                        INTEGER,
	        oramodWaitsEVENT
                        DisplayString,
                oramodWaitsTOTALWAITS
                        Counter,
                oramodWaitsTOTALTIMOUT
                        Counter,
                oramodWaitsTIMEWAITED
                        Counter,
                oramodWaitsAVGWAIT
                        Counter
        }

        oramodWaitsSIDINDX OBJECT-TYPE
                SYNTAX INTEGER (1..2147483647)
                ACCESS read-only
                STATUS mandatory
                DESCRIPTION
		   "Sid Index Number."
                ::= { oramodWaitsEntry 1 }

        oramodWaitsEVENTINDX OBJECT-TYPE
                SYNTAX INTEGER (1..2147483647)
                ACCESS read-only
                STATUS mandatory
                DESCRIPTION
		   "Event's Index Number."
                ::= { oramodWaitsEntry 2 }

        oramodWaitsEVENT OBJECT-TYPE
                SYNTAX DisplayString
                ACCESS read-only
                STATUS mandatory
                DESCRIPTION
		   "Resource Name."
                ::= { oramodWaitsEntry 3 }

        oramodWaitsTOTALWAITS OBJECT-TYPE
                SYNTAX Counter
                ACCESS read-only
                STATUS mandatory
                DESCRIPTION
		   "Resource Total Waits."
                ::= { oramodWaitsEntry 4 }

        oramodWaitsTOTALTIMOUT OBJECT-TYPE
                SYNTAX Counter
                ACCESS read-only
                STATUS mandatory
                DESCRIPTION
		   "Resource Total Timeouts."
                ::= { oramodWaitsEntry 5 }

        oramodWaitsTIMEWAITED OBJECT-TYPE
                SYNTAX Counter
                ACCESS read-only
                STATUS mandatory
                DESCRIPTION
		   "Resource Total Waited."
                ::= { oramodWaitsEntry 6 }

        oramodWaitsAVGWAIT OBJECT-TYPE
                SYNTAX Counter
                ACCESS read-only
                STATUS mandatory
                DESCRIPTION
		   "Resource average wait."
                ::= { oramodWaitsEntry 7}


-- Expensive SQL for each instance. Nothing will reduce the overall performance
-- of a system faster than bad application SQL. Use this table to monitor
-- statements that are running overlong or require many disk reads. 

        oramodExpSqlTable OBJECT-TYPE
                SYNTAX SEQUENCE OF OramodExpSqlEntry
                ACCESS not-accessible
                STATUS mandatory
                DESCRIPTION
                  "This table describes expensive sql."
                ::= { oramodExpSql 1 }

        oramodExpSqlEntry OBJECT-TYPE
                SYNTAX OramodExpSqlEntry
                ACCESS not-accessible
                STATUS mandatory
                DESCRIPTION
                   "An entry in the v$sqlarea table."
                INDEX { oramodExpSqlSIDINDX, oramodExpSqlSQLINDX }
                ::= {  oramodExpSqlTable 1 }

        OramodExpSqlEntry ::= SEQUENCE {
                oramodExpSqlSIDINDX
                        INTEGER,
                oramodExpSqlSQLINDX
                        INTEGER,
                oramodExpSqlEXECUTION
                        Gauge,
                oramodExpSqlDISKREAD
                        Gauge,
                oramodExpSqlPARSECNT
                        Gauge,
                oramodExpSqlBUFFGETS
                        Gauge,
                oramodExpSqlSORTS
                        Gauge,
                oramodExpSqlSQL
                        DisplayString 
        }

        oramodExpSqlSIDINDX OBJECT-TYPE
                SYNTAX INTEGER (1..2147483647)
                ACCESS read-only
                STATUS mandatory
                DESCRIPTION
		   "Sid Index Number."
                ::= { oramodExpSqlEntry 1 }

        oramodExpSqlSQLINDX OBJECT-TYPE
                SYNTAX INTEGER (1..2147483647)
                ACCESS read-only
                STATUS mandatory
                DESCRIPTION
		   "SQL Statement Index Number."
                ::= { oramodExpSqlEntry 2 }

        oramodExpSqlEXECUTION OBJECT-TYPE
                SYNTAX Gauge
                ACCESS read-only
                STATUS mandatory
                DESCRIPTION
		   "Number of Executions."
                ::= { oramodExpSqlEntry 3 }

        oramodExpSqlDISKREAD OBJECT-TYPE
                SYNTAX Gauge
                ACCESS read-only
                STATUS mandatory
                DESCRIPTION
		   "Number of Disk Reads."
                ::= { oramodExpSqlEntry 4 }

        oramodExpSqlPARSECNT OBJECT-TYPE
                SYNTAX Gauge
                ACCESS read-only
                STATUS mandatory
                DESCRIPTION
		   "Number of Parses. Parsing is the work done by the
		database to validate the SQL statement and generate the
		query plan used for execution. When the parse count is 
		close to that of executions for a given statement, you may
		be continually reparsing that statement. You can reduce 
		parsing by making sure that statements are reused as much 
		as possible in the shared pool. In general, a large number
		of hard parses indicates that more memory is needed for 
		the shared pool."
                ::= { oramodExpSqlEntry 5 }

        oramodExpSqlBUFFGETS OBJECT-TYPE
                SYNTAX Gauge
                ACCESS read-only
                STATUS mandatory
                DESCRIPTION
		   "Number of Buffer Gets."
                ::= { oramodExpSqlEntry 6 }

        oramodExpSqlSORTS OBJECT-TYPE
                SYNTAX Gauge
                ACCESS read-only
                STATUS mandatory
                DESCRIPTION
		   "Number of Sorts."
                ::= { oramodExpSqlEntry 7 }

       oramodExpSqlSQL OBJECT-TYPE
                SYNTAX DisplayString 
                ACCESS read-only
                STATUS mandatory
                DESCRIPTION
		   "Sql Statement truncated."
                ::= { oramodExpSqlEntry 8 }


-- Tablespace Information. This table is used to monitor fragmentation 
-- within each database tablespace. For maximum performance there must be 
-- enough large chunks of free block space to fill database requests. This
-- table describes the amount of space left in each tablespace after the 
-- next largest extent of any table in the tablespace is allocated. 
-- Returns a row for each tablespace containing tables.   

        oramodTblspTable OBJECT-TYPE
                SYNTAX SEQUENCE OF OramodTblspEntry
                ACCESS not-accessible
                STATUS mandatory
                DESCRIPTION
                  "List Fragmentation Information."
                ::= { oramodTblsp 1 }

        oramodTblspEntry OBJECT-TYPE
                SYNTAX OramodTblspEntry
                ACCESS not-accessible
                STATUS mandatory
                DESCRIPTION
                   "An entry from the dbafreespace table."
              INDEX { oramodTblspSIDINDX, oramodTblspTBLSPINDX}
                ::= {  oramodTblspTable 1 }

        OramodTblspEntry ::= SEQUENCE {
          	oramodTblspSIDINDX
                        INTEGER,
          	oramodTblspTBLSPINDX
                        INTEGER,
	        oramodTblspTBLSPNAME
                        DisplayString,
	        oramodTblspFILENAME
                        DisplayString,
                oramodTblspEXTENTS
                        Gauge,
                oramodTblspLRGEXTENT
                        Gauge,
                oramodTblspSMEXTENT
                        Gauge,
                oramodTblspINCREMENTBY
                        INTEGER,
                oramodTblspBYTESCOALSD
                        Gauge,
                oramodTblspBYTESFREE
                        Gauge
        }

        oramodTblspSIDINDX OBJECT-TYPE
                SYNTAX INTEGER (1..2147483647)
                ACCESS read-only
                STATUS mandatory
                DESCRIPTION
		   "Sid Index Number."
                ::= { oramodTblspEntry 1 }

        oramodTblspTBLSPINDX OBJECT-TYPE
                SYNTAX INTEGER (1..2147483647)
                ACCESS read-only
                STATUS mandatory
                DESCRIPTION
		   "Tablespace Index Number."
                ::= { oramodTblspEntry 2 }

        oramodTblspTBLSPNAME OBJECT-TYPE
                SYNTAX DisplayString
                ACCESS read-only
                STATUS mandatory
                DESCRIPTION
		   "Tablespace Name."
                ::= { oramodTblspEntry 3 }

        oramodTblspFILENAME OBJECT-TYPE
                SYNTAX DisplayString
                ACCESS read-only
                STATUS mandatory
                DESCRIPTION
		   "File Name."
                ::= { oramodTblspEntry 4 }

        oramodTblspEXTENTS OBJECT-TYPE
                SYNTAX Gauge
                ACCESS read-only
                STATUS mandatory
                DESCRIPTION
		   "The number of extents."
                ::= { oramodTblspEntry 5 }

        oramodTblspLRGEXTENT OBJECT-TYPE
                SYNTAX Gauge
                ACCESS read-only
                STATUS mandatory
                DESCRIPTION
		   "Size in bytes of the largest extent."
                ::= { oramodTblspEntry 6 }

        oramodTblspSMEXTENT OBJECT-TYPE
                SYNTAX Gauge
                ACCESS read-only
                STATUS mandatory
                DESCRIPTION
		   "Size in bytes of the smallest extent."
                ::= { oramodTblspEntry 7 }

        oramodTblspINCREMENTBY OBJECT-TYPE
                SYNTAX INTEGER
                ACCESS read-only
                STATUS mandatory
                DESCRIPTION
		   "Size in bytes of the next extent increment."
                ::= { oramodTblspEntry 8 }

        oramodTblspBYTESCOALSD OBJECT-TYPE
                SYNTAX Gauge
                ACCESS read-only
                STATUS mandatory
                DESCRIPTION
		   "The number of free bytes that were coalesed. If
		this number does not match bytesfree then the tablespace
		has some extents that can be coalesced."
                ::= { oramodTblspEntry 9 }

        oramodTblspBYTESFREE OBJECT-TYPE
                SYNTAX Gauge
                ACCESS read-only
                STATUS mandatory
                DESCRIPTION
		   "The total number of free bytes available." 
                ::= { oramodTblspEntry 10 }

-- Locks - A table of lock holders and requestors. Locks are used to protect
-- a data structure. It can be held continuously until a commit or rollback
-- takes place. This table can be used to check for lock contention. 


        oramodLockTable OBJECT-TYPE
                SYNTAX SEQUENCE OF OramodLockEntry
                ACCESS not-accessible
                STATUS mandatory
                DESCRIPTION
                  "Lock Holders and Requestors"
                ::= { oramodLock 1 }

        oramodLockEntry OBJECT-TYPE
                SYNTAX OramodLockEntry
                ACCESS not-accessible
                STATUS mandatory
                DESCRIPTION
                   "An entry from the lock and session tables."
                INDEX { oramodLockSIDINDX, oramodLockUSERINDX }
                ::= {  oramodLockTable 1 }

        OramodLockEntry ::= SEQUENCE {
                oramodLockSIDINDX
                        INTEGER,
                oramodLockUSERINDX
                        INTEGER,
                oramodLockUSERNAME
                        DisplayString,
                oramodLockOBJECT
                        DisplayString,
                oramodLockTYPE
                        DisplayString,
                oramodLockMODE
                        INTEGER,
                oramodLockCTIME
                        INTEGER,
                oramodLockBLOCK
                        INTEGER
        }

        oramodLockSIDINDX OBJECT-TYPE
                SYNTAX INTEGER
                ACCESS read-only
                STATUS mandatory
                DESCRIPTION
		   "Sid Index Number."
                ::= { oramodLockEntry 1 }

        oramodLockUSERINDX OBJECT-TYPE
                SYNTAX INTEGER
                ACCESS read-only
                STATUS mandatory
                DESCRIPTION
		   "User's Index Number."
                ::= { oramodLockEntry 2 }

        oramodLockUSERNAME OBJECT-TYPE
                SYNTAX DisplayString
                ACCESS read-only
                STATUS mandatory
                DESCRIPTION
		   "User Name holding the lock."
                ::= { oramodLockEntry 3 }

        oramodLockOBJECT OBJECT-TYPE
                SYNTAX DisplayString
                ACCESS read-only
                STATUS mandatory
                DESCRIPTION
		   "The name of the locked object."
                ::= { oramodLockEntry 4 }

        oramodLockTYPE OBJECT-TYPE
                SYNTAX DisplayString
                ACCESS read-only
                STATUS mandatory
                DESCRIPTION
		   "Object Type."
                ::= { oramodLockEntry 5 }

        oramodLockMODE OBJECT-TYPE
                SYNTAX INTEGER
                ACCESS read-only
                STATUS mandatory
                DESCRIPTION
		   "Lock Mode held (Null, Row share, Row exclusive, Share,
		Share row exclusive, Exclusive)."
                ::= { oramodLockEntry 6 }

        oramodLockCTIME OBJECT-TYPE
                SYNTAX INTEGER
                ACCESS read-only
                STATUS mandatory
                DESCRIPTION
		   "Time since current mode was granted."
                ::= { oramodLockEntry 7 }

        oramodLockBLOCK OBJECT-TYPE
                SYNTAX INTEGER
                ACCESS read-only
                STATUS mandatory
                DESCRIPTION
		   "The lock is blocking another lock."
                ::= { oramodLockEntry 8 }


--Latches - A table of latches held per instance. Latches are used to prevent
-- access to Oracle's internal memory structures while a process is accessing
-- them.  Too much time spent waiting for latches or increasing misses can  
-- indicate performance issues.

        oramodLatchTable OBJECT-TYPE
                SYNTAX SEQUENCE OF OramodLatchEntry
                ACCESS not-accessible
                STATUS mandatory
                DESCRIPTION
                  "Latch information."
                ::= { oramodLatch 1 }

        oramodLatchEntry OBJECT-TYPE
                SYNTAX OramodLatchEntry
                ACCESS not-accessible
                STATUS mandatory
                DESCRIPTION
                   "An entry from the latch table."
                INDEX { oramodLatchSIDINDX, oramodLatchLATCHINDX }
                ::= {  oramodLatchTable 1 }

        OramodLatchEntry ::= SEQUENCE {
                oramodLatchSIDINDX
                        INTEGER,
                oramodLatchLATCHINDX
                        INTEGER,
                oramodLatchLATCHNUM
                        INTEGER,
                oramodLatchLATCHNAME
                        DisplayString,
                oramodLatchGETS
                        Counter,
                oramodLatchMISSES
                        Counter,
                oramodLatchSLEEPS
                        Counter,
                oramodLatchIMDGETS
                        Counter,
                oramodLatchIMDMISSES
                        Counter,
                oramodLatchWAITSHOLDING
                        Counter,
                oramodLatchSPINGETS
                        Counter
        }

        oramodLatchSIDINDX OBJECT-TYPE
                SYNTAX INTEGER
                ACCESS read-only
                STATUS mandatory
                DESCRIPTION
		   "Sid Index Number."
                ::= { oramodLatchEntry 1 }

        oramodLatchLATCHINDX OBJECT-TYPE
                SYNTAX INTEGER
                ACCESS read-only
                STATUS mandatory
                DESCRIPTION
		   "Latch Index Number."
                ::= { oramodLatchEntry 2 }

        oramodLatchLATCHNUM OBJECT-TYPE
                SYNTAX INTEGER
                ACCESS read-only
                STATUS mandatory
                DESCRIPTION
		   "Latch Number."
                ::= { oramodLatchEntry 3 }

        oramodLatchLATCHNAME OBJECT-TYPE
                SYNTAX DisplayString
                ACCESS read-only
                STATUS mandatory
                DESCRIPTION
		   "Latch Name."
                ::= { oramodLatchEntry 4 }

        oramodLatchGETS OBJECT-TYPE
                SYNTAX Counter
                ACCESS read-only
                STATUS mandatory
                DESCRIPTION
		   "Latch Gets. Reflects the proportion of times requests
	 	for latches are satisfied without waiting. Should be high."
                ::= { oramodLatchEntry 5 }

        oramodLatchMISSES OBJECT-TYPE
                SYNTAX Counter
                ACCESS read-only
                STATUS mandatory
                DESCRIPTION
		   "Latch Misses."
                ::= { oramodLatchEntry 6 }

        oramodLatchSLEEPS OBJECT-TYPE
                SYNTAX Counter
                ACCESS read-only
                STATUS mandatory
                DESCRIPTION
		   "Latch Sleeps."
                ::= { oramodLatchEntry 7 }

        oramodLatchIMDGETS OBJECT-TYPE
                SYNTAX Counter
                ACCESS read-only
                STATUS mandatory
                DESCRIPTION
		   "Latch Immediate Gets."
                ::= { oramodLatchEntry 8 }

        oramodLatchIMDMISSES OBJECT-TYPE
                SYNTAX Counter
                ACCESS read-only
                STATUS mandatory
                DESCRIPTION
		   "Latch Immediate Misses."
                ::= { oramodLatchEntry 9 }

        oramodLatchWAITSHOLDING OBJECT-TYPE
                SYNTAX Counter
                ACCESS read-only
                STATUS mandatory
                DESCRIPTION
		   "Latch Immediate Misses. When a session cannot acquire
		a latch, it goes into active wait and repeatedly trys
		to acquire the latch impaitiently."
                ::= { oramodLatchEntry 10 }

        oramodLatchSPINGETS OBJECT-TYPE
                SYNTAX Counter
                ACCESS read-only
                STATUS mandatory
                DESCRIPTION
		   "Latch Spin Gets. Some latch types will lock into a 
		tight loop testing the latch and trying to get it. Spinning
		uses significant CPU resources. The number of spins depends
		on the spincount set it the init.ora file. Once the number
		of spincounts has been reached the prcess will back off 
		and sleep."
                ::= { oramodLatchEntry 11 }

-- Backup - A table of physical backup information.  

        oramodBackupTable OBJECT-TYPE
                SYNTAX SEQUENCE OF OramodBackupEntry
                ACCESS not-accessible
                STATUS mandatory
                DESCRIPTION
                  "Backup Information"
                ::= { oramodBackup 1 }

        oramodBackupEntry OBJECT-TYPE
                SYNTAX OramodBackupEntry
                ACCESS not-accessible
                STATUS mandatory
                DESCRIPTION
                   "An entry from the backup table."
                INDEX { oramodBackupSIDINDX, oramodBackupFILENUM }
                ::= {  oramodBackupTable 1 }

        OramodBackupEntry ::= SEQUENCE {
                oramodBackupSIDINDX
                        INTEGER,
                oramodBackupFILENUM
                        INTEGER,
                oramodBackupSTATUS
                        DisplayString,
                oramodBackupCHANGENUM
                        INTEGER,
                oramodBackupDATE
                        DisplayString
        }

        oramodBackupSIDINDX OBJECT-TYPE
                SYNTAX INTEGER
                ACCESS read-only
                STATUS mandatory
                DESCRIPTION
		   "Sid Index Number."
                ::= { oramodBackupEntry 1 }

        oramodBackupFILENUM OBJECT-TYPE
                SYNTAX INTEGER
                ACCESS read-only
                STATUS mandatory
                DESCRIPTION
		   "Backup File Index Number."
                ::= { oramodBackupEntry 2 }

        oramodBackupSTATUS OBJECT-TYPE
                SYNTAX DisplayString
                ACCESS read-only
                STATUS mandatory
                DESCRIPTION
		   "Status of the backup."
                ::= { oramodBackupEntry 3 }

        oramodBackupCHANGENUM OBJECT-TYPE
                SYNTAX INTEGER
                ACCESS read-only
                STATUS mandatory
                DESCRIPTION
		   "The change number of the backup file."
                ::= { oramodBackupEntry 4 }

        oramodBackupDATE OBJECT-TYPE
                SYNTAX DisplayString
                ACCESS read-only
                STATUS mandatory
                DESCRIPTION
		   "Date of the Backup."
                ::= { oramodBackupEntry 5 }


-- Archive - A table of archive information. 

        oramodArcDestTable OBJECT-TYPE
                SYNTAX SEQUENCE OF OramodArcDestEntry
                ACCESS not-accessible
                STATUS mandatory
                DESCRIPTION
                  "Archive Information."
                ::= { oramodArcDest 1 }

        oramodArcDestEntry OBJECT-TYPE
                SYNTAX OramodArcDestEntry
                ACCESS not-accessible
                STATUS mandatory
                DESCRIPTION
                   "An entry from the archive destination table."
                INDEX { oramodArcDestSIDINDX, oramodArcDestARCINDX  }
                ::= {  oramodArcDestTable 1 }

        OramodArcDestEntry ::= SEQUENCE {
                oramodArcDestSIDINDX
                        INTEGER,
                oramodArcDestARCINDX
                        INTEGER,
                oramodArcDestARCMODE
                        DisplayString,
                oramodArcDestSTATUS
                        DisplayString,
                oramodArcDestDEST
                        DisplayString
        }

        oramodArcDestSIDINDX OBJECT-TYPE
                SYNTAX INTEGER
                ACCESS read-only
                STATUS mandatory
                DESCRIPTION
		   "Sid Index Number."
                ::= { oramodArcDestEntry 1 }

        oramodArcDestARCINDX OBJECT-TYPE
                SYNTAX INTEGER
                ACCESS read-only
                STATUS mandatory
                DESCRIPTION
		   "Index of Archive log."
                ::= { oramodArcDestEntry 2 }

        oramodArcDestARCMODE OBJECT-TYPE
                SYNTAX DisplayString
                ACCESS read-only
                STATUS mandatory
                DESCRIPTION
		   "Mode of Archive log."
                ::= { oramodArcDestEntry 3 }

        oramodArcDestSTATUS OBJECT-TYPE
                SYNTAX DisplayString
                ACCESS read-only
                STATUS mandatory
                DESCRIPTION
		   "Status of Archive Log File."
                ::= { oramodArcDestEntry 4 }

        oramodArcDestDEST OBJECT-TYPE
                SYNTAX DisplayString
                ACCESS read-only
                STATUS mandatory
                DESCRIPTION
		   "Archive Log File Destination."
                ::= { oramodArcDestEntry 5 }

END
