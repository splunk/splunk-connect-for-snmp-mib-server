    SEQUENCE {
        sonusSonetLayerShelfIndex       SonusShelfIndex,
        sonusSonetLayerSlotIndex        SonusSlotIndex,
        sonusSonetLayerPortIndex        Integer32,
        sonusSonetLayerIfIndex          Integer32,
        sonusSonetLayerRowStatus        RowStatus
    }

