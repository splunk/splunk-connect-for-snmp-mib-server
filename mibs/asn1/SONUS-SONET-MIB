    SYNTAX          SonusServiceState
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION     
            "STS1 in-service/out-of-service."
    DEFVAL          { outOfService }
    ::= { sonusSonetPathAdmnEntry 8 }

sonusSonetPathAdmnAction OBJECT-TYPE
    SYNTAX          SonusAdminAction
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION     
            "The type of action to perform when a SONET path is taken out of 
             service. The SONET path can be dried up, or it can be forced
             out of service."
    DEFVAL          { dryUp }
    ::= { sonusSonetPathAdmnEntry 9 }

sonusSonetPathAdmnTimeout OBJECT-TYPE
    SYNTAX          Integer32 (0..1440)
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION     
            "The Dryup Timeout specifies the maximum amount of time to wait for
             all active calls to terminate on a SONET path before the path
             		 should be taken out-of-service."
    DEFVAL          { 5 }
    ::= { sonusSonetPathAdmnEntry 10 }

sonusSonetPathAdmnStatAction OBJECT-TYPE
    SYNTAX          INTEGER { init15MinuteNearEnd(1), 
                        init15MinuteFarEnd(2), init24HourNearEnd(4), 
                        init24HourFarEnd(8) }
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION     
            "Initialize Performance Monitoring Statistics.  A PM initialization
             action initiates one request for the action.  Requests to initialize
             more than one group of statistics can be accomplished by OR-ing the 
             appropriate bits (e.g., to initialize all 15 minute PM statistics, 
             set bits 1 and 2). Reading this variable always returns zero."
    ::= { sonusSonetPathAdmnEntry 11 }

sonusSonetPathAdmnRDIMode OBJECT-TYPE
    SYNTAX          INTEGER { normal(1), enhanced(2) }
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION     
            "The desired type of SONET port Path Remote Defect Indication to
             use.
             		 (1) normal - Use one-bit RDI-P signalling using bit 5 of G1.
             		 (2) enhanced - Use three-bit RDI-P (ERDI-P) using bits 5-7 of G1."
    DEFVAL          { enhanced }
    ::= { sonusSonetPathAdmnEntry 12 }

sonusSonetPathAdmnTraceState OBJECT-TYPE
    SYNTAX          SonusAdminState
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION     
            "The desired state of SONET Path Trace.
             (1) enabled - Enables the generation of SONET Path Trace Messages
             in the J1 bytes of the STS Path overhead, and enables the reception and
             checking of received Path Trace Messages with the Expected Path Trace
             Message.
             (2) disabled - Transmits all zero's in the J1 bytes of the STS Path
             overhead, and disables  the reception and checking of received
             Path Trace Messages with the Expected Path Trace Message."
    DEFVAL          { disabled }
    ::= { sonusSonetPathAdmnEntry 13 }

sonusSonetPathAdmnTraceExpMsg OBJECT-TYPE
    SYNTAX          DisplayString (SIZE(0..61))
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION     
            "The expected path trace string to be received on the SONET port in the
             J1 bytes of the STS Path overhead."
    ::= { sonusSonetPathAdmnEntry 14 }

sonusSonetPathAdmnTraceXmtMsg OBJECT-TYPE
    SYNTAX          DisplayString (SIZE(0..61))
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION     
            "The path trace string to be transmitted on the SONET port in the J1
             bytes of the STS Path overhead."
    ::= { sonusSonetPathAdmnEntry 15 }

sonusSonetPathAdmnTraceRcvMsg OBJECT-TYPE
    SYNTAX          DisplayString (SIZE(0..61))
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "The path trace string received on the SONET port in the J1
             bytes of the STS Path overhead."
    ::= { sonusSonetPathAdmnEntry 16 }

sonusSonetPathAdmnOperStatus OBJECT-TYPE
    SYNTAX          INTEGER { enabledInService(1), 
                        enabledOutOfService(2), enabledDown(3), 
                        dryingUp(4), disabled(5), up(6), down(7) }
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "The actual status of the Sonet path. Path is 
             Up if it is administratively in service and there are no
             line failures or hardware failures. Path is Down if
             it is administratively in service and there are line
             failures or hardware failures, or if it is administratively
             out of service. Path is DryingUp if it is administratively
             out of service and is drying up (no new	calls, still carrying
             in-progress calls) THE OTHER VALUES ARE NO LONGER SUPPORTED."
    ::= { sonusSonetPathAdmnEntry 17 }

sonusSonetPathAdmnDs3ModeOverride OBJECT-TYPE
    SYNTAX          SonusAdminState
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION     
            "This flag controls whether the mode and state of the DS3
             carried in the Path are modified when the mode and state of
             the Path is modified. If this flag is set to Enabled, the DS3
             state and mode are not changed when the Path mode and state
             are changed."
    DEFVAL          { disabled }
    ::= { sonusSonetPathAdmnEntry 18 }

sonusSonetPath OBJECT IDENTIFIER
    ::= { sonusSonetObjectsPath 2 }

sonusSonetPathThresholdTable OBJECT-TYPE
    SYNTAX          SEQUENCE OF SonusSonetPathThresholdEntry
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION     
            "The Sonus SONET Path Threshold Table. This table adds objects to
             set the Performance Monitoring thresholds for generating a
             Threshold Crossing Alert."
    ::= { sonusSonetPath 1 }

