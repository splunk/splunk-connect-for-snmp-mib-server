    STATUS          current
    DESCRIPTION     
            "The Sonus SONET Path Failure Interval table."
    ::= { sonusSonetPath 7 }

sonusSonetPathFailureIntervalEntry OBJECT-TYPE
    SYNTAX          SonusSonetPathFailureIntervalEntry
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION     
            "An entry in the Sonus SONET Path Failure Interval table."
    INDEX           { sonusSonetPathFailureIntervalIfIndex, 
                      sonusSonetPathFailureIntervalNumber }
    ::= { sonusSonetPathFailureIntervalTable 1 }

SonusSonetPathFailureIntervalEntry ::=
    SEQUENCE {
        sonusSonetPathFailureIntervalIfIndex    Integer32,
        sonusSonetPathFailureIntervalPathIndex      Integer32,
        sonusSonetPathFailureIntervalNumber   Integer32,
        sonusSonetPathFailureIntervalFCs PerfCurrentCount
    }

sonusSonetPathFailureIntervalIfIndex OBJECT-TYPE
    SYNTAX          Integer32
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION     
            "The ifIndex assigned to this path when it was created.
             This will not change until the path is deleted."
    ::= { sonusSonetPathFailureIntervalEntry 1 }

sonusSonetPathFailureIntervalPathIndex OBJECT-TYPE
    SYNTAX          Integer32 (1..3)
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "STS1 path index for this table."
    ::= { sonusSonetPathFailureIntervalEntry 2 }

sonusSonetPathFailureIntervalNumber OBJECT-TYPE
    SYNTAX          Integer32
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION     
            "A number between 1 and 32, which identifies the interval
             for which the set of statistics is available. The number 1
             identifies the most recently completed interval, and the
             interval identified by N is the interval immediately
             preceding the one identified by N-1."
    ::= { sonusSonetPathFailureIntervalEntry 3 }

sonusSonetPathFailureIntervalFCs OBJECT-TYPE
    SYNTAX          PerfCurrentCount
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "The counter associated with the number of Failures encountered
             by a SONET/SDH Path in the current 15 minute interval."
    ::= { sonusSonetPathFailureIntervalEntry 4 }

sonusSonetFarEndPathFailureCurrentTable OBJECT-TYPE
    SYNTAX          SEQUENCE OF SonusSonetFarEndPathFailureCurrentEntry
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION     
            "The Sonus SONET Far End Path Failure Current table."
    ::= { sonusSonetPath 8 }

sonusSonetFarEndPathFailureCurrentEntry OBJECT-TYPE
    SYNTAX          SonusSonetFarEndPathFailureCurrentEntry
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION     
            "An entry in the Sonus SONET Far End Path Failure Current table."
