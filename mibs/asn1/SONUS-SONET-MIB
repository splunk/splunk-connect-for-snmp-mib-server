        sonusSonetFarEndPathFailureCurrentIfIndex         Integer32,
        sonusSonetFarEndPathFailureCurrentPathIndex           Integer32,
        sonusSonetFarEndPathFailureCurrentFCs     PerfCurrentCount
    }

sonusSonetFarEndPathFailureCurrentIfIndex OBJECT-TYPE
    SYNTAX          Integer32
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION     
            "The ifIndex assigned to this path when it was created.
             This will not change until the path is deleted."
    ::= { sonusSonetFarEndPathFailureCurrentEntry 1 }

