    SYNTAX          Integer32
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION     
            "Port index for this table."
    ::= { sonusSonetLayerEntry 3 }

sonusSonetLayerIfIndex OBJECT-TYPE
    SYNTAX          Integer32
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "The ifIndex assigned to this shelf, slot, and port when the 
             SONET Interface was created."
