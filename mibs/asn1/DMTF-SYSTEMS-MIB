	REFERENCE "'DMTF|Mass Store Array Info Table|001' 1"
	::= { dmtfMassStoreArrayInfoEntry 1 }

driveArrayLevel OBJECT-TYPE
	SYNTAX		INTEGER {
		other(1),
		unknown(2),
		rAID0(3),
		rAID1(4),
		rAID2(5),
		rAID3(6),
		rAID4(7),
