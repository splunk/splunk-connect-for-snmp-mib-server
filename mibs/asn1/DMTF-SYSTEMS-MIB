	SYNTAX		INTEGER {
		false(0),
		true(1)
	}
	MAX-ACCESS	read-only
	STATUS		current
	DESCRIPTION	"If true, this partition is encrypted."
	REFERENCE "'DMTF|Partition|002' 8"
	::= { dmtfPartitionEntry 8 }

