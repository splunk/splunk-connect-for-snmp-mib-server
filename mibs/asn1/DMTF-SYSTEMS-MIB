	systemCacheErrorCorrection	DmiInteger,
	fRUGroupIndex	DmiInteger,
	operationalGroupIndex	DmiInteger,
	systemCacheType	DmiInteger,
	lineSize	DmiInteger,
	volatility	DmiInteger,
	replacementPolicy	DmiInteger,
	readPolicy	DmiInteger,
	flushTimer	DmiInteger,
	associativity	DmiInteger
}

