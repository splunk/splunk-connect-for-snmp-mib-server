	SYNTAX		INTEGER {
		false(0),
		true(1)
	}
	MAX-ACCESS	read-only
	STATUS		current
	DESCRIPTION	"If true, this DMA channel is available."
	REFERENCE "'DMTF|DMA|001' 2"
	::= { dmtfDMAEntry 2 }

