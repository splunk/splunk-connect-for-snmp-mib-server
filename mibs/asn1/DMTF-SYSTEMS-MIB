	SYNTAX		DmiString
	MAX-ACCESS	read-only
	STATUS		current
	DESCRIPTION	"The name of the port currently being used by this mouse."
	REFERENCE "'DMTF|Mouse|003' 4"
	::= { dmtfMouseEntry 4 }

