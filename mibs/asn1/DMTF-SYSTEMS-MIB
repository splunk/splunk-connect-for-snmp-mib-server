	SYNTAX		DmiInteger
	MAX-ACCESS	read-only
	STATUS		current
	DESCRIPTION	"The number of disks this partition occupies."
	REFERENCE "'DMTF|Partition|002' 9"
	::= { dmtfPartitionEntry 9 }

