	resourceNumber	DmiInteger,
	resourceInfoID	DmiInteger,
	startAddress	DmiInteger64,
	endAddress	DmiInteger64,
	resourceSize	DmiInteger,
	baseAlignment	DmiInteger,
	shareable	DmiInteger,
	shared	DmiInteger
}

