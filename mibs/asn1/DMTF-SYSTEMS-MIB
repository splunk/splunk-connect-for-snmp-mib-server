	MAX-ACCESS	read-only
	STATUS		current
	DESCRIPTION	"The speed of this System Cache module in nanoseconds."
	REFERENCE "'DMTF|System Cache|003' 3"
	::= { dmtfSystemCacheEntry 3 }

