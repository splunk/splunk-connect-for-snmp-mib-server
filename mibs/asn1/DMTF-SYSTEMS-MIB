	SYNTAX		DmiInteger
	MAX-ACCESS	read-only
	STATUS		current
	DESCRIPTION	"The IRQ number used by this mouse."
	REFERENCE "'DMTF|Mouse|003' 2"
	::= { dmtfMouseEntry 2 }

