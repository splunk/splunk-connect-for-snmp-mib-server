	SYNTAX		INTEGER {
		other(0),
		unknown(1)
	}
	MAX-ACCESS	not-accessible
	STATUS		current
	DESCRIPTION	"The major functional aspect of the product causing the fault."
	::= {dmtfCoolingDeviceTable 6 }

