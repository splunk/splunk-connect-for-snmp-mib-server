-- ********************** apFlowMgr DOS Scalars BEGIN *********************

apFlowMgrExtDosTotalAttacks OBJECT-TYPE
	SYNTAX Integer32
	MAX-ACCESS read-only
	STATUS current
	DESCRIPTION
		"This object specifies the total number of 
         Denial Of Service (DOS) attacks detected by 
         the flowmgr."
	::= { flowMgrExt 18 }

apFlowMgrExtDosTotalSynAttacks OBJECT-TYPE
	SYNTAX Integer32
	MAX-ACCESS read-only
	STATUS current
	DESCRIPTION
		"This object specifies the total number of 
         Denial Of Service (DOS) SYN attacks detected by 
         the flowmgr."
	::= { flowMgrExt 19 }

apFlowMgrExtDosTotalLandAttacks OBJECT-TYPE
	SYNTAX Integer32
	MAX-ACCESS read-only
	STATUS current
	DESCRIPTION
		"This object specifies the total number of 
         Denial Of Service (DOS) LAND attacks detected by 
         the flowmgr."
	::= { flowMgrExt 20 }

apFlowMgrExtDosTotalZeroPortAttacks OBJECT-TYPE
	SYNTAX Integer32
	MAX-ACCESS read-only
	STATUS current
	DESCRIPTION
		"This object specifies the total number of 
         Denial Of Service (DOS) attacks to a source or
         destination port of zero detected by the flowmgr."
	::= { flowMgrExt 21 }

apFlowMgrExtDosTotalIllegalSourceAttacks OBJECT-TYPE
	SYNTAX Integer32
	MAX-ACCESS read-only
	STATUS current
	DESCRIPTION
		"This object specifies the total number of 
         Denial Of Service (DOS) attacks to an illegal
         source address detected by the flowmgr."
	::= { flowMgrExt 22 }        
	
apFlowMgrExtDosTotalIllegalDestinationAttacks OBJECT-TYPE
	SYNTAX Integer32
	MAX-ACCESS read-only
	STATUS current
	DESCRIPTION
		"This object specifies the total number of 
		Denial Of Service (DOS) attacks to an illegal 
		destination address detected by the flowmgr."
	::= { flowMgrExt 23 }
	             
apFlowMgrExtDosTotalPingOfDeathAttacks OBJECT-TYPE
	SYNTAX Integer32
	MAX-ACCESS read-only
	STATUS obsolete
	DESCRIPTION
		"This object specifies the total number of 
         Denial Of Service (DOS) Ping of Death attacks 
         detected by the flowmgr."
	::= { flowMgrExt 24 }

apFlowMgrExtDosTotalSmurfAttacks OBJECT-TYPE
	SYNTAX Integer32
	MAX-ACCESS read-only
	STATUS current
	DESCRIPTION
		"This object specifies the total number of 
