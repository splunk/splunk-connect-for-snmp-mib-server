-- ********************** apFlowMgrPortTable BEGIN ***************************

apFlowMgrPortTable OBJECT-TYPE
	SYNTAX  SEQUENCE OF ApFlowMgrPortEntry
	MAX-ACCESS  not-accessible
	STATUS  current
	DESCRIPTION
		"The FlowMgr Port Table for a given slot/subslot in the chassis."
	::= { flowMgrExt 16 }

apFlowMgrPortEntry OBJECT-TYPE
	SYNTAX  ApFlowMgrPortEntry
    MAX-ACCESS  not-accessible
	STATUS  current
    DESCRIPTION
		"An entry in the Flow Manager port Table."
	INDEX { apFlowMgrPortIfIndex }
	::= { apFlowMgrPortTable 1 }

	ApFlowMgrPortEntry ::=
		SEQUENCE {
			apFlowMgrPortIfIndex                Integer32,
			apFlowMgrPortSeqBytes               Counter64,
			apFlowMgrPortInBytes                Counter64,
			apFlowMgrPortOutBytes               Counter64, 
			apFlowMgrPortInFrames               Counter64,
			apFlowMgrPortOutFrames              Counter64,
			apFlowMgrPortTotalInFlows           Counter64,
			apFlowMgrPortTotalOutFlows          Counter64,
			apFlowMgrPortTotalAcctFlows         Counter64,
			apFlowMgrPortCurrTCPFlows           Integer32,  
			apFlowMgrPortCurrUDPFlows           Integer32,
			apFlowMgrPortCurrInFlows            Integer32,
			apFlowMgrPortCurrOutFlows           Integer32,
			apFlowMgrPortResetCount             Integer32, 
			apFlowMgrPortMTU                    Integer32,
			apFlowMgrPortWccAccepted            Integer32, 
			apFlowMgrPortWccAcceptedSticky      Integer32,
			apFlowMgrPortWccAcceptedBridge      Integer32,
			apFlowMgrPortWccAcceptedSfpFwd      Integer32,
			apFlowMgrPortWccAcceptedSfp         Integer32,
			apFlowMgrPortWccForwardSfp          Integer32, 
			apFlowMgrPortWccForward             Integer32, 
			apFlowMgrPortWccLocal               Integer32,
			apFlowMgrPortWccTCPSpoof            Integer32, 
			apFlowMgrPortWccArpPending          Integer32, 
			apFlowMgrPortWccRedirect            Integer32,
			apFlowMgrPortWccRedirectSticky      Integer32, 
			apFlowMgrPortWccNoChange            Integer32, 
			apFlowMgrPortWccRouteNotFound       Integer32,
			apFlowMgrPortWccRtLookupError       Integer32,
			apFlowMgrPortWccDiscard             Integer32,
			apFlowMgrPortWccRejected            Integer32,
			apFlowMgrPortWccRejectedSfp         Integer32,
			apFlowMgrPortTotalAckDelete         Integer32,
			apFlowMgrPortPortAckDelete          Integer32,
			apFlowMgrPortMidSpoofRejects        Integer32,
 			apFlowMgrPortMidNATReject           Integer32,
			apFlowMgrPortMidStickyReject        Integer32,
			apFlowMgrPortSynExceedDelete        Integer32,
			apFlowMgrPortInvalidIP              Integer32,
			apFlowMgrPortBufferShortage         Integer32,
			apFlowMgrPortMovedAcctReports       Integer32,
			apFlowMgrPortUknownQueries          Integer32,
			apFlowMgrPortMappedLeftovers        Integer32,
			apFlowMgrPortUnmappedLeftovers      Integer32,
			apFlowMgrPortRemappedIngress        Integer32,
			apFlowMgrPortFlowsUnavailable       Integer32,
			apFlowMgrPortTCPCsumErrors          Integer32,
			apFlowMgrPortOverflowMaps           Integer32, 
			apFlowMgrPortRouteChangeEvents      Integer32, 
			apFlowMgrPortRouteChangeFlows       Integer32,
			apFlowMgrPortRouteChangeRemaps      Integer32,
			apFlowMgrPortOrphanMappings         Integer32
}


apFlowMgrPortIfIndex OBJECT-TYPE
	SYNTAX          Integer32
	MAX-ACCESS      read-only
	STATUS  		current
	DESCRIPTION     
		"IF index for this interface"
	::= {apFlowMgrPortEntry 1}

apFlowMgrPortSeqBytes OBJECT-TYPE
	SYNTAX          Counter64
	MAX-ACCESS      read-only
	STATUS  		current
	DESCRIPTION     
		"Total TCP sequence space bytes received on interface by flow manager"
	::= {apFlowMgrPortEntry 2}

