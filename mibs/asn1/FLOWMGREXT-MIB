-- ********************** apFlowMgrStatIfTable END ***************************

-- ********************** apFlowMgrStatSSTable BEGIN ************************

apFlowMgrStatSSTable OBJECT-TYPE
	SYNTAX  SEQUENCE OF ApFlowMgrStatSSEntry
	MAX-ACCESS  not-accessible
	STATUS  current
	DESCRIPTION
		"The FlowMgr Statistics Table"
	::= { flowMgrExt 15 }

apFlowMgrStatSSEntry OBJECT-TYPE
	SYNTAX  ApFlowMgrStatSSEntry
    MAX-ACCESS  not-accessible
	STATUS  current
    DESCRIPTION
		"An entry in the Flow Manager statistics Table referenced via an 
		chassis manager slot/subslot pair...."

	INDEX { apFlowMgrStatSSSlot, apFlowMgrStatSSSubSlot }
	::= { apFlowMgrStatSSTable 1 }

	ApFlowMgrStatSSEntry ::=
		SEQUENCE {
			apFlowMgrStatSSSlot                 Integer32,
			apFlowMgrStatSSSubSlot              Integer32,
			apFlowMgrStatSSUDPTotal             Integer32,
			apFlowMgrStatSSUDPCur               Integer32,
			apFlowMgrStatSSUDPHigh              Integer32,
			apFlowMgrStatSSUDPAvg               Integer32,
			apFlowMgrStatSSTCPTotal             Integer32,
			apFlowMgrStatSSTCPCur               Integer32,
			apFlowMgrStatSSTCPHigh              Integer32,
			apFlowMgrStatSSTCPAvg               Integer32,
			apFlowMgrStatSSCurrentActiveTCP		Integer32,
			apFlowMgrStatSSCurrentActiveUDP		Integer32,
			apFlowMgrStatSSAccountingReportsRx	Integer32,
			apFlowMgrStatSSResets				Integer32,
			apFlowMgrStatSSFcbCount				Integer32
		}

