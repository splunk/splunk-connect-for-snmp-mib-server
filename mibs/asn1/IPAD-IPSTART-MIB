    SYNTAX    INTEGER
              {
              unknown(1),
              other(2),
              pap(3),
              chap(4),
              spap(5),
              eap(6)
              }
    ACCESS    read-only
    STATUS    mandatory
    DESCRIPTION
        ""
    ::= { ipadPPPStartTableEntry 7 }

ipadPPPStartLCPQualityProtocol  OBJECT-TYPE
    SYNTAX    INTEGER
              {
              unknown(1),
              other(2),
