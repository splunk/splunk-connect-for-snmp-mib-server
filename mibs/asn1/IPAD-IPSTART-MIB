              }            
    ACCESS    read-only
    STATUS    mandatory
    DESCRIPTION
        ""
    ::= { ipadPPPStartTableEntry 8 }

ipadPPPStartLCPMagicNumber  OBJECT-TYPE
    SYNTAX    DisplayString(SIZE(4))
    ACCESS    read-only
    STATUS    mandatory
    DESCRIPTION
        ""
    ::= { ipadPPPStartTableEntry 9 }

ipadPPPStartLCPPFC  OBJECT-TYPE
    SYNTAX    INTEGER
              {
              enabled(1),
