              hwEthernetFlagC2Value     
                  ,
              hwEthernetUpHoldTime
                  ,
              hwEthernetDownHoldTime                      
                  ,
              hwEthernetSubinterfaceStatisticEnable
                  
                  }
          STATUS current   	
          DESCRIPTION 
              "The Port attribute group."
          ::= { hwPortGroups 1 }
          
