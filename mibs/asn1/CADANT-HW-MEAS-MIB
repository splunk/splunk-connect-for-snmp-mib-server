        cadDFIDMeasIndex           SFIDIndex,
        cadDFIDMeasBytsArrived     Counter64,
        cadDFIDMeasPktsArrived     Counter64,
        cadDFIDMeasBytsDropped     Counter64,
        cadDFIDMeasPktsDropped     Counter64,
        cadDFIDMeasBytsUnkDropped  Counter64,
        cadDFIDMeasDFID            DFIDIndex,
        cadDFIDMeasPHSUnknowns     Counter64,
        cadDFIDMeasMacAddress      MacAddress,
        cadDFIDMeasSCN             DisplayString,
        cadDFIDMeasPolicedDelayPkts Counter64,
	cadDFIDMeasGateID          Unsigned32
}


