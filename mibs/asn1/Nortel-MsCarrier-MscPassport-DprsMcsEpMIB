MscMcsMgrDprsMcsEpGEpLCoPathDataEntry ::= SEQUENCE {
   mscMcsMgrDprsMcsEpGEpLCoState
      INTEGER,
   mscMcsMgrDprsMcsEpGEpLCoOverrideRemoteName
      AsciiString,
   mscMcsMgrDprsMcsEpGEpLCoEnd
      INTEGER,
   mscMcsMgrDprsMcsEpGEpLCoCostMetric
      Unsigned32,
   mscMcsMgrDprsMcsEpGEpLCoDelayMetric
      Unsigned32,
