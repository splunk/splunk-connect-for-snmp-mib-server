	SYNTAX  Integer32
	MAX-ACCESS  read-write
	STATUS  current
	DESCRIPTION
		"The CLP=0+1 discard threshold in cells for nrtVBR
		 (Variable Bit Rate) traffic."
	::= { portShmem3ConfEntry 10 }

