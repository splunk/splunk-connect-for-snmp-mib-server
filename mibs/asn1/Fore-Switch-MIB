	q2931AFFilterTListIndex		Integer32,
	q2931AFFilterTListTemplateIndex	Integer32,
	q2931AFFilterTListStatus	RowStatus
}

q2931AFFilterTListIndex		OBJECT-TYPE
	SYNTAX	Integer32
	MAX-ACCESS	not-accessible
	STATUS	current
	DESCRIPTION
		"The value of this object identifies the order in this
		 NSAP Address Filter in which this template is applied."
	::= { q2931AFFilterTListEntry 1 }

q2931AFFilterTListTemplateIndex	OBJECT-TYPE
	SYNTAX	Integer32
	MAX-ACCESS	read-write
	STATUS	current
	DESCRIPTION
