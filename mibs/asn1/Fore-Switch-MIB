	::= { upcContractEntry 23 }

upcContractPoliceScheme OBJECT-TYPE
	SYNTAX  INTEGER {
			cbr1(1),
			cbr0(2),
			cbr0tag(3),
			vbr1(4),
			vbr2(5),
			vbr3(6),
			abr1(7),
			ubr1(8),
			ubr2(9)
		}
	MAX-ACCESS  read-create
	STATUS  current
	DESCRIPTION
		"It identifies the policing associated with the UPC
		 contract as defined in the ATM Forum TM 4.0
		 specifications. However, the policing schemes cbr0
		 and cbr0tag are specific to FORE switches"
	::= { upcContractEntry 24 }

