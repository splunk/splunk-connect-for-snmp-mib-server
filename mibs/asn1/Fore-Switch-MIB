	SEQUENCE {
		asxAtmIfName	DisplayString,
		asxAtmIfLinkid	Integer32
	}

asxAtmIfName OBJECT-TYPE
	SYNTAX  DisplayString
	MAX-ACCESS  not-accessible
	STATUS  current
	DESCRIPTION
		"The persistent name of this atm interface. It is
		used to index this atm interface. All letters in the
		asxAtmIfName are always represented in uppercase."
	::= { asxAtmIfEntry 1 }

asxAtmIfLinkid OBJECT-TYPE
	SYNTAX  Integer32
	MAX-ACCESS  read-only
	STATUS  current
	DESCRIPTION
