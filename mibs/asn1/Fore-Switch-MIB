	SYNTAX  AsxAtmIfEntry
	MAX-ACCESS  not-accessible
	STATUS  current
	DESCRIPTION
		"A table entry containing atm interface information."
	INDEX { asxAtmIfName }
	::= { asxAtmIfTable 1 }

