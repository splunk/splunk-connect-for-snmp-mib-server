                ::= 2008

        battery        TRAP-TYPE
                ENTERPRISE appliance
                VARIABLES  { evtTimestamp,
                        evtDir,
                        evtSensor,
                        evtDesc }        
                DESCRIPTION
                        " Websense Alert: appliance battery warning"
                ::= 2009

        batteryClear       TRAP-TYPE
