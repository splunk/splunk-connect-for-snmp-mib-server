                ::= 2009

        raid        TRAP-TYPE
                ENTERPRISE appliance
                VARIABLES  { dskPhysicalNr,
                        dskCriticalNr,
                        dskFailedNr,
                        dskPhysErrMsg,
                        dskVirtualNr,
