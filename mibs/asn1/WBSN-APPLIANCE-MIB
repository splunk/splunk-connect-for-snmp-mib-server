                ::= 2007

        intrusion        TRAP-TYPE
                ENTERPRISE appliance
                VARIABLES  { evtTimestamp,
                        evtDir,
                        evtSensor,
                        evtDesc }        
                DESCRIPTION
                        " Websense Alert: appliance chassis intrusion detected"
                ::= 2008

        intrusionClear        TRAP-TYPE
                ENTERPRISE errorClear
                VARIABLES  { evtTimestamp,
                        evtDir,
