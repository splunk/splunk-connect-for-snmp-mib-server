        Gauge,
    smSiaSystemDeviceTokenRingIntrLost
        Counter,
    smSiaSystemDeviceTokenRingWdtLost
        Counter,
    smSiaSystemDeviceTokenRingTimoLost
        Counter,
    smSiaSystemDeviceTokenRingStaQueOverflow
        Counter,
    smSiaSystemDeviceTokenRingRxQueOverflow
        Counter,
    smSiaSystemDeviceTokenRingRxQueNoMbuf
        Counter,
    smSiaSystemDeviceTokenRingRxQueNoMbufExt
        Counter,
