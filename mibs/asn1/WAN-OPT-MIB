            DESCRIPTION
                "Specifies the total number of Call Accept Packets 
                 received from the WAN."
        ::= { cdx6500WAPktStatEntry 7 }

        cdx6500WAClearReqPktsTx OBJECT-TYPE
            SYNTAX Counter
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
                "Specifies the total number of Clear Request Packets  
                 sent on the WAN."
        ::= { cdx6500WAPktStatEntry 8 }

        cdx6500WAClearReqPktsRcv OBJECT-TYPE
            SYNTAX Counter
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
                "Specifies the total number of Clear Request Packets  
                 received from the WAN."
        ::= { cdx6500WAPktStatEntry 9 }

        cdx6500WAClearConfPktsTx OBJECT-TYPE
            SYNTAX Counter
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
                "Specifies the total number of Clear Confirm Packets
                 sent on the WAN."
        ::= { cdx6500WAPktStatEntry 10 }

        cdx6500WAClearConfPktsRcv OBJECT-TYPE
