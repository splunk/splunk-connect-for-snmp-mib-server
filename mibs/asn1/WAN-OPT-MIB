            SYNTAX Counter
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
                "Specifies the total number of Reset Confirm Packets 
                 received from the WAN."
        ::= { cdx6500WAPktStatEntry 15 }

        -- The Wan Adaptor control tables for LAN Connection.

        cdx6500ContWANAdaptorTable OBJECT-TYPE                        
            SYNTAX SEQUENCE OF Cdx6500ContWANAdaptorEntry
            ACCESS not-accessible                                    
            STATUS mandatory                                         
            DESCRIPTION                                               
                "Control parameters for the LAN Connection."                 
        ::= { cdx6500ContWANAdaptor 1 }                           
                                                                        
        cdx6500ContWANAdaptorEntry OBJECT-TYPE                        
            SYNTAX Cdx6500ContWANAdaptorEntry
            ACCESS not-accessible                                    
            STATUS mandatory                                         
            DESCRIPTION                                               
                "Each control applies to a single LAN Connection."
            INDEX { cdx6500WAControlLanConnNumber }                 
        ::= { cdx6500ContWANAdaptorTable 1 }                      
                                                                        
        Cdx6500ContWANAdaptorEntry ::=
            SEQUENCE {
                cdx6500WAControlLanConnNumber
                    INTEGER,
                cdx6500WALanConnEnableDisable
                    INTEGER,
                cdx6500WALanConnBoot
                    INTEGER,
                cdx6500WALanConnResetStat
                    INTEGER
                }

        cdx6500WAControlLanConnNumber OBJECT-TYPE
            SYNTAX INTEGER (1..32)
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
                "This identifies a specific LAN Connection Record."
        ::= { cdx6500ContWANAdaptorEntry 1 }

        cdx6500WALanConnEnableDisable OBJECT-TYPE
            SYNTAX INTEGER {
                    enable       (0),
                    disable      (1),
                    newvalEnable (50)
                    }
            ACCESS read-write
            STATUS mandatory
            DESCRIPTION
                "This command enables or disables a specified LAN connection.
                 newvalEnable : same functionality as 'enable', new enumeration
                 added for RFC1155 compatibility."
        ::= { cdx6500ContWANAdaptorEntry 2 }

        cdx6500WALanConnBoot OBJECT-TYPE
            SYNTAX INTEGER {
                    boot (1)
                    }
            ACCESS write-only
            STATUS mandatory
            DESCRIPTION
                "This command causes the LAN connection record to be booted."
        ::= { cdx6500ContWANAdaptorEntry 3 }

        cdx6500WALanConnResetStat OBJECT-TYPE
            SYNTAX INTEGER {
                    resetStats (1)
                    }
            ACCESS write-only
            STATUS mandatory
            DESCRIPTION
                "When this command is entered the resettable statistics 
                 parameters are reset for the specified LAN Connection Number."
        ::= { cdx6500ContWANAdaptorEntry 4 }

        END
