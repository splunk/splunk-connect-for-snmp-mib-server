	   ::= { cjnIpGblStats 40 }

	cjnIcmpOutTimestamps OBJECT-TYPE
	   SYNTAX INTEGER
	   MAX-ACCESS read-write
	   STATUS current
	   DESCRIPTION
	   "The number of ICMP Timestamp (request) messages sent. "
	   ::= { cjnIpGblStats 41 }

	cjnIcmpOutTimestampReps OBJECT-TYPE
