		sheSystemIpNotificationGroup NOTIFICATION-GROUP
			NOTIFICATIONS { sheSystemIpHostCreated, sheSystemIpHostDeleted, sheSystemIpHostModified, sheSystemIpRouteCreated, sheSystemIpRouteDeleted, 
				sheSystemIpRouteModified }
			STATUS current
			DESCRIPTION 
				"Description."
			::= { sheSystemGroups 5 }

		
		sheSystemObsoleteGroup OBJECT-GROUP
			OBJECTS { sheSystemAutoConfigBackupEnabled, sheSystemAutoConfigBackupFtpServerAddress, sheSystemAutoConfigBackupFtpUser, sheSystemAutoConfigBackupFtpPassword, sheSystemAutoConfigBackupFtpPathName, 
				sheSystemAutoConfigBackupTime, sheSystemAutoConfigBackupControlRequest, sheSystemAutoConfigBackupControlEnabled, sheSystemAutoConfigBackupControlFtpServerAddress, sheSystemAutoConfigBackupControlFtpUser, 
				sheSystemAutoConfigBackupControlFtpPassword, sheSystemAutoConfigBackupControlFtpPathName, sheSystemAutoConfigBackupControlBackupTime }
			STATUS current
			DESCRIPTION 
				"Description."
			::= { sheSystemGroups 6 }

		
		sheSystemObsoleteNotificationGroup NOTIFICATION-GROUP
			NOTIFICATIONS { sheSystemAutoConfigBackupEnabledChanged, sheSystemAutoConfigBackupFtpLoginInfoChanged, sheSystemAutoConfigBackupTimeChanged }
			STATUS current
			DESCRIPTION 
				"Description."
			::= { sheSystemGroups 7 }

		
		sheSystemCompliances OBJECT IDENTIFIER ::= { sheSystemConformance 2 }

		
		sheSystemCompliance MODULE-COMPLIANCE
			STATUS current
			DESCRIPTION 
				"The compliance statement for SNMP entities which 
				manage VDSL interfaces in SHE."
			MODULE -- this module
				MANDATORY-GROUPS { sheSystemGroup, sheSystemControlGroup, sheSystemNotificationGroup, sheSystemIpGroup, sheSystemIpNotificationGroup
					 }
				GROUP sheSystemObsoleteGroup
					DESCRIPTION 
						"Not used by SHR R2.5.x..."
				GROUP sheSystemObsoleteNotificationGroup
					DESCRIPTION 
						"Not used by SHR R2.5.x..."
			::= { sheSystemCompliances 1 }

		
	
	END

