			::= { sheSystemIpRouteEntry 6 }

		
		sheSystemIpRouteMetric OBJECT-TYPE
			SYNTAX Integer32
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"The routing metric for this route.
				If this metric is not used, its value should be set to -1."
			DEFVAL { -1 }
			::= { sheSystemIpRouteEntry 7 }

		
