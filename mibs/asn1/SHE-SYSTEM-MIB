--   --------------------------------------------------------------
-- System notification subgroup
-- --------------------------------------------------------------
-- 1.3.6.1.4.1.231.7.1.2.2.1.2.3.1
		sheSystemEvents OBJECT IDENTIFIER ::= { sheSystemNotifications 1 }

		
--   Start up end event
-- 1.3.6.1.4.1.231.7.1.2.2.1.2.3.1.1
		sheSystemStartupEnd NOTIFICATION-TYPE
			OBJECTS { sheSystemExternalTime, sheSystemStartupType }
			STATUS current
			DESCRIPTION 
				" End of startup. "
			::= { sheSystemEvents 1 }

		
--  1.3.6.1.4.1.231.7.1.2.2.1.2.3.1.2
		sheSystemFtp NOTIFICATION-TYPE
			OBJECTS { sheSystemFileControlTimeStamp, sheSystemFileControlRequest, sheSystemFileControlTypeOfFile, sheSystemFileControlPathName, sheSystemFileControlFileName, 
				sheSystemFileControlEntPhysicalIndex, sheSystemFileControlLoadStatus, sheSystemFileControlReqResult }
			STATUS current
			DESCRIPTION 
				" Reports the result of a file up/download. 
				The sheSystemFileControlRequest reports either upload / download
				The sheSystemFileControlLoadStatus reports either 
				            finished / ongoing / broken.    
				"
			::= { sheSystemEvents 2 }

		
--  1.3.6.1.4.1.231.7.1.2.2.1.2.3.1.3
		sheSystemSwLoadPromoted NOTIFICATION-TYPE
			OBJECTS { sheSystemFileControlTimeStamp, entPhysicalIndex }
			STATUS current
			DESCRIPTION 
				" The upgraded load is promoted to be good/trusted. "
			::= { sheSystemEvents 3 }

		
--  1.3.6.1.4.1.231.7.1.2.2.1.2.3.1.4
		sheSystemSwLoadPromotionFailed NOTIFICATION-TYPE
			OBJECTS { sheSystemFileControlTimeStamp, entPhysicalIndex, sheSystemFileControlReqResult }
			STATUS current
			DESCRIPTION 
				" Sent when load promotion failed. 
				Card will restart with fallback load. The ReqResult should
				contain some detailed information about the error-reason (tbd.)."
			::= { sheSystemEvents 4 }

		
--   LCT connected event
-- 1.3.6.1.4.1.231.7.1.2.2.1.2.3.1.5
		sheSystemLctSession NOTIFICATION-TYPE
			OBJECTS { sheSystemExternalTime, sheSystemLctConnected }
			STATUS current
			DESCRIPTION 
				" A LCT session is connected or disconnected. "
			::= { sheSystemEvents 5 }

		
--  1.3.6.1.4.1.231.7.1.2.2.1.2.3.1.6
		sheSystemWriteDatatoFlashFinished NOTIFICATION-TYPE
			OBJECTS { sheSystemControlStatus, sheSystemControlReqResult, sheSystemControlTimeStamp }
			STATUS current
			DESCRIPTION 
				"Description."
			::= { sheSystemEvents 6 }

		
--  1.3.6.1.4.1.231.7.1.2.2.1.2.3.1.7
		sheSystemChangedCPULoadThreshold NOTIFICATION-TYPE
			OBJECTS { sheSystemControlStatus, sheSystemControlReqResult, sheSystemControlTimeStamp }
			STATUS current
			DESCRIPTION 
				"CPU Load Threshold change state by user."
			::= { sheSystemEvents 7 }

		
