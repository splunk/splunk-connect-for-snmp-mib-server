		sheSystemNtpClient OBJECT IDENTIFIER ::= { sheSystem 5 }

		
--   --------------------------------------------------------------
-- System NTP Client/Server table 
-- --------------------------------------------------------------
-- 1.3.6.1.4.1.231.7.1.2.2.1.2.5.1
		sheSystemNtpClientEnabled OBJECT-TYPE
			SYNTAX INTEGER
				{
				false(0),
				true(1)
				}
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"NTP Client enable status."
			::= { sheSystemNtpClient 1 }

		
--  1.3.6.1.4.1.231.7.1.2.2.1.2.5.2
		sheSystemNtpClientXmitInterval OBJECT-TYPE
			SYNTAX Integer32 (1..2160)
			UNITS "hour"
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"NTP Client transmit-packet interval to request time information from NTP server..
				Valid range is 1 hour up to 2160 hours (3 months).
				The default value should be 24 hours."
			::= { sheSystemNtpClient 2 }

		
--  1.3.6.1.4.1.231.7.1.2.2.1.2.5.3
		sheSystemNtpClientServerTable OBJECT-TYPE
			SYNTAX SEQUENCE OF SheSystemNtpClientServerEntry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				" NTP Client server Table of SHE."
			::= { sheSystemNtpClient 3 }

		
--  1.3.6.1.4.1.231.7.1.2.2.1.2.5.3.1
		sheSystemNtpClientServerEntry OBJECT-TYPE
			SYNTAX SheSystemNtpClientServerEntry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				" An entry in the sheSystemNtpClientTable.
				Notifications : none. "
			INDEX { sheSystemNtpClientServerIndex }
			::= { sheSystemNtpClientServerTable 1 }

		
		SheSystemNtpClientServerEntry ::=
			SEQUENCE { 
				sheSystemNtpClientServerIndex
					Integer32,
				sheSystemNtpClientServerName
					OCTET STRING
			 }

--  1.3.6.1.4.1.231.7.1.2.2.1.2.5.3.1.1
		sheSystemNtpClientServerIndex OBJECT-TYPE
			SYNTAX Integer32 (1..100)
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				" Index of file table.  "
			::= { sheSystemNtpClientServerEntry 1 }

		
--  1.3.6.1.4.1.231.7.1.2.2.1.2.5.3.1.2
		sheSystemNtpClientServerName OBJECT-TYPE
			SYNTAX OCTET STRING (SIZE (1..64))
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				" Server name of NTP Client in SHE NTP Client system. "
			::= { sheSystemNtpClientServerEntry 2 }

		
