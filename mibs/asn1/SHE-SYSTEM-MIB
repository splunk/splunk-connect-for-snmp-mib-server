-- --------------------------------------------------------------
--       DataBase Management control subgroup
-- --------------------------------------------------------------
		sheSystemDbmControl OBJECT IDENTIFIER ::= { sheSystemDbMaintenance 3 }

		
--  This managed entity manages the control request for storage
-- of the database.
		sheSystemDbmControlRequest OBJECT-TYPE
			SYNTAX SheSystemMaintenanceControlReq
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"  Database Management commands :
				
				setSystemMaintenanceBase:
				- Set needed ftp access information for automatic DB request's.
				notifications: 
				  sheSystemDbmBaseSet
				
				backupDb : 
				- The command for a manual database backup
				  into the FEPROM of the DSLAM and to the FTP server.
				  The return value commExecStarted indicates the beginning
				  of DB storage.
				notification:
				 sheSystemDbmResult
				 
				refreshDbBackupFilesTable :
				- The DSLAM supports several
				  backup files. The command retrieves the backup files 
				  at the FTP server and provides their file names and  
				  time and date of creation in the table sheSystemDbBackFilesTable.
				notification:
				 sheSystemRefreshDbmBackupFilesTable
				 
				dbReloadSpecificFile:
				- Reload a specific file from sheSystemDbBackFilesTable
				  into the DSLAM. If successful, the CXU perform a reset 
				  for activation.
				notification:
				 sheSystemDbmResult
				
				setBackupPeriod:
				- Configure the backup period settings for automatic remote 
				  backup to ftp server and local backup to EEPROM.   
				notification:
				 sheSystemDbmBackupPeriodSet
				
				setLocalBackupAdminState:
				- Enables or disables the automatic local backup feature.
				notification:
				 sheSystemDbmLocalBackupAdminstateSet
				
				setRemoteBackupAdminState:
				- Enables or disables the automatic remote backup feature.
				notification:
				 sheSystemDbmRemoteBackupAdminstateSet
				
				"
			::= { sheSystemDbmControl 1 }

		
		sheSystemDbmControlStatus OBJECT-TYPE
			SYNTAX SheControlStatusType
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				" The status of the control request. "
			::= { sheSystemDbmControl 2 }

		
		sheSystemDbmControlTimer OBJECT-TYPE
			SYNTAX Unsigned32
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				" The maximum wait time for the manager when a DB storage
				request cannot be executed immediately (value commExecStarted
				in the result attribute sheSystemMaintenanceControlReqResult).
				The manager reads this value from the DSLAM.
				The end of request indicates the trap sheSystemTrDbBackup. "
			::= { sheSystemDbmControl 3 }

		
		sheSystemDbmControlReqResultLocal OBJECT-TYPE
			SYNTAX SheSystemMaintenanceControlReqResult
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				" The result of the local backup request. "
			::= { sheSystemDbmControl 4 }

		
		sheSystemDbmControlReqResultRemote OBJECT-TYPE
			SYNTAX SheSystemMaintenanceControlReqResult
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				" The result of the last remote backup request. "
			::= { sheSystemDbmControl 5 }

		
		sheSystemDbmControlTimeStamp OBJECT-TYPE
