		sheSystemIpRouteTable OBJECT-TYPE
			SYNTAX SEQUENCE OF SheSystemIpRouteEntry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"This table contains attributes for each static configured IP route.
				Learned routes from routing protocols will not be shown here!"
			::= { sheSystemIpConfig 3 }

		
		sheSystemIpRouteEntry OBJECT-TYPE
			SYNTAX SheSystemIpRouteEntry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"An entry represents a set of configurations in sheSystemIpRouteTable. 
				"
			INDEX { sheSystemIpRouteIndex }
			::= { sheSystemIpRouteTable 1 }

		
		SheSystemIpRouteEntry ::=
			SEQUENCE { 
				sheSystemIpRouteIndex
					Unsigned32,
				sheSystemIpRouteName
					DisplayString,
				sheSystemIpRouteDestIpAddress
					IpAddress,
				sheSystemIpRouteSubnetMask
					IpAddress,
				sheSystemIpRouteGatewayIpAddress
					IpAddress,
				sheSystemIpRouteMetric
					Integer32
			 }

		sheSystemIpRouteIndex OBJECT-TYPE
			SYNTAX Unsigned32
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"Table index. Each index uniquely identifies one an Ip route table entry."
			::= { sheSystemIpRouteEntry 1 }

		
		sheSystemIpRouteName OBJECT-TYPE
			SYNTAX DisplayString (SIZE (1..32))
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"A describable name that uniquely identifies an  entry of this table.
				The name is user specified.
				Special characters like space ? . are not allowed."
			::= { sheSystemIpRouteEntry 2 }

		
		sheSystemIpRouteDestIpAddress OBJECT-TYPE
			SYNTAX IpAddress
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"Destination IpAddress for this route."
			::= { sheSystemIpRouteEntry 4 }

		
		sheSystemIpRouteSubnetMask OBJECT-TYPE
