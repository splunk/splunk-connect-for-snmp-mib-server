			SYNTAX Unsigned32
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"Number of last sended notification.
				
				This OID is last member of each sended notification from SHE NE. A TMN system should
				now be able to check, if all notification were received and if not a consistency check 
				could be performed.
				"
			::= { sheSystemSnmp 1 }

		
--  Configuration and status information for automatic sw upgrade and db backup controls.
		sheSystemMaintenance OBJECT IDENTIFIER ::= { sheSystem 11 }

		
		sheSystemSwMaintenance OBJECT IDENTIFIER ::= { sheSystemMaintenance 1 }

		
		sheSystemSwMaintenanceBase OBJECT IDENTIFIER ::= { sheSystemSwMaintenance 1 }

		
		sheSystemSwmSapsServerAddr OBJECT-TYPE
			SYNTAX OCTET STRING (SIZE (0..20))
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"Address of SAPS FTP server (dotted notation)."
			::= { sheSystemSwMaintenanceBase 1 }

		
		sheSystemSwmSapsPathName OBJECT-TYPE
			SYNTAX OCTET STRING (SIZE (0..128))
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"Path name of SAPS file on FTP server. 
				This path should be set relative to the ftp-server root.
				Use only linux notation ( / ). 
				
				Spaces in pathnames are not allowed, because of possible problems with
				different FTP implementations (client and/or server)!!"
			::= { sheSystemSwMaintenanceBase 2 }

		
		sheSystemSwmSapsFileName OBJECT-TYPE
			SYNTAX OCTET STRING (SIZE (1..128))
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"File name of SAPS file on FTP server. 
				
				Spaces in filenames are not allowed, because of possible problems with
				different FTP implementations (client and/or server)!!
				"
			::= { sheSystemSwMaintenanceBase 3 }

		
		sheSystemSwmAdminState OBJECT-TYPE
			SYNTAX SheSystemAdminState
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"This state describes the current usage of automatic sw upgrade.
				The following states are supported:
				
				unlocked(1)  -> saps handling used
				locked(2)    -> saps handling not used (default)
				
				Important Note: If the automatic sw upgrade system is enabled, the sheSystemFileControl group
				for single sw upgrades is always busy!! If a sw upgrade should be start for a single Card or a single
				card type, the sheSystemSwmAdminState has to be locked and then the sheSystemFileControl could be used!"
			DEFVAL { 2 }
			::= { sheSystemSwMaintenanceBase 4 }

		
		sheSystemSwmCurrentStatus OBJECT-TYPE
			SYNTAX SheSystemSwmCurrentStatusType
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"This parameter describes the current status of automatic sw upgrade process.
				The following states are supported:
				
				passive(1)	-> saps handling disabled (default)  or 
						    saps handling enabled but last request failed 
				started(2)	-> saps handling enabled, start loading S-APS file
				active(3)	-> saps handling enabled and running
				"
			::= { sheSystemSwMaintenanceBase 5 }

		
--    --------------------------------------------------------------
-- SW Upgrade Task Table
-- --------------------------------------------------------------
-- 
		sheSystemSwMaintenanceTaskTable OBJECT-TYPE
			SYNTAX SEQUENCE OF SheSystemSwMaintenanceTaskEntry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"Table of the current active or failed tasks of SW upgrade.
				This table is used to display the automatic upgrade status for single plugin
				units at the TMN. "
			::= { sheSystemSwMaintenance 2 }

		
--    info-IMPL:    V2.0->
		sheSystemSwMaintenanceTaskEntry OBJECT-TYPE
			SYNTAX SheSystemSwMaintenanceTaskEntry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"Entry of the SW upgrade task table.
				"
			INDEX { sheSystemSwmTIndex }
			::= { sheSystemSwMaintenanceTaskTable 1 }

		
		SheSystemSwMaintenanceTaskEntry ::=
			SEQUENCE { 
				sheSystemSwmTIndex
					Unsigned32,
				sheSystemSwmTCardType
					SheCardType,
				sheSystemSwmTEntPhysicalIndex
					PhysicalIndex,
				sheSystemSwmTTimeStamp
					Unsigned32,
				sheSystemSwmTUpgradeState
					SheSystemMaintenanceControlReqResult,
				sheSystemSwmTUpgradeResult
					SheSystemMaintenanceControlReqResult,
				sheSystemSwmTSwVersion
					OCTET STRING,
				sheSystemSwmTSwVersionNeed
					OCTET STRING
			 }

		sheSystemSwmTIndex OBJECT-TYPE
			SYNTAX Unsigned32 (1..255)
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"Unique value to identify an entry of this table. "
			::= { sheSystemSwMaintenanceTaskEntry 1 }

		
		sheSystemSwmTCardType OBJECT-TYPE
			SYNTAX SheCardType
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				" Indicates the plug-in unit type to be upgraded. "
			::= { sheSystemSwMaintenanceTaskEntry 2 }

		
		sheSystemSwmTEntPhysicalIndex OBJECT-TYPE
			SYNTAX PhysicalIndex
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"The entPhysicalIndex of PIU to be loaded."
			::= { sheSystemSwMaintenanceTaskEntry 3 }

		
		sheSystemSwmTTimeStamp OBJECT-TYPE
			SYNTAX Unsigned32
			UNITS "sec"
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"The time stamp of the task. It is updated anytime the upgrade task state changes. "
			::= { sheSystemSwMaintenanceTaskEntry 4 }

		
		sheSystemSwmTUpgradeState OBJECT-TYPE
			SYNTAX SheSystemMaintenanceControlReqResult
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"Identifies the current processing state of the upgrade task: 
				- sutInitialized
				- sutToDo
				- sutStarted
				- sutCrashed
				- sutFtpDone
				- sutDone
				- sutReadyForReset
				- sutResetInProgress
				- sutObsolete"
			::= { sheSystemSwMaintenanceTaskEntry 5 }

		
		sheSystemSwmTUpgradeResult OBJECT-TYPE
			SYNTAX SheSystemMaintenanceControlReqResult
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"This read-only attribute informs about the result of the performed upgrade
				(in case of failed upgrade the error reason). "
			::= { sheSystemSwMaintenanceTaskEntry 6 }

		
		sheSystemSwmTSwVersion OBJECT-TYPE
			SYNTAX OCTET STRING (SIZE (0..128))
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"String containing the APS version which is loaded into the plug-in unit. "
			::= { sheSystemSwMaintenanceTaskEntry 7 }

		
		sheSystemSwmTSwVersionNeed OBJECT-TYPE
			SYNTAX OCTET STRING (SIZE (0..128))
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"String containing the APS version which should be loaded into the plug-in unit. "
			::= { sheSystemSwMaintenanceTaskEntry 8 }

		
--  --------------------------------------------------------------
-- SW maintenance control subgroup
-- --------------------------------------------------------------
-- This managed entity represents the control group for saps sw maintenance user commands.
-- The following user commands are implemented :
-- 
-- setSystemMaintenanceBase
-- Set needed ftp access information for automatic SAPS request's.
-- 
-- setSwmAdminState
-- Enable/Disable the automatic saps sw maintenance system.
-- 
-- swmUpgrStartRequest:    
-- Start upgrade for the sytem in the background including sw activation.
-- 
-- swmUpgrStartRequestNoReset:    
-- Start upgrade for the sytem in the background without sw activation.
-- To activate new sw versions, the piu's should be made a reset.
-- 
-- swmReadSAPSContentFile : 
-- The SAPS (Sytem Application Software) content file describes an assignment of
-- available SW loads to loadable PIUs inclusively the CXU .
-- If there is a new SAPs content file at the FTP server f.ex. because of bug fix
-- release the crafts person has to request the reading of the SAPS content file
-- so that the information is available in the DSLAM.
-- The SAPS content file is automatically read after reload or after activation
-- of a CXU load.
-- 
-- 
		sheSystemSwMaintenanceControl OBJECT IDENTIFIER ::= { sheSystemSwMaintenance 3 }

		
		sheSystemSwmControlRequest OBJECT-TYPE
			SYNTAX SheSystemMaintenanceControlReq
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"SW upgrade command requests from TMN.
				
				setSystemMaintenanceBase(50), 
				- Set needed ftp access information for automatic SAPS request's.
				notifications: 
				  sheSystemSwmBaseSet
				
				setSwmAdminState(51),
				- lock or unlock the automatic saps system
				notifications:                       
				  sheSystemSwmBaseAdminStateSet
				  sheSystemReadSAPSContentFileReady (if set to unlock)
				  sheSystemSwmFTPError              (if ftp could not reached)
				
				swmUpgrStartRequest(100),
				- start new upgrade procedure including automatic load activation
				notifications:
				 sheSystemSwmFTPError
				 sheSystemSwmUpgradeStartResult
				
				swmUpgrStartRequestNoReset(101),
				- start new upgrade procedure without automatic load activation
				notifications:
				 sheSystemSwmReadyForReset
				 sheSystemSwmFTPError
				 sheSystemUpgradeCanceled (bad case only) 
				 sheSystemSwmUpgradeStartResult
				
				
				swmReadSAPSContentFile(102),
				- start request of new SAPS file from server, no download action is started 
				notification:
				 sheSystemReadSAPSContentFileReady
				 sheSystemSwmFTPError
				
				"
			::= { sheSystemSwMaintenanceControl 1 }

		
		sheSystemSwmControlStatus OBJECT-TYPE
			SYNTAX SheControlStatusType
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				" The status of the current request. "
			::= { sheSystemSwMaintenanceControl 2 }

		
		sheSystemSwmControlTimer OBJECT-TYPE
			SYNTAX Unsigned32
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"Wait time in seconds after complete upgrade for a unit type. "
			::= { sheSystemSwMaintenanceControl 3 }

		
		sheSystemSwmControlReqResult OBJECT-TYPE
			SYNTAX SheSystemMaintenanceControlReqResult
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"The result of the last user command."
			::= { sheSystemSwMaintenanceControl 4 }

		
		sheSystemSwmControlTimeStamp OBJECT-TYPE
			SYNTAX Unsigned32
			UNITS "sec"
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				" The time stamp of the last command (end of command). "
			::= { sheSystemSwMaintenanceControl 5 }

		
		sheSystemSwmControlSapsServerAddr OBJECT-TYPE
			SYNTAX OCTET STRING (SIZE (0..20))
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"Address of SAPS FTP server (dotted notation)."
			::= { sheSystemSwMaintenanceControl 6 }

		
		sheSystemSwmControlSapsPathName OBJECT-TYPE
			SYNTAX OCTET STRING (SIZE (0..128))
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"Path name of SAPS file on FTP server. 
				This path should be set relative to the ftp-server root.
				Use only linux notation ( / ). 
				
				Spaces in pathnames are not allowed, because of possible problems with
				different FTP implementations (client and/or server)!!"
			::= { sheSystemSwMaintenanceControl 7 }

		
		sheSystemSwmControlSapsFileName OBJECT-TYPE
			SYNTAX OCTET STRING (SIZE (1..128))
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"File name of SAPS file on FTP server. 
				
				Spaces in filenames are not allowed, because of possible problems with
				different FTP implementations (client and/or server)!!
				"
			::= { sheSystemSwMaintenanceControl 8 }

		
		sheSystemSwmControlSapsUsrName OBJECT-TYPE
			SYNTAX OCTET STRING (SIZE (0..20))
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"Username for SAPS FTP access.                                                             
				
				Read-access to this attribute will deliver 20 times sign '-'  "
			::= { sheSystemSwMaintenanceControl 9 }

		
		sheSystemSwmControlSapsPassword OBJECT-TYPE
			SYNTAX OCTET STRING (SIZE (0..20))
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"Password for SAPS FTP access.                                                              
				
				Read-access to this attribute will deliver 20 times sign '-'  "
			::= { sheSystemSwMaintenanceControl 10 }

		
		sheSystemSwmControlAdminState OBJECT-TYPE
			SYNTAX SheSystemAdminState
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"This set the current usage of automatic sw upgrade.
				The following states are supported:
				
				unlocked(1)  -> saps handling used
				locked(2)    -> saps handling not used (default)
				
				"
			::= { sheSystemSwMaintenanceControl 11 }

		
		sheSystemSwMaintenanceNotifications OBJECT IDENTIFIER ::= { sheSystemSwMaintenance 4 }

		
		sheSystemSwmBaseSet NOTIFICATION-TYPE
			OBJECTS { sheSystemSwmControlStatus, sheSystemSwmControlReqResult, sheSystemSwmControlTimeStamp }
			STATUS current
			DESCRIPTION 
				"Configuration trap for SAPS basic ftp access settings."
			::= { sheSystemSwMaintenanceNotifications 1 }

		
		sheSystemSwmAdminStateSet NOTIFICATION-TYPE
			OBJECTS { sheSystemSwmControlStatus, sheSystemSwmControlReqResult, sheSystemSwmControlTimeStamp, sheSystemSwmAdminState }
			STATUS current
			DESCRIPTION 
				"Configuration trap for SAPS basic admin state settings."
			::= { sheSystemSwMaintenanceNotifications 2 }

		
		sheSystemReadSAPSContentFileReady NOTIFICATION-TYPE
			OBJECTS { sheSystemSwmControlStatus, sheSystemSwmControlReqResult, sheSystemSwmControlTimeStamp }
			STATUS current
			DESCRIPTION 
				"Result trap for request of new SAPS file from server."
			::= { sheSystemSwMaintenanceNotifications 3 }

		
		sheSystemSwmReadyForReset NOTIFICATION-TYPE
			OBJECTS { sheSystemSwmControlStatus, sheSystemSwmControlReqResult, sheSystemSwmControlTimeStamp, sheSystemSwmTEntPhysicalIndex }
			STATUS current
			DESCRIPTION 
				"Trap will be send after succesfull SwUpgrade for a single card if a sw upgrade
				was started without automatic load activation."
			::= { sheSystemSwMaintenanceNotifications 4 }

		
		sheSystemSwmFTPError NOTIFICATION-TYPE
			OBJECTS { sheSystemSwmControlStatus, sheSystemSwmControlReqResult, sheSystemSwmControlTimeStamp }
			STATUS current
			DESCRIPTION 
				"Result trap for sw upgrade FTP access."
			::= { sheSystemSwMaintenanceNotifications 5 }

		
		sheSystemSwmUpgradeStartResult NOTIFICATION-TYPE
			OBJECTS { sheSystemSwmControlRequest, sheSystemSwmControlStatus, sheSystemSwmControlReqResult, sheSystemSwmControlTimeStamp }
			STATUS current
			DESCRIPTION 
				"Configuration result trap for long running command."
			::= { sheSystemSwMaintenanceNotifications 6 }

		
		sheSystemSwmCurrentStatusChanged NOTIFICATION-TYPE
			OBJECTS { sheSystemSwmControlTimeStamp, sheSystemSwmCurrentStatus }
			STATUS current
			DESCRIPTION 
				"The current operational status of the automatic sw upgrade process changed."
			::= { sheSystemSwMaintenanceNotifications 7 }

		
		sheSystemSwMaintenanceAlarms OBJECT IDENTIFIER ::= { sheSystemSwMaintenance 5 }

		
		sheSystemSwmFtpAccessError NOTIFICATION-TYPE
			OBJECTS { sheAlarmTimeAndDate, sheAlarmReportingSource, sheAlarmReportingSourceType, sheAlarmSeverity }
			STATUS current
			DESCRIPTION 
				"Alarm will be set because of failures during read or write during the NE internal 
				FTP communication. A successful ftp process will clear this alarm.
				
				Alarm will be used by manual download and automatic saps upgrade."
			::= { sheSystemSwMaintenanceAlarms 1 }

		
		sheSystemSwmFtpExternalAccessError NOTIFICATION-TYPE
			OBJECTS { sheAlarmTimeAndDate, sheAlarmReportingSource, sheAlarmReportingSourceType, sheAlarmSeverity }
			STATUS current
			DESCRIPTION 
				"Alarm will be set because of failures during read or write during the NE external 
				FTP communication (access to external ftp server failed).
				A successful ftp process will clear this alarm.
				
				Alarm will be used by manual download and automatic saps upgrade."
			::= { sheSystemSwMaintenanceAlarms 2 }

		
		sheSystemSwmStatusError NOTIFICATION-TYPE
			OBJECTS { sheAlarmTimeAndDate, sheAlarmReportingSource, sheAlarmReportingSourceType, sheAlarmSeverity }
			STATUS current
			DESCRIPTION 
				"Alarm will be set because of failures during automatic sw upgrade process which
				force a temporary switch off for this process.
				
				A restart could be triggered by the following command's:
				setSwmAdminState(51)
				swmUpgrStartRequest(100)
				swmUpgrStartRequestNoReset(101)
				"
			::= { sheSystemSwMaintenanceAlarms 3 }

		
		sheSystemDbMaintenance OBJECT IDENTIFIER ::= { sheSystemMaintenance 2 }

		
		sheSystemDbMaintenanceBase OBJECT IDENTIFIER ::= { sheSystemDbMaintenance 1 }

		
		sheSystemDbmServerAddr OBJECT-TYPE
			SYNTAX OCTET STRING (SIZE (0..20))
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"Address of DB FTP server (dotted notation)."
			::= { sheSystemDbMaintenanceBase 1 }

		
		sheSystemDbmPathName OBJECT-TYPE
			SYNTAX OCTET STRING (SIZE (0..128))
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"Path name of DB backup files on FTP server. 
				This path should be set relative to the ftp-server root.
				Use only linux notation ( / ). 
				
				Spaces in pathnames are not allowed, because of possible problems with
				different FTP implementations (client and/or server)!!"
			::= { sheSystemDbMaintenanceBase 2 }

		
		sheSystemDbmRemoteBackupAdminState OBJECT-TYPE
			SYNTAX SheSystemAdminState
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"This state describes the current usage of automatic remote db backup.
				The following states are supported:
				
				unlocked(1)  -> remote db backup handling used
				locked(2)    -> remote db backup handling not used (default)"
			DEFVAL { 2 }
			::= { sheSystemDbMaintenanceBase 3 }

		
		sheSystemDbmBackupStartTime OBJECT-TYPE
			SYNTAX Unsigned32 (0..1439)
			UNITS "minute"
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"Start time basis for remote DB backup after system restart, backup timer 
				reconfigurations or system clock reset.
				The first backup will be started at this time + sheSystemDbmBackupPeriod and
				will then be repeated each sheSystemDbmBackupPeriod.
				Range: 0 (0 am) .. 1439 (23.59 pm). 
				Default: 0  
				Reminder: NE uses always a UTC time base."
			DEFVAL { 0 }
			::= { sheSystemDbMaintenanceBase 4 }

		
		sheSystemDbmBackupPeriod OBJECT-TYPE
			SYNTAX Unsigned32 (1..48)
			UNITS "hour"
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"Interval between two remote backup operations in hours. 
				Backup is started only if there were relevant configuration
				changes during the last period.
				Default: 24 hours "
			DEFVAL { 24 }
			::= { sheSystemDbMaintenanceBase 5 }

		
		sheSystemDbmBackupWaitingPeriod OBJECT-TYPE
			SYNTAX Unsigned32 (1..59)
			UNITS "minute"
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"Waiting period in minutes before next local backup, in case of configuration changes.
				Default value : 5 minutes. "
			DEFVAL { 5 }
			::= { sheSystemDbMaintenanceBase 6 }

		
		sheSystemDbmBackupMaxFileNumber OBJECT-TYPE
			SYNTAX Unsigned32 (1..32)
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"This number describes the maximum set of different backup files on the remote ftp site.
				Default number is 5."
			DEFVAL { 5 }
			::= { sheSystemDbMaintenanceBase 7 }

		
		sheSystemDbmLocalBackupAdminState OBJECT-TYPE
			SYNTAX SheSystemAdminState
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"This value administrate the automatic local db backup after sheSystemDbmBackupWaitingPeriod
				in case of configuration actions.
				Default: unlocked  (automatic local backup is enabled)"
			DEFVAL { 1 }
			::= { sheSystemDbMaintenanceBase 8 }

		
		sheSystemDbBackupFilesTable OBJECT-TYPE
			SYNTAX SEQUENCE OF SheSystemDbBackupFilesEntry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				" Table shows the DB backup files at the FTP server 
				with time and date of their creation.
				The table is empty per default and can be updated 
				with user command refreshDbBackupFilesTable.
				This command searches all existing DB backup files
				at the FTP server. "
			::= { sheSystemDbMaintenance 2 }

		
--   info-IMPL:    V3.0->
		sheSystemDbBackupFilesEntry OBJECT-TYPE
			SYNTAX SheSystemDbBackupFilesEntry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				" Entry of the xldv20DbBackupFilesTable.
				Notifications :  . "
			INDEX { sheSystemDbBackupFileIndex }
			::= { sheSystemDbBackupFilesTable 1 }

		
		SheSystemDbBackupFilesEntry ::=
			SEQUENCE { 
				sheSystemDbBackupFileIndex
					Unsigned32,
				sheSystemDbBackupFileName
					DisplayString,
				sheSystemDbBackupFileTimeStamp
					Unsigned32
			 }

		sheSystemDbBackupFileIndex OBJECT-TYPE
			SYNTAX Unsigned32 (1..32)
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"Unique value to identify an entry of this table. "
			::= { sheSystemDbBackupFilesEntry 1 }

		
		sheSystemDbBackupFileName OBJECT-TYPE
			SYNTAX DisplayString (SIZE (0..128))
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				" The name of the DB backup file. "
			::= { sheSystemDbBackupFilesEntry 2 }

		
		sheSystemDbBackupFileTimeStamp OBJECT-TYPE
			SYNTAX Unsigned32
			UNITS "sec"
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				" The time stamp of the DB backup file, i.e. the time and
				date when the DB backup file has been created. "
			::= { sheSystemDbBackupFilesEntry 3 }

		
