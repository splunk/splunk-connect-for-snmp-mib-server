-- New supported with SHE R2.6.
		sheSystemIpConfig OBJECT IDENTIFIER ::= { sheSystem 12 }

		
		sheSystemIpHostTable OBJECT-TYPE
			SYNTAX SEQUENCE OF SheSystemIpHostEntry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"This table contains attributes for each IP host interface."
			::= { sheSystemIpConfig 1 }

		
		sheSystemIpHostEntry OBJECT-TYPE
			SYNTAX SheSystemIpHostEntry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"An entry represents a set of configurations in sheSystemIpHostTable. 
				"
			INDEX { sheSystemIpHostIndex }
			::= { sheSystemIpHostTable 1 }

		
		SheSystemIpHostEntry ::=
			SEQUENCE { 
				sheSystemIpHostIndex
					Unsigned32,
				sheSystemIpHostName
					DisplayString,
				sheSystemIpHostUseDhcp
					SheSystemIpHostUseDhcpType,
				sheSystemIpHostIpAddress
					IpAddress,
				sheSystemIpHostSubnetMask
					IpAddress,
				sheSystemIpHostOuterVlanId
					Unsigned32,
				sheSystemIpHostInnerVlanId
					Unsigned32,
				sheSystemIpHostDescription
					OCTET STRING,
				sheSystemIpHostOuterVlanPriority
					Unsigned32,
				sheSystemIpHostInnerVlanPriority
					Unsigned32
			 }

		sheSystemIpHostIndex OBJECT-TYPE
			SYNTAX Unsigned32
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"Table index. Each index uniquely identifies one Ip host table entry."
			::= { sheSystemIpHostEntry 1 }

		
		sheSystemIpHostName OBJECT-TYPE
			SYNTAX DisplayString (SIZE (1..32))
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"A descriptional name that uniquely identifies a entry of this table.
				The name is selected by the NE and identify the interface also on CLI."
			::= { sheSystemIpHostEntry 2 }

		
		sheSystemIpHostUseDhcp OBJECT-TYPE
			SYNTAX SheSystemIpHostUseDhcpType
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"Either to use Dhcp or not. If set to 0 (default) no DHCP will be used.
				Valid values:
				noUseDhcp(0),
				useDhcp(1)
				
				Note: Currently no support planed."
			DEFVAL { noUseDhcp }
			::= { sheSystemIpHostEntry 3 }

		
		sheSystemIpHostIpAddress OBJECT-TYPE
			SYNTAX IpAddress
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"IpAddress. Assigned by operator request or by DHCP if used."
