--   --------------------------------------------------------------
-- System file table 
-- --------------------------------------------------------------
-- 1.3.6.1.4.1.231.7.1.2.2.1.2.2.2
		sheSystemFileSwFileTable OBJECT-TYPE
			SYNTAX SEQUENCE OF SheSystemFileSwFileEntry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				" File Table of SHE. 
				List of all files per card present on the SHE and their 
				counterpart on the FTP server
				
				Information about Primary and Secondary 
				loads is also found in this table. "
			::= { sheSystemFile 2 }

		
--  1.3.6.1.4.1.231.7.1.2.2.1.2.2.2.1
		sheSystemFileSwFileEntry OBJECT-TYPE
			SYNTAX SheSystemFileSwFileEntry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				" An entry in the sheSystemFileSwTable.
				Notifications : none. "
			INDEX { entPhysicalIndex, sheSystemFileSwfIndex }
			::= { sheSystemFileSwFileTable 1 }

		
		SheSystemFileSwFileEntry ::=
			SEQUENCE { 
				sheSystemFileSwfIndex
					SheSystemFileIndexType,
				sheSystemFileSwfFileName
					OCTET STRING,
				sheSystemFileSwfTypeOfFile
					SheSystemTypeOfFile,
				sheSystemFileSwfVersion
					OCTET STRING,
				sheSystemFileSwfSize
					Integer32,
				sheSystemFileSwfTimeStamp
					Unsigned32
			 }

--  1.3.6.1.4.1.231.7.1.2.2.1.2.2.2.1.1
		sheSystemFileSwfIndex OBJECT-TYPE
			SYNTAX SheSystemFileIndexType
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				" Second Index of file table.  "
			::= { sheSystemFileSwFileEntry 1 }

		
--  1.3.6.1.4.1.231.7.1.2.2.1.2.2.2.1.2
		sheSystemFileSwfFileName OBJECT-TYPE
			SYNTAX OCTET STRING (SIZE (0..128))
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				" File name of file in SHE file system. "
			::= { sheSystemFileSwFileEntry 2 }

		
--  1.3.6.1.4.1.231.7.1.2.2.1.2.2.2.1.3
		sheSystemFileSwfTypeOfFile OBJECT-TYPE
			SYNTAX SheSystemTypeOfFile
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				" The file type definition: 
				1..stored load,    (only download) 
				2..error log,      (only upload)
				3..restoreFile     (both up/download)
				4..remoteInventory (only upload, AllOfSystem possible)
				5..performanceData (only upload)
				6..alarmLog        (only upload)
				7..uptimeLog       (only upload)"
			::= { sheSystemFileSwFileEntry 3 }

		
--  1.3.6.1.4.1.231.7.1.2.2.1.2.2.2.1.4
		sheSystemFileSwfVersion OBJECT-TYPE
			SYNTAX OCTET STRING (SIZE (0..20))
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				" File or software version of file in the SHE file system. "
			::= { sheSystemFileSwFileEntry 4 }

		
--  1.3.6.1.4.1.231.7.1.2.2.1.2.2.2.1.5
