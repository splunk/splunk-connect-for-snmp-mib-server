			STATUS current
			DESCRIPTION
				"subnet mask"
			::= { sheSystemIpHostEntry 5 }

		
		sheSystemIpHostOuterVlanId OBJECT-TYPE
			SYNTAX Unsigned32 (0 | 1..4093)
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"The host vlan for this ip interface which should be used as outer tag in double tagging mode
				or in single tagging mode.
				Set to 0 if no host VLAN should be used."
			DEFVAL { 0 }
			::= { sheSystemIpHostEntry 6 }

		
		sheSystemIpHostInnerVlanId OBJECT-TYPE
			SYNTAX Unsigned32 (0 | 1..4093)
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
