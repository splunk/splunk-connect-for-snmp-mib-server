			INTEGER,
			sonetPortLineCVS15Min
			INTEGER,
			sonetPortPathCVS15Min
			INTEGER,
			sonetPortSectCVS1Day
			INTEGER,
			sonetPortLineCVS1Day
			INTEGER,
			sonetPortPathCVS1Day
			INTEGER,
			sonetPortSectSESS15Min
			INTEGER,
			sonetPortLineSESS15Min
			INTEGER,
			sonetPortPathSESS15Min
			INTEGER,
