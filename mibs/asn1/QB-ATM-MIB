	QB-ATM-MIB DEFINITIONS ::= BEGIN

	IMPORTS 
	OBJECT-IDENTITY, MODULE-IDENTITY, OBJECT-TYPE, Counter32, IpAddress, Integer32
		 FROM SNMPv2-SMI 
	atmTrafficDescrParamEntry, atmVcCrossConnectEntry,
	atmVpCrossConnectEntry, atmInterfaceConfEntry,
	atmVclEntry, atmVplEntry
		FROM ATM-MIB 
	AtmAddr, AtmConnKind, AtmConnCastType,
        AtmServiceCategory, AtmTrafficDescrParamIndex,
        AtmVpIdentifier, AtmVcIdentifier,
        AtmVorXAdminStatus, AtmVorXLastChange,
        AtmVorXOperStatus, atmNoClpNoScr
            FROM ATM-TC-MIB
	TEXTUAL-CONVENTION,  DisplayString, RowStatus, TruthValue
        	FROM SNMPv2-TC
    InterfaceIndex, InterfaceIndexOrZero, ifIndex
		FROM IF-MIB
	MODULE-COMPLIANCE, OBJECT-GROUP
		FROM SNMPv2-CONF
	TimeStamp	
		FROM QBSYS-SYSTEM-MIB
	qbMibs
		FROM QUANTUMBRIDGE-REG
	QbEnableStatus,QbPvcConnKind
		FROM QB-DWS-MIB
	;    

    qbAtmMIB MODULE-IDENTITY
	    LAST-UPDATED "0101022155Z" -- January 2, 2001
	    ORGANIZATION "Quantum Bridge Inc."
	    CONTACT-INFO
		"mvaysman@quantumbridge.com"
	DESCRIPTION
		"This module defines additional objects for management of ATM 
		links in Quantum Bridge devices, above and beyond what is defined in the
		standard ATM mib, and proposed drafts."
    ::= { qbMibs 7 }

-- Top-level organization of the MIB

   qbAtmObjects          OBJECT IDENTIFIER ::= { qbAtmMIB 1 }
   qbAtmConformance      OBJECT IDENTIFIER ::= { qbAtmMIB 2 }
   qbAtmBaseGroup	 OBJECT IDENTIFIER ::= { qbAtmMIB 3 }

-- a truth value
   Boolean ::=   INTEGER { true(1), false(2) }

 QbAtmPnniWeight ::= TEXTUAL-CONVENTION
        STATUS        current
        DESCRIPTION
            "The ATM administrative weight of the ATM interface used
	    by the PNNI Routing Control"
        SYNTAX        INTEGER (100..1000000)   
        
 QbAtmPnniNodeId ::= TEXTUAL-CONVENTION
        STATUS        current
        DESCRIPTION
            "Global PNNI Node Id used in the QB PNNI system."
        SYNTAX        OCTET STRING (SIZE (22))
      

  QbAtmSigSpec ::= TEXTUAL-CONVENTION
        STATUS        current
        DESCRIPTION
            "The ATM interface signalling spec."
        SYNTAX        INTEGER { 	
		        other(1),
			uni30(2),
			uni31(3),
			uni40(4),
			pnni(5),
			iisp30(6),
			iisp31(7),
			aini(8)
			}

  QbAtmPnniHelloState ::= TEXTUAL-CONVENTION    
  		STATUS	current
		DESCRIPTION
            "The state of an instance of the PNNI Hello State machine."
        REFERENCE
            "ATM Forum PNNI 1.0 Section 5.6.2.1.2"
        SYNTAX        INTEGER {
                                notApplicable(1),
                                down(2),
                                attempt(3),
                                oneWayInside(4),
                                twoWayInside(5),
                                oneWayOutside(6),
                                twoWayOutside(7),
                                commonOutside(8)
                                }

    QbAtmPnniAtmAddr ::= TEXTUAL-CONVENTION
        STATUS        current
        DESCRIPTION
            "The ATM address used by the network entity.  The address
             types are: no address (0 octets), and NSAP (20 octets)."
        REFERENCE
            "ATM Forum PNNI 1.0 Section 5.2"
        SYNTAX        OCTET STRING (SIZE(0|20))


    QbAtmPnniNodeIndex ::= TEXTUAL-CONVENTION
        STATUS        current
        DESCRIPTION
            "An index that identifies a logical PNNI entity within the
             managed system.

             The distinguished value zero indicates the null instance or
             no instance in the qbAtmPnniNodeCfgParentNodeIndex.  In all
             other cases, the distinguished value zero indicates a
             logical entity within the switching system that manages
             routes only over non-PNNI interfaces.

             By default, only the node identified by node index one is
             created, and all PNNI interfaces are associated with that
             node."
        SYNTAX        Integer32 (0..65535)

    QbAtmPnniPeerGroupId ::= TEXTUAL-CONVENTION
        STATUS        current
        DESCRIPTION
            "A PNNI peer group ID."
        REFERENCE
            "ATM Forum PNNI 1.0 Section 5.3.2"
        SYNTAX        OCTET STRING (SIZE(14))
 

  QbF4F5LoopbackStatus 	::= TEXTUAL-CONVENTION
		STATUS          current
   		DESCRIPTION	"in-progress(1) or success(2) or
				timeout(3)." 
    		SYNTAX          INTEGER {
					in-progress(1),
					success(2),
					timeout(3)	
					}

 QbF4F5Status 	::= TEXTUAL-CONVENTION
		STATUS          current
   		DESCRIPTION	
   		   "Definition of F4/F5 OperStatus"	
    		SYNTAX          BITS {
    		                      cc-seg(0),
    		                      cc-ete(1),
    		                      rdi-seg(2),
    		                      rdi-ete(3),
    		                      ais-seg(4),
    		                      ais-ete(5),
					good(6),
					unavailable(7)
					}			                					

-- **************************************************************
-- General QB ATM System wide group
-- **************************************************************

   qbAtmF4F5LLID OBJECT-TYPE
	SYNTAX      OCTET STRING (SIZE(1..16))
	MAX-ACCESS  read-write
	STATUS      current
	DESCRIPTION
		"This object stores Loopback Location ID (LLID)."
    ::= {  qbAtmBaseGroup  1 }

  qbAtmDefaultPCBE OBJECT-TYPE
	SYNTAX      INTEGER (10..70)
	MAX-ACCESS  read-write
	STATUS      current
	DESCRIPTION
		"This object stores the default atm cell tax to be used by the system."
    ::= {  qbAtmBaseGroup  2 }
    
-- **************************************************************
-- Extensions to the ATM Interface Configuration Parameters Group
-- **************************************************************
  
    qbAtmInterfaceConfTable       OBJECT-TYPE
	SYNTAX      SEQUENCE OF QbAtmInterfaceConfEntry
	MAX-ACCESS  not-accessible
	STATUS      current
	DESCRIPTION
            "The Quantum Bridge ATM Interface Configuration Table contains 
             extensions to the ATM Interface Configuration Table,
             one entry per ATM interface."
    ::= {  qbAtmObjects 1  }
 
   qbAtmInterfaceConfEntry        OBJECT-TYPE
	SYNTAX      QbAtmInterfaceConfEntry
	MAX-ACCESS  not-accessible
	STATUS      current
	DESCRIPTION
            "An entry in the Quantum ATM Interface Configuration
	    Table. Every ATM interface contains a corresponding
	    entry in the table."
    AUGMENTS    { atmInterfaceConfEntry }
    ::= { qbAtmInterfaceConfTable 1 }

   QbAtmInterfaceConfEntry ::=
	SEQUENCE {
		qbAtmInterfaceConfPolicingStatus INTEGER,
		qbAtmInterfaceConfMaxPvcVci	 INTEGER,
		qbAtmInterfaceConfMinPvcVci	 INTEGER,
		qbAtmInterfaceConfMaxPvcVpi	 INTEGER,
		qbAtmInterfaceConfMinPvcVpi	 INTEGER,
		qbAtmInterfaceConfMaxPvpVpi	 INTEGER,
		qbAtmInterfaceConfMinPvpVpi	 INTEGER
    }

    qbAtmInterfaceConfPolicingStatus OBJECT-TYPE
        SYNTAX        INTEGER {
		      enable(1),
		      disable(2)
	}
        MAX-ACCESS    read-write
        STATUS        current
        DESCRIPTION
            "If this object is set to 'enable' the policing is enabled on this
	    interface. If the object set to 'disable,' the policing is
	    disabled. Note: policing can be set on the S622 and 155
	    cards  only."
	DEFVAL  { disable }
        ::= { qbAtmInterfaceConfEntry  1 }

   qbAtmInterfaceConfMaxPvcVpi OBJECT-TYPE
        SYNTAX        INTEGER 
        MAX-ACCESS    read-only
        STATUS        current
        DESCRIPTION
            "This object indicates the maximum value of VPI that can be
	    used to create a VC on a given ATM native interface."
        ::= { qbAtmInterfaceConfEntry  2 }

   qbAtmInterfaceConfMinPvcVpi OBJECT-TYPE
        SYNTAX        INTEGER 
        MAX-ACCESS    read-only
        STATUS        current
        DESCRIPTION
            "This object indicates the minimum value of VPI that can be
	    used to create a VC on a given ATM native interface."
        ::= { qbAtmInterfaceConfEntry  3 }

 qbAtmInterfaceConfMaxPvcVci OBJECT-TYPE
        SYNTAX        INTEGER 
        MAX-ACCESS    read-only
        STATUS        current
        DESCRIPTION
            "This object indicates the maximum value of VCI that can be
	    used to create a VC on a given ATM native interface."
        ::= { qbAtmInterfaceConfEntry  4 }

  qbAtmInterfaceConfMinPvcVci OBJECT-TYPE
        SYNTAX        INTEGER 
        MAX-ACCESS    read-only
        STATUS        current
        DESCRIPTION
            "This object indicates the minimum value of VCI that can be
	    used to create a VC on a given ATM native interface."
        ::= { qbAtmInterfaceConfEntry  5 }

 qbAtmInterfaceConfMaxPvpVpi OBJECT-TYPE
        SYNTAX        INTEGER 
        MAX-ACCESS    read-only
        STATUS        current
        DESCRIPTION
            "This object indicates the maximum value of VPI that can be
	    used to create a VP on a given ATM native interface."
        ::= { qbAtmInterfaceConfEntry  6 }

  qbAtmInterfaceConfMinPvpVpi OBJECT-TYPE
        SYNTAX        INTEGER 
        MAX-ACCESS    read-only
        STATUS        current
        DESCRIPTION
            "This object indicates the minimum value of VPI that can be
	    used to create a VP on a given ATM native interface."
        ::= { qbAtmInterfaceConfEntry  7 }
 
-- **************************************************************
-- Extensions to the ATM Traffic Descriptor Parameter Table
-- **************************************************************

    qbAtmTrafficDescrParamTable      OBJECT-TYPE
	SYNTAX      SEQUENCE OF QbAtmTrafficDescrParamEntry
	MAX-ACCESS  not-accessible
	STATUS      current
	DESCRIPTION
            "The Quantum Bridge ATM Traffic Descriptor Table contains 
             extensions to the ATM Traffic Descriptor Table."
    ::= {  qbAtmObjects 2 }
 
    qbAtmTrafficDescrParamEntry   OBJECT-TYPE
	SYNTAX      QbAtmTrafficDescrParamEntry
	MAX-ACCESS  not-accessible
	STATUS      current
	DESCRIPTION
            "An entry in the Quantum Bridge ATM Traffic Descriptor Table."
	AUGMENTS    { atmTrafficDescrParamEntry }
    ::= { qbAtmTrafficDescrParamTable  1 }

     QbAtmTrafficDescrParamEntry ::=
	SEQUENCE {
		 qbAtmTrafficDescrParamName		DisplayString,
		 qbAtmTrafficDescrParamDefault		Boolean
	}

    qbAtmTrafficDescrParamName OBJECT-TYPE
	SYNTAX      DisplayString (SIZE (0..20))
	MAX-ACCESS  read-create
	STATUS      current
	DESCRIPTION
		"A textual description of the traffic descriptor."
	DEFVAL  { "" }
    ::= { qbAtmTrafficDescrParamEntry 1 }

   qbAtmTrafficDescrParamDefault OBJECT-TYPE
	SYNTAX      Boolean
	MAX-ACCESS  read-only
	STATUS      current
	DESCRIPTION
		"The object indicates whether or not this is a default 
		traffic descriptor.
		Traffic descriptor with indexes from 1 to 20
		are default traffic descriptors and cannot be 
		deleted."
	DEFVAL  { false }
    ::= { qbAtmTrafficDescrParamEntry 2 }
    
-- ****************************************************************
-- Extension to the ATM VCL Table 
-- ****************************************************************

    qbAtmVclTable        OBJECT-TYPE
	SYNTAX      SEQUENCE OF QbAtmVclEntry
	MAX-ACCESS  not-accessible
	STATUS      current
	DESCRIPTION
		    "The Quantum Bridge Table contains an extension to
		    the atmVclConnectTable,		
		    An entry in this table models VCLs."
    ::= {  qbAtmObjects 3 }
 
    qbAtmVclEntry   OBJECT-TYPE
	SYNTAX      QbAtmVclEntry
	MAX-ACCESS  not-accessible
	STATUS      current
	DESCRIPTION
            "An entry in the QB ATM atmVclConnectTable."
	AUGMENTS    {  atmVclEntry }
    ::= { qbAtmVclTable  1 }

   QbAtmVclEntry  ::=
	SEQUENCE {
	qbAtmVclLoopback		QbEnableStatus,
	qbAtmVclF4F5LoopbackInsert	OCTET STRING,
	qbAtmVclF4F5LoopbackStatus	QbF4F5LoopbackStatus,
	qbAtmVclF4F5LoopbackTimestamp	TimeStamp,
	qbAtmVclMgmtIpAddr		IpAddress,
	qbAtmVclMgmtIpMask		IpAddress,
	qbAtmVclMgmtNeighborIP		IpAddress,
	qbAtmVclConnKind		QbPvcConnKind,	 
	qbAtmVclF4F5TxStatus		QbF4F5Status,
	qbAtmVclF4F5TxLastChange	TimeStamp,
	qbAtmVclF4F5RxStatus		QbF4F5Status,
	qbAtmVclF4F5RxLastChange	TimeStamp        
    }

   qbAtmVclLoopback OBJECT-TYPE
	SYNTAX      QbEnableStatus
	MAX-ACCESS  read-write
	STATUS      current
	DESCRIPTION
		"Enable loopback on this ATM VCL"
	DEFVAL  { disable }
    ::= {  qbAtmVclEntry  1 }

   qbAtmVclF4F5LoopbackInsert	OBJECT-TYPE
	SYNTAX      OCTET STRING (SIZE(16))
	MAX-ACCESS  read-write
	STATUS      current
	DESCRIPTION
		"This is a loopback Insert object for LLID.  
		 Start loopback Insert by setting this to an LLID value."
    ::= {  qbAtmVclEntry  2 }

    qbAtmVclF4F5LoopbackStatus	OBJECT-TYPE
	SYNTAX	QbF4F5LoopbackStatus
	MAX-ACCESS	read-only
	STATUS	current
	DESCRIPTION
		"Status of loopback Insert command: in-progress, success, timeout."
	::= { qbAtmVclEntry 3 }

    qbAtmVclF4F5LoopbackTimestamp	OBJECT-TYPE
	SYNTAX	TimeStamp
	MAX-ACCESS	read-only
	STATUS	current
	DESCRIPTION
		"Value of sysUptime when last Insert completed."
	::= { qbAtmVclEntry 4 }

   qbAtmVclMgmtIpAddr OBJECT-TYPE
        SYNTAX  IpAddress
        MAX-ACCESS  read-write
        STATUS  current
        DESCRIPTION
            "Active IP Address for in-band Management SNMP of this chassis."
    ::= { qbAtmVclEntry 5 }

    qbAtmVclMgmtIpMask OBJECT-TYPE
        SYNTAX  IpAddress
        MAX-ACCESS  read-write
        STATUS  current
        DESCRIPTION
            "This is the IP network mask associated with the in-band
	    Management interface." 
    ::= { qbAtmVclEntry 6 }  

   qbAtmVclMgmtNeighborIP OBJECT-TYPE
        SYNTAX  IpAddress
        MAX-ACCESS  read-write
        STATUS  current
        DESCRIPTION
                "This is the neighbor IP address associated with the in-band 
                  Management interface."
    ::= { qbAtmVclEntry 7 }

  qbAtmVclConnKind   OBJECT-TYPE
    SYNTAX        QbPvcConnKind 
    MAX-ACCESS    read-only
    STATUS        current
    DESCRIPTION
	 "The VCL connection type."
    ::= { qbAtmVclEntry 8 }

 qbAtmVclF4F5TxStatus   OBJECT-TYPE
    SYNTAX        QbF4F5Status 
    MAX-ACCESS    read-only
    STATUS        current
    DESCRIPTION
	 "The transmitted OAM status for the PVC."
    ::= { qbAtmVclEntry 9 }

 qbAtmVclF4F5TxLastChange   OBJECT-TYPE
    SYNTAX        TimeStamp 
    MAX-ACCESS    read-only
    STATUS        current
    DESCRIPTION
	 "The value of sysUPtime when qbAtmVclF4F5TxStatus last changed."
    ::= { qbAtmVclEntry 10 }

 qbAtmVclF4F5RxStatus   OBJECT-TYPE
    SYNTAX        QbF4F5Status 
    MAX-ACCESS    read-only
    STATUS        current
    DESCRIPTION
	 "The received OAM status of the PVC."
    ::= { qbAtmVclEntry 11 }

 qbAtmVclF4F5RxLastChange   OBJECT-TYPE
    SYNTAX        TimeStamp  
    MAX-ACCESS    read-only
    STATUS        current
    DESCRIPTION
	 "The value of sysUPtime when qbAtmVclF4F5TxStatus last changed."
    ::= { qbAtmVclEntry 12 }


-- ****************************************************************
-- Extension to the ATM VPL Table 
-- ****************************************************************

    qbAtmVplTable        OBJECT-TYPE
	SYNTAX      SEQUENCE OF QbAtmVplEntry
	MAX-ACCESS  not-accessible
	STATUS      current
	DESCRIPTION
		    "The Quantum Bridge Table contains an extension to
		    the atmVclConnectTable,		
		    An entry in this table models VPLs."
    ::= {  qbAtmObjects 4 }
 
    qbAtmVplEntry   OBJECT-TYPE
	SYNTAX      QbAtmVplEntry
	MAX-ACCESS  not-accessible
	STATUS      current
	DESCRIPTION
            "An entry in the QB ATM qbAtmVpTable."
	AUGMENTS    {  atmVplEntry }
    ::= { qbAtmVplTable  1 }

   QbAtmVplEntry  ::=
	SEQUENCE {
	qbAtmVplLoopback		QbEnableStatus,
	qbAtmVplF4F5LoopbackInsert	OCTET STRING,
	qbAtmVplF4F5LoopbackStatus	QbF4F5LoopbackStatus,
	qbAtmVplF4F5LoopbackTimestamp	TimeStamp,
	qbAtmVplF4F5TxStatus		QbF4F5Status,
	qbAtmVplF4F5TxLastChange	TimeStamp,
	qbAtmVplF4F5RxStatus		QbF4F5Status,
	qbAtmVplF4F5RxLastChange	TimeStamp    
    }

   qbAtmVplLoopback OBJECT-TYPE
	SYNTAX      QbEnableStatus
	MAX-ACCESS  read-write
	STATUS      current
	DESCRIPTION
 		"Enable loopback on this ATM VPL"
	DEFVAL  { disable }
    ::= {  qbAtmVplEntry  1 }

   qbAtmVplF4F5LoopbackInsert	OBJECT-TYPE
	SYNTAX      OCTET STRING (SIZE(16))
	MAX-ACCESS  read-write
	STATUS      current
	DESCRIPTION
		"This is a loopback Insert object for LLID.  
		 Start loopback Insert by setting this to an LLID value."
    ::= {  qbAtmVplEntry  2 }

    qbAtmVplF4F5LoopbackStatus	OBJECT-TYPE
	SYNTAX	QbF4F5LoopbackStatus
	MAX-ACCESS	read-only
	STATUS	current
	DESCRIPTION
		"Status of loopback Insert command: in-progress, success, timeout."
	::= { qbAtmVplEntry 3 }

    qbAtmVplF4F5LoopbackTimestamp	OBJECT-TYPE
	SYNTAX	TimeStamp
	MAX-ACCESS	read-only
	STATUS	current
	DESCRIPTION
		"Value of sysUptime when last Insert completed."
	::= { qbAtmVplEntry 4 }

 qbAtmVplF4F5TxStatus   OBJECT-TYPE
    SYNTAX        QbF4F5Status 
    MAX-ACCESS    read-only
    STATUS        current
    DESCRIPTION
	 "The transmitted OAM status for the PVP."
    ::= { qbAtmVplEntry 5 }

 qbAtmVplF4F5TxLastChange   OBJECT-TYPE
    SYNTAX        TimeStamp 
    MAX-ACCESS    read-only
    STATUS        current
    DESCRIPTION
	 "The value of sysUPtime when qbAtmVclF4F5TxStatus last changed."
    ::= { qbAtmVplEntry 6 }

 qbAtmVplF4F5RxStatus   OBJECT-TYPE
    SYNTAX        QbF4F5Status 
    MAX-ACCESS    read-only
    STATUS        current
    DESCRIPTION
	 "The received OAM status of the PVP."
    ::= { qbAtmVplEntry 7 }

 qbAtmVplF4F5RxLastChange   OBJECT-TYPE
    SYNTAX        TimeStamp  
    MAX-ACCESS    read-only
    STATUS        current
    DESCRIPTION
	 "The value of sysUPtime when qbAtmVclF4F5TxStatus last changed."
    ::= { qbAtmVplEntry 8 }

-- ****************************************************************
-- Extension to the ATM VC Cross Connect Table 
-- ****************************************************************

    qbAtmVcCrossConnectTable        OBJECT-TYPE
	SYNTAX      SEQUENCE OF QbAtmVcCrossConnectEntry
	MAX-ACCESS  not-accessible
	STATUS      current
	DESCRIPTION
		    "The Quantum Bridge Table contains an extension to
		    the atmVcCrossConnectTable,		
		    An entry in this table models two cross-connected VCLs."
    ::= {  qbAtmObjects 5 }
 
    qbAtmVcCrossConnectEntry        OBJECT-TYPE
	SYNTAX      QbAtmVcCrossConnectEntry
	MAX-ACCESS  not-accessible
	STATUS      current
	DESCRIPTION
            "An entry in the QB ATM atmVcCrossConnectTable."
	AUGMENTS    {  atmVcCrossConnectEntry }
    ::= { qbAtmVcCrossConnectTable  1 }

   QbAtmVcCrossConnectEntry  ::=
	SEQUENCE {
	qbAtmVcCrossConnectName			DisplayString,
	qbAtmVcCrossConnectRxTrafficDescrIndex	AtmTrafficDescrParamIndex,
	qbAtmVcCrossConnectTxTrafficDescrIndex	AtmTrafficDescrParamIndex
    }

   qbAtmVcCrossConnectName OBJECT-TYPE
	SYNTAX      DisplayString (SIZE (0..20))
	MAX-ACCESS  read-create
	STATUS      current
	DESCRIPTION
		"A textual description for the cross connect."
	DEFVAL  { "" }
    ::= { qbAtmVcCrossConnectEntry 1 }

   qbAtmVcCrossConnectRxTrafficDescrIndex  OBJECT-TYPE
          SYNTAX         AtmTrafficDescrParamIndex
          MAX-ACCESS     read-create
          STATUS         current
          DESCRIPTION
           "The value of this object identifies the row
            in the ATM Traffic Descriptor Table which
            applies to the receive direction of this VCLs cross-connect."
          DEFVAL { 0 }
           ::= { qbAtmVcCrossConnectEntry 2}

   qbAtmVcCrossConnectTxTrafficDescrIndex  OBJECT-TYPE
          SYNTAX         AtmTrafficDescrParamIndex
          MAX-ACCESS     read-create
          STATUS         current
          DESCRIPTION
           "The value of this object identifies the row
            of the ATM Traffic Descriptor Table which applies
            to the transmit direction of this VCLs cross-connect"
          DEFVAL { 0 }
           ::= { qbAtmVcCrossConnectEntry 3 }
  
