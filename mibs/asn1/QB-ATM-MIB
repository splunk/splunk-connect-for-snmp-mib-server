   qbAtmCompliances OBJECT IDENTIFIER ::= { qbAtmConformance 1 }
   qbAtmGroups      OBJECT IDENTIFIER ::= { qbAtmConformance 2 }

--
-- Compliance Statements
--

--
-- All Object Groups
--
 qbAtmCompliance MODULE-COMPLIANCE
    STATUS      current
    DESCRIPTION
        "The compliance statement for all agents that support this
        MIB. A compliant agent implements all objects defined in this
        MIB."
    MODULE  -- this module
    MANDATORY-GROUPS {
    	qbAtmInterfaceConfGroupInfo, 
    	qbAtmTrafficDescrParamGroupInfo,
    	qbAtmVclGroupInfo,   
    	qbAtmVplGroupInfo,
    	qbAtmVcCrossConnectGroupInfo,
	qbAtmVpCrossConnectGroupInfo,
	qbAtmPnniNodeGroupInfo,
	qbAtmPnniIfGroupInfo,
	qbAtmPnniRouteAddrGroupInfo,
	qbAtmInterfaceConfSigGroupInfo,
	qbAtmHiddenVclGroupInfo ,
	qbAtmPnniNbrPeerGroupInfo,
	qbAtmInBandMgtGroupInfo
    }
    ::= { qbAtmCompliances 1 }

