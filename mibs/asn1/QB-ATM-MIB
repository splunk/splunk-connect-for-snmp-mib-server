	qbAtmVpCrossConnectRxTrafficDescrIndex	AtmTrafficDescrParamIndex,
	qbAtmVpCrossConnectTxTrafficDescrIndex	AtmTrafficDescrParamIndex
    }

   qbAtmVpCrossConnectName OBJECT-TYPE
	SYNTAX      DisplayString (SIZE (0..20))
	MAX-ACCESS  read-create
	STATUS      current
	DESCRIPTION
		"A textual description for the cross connect."
	DEFVAL  { "" }
    ::= { qbAtmVpCrossConnectEntry 1 }

   qbAtmVpCrossConnectRxTrafficDescrIndex  OBJECT-TYPE
          SYNTAX         AtmTrafficDescrParamIndex
          MAX-ACCESS     read-create
          STATUS         current
          DESCRIPTION
           "The value of this object identifies the row
            in the ATM Traffic Descriptor Table which
            applies to the receive direction of this VPLs cross-connect."
          DEFVAL { 0 }
           ::= { qbAtmVpCrossConnectEntry 2}

   qbAtmVpCrossConnectTxTrafficDescrIndex  OBJECT-TYPE
          SYNTAX         AtmTrafficDescrParamIndex
          MAX-ACCESS     read-create
          STATUS         current
          DESCRIPTION
           "The value of this object identifies the row
            of the ATM Traffic Descriptor Table which applies
            to the transmit direction of this VPLs cross-connect"
          DEFVAL { 0 }
           ::= { qbAtmVpCrossConnectEntry 3 }

   --
   -- Signalling specific group information 
   --

   qbAtmSigGroup OBJECT IDENTIFIER ::= { qbAtmObjects 7 }

   qbAtmPnniNodeTable  OBJECT-TYPE
      SYNTAX        SEQUENCE OF QbAtmPnniNodeEntry
      MAX-ACCESS    not-accessible
      STATUS        current
      DESCRIPTION
            "The QbAtmpnniNodeTable contains attributes that affect the
             operation of a PNNI logical node.
             There is a single row in this table for each PNNI peer
             group that the managed system is expected or eligible
             to become a member of."
        ::= { qbAtmSigGroup 1 }

   qbAtmPnniNodeEntry OBJECT-TYPE
	SYNTAX        QbAtmPnniNodeEntry
        MAX-ACCESS    not-accessible
        STATUS        current
        DESCRIPTION
            "An entry in the table, containing information about a PNNI
             logical node in this switching system."
        INDEX         { qbAtmPnniNodeIndex }
        ::= { qbAtmPnniNodeTable 1 }

   QbAtmPnniNodeEntry ::=
        SEQUENCE {
        qbAtmPnniNodeIndex			INTEGER,
		qbAtmPnniNodeNetAddrPrefix	OCTET STRING,
		qbAtmPnniNodeEndSysId		OCTET STRING,
        qbAtmPnniNodeLevel			INTEGER,
		qbAtmPnniNodeRowStatus		RowStatus,
		qbAtmPnniNodeOperStatus		INTEGER,
		qbAtmPnniNodeAtmAddress		QbAtmPnniAtmAddr,
        qbAtmPnniNodePeerGroupId	QbAtmPnniPeerGroupId
	}

    qbAtmPnniNodeIndex OBJECT-TYPE
        SYNTAX        INTEGER (0..65535)
        MAX-ACCESS    not-accessible
        STATUS        current
        DESCRIPTION
            "A value assigned to a node in this switching system that
             uniquely identifies it in the MIB."
	DEFVAL  { 1 }
        ::= { qbAtmPnniNodeEntry 1 }

   qbAtmPnniNodeNetAddrPrefix OBJECT-TYPE
	SYNTAX     OCTET STRING  (SIZE (13))
	MAX-ACCESS read-create
	STATUS     current
	DESCRIPTION
		"Indicates the Network Prefix to use for the ATM End
		System Address. The value is 13 bytes long, and is
		specified in the hexadecimal format. If less than 13
		bytes(26 chars) are received, the agent appends
		trailing zero's. The object can be set to ''h, if PNNI 
		protocol is desirable on all interfaces."  
	DEFVAL  {''h }
    ::= { qbAtmPnniNodeEntry 2 }   

   qbAtmPnniNodeEndSysId OBJECT-TYPE
	SYNTAX     OCTET STRING  (SIZE (7))
	MAX-ACCESS read-create
	STATUS     current
	DESCRIPTION
                "Indicates the End System Identifier for ATM End
		System Address. The value is 7 bytes long, and is
		specified in the hexadecimal format.
		The object can be set to ''h, if PNNI 
		protocol is desirable on all interfaces."  
	DEFVAL  {''h }
    ::= { qbAtmPnniNodeEntry 3 }   
 
    qbAtmPnniNodeLevel OBJECT-TYPE
        SYNTAX        INTEGER(1..104)
        MAX-ACCESS    read-create
        STATUS        current
        DESCRIPTION
            "The level of PNNI hierarchy at which this node exists. This
             attribute is used to determine the default node ID and the
             default peer group ID for this node. This object may only
             be written when pnniNodeAdminStatus has the value down."
        DEFVAL { 80 }
        ::= { qbAtmPnniNodeEntry 4 }
 
     qbAtmPnniNodeRowStatus OBJECT-TYPE
        SYNTAX        RowStatus
        MAX-ACCESS    read-create
        STATUS        current
        DESCRIPTION
            "To create, delete, activate and de-activate a Node.
				Setting this object to createAndGo(4) causes 
				the corresponding row to be created.

				Setting this object to destroy(6) causes the corresponding
				row to be deleted.
						
				Note that -- currently-- only a single instance of a row 
				can be created in this table."
        ::= { qbAtmPnniNodeEntry 5 }

    qbAtmPnniNodeOperStatus OBJECT-TYPE
        SYNTAX        INTEGER {
        				up(1),
        				down(2)
        			}
        MAX-ACCESS    read-only
        STATUS        current
        DESCRIPTION
            "Indicates the Operational Status of the PNNI Node. For Logical Group 
            Nodes, the Operational Status will be up only if the child becomes the
            PGL."
        ::= { qbAtmPnniNodeEntry 6 }     

	qbAtmPnniNodeAtmAddress OBJECT-TYPE
        SYNTAX        QbAtmPnniAtmAddr
        MAX-ACCESS    read-only
        STATUS        current
        DESCRIPTION
            "This node's ATM End System Address.  Remote systems wishing
             to exchange PNNI protocol packets with this node should
             direct packets or calls to this address.

             The current Qb implementation does not support writing this value."
        REFERENCE
            "ATM Forum PNNI 1.0 Section 5.2.2"
        ::= { qbAtmPnniNodeEntry 7 }

	qbAtmPnniNodePeerGroupId OBJECT-TYPE
        SYNTAX        QbAtmPnniPeerGroupId
        MAX-ACCESS    read-only
        STATUS        current
        DESCRIPTION
            "The Peer Group Identifier of the peer group that the given
             node is to become a member of."
        REFERENCE
            "ATM Forum PNNI 1.0 Section 5.3.2, Annex F"
        ::= { qbAtmPnniNodeEntry 8 }
        

  --
  -- Interface table
  ---

    qbAtmPnniIfTable OBJECT-TYPE
        SYNTAX        SEQUENCE OF QbAtmPnniIfEntry
        MAX-ACCESS    not-accessible
        STATUS        current
        DESCRIPTION
            "The qbAtmPnniIfTable contains the attributes necessary to
             configure a physical interface on a switching system 
             capable of being used for PNNI routing.

             An ifIndex is used as the instance ID to uniquely identify
             the interface on the local switching system. This index has
             the same value as the ifIndex object defined in RFC 1573
             for the same interface, since this table correlates with
             the ifTable in RFC 1573.

             One row in this table is created by the managed system for
             each row in the ifTable that has an ifType of atm(37) or
             atmLogical(80)."
        ::= { qbAtmSigGroup 2 }

    qbAtmPnniIfEntry OBJECT-TYPE
        SYNTAX        QbAtmPnniIfEntry
        MAX-ACCESS    not-accessible
        STATUS        current
        DESCRIPTION
            "An entry in the table, containing PNNI specific interface
             information in this switching system."
        INDEX         { ifIndex }
        ::= {  qbAtmPnniIfTable 1 }

     QbAtmPnniIfEntry ::=
        SEQUENCE {
                qbAtmPnniIfNodeIndex         INTEGER,
                qbAtmPnniIfAdmWeightCbr      QbAtmPnniWeight,
                qbAtmPnniIfAdmWeightRtVbr    QbAtmPnniWeight,
                qbAtmPnniIfAdmWeightNrtVbr   QbAtmPnniWeight,
                qbAtmPnniIfAdmWeightAbr      QbAtmPnniWeight,
                qbAtmPnniIfAdmWeightUbr      QbAtmPnniWeight,
                qbAtmPnniIfRccServiceCategory    AtmServiceCategory,
                qbAtmPnniIfRccTrafficDescrIndex AtmTrafficDescrParamIndex,
		qbAtmPnniIfPolicingStatus	INTEGER
                }

    qbAtmPnniIfNodeIndex OBJECT-TYPE
        SYNTAX        INTEGER (1..1)
        MAX-ACCESS    read-write
        STATUS        current
        DESCRIPTION
            "Identifies the node within the switching system that the
             interface is directly attached to.  Note: For this release
             this value can be set only to 1."
        DEFVAL { 1 }
        ::= { qbAtmPnniIfEntry 1 }

    qbAtmPnniIfAdmWeightCbr OBJECT-TYPE
        SYNTAX        QbAtmPnniWeight
        MAX-ACCESS    read-write
        STATUS        current
        DESCRIPTION
            "The administrative weight of this interface for the
             constant bit rate service category."
        DEFVAL { 5040 }
        ::= { qbAtmPnniIfEntry  2 }

    qbAtmPnniIfAdmWeightRtVbr OBJECT-TYPE
        SYNTAX        QbAtmPnniWeight
        MAX-ACCESS    read-write
        STATUS        current
        DESCRIPTION
            "The administrative weight of this interface for the
             real-time variable bit rate service category."
        DEFVAL { 5040 }
        ::= { qbAtmPnniIfEntry  3 }

   qbAtmPnniIfAdmWeightNrtVbr OBJECT-TYPE
        SYNTAX        QbAtmPnniWeight
        MAX-ACCESS    read-write
        STATUS        current
        DESCRIPTION
            "The administrative weight of this interface for the
             non-real-time variable bit rate service category."
        DEFVAL { 5040 }
        ::= { qbAtmPnniIfEntry 4 }

    qbAtmPnniIfAdmWeightAbr OBJECT-TYPE
        SYNTAX        QbAtmPnniWeight
        MAX-ACCESS    read-write
        STATUS        current
        DESCRIPTION
            "The administrative weight of this interface for the
             available bit rate service category."
        DEFVAL { 5040 }
        ::= { qbAtmPnniIfEntry 5 }

   qbAtmPnniIfAdmWeightUbr OBJECT-TYPE
        SYNTAX        QbAtmPnniWeight
        MAX-ACCESS    read-write
        STATUS        current
        DESCRIPTION
            "The administrative weight of this interface for the
             unspecified bit rate service category."
        DEFVAL { 5040 }
        ::= { qbAtmPnniIfEntry 6 }

  qbAtmPnniIfRccServiceCategory OBJECT-TYPE
        SYNTAX        AtmServiceCategory
        MAX-ACCESS    read-write
        STATUS        current
        DESCRIPTION
            "The service category used for the PNNI routing control
             channel (VCI=18) on this interface."
	 DEFVAL { 4 }
         ::= { qbAtmPnniIfEntry 7 }

  qbAtmPnniIfRccTrafficDescrIndex OBJECT-TYPE
        SYNTAX        AtmTrafficDescrParamIndex
        MAX-ACCESS    read-write
        STATUS        current
        DESCRIPTION
            "The traffic descriptor index referring to the entry in the
             atmTrafficDescrParamTable defined in RFC 1695 that
             specifies the traffic allocation for the PNNI routing
             control channel (VCI=18) on this interface."
	DEFVAL  { 7 }
        ::= { qbAtmPnniIfEntry 8 }

    qbAtmPnniIfPolicingStatus OBJECT-TYPE
        SYNTAX        INTEGER {
		      enable(1),
		      disable(2)
	}
        MAX-ACCESS    read-write
        STATUS        deprecated
        DESCRIPTION
            "If this object is set to enable, the policing is enabled on this
	    interface. If the object is set to 'disable' the policing is
	    disabled."
	DEFVAL  { disable }
        ::= { qbAtmPnniIfEntry 9 }

 --
 -- Table of routes from nodes to reachable addresses
 --
   qbAtmPnniRouteAddrTable OBJECT-TYPE
       SYNTAX        SEQUENCE OF QbAtmPnniRouteAddrEntry
       MAX-ACCESS    not-accessible
       STATUS        current
       DESCRIPTION
            "A table containing all the attributes necessary to
             determine what the PNNI entity believes is reachable in
             terms of ATM End System Addresses, and to determine which
             nodes are advertising this reachability.  This table is
             also used to configure static routes to reachable address
             prefixes.  The local node index that receives the
             reachability information, reachable address, address prefix
             length, and an index that distinguishes between multiple
             listings of connectivity to a given address prefix from a
             given local node are combined to form an instance ID for
             this object."
        ::= { qbAtmSigGroup 3 }

    qbAtmPnniRouteAddrEntry OBJECT-TYPE
        SYNTAX        QbAtmPnniRouteAddrEntry
        MAX-ACCESS    not-accessible
        STATUS        current
        DESCRIPTION
            "An entry in the table, containing information about a
             reachable address prefix."
        INDEX         { qbAtmPnniNodeIndex,
			qbAtmPnniRouteAddrIfIndex,
                        qbAtmPnniRouteAddrAddress
		     }
        ::= { qbAtmPnniRouteAddrTable 1}

    QbAtmPnniRouteAddrEntry ::=
        SEQUENCE {
                qbAtmPnniRouteAddrAddress            OCTET STRING,
		qbAtmPnniRouteAddrIfIndex	     InterfaceIndex,
                qbAtmPnniRouteAddrPrefixLength       INTEGER,
 		qbAtmPnniRouteAddrRowStatus          RowStatus,
                qbAtmPnniRouteAddrProto              INTEGER
	}


    qbAtmPnniRouteAddrIfIndex OBJECT-TYPE
        SYNTAX        InterfaceIndex
        MAX-ACCESS    not-accessible
        STATUS        current
        DESCRIPTION
            "The local interface over which the reachable address can be
             reached. "
        ::= { qbAtmPnniRouteAddrEntry 1 }

   qbAtmPnniRouteAddrAddress OBJECT-TYPE
        SYNTAX        OCTET STRING (SIZE(19))
        MAX-ACCESS    not-accessible
        STATUS        current
        DESCRIPTION
            "The value of the ATM End System Address prefix."
        ::= { qbAtmPnniRouteAddrEntry 2 }

   qbAtmPnniRouteAddrPrefixLength OBJECT-TYPE
        SYNTAX        INTEGER (1..152)
        MAX-ACCESS    read-create
        STATUS        current
        DESCRIPTION
            "Indicates the number of bits to be applied to the ATM End System
             Address prefix."
	DEFVAL       { 104 }
        ::= { qbAtmPnniRouteAddrEntry 3 }

    qbAtmPnniRouteAddrRowStatus OBJECT-TYPE
       SYNTAX        RowStatus
       MAX-ACCESS    read-create
       STATUS        current
       DESCRIPTION
            "To create, delete, activate and de-activate a reachable
             address prefix."
        ::= { qbAtmPnniRouteAddrEntry 4 }

    qbAtmPnniRouteAddrProto OBJECT-TYPE
        SYNTAX        INTEGER {
			      iisp(1),
			      pnni(2)
		      }	
        MAX-ACCESS    read-only
        STATUS        current
        DESCRIPTION
            "This object indicates the Routing mechanism through which 
	    the connectivity from the advertising node to the
	    reachable address prefix was learned."
        ::= { qbAtmPnniRouteAddrEntry 5 }

   -- ****************************************************************
   -- ATM Intraface Signalling Parameters Table 
   -- Extensions to the ATM Interface Configuration Table 
   -- ****************************************************************
   qbAtmInterfaceConfSigTable OBJECT-TYPE
	SYNTAX     SEQUENCE OF QbAtmInterfaceConfSigEntry
	MAX-ACCESS not-accessible
	STATUS     current
	DESCRIPTION
		"This table contains ATM local interface configuration 
		parameters, one entry per ATM interface port."
    ::= { qbAtmSigGroup 4 }

   qbAtmInterfaceConfSigEntry OBJECT-TYPE
    SYNTAX     QbAtmInterfaceConfSigEntry
    MAX-ACCESS not-accessible
    STATUS     current
    DESCRIPTION
        " This list contains additonal ATM interface configuration
          parameters and state variables."
    AUGMENTS { atmInterfaceConfEntry }
    ::= { qbAtmInterfaceConfSigTable 1 }

    QbAtmInterfaceConfSigEntry ::=
	SEQUENCE {
		qbAtmInterfaceConfSigMode	QbAtmSigSpec,
		qbAtmInterfaceConfSigSide       INTEGER,
		qbAtmInterfaceConfSigParseMode  INTEGER,
		qbAtmInterfaceConfSigAdminStatus      INTEGER,
		qbAtmInterfaceConfSigSvcMinVci        INTEGER,
		qbAtmInterfaceConfSigSvcMaxVci        INTEGER,
		qbAtmInterfaceConfSigSvcMinVpi        INTEGER,
		qbAtmInterfaceConfSigSvcMaxVpi        INTEGER,
		qbAtmInterfaceConfSigSvpMinVpi        INTEGER,
		qbAtmInterfaceConfSigSvpMaxVpi        INTEGER,
		qbAtmInterfaceConfSigLastChange AtmVorXLastChange,
		qbAtmInterfaceConfSigIlmiEnable      Boolean
 	}

    qbAtmInterfaceConfSigMode    OBJECT-TYPE
	SYNTAX        QbAtmSigSpec
	MAX-ACCESS   read-write
	STATUS       current
	DESCRIPTION
		"The type of ATM interface which is either a UNI 
		(User to Network), PNNI or IISP.
		To modify the qbAtmInterfaceConfSignType, ifAdminStatus has 
		to be down."
	DEFVAL {uni31}
	::= { qbAtmInterfaceConfSigEntry 1 } 

   qbAtmInterfaceConfSigSide    OBJECT-TYPE
	SYNTAX       INTEGER {
			user(1),
			network(2),
			notApplicable(3)
	}
	MAX-ACCESS   read-write
	STATUS       current
	DESCRIPTION
	       "The side of ATM interface which is either a user or 
	       network side. NotApplicable value implies that 
	       qbAtmInterfaceConfSignType is other than uni or iisp."
	DEFVAL {network}
	::= {  qbAtmInterfaceConfSigEntry 2 }

     qbAtmInterfaceConfSigParseMode    OBJECT-TYPE
	SYNTAX       INTEGER {
			on(1),
			off(2)
	}
	MAX-ACCESS   read-write
	STATUS       current
	DESCRIPTION
	       "Indicates to the signalling stack to use strict
	       parsing of the INformation Elements (IEs) within
	       signalling messages. This parameter is set to 'on' for
	       conformance testing. For systems that do not test
	       conformance it is recommended to set the object to
	       'off'." 
 	DEFVAL { off }
	::= {  qbAtmInterfaceConfSigEntry 3 }

    qbAtmInterfaceConfSigAdminStatus OBJECT-TYPE
        SYNTAX       INTEGER {
			enabled(1), 
			disabled(2)
			}
        MAX-ACCESS   read-write
        STATUS       current
        DESCRIPTION
		"Enable/Disable signalling/sscop on this interface. 
		To modify the  qbAtmInterfaceConfSignAdminStatus, 
		ifAdminStatus has to be down."
        DEFVAL {disabled}
        ::= { qbAtmInterfaceConfSigEntry  4}

	qbAtmInterfaceConfSigSvcMinVci OBJECT-TYPE
        SYNTAX       INTEGER (32..16383)
	MAX-ACCESS   read-write
        STATUS       current
        DESCRIPTION
		"The minimum value of SVC connection for vci."
        ::= { qbAtmInterfaceConfSigEntry  5}

	qbAtmInterfaceConfSigSvcMaxVci OBJECT-TYPE
        SYNTAX        INTEGER (32..16383)
	MAX-ACCESS    read-write
        STATUS        current
        DESCRIPTION
		"The maximum value of SVC connection for vci."
        ::= { qbAtmInterfaceConfSigEntry  6}

	qbAtmInterfaceConfSigSvcMinVpi OBJECT-TYPE
        SYNTAX        INTEGER (0..255)
	MAX-ACCESS    read-write
        STATUS        current
        DESCRIPTION
		"The minimum value of SVC connection for vpi."
        ::= { qbAtmInterfaceConfSigEntry  7}

	qbAtmInterfaceConfSigSvcMaxVpi OBJECT-TYPE
        SYNTAX        INTEGER (0..255)
	MAX-ACCESS    read-write
        STATUS        current
        DESCRIPTION
		"The maximum value of SVC connection for vpi."
        ::= { qbAtmInterfaceConfSigEntry  8}

	qbAtmInterfaceConfSigSvpMinVpi OBJECT-TYPE
        SYNTAX        INTEGER (0..255)
	MAX-ACCESS    read-write
        STATUS        current
        DESCRIPTION
		"The minimum value of SVP connection for vpi."
        ::= { qbAtmInterfaceConfSigEntry  9}

	qbAtmInterfaceConfSigSvpMaxVpi OBJECT-TYPE
        SYNTAX        INTEGER (0..255)
	MAX-ACCESS    read-write
        STATUS        current
        DESCRIPTION
		"The maximum value of SVP connection for vpi."
        ::= { qbAtmInterfaceConfSigEntry  10}

    qbAtmInterfaceConfSigLastChange    OBJECT-TYPE
	SYNTAX         AtmVorXLastChange
        MAX-ACCESS     read-only
        STATUS         current
        DESCRIPTION
              "The value of sysUpTime at the time this
               interface changed its current administrative state."
          ::= { qbAtmInterfaceConfSigEntry  11 }
                                                    
   qbAtmInterfaceConfSigIlmiEnable    OBJECT-TYPE
	SYNTAX         Boolean
        MAX-ACCESS     read-write
        STATUS         current
        DESCRIPTION
              "Can be enabled only if signaling is configured. ILMI is enabled by default when signaling
              is configured on the system and the user may enter the disable option ."
          ::= { qbAtmInterfaceConfSigEntry  12 }                                               
------------------------------- qb adaptation of atm forum pnni 0055 table ---------------------

qbAtmPnniNbrPeerTable OBJECT-TYPE
        SYNTAX        SEQUENCE OF QbAtmPnniNbrPeerEntry
        MAX-ACCESS    not-accessible
        STATUS        current
        DESCRIPTION
            "The qbAtmPnniNbrPeer Object contains all the attributes
             necessary to describe the relationship a node in this
             switching system has with a neighboring node within the
             same peer group.  A concatenation of the Node Identifier of
             the node within the local switching system and the
             neighboring peer's Node Identifier is used to form the
             instance ID for this object.

             The entire qbAtmPnniNbrPeer object is read-only, reflecting the
             fact that neighboring peers are discovered dynamically by
             the PNNI protocol rather than configured."
        REFERENCE
            "ATM Forum PNNI 1.0 Sections 5.7, 5.8"
        ::= { qbAtmSigGroup 5 }

qbAtmPnniNbrPeerEntry OBJECT-TYPE
        SYNTAX        QbAtmPnniNbrPeerEntry
        MAX-ACCESS    not-accessible
        STATUS        current
        DESCRIPTION
            "An entry in the table, containing information about this
             node's relationship with a neighboring peer node."
        REFERENCE
            "ATM Forum PNNI 1.0 Sections 5.7, 5.8"
        INDEX         { qbAtmPnniNodeIndex,
                        qbAtmPnniPeerRemoteNodeId }
        ::= { qbAtmPnniNbrPeerTable 1 }

QbAtmPnniNbrPeerEntry ::=
        SEQUENCE {   
                qbAtmPnniPeerRemoteNodeId         QbAtmPnniNodeId,
                qbAtmPnniPeerState                INTEGER,
                qbAtmPnniPeerSvccRccIndex         INTEGER,
                qbAtmPnniPeerPortCount            INTEGER,
                qbAtmPnniPeerRcvDbSums            Counter32,
                qbAtmPnniPeerXmtDbSums            Counter32,
                qbAtmPnniPeerRcvPtsps             Counter32,
                qbAtmPnniPeerXmtPtsps             Counter32,
                qbAtmPnniPeerRcvPtseReqs          Counter32,
                qbAtmPnniPeerXmtPtseReqs          Counter32,
                qbAtmPnniPeerRcvPtseAcks          Counter32,
                qbAtmPnniPeerXmtPtseAcks          Counter32
                }

qbAtmPnniPeerRemoteNodeId OBJECT-TYPE
        SYNTAX        QbAtmPnniNodeId
        MAX-ACCESS    not-accessible
        STATUS        current
        DESCRIPTION
            "The Node Identifier of the neighboring peer node."
        ::= { qbAtmPnniNbrPeerEntry 1 }

qbAtmPnniPeerState OBJECT-TYPE
        SYNTAX        INTEGER {
                                npdown(1),
                                negotiating(2),
                                exchanging(3),
                                loading(4),
                                full(5)
                                }
        MAX-ACCESS    read-only
        STATUS        current
        DESCRIPTION
            "Indicates the state of this node's Neighboring Peer State
             Machine associated with qbAtmPnniPeerRemoteNodeId."
        REFERENCE
            "ATM Forum PNNI 1.0 Section 5.7.2"
        ::= { qbAtmPnniNbrPeerEntry 2 }

qbAtmPnniPeerSvccRccIndex OBJECT-TYPE
        SYNTAX        INTEGER
        MAX-ACCESS    read-only
        STATUS        current
        DESCRIPTION
            "Identifies the SVCC-based RCC being used to communicate
             with the neighboring peer if one exists.  If both the local
             node and the neighboring peer node are lowest-level nodes,
             this attribute is set to zero."
        ::= { qbAtmPnniNbrPeerEntry 3 }

qbAtmPnniPeerPortCount OBJECT-TYPE
        SYNTAX        INTEGER
        MAX-ACCESS    read-only
        STATUS        current
        DESCRIPTION
            "A count of the total number of ports that connect to the
             neighboring peer.  If the neighboring peer only
             communicates via an SVCC-based RCC, the value of this
             attribute is set to zero.  Otherwise it is set to the total
             number of ports to the neighboring peer in the Hello state
             2-WayInside."
        ::= { qbAtmPnniNbrPeerEntry 4 }

qbAtmPnniPeerRcvDbSums OBJECT-TYPE
        SYNTAX        Counter32
        MAX-ACCESS    read-only
        STATUS        current
        DESCRIPTION
            "A count of the number of Database Summary Packets received
             from the neighboring peer."
        ::= { qbAtmPnniNbrPeerEntry 5 }

qbAtmPnniPeerXmtDbSums OBJECT-TYPE
        SYNTAX        Counter32
        MAX-ACCESS    read-only
        STATUS        current
        DESCRIPTION
            "A count of the number of Database Summary Packets
             transmitted to the neighboring peer."
        ::= { qbAtmPnniNbrPeerEntry 6 }

qbAtmPnniPeerRcvPtsps OBJECT-TYPE
        SYNTAX        Counter32
        MAX-ACCESS    read-only
        STATUS        current
        DESCRIPTION
            "A count of the number of PTSPs received from the
             neighboring peer."
        ::= { qbAtmPnniNbrPeerEntry 7 }

qbAtmPnniPeerXmtPtsps OBJECT-TYPE
        SYNTAX        Counter32
        MAX-ACCESS    read-only
        STATUS        current
        DESCRIPTION
            "A count of the number of PTSPs (re)transmitted to the
             neighboring peer."
        ::= { qbAtmPnniNbrPeerEntry 8 }

qbAtmPnniPeerRcvPtseReqs OBJECT-TYPE
        SYNTAX        Counter32
        MAX-ACCESS    read-only
        STATUS        current
        DESCRIPTION
            "A count of the number of PTSE Request packets received from
             the neighboring peer."
        ::= { qbAtmPnniNbrPeerEntry 9 }

qbAtmPnniPeerXmtPtseReqs OBJECT-TYPE
        SYNTAX        Counter32
        MAX-ACCESS    read-only
        STATUS        current
        DESCRIPTION
            "A count of the number of PTSE Request packets transmitted
             to the neighboring peer."
        ::= { qbAtmPnniNbrPeerEntry 10 }

qbAtmPnniPeerRcvPtseAcks OBJECT-TYPE
        SYNTAX        Counter32
        MAX-ACCESS    read-only
        STATUS        current
        DESCRIPTION
            "A count of the number of PTSE Ack packets received from the
             neighboring peer."
        ::= { qbAtmPnniNbrPeerEntry 11 }

qbAtmPnniPeerXmtPtseAcks OBJECT-TYPE
        SYNTAX        Counter32
        MAX-ACCESS    read-only
        STATUS        current
        DESCRIPTION
            "A count of the number of PTSE Ack packets transmitted to
             the neighboring peer."
        ::= { qbAtmPnniNbrPeerEntry 12 }

-- qbAtmPnniNodePgl Table - based on ATM Forum PGL election table

qbAtmPnniNodePglTable OBJECT-TYPE
        SYNTAX        SEQUENCE OF QbAtmPnniNodePglEntry
        MAX-ACCESS    not-accessible
        STATUS        current
        DESCRIPTION
            "Peer group leader election information for a PNNI node in
             this switching system."
        REFERENCE
            "ATM Forum PNNI 1.0 Section 5.10.1"
        ::= { qbAtmSigGroup 6 }

qbAtmPnniNodePglEntry OBJECT-TYPE
        SYNTAX        QbAtmPnniNodePglEntry
        MAX-ACCESS    not-accessible
        STATUS        current
        DESCRIPTION
            "An entry in the table, containing PGL election information
             of a PNNI logical node in this switching system."
        REFERENCE
            "ATM Forum PNNI 1.0 Section 5.10.1"
        AUGMENTS      { qbAtmPnniNodeEntry }
        ::= { qbAtmPnniNodePglTable 1 }

QbAtmPnniNodePglEntry ::=
        SEQUENCE {
                qbAtmPnniNodePglLeadershipPriority   INTEGER,
                qbAtmPnniNodeCfgParentNodeIndex      Integer32,
                qbAtmPnniNodePglInitTime             Integer32,
                qbAtmPnniNodePglOverrideDelay        Integer32,
                qbAtmPnniNodePglReelectTime          Integer32,
                qbAtmPnniNodePglState                INTEGER,
                qbAtmPnniNodePreferredPgl            QbAtmPnniNodeId,
                qbAtmPnniNodePeerGroupLeader       	 QbAtmPnniNodeId,
                qbAtmPnniNodePglTimeStamp            TimeStamp,
                qbAtmPnniNodeActiveParentNodeId      QbAtmPnniNodeId
                }

qbAtmPnniNodePglLeadershipPriority OBJECT-TYPE
        SYNTAX        INTEGER (0..205)
        MAX-ACCESS    read-create
        STATUS        current
        DESCRIPTION
            "The Leadership priority value this node should advertise in
             its nodal information group for the given peer group.  Only
             the value zero can be used with nodes that are not PGL/LGN
             capable.  If there is no configured parent node index or no
             corresponding entry in the pnniNodeTable, then the
             advertised leadership priority is zero regardless of this
             value."
        REFERENCE
            "ATM Forum PNNI 1.0 Section 5.10.1.2"
        DEFVAL { 0 }
        ::= { qbAtmPnniNodePglEntry 1 }

qbAtmPnniNodeCfgParentNodeIndex OBJECT-TYPE
        SYNTAX        Integer32
        MAX-ACCESS    read-create
        STATUS        current
        DESCRIPTION
            "The local node index used to identify the node that will
             represent this peer group at the next higher level of
             hierarchy, if this node becomes peer group leader.  The
             value 0 indicates that there is no parent node."
        REFERENCE
            "ATM Forum PNNI 1.0 Annex F"
        DEFVAL { 0 }
        ::= { qbAtmPnniNodePglEntry 2 }

qbAtmPnniNodePglInitTime OBJECT-TYPE
        SYNTAX        Integer32
        UNITS         "seconds"
        MAX-ACCESS    read-create
        STATUS        current
        DESCRIPTION
            "The amount of time in seconds this node will delay
             advertising its choice of preferred PGL after having
             initialized operation and reached the full state with at
             least one neighbor in the peer group."
        REFERENCE
            "ATM Forum PNNI 1.0 Annex G PGLInitTime"
        DEFVAL  { 15 }
        ::= { qbAtmPnniNodePglEntry 3 }

qbAtmPnniNodePglOverrideDelay OBJECT-TYPE
        SYNTAX        Integer32
        UNITS         "seconds"
        MAX-ACCESS    read-create
        STATUS        current
        DESCRIPTION
            "The amount of time in seconds a node will wait for itself
             to be declared the preferred PGL by unanimous agreement
             among its peers.  In the absence of unanimous agreement
             this will be the amount of time that will pass before this
             node considers a two thirds majority as sufficient
             agreement to declare itself peer group leader, abandoning
             the attempt to get unanimous agreement."
        REFERENCE
            "ATM Forum PNNI 1.0 Annex G OverrideDelay"
        DEFVAL  { 30 }
        ::= { qbAtmPnniNodePglEntry 4 }

qbAtmPnniNodePglReelectTime OBJECT-TYPE
        SYNTAX        Integer32
        UNITS         "seconds"
        MAX-ACCESS    read-create
        STATUS        current
        DESCRIPTION
            "The amount of time in seconds after losing connectivity to
             the current peer group leader, that this node will wait
             before re-starting the process of electing a new peer group
             leader."
        REFERENCE
            "ATM Forum PNNI 1.0 Annex G ReElectionInterval"
        DEFVAL  { 15 }
        ::= { qbAtmPnniNodePglEntry 5 }

qbAtmPnniNodePglState OBJECT-TYPE
        SYNTAX        INTEGER {
                                starting(1),
                                awaiting(2),
                                awaitingFull(3),
                                initialDelay(4),
                                calculating(5),
                                awaitUnanimity(6),
                                operPgl(7),
                                operNotPgl(8),
                                hungElection(9),
                                awaitReElection(10),
                                other(11)
                                }
        MAX-ACCESS    read-only
        STATUS        current
        DESCRIPTION
            "Indicates the state that this node is in with respect to
             the Peer Group Leader election that takes place in the
             node's peer group. The values are enumerated in the Peer
             Group Leader State Machine."
        REFERENCE
            "ATM Forum PNNI 1.0 Section 5.10.1.1.2"
        ::= { qbAtmPnniNodePglEntry 6 }

qbAtmPnniNodePreferredPgl OBJECT-TYPE
        SYNTAX        QbAtmPnniNodeId
        MAX-ACCESS    read-only
        STATUS        current
        DESCRIPTION
            "The Node ID of
             the node which the local node believes should be or become
             the peer group leader.  This is also the value the local
             node is currently advertising in the `Preferred Peer Group
             Leader Node ID' field of its nodal information group within
             the given peer group.  If a Preferred PGL has not been
             chosen, this attribute's value is set to (all) zero(s)."
        REFERENCE
            "ATM Forum PNNI 1.0 Section 5.10.1.1.6"
        ::= { qbAtmPnniNodePglEntry 7 }

qbAtmPnniNodePeerGroupLeader OBJECT-TYPE
        SYNTAX        QbAtmPnniNodeId
        MAX-ACCESS    read-only
        STATUS        current
        DESCRIPTION
            "The Node Identifier of the node which is currently
             operating as peer group leader of the peer group this node
             belongs to. If a PGL has not been elected, this attribute's
             value is set to (all) zero(s)."
        ::= { qbAtmPnniNodePglEntry 8 }

qbAtmPnniNodePglTimeStamp OBJECT-TYPE
        SYNTAX        TimeStamp
        MAX-ACCESS    read-only
        STATUS        current
        DESCRIPTION
            "The time at which the current Peer Group Leader established
             itself."
        ::= { qbAtmPnniNodePglEntry 9 }

qbAtmPnniNodeActiveParentNodeId OBJECT-TYPE
        SYNTAX        QbAtmPnniNodeId
        MAX-ACCESS    read-only
        STATUS        current
        DESCRIPTION
            "The Node Identifier value being used by the Peer Group
             Leader to represent this peer group at the next higher
             level of the hierarchy. If this node is at the highest
             level of the hierarchy or if no PGL has yet been elected
             the PNNI Protocol Entity sets the value of this attribute
             to (all) zero(s)."
        ::= { qbAtmPnniNodePglEntry 10 }


-- initial timer values table - based on atm forum's qbAtmPnniNodeTimerTable

qbAtmPnniNodeTimerTable OBJECT-TYPE
        SYNTAX        SEQUENCE OF QbAtmPnniNodeTimerEntry
        MAX-ACCESS    not-accessible
        STATUS        current
        DESCRIPTION
            "A table of initial PNNI timer values and significant
             change thresholds."
        ::= { qbAtmSigGroup 7 }

qbAtmPnniNodeTimerEntry OBJECT-TYPE
        SYNTAX        QbAtmPnniNodeTimerEntry
        MAX-ACCESS    not-accessible
        STATUS        current
        DESCRIPTION
            "An entry in the table, containing initial PNNI timer values
             and significant change thresholds of a PNNI logical node in
             this switching system."
        AUGMENTS      { qbAtmPnniNodeEntry }
        ::= { qbAtmPnniNodeTimerTable 1 }

QbAtmPnniNodeTimerEntry ::=
        SEQUENCE {
                qbAtmPnniNodePtseHolddown            Integer32,
                qbAtmPnniNodeHelloHolddown           Integer32,
                qbAtmPnniNodeHelloInterval           Integer32,
                qbAtmPnniNodeHelloInactivityFactor   Integer32,
                qbAtmPnniNodeHlinkInact              Integer32,
                qbAtmPnniNodePtseRefreshInterval     Integer32,
                qbAtmPnniNodePtseLifetimeFactor      INTEGER,
                qbAtmPnniNodeRxmtInterval            Integer32,
                qbAtmPnniNodePeerDelayedAckInterval  Integer32,
                qbAtmPnniNodeAvcrPm                  INTEGER,
                qbAtmPnniNodeAvcrMt                  INTEGER,
                qbAtmPnniNodeCdvPm                   INTEGER,
                qbAtmPnniNodeCtdPm                   INTEGER
                }

qbAtmPnniNodePtseHolddown OBJECT-TYPE
        SYNTAX        Integer32
        UNITS         "100 milliseconds"
        MAX-ACCESS    read-create
        STATUS        current
        DESCRIPTION
            "The initial value for the PTSE hold down timer that will be
             used by the given node to limit the rate at which it can
             re-originate PTSEs. It must be a positive non-zero number."
        REFERENCE
            "ATM Forum PNNI 1.0 Annex G MinPTSEInterval"
        DEFVAL { 10 }
        ::= { qbAtmPnniNodeTimerEntry 1 }

qbAtmPnniNodeHelloHolddown OBJECT-TYPE
        SYNTAX        Integer32
        UNITS         "100 milliseconds"
        MAX-ACCESS    read-create
        STATUS        current
        DESCRIPTION
            "The initial value for the Hello hold down timer that will
             be used by the given node to limit the rate at which it
             sends Hellos. It must be a positive non-zero number."
        REFERENCE
            "ATM Forum PNNI 1.0 Annex G MinHelloInterval"
        DEFVAL { 10 }
        ::= { qbAtmPnniNodeTimerEntry 2 }

qbAtmPnniNodeHelloInterval OBJECT-TYPE
        SYNTAX        Integer32
        UNITS         "seconds"
        MAX-ACCESS    read-create
        STATUS        current
        DESCRIPTION
            "The initial value for the Hello Timer.
             In the absence of triggered Hellos, this node will send one
             Hello packet on each of its ports on this interval."
        REFERENCE
            "ATM Forum PNNI 1.0 Annex G HelloInterval"
        DEFVAL { 15 }
        ::= { qbAtmPnniNodeTimerEntry 3 }

qbAtmPnniNodeHelloInactivityFactor OBJECT-TYPE
        SYNTAX        Integer32
        MAX-ACCESS    read-create
        STATUS        current
        DESCRIPTION
            "The value for the Hello Inactivity factor that this
             node will use to determine when a neighbor has gone down."
        REFERENCE
            "ATM Forum PNNI 1.0 Annex G InactivityFactor"
        DEFVAL { 5 }
        ::= { qbAtmPnniNodeTimerEntry 4 }

qbAtmPnniNodeHlinkInact OBJECT-TYPE
        SYNTAX        Integer32
        UNITS         "seconds"
        MAX-ACCESS    read-create
        STATUS        current
        DESCRIPTION
            "The amount of time a node will continue to
             advertise a horizontal (logical) link for which it has
             not received and processed a LGN Horizontal Link
             information group."
        REFERENCE
            "ATM Forum PNNI 1.0 Annex G HorizontalLinkInactivityTime"
        DEFVAL { 120 }
        ::= { qbAtmPnniNodeTimerEntry 5 }

qbAtmPnniNodePtseRefreshInterval OBJECT-TYPE
        SYNTAX        Integer32
        UNITS         "seconds"
        MAX-ACCESS    read-create
        STATUS        current
        DESCRIPTION
            "The initial value for the Refresh timer that this node will
             use to drive (re-)origination of PTSEs in the absence of
             triggered updates."
        REFERENCE
            "ATM Forum PNNI 1.0 Annex G PTSERefreshInterval"
        DEFVAL  { 1800 }
        ::= { qbAtmPnniNodeTimerEntry 6 }

qbAtmPnniNodePtseLifetimeFactor OBJECT-TYPE
        SYNTAX        INTEGER (101..1000)
        UNITS         "percent"
        MAX-ACCESS    read-create
        STATUS        current
        DESCRIPTION
            "The value for the lifetime multiplier, expressed as a
             percentage.  The result of multiplying the
             qbAtmPnniNodePtseRefreshInterval attribute value by this
             attribute value is used as the initial lifetime that this
             node places into self-originated PTSEs."
        REFERENCE
            "ATM Forum PNNI 1.0 Annex G PTSELifetimeFactor"
        DEFVAL  { 200 }
        ::= { qbAtmPnniNodeTimerEntry 7 }

qbAtmPnniNodeRxmtInterval OBJECT-TYPE
        SYNTAX        Integer32
        UNITS         "seconds"
        MAX-ACCESS    read-create
        STATUS        current
        DESCRIPTION
            "The period between retransmissions of unacknowledged
             Database Summary packets, PTSE Request packets, and PTSPs."
        REFERENCE
            "ATM Forum PNNI 1.0 Annex G DSRxmtInterval,
             RequestRxmtInterval, PTSERetransmissionInterval"
        DEFVAL { 5 }
        ::= { qbAtmPnniNodeTimerEntry 8 }

qbAtmPnniNodePeerDelayedAckInterval OBJECT-TYPE
        SYNTAX        Integer32
        UNITS         "100 milliseconds"
        MAX-ACCESS    read-create
        STATUS        current
        DESCRIPTION
            "The minimum amount of time between transmissions of
             delayed PTSE acknowledgement packets."
        REFERENCE
            "ATM Forum PNNI 1.0 Annex G PeerDelayedAckInterval,
             Appendix G"
        DEFVAL { 10 }
        ::= { qbAtmPnniNodeTimerEntry 9 }

qbAtmPnniNodeAvcrPm OBJECT-TYPE
        SYNTAX        INTEGER (1..99)
        UNITS         "percent"
        MAX-ACCESS    read-create
        STATUS        current
        DESCRIPTION
            "The proportional multiplier used in the algorithms that
             determine significant change for AvCR parameters, expressed
             as a percentage."
        REFERENCE
            "ATM Forum PNNI 1.0 Section 5.8.5.2.5.4, Annex G AvCR_PM"
        DEFVAL  { 50 }
        ::= { qbAtmPnniNodeTimerEntry 10 }

qbAtmPnniNodeAvcrMt OBJECT-TYPE
        SYNTAX        INTEGER (1..99)
        UNITS         "percent"
        MAX-ACCESS    read-create
        STATUS        current
        DESCRIPTION
            "The minimum threshold used in the algorithms that determine
             significant change for AvCR parameters, expressed as a
             percentage."
        REFERENCE
            "ATM Forum PNNI 1.0 Section 5.8.5.2.5.4, Annex G AvCR_mT"
        DEFVAL  { 3 }
        ::= { qbAtmPnniNodeTimerEntry 11 }

qbAtmPnniNodeCdvPm OBJECT-TYPE
        SYNTAX        INTEGER (1..99)
        UNITS         "percent"
        MAX-ACCESS    read-create
        STATUS        current
        DESCRIPTION
            "The proportional multiplier used in the algorithms that
             determine significant change for CDV metrics, expressed as
             a percentage."
        REFERENCE
            "ATM Forum PNNI 1.0 Section 5.8.5.2.5.6, Annex G CDV_PM"
        DEFVAL  { 25 }
        ::= { qbAtmPnniNodeTimerEntry 12 }

qbAtmPnniNodeCtdPm OBJECT-TYPE
        SYNTAX        INTEGER (1..99)
        UNITS         "percent"
        MAX-ACCESS    read-create
        STATUS        current
        DESCRIPTION
            "The proportional multiplier used in the algorithms that
             determine significant change for CTD metrics, expressed as
             a percentage."
        REFERENCE
            "ATM Forum PNNI 1.0 Section 5.8.5.2.5.5, Annex G maxCTD_PM"
        DEFVAL  { 50 }
        ::= { qbAtmPnniNodeTimerEntry 13 }   
        
-- qbAtmPnniLink table - based on ATM Forum PNNI Link Table

qbAtmPnniLinkTable OBJECT-TYPE
        SYNTAX        SEQUENCE OF QbAtmPnniLinkEntry
        MAX-ACCESS    not-accessible
        STATUS        current
        DESCRIPTION
            "This table contains the attributes necessary to describe
             the operation of logical links attached to the local
             switching system and the relationship with the neighbor
             nodes on the other end of the links.  Links are attached to
             a specific node within the switching system.  A
             concatenation of the Node Index of the node within the
             local switching system and the port ID are used as the
             instance ID to uniquely identify the link.  Links may
             represent horizontal links between lowest level neighboring
             peers, outside links, uplinks, or horizontal links to/from
             LGNs.

             The entire qbAtmPnniLink object is read-only, reflecting the
             fact that this information is discovered dynamically by the
             PNNI protocol rather than configured."
        REFERENCE
            "ATM Forum PNNI 1.0 Section 5.6"
        ::= { qbAtmSigGroup 8 }

qbAtmPnniLinkEntry OBJECT-TYPE
        SYNTAX        QbAtmPnniLinkEntry
        MAX-ACCESS    not-accessible
        STATUS        current
        DESCRIPTION
            "An entry in the table, containing information about a link
             attached to a PNNI logical node in this switching system."
        REFERENCE
            "ATM Forum PNNI 1.0 Section 5.6"
        INDEX         { qbAtmPnniNodeIndex,
                        qbAtmPnniLinkPortId }
        ::= { qbAtmPnniLinkTable 1 }

QbAtmPnniLinkEntry ::=
        SEQUENCE {
                qbAtmPnniLinkPortId          Integer32,
                qbAtmPnniLinkType            INTEGER,
                qbAtmPnniLinkHelloState      QbAtmPnniHelloState
                }

qbAtmPnniLinkPortId OBJECT-TYPE
        SYNTAX        Integer32
        MAX-ACCESS    not-accessible
        STATUS        current
        DESCRIPTION
            "The Port Identifier of the link as selected by the local
             node.  This value has meaning only within the context of
             the node to which the port is attached."
        ::= { qbAtmPnniLinkEntry 1 }

qbAtmPnniLinkType OBJECT-TYPE
        SYNTAX        INTEGER {
                                unknown(1),
                                lowestLevelHorizontalLink(2),
                                horizontalLinkToFromLgn(3),
                                lowestLevelOutsideLink(4),
                                uplink(5),
                                outsideLinkAndUplink(6)
                                }
        MAX-ACCESS    read-only
        STATUS        current
        DESCRIPTION
            "Indicates the type of link being described."
        ::= { qbAtmPnniLinkEntry 2 }


qbAtmPnniLinkHelloState OBJECT-TYPE
        SYNTAX        QbAtmPnniHelloState
        MAX-ACCESS    read-only
        STATUS        current
        DESCRIPTION
            "For horizontal and outside links between lowest-level nodes
             and for links of unknown type, this attribute indicates the
             state of the Hello protocol exchange over this link.  For
             links to/from LGNs, this attribute indicates the state of
             the corresponding LGN Horizontal Link Hello State Machine.
             For uplinks (where the port ID is not also used for the
             underlying outside link), this attribute is set to
             notApplicable."
        REFERENCE
            "ATM Forum PNNI 1.0 Section 5.6.2.1.2"
        ::= { qbAtmPnniLinkEntry 3 }
                                                   
-- ****************************************************************
-- Extension to the ATM VCL Table for hidden interfaces
-- ****************************************************************

qbAtmHiddenVclTable	OBJECT-TYPE
	SYNTAX	SEQUENCE OF QbAtmHiddenVclEntry
	MAX-ACCESS	not-accessible
	STATUS	current
	DESCRIPTION
		"The QB table with extensions to atmVclTable"
	::= { qbAtmObjects 8 }

qbAtmHiddenVclEntry OBJECT-TYPE 
	SYNTAX QbAtmHiddenVclEntry 
        MAX-ACCESS not-accessible 
        STATUS current 
        DESCRIPTION "Entry in qbAtmHiddenVclTable" 
	AUGMENTS { atmVclEntry }
		
	::= { qbAtmHiddenVclTable 1 }

QbAtmHiddenVclEntry ::=
	SEQUENCE	{
		qbAtmHiddenVclName		DisplayString,
		qbAtmHiddenVclType		INTEGER,
		qbAtmHiddenVclIfIndex		InterfaceIndex,
		qbAtmHiddenVclVtIndex		INTEGER
		}


qbAtmHiddenVclName OBJECT-TYPE
	SYNTAX      DisplayString (SIZE (0..20))
	MAX-ACCESS  read-create
	STATUS      current
	DESCRIPTION
		"A textual description for the SPVC name."
	DEFVAL  { "" }
    ::= { qbAtmHiddenVclEntry 1 }


qbAtmHiddenVclType OBJECT-TYPE
	SYNTAX      INTEGER {
			 native (1),
			 non-native (2)
		    }
	MAX-ACCESS  read-create
	STATUS      current
	DESCRIPTION
		"Value for the interface type.  
                 Native ATM - Q155, S622; 
		 non-native - E1, DS1, ENET, VT"
    ::= { qbAtmHiddenVclEntry 2 }


qbAtmHiddenVclIfIndex OBJECT-TYPE
	SYNTAX      InterfaceIndex
	MAX-ACCESS  read-create
	STATUS      current
	DESCRIPTION
		"ifIndex value for IOT interface.  This
	         value is not used for native ATM interfaces."
    ::= { qbAtmHiddenVclEntry 3 }

  qbAtmHiddenVclVtIndex OBJECT-TYPE
	SYNTAX      INTEGER
	MAX-ACCESS  read-create
	STATUS      current
	DESCRIPTION
		"Address information for a VT card.  This
	         value is used for VT cards only."
    ::= { qbAtmHiddenVclEntry 4 }
                               
                                                              
                               
-- ****************************************************************
-- This section defines the swicth statistics collected
