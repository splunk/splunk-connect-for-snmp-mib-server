qbAtmSwitchModuleGroup OBJECT IDENTIFIER ::= { qbAtmObjects 9 }
	
qbAtmStatsBuffOverCells OBJECT-TYPE
	SYNTAX      Counter32
	MAX-ACCESS  read-only
	STATUS      current
	DESCRIPTION
		"This object gives the number of cells discarded due to
		buffer overflows."
    ::= {  qbAtmSwitchModuleGroup 1 }
    
qbAtmStatsInvalidCells OBJECT-TYPE
	SYNTAX      Counter32
	MAX-ACCESS  read-only
	STATUS      current
	DESCRIPTION
		"This object gives the number of cells discarded due to
		unknown VPI/VCI."
    ::= {  qbAtmSwitchModuleGroup 2 }
    
qbAtmStatsInvUpdateCells OBJECT-TYPE
	SYNTAX      Counter32
	MAX-ACCESS  read-only
	STATUS      current
	DESCRIPTION
		"This object gives the number of cells discarded due to
		internal switch errors."
    ::= {  qbAtmSwitchModuleGroup 3 }

-- **************************************************************
-- ATM Interface Statistics Table
-- **************************************************************

   qbAtmInterfaceStatTable       OBJECT-TYPE
	SYNTAX      SEQUENCE OF QbAtmInterfaceStatEntry
	MAX-ACCESS  not-accessible
	STATUS      current
	DESCRIPTION
            "The Quantum Bridge ATM Interface Statistic Table contains 
	    ATM layer statistics for each ATM interface."
    ::= {  qbAtmObjects 10 }
 
   qbAtmInterfaceStatEntry    OBJECT-TYPE
	SYNTAX      QbAtmInterfaceStatEntry
	MAX-ACCESS  not-accessible
	STATUS      current
	DESCRIPTION
            "An entry in the Quantum ATM Interface Table. 
	    Every ATM interface contains a corresponding
	    entry in the table."
    INDEX   { ifIndex }
    ::=  { qbAtmInterfaceStatTable 1 }

   QbAtmInterfaceStatEntry ::=
	SEQUENCE {
		qbAtmInterfaceStatTxOAMCells     Counter32,
		qbAtmInterfaceStatRxOAMCells	 Counter32
    }

    qbAtmInterfaceStatTxOAMCells  OBJECT-TYPE
        SYNTAX        Counter32
        MAX-ACCESS    read-only
        STATUS        current
        DESCRIPTION
            "The total number of valid OAM cells transmitted  by this
	    atm layer interface." 
        ::= { qbAtmInterfaceStatEntry  1 }

   qbAtmInterfaceStatRxOAMCells  OBJECT-TYPE
        SYNTAX        Counter32
        MAX-ACCESS    read-only
        STATUS        current
        DESCRIPTION
            "The total number of OAS ATM cells received by this atm
	    layer interface."
        ::= { qbAtmInterfaceStatEntry  2 }

   --
   --  This table defines a set of in-band management IP addresses and the ATM connections
   --  that can access them. It is based largely on the MIB defined in Sudha Mulpur's document
   --  'Multiple In-Band Management PVC/Soft PVC Functional and Design Specification'.
   --

   qbAtmInBandMgtTable  OBJECT-TYPE
      SYNTAX        SEQUENCE OF QbAtmInBandMgtEntry
      MAX-ACCESS    not-accessible
      STATUS        current
      DESCRIPTION
            "The QbAtmInBandMgtTable defines in-band Management IP channels for use by ATM connections."
        ::= { qbAtmObjects 11 }
        
  qbAtmTrafficDescriptorTypes OBJECT IDENTIFIER ::= {qbAtmObjects 12}
  
  qbAtmPkt  OBJECT-IDENTITY
    STATUS  current
    DESCRIPTION
        "This traffic descriptor type is to define a connection with an Ethernet endpoint, and
        it is for a user that does not want to use ATM parameters directly.  The EtherNet bandwidth 
        will be converted to a nrtVbr traffic descriptor. The conversion from Ethernet bandwidth to 
        ATM cell rate must add an ATM cell tax. This is defined using the pcbe (packet-to-cell bandwidth expansion)
        factor. If this is not defined for the traffic descriptor, then the system default (qbAtmDefaultPCBE)should be used.
         
        The use of the parameter vector for this type:
        Parameter 1: minbw, the minimum guarnteed bandwidth fo rthe connection. This is defined in Mb/sec. (0-100).            
        Parameter 2: maxbw, the maximum bursting capability of the connection. This is defined in Mb/sec (1-100).
                                 It is default to minbw.
        Parameter 3: bw, indicates the bandwidth  for the PON segment. Vaules are expressed in Mb, and may take 
                                 interger value between 1 and 100. (only apply to 155Mb PON). It is default to minbw.
        Parameter 4: pcbe, it defines the packed-to-cell bandwidth expansion. This is the percentage overhead that
                                 is used to convert Ethernet bandwidth to ATM cell rates. It is default to qbAtmDefaultPCBE.
        Parameter 5: not used."
    REFERENCE
        ""
    ::= {qbAtmTrafficDescriptorTypes 1}


   qbAtmInBandMgtEntry OBJECT-TYPE
	SYNTAX        QbAtmInBandMgtEntry
        MAX-ACCESS    not-accessible
        STATUS        current
        DESCRIPTION
            "An entry in the table, containing information about an in-band Management
             channel."
        INDEX         { qbAtmInBandMgtIpAddr }
        ::= { qbAtmInBandMgtTable 1 }

   QbAtmInBandMgtEntry ::=
        SEQUENCE {
		qbAtmInBandMgtIpAddr		IpAddress,                  
		qbAtmInBandMgtIpMask		IpAddress,  
		qbAtmInBandMgtIpIfIndex		InterfaceIndex,
		qbAtmInBandMgtNeighborIp	IpAddress,
		qbAtmInBandMgtIfIndex		InterfaceIndex,
		qbAtmInBandMgtVpi			AtmVpIdentifier,
		qbAtmInBandMgtVci			AtmVcIdentifier,
		qbAtmInBandMgtConnKind		AtmConnKind,
		qbAtmInBandMgtTxTd			AtmTrafficDescrParamIndex,
		qbAtmInBandMgtRxTd          AtmTrafficDescrParamIndex,   			
		qbAtmInBandMgtTargetNsapAddr		AtmAddr,
		qbAtmInBandMgtRowStatus		RowStatus
	}

    qbAtmInBandMgtIpAddr OBJECT-TYPE
        SYNTAX        IpAddress
        MAX-ACCESS    not-accessible
        STATUS        current
        DESCRIPTION
            "An IP Address for in-band SNMP Management of this chassis."
        ::= { qbAtmInBandMgtEntry 1 }    
        
    qbAtmInBandMgtIpMask OBJECT-TYPE
        SYNTAX        IpAddress
        MAX-ACCESS    read-create
        STATUS        current
        DESCRIPTION
            "The IP Network Mask associated with this in-band SNMP Management IP Address."
        ::= { qbAtmInBandMgtEntry 2 }
        
    qbAtmInBandMgtIpIfIndex OBJECT-TYPE
        SYNTAX        InterfaceIndex
        MAX-ACCESS    read-create
        STATUS        current
        DESCRIPTION
            "The Interface associated with this in-band management address."
        ::= { qbAtmInBandMgtEntry 3 }       
        
    qbAtmInBandMgtNeighborIp OBJECT-TYPE
        SYNTAX        IpAddress
        MAX-ACCESS    read-create
        STATUS        current
        DESCRIPTION
            "This is the neighbor IP address associated with the in-band Management Interface. If the
            qbAtmInBandMgtConnKind is any value other than pvc(1), this object is non-applicable."
        ::= { qbAtmInBandMgtEntry 4 }

    qbAtmInBandMgtIfIndex OBJECT-TYPE
        SYNTAX        InterfaceIndex
        MAX-ACCESS    read-create
        STATUS        current
        DESCRIPTION
            "This is the interface index associated with the in-band Management connection. If the
            qbAtmInBandMgtConnKind is any value other than pvc(1), this object is non-applicable."
        ::= { qbAtmInBandMgtEntry 5 }  
        
    qbAtmInBandMgtVpi OBJECT-TYPE
        SYNTAX        AtmVpIdentifier
        MAX-ACCESS    read-create
        STATUS        current
        DESCRIPTION
            "If the value of qbAtmInBandMgtConnKind is pvc(1), this is the VPI associated with 
            this in-band management connection. If the value of qbAtmInBandMgtConnKind is spvcInitiator(4),
            this object is target VPI. In all other cases, the value of this object is non-applicable."
        ::= { qbAtmInBandMgtEntry 6 }
        
    qbAtmInBandMgtVci OBJECT-TYPE
        SYNTAX        AtmVcIdentifier
        MAX-ACCESS    read-create
        STATUS        current
        DESCRIPTION
            "If the value of qbAtmInBandMgtConnKind is pvc(1), this is the VCI associated with 
            this in-band management connection. If the value of qbAtmInBandMgtConnKind is spvcInitiator(4),
            this object is target VCI. In all other cases, the value of this object is non-applicable."
        ::= { qbAtmInBandMgtEntry 7 }
        
    qbAtmInBandMgtConnKind OBJECT-TYPE
        SYNTAX        AtmConnKind
        MAX-ACCESS    read-create
        STATUS        current
        DESCRIPTION
            "This is the connection type associated with the in-band Management connection. The currently 
            accepted types are pvc(1), spvcInitiator(4) and spvcTarget(5)."
        ::= { qbAtmInBandMgtEntry 8 }

    qbAtmInBandMgtTxTd OBJECT-TYPE
        SYNTAX        AtmTrafficDescrParamIndex
        MAX-ACCESS    read-create
        STATUS        current
        DESCRIPTION
            "The Transmit traffic descriptor associated with this in-band management channel."
        ::= { qbAtmInBandMgtEntry 9 }   
        
    qbAtmInBandMgtRxTd OBJECT-TYPE
        SYNTAX        AtmTrafficDescrParamIndex
        MAX-ACCESS    read-create
        STATUS        current
        DESCRIPTION
            "The Receive traffic descriptor associated with this in-band management channel."
        ::= { qbAtmInBandMgtEntry 10 }
                                         
	qbAtmInBandMgtTargetNsapAddr OBJECT-TYPE
        SYNTAX        AtmAddr
        MAX-ACCESS    read-create
        STATUS        current
        DESCRIPTION
            "If the value of qbAtmInBandMgtConnKind is spvcInitiator(4) this object 
            represetns the Target NSAP Address of the connection. In all other cases, 
            the value of this object is non-applicable."
        ::= { qbAtmInBandMgtEntry 11 }                                        

     qbAtmInBandMgtRowStatus OBJECT-TYPE
        SYNTAX        RowStatus
        MAX-ACCESS    read-create
        STATUS        current
        DESCRIPTION
            "To create, or delete an Entry in this table.
				Setting this object to createAndGo(4) causes 
				the corresponding row to be created.

				Setting this object to destroy(6) causes the corresponding
				row to be deleted.
						
				Note that -- currently-- only a single instance of a row 
				can be created in this table."
        ::= { qbAtmInBandMgtEntry 12 }


-- ****************************************************************
-- Conformance Information
