--     AtmEbr Enterprise MIB."

atmEbrMIB OBJECT IDENTIFIER ::= { mscPassportMIBs 159 }










-- AtmIf/n Vcc/vpi.vci SrcPvc EdgeBasedReroutingOverride
-- AtmIf/n Vpc/vpi SrcPvp EdgeBasedReroutingOverride
-- AtmIf/n Vpt/vpi Vcc/vci SrcPvc EdgeBasedReroutingOverride
-- 
-- This component represents the ability to override Edge Based 
-- Rerouting capabilities on a per connection basis for provisioned 
-- SPVC and SPVP connections. 
-- 
-- The addition of this component and changes to the attributes 
-- contained in this component are critical to the parent SPVC or 
-- SPVP. The deletion of this component is not a critical change for 
-- the connection. If this component is deleted, the connection remains 
-- active with the current subscription options until it restages.

mscAtmIfVpcSrcEbrOv OBJECT IDENTIFIER ::= { mscAtmIfVpcSrc 2 }



mscAtmIfVpcSrcEbrOvRowStatusTable OBJECT-TYPE
   SYNTAX     SEQUENCE OF MscAtmIfVpcSrcEbrOvRowStatusEntry
   ACCESS     not-accessible
   STATUS     mandatory
   DESCRIPTION
     "This entry controls the addition and deletion of 
      mscAtmIfVpcSrcEbrOv components."
::= { mscAtmIfVpcSrcEbrOv 1 }

mscAtmIfVpcSrcEbrOvRowStatusEntry OBJECT-TYPE
   SYNTAX     MscAtmIfVpcSrcEbrOvRowStatusEntry
   ACCESS     not-accessible
   STATUS     mandatory
   DESCRIPTION
     "A single entry in the table represents a single 
      mscAtmIfVpcSrcEbrOv component."
   INDEX {
      mscAtmIfIndex,
      mscAtmIfVpcIndex,
      mscAtmIfVpcSrcIndex,
      mscAtmIfVpcSrcEbrOvIndex }
::= { mscAtmIfVpcSrcEbrOvRowStatusTable 1 }

MscAtmIfVpcSrcEbrOvRowStatusEntry ::= SEQUENCE {
   mscAtmIfVpcSrcEbrOvRowStatus
      RowStatus,
   mscAtmIfVpcSrcEbrOvComponentName
      DisplayString,
   mscAtmIfVpcSrcEbrOvStorageType
      StorageType,
   mscAtmIfVpcSrcEbrOvIndex
      NonReplicated }

mscAtmIfVpcSrcEbrOvRowStatus OBJECT-TYPE
   SYNTAX     RowStatus 
   ACCESS     read-write
   STATUS     mandatory
   DESCRIPTION
     "This variable is used as the basis for SNMP naming of
      mscAtmIfVpcSrcEbrOv components.
      These components can be added and deleted."
::= { mscAtmIfVpcSrcEbrOvRowStatusEntry 1 }

mscAtmIfVpcSrcEbrOvComponentName OBJECT-TYPE
   SYNTAX     DisplayString 
   ACCESS     read-only
   STATUS     mandatory
   DESCRIPTION
     "This variable provides the component's string name for
      use with the ASCII Console Interface"
::= { mscAtmIfVpcSrcEbrOvRowStatusEntry 2 }

mscAtmIfVpcSrcEbrOvStorageType OBJECT-TYPE
   SYNTAX     StorageType 
   ACCESS     read-only
   STATUS     mandatory
   DESCRIPTION
     "This variable represents the storage type value for the
      mscAtmIfVpcSrcEbrOv tables."
::= { mscAtmIfVpcSrcEbrOvRowStatusEntry 4 }

mscAtmIfVpcSrcEbrOvIndex OBJECT-TYPE
   SYNTAX     NonReplicated 
   ACCESS     not-accessible
   STATUS     mandatory
   DESCRIPTION
     "This variable represents the index for the mscAtmIfVpcSrcEbrOv tables."
::= { mscAtmIfVpcSrcEbrOvRowStatusEntry 10 }

mscAtmIfVpcSrcEbrOvProvTable OBJECT-TYPE
   SYNTAX     SEQUENCE OF MscAtmIfVpcSrcEbrOvProvEntry
   ACCESS     not-accessible
   STATUS     mandatory
   DESCRIPTION
     "This group contains the provisionable attributes for the Edge Based 
      Rerouting subscription options for provisioned connections."
::= { mscAtmIfVpcSrcEbrOv 20 }

mscAtmIfVpcSrcEbrOvProvEntry OBJECT-TYPE
   SYNTAX     MscAtmIfVpcSrcEbrOvProvEntry
   ACCESS     not-accessible
   STATUS     mandatory
   DESCRIPTION
     "An entry in the mscAtmIfVpcSrcEbrOvProvTable."
   INDEX {
      mscAtmIfIndex,
      mscAtmIfVpcIndex,
      mscAtmIfVpcSrcIndex,
      mscAtmIfVpcSrcEbrOvIndex }
::= { mscAtmIfVpcSrcEbrOvProvTable 1 }

MscAtmIfVpcSrcEbrOvProvEntry ::= SEQUENCE {
   mscAtmIfVpcSrcEbrOvRecoverySubscribed
      INTEGER,
   mscAtmIfVpcSrcEbrOvOptimizationSubscribed
      INTEGER }

mscAtmIfVpcSrcEbrOvRecoverySubscribed OBJECT-TYPE
   SYNTAX     INTEGER  {
                 no(0),
                 yes(1) }
   ACCESS     read-write
   STATUS     mandatory
   DESCRIPTION
     "This attributes specifies whether connection recovery is subscribed 
      for this connection."
   DEFVAL     { yes }
::= { mscAtmIfVpcSrcEbrOvProvEntry 1 }

mscAtmIfVpcSrcEbrOvOptimizationSubscribed OBJECT-TYPE
   SYNTAX     INTEGER  {
                 no(0),
                 yes(1) }
   ACCESS     read-write
   STATUS     mandatory
   DESCRIPTION
     "This attributes specifies whether path optimization is subscribed for 
      this connection."
   DEFVAL     { yes }
::= { mscAtmIfVpcSrcEbrOvProvEntry 2 }


-- AtmIf/n Vcc/vpi.vci EdgeBasedReroutingInformation
-- AtmIf/n Vpc/vpi EdgeBasedReroutingInformation
-- AtmIf/n Vpt/vpi Vcc/vci EdgeBasedReroutingInformation
-- 
-- This component represents the Edge Based Rerouting capabilities 
-- of a connection.

