Nortel-MsCarrier-MscPassport-AtmEbrMIB
DEFINITIONS ::= BEGIN



IMPORTS

      OBJECT-TYPE
   FROM RFC-1212

      mscPassportMIBs
   FROM Nortel-MsCarrier-MscPassport-UsefulDefinitionsMIB

      Counter32,
      DisplayString,
      StorageType,
      RowStatus,
      Gauge32
   FROM Nortel-MsCarrier-MscPassport-StandardTextualConventionsMIB

      NonReplicated
   FROM Nortel-MsCarrier-MscPassport-TextualConventionsMIB

      mscAtmIfVpcSrc,
      mscAtmIfVptVccSrcIndex,
      mscAtmIfVptVccSrc,
      mscAtmIfVccSrcIndex,
      mscAtmIfVccSrc,
      mscAtmIfVpcSrcIndex
   FROM Nortel-MsCarrier-MscPassport-AtmNetworkingMIB

      mscAtmIfVptVcc,
      mscAtmIfVccIndex,
      mscAtmIfVptIndex,
      mscAtmIfVcc,
      mscAtmIfVpcIndex,
      mscAtmIfVpc,
      mscAtmIfVptVccIndex,
      mscAtmIfIndex
   FROM Nortel-MsCarrier-MscPassport-AtmCoreMIB

      mscAtmIfUni,
      mscAtmIfVptUniIndex,
      mscAtmIfVptUni,
      mscAtmIfUniIndex
   FROM Nortel-MsCarrier-MscPassport-AtmUniMIB

      mscAtmIfVptIisp,
      mscAtmIfIispIndex,
      mscAtmIfIisp,
      mscAtmIfVptIispIndex
   FROM Nortel-MsCarrier-MscPassport-AtmIispMIB

      mscAtmIfVptPnni,
      mscAtmIfPnniIndex,
      mscAtmIfPnni,
      mscAtmIfVptPnniIndex
   FROM Nortel-MsCarrier-MscPassport-AtmPnniMIB;




