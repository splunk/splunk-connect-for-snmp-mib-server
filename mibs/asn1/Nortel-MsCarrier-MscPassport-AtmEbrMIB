-- AtmIf/n Iisp EdgeBasedRerouting
-- AtmIf/n Uni EdgeBasedRerouting
-- AtmIf/n Pnni EdgeBasedRerouting
-- AtmIf/n Vpt/vpi Iisp EdgeBasedRerouting
-- AtmIf/n Vpt/vpi Uni EdgeBasedRerouting
-- AtmIf/n Vpt/vpi Pnni EdgeBasedRerouting
-- 
-- This component represents the Edge Based Rerouting capabilities 
-- of the ATM Signaling interface. If this component is provisioned, 
-- the Artg Pnni component must already exist. The addition or 
-- deletion of this component, or changes to the provisioned 
-- subscription options, does not affect existing connections.

mscAtmIfUniEbr OBJECT IDENTIFIER ::= { mscAtmIfUni 7 }



mscAtmIfUniEbrRowStatusTable OBJECT-TYPE
   SYNTAX     SEQUENCE OF MscAtmIfUniEbrRowStatusEntry
   ACCESS     not-accessible
   STATUS     mandatory
   DESCRIPTION
     "This entry controls the addition and deletion of 
      mscAtmIfUniEbr components."
::= { mscAtmIfUniEbr 1 }

mscAtmIfUniEbrRowStatusEntry OBJECT-TYPE
   SYNTAX     MscAtmIfUniEbrRowStatusEntry
   ACCESS     not-accessible
   STATUS     mandatory
   DESCRIPTION
     "A single entry in the table represents a single 
      mscAtmIfUniEbr component."
   INDEX {
      mscAtmIfIndex,
      mscAtmIfUniIndex,
      mscAtmIfUniEbrIndex }
::= { mscAtmIfUniEbrRowStatusTable 1 }

MscAtmIfUniEbrRowStatusEntry ::= SEQUENCE {
   mscAtmIfUniEbrRowStatus
      RowStatus,
   mscAtmIfUniEbrComponentName
      DisplayString,
   mscAtmIfUniEbrStorageType
      StorageType,
   mscAtmIfUniEbrIndex
      NonReplicated }

mscAtmIfUniEbrRowStatus OBJECT-TYPE
   SYNTAX     RowStatus 
   ACCESS     read-write
   STATUS     mandatory
   DESCRIPTION
     "This variable is used as the basis for SNMP naming of
      mscAtmIfUniEbr components.
      These components can be added and deleted."
::= { mscAtmIfUniEbrRowStatusEntry 1 }

mscAtmIfUniEbrComponentName OBJECT-TYPE
   SYNTAX     DisplayString 
   ACCESS     read-only
   STATUS     mandatory
   DESCRIPTION
     "This variable provides the component's string name for
      use with the ASCII Console Interface"
::= { mscAtmIfUniEbrRowStatusEntry 2 }

mscAtmIfUniEbrStorageType OBJECT-TYPE
   SYNTAX     StorageType 
   ACCESS     read-only
   STATUS     mandatory
   DESCRIPTION
     "This variable represents the storage type value for the
      mscAtmIfUniEbr tables."
::= { mscAtmIfUniEbrRowStatusEntry 4 }

mscAtmIfUniEbrIndex OBJECT-TYPE
   SYNTAX     NonReplicated 
   ACCESS     not-accessible
   STATUS     mandatory
   DESCRIPTION
     "This variable represents the index for the mscAtmIfUniEbr tables."
::= { mscAtmIfUniEbrRowStatusEntry 10 }

mscAtmIfUniEbrProvTable OBJECT-TYPE
   SYNTAX     SEQUENCE OF MscAtmIfUniEbrProvEntry
   ACCESS     not-accessible
   STATUS     mandatory
   DESCRIPTION
     "This group contains the provisionable attributes for Edge Based 
      Rerouting at the ATM Signaling interface."
::= { mscAtmIfUniEbr 20 }

mscAtmIfUniEbrProvEntry OBJECT-TYPE
   SYNTAX     MscAtmIfUniEbrProvEntry
   ACCESS     not-accessible
   STATUS     mandatory
   DESCRIPTION
     "An entry in the mscAtmIfUniEbrProvTable."
   INDEX {
      mscAtmIfIndex,
      mscAtmIfUniIndex,
      mscAtmIfUniEbrIndex }
::= { mscAtmIfUniEbrProvTable 1 }

MscAtmIfUniEbrProvEntry ::= SEQUENCE {
   mscAtmIfUniEbrConnectionRecovery
      OCTET STRING,
   mscAtmIfUniEbrPathOptimization
      OCTET STRING }

mscAtmIfUniEbrConnectionRecovery OBJECT-TYPE
   SYNTAX     OCTET STRING (SIZE (1))
   ACCESS     read-write
   STATUS     mandatory
   DESCRIPTION
     "This attribute specifies the subscription of connections to hard 
      rerouting.
      dynamic: Connections that are transiting through this interface.
      provisioned: SPVC and SPVP connections that are provisioned at 
      this interface.

      Description of bits:
      dynamic(0)
      provisioned(1)"
   DEFVAL     { 'c0'H }
::= { mscAtmIfUniEbrProvEntry 1 }

mscAtmIfUniEbrPathOptimization OBJECT-TYPE
   SYNTAX     OCTET STRING (SIZE (1))
   ACCESS     read-write
   STATUS     mandatory
   DESCRIPTION
     "This attribute specifies the subscription of connections to soft 
      rerouting.
      dynamic: Connections that are transiting through this interface.
      provisioned: SPVC and SPVP connections that are provisioned at 
      this interface.

      Description of bits:
      dynamic(0)
      provisioned(1)"
   DEFVAL     { 'c0'H }
::= { mscAtmIfUniEbrProvEntry 2 }

mscAtmIfUniEbrOperTable OBJECT-TYPE
   SYNTAX     SEQUENCE OF MscAtmIfUniEbrOperEntry
   ACCESS     not-accessible
   STATUS     mandatory
   DESCRIPTION
     "This group contains the operational attributes that indicate the 
      rerouting activities for an ATM Signaling interface."
::= { mscAtmIfUniEbr 30 }

mscAtmIfUniEbrOperEntry OBJECT-TYPE
   SYNTAX     MscAtmIfUniEbrOperEntry
   ACCESS     not-accessible
   STATUS     mandatory
   DESCRIPTION
     "An entry in the mscAtmIfUniEbrOperTable."
   INDEX {
      mscAtmIfIndex,
      mscAtmIfUniIndex,
      mscAtmIfUniEbrIndex }
::= { mscAtmIfUniEbrOperTable 1 }

MscAtmIfUniEbrOperEntry ::= SEQUENCE {
   mscAtmIfUniEbrSubscribedConnections
      Gauge32,
   mscAtmIfUniEbrEligibleRecoveredConnections
      Gauge32,
   mscAtmIfUniEbrIneligibleRecoveredConnections
      Gauge32 }

mscAtmIfUniEbrSubscribedConnections OBJECT-TYPE
   SYNTAX     Gauge32 (0..4294967295)
   ACCESS     read-only
   STATUS     mandatory
   DESCRIPTION
     "This attribute indicates the number of connections subscribed to 
      rerouting capabilities."
::= { mscAtmIfUniEbrOperEntry 1 }

mscAtmIfUniEbrEligibleRecoveredConnections OBJECT-TYPE
   SYNTAX     Gauge32 (0..4294967295)
   ACCESS     read-only
   STATUS     mandatory
   DESCRIPTION
     "This attribute indicates the number of connections that have been 
      recovered and are eligible for path optimization. The count is reset 
      to zero when the Optimize command is executed on this component."
::= { mscAtmIfUniEbrOperEntry 2 }

mscAtmIfUniEbrIneligibleRecoveredConnections OBJECT-TYPE
   SYNTAX     Gauge32 (0..4294967295)
   ACCESS     read-only
   STATUS     mandatory
   DESCRIPTION
     "This attribute indicates the number of connections that have been 
      recovered but are not eligible for path optimization. The value 
      decreases as counted connections are released. The count is not 
      reset by the Optimize command."
::= { mscAtmIfUniEbrOperEntry 3 }

mscAtmIfUniEbrStatsTable OBJECT-TYPE
   SYNTAX     SEQUENCE OF MscAtmIfUniEbrStatsEntry
   ACCESS     not-accessible
   STATUS     mandatory
   DESCRIPTION
     "This group contains the statistics related to Edge Based Rerouting."
::= { mscAtmIfUniEbr 40 }

mscAtmIfUniEbrStatsEntry OBJECT-TYPE
   SYNTAX     MscAtmIfUniEbrStatsEntry
   ACCESS     not-accessible
   STATUS     mandatory
   DESCRIPTION
     "An entry in the mscAtmIfUniEbrStatsTable."
   INDEX {
      mscAtmIfIndex,
      mscAtmIfUniIndex,
      mscAtmIfUniEbrIndex }
::= { mscAtmIfUniEbrStatsTable 1 }

MscAtmIfUniEbrStatsEntry ::= SEQUENCE {
   mscAtmIfUniEbrTotalConnectionRecoveries
      Counter32,
   mscAtmIfUniEbrTotalPathOptimizations
      Counter32 }

mscAtmIfUniEbrTotalConnectionRecoveries OBJECT-TYPE
   SYNTAX     Counter32 
   ACCESS     read-only
   STATUS     mandatory
   DESCRIPTION
     "This attribute counts connection recovery, or hard reroute, events 
      that occur. The counter wraps to 0 when the maximum value is 
      exceeded."
::= { mscAtmIfUniEbrStatsEntry 1 }

mscAtmIfUniEbrTotalPathOptimizations OBJECT-TYPE
   SYNTAX     Counter32 
   ACCESS     read-only
   STATUS     mandatory
   DESCRIPTION
     "This attributes counts path optimization, or soft reroute, events that 
      occur. The counter wraps to 0 when the maximum value is 
      exceeded."
::= { mscAtmIfUniEbrStatsEntry 2 }


