   ACCESS     not-accessible
   STATUS     mandatory
   DESCRIPTION
     "This variable represents the index for the mscAtmIfVpcEbrInfo tables."
::= { mscAtmIfVpcEbrInfoRowStatusEntry 10 }

mscAtmIfVpcEbrInfoOperTable OBJECT-TYPE
   SYNTAX     SEQUENCE OF MscAtmIfVpcEbrInfoOperEntry
   ACCESS     not-accessible
   STATUS     mandatory
   DESCRIPTION
     "This group contains the attributes that indicates the Edge Based 
      Rerouting characteristics of a connection."
::= { mscAtmIfVpcEbrInfo 30 }

mscAtmIfVpcEbrInfoOperEntry OBJECT-TYPE
   SYNTAX     MscAtmIfVpcEbrInfoOperEntry
   ACCESS     not-accessible
   STATUS     mandatory
   DESCRIPTION
     "An entry in the mscAtmIfVpcEbrInfoOperTable."
   INDEX {
      mscAtmIfIndex,
      mscAtmIfVpcIndex,
      mscAtmIfVpcEbrInfoIndex }
::= { mscAtmIfVpcEbrInfoOperTable 1 }

MscAtmIfVpcEbrInfoOperEntry ::= SEQUENCE {
   mscAtmIfVpcEbrInfoRecoverySubscribed
      INTEGER,
   mscAtmIfVpcEbrInfoOptimizationSubscribed
      INTEGER,
   mscAtmIfVpcEbrInfoConnectionRecovered
      INTEGER }

mscAtmIfVpcEbrInfoRecoverySubscribed OBJECT-TYPE
   SYNTAX     INTEGER  {
                 no(0),
                 yes(1) }
   ACCESS     read-only
   STATUS     mandatory
   DESCRIPTION
     "This attribute indicates whether the connection is subscribed to 
      connection recovery."
::= { mscAtmIfVpcEbrInfoOperEntry 1 }

mscAtmIfVpcEbrInfoOptimizationSubscribed OBJECT-TYPE
   SYNTAX     INTEGER  {
                 no(0),
                 yes(1) }
   ACCESS     read-only
   STATUS     mandatory
   DESCRIPTION
     "This attribute indicates whether the connection is subscribed to path 
      optimization."
::= { mscAtmIfVpcEbrInfoOperEntry 2 }

mscAtmIfVpcEbrInfoConnectionRecovered OBJECT-TYPE
   SYNTAX     INTEGER  {
                 no(0),
                 yes(1) }
   ACCESS     read-only
   STATUS     mandatory
   DESCRIPTION
     "This attribute indicates whether the connection has been recovered 
      since the last optimization occurred. The value is reset to no by the 
      Optimize command."
::= { mscAtmIfVpcEbrInfoOperEntry 3 }

mscAtmIfVpcEbrInfoStatsTable OBJECT-TYPE
   SYNTAX     SEQUENCE OF MscAtmIfVpcEbrInfoStatsEntry
   ACCESS     not-accessible
   STATUS     mandatory
   DESCRIPTION
     "This group contains the statistics related to Edge Based Rerouting."
::= { mscAtmIfVpcEbrInfo 40 }

mscAtmIfVpcEbrInfoStatsEntry OBJECT-TYPE
   SYNTAX     MscAtmIfVpcEbrInfoStatsEntry
   ACCESS     not-accessible
   STATUS     mandatory
   DESCRIPTION
     "An entry in the mscAtmIfVpcEbrInfoStatsTable."
   INDEX {
      mscAtmIfIndex,
      mscAtmIfVpcIndex,
      mscAtmIfVpcEbrInfoIndex }
::= { mscAtmIfVpcEbrInfoStatsTable 1 }

MscAtmIfVpcEbrInfoStatsEntry ::= SEQUENCE {
   mscAtmIfVpcEbrInfoTotalConnectionRecoveries
      Counter32,
   mscAtmIfVpcEbrInfoTotalPathOptimizations
      Counter32 }

mscAtmIfVpcEbrInfoTotalConnectionRecoveries OBJECT-TYPE
   SYNTAX     Counter32 
   ACCESS     read-only
   STATUS     mandatory
   DESCRIPTION
     "This attribute counts connection recovery, or hard reroute, events 
      that occur. The counter wraps to 0 when the maximum value is 
      exceeded."
::= { mscAtmIfVpcEbrInfoStatsEntry 1 }

mscAtmIfVpcEbrInfoTotalPathOptimizations OBJECT-TYPE
   SYNTAX     Counter32 
   ACCESS     read-only
   STATUS     mandatory
   DESCRIPTION
     "This attributes counts path optimization, or soft reroute, events that 
      occur. The counter wraps to 0 when the maximum value is 
      exceeded."
::= { mscAtmIfVpcEbrInfoStatsEntry 2 }


