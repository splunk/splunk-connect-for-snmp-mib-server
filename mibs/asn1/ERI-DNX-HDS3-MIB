ERI-DNX-HDS3-MIB DEFINITIONS ::= BEGIN

IMPORTS
	MODULE-IDENTITY, OBJECT-IDENTITY, OBJECT-TYPE, Integer32,
	Counter32, NOTIFICATION-TYPE
		FROM SNMPv2-SMI
    DisplayString, TEXTUAL-CONVENTION
        FROM SNMPv2-TC
    eriMibs
        FROM ERI-ROOT-SMI
    devices, PortStatus, DecisionType, FunctionSwitch, 
    LinkPortAddress, LinkCmdStatus, trapSequence, OneByteField
        FROM ERI-DNX-SMC-MIB;

eriDNXHybridDS3MIB MODULE-IDENTITY 
      LAST-UPDATED    "200301270000Z"
      ORGANIZATION    "Eastern Research, Inc."
      CONTACT-INFO
