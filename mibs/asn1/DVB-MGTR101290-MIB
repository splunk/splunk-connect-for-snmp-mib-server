					Enable,
				estNoiseMarginCounter
					Counter32,
				estNoiseMarginCounterDiscontinuity
					DateAndTime,
				estNoiseMarginCounterReset
					TruthValue,
				estNoiseMarginLatestError
					DateAndTime,
				estNoiseMarginActiveTime
					ActiveTime,
				estNoiseMarginMeasurementState
					MeasurementState,
				estNoiseMarginValue
					FloatingPoint
			 }

		estNoiseMarginInputNumber OBJECT-TYPE
			SYNTAX InputNumber
