			SEQUENCE { 
				tsTestsPrefPIDInputNumber
					InputNumber,
				tsTestsPrefPIDPID
					PIDPlusOne,
				tsTestsPrefPIDRowStatus
					RowStatus,
				tsTestsPrefPIDReferredIntervalMax
					FloatingPoint
			 }

