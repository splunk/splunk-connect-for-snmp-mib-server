		-- 1.3.6.1.4.1.388.14.3.3.1.6
		wsCcStatsSwitch OBJECT IDENTIFIER ::= { wsCcStatsMIB 6 }

		
		-- 1.3.6.1.4.1.388.14.3.3.1.6.1
		wsCcStatsSwitchTxRetriesPktsTable OBJECT-TYPE
