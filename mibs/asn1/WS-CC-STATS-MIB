					Counter32,
				wsCcStatsSwitchTxRetriesPkts07
					Counter32,
				wsCcStatsSwitchTxRetriesPkts08
					Counter32,
				wsCcStatsSwitchTxRetriesPkts09
					Counter32,
				wsCcStatsSwitchTxRetriesPkts10
					Counter32,
				wsCcStatsSwitchTxRetriesPkts11
					Counter32,
				wsCcStatsSwitchTxRetriesPkts12
					Counter32,
				wsCcStatsSwitchTxRetriesPkts13
					Counter32,
				wsCcStatsSwitchTxRetriesPkts14
					Counter32,
