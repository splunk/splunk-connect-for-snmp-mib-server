			OBJECTS { wsCcStatsMeshIndex, wsCcStatsMeshSumShortTimestamp, wsCcStatsMeshSumShortNumPkts, wsCcStatsMeshSumShortPktsPerSec100, wsCcStatsMeshSumShortPktsPerSecTx100, 
				wsCcStatsMeshSumShortPktsPerSecRx100, wsCcStatsMeshSumShortThroughput, wsCcStatsMeshSumShortThroughputTx, wsCcStatsMeshSumShortThroughputRx, wsCcStatsMeshSumShortAvgBitSpeed, 
				wsCcStatsMeshSumShortAvgMuSignal, wsCcStatsMeshSumShortAvgMuNoise, wsCcStatsMeshSumShortAvgMuSnr, wsCcStatsMeshSumShortPp10kTxWithRetries, wsCcStatsMeshSumShortPp10kDropped, 
				wsCcStatsMeshSumShortTxAvgRetries100, wsCcStatsMeshSumShortPp10kRxUndecrypt, wsCcStatsMeshSumLongTimestamp, wsCcStatsMeshSumLongNumPkts, wsCcStatsMeshSumLongPktsPerSec100, 
				wsCcStatsMeshSumLongPktsPerSecTx100, wsCcStatsMeshSumLongPktsPerSecRx100, wsCcStatsMeshSumLongThroughput, wsCcStatsMeshSumLongThroughputTx, wsCcStatsMeshSumLongThroughputRx, 
				wsCcStatsMeshSumLongAvgBitSpeed, wsCcStatsMeshSumLongAvgMuSignal, wsCcStatsMeshSumLongAvgMuNoise, wsCcStatsMeshSumLongAvgMuSnr, wsCcStatsMeshSumLongPp10kTxWithRetries, 
				wsCcStatsMeshSumLongPp10kDropped, wsCcStatsMeshSumLongTxAvgRetries100, wsCcStatsMeshWlanEssid, wsCcStatsMeshSumLongPp10kRxUndecrypt, wsCcStatsMeshConnType, 
				wsCcStatsMeshMac, wsCcStatsMeshRadioType, wsCcStatsMeshThroughput, wsCcStatsMeshRetries100, wsCcStatsMeshAvgBitSpeed, wsCcStatsMeshRadioIndex
				 }
			STATUS current
			DESCRIPTION 
				"Cell Controller Statistics MIB Mesh Group."
			::= { wsCcStatsMIBGroups 7 }

		
	
	END

