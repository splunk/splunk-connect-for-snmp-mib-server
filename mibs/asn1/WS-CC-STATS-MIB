		-- 1.3.6.1.4.1.388.14.3.3.1.7
		wsCcStatsRadioExt OBJECT IDENTIFIER ::= { wsCcStatsMIB 7 }

		
		-- 1.3.6.1.4.1.388.14.3.3.1.7.1
		wsCcStatsRadioExtTable OBJECT-TYPE
			SYNTAX SEQUENCE OF WsCcStatsRadioExtEntry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"Description."
			::= { wsCcStatsRadioExt 1 }

		
		-- 1.3.6.1.4.1.388.14.3.3.1.7.1.1
		wsCcStatsRadioExtEntry OBJECT-TYPE
			SYNTAX WsCcStatsRadioExtEntry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"Description."
			INDEX { wsCcStatsRadioIndex }
			::= { wsCcStatsRadioExtTable 1 }

		
		WsCcStatsRadioExtEntry ::=
			SEQUENCE { 
				wsCcStatsRadioExtDownLinkThroughput
					Gauge32,
				wsCcStatsRadioExtUpLinkThroughput
					Gauge32
			 }

		-- 1.3.6.1.4.1.388.14.3.3.1.7.1.1.1
		wsCcStatsRadioExtDownLinkThroughput OBJECT-TYPE
			SYNTAX Gauge32
			UNITS "Kbps"
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"The data throughput of the ethernet down link of this radio."
			::= { wsCcStatsRadioExtEntry 1 }

		
		-- 1.3.6.1.4.1.388.14.3.3.1.7.1.1.2
		wsCcStatsRadioExtUpLinkThroughput OBJECT-TYPE
			SYNTAX Gauge32
			UNITS "Kbps"
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"The data throughput of the ethernet up link of this radio."
			::= { wsCcStatsRadioExtEntry 2 }

		
-- Mesh Statistics.
		-- 1.3.6.1.4.1.388.14.3.3.1.8
		wsCcStatsMesh OBJECT IDENTIFIER ::= { wsCcStatsMIB 8 }

		
		-- 1.3.6.1.4.1.388.14.3.3.1.8.1
		wsCcStatsMeshTable OBJECT-TYPE
			SYNTAX SEQUENCE OF WsCcStatsMeshEntry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"This table contains general statistics about
				packets sent/recieved via each mesh on the switch."
			::= { wsCcStatsMesh 1 }

		
		-- 1.3.6.1.4.1.388.14.3.3.1.8.1.1
		wsCcStatsMeshEntry OBJECT-TYPE
			SYNTAX WsCcStatsMeshEntry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				" An entry in the table."
			INDEX { wsCcStatsMeshIndex }
			::= { wsCcStatsMeshTable 1 }

		
		WsCcStatsMeshEntry ::=
			SEQUENCE { 
				wsCcStatsMeshIndex
					Unsigned32,
				wsCcStatsMeshConnType
					INTEGER,
				wsCcStatsMeshMac
					PhysAddress,
				wsCcStatsMeshWlanEssid
					DisplayString,
				wsCcStatsMeshRadioType
					RadioType,
				wsCcStatsMeshThroughput
					Unsigned32,
				wsCcStatsMeshAvgBitSpeed
					Unsigned32,
				wsCcStatsMeshRetries100
					ScaleBy100,
				wsCcStatsMeshRadioIndex
					Unsigned32
			 }

		-- 1.3.6.1.4.1.388.14.3.3.1.8.1.1.1
		wsCcStatsMeshIndex OBJECT-TYPE
			SYNTAX Unsigned32 (1..1001)
			MAX-ACCESS accessible-for-notify
			STATUS current
			DESCRIPTION
				"Index of the radio corresponding to the mesh."
			::= { wsCcStatsMeshEntry 1 }

		
		-- 1.3.6.1.4.1.388.14.3.3.1.8.1.1.2
		wsCcStatsMeshConnType OBJECT-TYPE
			SYNTAX INTEGER
				{
				none(1),
				baseBridge(2),
				clientBridge(3)
				}
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"Mesh Connection Type."
			::= { wsCcStatsMeshEntry 2 }

		
		-- 1.3.6.1.4.1.388.14.3.3.1.8.1.1.3
		wsCcStatsMeshMac OBJECT-TYPE
			SYNTAX PhysAddress
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"MAC Address."
			::= { wsCcStatsMeshEntry 3 }

		
		-- 1.3.6.1.4.1.388.14.3.3.1.8.1.1.4
		wsCcStatsMeshWlanEssid OBJECT-TYPE
			SYNTAX DisplayString
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"ESSID of associated WLAN."
			::= { wsCcStatsMeshEntry 4 }

		
		-- 1.3.6.1.4.1.388.14.3.3.1.8.1.1.5
		wsCcStatsMeshRadioType OBJECT-TYPE
			SYNTAX RadioType
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"Radio Type."
			::= { wsCcStatsMeshEntry 5 }

		
		-- 1.3.6.1.4.1.388.14.3.3.1.8.1.1.6
		wsCcStatsMeshThroughput OBJECT-TYPE
			SYNTAX Unsigned32
			UNITS "bits per second"
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"Actual number of bits sent and received.
				
				For transmitted packets, each packet sucessfully sent
				counts as 1, (not N, where N is the number of retries)."
			::= { wsCcStatsMeshEntry 6 }

		
		-- 1.3.6.1.4.1.388.14.3.3.1.8.1.1.7
		wsCcStatsMeshAvgBitSpeed OBJECT-TYPE
			SYNTAX Unsigned32
			UNITS "bits per second"
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"An octet-weighted average of the speeds of all packets
				sent/received.  (For each possible speed, multiply the
				number of octets sent/received by that speed;  divide the
				sum by the total number of octets;  multiply by 8).
				
				For transmitted packets, each packet sucessfully sent 
				counts as 1, (not N, where N is the number of retries).
				
				This can be said in more plain english as: the average
				bit-speed at which packets were sent/received."
			::= { wsCcStatsMeshEntry 7 }

		
		-- 1.3.6.1.4.1.388.14.3.3.1.8.1.1.8
		wsCcStatsMeshRetries100 OBJECT-TYPE
			SYNTAX ScaleBy100
			UNITS "average x100"
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"For all transmit packets (those that experienced some
				retries, those that were successfully transmitted with
				only 1 transmission, and those that attempted max times
				and gave-up), the average number of RE-transmission
				attempts.
				
				Said another (though identical) way: the sum of all
				tranmission attempts divided by the number of unique
				packets attempted to be sent.
				
				Since SNMP does not convey decimal values, the result
				above is multiplied by 100.
				
				If there were no RE-transmissions, this value would be 0.
				If every single packet required exactly two tranmissions,
				this value would be 100, (representing 1.00)."
			::= { wsCcStatsMeshEntry 8 }

		
		-- 1.3.6.1.4.1.388.14.3.3.1.8.1.1.9
		wsCcStatsMeshRadioIndex OBJECT-TYPE
			SYNTAX Unsigned32
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"Radio index of the assoicated mesh statistic."
			::= { wsCcStatsMeshEntry 9 }

		
-- The switch roll-up values are reported at entry/instance
-- 1001, as with the other wsCcStatsSum tables.  Note, however,
-- that the switch reports very few of the values that mesh
-- report, and will return NO-SUCH for those values on GET,
-- and will 'skip-over' those columns for GET-NEXT.
		-- 1.3.6.1.4.1.388.14.3.3.1.8.2
		wsCcStatsMeshSumShortTable OBJECT-TYPE
			SYNTAX SEQUENCE OF WsCcStatsMeshSumShortEntry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"Averages and rates calculated over the SHORT window for
				each mesh."
			::= { wsCcStatsMesh 2 }

		
		-- 1.3.6.1.4.1.388.14.3.3.1.8.2.1
		wsCcStatsMeshSumShortEntry OBJECT-TYPE
			SYNTAX WsCcStatsMeshSumShortEntry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"An entry in the table."
			INDEX { wsCcStatsMeshIndex }
			::= { wsCcStatsMeshSumShortTable 1 }

		
		WsCcStatsMeshSumShortEntry ::=
			SEQUENCE { 
				wsCcStatsMeshSumShortTimestamp
					TimeTicks,
				wsCcStatsMeshSumShortNumPkts
					Counter32,
				wsCcStatsMeshSumShortPktsPerSec100
					ScaleBy100,
				wsCcStatsMeshSumShortPktsPerSecTx100
					ScaleBy100,
				wsCcStatsMeshSumShortPktsPerSecRx100
					ScaleBy100,
				wsCcStatsMeshSumShortThroughput
					Unsigned32,
				wsCcStatsMeshSumShortThroughputTx
					Unsigned32,
				wsCcStatsMeshSumShortThroughputRx
					Unsigned32,
				wsCcStatsMeshSumShortAvgBitSpeed
					Unsigned32,
				wsCcStatsMeshSumShortAvgMuSignal
					Integer32,
				wsCcStatsMeshSumShortAvgMuNoise
					Integer32,
				wsCcStatsMeshSumShortAvgMuSnr
					Integer32,
				wsCcStatsMeshSumShortPp10kTxWithRetries
					PartsPer10k,
				wsCcStatsMeshSumShortPp10kDropped
					PartsPer10k,
				wsCcStatsMeshSumShortTxAvgRetries100
					ScaleBy100,
				wsCcStatsMeshSumShortPp10kRxUndecrypt
					PartsPer10k
			 }

		-- 1.3.6.1.4.1.388.14.3.3.1.8.2.1.1
		wsCcStatsMeshSumShortTimestamp OBJECT-TYPE
			SYNTAX TimeTicks
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"Snapshot of sysUpTime as of the END of the 'window' of 
				time that this entry of values represents."
			::= { wsCcStatsMeshSumShortEntry 1 }

		
		-- 1.3.6.1.4.1.388.14.3.3.1.8.2.1.2
		wsCcStatsMeshSumShortNumPkts OBJECT-TYPE
			SYNTAX Counter32
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"The number of packets used to calculate the statistics in
				this window."
			::= { wsCcStatsMeshSumShortEntry 2 }

		
		-- 1.3.6.1.4.1.388.14.3.3.1.8.2.1.3
		wsCcStatsMeshSumShortPktsPerSec100 OBJECT-TYPE
			SYNTAX ScaleBy100
			UNITS "pkts per sec x100"
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"Packets per second as averaged over the 'window'.
				
				For transmitted packets, each packet sucessfully sent 
				counts as 1, (not N, where N is the number of retries).
				
				Since SNMP does not convey decimal values, the result 
				is multiplied by 100."
			::= { wsCcStatsMeshSumShortEntry 3 }

		
		-- 1.3.6.1.4.1.388.14.3.3.1.8.2.1.4
		wsCcStatsMeshSumShortPktsPerSecTx100 OBJECT-TYPE
			SYNTAX ScaleBy100
			UNITS "pkts per sec x100"
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"Packets per second TX as averaged over the 'window'.
				
				Each packet sucessfully sent counts as 1, (not N, where N
				is the number of retries).
				
				Since SNMP does not convey decimal values, the result 
				is multiplied by 100."
			::= { wsCcStatsMeshSumShortEntry 4 }

		
		-- 1.3.6.1.4.1.388.14.3.3.1.8.2.1.5
		wsCcStatsMeshSumShortPktsPerSecRx100 OBJECT-TYPE
			SYNTAX ScaleBy100
			UNITS "pkts per sec x100"
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"Packets per second RX as averaged over the 'window'.
				
				Since SNMP does not convey decimal values, the result 
				is multiplied by 100."
			::= { wsCcStatsMeshSumShortEntry 5 }

		
		-- 1.3.6.1.4.1.388.14.3.3.1.8.2.1.6
		wsCcStatsMeshSumShortThroughput OBJECT-TYPE
			SYNTAX Unsigned32
			UNITS "bits per second"
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"Actual number of bits sent and received over the window,
				divided by the number of seconds in the window.
				
				For transmitted packets, each packet sucessfully sent 
				counts as 1, (not N, where N is the number of retries)."
			::= { wsCcStatsMeshSumShortEntry 6 }

		
		-- 1.3.6.1.4.1.388.14.3.3.1.8.2.1.7
		wsCcStatsMeshSumShortThroughputTx OBJECT-TYPE
			SYNTAX Unsigned32
			UNITS "bits per second"
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"Actual number of bits TX over the window,
				divided by the number of seconds in the window.
				
				Each packet sucessfully sent counts as 1, (not N, where N
				is the number of retries)."
			::= { wsCcStatsMeshSumShortEntry 7 }

		
		-- 1.3.6.1.4.1.388.14.3.3.1.8.2.1.8
		wsCcStatsMeshSumShortThroughputRx OBJECT-TYPE
			SYNTAX Unsigned32
			UNITS "bits per second"
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"Actual number of bits RX over the window,
				divided by the number of seconds in the window."
			::= { wsCcStatsMeshSumShortEntry 8 }

		
		-- 1.3.6.1.4.1.388.14.3.3.1.8.2.1.9
		wsCcStatsMeshSumShortAvgBitSpeed OBJECT-TYPE
			SYNTAX Unsigned32
			UNITS "bits per second"
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"An octet-weighted average of the speeds of all packets
				sent/received.  (For each possible speed, multiply the
				number of octets sent/received by that speed;  divide the
				sum by the total number of octets;  multiply by 8).
				
				For transmitted packets, each packet sucessfully sent 
				counts as 1, (not N, where N is the number of retries).
				
				This can be said in more plain english as: the average
				bit-speed at which packets were sent/received."
			::= { wsCcStatsMeshSumShortEntry 9 }

		
		-- 1.3.6.1.4.1.388.14.3.3.1.8.2.1.10
		wsCcStatsMeshSumShortAvgMuSignal OBJECT-TYPE
			SYNTAX Integer32
			UNITS "dBm"
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"The average of all signal values over the window.
				
				NOTE: dBm are normally negative.  Here the NEGATIVE values
				are reported as POSITIVE values."
			::= { wsCcStatsMeshSumShortEntry 10 }

		
		-- 1.3.6.1.4.1.388.14.3.3.1.8.2.1.11
		wsCcStatsMeshSumShortAvgMuNoise OBJECT-TYPE
			SYNTAX Integer32
			UNITS "dBm"
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"The average of all noise values over the window."
			::= { wsCcStatsMeshSumShortEntry 11 }

		
		-- 1.3.6.1.4.1.388.14.3.3.1.8.2.1.12
		wsCcStatsMeshSumShortAvgMuSnr OBJECT-TYPE
			SYNTAX Integer32
			UNITS "dB"
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"The average of all SNR values over the window."
			::= { wsCcStatsMeshSumShortEntry 12 }

		
		-- 1.3.6.1.4.1.388.14.3.3.1.8.2.1.13
		wsCcStatsMeshSumShortPp10kTxWithRetries OBJECT-TYPE
			SYNTAX PartsPer10k
			UNITS "parts-per-10000"
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"Ratio of TX packets that experienced one or more retries.
				
				Expressed as parts-per-10000."
			::= { wsCcStatsMeshSumShortEntry 13 }

		
		-- 1.3.6.1.4.1.388.14.3.3.1.8.2.1.14
		wsCcStatsMeshSumShortPp10kDropped OBJECT-TYPE
			SYNTAX PartsPer10k
			UNITS "parts-per-10000"
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"Packets dropped divided by packets sent.  Dropped here 
				means dropped intentionally due to the appropriate QoS 
				queue being full.  Other reasons for dropped packets are 
				not counted in this ratio."
			::= { wsCcStatsMeshSumShortEntry 14 }

		
		-- 1.3.6.1.4.1.388.14.3.3.1.8.2.1.15
		wsCcStatsMeshSumShortTxAvgRetries100 OBJECT-TYPE
			SYNTAX ScaleBy100
			UNITS "average x100"
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"For all transmit packets (those that experienced some 
				retries, those that were successfully transmitted with
				only 1 transmission, and those that attempted max times
				and gave-up), the average number of RE-transmission 
				attempts.
				
				Said another (though identical) way: the sum of all 
				tranmission attempts divided by the number of unique 
				packets attempted to be sent.
				
				Since SNMP does not convey decimal values, the result 
				above is multiplied by 100.
				
				If there were no RE-transmissions, this value would be 0.
				If every single packet required exactly two tranmissions,
				this value would be 100, (representing 1.00)."
			::= { wsCcStatsMeshSumShortEntry 15 }

		
		-- 1.3.6.1.4.1.388.14.3.3.1.8.2.1.16
		wsCcStatsMeshSumShortPp10kRxUndecrypt OBJECT-TYPE
			SYNTAX PartsPer10k
			UNITS "parts-per-10000"
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"Ratio of RX packets that were undecryptable.
				
				Expressed as parts-per-10000."
			::= { wsCcStatsMeshSumShortEntry 16 }

		
-- The switch roll-up values are reported at entry/instance
-- 1001, as with the other wsCcStatsSum tables.  Note, however,
-- that the switch reports very few of the values that mesh
-- report, and will return NO-SUCH for those values on GET,
-- and will 'skip-over' those columns for GET-NEXT.
		-- 1.3.6.1.4.1.388.14.3.3.1.8.3
		wsCcStatsMeshSumLongTable OBJECT-TYPE
			SYNTAX SEQUENCE OF WsCcStatsMeshSumLongEntry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"Averages and rates calculated over the LONG window for
				each mesh.
				
				(Please refer to wsCcStatsMeshSumShortTable for descriptions)."
			::= { wsCcStatsMesh 3 }

		
		-- 1.3.6.1.4.1.388.14.3.3.1.8.3.1
		wsCcStatsMeshSumLongEntry OBJECT-TYPE
			SYNTAX WsCcStatsMeshSumLongEntry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"An entry in the table."
			INDEX { wsCcStatsMeshIndex }
			::= { wsCcStatsMeshSumLongTable 1 }

		
		WsCcStatsMeshSumLongEntry ::=
			SEQUENCE { 
				wsCcStatsMeshSumLongTimestamp
					TimeTicks,
				wsCcStatsMeshSumLongNumPkts
					Counter32,
				wsCcStatsMeshSumLongPktsPerSec100
					ScaleBy100,
				wsCcStatsMeshSumLongPktsPerSecTx100
					ScaleBy100,
				wsCcStatsMeshSumLongPktsPerSecRx100
					ScaleBy100,
				wsCcStatsMeshSumLongThroughput
					Unsigned32,
				wsCcStatsMeshSumLongThroughputTx
					Unsigned32,
				wsCcStatsMeshSumLongThroughputRx
					Unsigned32,
				wsCcStatsMeshSumLongAvgBitSpeed
					Unsigned32,
				wsCcStatsMeshSumLongAvgMuSignal
					Integer32,
				wsCcStatsMeshSumLongAvgMuNoise
					Integer32,
				wsCcStatsMeshSumLongAvgMuSnr
					Integer32,
				wsCcStatsMeshSumLongPp10kTxWithRetries
					PartsPer10k,
				wsCcStatsMeshSumLongPp10kDropped
					PartsPer10k,
				wsCcStatsMeshSumLongTxAvgRetries100
					ScaleBy100,
				wsCcStatsMeshSumLongPp10kRxUndecrypt
					PartsPer10k
			 }

		-- 1.3.6.1.4.1.388.14.3.3.1.8.3.1.1
