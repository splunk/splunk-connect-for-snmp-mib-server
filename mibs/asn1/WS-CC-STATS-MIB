		IMPORTS
			OBJECT-GROUP, MODULE-COMPLIANCE			
				FROM SNMPv2-CONF			
			TimeTicks, Integer32, Unsigned32, Gauge32, Counter32, 
			Counter64, OBJECT-TYPE, MODULE-IDENTITY			
				FROM SNMPv2-SMI			
			DisplayString, TruthValue, PhysAddress, TEXTUAL-CONVENTION			
				FROM SNMPv2-TC			
			wsCcStats			
				FROM WS-CC-SMI			
			DoActionNow, RadioType			
				FROM WS-TYPE-MIB;

