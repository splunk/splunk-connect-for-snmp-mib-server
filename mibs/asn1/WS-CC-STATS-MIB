					Counter32,
				wsCcStatsWlanRxPktsAt12Mb
					Counter32,
				wsCcStatsWlanRxPktsAt18Mb
					Counter32,
				wsCcStatsWlanRxPktsAt22Mb
					Counter32,
				wsCcStatsWlanRxPktsAt24Mb
					Counter32,
