    ::= { s5EthernetTrapMib 8 }

s5EtrNewPortDteJabbering NOTIFICATION-TYPE
    OBJECTS { s5EnPortJabberStatus }
    STATUS  current
    DESCRIPTION
        "An ethernet DTE started jabbering.  This trap
