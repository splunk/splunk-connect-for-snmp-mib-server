   mscFrAtmDlciSiwfRowStatus
      RowStatus,
   mscFrAtmDlciSiwfComponentName
      DisplayString,
   mscFrAtmDlciSiwfStorageType
      StorageType,
   mscFrAtmDlciSiwfIndex
      NonReplicated }

