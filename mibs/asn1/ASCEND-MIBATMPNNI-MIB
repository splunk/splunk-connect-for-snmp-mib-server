	    pnniDTLConfig-DtlPortId
		INTEGER,
	    pnniDTLConfig-DtlLinkType
		INTEGER,
	    pnniDTLConfig-Active
		INTEGER,
	    pnniDTLConfig-Action-o
		INTEGER
	}

