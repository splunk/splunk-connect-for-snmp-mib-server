	::= { mibpnniRouteNodeConfigEntry 4 }

    pnniRouteNodeConfig-DestPortId	OBJECT-TYPE
	SYNTAX	INTEGER
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"The port ID of the destination node at which the route terminates.A port ID of zero indicates the node nucleus.When the destination node is represented by the simple node representation, this value should be set to zero."
	::= { mibpnniRouteNodeConfigEntry 5 }

    pnniRouteNodeConfig-Proto	OBJECT-TYPE
	SYNTAX	INTEGER {
		    other (2),
