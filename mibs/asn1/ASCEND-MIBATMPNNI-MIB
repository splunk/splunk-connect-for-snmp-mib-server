	::= { mibpnniRouteNodeConfigEntry 27 }

    pnniRouteNodeConfig-VpCapability	OBJECT-TYPE
	SYNTAX	INTEGER {
		    true (2),
		    false (3)
		}
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION	"Attribute indicates whether a VPC setup on this route is possible."
	::= { mibpnniRouteNodeConfigEntry 28 }

