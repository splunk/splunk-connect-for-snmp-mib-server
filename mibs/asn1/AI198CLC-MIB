    aiCLCFaultSwitch OBJECT-TYPE
    	SYNTAX INTEGER {
                   on(1),
                   off(2)
                }
        MAX-ACCESS read-write
        STATUS current
        DESCRIPTION
                "In dual CLC systems, a fault on the active CLC will cause
                 automatic switchover from the faulted CLC to the backup
                 CLC."
        DEFVAL { 1 }
        ::= { aiCLCSystem 25 }

    aiCLCDestMenuFmt OBJECT-TYPE
