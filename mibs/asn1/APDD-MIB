	SYNTAX		Counter32
	MAX-ACCESS 	read-only
	STATUS		current
	DESCRIPTION
	    "Total number of sockets in life time."
	::= {apDdInterfaceStatsEntry 18}

apDdGenSocketsLTPerMax	OBJECT-TYPE
	SYNTAX		Counter32
	MAX-ACCESS 	read-only
