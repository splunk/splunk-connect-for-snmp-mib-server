	SYNTAX     	     	DisplayString (SIZE (1..255))
	MAX-ACCESS		accessible-for-notify
	STATUS	   		current
	DESCRIPTION
	    "The origin hostname of the Diameter Agent where it is hosted."
	::= { apDDNotifObjects 3 }

