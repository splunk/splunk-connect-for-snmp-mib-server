apDdSessionSubscriberGeneralGroup OBJECT-GROUP
      OBJECTS {

--  Session Stats
		apDdSessionPeriodActive,
		apDdSessionPeriodHigh,
                apDdSessionPeriodTotal,
		apDdSessionLifeTotal,
