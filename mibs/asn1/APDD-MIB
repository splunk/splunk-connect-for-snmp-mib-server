-- units of conformance


----------------------------------------------------
--
-- apDdObjectGroups
--
----------------------------------------------------

apDdGeneralGroup OBJECT-GROUP
      OBJECTS {
		apDdInterfaceNumber,
		apDdCurrentTransRate,
		apDdHighTransRate,
		apDdLowTransRate
	         }
      STATUS		current
