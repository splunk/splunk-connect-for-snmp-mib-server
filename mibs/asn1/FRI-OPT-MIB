        -- obsolete
        cdx6500SPSTFRISStatsTable OBJECT IDENTIFIER
        ::= { cdx6500PSTStationProtocolGroup 10 }

        -- General FRI Statistics Table
        friSGenStatsTable OBJECT-TYPE
            SYNTAX SEQUENCE OF Cdx6500friSGenStatsEntry
            ACCESS not-accessible
            STATUS mandatory
            DESCRIPTION
                "This table contains FRI Station Statistics parameters."
        ::= { cdx6500SPSTFRISStatsTable 1 }

        cdx6500friSGenStatsEntry OBJECT-TYPE
            SYNTAX Cdx6500friSGenStatsEntry
            ACCESS not-accessible
            STATUS mandatory
            DESCRIPTION
                "Entries in the FRI Station Statistics Table."
            INDEX { friSGenStatsPortNumber, friSGenStatsStnNumber }
        ::= { friSGenStatsTable 1 }

        Cdx6500friSGenStatsEntry ::=
            SEQUENCE {
                friSGenStatsPortNumber
                    INTEGER,
                friSGenStatsStnNumber
                    INTEGER,
                friSStatsStnType
                    INTEGER,
                friSStatsStnStatus
                    DisplayString,
                friSStatsDLCI
                    INTEGER,
                friSStatsStnState
                    DisplayString,
                friSStatsConfiguredCIR
                    INTEGER,
                friSStatsAllowedInfoRate
                    INTEGER,
                friSStatsImpCongDetect
                    INTEGER,
                friSStatsExpCongDetect
                    INTEGER,
                friSStatsLMIFlowCntrl
                    INTEGER,
                friSStatsMaxSVCCount
                    INTEGER,
                friSStatsCurrSVCCount
                    INTEGER,
                friSStatsMaxPVCCount
                    INTEGER,
                friSStatsCurrPVCCount
                    INTEGER,
                friSStatsLastStatsReset
                    DisplayString,
                friSStatsFrameSegState
                    DisplayString,
                friSStatsFrameSegSyncLost
                    INTEGER,
                friSStatsPVCState
                    DisplayString,
                friSStatsVoiceCongDetect
                    INTEGER,
                friSStatsMaxVSVCCount
                    INTEGER,
                friSStatsCurrVSVCCount
                    INTEGER
                }

        friSGenStatsPortNumber OBJECT-TYPE
            SYNTAX INTEGER (1..255)
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
                "The FRI port number corresponding to this station."
        ::= { cdx6500friSGenStatsEntry 1 }

