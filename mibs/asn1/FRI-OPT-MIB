        ::= { cdx6500friSGenStatsEntry 4 }

        friSStatsDLCI OBJECT-TYPE
            SYNTAX INTEGER
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
                "Data Link Connection Identifier is the unique 
                 identifier for the station on the FRI port and 
                 must match the DLCI configured on the Frame Relay 
                 networking node.
                 The DLCI is 0 in Auto-Learning mode."
        ::= { cdx6500friSGenStatsEntry 5 }

        friSStatsStnState OBJECT-TYPE
            SYNTAX DisplayString
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
                "Specifies the state of the station :
                 unused       - station is unconfigured/disabled
                 uncontrolled - station is not controlled.
                 controlled   - station is controlled, congestion detected."
        ::= { cdx6500friSGenStatsEntry 6 }

        friSStatsConfiguredCIR OBJECT-TYPE
            SYNTAX INTEGER
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
