        -- Version 2.15.1.2, Modified: 2001-10-20 11:28:21-04 


        FRI-OPT-MIB DEFINITIONS ::= BEGIN

        IMPORTS
            Counter, enterprises
                FROM RFC1155-SMI

            OBJECT-TYPE
                FROM RFC-1212;

                         codex OBJECT IDENTIFIER ::= { enterprises 449 }
            cdxProductSpecific OBJECT IDENTIFIER ::= { codex 2 }
                       cdx6500 OBJECT IDENTIFIER ::= { cdxProductSpecific 1 }

         cdx6500Configuration  OBJECT IDENTIFIER ::= { cdx6500 2 }
       cdx6500CfgProtocolGroup OBJECT IDENTIFIER ::= { cdx6500Configuration 1 }
   cdx6500PCTPortProtocolGroup OBJECT IDENTIFIER ::= { cdx6500CfgProtocolGroup 1 }
cdx6500PCTStationProtocolGroup OBJECT IDENTIFIER ::= { cdx6500CfgProtocolGroup 3 }

             cdx6500Statistics OBJECT IDENTIFIER ::= { cdx6500 3 }
      cdx6500StatProtocolGroup OBJECT IDENTIFIER ::= { cdx6500Statistics 1 }
   cdx6500PSTPortProtocolGroup OBJECT IDENTIFIER ::= { cdx6500StatProtocolGroup 1 }
cdx6500PSTStationProtocolGroup OBJECT IDENTIFIER ::= { cdx6500StatProtocolGroup 3 }

               cdx6500Controls OBJECT IDENTIFIER ::= { cdx6500 4 }

        DisplayString ::=
            OCTET STRING
        -- This data type is used to model textual information taken
        -- from the NVT ASCII character set.  By convention, objects
        -- with this syntax are declared as having SIZE (0..255)


        -- The FRI Port Configuration Table
        -- This Table replaces cdx6500PCTFRDTEPortTable which is now obsolete

        cdx6500PPCTFRIPortTable OBJECT-TYPE
            SYNTAX SEQUENCE OF Cdx6500PPCTFRIPortEntry
            ACCESS not-accessible
            STATUS mandatory
            DESCRIPTION
                "This table contains Frame Relay DTE Port configuration 
                 parameters."
        ::= { cdx6500PCTPortProtocolGroup 31 }

        cdx6500PPCTFRIPortEntry OBJECT-TYPE
            SYNTAX Cdx6500PPCTFRIPortEntry
            ACCESS not-accessible
            STATUS mandatory
            DESCRIPTION
                "Entries in the FRI Port Configuration Table."
            INDEX { friPCfgPortNumber }
        ::= { cdx6500PPCTFRIPortTable 1 }

        -- NOTE : Numeric objects which are skipped during configuration
        --        will return 0 on query.
        Cdx6500PPCTFRIPortEntry ::=
            SEQUENCE {
                friPCfgPortNumber
                    INTEGER,
                friPCfgPortType
                    INTEGER,
                friPCfgConnectionType
                    INTEGER,
                friPCfgClockSource
                    INTEGER,
                friPCfgClockSpeed
                    INTEGER,
                friPCfgHighestStnNumber
                    INTEGER,
                friPCfgFrameSeqCounting
                    INTEGER,
                friPCfgPktSeqCounting
                    INTEGER,
                friPCfgCntrlProtocolSupport
                    INTEGER,
                friPCfgHighPriorityStn
                    INTEGER,
                friPCfgMaxVoiceBWBitsPerSec
                    INTEGER,
                friPCfgSegSizeVoicePresent
                    INTEGER,
                friPCfgSegSizeVoiceNotPresent
                    INTEGER,
                friPCfgT391Timer
                    INTEGER,
                friPCfgT392Timer
                    INTEGER,
                friPCfgN391PollCycle
                    INTEGER,
                friPCfgN392ErrsDuringMonitoredEvents
                    INTEGER,
                friPCfgN393MonitoredEvents
                    INTEGER,
                friPCfgNT1PollTimer
                    INTEGER,
                friPCfgNT2VerificationTimer
                    INTEGER,
                friPCfgNN1StatusPollingCycle
                    INTEGER,
                friPCfgNN2ErrsDuringMonitoredEvents
                    INTEGER,
                friPCfgNN3MonitoredEvents
                    INTEGER,
                friPCfgInvertTXClock
                    INTEGER,
                friPCfgControlProtocolOptions
                    DisplayString,
                friPCfgDiscardControlOptions
                    INTEGER,
		friPCfgStartSvcDlci
                    INTEGER,
		friPCfgSubscriberNumber
                    OCTET STRING,
                friPCfgMaxFmif
                    INTEGER,
                friPCfgT200
                    INTEGER,
                friPCfgN200
		    INTEGER,
                friPCfgWindowK
		    INTEGER,
                friPCfgT203
		    INTEGER,
                friPCfgT303
		    INTEGER,
                friPCfgT305
		    INTEGER,
                friPCfgT308
		    INTEGER,
                friPCfgT310
		    INTEGER,
                friPCfgControlProtocol
		    INTEGER,
                friPCfguniState 
                    INTEGER, 
                friPCfguniSSVP 
                    INTEGER, 
                friPCfguniSSVNP 
                    INTEGER, 
                friPCfguniSegDelay 
                    INTEGER, 
                friPCfguniCheckPckSize 
                    INTEGER,
		friPCfgElectricalInterfaceType
    		    INTEGER,
		friPCfgV24ElectricalInterfaceOption
    		    INTEGER,
		friPCfgHighSpeedElectricalInterfaceOption
    		    INTEGER
		}

        friPCfgPortNumber OBJECT-TYPE
            SYNTAX  INTEGER (1..255)
            ACCESS  read-only
            STATUS  mandatory
            DESCRIPTION
                "Port number of this port."
        ::= { cdx6500PPCTFRIPortEntry 1 }

        friPCfgPortType OBJECT-TYPE
            SYNTAX INTEGER {
                   fri(18)
                   }
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
                "Port type of the Port."
--            DEFVAL { fri }
        ::= { cdx6500PPCTFRIPortEntry 2 }

        friPCfgConnectionType OBJECT-TYPE
            SYNTAX INTEGER {
                   simp  (1),
                   dtr   (2),
                   simpb (21),
                   nc    (100)
                   }
            ACCESS read-write
            STATUS mandatory
            DESCRIPTION
                "The Specified control signal handshake and clocking required 
                 for a connection to be made to this port :
                 simp  - simple, no control signals required.
                 dtr   - dedicated, require the data terminal ready signal (DTR).
                 simpb - simple, no control signals required for data 
                         transmission and reception. By default output 
                         leads will be low.
                 nc    - Skipped during configuration."
--            DEFVAL { simp }
        ::= { cdx6500PPCTFRIPortEntry 3 }

        friPCfgClockSource OBJECT-TYPE
            SYNTAX INTEGER {
                   int    (1),
                   ext    (2),
                   extint (3),
                   extlp  (4),
                   nc     (100)
                   }
            ACCESS read-write
            STATUS mandatory
            DESCRIPTION
                "int     - Internal clock source
                 ext     - External clock source.
                 extint  - Internal receive and external transmit 
                           clock source (DCE only).
                 extlp   - external receive and loopback transmit 
                           clock source (DTE only).
                           EXTLP must be configured in conjunction
                           with EXTINT.
                 nc      - Skipped during configuration."
--            DEFVAL { ext }
        ::= { cdx6500PPCTFRIPortEntry 4 }

        friPCfgClockSpeed OBJECT-TYPE
            SYNTAX INTEGER (1200..2048000)
            ACCESS read-write
            STATUS mandatory
            DESCRIPTION
                "This is the speed of the port in bits per second, when using
                 internal clocking. The highest available speed is dependent
                 on the card type, and on the interface type."
--            DEFVAL { 64000 }
        ::= { cdx6500PPCTFRIPortEntry 5 }

        friPCfgHighestStnNumber OBJECT-TYPE
            SYNTAX INTEGER (1..254)
            ACCESS read-only
            STATUS deprecated
            DESCRIPTION
                "This is the highest station number that can be  configured on
                 the FRI port. It is used to limit the configuration of 
                 stations."
        ::= { cdx6500PPCTFRIPortEntry 6 }

        friPCfgFrameSeqCounting OBJECT-TYPE
            SYNTAX INTEGER {
                   normal   (1),
                   extended (2),
                   nc       (100)
                   }
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
                "Port uses normal or extended frame sequence numbering:
                 normal   - normal sequencing   (modulo 8)
                 extended - extended sequencing (modulo 128).
                 nc       - Skipped during configuration.
                 All stations on this port will be set to this value."
--            DEFVAL { normal }
        ::= { cdx6500PPCTFRIPortEntry 7 }

        friPCfgPktSeqCounting OBJECT-TYPE
            SYNTAX INTEGER {
                   normal   (1),
                   extended (2),
                   nc       (100)
                   }
            ACCESS read-write
            STATUS mandatory
            DESCRIPTION
                "Port uses normal or extended packet level sequence numbers
                 normal   - normal sequencing   (modulo 8)
                 extended - extended sequencing (modulo 128)
                 nc       - Skipped during configuration.
                 This parameter must be set to the same value
                 at both this FRI port and the remote FRI port.
                 All stations on this port will be set to this value."
--            DEFVAL { normal }
        ::= { cdx6500PPCTFRIPortEntry 8 }

        friPCfgCntrlProtocolSupport OBJECT-TYPE
            SYNTAX INTEGER {
                   annexD (1),
                   none   (2),
                   lmi    (3),
                   annexA (4),
                   auto   (5),
                   nc     (100)
                   }
            ACCESS read-write
            STATUS mandatory
            DESCRIPTION
                "Whether ANSI T1.617 Annex D, CCITT Q.933 Annex A or 
                 LMI Support is enabled.
                 annexD - Annex D is enabled.
                 none   - None is enabled.
                 lmi    - LMI is enabled.
                 annexA - Annex A is enabled.
                 auto   - Auto-learn the Network control protocol type
                 nc     - Skipped during configuration."
--            DEFVAL { auto }
        ::= { cdx6500PPCTFRIPortEntry 9 }

        friPCfgHighPriorityStn OBJECT-TYPE
            SYNTAX INTEGER (1..254)
            ACCESS read-write
            STATUS mandatory
            DESCRIPTION
                "This parameter specifies a station who's PVC status has 
                 priority over all other stations when FRI same port back 
                 up is enabled. When the network reports this DLCI inactive, 
                 an alternate connection over a backup link is attempted."
        ::= { cdx6500PPCTFRIPortEntry 10 }

        friPCfgMaxVoiceBWBitsPerSec OBJECT-TYPE
            SYNTAX INTEGER (0..2048000)
            ACCESS read-write
            STATUS mandatory
            DESCRIPTION
                "This parameter limits the bandwidth, in bits per sec 
                 (including overhead), for voice traffic passing through this 
                 frame relay port. A zero value disables any voice passing 
                 through this port."
--            DEFVAL { 2048000 }
        ::= { cdx6500PPCTFRIPortEntry 11 }

        friPCfgSegSizeVoicePresent OBJECT-TYPE
            SYNTAX INTEGER {
                   segSize32   (33),
                   segSize64   (65),
                   segSize128  (129),
                   segSize256  (257),
                   segSize512  (513),
                   segSize1024 (1025),
                   nc          (100)
                   }
            ACCESS read-write
            STATUS mandatory
            DESCRIPTION
                "Data packets to be segmented into this maximum frame size when
                 voice is active on this port.
                 segSize32   - 32   byte segments
                 segSize64   - 64   byte segments
                 segSize128  - 128  byte segments
                 segSize256  - 256  byte segments
                 segSize512  - 512  byte segments
                 segSize1024 - 1024 byte segments
                 nc          - Skipped during configuration."
--            DEFVAL { segSize64 }
        ::= { cdx6500PPCTFRIPortEntry 12 }

        friPCfgSegSizeVoiceNotPresent  OBJECT-TYPE
            SYNTAX INTEGER {
                   segSize32   (33),
                   segSize64   (65),
                   segSize128  (129),
                   segSize256  (257),
                   segSize512  (513),
                   segSize1024 (1025),
                   segSize2048 (2049),
                   segSize4096 (4097),
                   disable     (32000),
                   nc          (100)
                   }
            ACCESS read-write
            STATUS mandatory
            DESCRIPTION
                "Data packets to be segmented into this maximum frame size when
                 voice is not active on this port.
                 segSize32   - 32   byte segments
                 segSize64   - 64   byte segments
                 segSize128  - 128  byte segments
                 segSize256  - 256  byte segments
                 segSize512  - 512  byte segments
                 segSize1024 - 1024 byte segments
                 segSize2048 - 2048 byte segments
                 segSize4096 - 4096 byte segments
                 disable     - segment size is 32768 bytes
                 nc          - Skipped during configuration."
--            DEFVAL { disable }
        ::= { cdx6500PPCTFRIPortEntry 13 }

        friPCfgT391Timer OBJECT-TYPE
            SYNTAX INTEGER (5..30)
            ACCESS read-write
            STATUS mandatory
            DESCRIPTION
                "This parameter is the Link Integrity Verification
                 Timer polling period. The FRI port sends status enquiry
                 messages to network every T391/nT1 seconds."
--            DEFVAL { 10 }
        ::= { cdx6500PPCTFRIPortEntry 14 }

        friPCfgT392Timer OBJECT-TYPE
            SYNTAX INTEGER (5..30)
            ACCESS read-write
            STATUS mandatory
            DESCRIPTION
                "This parameter is the polling Cycle Verification
                 Timer period. The FRI port expects status enquiry messages
                 every T392/nT2 seconds. This parameter must be set to a
                 value greater than N391/nT1, and applies only in
                 bi-directional mode."
--            DEFVAL { 15 }
        ::= { cdx6500PPCTFRIPortEntry 15 }

        friPCfgN391PollCycle OBJECT-TYPE
            SYNTAX INTEGER
            ACCESS read-write
            STATUS mandatory
            DESCRIPTION
                "Full Status polling cycle.  FRI port sends Full status
                 enquiry messages to network every nN1 polls."
--            DEFVAL { 5 }
        ::= { cdx6500PPCTFRIPortEntry 16 }

        friPCfgN392ErrsDuringMonitoredEvents OBJECT-TYPE
            SYNTAX INTEGER
            ACCESS read-write
            STATUS mandatory
            DESCRIPTION
                "Error inactivity threshold.  Number of errors during NN3
                 events that cause the station to be declared inactive.
                 Should be less than or equal to nN3."
--            DEFVAL { 5 }
        ::= { cdx6500PPCTFRIPortEntry 17 }

        friPCfgN393MonitoredEvents OBJECT-TYPE
            SYNTAX INTEGER
            ACCESS read-write
            STATUS mandatory
            DESCRIPTION
                "Monitored events count for measuring nN3. nN2 errors during
                 nN3 events will cause the station to be declared inactive.
                 Should be greater than nN3."
--            DEFVAL { 4 }
        ::= { cdx6500PPCTFRIPortEntry 18 }

        friPCfgNT1PollTimer OBJECT-TYPE
            SYNTAX INTEGER (5..30)
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
                "This parameter is the Link Integrity Verification
                 Timer polling period. The FRI port sends status enquiry
                 messages to network every T391/nT1 seconds."
--            DEFVAL { 10 }
        ::= { cdx6500PPCTFRIPortEntry 19 }

        friPCfgNT2VerificationTimer OBJECT-TYPE
            SYNTAX INTEGER (5..30)
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
                "This parameter is the polling Cycle Verification
                 Timer period. The FRI port expects status enquiry messages
                 every T392/nT2 seconds. This parameter must be set to a
                 value greater than N391/nT1, and applies only in
                 bi-directional mode."
--            DEFVAL { 15 }
        ::= { cdx6500PPCTFRIPortEntry 20 }

        friPCfgNN1StatusPollingCycle OBJECT-TYPE
            SYNTAX INTEGER
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
                "Full Status polling cycle.  FRI port sends Full status
                 enquiry messages to network every nN1 polls."
--            DEFVAL { 5 }
        ::= { cdx6500PPCTFRIPortEntry 21 }

        friPCfgNN2ErrsDuringMonitoredEvents OBJECT-TYPE
            SYNTAX INTEGER
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
                "Error inactivity threshold.  Number of errors during NN3
                 events that cause the station to be declared inactive.
                 Should be less than or equal to nN3."
--            DEFVAL { 3 } 
        ::= { cdx6500PPCTFRIPortEntry 22 }

        friPCfgNN3MonitoredEvents OBJECT-TYPE
            SYNTAX INTEGER
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
                "Monitored events count for measuring nN3. nN2 errors during
                 nN3 events will cause the station to be declared inactive.
                 Should be greater than nN3."
--            DEFVAL { 4 }
        ::= { cdx6500PPCTFRIPortEntry 23 }

        friPCfgInvertTXClock OBJECT-TYPE
            SYNTAX INTEGER {
                   no  (1),
                   yes (2),
                   nc (100)
                   }
            ACCESS read-write
            STATUS mandatory
            DESCRIPTION
                "This parameter specifies whether the phase of the transmit
                 clock should be inverted. Note: This parameter is primarily
                 for X.21 electrical interfaces.
                   no - Don't invert.
                   yes - Invert."
--            DEFVAL { no }
        ::= { cdx6500PPCTFRIPortEntry 24 }

        friPCfgControlProtocolOptions OBJECT-TYPE
            SYNTAX DisplayString (SIZE(0..24))
            ACCESS read-write
            STATUS mandatory
            DESCRIPTION
                "These options control the PVC management protocol
                   none  - No option selected
                   async - The port will send/receive asynchronous
                           A-bit if it is performing Network side
                           protocol functionality
                   nbit  - The port will send and accept N-bit messages
                   dbit  - The port will send and accept D-bit messages
                           This forces ASYNC to be specified (dbit+async)
                   dte-only - This forces a DTE Interface Type to remain as DTE,
	                      suppressing changes to BI-DIR using Annex A or D"

        ::= { cdx6500PPCTFRIPortEntry 25 }

        friPCfgDiscardControlOptions OBJECT-TYPE
            SYNTAX INTEGER {
                   none  (1),
                   debit (2)
                   }
            ACCESS read-write
            STATUS mandatory
            DESCRIPTION
                "This parameter controls the discard frame options
                   none - No option selected
                   debit - DE marked frames are discarded before
                           non-DE frames"
--            DEFVAL { none }
        ::= { cdx6500PPCTFRIPortEntry 26 }

	friPCfgStartSvcDlci OBJECT-TYPE
            SYNTAX INTEGER
            ACCESS read-write
            STATUS mandatory
            DESCRIPTION
                "The configured number is the lowest value DLCI the FRI port will
                 allow for use of FR SVCs. A value of 0 means SVC procedures are not
                 operating on this port and the signalling channel will not be active."
        ::= { cdx6500PPCTFRIPortEntry 27 }

        friPCfgSubscriberNumber        OBJECT-TYPE
            SYNTAX OCTET STRING (SIZE(3..19))
            ACCESS read-write
            STATUS mandatory
            DESCRIPTION
	        "All stations on this port use this value for their calling
                 address information element. The valid format of this entry is:
                    xx...xx000101 or xx...xx000103
                 Where:
                    xx...xx00 represents the address of the station in IA5/ASCII
                              coded hex terminated with a NUL character suffix.
                              For example, an address of 123 would be represented
                              by the string 31323300.
                    0101      is the suffix used to specify E.164 Number Plan
                    0103      is the suffix used to specify X.121 Number Plan
                 To send a call without the Subscriber Number IE, set the
                 address string to NUL only."
        ::= { cdx6500PPCTFRIPortEntry 28 }

        friPCfgMaxFmif        OBJECT-TYPE
            SYNTAX INTEGER (262..4096)
            ACCESS read-write
            STATUS mandatory
            DESCRIPTION
	        "This is the value used for link layer core parameters information
                 element negotiation of maximum frame mode information field (FRIF)
                 size in bytes. The value applies to both inbound and outbound frames.
                 The value is specified in outgoing SETUP messages and is the maximum
                 accepted value on incoming SETUP messages."
        ::= { cdx6500PPCTFRIPortEntry 29 }

        friPCfgT200        OBJECT-TYPE
            SYNTAX INTEGER (1..255)
            ACCESS read-write
            STATUS mandatory
            DESCRIPTION
	        "This is the value in tenths of seconds (1/10 s) of the maximum time
	         the DTE will wait for an acknowledgment of a transmitted Information
	         frame. The DTE will continue retransmissions at an interval of T200
	         until the tries count expires or proper acknowledgment is received
                 from the attached equipment."
        ::= { cdx6500PPCTFRIPortEntry 30 }

        friPCfgN200        OBJECT-TYPE
            SYNTAX INTEGER (0..16)
            ACCESS read-write
            STATUS mandatory
            DESCRIPTION
	        "This is the maximum number of times the DTE will retransmit in order
                 to recover from a T200 timeout condition. If this count expires, the
                 DTE will declare the LAPF link to be down and initiate link
                 establishment procedures."
        ::= { cdx6500PPCTFRIPortEntry 31 }

        friPCfgWindowK        OBJECT-TYPE
            SYNTAX INTEGER (1..32)
            ACCESS read-write
            STATUS mandatory
            DESCRIPTION
	        "This is the maximum sending window size which is the maximum number
                 (k) of sequentially numbered I frames that may be outstanding
                 (unacknowledged) at any given time. The recommended values are:
                 Line speed (kbps)     Window, k
                 -----------------     --------------
                 less than 16            3
                 greater than 16         7"
        ::= { cdx6500PPCTFRIPortEntry 32 }

        friPCfgT203        OBJECT-TYPE
            SYNTAX INTEGER (0..255)
            ACCESS read-write
            STATUS mandatory
            DESCRIPTION
	        "The idle timer (T203) represents the maximum time in seconds
                 allowed without frames being exchanged. If the link remains
                 idle for this period of time, the DTE will send a supervisory
                 frame with the poll bit set.

                 A value of zero disables this timer."
        ::= { cdx6500PPCTFRIPortEntry 33 }

        friPCfgT303        OBJECT-TYPE
            SYNTAX INTEGER (1..60)
            ACCESS read-write
            STATUS mandatory
            DESCRIPTION
	        "The Setup Timer (T303) represents the maximum time in seconds
                 allowed without a response to a SETUP message being received.
                 Upon expiry, the SETUP message is retransmitted and the timer
                 is restarted. The second expiry of the timer will cause the
                 call to be cleared."
        ::= { cdx6500PPCTFRIPortEntry 34 }

        friPCfgT305        OBJECT-TYPE
            SYNTAX INTEGER (1..255)
            ACCESS read-write
            STATUS mandatory
            DESCRIPTION
	        "The Disconnect Timer (T305) represents the maximum time
                 in seconds allowed without a response to a DISCONNECT message
                 being received. Upon expiry, a RELEASE message is sent."
        ::= { cdx6500PPCTFRIPortEntry 35 }

        friPCfgT308        OBJECT-TYPE
            SYNTAX INTEGER (1..60)
            ACCESS read-write
            STATUS mandatory
            DESCRIPTION
	        "The Release Timer (T308) represents the maximum time in
                 seconds allowed without a response to a RELEASE message being
                 received. Upon expiry, the RELEASE message is retransmitted
                 and the timer is restarted. The second expiry of the timer
                 will cause the call to be cleared."
        ::= { cdx6500PPCTFRIPortEntry 36 }

        friPCfgT310        OBJECT-TYPE
            SYNTAX INTEGER (1..255)
            ACCESS read-write
            STATUS mandatory
            DESCRIPTION
	        "The Call Proceeding Timer (T310) value is in seconds. This timer
                 is started when a CALL PROCEEDING response to a SETUP message is
                 received. The timer is stopped when a CONNECT or DISCONNECT or
                 RELEASE message is received. If the timer expires, the call will
                 be cleared."
        ::= { cdx6500PPCTFRIPortEntry 37 }

        friPCfgControlProtocol        OBJECT-TYPE
            SYNTAX INTEGER {
                   dte (1),
                   dce (2)
                   }
            ACCESS read-write
            STATUS mandatory
            DESCRIPTION
            "This option controls whether the PVC management protocol acts
    		 like a DTE or a DCE
    		 DTE  - This ports takes on the the DTE or User Side role
    		 DCE  - This ports takes on the the DCE or Network Side role"

        ::= { cdx6500PPCTFRIPortEntry 38 }
        
        friPCfguniState		        OBJECT-TYPE
            SYNTAX INTEGER {
                   disable (1),
                   enable  (2)
                   }
            ACCESS read-write
            STATUS mandatory
            DESCRIPTION
	        "Enable FRF.12 UNI Segmentation.  Peer must be enabled too. 
 	         enable  - Enable UNI Segmentation
 	         disable - Disable UNI Segmentation"
--            DEFVAL { disable }
        ::= { cdx6500PPCTFRIPortEntry 39 }
        
        friPCfguniSSVP 				OBJECT-TYPE
            SYNTAX INTEGER {
                   segSize64   (65),
                   segSize128  (129),
                   segSize256  (257),
                   segSize512  (513),
                   segSize1024 (1025)
                   }
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
                "UNI segment size when voice is present. 
                 Affects transmit path only.
                 segSize64   - 64   byte segments
                 segSize128  - 128  byte segments
                 segSize256  - 256  byte segments
                 segSize512  - 512  byte segments
                 segSize1024 - 1024 byte segments"
--            DEFVAL { 64 }
        ::= { cdx6500PPCTFRIPortEntry 40 }

        friPCfguniSSVNP OBJECT-TYPE
            SYNTAX INTEGER {
                   segSize64   (65),
                   segSize128  (129),
                   segSize256  (257),
                   segSize512  (513),
                   segSize1024 (1025), 
                   segSize2048 (2049), 
                   segSize4096 (4097), 
                   disable	   (32000)
                   }
            ACCESS read-write
            STATUS mandatory
            DESCRIPTION
                "UNI segment size when voice is not present. 
		         Affects transmit path only.
                 segSize64   - 64   byte segments
                 segSize128  - 128  byte segments
                 segSize256  - 256  byte segments
                 segSize512  - 512  byte segments
                 segSize1024 - 1024 byte segments 
		         segSize2048 - 2048 byte segments 
		         segSize4096 - 4096 byte segments 
		         disable - this selection doesn't affect the 
		         packet size.  UNI segmentation header 
		         will be inserted only."
--            DEFVAL { disable }
        ::= { cdx6500PPCTFRIPortEntry 41 }
 
        friPCfguniSegDelay		        OBJECT-TYPE
            SYNTAX INTEGER {
                   disable (1),
                   enable  (2)
                   }
            ACCESS read-write
            STATUS mandatory
            DESCRIPTION
                "Discards a packet if any segment delays more 
                 than 10 seconds. 
                 Affects receive path only. 
                 enabled  - discard the packet
                 disabled - do not discard the packet"
--            DEFVAL { disable }
        ::= { cdx6500PPCTFRIPortEntry 42 }

        friPCfguniCheckPckSize		        OBJECT-TYPE
            SYNTAX INTEGER {
                   disable (1),
                   enable  (2)
                   }
            ACCESS read-write
            STATUS mandatory
            DESCRIPTION
                "Check if the packet size is larger than the node
                 maximum packet size.
                   enabled  - discard packet than is larger than
                              the maximum size.
                   disabled - accept packet than is larger than
                              the maximum size."
--            DEFVAL { disable }
        ::= { cdx6500PPCTFRIPortEntry 43 }
        friPCfgElectricalInterfaceType		        OBJECT-TYPE
            SYNTAX INTEGER {
                    v24       (1),
                    v35       (2),
                    v36       (3),
                    x21       (4),
                    none      (5)
                    }
            ACCESS read-write
            STATUS mandatory
            DESCRIPTION
                 "Specify the Electrical Interface Type:
                   V.24  - V.24 Electrical Interface Type
                   V.35  - V.35 Electrical Interface Type
                   V.36  - V.36 Electrical Interface Type
                   X.21  - X.21 Electrical Interface Type
                   NONE  - Electrically disabled"
        ::= { cdx6500PPCTFRIPortEntry 44 }

        friPCfgV24ElectricalInterfaceOption		        OBJECT-TYPE
            SYNTAX INTEGER {
                    ri        (1),
                    tm        (2)
                    }
            ACCESS read-write
            STATUS mandatory
            DESCRIPTION
                 "Specify the Pin 22 option:
                  RI    - V.24 uses Pin 22 for Ring Indicator output signal
                  TM    - V.24 uses Pin 22 for Test Mode input signal"
        ::= { cdx6500PPCTFRIPortEntry 45 }

        friPCfgHighSpeedElectricalInterfaceOption		        OBJECT-TYPE
            SYNTAX INTEGER {
                    none      (1),
                    xover     (2)
                    }
            ACCESS read-write
            STATUS mandatory
            DESCRIPTION
                 "Specify the cable type:
                  NONE  - V.35/V.36/X.21 DCE with straight through cable
                  XOVER - V.35/V.36/X.21 DCE with crossover adapter cable"
        ::= { cdx6500PPCTFRIPortEntry 46 }

        -- The FRI Station Configuration Table
        -- This Table replaces cdx6500SPCTFRDTEStationTable which is now 
        -- obsolete

        cdx6500SPCTFRIStationTable OBJECT-TYPE
            SYNTAX SEQUENCE OF Cdx6500SPCTFRIStationEntry
            ACCESS  not-accessible
            STATUS  mandatory
            DESCRIPTION
                "This table contains Frame Relay DTE Station configuration
                 parameters."
        ::= { cdx6500PCTStationProtocolGroup 9 }

        cdx6500SPCTFRIStationEntry OBJECT-TYPE
            SYNTAX  Cdx6500SPCTFRIStationEntry
            ACCESS  not-accessible
            STATUS  mandatory
            DESCRIPTION
                "Entries in the FRI Station Configuration Table."
            INDEX  { friSCfgPortNumber, friSCfgStnNumber }
        ::= { cdx6500SPCTFRIStationTable 1 }

        -- NOTE : Numeric objects which are skipped during configuration
        --        will return 0 on query.
        Cdx6500SPCTFRIStationEntry ::=
            SEQUENCE {
                friSCfgPortNumber
                    INTEGER,
                friSCfgStnNumber
                    INTEGER,
                friSCfgStnType
                    INTEGER,
                friSCfgDLCI
                    INTEGER,
                friSCfgCommInfoRate
                    INTEGER,
                friSCfgCommBurstSize
                    INTEGER,
                friSCfgTransDelay
                    INTEGER,
                friSCfgCongControlMode
                    INTEGER,
                friSCfgLinkAddress
                    INTEGER,
                friSCfgPVCChannels
                    INTEGER,
                friSCfgStartingPVC
                    INTEGER,
                friSCfgSVCChannels
                    INTEGER,
                friSCfgStartingSVC
                    INTEGER,
                friSCfgVoiceSVCChannels
                    INTEGER,
                friSCfgInitialFrame
                    INTEGER,
                friSCfgT1RetryTimer
                    INTEGER,
                friSCfgT4PollTimer
                    INTEGER,
                friSCfgN2TransmissionTries
                    INTEGER,
                friSCfgKFrameWindow
                    INTEGER,
                friSCfgWPacketWindow
                    INTEGER,
                friSCfgPPacketSize
                    INTEGER,
                friSCfgDataQUpperThreshold
                    INTEGER,
                friSCfgDataQLowerThreshold
                    INTEGER,
                friSCfgRestartTimer
                    INTEGER,
                friSCfgResetTimer
                    INTEGER,
                friSCfgCallTimer
                    INTEGER,
                friSCfgClearTimer
                    INTEGER,
                friSCfgX25Options
                    DisplayString,
                friSCfgRestrictedConnDest
                    DisplayString,
                friSCfgCUGMembership
                    DisplayString,
                friSCfgBillingRecords
                    INTEGER,
                friSCfgFrameSegmenter
                    INTEGER,
                friSCfgVoiceCongCtrlMode
                    INTEGER,
                friSCfgPeakUtilization
                    INTEGER,
                friSCfgMaxInboundQueue
                    INTEGER,
                friSCfgAnnexGRateReduction
                    INTEGER,
		friSCfgCctType
		    INTEGER,
		friSCfgCallControl
		    INTEGER,
		friSCfgRetryInterval
		    INTEGER,
		friSCfgCallAttempts
		    INTEGER,
		friSCfgIdleInterval
		    INTEGER,
		friSCfgIeNegotiation
		    DisplayString,
		friSCfgSubaddress
		    OCTET STRING,
		friSCfgCalledNumber
		    OCTET STRING,             
		friSCfgCalledSubaddress
		    OCTET STRING,
		friSCfgMinCir
		    INTEGER,
		friSCfgBurstExcess
		    INTEGER,
                friSCfgE2EEnabled
                    INTEGER,
                friSCfgE2EType
                    INTEGER,
                friSCfgE2ESSVP
                    INTEGER,
                friSCfgE2ESSVNP
                    INTEGER,
                friSCfgE2ESegDelayEnabled
                    INTEGER,
                friSCfgE2ECheckPckSize
                    INTEGER,
                 friSCfgvoice-header
                    INTEGER
                }

        friSCfgPortNumber OBJECT-TYPE
            SYNTAX INTEGER (1..255)
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
                "The FRI port number corresponding to this station."
        ::= { cdx6500SPCTFRIStationEntry 1 }

        friSCfgStnNumber OBJECT-TYPE
            SYNTAX INTEGER (1..254)
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
                "The number of the configured FRI Station."
        ::= { cdx6500SPCTFRIStationEntry 2 }

        friSCfgStnType OBJECT-TYPE
            SYNTAX INTEGER {
                   annexG      (1),
                   bypass      (2),
                   voiceRelay  (3)
                   }
            ACCESS read-write
            STATUS mandatory
            DESCRIPTION
                "Identifies the type of station configured.
                 annexG      - This station uses X.25 signaling for layer 3.
                 bypass      - This station type bypasses layer 3. There is no
                               signaling for SVC's on this station.
                 voiceRelay  - Not used in the current version."
        ::= { cdx6500SPCTFRIStationEntry 3 }

        friSCfgDLCI OBJECT-TYPE
            SYNTAX INTEGER
            ACCESS read-write
            STATUS mandatory
            DESCRIPTION
                "The Data Link Connection Identifier (DLCI) is the unique 
                 identifier for the station on the FRI Port. Its valid range
                 is 0, 16 - 1007.
                 A zero value in this field places the FRI station in DLCI
                 auto-learn mode. In auto learn mode, the station is eligible
                 for automatic assignment of an available network DLCI.
                 A configured value of 16-1007 must match a DLCI on the FR
                 networking node.
                 NOTE: When a station record is configured for the first time,
                 the DLCI prompt displays an auto-incrementing DLCI value.
                 The DLCI's start from 16 for station 1, and increment by one 
                 for each station configured. If the station record is not 
                 saved to memory, a default DLCI value of zero is used."
        ::= { cdx6500SPCTFRIStationEntry 4 }

        friSCfgCommInfoRate OBJECT-TYPE
            SYNTAX INTEGER (0..2048000)
            ACCESS read-write
            STATUS mandatory
            DESCRIPTION
                "This parameter (CIR) specifies the rate (in bits-per-second)
                 that the Frame Relay network agrees to transfer information
                 under congested conditions for the FRI Station.  
                 This value is used to throttle data on the FRI station for
                 congestion control purposes. The CIR cannot be greater than
                 the link speed."
--            DEFVAL { 16000 }
        ::= { cdx6500SPCTFRIStationEntry 5 }

        friSCfgCommBurstSize OBJECT-TYPE
            SYNTAX INTEGER (0..768000)
            ACCESS read-write
            STATUS mandatory
            DESCRIPTION
                "This parameter (BC) specifies the maximum amount of data (in
                 bits) that the Frame Relay network agrees to transfer over a
                 time interval T = Committed Burst Size / Committed Information
                 Rate.This must be greater than 1/20th of the Committed 
                 Information Rate.This is used for congestion control purposes."
--            DEFVAL { 16000 }
        ::= { cdx6500SPCTFRIStationEntry 6 }

        friSCfgTransDelay OBJECT-TYPE
            SYNTAX INTEGER (1..65535)
            ACCESS read-write
            STATUS mandatory
            DESCRIPTION
                "This parameter is the expected one-way delay through the
                 network. This is specified in milliseconds."
--            DEFVAL { 50 }
        ::= { cdx6500SPCTFRIStationEntry 7 }

        friSCfgCongControlMode OBJECT-TYPE
            SYNTAX INTEGER {
                   normal    (1),
                   disable   (2),
                   congested (3),
                   limit     (4),
                   nc        (100)
                   }
            ACCESS read-write
            STATUS mandatory
            DESCRIPTION
                "This parameter controls the method of congestion control:
                 normal       - congestion control is normal
                 disable      - never enter Congested State (CTRL)
                 congested    - never enter a Non-Congested State (UCTRL)
                 limit        - congestion control is normal but limits the
                                data rate to no lower than CIR.
                 nc           - Skipped during configuration"
--            DEFVAL { normal }
        ::= { cdx6500SPCTFRIStationEntry 8 }

        friSCfgLinkAddress OBJECT-TYPE
            SYNTAX INTEGER {
                   dte (1),
                   dce (2),
                   nc  (100)
                   }
            ACCESS read-write
            STATUS mandatory
            DESCRIPTION
                "The logical link address must be set to complementary values
                 in this FRI Station and its corresponding remote FRI station:
                 dte - station to have logical DTE address (A)
                 dce - station to have logical DCE address (B)
                 nc  - Skipped during configuration."
--            DEFVAL { dte }
        ::= { cdx6500SPCTFRIStationEntry 9 }

        friSCfgPVCChannels OBJECT-TYPE
            SYNTAX INTEGER (0..128)
            ACCESS read-write
            STATUS mandatory
            DESCRIPTION
                "Number of logical channels used for Permanent Virtual 
                 Circuits. The total number of PVC and SVC channels on a 
                 station (logical link) should be kept as small as possible and
                 consistent with needs. PVC connections must be configured in
                 the PVC Table."
--            DEFVAL { 0 }
        ::= { cdx6500SPCTFRIStationEntry 10 }

        friSCfgStartingPVC OBJECT-TYPE
            SYNTAX INTEGER (1..4095)
            ACCESS read-write
            STATUS mandatory
            DESCRIPTION
                "This is the starting logical channel number for the 
                 Permanent Virtual Circuits on this FRI station. Not used if 
                 the number of PVCs = 0."
--            DEFVAL { 1 }
        ::= { cdx6500SPCTFRIStationEntry 11 }


        friSCfgSVCChannels OBJECT-TYPE
            SYNTAX INTEGER (0..512)
            ACCESS read-write
            STATUS mandatory
            DESCRIPTION
                "Number of logical channels used in Switched Virtual Circuits.
                 The total number of PVC and SVC channels on a station 
                 (logical link) should be kept as small as possible and 
                 consistent with needs."
--            DEFVAL { 16 }
        ::= { cdx6500SPCTFRIStationEntry 12 }

        friSCfgStartingSVC OBJECT-TYPE
            SYNTAX INTEGER (0..4095)
            ACCESS read-write
            STATUS mandatory
            DESCRIPTION
                "This is the starting logical channel number for the switched
                 Virtual Circuits on this station (logical link). Not used if
                 the number of SVCs = 0."
--            DEFVAL { 1 }
        ::= { cdx6500SPCTFRIStationEntry 13 }

        friSCfgVoiceSVCChannels OBJECT-TYPE
            SYNTAX INTEGER (0..15)
            ACCESS read-write
            STATUS mandatory
            DESCRIPTION
                "Number of logical voice channels used in Switched Virtual
                 Circuits.The total number of voice and data channels on a
                 station (logical link) should be kept as small as possible
                 and consistent with needs."
--            DEFVAL { 0 }
        ::= { cdx6500SPCTFRIStationEntry 14 }

        friSCfgInitialFrame OBJECT-TYPE
            SYNTAX INTEGER {
                   sabm (1),
                   disc (2),
                   none (3),
                   nc   (100)
                   }
            ACCESS read-write
            STATUS mandatory
            DESCRIPTION
                "Specify the initial frame required by other end for (logical)
                 link startup:
                 none - do nothing (other end initiates link-up)
                 sabm - send SABM
                 disc - send DISC, then SABM
                 nc   - Skipped during configuration"
--            DEFVAL { sabm }
        ::= { cdx6500SPCTFRIStationEntry 15 }

        friSCfgT1RetryTimer OBJECT-TYPE
            SYNTAX INTEGER (1..254)
            ACCESS read-write
            STATUS mandatory
            DESCRIPTION
                "T1 link retry timer value in tenths of a second (e.g. 
                 30 = 3.0 seconds)."
--            DEFVAL { 30 }
        ::= { cdx6500SPCTFRIStationEntry 16 }

        friSCfgT4PollTimer OBJECT-TYPE
            SYNTAX INTEGER (10..255)
            ACCESS read-write
            STATUS mandatory
            DESCRIPTION
                "This sets the time an idle link is probed for assurance of
                 connection to the remote FRI station.
                 0 - disable the timer
                 X - X is in tenths of a second (e.g. 40 = 4.0 seconds) and
                 must be set greater than T1."
--            DEFVAL { 40 }
        ::= { cdx6500SPCTFRIStationEntry 17 }

        friSCfgN2TransmissionTries OBJECT-TYPE
            SYNTAX INTEGER (1..20)
            ACCESS read-write
            STATUS mandatory
            DESCRIPTION
                "The maximum number of attempts allowed to complete a
                 transmission."
--            DEFVAL { 10 }
        ::= { cdx6500SPCTFRIStationEntry 18 }

        friSCfgKFrameWindow OBJECT-TYPE
            SYNTAX INTEGER (1..63)
            ACCESS read-write
            STATUS mandatory
            DESCRIPTION
                "Frame level window size must be set to the same value
                 at both this FRI station and the remote FRI station."
--            DEFVAL { 7 }
        ::= { cdx6500SPCTFRIStationEntry 19 }

        friSCfgWPacketWindow OBJECT-TYPE
            SYNTAX INTEGER (1..63)
            ACCESS read-write
            STATUS mandatory
            DESCRIPTION
                "Packet level window size must be set to the same value
                 at both this FRI station and the remote FRI station."
--            DEFVAL { 2 }
        ::= { cdx6500SPCTFRIStationEntry 20 }

        friSCfgPPacketSize  OBJECT-TYPE
            SYNTAX INTEGER {
                   psize32   (6),
                   psize64   (7),
                   psize128  (8),
                   psize256  (9),
                   psize512  (10),
                   psize1024 (11),
                   nc        (100)
                   }
            ACCESS read-write
            STATUS mandatory
            DESCRIPTION
                "This is the maximum X.25 packet size used on the station
                 (logical link).For proper operation, this must be set to the
                 same value at both this FRI station and the remote FRI 
                 station."
--            DEFVAL { psize128 }
        ::= { cdx6500SPCTFRIStationEntry 21 }

        friSCfgDataQUpperThreshold OBJECT-TYPE
            SYNTAX INTEGER (0..63)
            ACCESS read-write
            STATUS mandatory
            DESCRIPTION
                "This is the maximum number of data packets a channel on this
                 station (logical link) will queue for transmission before
                 invoking flow control to the attached channel"
--            DEFVAL { 5 }
        ::= { cdx6500SPCTFRIStationEntry 22 }

        friSCfgDataQLowerThreshold OBJECT-TYPE
            SYNTAX INTEGER (0..63)
            ACCESS read-write
            STATUS mandatory
            DESCRIPTION
                "This is the number of data packets a channel on this FRI
                 station will have queued for transmission when it releases
                 flow control to the attached channel"
--            DEFVAL { 0 }
        ::= { cdx6500SPCTFRIStationEntry 23 }

        friSCfgRestartTimer OBJECT-TYPE
            SYNTAX INTEGER (5..255)
            ACCESS read-write
            STATUS mandatory
            DESCRIPTION
                "Restart-Request timeout timer in seconds. If timer expires on
                 this FRI station, the Restart request is sent again."
--            DEFVAL { 180 }
        ::= { cdx6500SPCTFRIStationEntry 24 }

        friSCfgResetTimer OBJECT-TYPE
            SYNTAX INTEGER (5..255)
            ACCESS read-write
            STATUS mandatory
            DESCRIPTION
                "Reset-Request timeout timer in seconds. If timer expires on
                 this FRI station, the Reset request is sent again."
--            DEFVAL { 180 }
        ::= { cdx6500SPCTFRIStationEntry 25 }

        friSCfgCallTimer OBJECT-TYPE
            SYNTAX INTEGER (5..255)
            ACCESS read-write
            STATUS mandatory
            DESCRIPTION
                "Call-Request timeout timer in seconds. If timer expires on
                 this FRI station, the call is cleared."
--            DEFVAL { 200 }
        ::= { cdx6500SPCTFRIStationEntry 26 }

        friSCfgClearTimer OBJECT-TYPE
            SYNTAX INTEGER (5..255)
            ACCESS read-write
            STATUS mandatory
            DESCRIPTION
                "Clear-Request timeout timer in seconds. If timer expires on
                 this FRI station, the Clear request is sent again."
--            DEFVAL { 180 }
        ::= { cdx6500SPCTFRIStationEntry 27 }

        friSCfgX25Options OBJECT-TYPE
            SYNTAX DisplayString (SIZE(0..24))
            ACCESS read-write
            STATUS mandatory
            DESCRIPTION
                "Select X.25 options on this station (logical link) as 
                 follows:
                 NONE  - no option specified
                 CUG   - check closed user group; otherwise, CUG passes
                         transparently
                 CAUSE - pass cause, diagnostic, code in outbound packets
                 HOLD  - hold calls over link restart
                 INL   - inter-node link. Link goes to another vanguard device
                         INL may be a poor choice for high speed links.
                         INLB is set with INL to improve performance of
                         high speed links.
                 INLB -  set with INL options to improve performance if
                         inter-node link is a high speed link.
                 DELAY - Enable Delay and Path Trace on this link
                 Any combination of above specified by summing (e.g. CUG+ 
                 CAUSE+....). "
        ::= { cdx6500SPCTFRIStationEntry 28 }

        friSCfgRestrictedConnDest OBJECT-TYPE
            SYNTAX DisplayString (SIZE(0..255))
            ACCESS read-write
            STATUS mandatory
            DESCRIPTION
                "All calls entering this station will be routed to the
                 destination specified in this parameter,irrespective of route
                 selection table entries. For example, to route all calls to
                 X.25 port 3, use X25-3.
                 To route calls to SDLC port 2, station 4, use SDLC-2S4. Blank
                 in this  field disables this function."
        ::= { cdx6500SPCTFRIStationEntry 29 }

        friSCfgCUGMembership OBJECT-TYPE
            SYNTAX DisplayString (SIZE(0..23))
            ACCESS read-write
            STATUS mandatory
            DESCRIPTION
                "The station may be a member of up to 8 different Closed User
                 Groups (CUGs).Each CUG membership must be a two digit number
                 (except --) and separated by a comma. ( e.g. 12,34,56,09,02,
                 03).
                 --    - No CUG Membership
                 00-99 - CUG Membership"
        ::= { cdx6500SPCTFRIStationEntry 30 }

        friSCfgBillingRecords OBJECT-TYPE
            SYNTAX INTEGER {
                   off (1),
                   on  (2),
                   nc  (100)
                   }
            ACCESS read-write
            STATUS mandatory
            DESCRIPTION
                "This controls whether billing (accounting) records will be
                 created for calls on this FRI station.
                 off - Billing records will not be created.
                 on  - Billing records will be created.
                 nc  - Skipped during configuration."
--            DEFVAL { off }
        ::= { cdx6500SPCTFRIStationEntry 31 }

        friSCfgFrameSegmenter OBJECT-TYPE
            SYNTAX INTEGER {
                   disabled (1),
                   enabled  (2),
                   nc       (100)
                   }
            ACCESS read-write
            STATUS mandatory
            DESCRIPTION
                "Determine if this station will support the Frame Segmenter
                 Header.The Frame Segmenter should be enabled if the frame
                 relay port is configured to pass voice traffic.
                 disabled - Frame Segmenter disabled.
                 enabled  - Frame Segmenter enabled.
                 nc       - Skipped during configuration."
--            DEFVAL { disabled }
        ::= { cdx6500SPCTFRIStationEntry 32 }

        friSCfgVoiceCongCtrlMode OBJECT-TYPE
            SYNTAX INTEGER {
                   disabled (1),
                   enabled  (2),
                   nc       (100)
                   }
            ACCESS read-write
            STATUS mandatory
            DESCRIPTION
                "This parameter controls the method of voice congestion 
                 control:
                 enabled  - Station is forced into congestion when voice 
                            calls are detected.
                 disabled - Station operates normally
                 Use this parameter in voice applications, when the frame 
                 relay network can have more than 140ms of data buffered. 
                 One typical application is asymmetrical speeds. When this 
                 parameter is enabled, and voice is detected, delta T is 
                 forced to 50 ms and the BC is forced to 1/20 CIR."
--            DEFVAL { disabled }
        ::= { cdx6500SPCTFRIStationEntry 33 }

        friSCfgPeakUtilization OBJECT-TYPE
            SYNTAX INTEGER (0..240)
            ACCESS read-write
            STATUS mandatory
            DESCRIPTION
                 "The monitoring interval size specifies the time period
                  to use when monitoring peak data link/CIR (Committed
                  Information Rate) utilization.

                  0 - Disables this monitoring feature.
                  1 to 240 - The number of 64 second increments to use for the
                  monitoring interval time period.
                  For example: a value of 30 specifies a time duration
                  equal to 30 * 64 seconds = 1920 seconds or 32 minutes."
--            DEFVAL { 0 }
        ::= { cdx6500SPCTFRIStationEntry 34 }

        friSCfgMaxInboundQueue OBJECT-TYPE
            SYNTAX INTEGER (100..2500)
            ACCESS read-write
            STATUS mandatory
            DESCRIPTION
                "This specifies the maximum number of frames from the
                 network which may be queued in a BYPASS station. A
                 smaller value may ve choosen for applications which
                 experience objectable delays, if due to excessive
                 growth of the BYPASS inbound queue."
--            DEFVAL { 2500 }
        ::= { cdx6500SPCTFRIStationEntry 35 }

        friSCfgAnnexGRateReduction OBJECT-TYPE
            SYNTAX INTEGER {
                   disabled (1),
                   enabled  (2),
                   nc (100)
                }
            ACCESS read-write
            STATUS mandatory
            DESCRIPTION
                "This parameter determines whether Annex G stations will
                 invoke implicit congestion control when a link error is
                 detected, reducing their transmitted information rate 
                 to 1/4 CIR. The rate will be gradually increased in the
                 normal way as non-BECN frames are received."
--            DEFVAL { enabled }
        ::= { cdx6500SPCTFRIStationEntry 36 }

	friSCfgCctType OBJECT-TYPE
            SYNTAX INTEGER {
	           pvc (1),
		   svc (2)
                }
            ACCESS read-write
            STATUS mandatory
            DESCRIPTION
	        "Select the station circuit type defined for the Frame Relay line.
                    pvc - station will operate on its assigned DLCI as a PVC.
                    svc - station will operate on a selected DLCI as a SVC."
        ::= { cdx6500SPCTFRIStationEntry 37 }

        friSCfgCallControl OBJECT-TYPE
            SYNTAX INTEGER {
	           auto  (1),
		   autd  (2),
		   recv  (3),
		   cnorm (4)
                }
            ACCESS read-write
            STATUS mandatory
            DESCRIPTION
	        "This parameter is used to control the calling behavior of the SVC station.
                    auto  - the station will automatically initiate
                            a FR SVC call on power up
                    autd  - the station will initiate a FR SVC call
                            on demand of data to transmit
                    recv  - the station can be used to receive inbound FR SVC
                            calls from attached network equipment
                    cnorm - Controlled Normal. This allows Annex G stations to
                            set up and disconnect Frame Relay SVC calls through
                            Network Services"
        ::= { cdx6500SPCTFRIStationEntry 38 }

        friSCfgRetryInterval OBJECT-TYPE
            SYNTAX INTEGER (1..255)
            ACCESS read-write
            STATUS mandatory
            DESCRIPTION
	        "This is the time interval between call attempts, in seconds, that a
                 station configured to initiate an SVC call will use. If a call attempt
                 fails, either because there was no response to the SETUP message or the
                 response was a RELEASE, the station will wait this amount of time before
                 sending the next SETUP message."
        ::= { cdx6500SPCTFRIStationEntry 39 }

        friSCfgCallAttempts OBJECT-TYPE
            SYNTAX INTEGER (0..255)
            ACCESS read-write
            STATUS mandatory
            DESCRIPTION
	        "This is the number of contiguous call attempts that a station
                 configured to initiate an SVC call will make to establish the call.
                 If this count expires, the station will not make any further call
                 attempts. A station (or port or node) boot is required to re-initiate
                 call attempts.
                 A value of 0 means the station makes call attempts indefinitely."
        ::= { cdx6500SPCTFRIStationEntry 40 }

        friSCfgIdleInterval OBJECT-TYPE
            SYNTAX INTEGER (10..255)
            ACCESS read-write
            STATUS mandatory
            DESCRIPTION
	        "This is the idle time, in seconds, allowed for FRI station to remain
                 idle. If the station using the SVC passes no data for this time
                 period, the SVC will be cleared. If the SVC is cleared and the
                 stations attempts to send data an SVC will be established for the
                 station."
        ::= { cdx6500SPCTFRIStationEntry 41 }

        friSCfgIeNegotiation OBJECT-TYPE
            SYNTAX DisplayString
            ACCESS read-write
            STATUS mandatory
            DESCRIPTION
	        "This allows the listing of parameters to be used in a
                 SETUP/CONNECT message.
                    dlci  - If the station initiates a call, it will select the
                            lowest value of DLCI available and specify it to be
                            used for the SVC.
                    llcp  - If the station initiates a call it specifies a full
                            set of Link Layer Core Parameters. If it receives a
                            call LLCP is used to set CIR and BC.
                    tpie  - If set, the station will send a Traffic Priority IE. The
                            value of the TP IE is determined by the application that
                            causes the call to be made. This parameter is only effective
                            on stations configured for CNORM Call Control.
                 Any combination of above specified by summing (DLCI+LLCP)."
        ::= { cdx6500SPCTFRIStationEntry 42 }

        friSCfgSubaddress OBJECT-TYPE
            SYNTAX OCTET STRING (SIZE(3..6))
            ACCESS read-write
            STATUS mandatory
            DESCRIPTION
	        "This specifies the station subaddress coded and an IE. it is coded as
                 an IA5 string with type of subaddress NSAP and IA5 AFI prefix added.
                 Incoming calls with a called subaddress of NSAP type and IA5 AFI prefix
                 that match this configured string will connect to this station.
                 If this parameter is blank, the corresponding IE is not sent on outgoing
                 calls. Incoming calls without the corresponding IE can connect to this
                 station. The valid format of this entry is:
                    xx...xx000000 or xx...xx000000
                 Where:
                    xx...xx00 represents the station number in IA5/ASCII coded hex
                              terminated with a NUL character suffix.
                              For example, a sub-address of 12 would be represented
                              by the string 313200.
                 To set the subaddress blank, set the address string to NUL only."
        ::= { cdx6500SPCTFRIStationEntry 43 }

        friSCfgCalledNumber OBJECT-TYPE
            SYNTAX OCTET STRING (SIZE(3..19))
            ACCESS read-write
            STATUS mandatory
            DESCRIPTION
	        "This is the address of the destination, in the appropriate numbering
                 plan, to which a call will be placed by this station. Called Party
                 Number is a manditory IE and should be specified if calls are
                 originated by this station. The valid format of this entry is:
                    xx...xx000101 or xx...xx000103
                 Where:
                    xx...xx00 represents the address of the destination in IA5/ASCII
                              coded hex terminated with a NUL character suffix.
                              For example, an address of 123 would be represented
                              by the string 31323300.
                    0101      is the suffix used to specify E.164 Number Plan
                    0103      is the suffix used to specify X.121 Number Plan"
        ::= { cdx6500SPCTFRIStationEntry 44 }

        friSCfgCalledSubaddress OBJECT-TYPE
            SYNTAX OCTET STRING (SIZE(3..6))
            ACCESS read-write
            STATUS mandatory
            DESCRIPTION
                "For outgoing calls, this specifies the calling subaddress IE coded as
                 an IA5 string with type of subaddress NSAP and IA5 AFI prefix added.
                 Incoming calls with a called subaddress of NSAP type and IA5 AFI prefix
                 that match this configured string will connect to this station.
                 If this parameter is blank, the corresponding IE is not sent on outgoing
                 calls. Incoming calls without the corresponding IE can connect to this
                 station. The valid format of this entry is:
                    xx...xx000000 or xx...xx000000
                 Where:
                    xx...xx00 represents the station number in IA5/ASCII coded hex
                              terminated with a NUL character suffix.
                              For example, a sub-address of 12 would be represented
                              by the string 313200.
                 To set the subaddress blank, set the address string to NUL only."
        ::= { cdx6500SPCTFRIStationEntry 45 }

        friSCfgMinCir OBJECT-TYPE
            SYNTAX INTEGER (0..155000000)
            ACCESS read-write
            STATUS mandatory
            DESCRIPTION
	        "This parameter specifies the minimum rate (in bits-per-second)
                 that the Frame Relay network agrees to transfer information under congested 
                 conditions for the FRI Station.
                 This value is used to throttle data on the FRI station for congestion 
                 control purposes.  The MinCIR cannot be greater than the link speed."
        ::= { cdx6500SPCTFRIStationEntry 46 }

        friSCfgBurstExcess OBJECT-TYPE
            SYNTAX INTEGER (0..4096000)
            ACCESS read-write
            STATUS mandatory
            DESCRIPTION
	        "This parameter is negotiated during the call establishment phase of
                 Frame Relay SVC calls.  It has no effect on data transfer after the call has
                 been established."
        ::= { cdx6500SPCTFRIStationEntry 47 }

        
        friSCfgE2EEnabled		        OBJECT-TYPE
            SYNTAX INTEGER {
                   disable (1),
                   enable  (2)
                   }
            ACCESS read-write
            STATUS mandatory
            DESCRIPTION
            "Enable End-to-End Segmentation. Peer must be enabled too.
              enabled  - Enable End-to-End Segmentation
              disabled - Disable End-to-End Segmentation"
--            DEFVAL { disable }
        ::= { cdx6500SPCTFRIStationEntry 48 }
        
        friSCfgE2EType		        OBJECT-TYPE
            SYNTAX INTEGER {
                   motorola (1),
                   frf12    (2)
                   }
            ACCESS read-write
            STATUS mandatory
            DESCRIPTION
            "Type of End-to-End Segmentation.  Peer must be configured in the same way.
              FRF12    - FRF.12 End-to-End Segmentation
              Motorola - Motorola End-to-End Segmentation"
--            DEFVAL { FRF12 }
        ::= { cdx6500SPCTFRIStationEntry 49 }

        friSCfgE2ESSVP 				OBJECT-TYPE
            SYNTAX INTEGER {
                   segSize64   (65),
                   segSize128  (129),
                   segSize256  (257),
                   segSize512  (513),
                   segSize1024 (1025)
                   }
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
                "Segment size when voice is present. 
                 Affects transmit path only.
                 segSize64   - 64   byte segments
                 segSize128  - 128  byte segments
                 segSize256  - 256  byte segments
                 segSize512  - 512  byte segments
                 segSize1024 - 1024 byte segments"
--            DEFVAL { 64 }
        ::= { cdx6500SPCTFRIStationEntry 50 }

        friSCfgE2ESSVNP     OBJECT-TYPE
            SYNTAX INTEGER {
                   segSize64   (65),
                   segSize128  (129),
                   segSize256  (257),
                   segSize512  (513),
                   segSize1024 (1025), 
                   segSize2048 (2049), 
                   segSize4096 (4097), 
                   disable	   (32000)
                   }
            ACCESS read-write
            STATUS mandatory
            DESCRIPTION
                "Segment size when voice is not present. 
		         Affects transmit path only.
                 segSize64   - 64   byte segments
                 segSize128  - 128  byte segments
                 segSize256  - 256  byte segments
                 segSize512  - 512  byte segments
                 segSize1024 - 1024 byte segments 
		         segSize2048 - 2048 byte segments 
		         segSize4096 - 4096 byte segments 
		         disable - this selection doesn't affect the 
		         packet size.  End-to-End segmentation header 
		         will be inserted only."
--            DEFVAL { disable }
        ::= { cdx6500SPCTFRIStationEntry 51 }
 
        friSCfgE2ESegDelayEnabled		        OBJECT-TYPE
            SYNTAX INTEGER {
                   disable (1),
                   enable  (2)
                   }
            ACCESS read-write
            STATUS mandatory
            DESCRIPTION
                "Discards a packet if any segment delays more 
                 than 10 seconds. 
                 Affects receive path only. 
                 enabled  - discard the packet
                 disabled - do not discard the packet"
--            DEFVAL { disable }
        ::= { cdx6500SPCTFRIStationEntry 52 }
  
        friSCfgE2ECheckPckSize		        OBJECT-TYPE
            SYNTAX INTEGER {
                   disable (1),
                   enable  (2)
                   }
            ACCESS read-write
            STATUS mandatory
            DESCRIPTION
                "Check if the packet size is larger than the node
                 maximum packet size.
                   enabled  - discard packet than is larger than
                              the maximum size.
                   disabled - accept packet than is larger than
                              the maximum size."
--            DEFVAL { disable }
        ::= { cdx6500SPCTFRIStationEntry 53 }

        friSCfgvoice-header		        OBJECT-TYPE
            SYNTAX INTEGER {
                   disable (1),
                   enable  (2)
                   }
            ACCESS read-write
            STATUS mandatory
            DESCRIPTION
            "Voice Header.
              enabled  - Enable Voice Header Insertion
              disabled - Disable Voice Header Insertion"
--            DEFVAL { disable }
        ::= { cdx6500SPCTFRIStationEntry 54 }

        -- FRI Port Statistics Table
