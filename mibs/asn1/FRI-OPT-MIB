        -- Packet Summary Statistics Table
        friSPacketSummaryStatsTable OBJECT-TYPE
            SYNTAX SEQUENCE OF Cdx6500friSPacketSummaryStatsEntry
            ACCESS not-accessible
            STATUS mandatory
            DESCRIPTION
                "This table contains FRI Station Packet Summary Statistics
                 parameters."
        ::= { cdx6500SPSTFRISStatsTable 5 }

        cdx6500friSPacketSummaryStatsEntry OBJECT-TYPE
            SYNTAX Cdx6500friSPacketSummaryStatsEntry
            ACCESS not-accessible
            STATUS mandatory
            DESCRIPTION
                "Entries in the FRI Station Packet Summary Statistics Table.
                 NOTE : Value of all objects in this table will be 0 for
                 Bypass stations (not available)"
            INDEX { friSPktSummStatsPortNumber, 
                    friSPktSummStatsStnNumber }
        ::= { friSPacketSummaryStatsTable 1 }

        Cdx6500friSPacketSummaryStatsEntry ::=
            SEQUENCE {
                friSPktSummStatsPortNumber
                    INTEGER,
                friSPktSummStatsStnNumber
                    INTEGER,
                friSStatsDataPktInTotal
                    Counter,
                friSStatsDataPktOutTotal
                    Counter,
                friSStatsRRPktInTotal
                    Counter,
                friSStatsRRPktOutTotal
                    Counter,
                friSStatsRNRPktInTotal
                    Counter,
                friSStatsRNRPktOutTotal
                    Counter,
                friSStatsREJPktInTotal
                    Counter,
                friSStatsREJPktOutTotal
                    Counter,
                friSStatsCallReqPktInTotal
                    Counter,
                friSStatsCallReqPktOutTotal
                    Counter,
                friSStatsCallAccPktInTotal
                    Counter,
                friSStatsCallAccPktOutTotal
                    Counter,
                friSStatsClrReqPktInTotal
                    Counter,
                friSStatsClrReqPktOutTotal
                    Counter,
                friSStatsClrConfPktInTotal
                    Counter,
                friSStatsClrConfPktOutTotal
                    Counter,
                friSStatsIntrReqPktInTotal
                    Counter,
                friSStatsIntrReqPktOutTotal
                    Counter,
                friSStatsIntrConfPktInTotal
                    Counter,
                friSStatsIntrConfPktOutTotal
                    Counter,
                friSStatsResetReqPktInTotal
                    Counter,
                friSStatsResetReqPktOutTotal
                    Counter,
                friSStatsResetConfPktInTotal
                    Counter,
                friSStatsResetConfPktOutTotal
                    Counter,
                friSStatsRestartReqPktInTotal
                    Counter,
                friSStatsRestartReqPktOutTotal
                    Counter,
                friSStatsRestartConfPktInTotal
                    Counter,
                friSStatsRestartConfPktOutTotal
                    Counter
                }

        friSPktSummStatsPortNumber OBJECT-TYPE
            SYNTAX INTEGER (1..255)
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
                "The FRI port number corresponding to this station."
        ::= { cdx6500friSPacketSummaryStatsEntry 1 }

        friSPktSummStatsStnNumber OBJECT-TYPE
            SYNTAX INTEGER (1..254)
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
                "The number of the FRI station."
        ::= { cdx6500friSPacketSummaryStatsEntry 2 }

        friSStatsDataPktInTotal OBJECT-TYPE
