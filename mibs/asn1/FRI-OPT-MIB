        -- FRI Summary Statistics Table
        friSFRISummaryStatsTable OBJECT-TYPE
            SYNTAX SEQUENCE OF Cdx6500friSFRISummaryStatsEntry
            ACCESS not-accessible
            STATUS mandatory
            DESCRIPTION
                "This table contains FRI Station FRI Summary Statistics
                 parameters."
        ::= { cdx6500SPSTFRISStatsTable 6 }

        cdx6500friSFRISummaryStatsEntry OBJECT-TYPE
            SYNTAX Cdx6500friSFRISummaryStatsEntry
            ACCESS not-accessible
            STATUS mandatory
            DESCRIPTION
                "Entries in the FRI Station FRI Summary Statistics Table.
                 NOTE : Value of all objects in this table are valid only
                 for Bypass stations; 0 will be returned otherwise."
            INDEX { friSFRISummStatsPortNumber, 
                    friSFRISummStatsStnNumber }
        ::= { friSFRISummaryStatsTable 1 }

        Cdx6500friSFRISummaryStatsEntry ::=
            SEQUENCE {
                friSFRISummStatsPortNumber
                    INTEGER,
                friSFRISummStatsStnNumber
                    INTEGER,
                friSStatsFECNInTotal
                    Counter,
                friSStatsFECNOutTotal
                    Counter,
                friSStatsBECNInTotal
                    Counter,
                friSStatsBECNOutTotal
                    Counter,
                friSStatsDEInTotal
                    Counter,
                friSStatsDEOutTotal
                    Counter
                }

        friSFRISummStatsPortNumber OBJECT-TYPE
            SYNTAX INTEGER (1..255)
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
                "The FRI port number corresponding to this station."
        ::= { cdx6500friSFRISummaryStatsEntry 1 }

        friSFRISummStatsStnNumber OBJECT-TYPE
            SYNTAX INTEGER (1..254)
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
                "The number of the FRI station."
        ::= { cdx6500friSFRISummaryStatsEntry 2 }

        friSStatsFECNInTotal OBJECT-TYPE
            SYNTAX Counter
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
                "Number of Forward Explicit Congestion Notification (FECN)
                 packets received since last node boot or statistics reset."
        ::= { cdx6500friSFRISummaryStatsEntry 3 }

        friSStatsFECNOutTotal OBJECT-TYPE
            SYNTAX Counter
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
                 "Number of Forward Explicit Congestion Notification (FECN)
                  packets transmitted since last node boot or statistics reset."
        ::= { cdx6500friSFRISummaryStatsEntry 4 }

        friSStatsBECNInTotal OBJECT-TYPE
            SYNTAX Counter
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
                "Number of Backward Explicit Congestion Notification (BECN)
                 packets received since last node boot or statistics reset."
        ::= { cdx6500friSFRISummaryStatsEntry 5 }

        friSStatsBECNOutTotal OBJECT-TYPE
            SYNTAX Counter
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
                "Number of Backward Explicit Congestion Notification (BECN)
                 packets transmitted since last node boot or statistics reset."
        ::= { cdx6500friSFRISummaryStatsEntry 6 }

        friSStatsDEInTotal OBJECT-TYPE
            SYNTAX Counter
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
                "Number of Discard Eligibility (DE) packets received 
                 since last node boot or statistics reset."
        ::= { cdx6500friSFRISummaryStatsEntry 7 }

        friSStatsDEOutTotal OBJECT-TYPE
            SYNTAX Counter
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
                "Number of Discard Eligibility (DE) packets transmitted 
                 since last node boot or statistics reset."
        ::= { cdx6500friSFRISummaryStatsEntry 8 }

        -- The FRI Port Control Table
        cdx6500ContFRITable OBJECT IDENTIFIER 
        ::= { cdx6500Controls 14 }

        cdx6500FRIPContTable OBJECT-TYPE
            SYNTAX SEQUENCE OF Cdx6500FRIPContEntry
            ACCESS not-accessible
            STATUS mandatory
            DESCRIPTION
                "Control parameters for FRI ports."
        ::= { cdx6500ContFRITable 1 }

        cdx6500FRIPContEntry OBJECT-TYPE
            SYNTAX Cdx6500FRIPContEntry
            ACCESS not-accessible
            STATUS mandatory
            DESCRIPTION
                "Each control applies to a single FRI port."
            INDEX { friPContPortNumber }
        ::= { cdx6500FRIPContTable 1 }

        Cdx6500FRIPContEntry ::=
            SEQUENCE {
                friPContPortNumber
                    INTEGER,
                friPContPortControl
                    INTEGER
                }

        friPContPortNumber OBJECT-TYPE
            SYNTAX INTEGER (1..255)
            ACCESS not-accessible
            STATUS mandatory
            DESCRIPTION
                "Port number of the FRI port."
        ::= { cdx6500FRIPContEntry 1 }

        friPContPortControl OBJECT-TYPE
            SYNTAX INTEGER {
                   boot       (1),
                   enable     (2),
                   disable    (3),
                   resetstats (4)
                   }
            ACCESS write-only
            STATUS mandatory
            DESCRIPTION
                "Control operations for the specified FRI port.
                 boot       - Boots the FRI port
                 enable     - Enables the FRI port
                 disable    - Disables the FRI port
                 resetstats - Resets the FRI port statistics."
        ::= { cdx6500FRIPContEntry 2 }

        -- The FRI Station Control Table

        cdx6500FRISContTable OBJECT-TYPE
            SYNTAX SEQUENCE OF Cdx6500FRISContEntry
            ACCESS not-accessible
            STATUS mandatory
            DESCRIPTION
                "Control parameters for FRI stations."
        ::= { cdx6500ContFRITable 2 }

        cdx6500FRISContEntry OBJECT-TYPE
            SYNTAX Cdx6500FRISContEntry
            ACCESS not-accessible
            STATUS mandatory
            DESCRIPTION
                "Each control applies to a single FRI station."
            INDEX { friSContPortNumber, friSContStnNumber }
        ::= { cdx6500FRISContTable 1 }

        Cdx6500FRISContEntry ::=
            SEQUENCE {
                friSContPortNumber
                    INTEGER,
                friSContStnNumber
                    INTEGER,
                friSContStnControl
                    INTEGER
            }

        friSContPortNumber OBJECT-TYPE
            SYNTAX INTEGER (1..255)
            ACCESS not-accessible
            STATUS mandatory
            DESCRIPTION
                "The FRI port number corresponding to this station."
        ::= { cdx6500FRISContEntry 1 }

        friSContStnNumber OBJECT-TYPE
            SYNTAX INTEGER (1..254)
            ACCESS not-accessible
            STATUS mandatory
            DESCRIPTION
                "FRI station number."
        ::= { cdx6500FRISContEntry 2 }

        friSContStnControl OBJECT-TYPE
            SYNTAX INTEGER {
                   boot       (1),
                   enable     (2),
                   disable    (3),
                   resetstats (4)
                   }
            ACCESS write-only
            STATUS mandatory
            DESCRIPTION
                "Control operations for the specified FRI station.
                 boot       - Boots the FRI station
                 enable     - Enables the FRI station
                 disable    - Disables the FRI station
                 resetstats - Resets the FRI station statistics."
        ::= { cdx6500FRISContEntry 3 }

        END
