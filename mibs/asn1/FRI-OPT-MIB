        cdx6500PPSTFRIPortTable OBJECT-TYPE
            SYNTAX SEQUENCE OF Cdx6500PPSTFRIPortEntry
            ACCESS not-accessible
            STATUS mandatory
            DESCRIPTION
                "This table contains FRI Port Statistics parameters."
        ::= { cdx6500PPSTFRIPStatsTable 1 }

        cdx6500PPSTFRIPortEntry OBJECT-TYPE
            SYNTAX Cdx6500PPSTFRIPortEntry
            ACCESS not-accessible
            STATUS mandatory
            DESCRIPTION
                "Entries in the FRI Port Statistics Table."
            INDEX { friPStatsPortNumber }
        ::= { cdx6500PPSTFRIPortTable 1 }

        Cdx6500PPSTFRIPortEntry ::=
            SEQUENCE {
                friPStatsPortNumber
                    INTEGER,
                friPStatsPortType
                    INTEGER,
                friPStatsPortStatus
                    INTEGER,
                friPStatsConfiguredStns
                    DisplayString,
                friPStatsPortSpeed
                    INTEGER,
                friPStatsOpCntrlProtocol
                    DisplayString,
                friPStatsSPBackup
                    DisplayString,
                friPStatsPriorityStn
                    INTEGER,
                friPStatsLastStatsReset
                    DisplayString
                }

        friPStatsPortNumber OBJECT-TYPE
            SYNTAX INTEGER (1..255)
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
                "The port number of the FRI port."
        ::= { cdx6500PPSTFRIPortEntry 1 }

