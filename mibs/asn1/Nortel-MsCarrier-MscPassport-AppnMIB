   SYNTAX     MscAppnDnaCallOptionsEntry
   ACCESS     not-accessible
   STATUS     mandatory
   DESCRIPTION
     "An entry in the mscAppnDnaCallOptionsTable."
   INDEX {
      mscAppnIndex,
      mscAppnDnaIndex }
::= { mscAppnDnaCallOptionsTable 1 }

MscAppnDnaCallOptionsEntry ::= SEQUENCE {
   mscAppnDnaDefaultRecvFrmNetworkThruputClass
      Unsigned32,
   mscAppnDnaDefaultSendToNetworkThruputClass
      Unsigned32,
   mscAppnDnaDefaultRecvFrmNetworkWindowSize
      Unsigned32,
   mscAppnDnaDefaultSendToNetworkWindowSize
      Unsigned32,
   mscAppnDnaAccountClass
      Unsigned32,
   mscAppnDnaAccountCollection
      OCTET STRING,
   mscAppnDnaServiceExchange
      Unsigned32 }

mscAppnDnaDefaultRecvFrmNetworkThruputClass OBJECT-TYPE
   SYNTAX     Unsigned32 (0..15)
   ACCESS     read-write
   STATUS     mandatory
   DESCRIPTION
     "This attribute indicates the default receive throughput class for all 
      calls using this particular Dna."
   DEFVAL     { 13 }
::= { mscAppnDnaCallOptionsEntry 5 }

mscAppnDnaDefaultSendToNetworkThruputClass OBJECT-TYPE
   SYNTAX     Unsigned32 (0..15)
   ACCESS     read-write
   STATUS     mandatory
   DESCRIPTION
     "This attribute indicates the default send throughput class for all calls 
      using this particular Dna."
   DEFVAL     { 13 }
::= { mscAppnDnaCallOptionsEntry 6 }

mscAppnDnaDefaultRecvFrmNetworkWindowSize OBJECT-TYPE
   SYNTAX     Unsigned32 (1..7)
   ACCESS     read-write
   STATUS     mandatory
   DESCRIPTION
     "This attribute indicates the default number of data packets that can 
      be received by the DTE from the DCE before more packets can be 
      received. This view is oriented with respect to the DTE."
   DEFVAL     { 2 }
::= { mscAppnDnaCallOptionsEntry 7 }

mscAppnDnaDefaultSendToNetworkWindowSize OBJECT-TYPE
   SYNTAX     Unsigned32 (1..7)
   ACCESS     read-write
   STATUS     mandatory
   DESCRIPTION
     "This attribute indicates the number of data packets that can be 
      transmitted from the DTE to the DCE and must be acknowledged 
      before more packets can be transmitted."
   DEFVAL     { 2 }
::= { mscAppnDnaCallOptionsEntry 8 }

mscAppnDnaAccountClass OBJECT-TYPE
   SYNTAX     Unsigned32 (0..255)
   ACCESS     read-write
   STATUS     mandatory
   DESCRIPTION
     "This attribute specifies the accounting class which is reserved for 
      network operations usage. Its value is returned in the accounting 
      record in the local and remote service type attributes. Use of this 
      attribute is decided by network operator and it is an arbitrary 
      number."
   DEFVAL     { 0 }
::= { mscAppnDnaCallOptionsEntry 10 }

mscAppnDnaAccountCollection OBJECT-TYPE
   SYNTAX     OCTET STRING (SIZE (1))
   ACCESS     read-write
   STATUS     mandatory
   DESCRIPTION
     "This attribute indicates that accounting records are to be collected 
      by the network for the various reasons: billing, test, study, auditing. 
      The last of the parameters, force, indicates that accounting records 
      are to be collected irrespective of other collection reasons. If none 
      of these reasons are set, then accounting will be suppressed.

      Description of bits:
      bill(0)
      test(1)
      study(2)
      audit(3)
      force(4)"
   DEFVAL     { '80'H }
::= { mscAppnDnaCallOptionsEntry 11 }

mscAppnDnaServiceExchange OBJECT-TYPE
   SYNTAX     Unsigned32 (0..255)
   ACCESS     read-write
   STATUS     mandatory
   DESCRIPTION
     "This attribute is an arbitrary number, entered by the network 
      operator. It is included in the accounting record created by Vc."
   DEFVAL     { 0 }
::= { mscAppnDnaCallOptionsEntry 12 }


