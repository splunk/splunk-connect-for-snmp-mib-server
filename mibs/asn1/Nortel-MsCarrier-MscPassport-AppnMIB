      AsciiString,
   mscAppnDLURSecondaryDefDlusName
      AsciiString,
   mscAppnDLURDlusRetryTimeout
      Unsigned32,
   mscAppnDLURDlusRetryLimit
      Unsigned32 }

mscAppnDLURPrimaryDefDlusName OBJECT-TYPE
   SYNTAX     AsciiString (SIZE (3..17))
   ACCESS     read-write
   STATUS     mandatory
   DESCRIPTION
     "The fully qualified CP name of the node that will serve as the 
      primary dependent LU server (DLUS)."
::= { mscAppnDLURDlurParmsEntry 2 }

mscAppnDLURSecondaryDefDlusName OBJECT-TYPE
   SYNTAX     AsciiString (SIZE (0..17))
   ACCESS     read-write
   STATUS     mandatory
   DESCRIPTION
     "The fully qualified CP name of the node that will serve as the 
      backup or secondary dependent LU server (DLUS)."
::= { mscAppnDLURDlurParmsEntry 3 }

mscAppnDLURDlusRetryTimeout OBJECT-TYPE
   SYNTAX     Unsigned32 (1..65535)
   ACCESS     read-write
   STATUS     mandatory
   DESCRIPTION
     "This attribute specifies the time in seconds before a network search  
      to locate a DLUS times out."
   DEFVAL     { 5 }
::= { mscAppnDLURDlurParmsEntry 4 }

mscAppnDLURDlusRetryLimit OBJECT-TYPE
   SYNTAX     Unsigned32 (0..65535)
   ACCESS     read-write
   STATUS     mandatory
   DESCRIPTION
     "This attribute specifies the maximum number of retry attempts that 
      will be made after an initial failure to contact a DLUS. If zero is 
      specifed, APPN will retry indefinitely. When this limit has been 
      reached, downstream PUs that are connected to APPN will be sent a 
      QDISC frame."
   DEFVAL     { 0 }
::= { mscAppnDLURDlurParmsEntry 5 }


-- Appn/<string> Cos/<string>
-- This component defines the characteristics of a named class of 
-- service. A class of service contains information used in selecting a 
-- route and priority for a specific session. The class of service 
-- component requires at least one tgRow and one nodeRow 
-- subcomponent  to a  maximum of eight.
-- A default set of class of service entries is added with the following 
-- names: SNASVCMG, CPSVCMG, #CONNECT, #INTER, #I 
-- NTERSC, #BATCH, #BATCHSC. These class of service entries  
-- are not to be deleted.

mscAppnCos OBJECT IDENTIFIER ::= { mscAppn 17 }



mscAppnCosRowStatusTable OBJECT-TYPE
   SYNTAX     SEQUENCE OF MscAppnCosRowStatusEntry
   ACCESS     not-accessible
   STATUS     mandatory
   DESCRIPTION
     "This entry controls the addition and deletion of 
      mscAppnCos components."
::= { mscAppnCos 1 }

mscAppnCosRowStatusEntry OBJECT-TYPE
   SYNTAX     MscAppnCosRowStatusEntry
   ACCESS     not-accessible
   STATUS     mandatory
   DESCRIPTION
     "A single entry in the table represents a single 
      mscAppnCos component."
   INDEX {
      mscAppnIndex,
      mscAppnCosIndex }
::= { mscAppnCosRowStatusTable 1 }

MscAppnCosRowStatusEntry ::= SEQUENCE {
   mscAppnCosRowStatus
      RowStatus,
   mscAppnCosComponentName
      DisplayString,
   mscAppnCosStorageType
      StorageType,
   mscAppnCosIndex
      AsciiStringIndex }

mscAppnCosRowStatus OBJECT-TYPE
   SYNTAX     RowStatus 
   ACCESS     read-write
   STATUS     mandatory
   DESCRIPTION
     "This variable is used as the basis for SNMP naming of
      mscAppnCos components.
      These components can be added and deleted."
::= { mscAppnCosRowStatusEntry 1 }

mscAppnCosComponentName OBJECT-TYPE
   SYNTAX     DisplayString 
   ACCESS     read-only
   STATUS     mandatory
   DESCRIPTION
     "This variable provides the component's string name for
      use with the ASCII Console Interface"
::= { mscAppnCosRowStatusEntry 2 }

mscAppnCosStorageType OBJECT-TYPE
   SYNTAX     StorageType 
   ACCESS     read-only
   STATUS     mandatory
   DESCRIPTION
     "This variable represents the storage type value for the
      mscAppnCos tables."
::= { mscAppnCosRowStatusEntry 4 }

mscAppnCosIndex OBJECT-TYPE
   SYNTAX     AsciiStringIndex (SIZE (1..8))
   ACCESS     not-accessible
   STATUS     mandatory
   DESCRIPTION
     "This variable represents the index for the mscAppnCos tables."
::= { mscAppnCosRowStatusEntry 10 }

mscAppnCosProvTable OBJECT-TYPE
   SYNTAX     SEQUENCE OF MscAppnCosProvEntry
   ACCESS     not-accessible
   STATUS     mandatory
   DESCRIPTION
     "This provisioning group specifies the transmission priority 
      associated with a Class of Service."
::= { mscAppnCos 104 }

mscAppnCosProvEntry OBJECT-TYPE
   SYNTAX     MscAppnCosProvEntry
   ACCESS     not-accessible
   STATUS     mandatory
   DESCRIPTION
     "An entry in the mscAppnCosProvTable."
   INDEX {
      mscAppnIndex,
      mscAppnCosIndex }
::= { mscAppnCosProvTable 1 }

MscAppnCosProvEntry ::= SEQUENCE {
   mscAppnCosTransmissionPriority
      INTEGER }

mscAppnCosTransmissionPriority OBJECT-TYPE
   SYNTAX     INTEGER  {
                 low(1),
                 medium(2),
                 high(3),
                 network(4) }
   ACCESS     read-write
   STATUS     mandatory
   DESCRIPTION
     "This attribute specifies the transmission priority of session traffic 
      using this class of service."
   DEFVAL     { low }
::= { mscAppnCosProvEntry 1 }


-- Appn/<string> Cos/<string> TgRow/<weight>
-- This component specifies the allowable ranges for transmission 
-- group characteristics for a class of service. During route calculation, 
-- a transmission group is considered acceptable if all of the  actual 
-- TG characteristics of a link fall within the range of required TG 
-- characteristics for a COS. A TG is considered unacceptable if at 
-- least one of the actual TG characteristics falls outside the range of 
-- the required TG values. 

mscAppnCosTg OBJECT IDENTIFIER ::= { mscAppnCos 10 }



mscAppnCosTgRowStatusTable OBJECT-TYPE
   SYNTAX     SEQUENCE OF MscAppnCosTgRowStatusEntry
   ACCESS     not-accessible
   STATUS     mandatory
   DESCRIPTION
     "This entry controls the addition and deletion of 
      mscAppnCosTg components."
::= { mscAppnCosTg 1 }

mscAppnCosTgRowStatusEntry OBJECT-TYPE
   SYNTAX     MscAppnCosTgRowStatusEntry
   ACCESS     not-accessible
   STATUS     mandatory
   DESCRIPTION
     "A single entry in the table represents a single 
      mscAppnCosTg component."
   INDEX {
      mscAppnIndex,
      mscAppnCosIndex,
      mscAppnCosTgIndex }
::= { mscAppnCosTgRowStatusTable 1 }

MscAppnCosTgRowStatusEntry ::= SEQUENCE {
   mscAppnCosTgRowStatus
      RowStatus,
   mscAppnCosTgComponentName
      DisplayString,
   mscAppnCosTgStorageType
      StorageType,
   mscAppnCosTgIndex
      Integer32 }

mscAppnCosTgRowStatus OBJECT-TYPE
   SYNTAX     RowStatus 
   ACCESS     read-write
   STATUS     mandatory
   DESCRIPTION
     "This variable is used as the basis for SNMP naming of
      mscAppnCosTg components.
      These components can be added and deleted."
::= { mscAppnCosTgRowStatusEntry 1 }

mscAppnCosTgComponentName OBJECT-TYPE
   SYNTAX     DisplayString 
   ACCESS     read-only
   STATUS     mandatory
   DESCRIPTION
     "This variable provides the component's string name for
      use with the ASCII Console Interface"
::= { mscAppnCosTgRowStatusEntry 2 }

mscAppnCosTgStorageType OBJECT-TYPE
   SYNTAX     StorageType 
   ACCESS     read-only
   STATUS     mandatory
   DESCRIPTION
     "This variable represents the storage type value for the
      mscAppnCosTg tables."
::= { mscAppnCosTgRowStatusEntry 4 }

mscAppnCosTgIndex OBJECT-TYPE
   SYNTAX     Integer32 (1..255)
   ACCESS     not-accessible
   STATUS     mandatory
   DESCRIPTION
     "This variable represents the index for the mscAppnCosTg tables."
::= { mscAppnCosTgRowStatusEntry 10 }

mscAppnCosTgProvTable OBJECT-TYPE
   SYNTAX     SEQUENCE OF MscAppnCosTgProvEntry
   ACCESS     not-accessible
   STATUS     mandatory
   DESCRIPTION
     "This group specifies a range of TG characteristics for a class of 
      service and is used in route selection for session traffic. 
      
      During route computation, APPN determines the optimum route 
      through a network in the following steps:
      - obtains the required route characteristics from the COS database  
      using the mode name specified in the session request
      - determines all possible combinations of transmission groups and 
      nodes that form a route to the destination database
      - the actual characteristics of the Nodes and TGs in these routes are 
      compared against the COS tables that contain the required 
      characteristics of the session.  A weight is assigned based on how 
      well the actual characteristics meet the required characteristics. 
      Where there is no match between actual and required 
      characteristics, the route containing the node or TG is excluded 
      from further computation.
      - Each possible route is quantified by adding the weights assigned 
      to the node and TGs in the route.  The route with the lowest weight 
      is the one used for the session."
::= { mscAppnCosTg 10 }

mscAppnCosTgProvEntry OBJECT-TYPE
   SYNTAX     MscAppnCosTgProvEntry
   ACCESS     not-accessible
   STATUS     mandatory
   DESCRIPTION
     "An entry in the mscAppnCosTgProvTable."
   INDEX {
      mscAppnIndex,
      mscAppnCosIndex,
      mscAppnCosTgIndex }
::= { mscAppnCosTgProvTable 1 }

MscAppnCosTgProvEntry ::= SEQUENCE {
   mscAppnCosTgMinEffectiveCapacity
      INTEGER,
   mscAppnCosTgMaxEffectiveCapacity
      INTEGER,
   mscAppnCosTgMinConnectCost
      INTEGER,
   mscAppnCosTgMaxConnectCost
      INTEGER,
   mscAppnCosTgMinByteCost
      INTEGER,
   mscAppnCosTgMaxByteCost
      INTEGER,
   mscAppnCosTgMinSecurity
      INTEGER,
   mscAppnCosTgMaxSecurity
      INTEGER,
   mscAppnCosTgMinPropDelay
      INTEGER,
   mscAppnCosTgMaxPropDelay
      INTEGER,
   mscAppnCosTgMinModemClass
      INTEGER,
   mscAppnCosTgMaxModemClass
      INTEGER,
   mscAppnCosTgMinUserDefParm1
      INTEGER,
   mscAppnCosTgMaxUserDefParm1
      INTEGER,
   mscAppnCosTgMinUserDefParm2
      INTEGER,
   mscAppnCosTgMaxUserDefParm2
      INTEGER,
   mscAppnCosTgMinUserDefParm3
      INTEGER,
   mscAppnCosTgMaxUserDefParm3
      INTEGER }

