   INDEX {
      mscAppnIndex,
      mscAppnRtpIndex }
::= { mscAppnRtpStatsTable 1 }

MscAppnRtpStatsEntry ::= SEQUENCE {
   mscAppnRtpTxBytes
      PassportCounter64,
   mscAppnRtpRxBytes
      PassportCounter64,
   mscAppnRtpBytesResent
      PassportCounter64,
   mscAppnRtpBytesDiscarded
      PassportCounter64,
   mscAppnRtpPktTx
      PassportCounter64,
   mscAppnRtpPktRx
      PassportCounter64,
   mscAppnRtpPktResent
      PassportCounter64,
   mscAppnRtpPktDiscard
      PassportCounter64,
   mscAppnRtpLostFrames
      PassportCounter64,
   mscAppnRtpCurTxRate
      Gauge32,
   mscAppnRtpMaxTxRate
      Gauge32,
   mscAppnRtpMinTxRate
      Gauge32,
   mscAppnRtpCurRxRate
      Gauge32,
   mscAppnRtpMaxRxRate
      Gauge32,
   mscAppnRtpMinRxRate
      Gauge32,
   mscAppnRtpBurstSize
      Gauge32,
   mscAppnRtpUptime
      TimeTicks,
   mscAppnRtpSmoothRoundTripTime
      Gauge32,
   mscAppnRtpLastRoundTripTime
      Gauge32,
   mscAppnRtpShortReqTimer
      Gauge32,
   mscAppnRtpShortReqTimeouts
      PassportCounter64,
   mscAppnRtpIdleTimeouts
      PassportCounter64,
   mscAppnRtpRxInvalidSnaFrames
      PassportCounter64,
   mscAppnRtpInSessionControlFrames
      PassportCounter64,
   mscAppnRtpOutSessionControlFrames
      PassportCounter64 }

mscAppnRtpTxBytes OBJECT-TYPE
   SYNTAX     PassportCounter64 
   ACCESS     read-only
   STATUS     mandatory
   DESCRIPTION
     "This attribute is the total number of bytes sent on this High 
      Performance Routing-Rapid Transport Protocol pipe. This counter 
      wraps to zero."
::= { mscAppnRtpStatsEntry 1 }

mscAppnRtpRxBytes OBJECT-TYPE
   SYNTAX     PassportCounter64 
   ACCESS     read-only
   STATUS     mandatory
   DESCRIPTION
     "This attribute is the total number of bytes received on this High 
      Performance Routing-Rapid Transport Protocol pipe. This counter 
      wraps to zero."
::= { mscAppnRtpStatsEntry 2 }

mscAppnRtpBytesResent OBJECT-TYPE
   SYNTAX     PassportCounter64 
   ACCESS     read-only
   STATUS     mandatory
   DESCRIPTION
     "This attribute counts the total number of bytes resent on this High 
      Performance Routing-Rapid Transport Protocol pipe. This counter 
      wraps to zero."
::= { mscAppnRtpStatsEntry 3 }

mscAppnRtpBytesDiscarded OBJECT-TYPE
   SYNTAX     PassportCounter64 
   ACCESS     read-only
   STATUS     mandatory
   DESCRIPTION
     "This attribute counts the total number of bytes discarded on this 
      High Performance Routing-Rapid Transport Protocol pipe. High 
      numbers of discarded bytes may indicate a problem in the APPN 
      network. This counter wraps to zero."
::= { mscAppnRtpStatsEntry 4 }

