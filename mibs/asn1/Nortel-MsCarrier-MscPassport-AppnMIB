   SYNTAX     Unsigned32 (0..65535)
   ACCESS     read-only
   STATUS     mandatory
   DESCRIPTION
     "This is the exchange limit for non-activation XIDs."
   DEFVAL     { 5 }
::= { mscAppnPortConfigEntry 9 }

mscAppnPortLsXmitRxCap OBJECT-TYPE
   SYNTAX     INTEGER  {
                 twa(0),
                 tws(1) }
   ACCESS     read-only
   STATUS     mandatory
   DESCRIPTION
     "This is Link Station transmit/receive capability. This is either tws 
      (two-way simultaneous (full-duplex)) or twa (two-way alternate 
      (half-duplex))."
   DEFVAL     { tws }
::= { mscAppnPortConfigEntry 10 }

