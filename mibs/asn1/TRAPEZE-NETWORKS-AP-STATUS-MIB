                    trpzApStatRadioOpStatsRxAssocPkt,
                    trpzApStatRadioOpStatsMacRxAssocPkt,
                    trpzApStatRadioOpStatsTxDataPkt,
                    trpzApStatRadioOpStatsMacTxDataPkt,
                    trpzApStatRadioOpStatsTxAuthRespPkt,
