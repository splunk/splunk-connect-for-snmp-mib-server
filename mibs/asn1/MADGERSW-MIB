----------------------------------------------------------------------------
--
-- The Madge Ringswitch MIB                             Madge Networks Ltd
--
-- Version 4.04                                               11th May 2000
--
----------------------------------------------------------------------------
--
-- Overview
-- --------
-- The Madge Ringswitch MIB defines enterprise specific objects to allow the
-- management of Madge Smart Ringswitches.
--
-- The Madge Ringswitch MIB contains the following groups:
--
--   ringswitchBase    - Ringswitch global information
--   ringswitchPort    - Ringswitch per-port information
--   ringswitchFwd     - Source routing and transparent bridging information
--   ringswitchLCD     - LCD message information
--   ringswitchLAN     - Virtual LAN information
--   ringswitchVirt    - Additional virtual LAN information
--   ringswitchSlot    - Ringswitch per slot information
--   ringswitchGenPort - Ringswitch general per-port information
--   ringswitchTR      - Ringswitch Token Ring per-interface information
--   ringswitchFDDI    - Ringswitch FDDI per-card information
--   ringswitchATM     - Ringswitch ATM per-card information
--   ringswitchPSU     - Ringswitch power supply information
--   ringswitchFan     - Ringswitch fan information
--   ringswitchTrap    - Variables used in TRAPS
--   ringswitchFilter  - Ringswitch protocol filter information
--   ringswitchTLS     - Ringswitch TLS per-card information
--   ringswitchEthernet- Ringswitch Ethernet per-card information
--
--   Traps:            - Ringswitch specific traps are at 494.4.0
--
----------------------------------------------------------------------------
	MADGERSW-MIB DEFINITIONS ::= BEGIN
	IMPORTS
		IpAddress, OBJECT-TYPE, enterprises
			FROM RFC1155-SMI
	TRAP-TYPE FROM RFC-1215;
-- Data Types
	DisplayString   ::= OCTET STRING
	--------------------------------------------------------------------
	-- Octet String encoding of a 48 bit integer, transmitted Most
	-- Significant Byte first
	--------------------------------------------------------------------
	INTEGER48 ::= OCTET STRING (SIZE (6))
	--------------------------------------------------------------------
	-- ATM address
	--------------------------------------------------------------------
	NSAP ::= OCTET STRING (SIZE(20))
	--------------------------------------------------------------------
	-- MAC addresses
	--
	-- TRNMacAddress is a 6 octet address in the "non-canonical" order,
	-- ie according to the IEEE 802.5 definition of MAC address
	--
	-- ETHMacAddress is a 6 octet address in the "canonical" order, ie
	-- according to the IEEE 802.1a definition of MAC address
	--------------------------------------------------------------------
	TRNMacAddress ::= OCTET STRING (SIZE (6))
	ETHMacAddress ::= OCTET STRING (SIZE (6))
	--------------------------------------------------------------------
	-- The state machine described by RingswitchRowStatus allows the
	-- following transitions, returning noSuchName for any other
	-- attempted set request:
	--
	-- valid ==> invalid            Use this to delete an entry from
	--                              the table.
	--
	-- valid ==> createRequest      Use this to change an entry in the
	--                              table.  The Ringswitch sets the
	--                              status to underCreation immediately.
	--
	-- invalid ==> createRequest    Use this to create an entry in the
	--                              table.  The Ringswitch sets the
	--                              status to underCreation immediately.
	--
	-- underCreation ==> valid      Use this to validate an entry in
	--                              the table; until this point, the
	--                              Ringswitch does not change its
	--                              switching behaviour.
	--
	-- underCreation ==> invalid    If the entry is not validated after
	--                              ?? seconds, the Ringswitch does this
	--                              transition automatically.
	--------------------------------------------------------------------
	RingswitchRowStatus ::= INTEGER {
					valid           (1),
					createRequest   (2),
					underCreation   (3),
					invalid         (4)
					}
-- the Ringswitch MIB
	madge                   OBJECT IDENTIFIER ::= { enterprises 494 }
	ringswitch              OBJECT IDENTIFIER ::= { madge 4 }
-- groups in the Ringswitch MIB
-- specific traps:                                { ringswitch 0 }
	ringswitchBase          OBJECT IDENTIFIER ::= { ringswitch 1 }
	ringswitchPort          OBJECT IDENTIFIER ::= { ringswitch 2 }
	ringswitchFwd           OBJECT IDENTIFIER ::= { ringswitch 3 }
	ringswitchLCD           OBJECT IDENTIFIER ::= { ringswitch 4 }
	ringswitchLAN           OBJECT IDENTIFIER ::= { ringswitch 5 }
	ringswitchVirt          OBJECT IDENTIFIER ::= { ringswitch 6 }
	ringswitchSlot          OBJECT IDENTIFIER ::= { ringswitch 7 }
	ringswitchGenPort       OBJECT IDENTIFIER ::= { ringswitch 8 }
	ringswitchTR            OBJECT IDENTIFIER ::= { ringswitch 9 }
	ringswitchFDDI          OBJECT IDENTIFIER ::= { ringswitch 10 }
	ringswitchATM           OBJECT IDENTIFIER ::= { ringswitch 11 }
	ringswitchPSU           OBJECT IDENTIFIER ::= { ringswitch 12 }
	ringswitchFan           OBJECT IDENTIFIER ::= { ringswitch 13 }
	ringswitchTrap          OBJECT IDENTIFIER ::= { ringswitch 14 }
	ringswitchFilter        OBJECT IDENTIFIER ::= { ringswitch 15 }
	ringswitchTLS	        OBJECT IDENTIFIER ::= { ringswitch 16 }
        ringswitchEthernet      OBJECT IDENTIFIER ::= { ringswitch 21 }
----------------------------------------------------------------------------
-- the ringswitchBase group
	ringswitchBasePSFanSpeed OBJECT-TYPE
		SYNTAX  INTEGER
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"The number of revolutions per minute being reported
			by the fan internal to the system power supply. If
			this value is read as zero, the fan is no longer
			operating"
		::= { ringswitchBase 1 }
	ringswitchBaseExtFanSpeed OBJECT-TYPE
		SYNTAX  INTEGER
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"The number of revolutions per minute being reported
			by the external system fan. If this value is read as
			zero, the fan is no longer operating"
		::= { ringswitchBase 2 }
	ringswitchBaseRipSapSuppression OBJECT-TYPE
		SYNTAX  INTEGER {
				enable          (1),
				disable         (2)
				}
		ACCESS  read-write
		STATUS  mandatory
		DESCRIPTION
			"If this variable is set to enable the system will
			only forward broadcast-addressed IPX RIP and SAP
			Response frames to those ports that have recently
			sourced such a frame. This reduces the number of
			gratuitous frames sent out on LANs that only have
			IPX workstations attached. A value of disable causes
			no special action to be taken for such frames."
		::= { ringswitchBase 3 }
	ringswitchBaseAREConversion OBJECT-TYPE
		SYNTAX  INTEGER {
				enable-first            (1),
				enable-all              (2),
				enable-bcast-first      (3),
				enable-bcast-all        (4),
				disable                 (5)
				}
		ACCESS  read-write
		STATUS  mandatory
		DESCRIPTION
			"This variable controls the action taken when
			forwarding All Routes Explorer frames.
			enable-first:       convert all ARE frames with a
					    RIF length of 2 to STE frames.
			enable-all:         convert all ARE frames to STE
					    frames.
			enable-bcast-first: convert all ARE frames with the
					    broadcast destination address,
					    with a RIF length of 2 to STE
					    frames.
			enable-bcast-all:   convert all ARE frames with the
					    broadcast destination address to
					    STE frames.
			disable:            follow normal forwarding rules
					    for ARE frames."
		::= { ringswitchBase 4 }
	ringswitchBaseStpMode OBJECT-TYPE
		SYNTAX  INTEGER  {
				ibm-sr          (1),
				ieee8021d       (2)
				}
		ACCESS  read-write
		STATUS  mandatory
		DESCRIPTION
			"This selects the mode of operation of the spanning
			tree protocol. When set to ibm-sr, spanning tree
			protocol BPDUs are sent to the 802.5 LBS functional
			address on all interface types, and TCN BPDUs are
			not generated.  When set to ieee8021d, TCN BPDUs are
			generated, and the IEEE 802.1D group address
			(01 80 C2 00 00 00 in canonical form) is used
			as the destination address for BPDUs on interfaces
			of types other than 802.5"
		::= { ringswitchBase 5 }
	ringswitchBaseRMONState OBJECT-TYPE
		SYNTAX  INTEGER {
				enable          (1),
				disable         (2)
				}
		ACCESS  read-write
		STATUS  mandatory
		DESCRIPTION
			"Setting this to 'enable' activates the RMON
			probe function in the Ringswitch. When RMON
			has been enabled it is configured according to
			ringswitchBaseRMONMirrorPort,
			ringswitchPortRMONMode and ringswitchPortRMONSnoop
			mib entries.
			Setting it to 'disable' de-activates the RMON probe
			function. When RMON is disabled, all other RMON
			related mib entries defined by the Madge Ringwitch
			MIB will be ignored (but their values will still
			be settable). ringswitchBaseRMONState defaults to
			'disable'."
		::= { ringswitchBase 6 }
	ringswitchBaseBackPlaneType OBJECT-TYPE
		SYNTAX  INTEGER {
				original          (1),
				dual-psu          (2),
				express           (3)
				}
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"This variable reports the type of chassis that
			the Ringswitch is mounted in."
	::= { ringswitchBase 7 }
	ringswitchBaseRMONMirrorPort OBJECT-TYPE
		SYNTAX  INTEGER
		ACCESS  read-write
		STATUS  mandatory
		DESCRIPTION
			"This variable controls the RMON mirroring facility.
			A value of 0 means mirroring is disabled.
			Mirroring is enabled by setting this variable to
			the ifNumber of the port to which mirroring will
			apply and by setting ringswitchBaseRMONState to
			'enable'.
			When mirroring is enabled, the Ringswitch sends
			all RMON related traffic to the mirror port (except
			for MAC frames)."
		::= { ringswitchBase 8 }
	ringswitchBaseTotalResetCounters OBJECT-TYPE
		SYNTAX  INTEGER {
				reset   (1)
				}
		ACCESS  read-write
		STATUS  mandatory
		DESCRIPTION
			"Use this variable to reset all the counters
			on the Ringswitch, including those in the
			ringswitchGenPort, dot1dSRPort, ifEntry and
			fddimib groups."
		::= { ringswitchBase 9 }
	ringswitchBaseDownloadMode OBJECT-TYPE
		SYNTAX  INTEGER {
				normal (1),
				active (2)
				}
		ACCESS  read-write
		STATUS  mandatory
		DESCRIPTION
			"Use this variable to select the mode for all 
			 subsequent download operations.
			 normal - device will enter the loader prior to 
				  downloading.
			 active - device will NOT enter the loader and 
				  perform the download online."
		::= { ringswitchBase 10 }
----------------------------------------------------------------------------
-- the ringswitchPort group
	ringswitchPortTable OBJECT-TYPE
		SYNTAX  SEQUENCE OF RingswitchPortEntry
		ACCESS  not-accessible
		STATUS  mandatory
		DESCRIPTION
			"A table that contains general port information."
		::= { ringswitchPort 1 }
	ringswitchPortEntry OBJECT-TYPE
		SYNTAX  RingswitchPortEntry
		ACCESS  not-accessible
		STATUS  mandatory
		DESCRIPTION
			""
		INDEX { ringswitchPortNum }
		::= { ringswitchPortTable 1 }
	RingswitchPortEntry ::=
		SEQUENCE        {
				ringswitchPortNum
					INTEGER,
				ringswitchPortRingStatus
					INTEGER,
				ringswitchPortAdapterStatus
					INTEGER,
				ringswitchPortMediaType
					INTEGER,
				ringswitchPortIfMode
					INTEGER,
				ringswitchPortRingSpeed
					INTEGER,
				ringswitchPortTestState
					INTEGER,
				ringswitchPortTestError
					INTEGER,
				ringswitchPortTestPhase
					INTEGER,
				ringswitchPortSummary
					INTEGER,
				ringswitchPortAddress
					TRNMacAddress,
				ringswitchPortLAA
					TRNMacAddress,
				ringswitchPortStationType
					INTEGER,
				ringswitchPortRPSEnable
					INTEGER,
				ringswitchPortCutThruEnable
					INTEGER,
				ringswitchPortInOctets
					INTEGER48,
				ringswitchPortOutOctets
					INTEGER48,
				ringswitchPortSpecInFrames
					INTEGER48,
				ringswitchPortSpecOutFrames
					INTEGER48,
				ringswitchPortApeInFrames
					INTEGER48,
				ringswitchPortApeOutFrames
					INTEGER48,
				ringswitchPortSteInFrames
					INTEGER48,
				ringswitchPortSteOutFrames
					INTEGER48,
				ringswitchPortResetCounters
					INTEGER,
				ringswitchPortFixupMode
					INTEGER,
				ringswitchPortForwardMode
					INTEGER,
				ringswitchPortRMONCapabilities
					INTEGER,
				ringswitchPortRMONMode
					INTEGER,
				ringswitchPortRMONSnoop
					TRNMacAddress,
				ringswitchPortTransparentInFrames
					INTEGER48,
				ringswitchPortTransparentOutFrames
					INTEGER48,
				ringswitchPortTransparentDiscFrames
					INTEGER48,
				ringswitchPortLEDs
					OCTET STRING
				}
	ringswitchPortNum OBJECT-TYPE
		SYNTAX  INTEGER (1..65535)
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"The port number of the port for which this entry
			contains Ringswitch management information."
		::= { ringswitchPortEntry 1 }
	ringswitchPortRingStatus OBJECT-TYPE
		SYNTAX  INTEGER {
				normal          (1),
				single          (2),
				beaconing       (3),
				normal-fdx      (4),
				dtr-wait        (5)
				}
		ACCESS  read-only       
		STATUS  mandatory
		DESCRIPTION
			"The status of the ring to which this port is
			connected.
			normal:     Inserted into Classic ring
			single:     Single Station
			beaconing:  Beaconing Detected
			normal-fdx: DTR link in full duplex (TXI) mode
			dtr-wait:   DTR Concentrator port waiting for
				    attached station to insert"
		::= { ringswitchPortEntry 2 }
	ringswitchPortAdapterStatus OBJECT-TYPE
		SYNTAX  INTEGER {
				open    (1),
				closed  (2),
				opening (3)
				}
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"The status of the adapter used by this port."
		::= { ringswitchPortEntry 3 }
	ringswitchPortMediaType OBJECT-TYPE
		SYNTAX  INTEGER {
				tr-copper       (1),
				tr-fiber        (2),
				fddi-fiber      (3),
				absent          (4)
				}
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"Media type of the port."
		::= { ringswitchPortEntry 4 }
	ringswitchPortIfMode OBJECT-TYPE
		SYNTAX  INTEGER {
				node            (1),
				concentrator    (2),
				node-fdx        (3),
				conc-fdx        (4),
				node-classic    (5),
				conc-classic    (6),
				node-cau-rio    (7)
				}
		ACCESS  read-write
		STATUS  mandatory
		DESCRIPTION
			"Node or concentrator status of the port."
		::= { ringswitchPortEntry 5 }
	ringswitchPortRingSpeed OBJECT-TYPE
		SYNTAX  INTEGER {
				four    (1),
				sixteen (2),
				hundred (3)
				}
		ACCESS  read-write
		STATUS  mandatory
		DESCRIPTION
			"Speed of the ring to which this port is connected."
		::= { ringswitchPortEntry 6 }
	ringswitchPortTestState OBJECT-TYPE
		SYNTAX  INTEGER {
				disabled        (1),
				running         (2),
				failed          (3),
				ok              (4),
				unknown         (5)
				}
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"Current state of the bridge test."
		::= { ringswitchPortEntry 7 }
	ringswitchPortTestError OBJECT-TYPE
		SYNTAX  INTEGER {
				no-error        (1),
				same-ring       (2),
				duplicate-ring  (3),
				fail-nb         (4),
				bad-rnum        (5),
				fail-b          (6)
				}
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"Result of the bridge test."
		::= { ringswitchPortEntry 8 }
	ringswitchPortTestPhase OBJECT-TYPE
		SYNTAX  INTEGER {
				not-running     (1),
				same-ring       (2),
				routed          (3),
				broadcast       (4),
				success         (5)
				}
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"Current phase of the bridge test."
		::= { ringswitchPortEntry 9 }
	ringswitchPortSummary OBJECT-TYPE
		SYNTAX  INTEGER
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"This is a four octet field which packs some of the
			port attributes into a single mib entry. Each nibble
			represents a different attribute of the port:
				ifAdminStatus (most significant),
				ringswitchPortRingStatus,
				ringswitchPortAdapterStatus,
				ringswitchPortMediaType,
				ringswitchPortIfMode,
				ringswitchPortRingSpeed,
				ringswitchPortTestState,
				ringswitchPortTestError (least significant)"
		::= { ringswitchPortEntry 10 }
	ringswitchPortAddress OBJECT-TYPE
		SYNTAX  TRNMacAddress
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"The hard wired address of the port."
		::= { ringswitchPortEntry 11 }
	ringswitchPortLAA OBJECT-TYPE
		SYNTAX  TRNMacAddress
		ACCESS  read-write
		STATUS  mandatory
		DESCRIPTION
			"The locally administered address of the port."
		::= { ringswitchPortEntry 12 }
	ringswitchPortStationType OBJECT-TYPE
		SYNTAX  INTEGER {
				anything        (1),
				workstations    (2)
				}
		ACCESS  read-write
		STATUS  mandatory
		DESCRIPTION
			"Workstations-only status of the port; if this is
			limited, the port only has workstations on it, and
			broadcast frames received on this port will not be
			transmitted on other limited ports."
		::= { ringswitchPortEntry 13 }
	ringswitchPortRPSEnable OBJECT-TYPE
		SYNTAX  INTEGER {
				enable          (1),
				disable         (2)
				}
		ACCESS  read-write
		STATUS  mandatory
		DESCRIPTION
			"Enables or disables the Ring Parameter Server
			function on the port. If this variable is set to
			enabled, the port will enable the RPS functional
			address, and respond to Request Parameters MAC
			frames, supplying the port's segment number in
			the response."
		::= { ringswitchPortEntry 14 }
	ringswitchPortCutThruEnable OBJECT-TYPE
		SYNTAX  INTEGER {
				enable          (1),
				disable         (2)
				}
		ACCESS  read-write
		STATUS  mandatory
		DESCRIPTION
			"Enables or disables cut-through switching on the
			port. In some circumstances (e.g., 4 to 16 MB
			switching), cut-through will always be disabled,
			irrespective of the state of this variable."
		::= { ringswitchPortEntry 15 }
	ringswitchPortInOctets OBJECT-TYPE
		SYNTAX  INTEGER48
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"48 bit version of the interface MIB counter
			ifInOctets."
		::= { ringswitchPortEntry 16 }
	ringswitchPortOutOctets OBJECT-TYPE
		SYNTAX  INTEGER48
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"48 bit version of the interface MIB counter
			ifOutOctets."
		::= { ringswitchPortEntry 17 }
	ringswitchPortSpecInFrames OBJECT-TYPE
		SYNTAX  INTEGER48
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"48 bit version of the Bridge MIB counter
			srPortSpecInFrames."
		::= { ringswitchPortEntry 18 }
	ringswitchPortSpecOutFrames OBJECT-TYPE
		SYNTAX  INTEGER48
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"48 bit version of the Bridge MIB counter
			srPortSpecOutFrames."
		::= { ringswitchPortEntry 19 }
	ringswitchPortApeInFrames OBJECT-TYPE
		SYNTAX  INTEGER48
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"48 bit version of the Bridge MIB counter
			srPortApeInFrames."
		::= { ringswitchPortEntry 20 }
	ringswitchPortApeOutFrames OBJECT-TYPE
		SYNTAX  INTEGER48
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"48 bit version of the Bridge MIB counter
			srPortApeOutFrames."
		::= { ringswitchPortEntry 21 }
	ringswitchPortSteInFrames OBJECT-TYPE
		SYNTAX  INTEGER48
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"48 bit version of the Bridge MIB counter
			srPortSteInFrames."
		::= { ringswitchPortEntry 22 }
	ringswitchPortSteOutFrames OBJECT-TYPE
		SYNTAX  INTEGER48
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"48 bit version of the Bridge MIB counter
			srPortSteOutFrames."
		::= { ringswitchPortEntry 23 }
	ringswitchPortResetCounters OBJECT-TYPE
		SYNTAX  INTEGER {
				reset   (1)
				}
		ACCESS  read-write
		STATUS  mandatory
		DESCRIPTION
			"Use this variable to reset all the counters on
			this port, including those in the ringswitchPort,
			dot1dSRPort, ifEntry and fddimib groups."
		::= { ringswitchPortEntry 24 }
	ringswitchPortFixupMode OBJECT-TYPE
		SYNTAX  INTEGER {
				disabled    (1),
				fddi-all    (2)
				}
		ACCESS  read-write
		STATUS  mandatory
		DESCRIPTION
			"This MIB variable controls operation of protocol
			fixups on an FDDI linecard. When set to fddi-all,
			embedded MAC addresses in frames are bit-reversed to
			convert the frames into native FDDI format"
		::= { ringswitchPortEntry 25 }
	ringswitchPortForwardMode OBJECT-TYPE
		SYNTAX  INTEGER {
				sr-only (1),
				tb-only (2),
				sr-tb   (3)
				}
		ACCESS  read-write
		STATUS  mandatory
		DESCRIPTION
			"This MIB variable controls the bridging modes that
			are enabled on this port. Any combination of the
			two types of bridging - transparent and source
			routing can be enabled/disabled."
		::= { ringswitchPortEntry 26 }
	ringswitchPortRMONCapabilities OBJECT-TYPE
		SYNTAX  INTEGER  {
				disabled      (1),
				economy       (2),
				standard      (3),
				snooping      (4)
				}
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"A value that indicates the RMON capabilities of the
			port. The value represents the highest level RMON
			mode which the port can support.
			  'disabled' is the lowest level and indicates
				     no RMON capabilities
			  'snooping' is the highest level and indicates
				     that the port is capable of
				     running in all RMON modes"
		::= { ringswitchPortEntry 27 }
	ringswitchPortRMONMode OBJECT-TYPE
		SYNTAX  INTEGER {
				disabled      (1),
				economy       (2),
				standard      (3),
				snooping      (4)
				}
		ACCESS  read-write
		STATUS  mandatory
		DESCRIPTION
			"Controls the types of frames passed to the RMON
			probe function for processing as follows:
			disabled - no frames passed to the RMON probe
			economy  - MAC frames required for token ring
				   specific groups as follows:
				   MLStats table
				   Token Ring Ring station table
			standard - all frames
			snooping - all frames provided by the 'economy'
				   mode plus any frames originating
				   from or destined for the 'snoop address'
				   controlled by ringswitchPortRMONSnoop"
		::= { ringswitchPortEntry 28 }
	ringswitchPortRMONSnoop OBJECT-TYPE
		SYNTAX  TRNMacAddress
		ACCESS  read-write
		STATUS  mandatory
		DESCRIPTION
			"This controls the RMON snoop address used when
			ringswitchPortRMONMode is set to 'snooping'.
			A value of all zeroes results in no snooping
			taking place."
		::= { ringswitchPortEntry 29 }
	ringswitchPortTransparentInFrames OBJECT-TYPE
		SYNTAX  INTEGER48
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"48 bit version of the Bridge MIB counter
			TpPortInFrames."
		::= { ringswitchPortEntry 30 }
	ringswitchPortTransparentOutFrames OBJECT-TYPE
		SYNTAX  INTEGER48
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"48 bit version of the Bridge MIB counter
			TpPortOutFrames."
		::= { ringswitchPortEntry 31 }
	ringswitchPortTransparentDiscFrames OBJECT-TYPE
		SYNTAX  INTEGER48
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"48 bit version of the Bridge MIB counter
			TpPortInDiscards."
		::= { ringswitchPortEntry 32 }
	ringswitchPortLEDs OBJECT-TYPE
		SYNTAX  OCTET STRING
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"An octet string containing the colour of every LED
			on the port, one per octet:
				off             	(1),
				on-red          	(2),
				on-green        	(3),
				on-yellow       	(4),
				flash-red       	(5),
				flash-green     	(6),
				flash-yellow    	(7),
				flash-blue      	(8),
				flash-green-anti-phase	(9),
				flash-yellow-anti-phase	(10)"
		::= { ringswitchPortEntry 33 }
----------------------------------------------------------------------------
-- the ringswitchFwd group
	ringswitchFwdAdminState OBJECT-TYPE
		SYNTAX  INTEGER {
				sr-only         (1),
				disable         (2),
				tb-only         (3),
				srt             (4),
				srt-plus        (5)
				}
		ACCESS  read-write
		STATUS  mandatory
		DESCRIPTION
			"The desired state of the switches forwarding
			modes. This allows source routing and/or transparent
			bridging to be enabled/disabled for the whole
			Ringswitch"
		::= { ringswitchFwd 1 }
	ringswitchFwdOperState OBJECT-TYPE
		SYNTAX  INTEGER {
				sr-only         (1),
				inactive        (2),
				tb-only         (3),
				srt             (4),
				srt-plus        (5)
				}
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"The actual state of switches forwarding modes.
			If for some reason the switch is unable operate in
			the mode requested then the operating state may
			differ from the AdminState."
		::= { ringswitchFwd 2 }

	-- the Forwarding table

	ringswitchFwdPortTable OBJECT-TYPE
		SYNTAX  SEQUENCE OF RingswitchFwdPortEntry
		ACCESS  not-accessible
		STATUS  mandatory
		DESCRIPTION
			"A table that contains forwarding information about
			every port that is associated with this Ringswitch.
			Transparent, source-route, srt and srt plus ports
			are included."
		::= { ringswitchFwd 3 }
	ringswitchFwdPortEntry OBJECT-TYPE
		SYNTAX  RingswitchFwdPortEntry
		ACCESS  not-accessible
		STATUS  mandatory
		DESCRIPTION
			"A list of forwarding information for each port of
			the Ringswitch."
		INDEX { ringswitchFwdPort }
		::= { ringswitchFwdPortTable 1 }

	RingswitchFwdPortEntry ::=
			SEQUENCE        {
					ringswitchFwdPort
						INTEGER,
					ringswitchFwdPortMode
						INTEGER,
					ringswitchFwdPortStationType
						INTEGER,
					ringswitchFwdPortCutThruEnable
						INTEGER,
					ringswitchFwdPortSpecInFrames
						INTEGER48,
					ringswitchFwdPortSpecOutFrames
						INTEGER48,
					ringswitchFwdPortApeInFrames
						INTEGER48,
					ringswitchFwdPortApeOutFrames
						INTEGER48,
					ringswitchFwdPortSteInFrames
						INTEGER48,
					ringswitchFwdPortSteOutFrames
						INTEGER48,
					ringswitchFwdPortTransparentInFrames
						INTEGER48,
					ringswitchFwdPortTransparentOutFrames
						INTEGER48,
					ringswitchFwdPortTransparentDiscFrames
						INTEGER48,
					ringswitchFwdPortTestState
						INTEGER,
					ringswitchFwdPortTestError
						INTEGER,
					ringswitchFwdPortTestPhase
						INTEGER,
					ringswitchFwdPortTbForce
						INTEGER,
					ringswitchFwdPortStpMaster
						INTEGER,
					ringswitchFwdPortTbState
						INTEGER,
					ringswitchFwdPortSteState
						INTEGER,
					ringswitchFwdPortTbUnicastInFrames
						INTEGER48,
					ringswitchFwdPortTbUnicastOutFrames
						INTEGER48,
					ringswitchFwdPortTbBroadcastInFrames
						INTEGER48,
					ringswitchFwdPortTbBroadcastOutFrames
						INTEGER48,
					ringswitchFwdPortTbMulticastInFrames
						INTEGER48,
					ringswitchFwdPortTbMulticastOutFrames
						INTEGER48,
					ringswitchFwdPortTbMiscInFrames
						INTEGER48,
					ringswitchFwdPortTbMiscOutFrames
						INTEGER48,
					ringswitchFwdPortMatrixSrSpecFrames
						OCTET STRING,
					ringswitchFwdPortMatrixTbUniFrames
						OCTET STRING,
					ringswitchFwdPortEthernetSRBlk
						INTEGER,
					ringswitchFwdPortAutoPathCost
						INTEGER,
                    ringswitchFwdPortBroadcastReflect
	                    INTEGER
					}
	ringswitchFwdPort OBJECT-TYPE
		SYNTAX  INTEGER (1..65535)
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"The port number of the port for which this entry
			contains forwarding information. This value is the
			same as the value of dot1dBasePort for the equivalent
			port."
		::= { ringswitchFwdPortEntry 1 }
	ringswitchFwdPortMode OBJECT-TYPE
		SYNTAX  INTEGER {
				sr-only     (1),
				tb-only     (2),
				sr-tb       (3)
				}
		ACCESS  read-write
		STATUS  mandatory
		DESCRIPTION
			"This MIB variable controls the bridging modes that
			are enabled on this port. Any combination of the
			two types of bridging - transparent and source
			routing can be enabled/disabled."
		::= { ringswitchFwdPortEntry 2 }
	ringswitchFwdPortStationType OBJECT-TYPE
		SYNTAX  INTEGER {
				anything        (1),
				workstations    (2)
				}
		ACCESS  read-write
		STATUS  mandatory
		DESCRIPTION
			"Workstations-only status of the port; if this is
			limited, the port only has workstations on it, and
			broadcast frames received on this port will not be
			transmitted on other limited ports."
		::= { ringswitchFwdPortEntry 3 }
	ringswitchFwdPortCutThruEnable OBJECT-TYPE
		SYNTAX  INTEGER {
				enable          (1),
				disable         (2)
				}
		ACCESS  read-write
		STATUS  mandatory
		DESCRIPTION
			"Enables or disables cut-through switching on the
			port. In some circumstances (e.g., 4 to 16 MB
			switching), cut-through will always be disabled,
			irrespective of the state of this variable."
		::= { ringswitchFwdPortEntry 4 }
	ringswitchFwdPortSpecInFrames OBJECT-TYPE
		SYNTAX  INTEGER48
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"48 bit version of the Bridge MIB counter
			srPortSpecInFrames."
		::= { ringswitchFwdPortEntry 5 }
	ringswitchFwdPortSpecOutFrames OBJECT-TYPE
		SYNTAX  INTEGER48
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"48 bit version of the Bridge MIB counter
			srPortSpecOutFrames."
		::= { ringswitchFwdPortEntry 6 }
	ringswitchFwdPortApeInFrames OBJECT-TYPE
		SYNTAX  INTEGER48
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"48 bit version of the Bridge MIB counter
			srPortApeInFrames."
		::= { ringswitchFwdPortEntry 7 }
	ringswitchFwdPortApeOutFrames OBJECT-TYPE
		SYNTAX  INTEGER48
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"48 bit version of the Bridge MIB counter
			srPortApeOutFrames."
		::= { ringswitchFwdPortEntry 8 }
	ringswitchFwdPortSteInFrames OBJECT-TYPE
		SYNTAX  INTEGER48
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"48 bit version of the Bridge MIB counter
			srPortSteInFrames."
		::= { ringswitchFwdPortEntry 9 }
	ringswitchFwdPortSteOutFrames OBJECT-TYPE
		SYNTAX  INTEGER48
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"48 bit version of the Bridge MIB counter
			srPortSteOutFrames."
		::= { ringswitchFwdPortEntry 10 }
	ringswitchFwdPortTransparentInFrames OBJECT-TYPE
		SYNTAX  INTEGER48
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"48 bit version of the Bridge MIB counter
			TpPortInFrames."
		::= { ringswitchFwdPortEntry 11 }
	ringswitchFwdPortTransparentOutFrames OBJECT-TYPE
		SYNTAX  INTEGER48
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"48 bit version of the Bridge MIB counter
			TpPortOutFrames."
		::= { ringswitchFwdPortEntry 12 }
	ringswitchFwdPortTransparentDiscFrames OBJECT-TYPE
		SYNTAX  INTEGER48
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"48 bit version of the Bridge MIB counter
			TpPortInDiscards."
		::= { ringswitchFwdPortEntry 13 }
	ringswitchFwdPortTestState OBJECT-TYPE
		SYNTAX  INTEGER {
				disabled        (1),
				running         (2),
				failed          (3),
				ok              (4),
				unknown         (5),
				ok-use-spt      (6)
				}
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"Current state of the bridge test."
		::= { ringswitchFwdPortEntry 14 }
	ringswitchFwdPortTestError OBJECT-TYPE
		SYNTAX  INTEGER {
				no-error        (1),
				same-ring       (2),
				duplicate-ring  (3),
				fail-nb         (4),
				bad-rnum        (5),
				fail-b          (6)
				}
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"Result of the bridge test."
		::= { ringswitchFwdPortEntry 15 }
	ringswitchFwdPortTestPhase OBJECT-TYPE
		SYNTAX  INTEGER {
				not-running     (1),
				same-ring       (2),
				routed          (3),
				broadcast       (4),
				success         (5)
				}
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"Current phase of the bridge test."
		::= { ringswitchFwdPortEntry 16 }
	ringswitchFwdPortTbForce OBJECT-TYPE
		SYNTAX  INTEGER {
				enable          (1),
				disable         (2)
				}
		ACCESS  read-write
		STATUS  mandatory
		DESCRIPTION
			"If this variable is set to enable, transparent
			forwarding will be enabled, regardless of the
			spanning tree state machine."
		::= { ringswitchFwdPortEntry 17 }
	ringswitchFwdPortStpMaster OBJECT-TYPE
		SYNTAX  INTEGER
		ACCESS  read-write
		STATUS  mandatory
		DESCRIPTION
			"If this variable is non-zero, it is the interface
			number of another port, and the spanning tree state
			machine will be slaved to that port. A port may not
			be slaved to itself, to any other slaved port, or
			to a port where transparent bridging is forced (see
			description of ringswitchFwdPortTbForce)."
		::= { ringswitchFwdPortEntry 18 }
	ringswitchFwdPortTbState OBJECT-TYPE
		SYNTAX  INTEGER {
				disabled        (1),
				blocking        (2),
				listening       (3),
				learning        (4),
				forwarding      (5),
				broken          (6)
				}
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"The current transparent bridging state."
		::= { ringswitchFwdPortEntry 19 }
	ringswitchFwdPortSteState OBJECT-TYPE
		SYNTAX  INTEGER {
				enabled         (1),
				disabled        (2)
				}
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"The current spanning tree explorer propagation
			state."
		::= { ringswitchFwdPortEntry 20 }
	ringswitchFwdPortTbUnicastInFrames OBJECT-TYPE
		SYNTAX  INTEGER48
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"The number of unicast frames received by this port
			and transparently forwarded to another port."
		::= { ringswitchFwdPortEntry 21 }
	ringswitchFwdPortTbUnicastOutFrames OBJECT-TYPE
		SYNTAX  INTEGER48
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"The number of unicast frames transmitted by this port
			having been transparently forwarded from another
			port."
		::= { ringswitchFwdPortEntry 22 }
	ringswitchFwdPortTbBroadcastInFrames OBJECT-TYPE
		SYNTAX  INTEGER48
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"The number of broadcast frames received by this port
			and transparently forwarded to other ports."
		::= { ringswitchFwdPortEntry 23 }
	ringswitchFwdPortTbBroadcastOutFrames OBJECT-TYPE
		SYNTAX  INTEGER48
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"The number of broadcast frames transmitted by this
			port having been transparently forwarded from another
			port."
		::= { ringswitchFwdPortEntry 24 }
	ringswitchFwdPortTbMulticastInFrames OBJECT-TYPE
		SYNTAX  INTEGER48
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"The number of multicast frames received by this port
			and transparently forwarded to other ports."
		::= { ringswitchFwdPortEntry 25 }
	ringswitchFwdPortTbMulticastOutFrames OBJECT-TYPE
		SYNTAX  INTEGER48
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"The number of multicast frames transmitted by this
			port having been transparently forwarded from another
			port."
		::= { ringswitchFwdPortEntry 26 }
	ringswitchFwdPortTbMiscInFrames OBJECT-TYPE
		SYNTAX  INTEGER48
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"The number of unicast frames received by this port
			and transparently forwarded (flooded) to other ports."
		::= { ringswitchFwdPortEntry 27 }
	ringswitchFwdPortTbMiscOutFrames OBJECT-TYPE
		SYNTAX  INTEGER48
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"The number of unicast frames transmitted by this
			port having been transparently forwarded (flooded) 
			from another port."
		::= { ringswitchFwdPortEntry 28 }
	ringswitchFwdPortMatrixSrSpecFrames OBJECT-TYPE
		SYNTAX  OCTET STRING
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"This string contains a count of the specifically
			routed frames that have been switched from port
			ringswitchFwdPortNum to each of the possible
			ports on the ringswitch.
			The string contains one four byte entry (MSB first)
			per port."
		::= { ringswitchFwdPortEntry 29 }
	ringswitchFwdPortMatrixTbUniFrames OBJECT-TYPE
		SYNTAX  OCTET STRING
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"This string contains a count of the transparent
			unicast frames that have been switched from port
			ringswitchFwdPortNum to each of the possible
			ports on the ringswitch.
			The string contains one four byte entry (MSB first)
			per port."
		::= { ringswitchFwdPortEntry 30 }
	ringswitchFwdPortEthernetSRBlk OBJECT-TYPE
		SYNTAX  INTEGER {
				enabled         (1),
				disabled        (2)
				}
		ACCESS  read-write
		STATUS  mandatory
		DESCRIPTION
			"If this variable is enabled, on an Ethernet LEC 
			port, then the switch will treat any SR frame (STE, 
			ARE or SRF) going to or from this port as an STE for 
			the purposes of deciding whether to forward it or 
			block it.  If it is disabled then the normal SR rules 
			apply.  It is ignored for non Ethernet-LEC ports."
		::= { ringswitchFwdPortEntry 31 }
       ringswitchFwdPortAutoPathCost OBJECT-TYPE
	       SYNTAX   INTEGER {
				enabled         (1),
				disabled        (2)
				}
	       ACCESS   read-write
	       STATUS   mandatory
	       DESCRIPTION
		       "If this variable is enabled on a switch running
		       microcode >= R3.3 then the spanning tree cost for
		       this port will be selected automatically."
		::= { ringswitchFwdPortEntry 32 }
       ringswitchFwdPortBroadcastReflect OBJECT-TYPE
	       SYNTAX   INTEGER {
				enabled         (1),
				disabled        (2)
				}
	       ACCESS   read-write
	       STATUS   mandatory
	       DESCRIPTION
                      "If this variable is enabled on a switch running
                       microcode >= v4.08.01 then, on a DTR link, broadcast
                       frames will be reflected back to the node."
		::= { ringswitchFwdPortEntry 33 }
-- Global forwarding counters
	ringswitchFwdTotalRxFrames OBJECT-TYPE
		SYNTAX  INTEGER48
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"The total number of received frames switched."
		::= { ringswitchFwd 4 }
	ringswitchFwdTotalTxFrames OBJECT-TYPE
		SYNTAX  INTEGER48
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"The total number of transmitted frames switched."
		::= { ringswitchFwd 5 }
	ringswitchFwdBridgeNum OBJECT-TYPE
		SYNTAX  INTEGER (0..15)
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"The global bridge number of the Ringswitch."
		::= { ringswitchFwd 6 }
	ringswitchFwdTbForced OBJECT-TYPE
		SYNTAX  OCTET STRING
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"An octet string containing the transparent
			force state of every port on the Ringswitch. See
			the description of ringswitchFwdPortTbForce for
			details."
		::= { ringswitchFwd 7 }
	ringswitchFwdStpMasters OBJECT-TYPE
		SYNTAX  OCTET STRING
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"An octet string containing one octet for each
			port on the Ringswitch. If an octet is non-zero,
			it is the interface number of another port, and
			the spanning tree state machine is slaved to that
			port. See description of ringswitchFwdPortStpMaster
			for details."
		::= { ringswitchFwd 8 }
	ringswitchFwdGlobalHopLimit OBJECT-TYPE
		SYNTAX  INTEGER (1..13)
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"The maximum number of source routing hops that
			the Ringswitch will forward on any port. This limit
			is applied to all types of source route frame
			including all routes explorer frames where it
			modifies the port hop count limits defined by
			dot1dSrPortHopCount."
		::= { ringswitchFwd 9 }
	ringswitchFwdTotalBadFrames OBJECT-TYPE
		SYNTAX  INTEGER48
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"The total number of incoming frames discarded 
			because of some error. This is the sum of the
			SR bridge error counters and CRC errors."
		::= { ringswitchFwd 10 }
	ringswitchFwdTotalSpecInFrames OBJECT-TYPE
		SYNTAX  INTEGER48
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"This is the sum of the dot1dSrPortSpecInFrames
