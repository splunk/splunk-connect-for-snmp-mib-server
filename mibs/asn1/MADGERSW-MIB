-- the Slot group
	ringswitchSlotNumSlots OBJECT-TYPE
		SYNTAX  INTEGER
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"The number of slots available in this Ringswitch."
		::= { ringswitchSlot 1 }
	ringswitchSlotTable OBJECT-TYPE
		SYNTAX  SEQUENCE OF RingswitchSlotEntry
		ACCESS  not-accessible
		STATUS  mandatory
		DESCRIPTION
			"A table that contains information about each slot
			in the Ringswitch."
		::= { ringswitchSlot 2 }
	ringswitchSlotEntry OBJECT-TYPE
		SYNTAX  RingswitchSlotEntry
		ACCESS  not-accessible
		STATUS  mandatory
		DESCRIPTION ""
		INDEX { ringswitchSlotIndex }
		::= { ringswitchSlotTable 1 }
	RingswitchSlotEntry ::=
		SEQUENCE        {
				ringswitchSlotIndex           INTEGER,
				ringswitchSlotStatus          INTEGER,
				ringswitchSlotBasePort        INTEGER,
				ringswitchSlotCardType        INTEGER,
				ringswitchSlotCardId          INTEGER,
				ringswitchSlotCardRevision    INTEGER,
				ringswitchSlotCardDescription DisplayString,
				ringswitchSlotCardNumPorts    INTEGER,
				ringswitchSlotCardLAN         INTEGER,
				ringswitchSlotCardLEDs        OCTET STRING,
				ringswitchSlotIfAdminStatus   OCTET STRING,
				ringswitchSlotAdapterStatus   OCTET STRING,
				ringswitchSlotTestError       OCTET STRING,
				ringswitchSlotPortHealth      OCTET STRING,
				ringswitchSlotLastStatus      INTEGER,
				ringswitchSlotPortCreated     OCTET STRING,
				ringswitchSlotHealth          INTEGER,
                                ringswitchSlotVersion         INTEGER
				}
	ringswitchSlotIndex OBJECT-TYPE
		SYNTAX  INTEGER (1..65535)
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"The value of this object identifies the slot
			for which this entry contains management
			information."
		::= { ringswitchSlotEntry 1 }
	ringswitchSlotStatus OBJECT-TYPE
		SYNTAX  INTEGER {
				invalid         (1),
				full-support    (2),
				no-port-open    (3),
				no-port-fwd     (4)
				}
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"The value of this variable indicates whether or not
			this slot supports the card that is currently placed
			in it:
			invalid      - card not supported
			full-support - card fully supported
			no-port-open - cannot open ports on card
			no-port-fwd  - cannot forward frames."
		::= { ringswitchSlotEntry 2 }
	ringswitchSlotBasePort OBJECT-TYPE
		SYNTAX  INTEGER
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"The port number of the first port available
			in this slot."
		::= { ringswitchSlotEntry 3 }
	ringswitchSlotCardType OBJECT-TYPE
		SYNTAX  INTEGER {
				tr-copper-4        (1),
				tr-fiber-4         (2),
				fddi-fiber         (3),
				absent             (4),
				unknown            (5),
				tr-copper-8        (6),
				atm-fiber          (7),
				tr-fiber-8         (8),
				group-switch       (9),
				tr-copper-al-4     (10),
				tr-copper-hstr-2   (11),
				tr-copper-hstr-4   (12),
				tr-fiber-hstr-2    (13),
				tr-fiber-hstr-4    (14),
                                three-layer-switch (15),
				tr-copper-hstr-8   (16),
				tr-fiber-hstr-8    (17),
				ethernet           (18),
				gigabit-ethernet   (19)
				}
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"The type of card in this slot."
		::= { ringswitchSlotEntry 4 }
	ringswitchSlotCardId OBJECT-TYPE
		SYNTAX  INTEGER (0..255)
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"The hardware ID number for the card in this slot."
		::= { ringswitchSlotEntry 5 }
	ringswitchSlotCardRevision OBJECT-TYPE
		SYNTAX  INTEGER (0..255)
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"The hardware revision number for the card in this
			slot."
		::= { ringswitchSlotEntry 6 }
	ringswitchSlotCardDescription OBJECT-TYPE
		SYNTAX  DisplayString (SIZE (0..64))
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"A string containing a textual description for
			the card."
		::= { ringswitchSlotEntry 7 }
	ringswitchSlotCardNumPorts OBJECT-TYPE
		SYNTAX  INTEGER
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"The actual number of ports provided by this card
			when fully configured in this slot."
		::= { ringswitchSlotEntry 8 }
	ringswitchSlotCardLAN OBJECT-TYPE
		SYNTAX  INTEGER {
                                none            (1),
                                token-ring      (2),
                                fddi            (3),
                                atm             (4),
                                tls             (5),
                                ethernet        (6),
                                gigabit         (7)
				}
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"The type of LAN supported by the card in this slot."
		::= { ringswitchSlotEntry 9 }
	ringswitchSlotCardLEDs OBJECT-TYPE
		SYNTAX  OCTET STRING
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"An octet string containing the colour of every LED
			on the card, one per octet:
				off             	(1),
				on-red          	(2),
				on-green        	(3),
				on-yellow       	(4),
				flash-red       	(5),
				flash-green     	(6),
				flash-yellow    	(7),
				flash-blue      	(8),
				flash-green-anti-phase  (9),
				flash-yellow-anti-phase (10)"
		::= { ringswitchSlotEntry 10 }
	ringswitchSlotIfAdminStatus OBJECT-TYPE
		SYNTAX  OCTET STRING
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"An octet string containing the ifAdminStatus of
			every interface on the card, one per octet:
				up      (1),
				down    (2),
				testing (3)"
		::= { ringswitchSlotEntry 11 }
	ringswitchSlotAdapterStatus OBJECT-TYPE
		SYNTAX  OCTET STRING
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"An octet string containing the
			ringswitchGenPortAdapterStatus of every
			port on the card, one per octet:
				open    (1),
				closed  (2),
				opening (3)"
		::= { ringswitchSlotEntry 12 }
	ringswitchSlotTestError OBJECT-TYPE
		SYNTAX  OCTET STRING
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"An octet string containing the
			ringswitchFwdPortTestError of every
			port on the card, one per octet:
				no-error        (1),
				same-ring       (2),
				duplicate-ring  (3),
				fail-nb         (4),
				bad-rnum        (5),
				fail-b          (6)"
		::= { ringswitchSlotEntry 13 }
	ringswitchSlotPortHealth OBJECT-TYPE
		SYNTAX  OCTET STRING
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"An octet string containing the health of every
			port on the card, one per octet:
				normal          (1),
				warning         (2),
				degraded        (3),
				critical        (4)"
		::= { ringswitchSlotEntry 14 }
	ringswitchSlotLastStatus OBJECT-TYPE
		SYNTAX  INTEGER {
				atm-if-down     (1),
				atm-porta-down  (2),
				atm-portb-down  (3),
				fddi-if-down    (4),
				fddi-porta-iso  (5),
				fddi-portb-iso  (6)
				}
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"An enumerated value that records the last
			important event that occurred during
			the card's operation. This is used in
			sending traps when such events occur."
		::= { ringswitchSlotEntry 15 }
	ringswitchSlotPortCreated OBJECT-TYPE
		SYNTAX  OCTET STRING
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"An octet string containing the
			ringswitchGenPortCreate of every
			possible port on the card, one per octet:
				active          (1),
				inactive        (2)"
		::= { ringswitchSlotEntry 16 }
	ringswitchSlotHealth OBJECT-TYPE
		SYNTAX  INTEGER {
				normal   (1),
				warning  (2),
				degraded (3),
				critical (4)
				}
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"Current health of the card in the slot."
		::= { ringswitchSlotEntry 17 }
	ringswitchSlotVersion OBJECT-TYPE
		SYNTAX  INTEGER
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
                        "The version of code / hardware associated with
                         the slot."
		::= { ringswitchSlotEntry 18 }
----------------------------------------------------------------------------
-- the general port group
	ringswitchGenPortTable OBJECT-TYPE
		SYNTAX  SEQUENCE OF RingswitchGenPortEntry
		ACCESS  not-accessible
		STATUS  mandatory
		DESCRIPTION
			"A table that contains information applicable to all
			ports."
		::= { ringswitchGenPort 1 }
	ringswitchGenPortEntry OBJECT-TYPE
		SYNTAX  RingswitchGenPortEntry
		ACCESS  not-accessible
		STATUS  mandatory
		DESCRIPTION
			""
		INDEX { ringswitchGenPortNum }
		::= { ringswitchGenPortTable 1 }
	RingswitchGenPortEntry ::=
		SEQUENCE        {
				ringswitchGenPortNum
					INTEGER,
				ringswitchGenPortAdapterStatus
					INTEGER,
				ringswitchGenPortAddress
					TRNMacAddress,
				ringswitchGenPortLAA
					TRNMacAddress,
				ringswitchGenPortInOctets
					INTEGER48,
				ringswitchGenPortOutOctets
					INTEGER48,
				ringswitchGenPortResetCounters
					INTEGER,
				ringswitchGenPortRMONCapabilities
					INTEGER,
				ringswitchGenPortRMONMode
					INTEGER,
				ringswitchGenPortRMONSnoop
					TRNMacAddress,
				ringswitchGenPortIPXNet
					INTEGER,
				ringswitchGenPortLastStatus
					INTEGER,
				ringswitchGenPortCreate
					INTEGER,
				ringswitchGenPortHealth
					INTEGER
				}
	ringswitchGenPortNum OBJECT-TYPE
		SYNTAX  INTEGER (1..65535)
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"The port number of the port for which this entry
			contains Ringswitch management information."
		::= { ringswitchGenPortEntry 1 }
	ringswitchGenPortAdapterStatus OBJECT-TYPE
		SYNTAX  INTEGER {
				open    (1),
				closed  (2),
				opening (3),
				ready   (4)
				}
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"The status of the adapter used by this port."
		::= { ringswitchGenPortEntry 2 }
	ringswitchGenPortAddress OBJECT-TYPE
		SYNTAX  TRNMacAddress
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"The hard wired address of the port."
		::= { ringswitchGenPortEntry 3 }
	ringswitchGenPortLAA OBJECT-TYPE
		SYNTAX  TRNMacAddress
		ACCESS  read-write
		STATUS  mandatory
		DESCRIPTION
			"The locally administered address of the port."
		::= { ringswitchGenPortEntry 4 }
	ringswitchGenPortInOctets OBJECT-TYPE
		SYNTAX  INTEGER48
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"48 bit version of the interface MIB counter
			ifInOctets."
		::= { ringswitchGenPortEntry 5 }
	ringswitchGenPortOutOctets OBJECT-TYPE
		SYNTAX  INTEGER48
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"48 bit version of the interface MIB counter
			ifOutOctets."
		::= { ringswitchGenPortEntry 6 }
	ringswitchGenPortResetCounters OBJECT-TYPE
		SYNTAX  INTEGER {
				reset   (1)
				}
		ACCESS  read-write
		STATUS  mandatory
		DESCRIPTION
			"Use this variable to reset all the counters on
			this port, including those in the ringswitchGenPort,
			dot1dSRPort, ifEntry and fddimib groups."
		::= { ringswitchGenPortEntry 7 }
	ringswitchGenPortRMONCapabilities OBJECT-TYPE
		SYNTAX  INTEGER  {
				disabled      (1),
				economy       (2),
				standard      (3),
				snooping      (4)
				}
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"A value that indicates the RMON capabilities of the
			port. The value represents the highest level RMON
			mode which the port can support.
			   'disabled' is the lowest level and indicates
				      no RMON capabilities
			   'snooping' is the highest level and indicates
				      that the port is capable of
				      running in all RMON modes."
		::= { ringswitchGenPortEntry 8 }
	ringswitchGenPortRMONMode OBJECT-TYPE
		SYNTAX  INTEGER {
				disabled      (1),
				economy       (2),
				standard      (3),
				snooping      (4)
				}
		ACCESS  read-write
		STATUS  mandatory
		DESCRIPTION
			"Controls the types of frames passed to the RMON
			probe function for processing as follows:
			disabled - no frames passed to the RMON probe
			economy  - MAC frames required for token ring
				   specific groups as follows:
				   MLStats table
				   Token Ring Ring station table
			standard - all frames
			snooping - all frames provided by the 'economy'
				   mode plus any frames originating
				   from or destined for the 'snoop address'
				   controlled by ringswitchGenPortRMONSnoop"
		::= { ringswitchGenPortEntry 9 }
	ringswitchGenPortRMONSnoop OBJECT-TYPE
		SYNTAX  TRNMacAddress
		ACCESS  read-write
		STATUS  mandatory
		DESCRIPTION
			"This controls the RMON snoop address used when
			ringswitchGenPortRMONMode is set to 'snooping'.
			A value of all zeroes results in no snooping
			taking place."
		::= { ringswitchGenPortEntry 10 }
	ringswitchGenPortIPXNet OBJECT-TYPE
		SYNTAX  INTEGER
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"The IPX network number of the port."
		::= { ringswitchGenPortEntry 11 }
	ringswitchGenPortLastStatus OBJECT-TYPE
		SYNTAX  INTEGER {
				fddi-remove     (1),
				fddi-twisted    (2),
				fddi-bypass     (3)
				}
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"An enumerated value that records the last
			important event that occurred during the
			port's operation. This is used in sending
			traps when such events occur."
		::= { ringswitchGenPortEntry 12 }
	ringswitchGenPortCreate OBJECT-TYPE
		SYNTAX  INTEGER {
				active          (1),
				inactive        (2)
				}
		ACCESS  read-write
		STATUS  mandatory
		DESCRIPTION
			"This variable is used to dynamically create and
			destroy Ringswitch ports.
			When a port is active, all the mib table entries
			relevant to the type of port will be in existence.
			When a port is inactive no entry will exist for that
			port in the ringswitchGenPort table and no other port
			based mib table entries will exist for that port.
			To create a port the manager should issue a
			set request with a value of active which will
			result in an entry being created in the
			ringswitchGenPort table and all other port based
			tables relevant to the port.
			To destroy a port the manager should issue a set
			request with a value of inactive which will result
			in the deletion of the ringswitchGenPort table
			entry for the port and also the deletion of all
			other port based table entries associated with the
			port.
			Not all types of port can be created and destroyed
			dynamically."
		::= { ringswitchGenPortEntry 13 }
	ringswitchGenPortHealth OBJECT-TYPE
		SYNTAX INTEGER {
				normal   (1),
				warning  (2),
				degraded (3),
				critical (4)
				}
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"Current health of this port."
		::= { ringswitchGenPortEntry 14 }

----------------------------------------------------------------------------
-- the Ringswitch token ring group
	ringswitchTRIfTable OBJECT-TYPE
		SYNTAX  SEQUENCE OF RingswitchTRIfEntry
		ACCESS  not-accessible
		STATUS  mandatory
		DESCRIPTION
			"This table contains Token Ring interface
			parameters and state variables not available
			in dot5Table. There is one entry per 802.5
			interface."
		::= { ringswitchTR 1 }
	ringswitchTRIfEntry OBJECT-TYPE
		SYNTAX  RingswitchTRIfEntry
		ACCESS  not-accessible
		STATUS  mandatory
		DESCRIPTION
			"A list of Token Ring status and parameter
			values for an 802.5 interface."
		INDEX { ringswitchTRIfIndex }
		::= { ringswitchTRIfTable 1 }
	RingswitchTRIfEntry ::= 
                SEQUENCE        {
				ringswitchTRIfIndex                	 
                                      INTEGER,
				ringswitchTRIfRingSpeed            	 
                                      INTEGER,
				ringswitchTRIfMode                 	 
                                      INTEGER,
				ringswitchTRIfRPSEnable            	 
                                      INTEGER,
				ringswitchTRIfRingStatus           	 
                                      INTEGER,
				ringswitchTRIfSoftErrTimer         	 
                                      INTEGER,
				ringswitchTRIfFrameStatusControl   	 
                                      INTEGER,
				ringswitchTRIfTokenPriorityControl 	 
                                      INTEGER,
				ringswitchTRIfFastFailoverEnable   	 
                                      INTEGER,
				ringswitchTRIfFastFailoverStandbyStatus  
                                      INTEGER,
				ringswitchTRIfFastFailoverPrimaryPort    
                                      INTEGER,
				ringswitchTRIfFastFailoverTrapInfoReason 
                                      INTEGER				
				}
	ringswitchTRIfIndex OBJECT-TYPE
		SYNTAX  INTEGER
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"The value of this object identifies the
			802.5 interface for which this entry
			contains management information.  The
			value of this object for a particular
			interface has the same value as the
			ifIndex object for the same interface."
		::= { ringswitchTRIfEntry 1 }
	ringswitchTRIfRingSpeed OBJECT-TYPE
		SYNTAX  INTEGER {
				four    (1),
				sixteen (2),
				hundred (3)
				}
		ACCESS  read-write
		STATUS  mandatory
		DESCRIPTION
			"Speed of the ring to which this interface
			is connected."
		::= { ringswitchTRIfEntry 2 }
	ringswitchTRIfMode OBJECT-TYPE
		SYNTAX  INTEGER {
				node            (1),
				concentrator    (2),
				node-fdx        (3),
				conc-fdx        (4),
				node-classic    (5),
				conc-classic    (6),
				node-cau-rio    (7)
				}
		ACCESS  read-write
		STATUS  mandatory
		DESCRIPTION
			""
        ::= { ringswitchTRIfEntry 3 }
	ringswitchTRIfRPSEnable OBJECT-TYPE
		SYNTAX  INTEGER {
				enable          (1),
				disable         (2)
				}
		ACCESS  read-write
		STATUS  mandatory
		DESCRIPTION
			"Enables or disables the Ring Parameter Server
			function on the interface. If this variable is set to
			enabled, the interface will enable the RPS functional
			address, and respond to Request Parameters MAC
			frames, supplying the interface's segment number and
			soft error report timer value in the response."
		::= { ringswitchTRIfEntry 4 }
	ringswitchTRIfRingStatus OBJECT-TYPE
		SYNTAX  INTEGER {
				normal          (1),
				single          (2),
				beaconing       (3),
				normal-fdx      (4),
				dtr-wait        (5)
				}
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"The status of the ring to which this interface is
			connected.
			normal:     Inserted into Classic ring
			single:     Single Station
			beaconing:  Beaconing Detected
			normal-fdx: DTR link in full duplex (TXI) mode
			dtr-wait:   DTR Concentrator port waiting for
				    attached station to insert"
		::= { ringswitchTRIfEntry 5 }
	ringswitchTRIfSoftErrTimer OBJECT-TYPE
		SYNTAX  INTEGER (1..65535)
		ACCESS  read-write
		STATUS  mandatory
		DESCRIPTION
			"Sets the value of the Soft Error Timer Value used
			by the Ring Parameter Server function on this    
			interface. When this function is enabled it will
			respond to Request Parameters MAC frames, supplying 
			this value in the response as well as the segment
			number. Note that this value is in centiseconds."
		::= { ringswitchTRIfEntry 6 }
	ringswitchTRIfFrameStatusControl OBJECT-TYPE
		SYNTAX  INTEGER {
				enabled         (1),
				disabled        (2)
				}
		ACCESS  read-write
		STATUS  mandatory
		DESCRIPTION
			"Frame Status control is available on Aleutian
			based hardware. enabled (1); disabled(2)."
		::= { ringswitchTRIfEntry 7 }
	 ringswitchTRIfTokenPriorityControl OBJECT-TYPE
		SYNTAX  INTEGER {
				enabled         (1),
				disabled        (2)
				}
		ACCESS  read-write
		STATUS  mandatory
		DESCRIPTION
			"Frame Status control is available on Aleutian
			based hardware. enabled (1); disabled(2)."
		::= { ringswitchTRIfEntry 8 }
	 ringswitchTRIfFastFailoverEnable OBJECT-TYPE
		SYNTAX  INTEGER {
				enabled         (1),
				disabled        (2)
				}
		ACCESS  read-write
		STATUS  mandatory
		DESCRIPTION
			"Whether the port is a fast failover port. Only ports
			with odd port number on HSTR cards can be fast 
                        failover cards. On other ports this value is 
                        ignored. enabled (1); disabled(2)."
		::= { ringswitchTRIfEntry 9 }
	 ringswitchTRIfFastFailoverStandbyStatus OBJECT-TYPE
		SYNTAX  INTEGER {
				open	      (1),
				closed        (2),
				opening       (3),
				ready         (4),
				open-fail     (5)
				}
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"The status of the standby port in a fast failover 
                        pair. open (1); closed (2); opening (3); ready (4); 
                        open-fail(5)."
		::= { ringswitchTRIfEntry 10 }
	 ringswitchTRIfFastFailoverPrimaryPort OBJECT-TYPE
		SYNTAX  INTEGER {
				unknown         (0),
				left            (1),
				right           (2)
				}
		ACCESS  read-write
		STATUS  mandatory
		DESCRIPTION
			"The primary port in a fast failover pair. unknown (0);
			left (1); right (2)."
		::= { ringswitchTRIfEntry 11 }
	 ringswitchTRIfFastFailoverTrapInfoReason OBJECT-TYPE
		SYNTAX  INTEGER {
				primary-open-fail (1),
				standby-open-fail (2),
				links-swapped     (3)
				}
		ACCESS  not-accessible
		STATUS  mandatory
		DESCRIPTION
			"The reason why a fast failover trap was sent.
			primary-open-fail (1); standby-open-fail (2); 
                        links-swapped (3)"
		::= { ringswitchTRIfEntry 12 }



-- the Ringswitch token ring Group Switch group
	ringswitchTRGrpSwtchTable OBJECT-TYPE
		SYNTAX  SEQUENCE OF RingswitchTRGrpSwtchEntry
		ACCESS  not-accessible
		STATUS  mandatory
		DESCRIPTION
			"This table contains information relating to group 
                        switch rings (each containing five individual ports)."
		::= { ringswitchTR 2 }
	ringswitchTRGrpSwtchEntry OBJECT-TYPE
		SYNTAX  RingswitchTRGrpSwtchEntry
		ACCESS  not-accessible
		STATUS  mandatory
		DESCRIPTION
			"A list containing Group Switch ring information."
		INDEX { ringswitchTRGrpSwtchIfIndex }
		::= { ringswitchTRGrpSwtchTable 1 }
	RingswitchTRGrpSwtchEntry ::=
		SEQUENCE        {
				ringswitchTRGrpSwtchIfIndex
					INTEGER,
				ringswitchTRGrpSwtchRemoveThreshold
					INTEGER,
				ringswitchTRGrpSwtchRingPollAction
					INTEGER,
				ringswitchTRGrpSwtchBeaconThreshold
					INTEGER,
				ringswitchTRGrpSwtchBeaconAction
					INTEGER,
				ringswitchTRGrpSwtchPurgeThreshold
					INTEGER,
				ringswitchTRGrpSwtchPurgeAction
					INTEGER,
				ringswitchTRGrpSwtchIsoErrThreshold
					INTEGER,
				ringswitchTRGrpSwtchIsoErrAction
					INTEGER,
				ringswitchTRGrpSwtchRingPollThreshold
					INTEGER
				}
	ringswitchTRGrpSwtchIfIndex OBJECT-TYPE
		SYNTAX  INTEGER
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"The value of this object identifies the
			802.5 interface for which this entry
			contains management information.  The
			value of this object for a particular
			interface has the same value as the
			ifIndex object for the same interface."
		::= { ringswitchTRGrpSwtchEntry 1 }
	ringswitchTRGrpSwtchRemoveThreshold OBJECT-TYPE
		SYNTAX  INTEGER (1..255)
		ACCESS  read-write
		STATUS  mandatory
		DESCRIPTION
			"The removal threshold, this controls how many
			times a faulty but persistent node is allowed to
			attempt to insert into the ring before its port
			is permanently disabled."
		::= { ringswitchTRGrpSwtchEntry 2 }
	ringswitchTRGrpSwtchRingPollAction OBJECT-TYPE
		SYNTAX  INTEGER {
				ignore(1),
				inform(2),
				remove(3)
				}
		ACCESS  read-write
		STATUS  mandatory
		DESCRIPTION
			"Defines what happens when the group switch detects a
			ring poll failure."
		::= { ringswitchTRGrpSwtchEntry 3 }
	ringswitchTRGrpSwtchBeaconThreshold OBJECT-TYPE
		SYNTAX  INTEGER (1..255)
		ACCESS  read-write
		STATUS  mandatory
		DESCRIPTION
			"Number of successive beacon frames from one
			node before a problem is noticed."
		::= { ringswitchTRGrpSwtchEntry 4 }
	ringswitchTRGrpSwtchBeaconAction OBJECT-TYPE
		SYNTAX  INTEGER {
				ignore(1),
				inform(2),
				remove(3)
				}
		ACCESS  read-write
		STATUS  mandatory
		DESCRIPTION
			"Defines what happens when the group switch detects
			a succession of beacon frames from the same
			node."
		::= { ringswitchTRGrpSwtchEntry 5 }
	ringswitchTRGrpSwtchPurgeThreshold OBJECT-TYPE
		SYNTAX  INTEGER (1..255)
		ACCESS  read-write
		STATUS  mandatory
		DESCRIPTION
			"Number of successive ring purge frames from
			one node before a problem is noticed."
		::= { ringswitchTRGrpSwtchEntry 6 }
	ringswitchTRGrpSwtchPurgeAction OBJECT-TYPE
		SYNTAX  INTEGER {
				ignore(1),
				inform(2),
				remove(3)
				}
		ACCESS  read-write
		STATUS  mandatory
		DESCRIPTION
			"Defines what happens when the group switch detects a
			succession of ring purge frames from the same
			node."
		::= { ringswitchTRGrpSwtchEntry 7 }
	ringswitchTRGrpSwtchIsoErrThreshold OBJECT-TYPE
		SYNTAX  INTEGER (1..255)
		ACCESS  read-write
		STATUS  mandatory
		DESCRIPTION
			"Number of isolating errors before a problem
			is noticed."
		::= { ringswitchTRGrpSwtchEntry 8 }
	ringswitchTRGrpSwtchIsoErrAction OBJECT-TYPE
		SYNTAX  INTEGER {
				ignore(1),
				inform(2),
				remove(3)
				}
		ACCESS  read-write
		STATUS  mandatory
		DESCRIPTION
			"Defines what happens when the group switch detects
			too many isolating errors."
		::= { ringswitchTRGrpSwtchEntry 9 }
	ringswitchTRGrpSwtchRingPollThreshold OBJECT-TYPE
		SYNTAX  INTEGER (1..255)
		ACCESS  read-write
		STATUS  mandatory
		DESCRIPTION
			"Number of successive ring poll failures caused by one
			node before a problem is noticed."
		::= { ringswitchTRGrpSwtchEntry 10 }
-- the Ringswitch token ring group switch port group
	ringswitchTRGrpSwtchPortTable OBJECT-TYPE
		SYNTAX  SEQUENCE OF RingswitchTRGrpSwtchPortEntry
		ACCESS  not-accessible
		STATUS  mandatory
		DESCRIPTION
			"This table contains information relating to
			individual Group Switch ports."
		::= { ringswitchTR 3 }
	ringswitchTRGrpSwtchPortEntry OBJECT-TYPE
		SYNTAX  RingswitchTRGrpSwtchPortEntry
		ACCESS  not-accessible
		STATUS  mandatory
		DESCRIPTION
			"A list containing Group Switch port information."
		INDEX { ringswitchTRGrpSwtchPortIfIndex,
			ringswitchTRGrpSwtchPortIndex }
		::= { ringswitchTRGrpSwtchPortTable 1 }
	RingswitchTRGrpSwtchPortEntry
			::= SEQUENCE
				{
				ringswitchTRGrpSwtchPortIfIndex
					INTEGER,
				ringswitchTRGrpSwtchPortIndex
					INTEGER,
				ringswitchTRGrpSwtchPortAddress
					TRNMacAddress
				}
	ringswitchTRGrpSwtchPortIfIndex OBJECT-TYPE
		SYNTAX  INTEGER
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"The value of this object identifies the
			802.5 interface for which this entry
			contains management information.  The
			value of this object for a particular
			interface has the same value as the
			ifIndex object for the same interface."
		::= { ringswitchTRGrpSwtchPortEntry 1 }
	ringswitchTRGrpSwtchPortIndex OBJECT-TYPE
		SYNTAX  INTEGER (1..5)
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"Individual Group Switch port number"
		::= { ringswitchTRGrpSwtchPortEntry 2 }
	ringswitchTRGrpSwtchPortAddress OBJECT-TYPE
		SYNTAX  TRNMacAddress
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"Node address of device last inserted in the
			Group Switch port."
		::= { ringswitchTRGrpSwtchPortEntry 3 }
-- the Ringswitch token ring Group Switch trap information group
	ringswitchTRGrpSwtchTrapInfo OBJECT IDENTIFIER ::= { ringswitchTR 4 }
	ringswitchTRGrpSwtchTrapInfoReason OBJECT-TYPE
		SYNTAX  INTEGER{
			reason-unknown(1),    -- None of the reasons below
			ring-poll-failure(2), -- Node caused ring poll failure
			beaconing(3),         -- Node keeps beaconing
			ring-purges(4),       -- Node keeps purging ring
			isolating-errors(5),  -- Node has isolating errors
			removal-exceeded(6),  -- Node exceeded removal threshold
			beaconing-fault(7)    -- Node caused a beaconing fault
			}
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"Crime committed."
		::= { ringswitchTRGrpSwtchTrapInfo 1 }
-- the Ringswitch token ring Group Switch slot group
	ringswitchTRGrpSwtchSlotTable OBJECT-TYPE
		SYNTAX  SEQUENCE OF RingswitchTRGrpSwtchSlotEntry
		ACCESS  not-accessible
		STATUS  mandatory
		DESCRIPTION
			"This table contains information relating to
			Group Switch cards."
		::= { ringswitchTR 5 }
	ringswitchTRGrpSwtchSlotEntry OBJECT-TYPE
		SYNTAX  RingswitchTRGrpSwtchSlotEntry
		ACCESS  not-accessible
		STATUS  mandatory
		DESCRIPTION
			"A list containing Group Switch card information."
		INDEX { ringswitchTRGrpSwtchSlotIndex }
		::= { ringswitchTRGrpSwtchSlotTable 1 }
	RingswitchTRGrpSwtchSlotEntry
			::= SEQUENCE
				{
				ringswitchTRGrpSwtchSlotIndex
					INTEGER,
				ringswitchTRGrpSwtchSlotAdminStatii
					OCTET STRING,
				ringswitchTRGrpSwtchSlotAutoPartitions
					OCTET STRING,
				ringswitchTRGrpSwtchSlotOperStatii
					OCTET STRING
				}
	ringswitchTRGrpSwtchSlotIndex OBJECT-TYPE
		SYNTAX  INTEGER (1..65535)
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"Group Switch slot number"
		::= { ringswitchTRGrpSwtchSlotEntry 1 }
	ringswitchTRGrpSwtchSlotAdminStatii OBJECT-TYPE
		SYNTAX  OCTET STRING
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"An octet string containing the rptrPortAdminStatus
			of every Group Switch port on the card, one per octet:
				enabled(1),
				disabled(2)"
		::= { ringswitchTRGrpSwtchSlotEntry 2 }
	ringswitchTRGrpSwtchSlotAutoPartitions OBJECT-TYPE
		SYNTAX  OCTET STRING
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"An octet string with the rptrPortAutoPartitionState
			of every Group Switch port on the card, one per octet:
				notAutoPartitioned(1),
				autoPartitioned(2)"
		::= { ringswitchTRGrpSwtchSlotEntry 3 }
	ringswitchTRGrpSwtchSlotOperStatii OBJECT-TYPE
		SYNTAX  OCTET STRING
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"An octet string containing the rptrPortOperStatus
			of every Group Switch port on the card, one per octet:
				operational(1),
				notOperational(2),
				notPresent(3)"
		::= { ringswitchTRGrpSwtchSlotEntry 4 }
----------------------------------------------------------------------------
-- the FDDI group
	ringswitchFDDICardTable OBJECT-TYPE
		SYNTAX  SEQUENCE OF RingswitchFDDICardEntry
		ACCESS  not-accessible
		STATUS  mandatory
		DESCRIPTION
			"A table that contains information about each FDDI
			card in the Ringswitch."
		::= { ringswitchFDDI 1 }
	ringswitchFDDICardEntry OBJECT-TYPE
		SYNTAX  RingswitchFDDICardEntry
		ACCESS  not-accessible
		STATUS  mandatory
		DESCRIPTION
			"A list of FDDI status and parameter values for
			a FDDI card."
		INDEX { ringswitchFDDICardNum }
		::= { ringswitchFDDICardTable 1 }
	RingswitchFDDICardEntry ::=
		SEQUENCE
			{
			ringswitchFDDICardNum           INTEGER,
			ringswitchFDDICardSMTIndex      INTEGER,
			ringswitchFDDICardFixupMode     INTEGER,
			ringswitchFDDICardVRStatus      INTEGER,
			ringswitchFDDICardVRRingNum     INTEGER,
			ringswitchFDDICardVRBridgeNum   INTEGER,
			ringswitchFDDICardStatus        INTEGER
			}
	ringswitchFDDICardNum OBJECT-TYPE
		SYNTAX  INTEGER (1..65535)
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"The slot number of the card for which this entry
			contains Ringswitch management information."
		::= { ringswitchFDDICardEntry 1 }
	ringswitchFDDICardSMTIndex OBJECT-TYPE
		SYNTAX  INTEGER (1..65535)
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"This variable takes the same value as the
			fddimibSMTIndex variable associated with this
			FDDI card."
		::= { ringswitchFDDICardEntry 2 }
	ringswitchFDDICardFixupMode OBJECT-TYPE
		SYNTAX  INTEGER {
				disabled    (1),
				fddi-all    (2)
				}
		ACCESS  read-write
		STATUS  mandatory
		DESCRIPTION
			"This MIB variable controls operation of protocol
			fixups on the FDDI linecard. When set to fddi-all,
			embedded MAC addresses in frames are bit-reversed to
			convert the frames into native FDDI format"
		::= { ringswitchFDDICardEntry 3 }
	ringswitchFDDICardVRStatus OBJECT-TYPE
		SYNTAX  INTEGER {
				enable    (1),
				disable   (2)
				}
		ACCESS  read-write
		STATUS  mandatory
		DESCRIPTION
			"This variable is used to enable / disable the
			FDDI virtual ring."
		::= { ringswitchFDDICardEntry 4 }
	ringswitchFDDICardVRRingNum OBJECT-TYPE
		SYNTAX  INTEGER
		ACCESS  read-write
		STATUS  mandatory
		DESCRIPTION
			"The ring number of the virtual ring."
		::= { ringswitchFDDICardEntry 5 }
	ringswitchFDDICardVRBridgeNum OBJECT-TYPE
		SYNTAX  INTEGER
		ACCESS  read-write
		STATUS  mandatory
		DESCRIPTION
			"The bridge number of the virtual ring."
		::= { ringswitchFDDICardEntry 6 }
	ringswitchFDDICardStatus OBJECT-TYPE
		SYNTAX  INTEGER
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"This variable gives the status of the SMT.
			Each bit (when set) carries the following
			information:
			bit 0 (lsb) : ring-inop,
			bit 1       : mac-unavailable,
			bit 2       : twisted,
			bit 3       : i-am-duplicate,
			bit 4       : he-is-duplicate,
			bit 5       : bypass-stuck,
			bit 6       : trace,
			bit 7       : long-ring-inop
			Multiple status values can occur simultaneously.
			If the port is not open then all bits are set to 0."
		::= { ringswitchFDDICardEntry 7 }
----------------------------------------------------------------------------
-- the ATM card group
	ringswitchATMCardTable OBJECT-TYPE
		SYNTAX  SEQUENCE OF RingswitchATMCardEntry
		ACCESS  not-accessible
		STATUS  mandatory
		DESCRIPTION
			"A table that contains information about each ATM
			card in the Ringswitch."
		::= { ringswitchATM 1 }
	ringswitchATMCardEntry OBJECT-TYPE
		SYNTAX  RingswitchATMCardEntry
		ACCESS  not-accessible
		STATUS  mandatory
		DESCRIPTION
			"A list that contains information about each ATM
			card in the Ringswitch."
		INDEX { ringswitchATMCardNum }
		::= { ringswitchATMCardTable 1 }
	RingswitchATMCardEntry ::=
		SEQUENCE        {
				ringswitchATMCardNum            INTEGER,
				ringswitchATMCardAdminStatus    INTEGER,
				ringswitchATMCardRemoteUNIVer   INTEGER,
				ringswitchATMCardLECSAddress    NSAP,
				ringswitchATMCardLECSIlmi       INTEGER,
				ringswitchATMCardLECSWka        INTEGER,
				ringswitchATMCardLECSPvc        INTEGER,
				ringswitchATMCardActivePort     INTEGER,
				ringswitchATMCardILMIStatus     INTEGER,
				ringswitchATMCardSignalStatus   INTEGER,
				ringswitchATMCardHardWiredESI   ETHMacAddress,
				ringswitchATMCardLocalESI       ETHMacAddress,
				ringswitchATMCardPortB          INTEGER,
				ringswitchATMCardMode           INTEGER,
				ringswitchATMCardSONETIfIndex   INTEGER,
				ringswitchATMCardATMIfIndex     INTEGER,
				ringswitchATMCardAAL5IfIndex    INTEGER,
				ringswitchATMCardRowStatuses    OCTET STRING,
				ringswitchATMCardMediaType      INTEGER,
				ringswitchATMCardConfigUNIVer   INTEGER,
				ringswitchATMCardActualUNIVer   INTEGER,
				ringswitchATMCardEthLecSupport  INTEGER
				}
	ringswitchATMCardNum OBJECT-TYPE
		SYNTAX  INTEGER (1..65535)
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"The slot number of the card for which this entry
			contains Ringswitch management information."
		::= { ringswitchATMCardEntry 1 }
	ringswitchATMCardAdminStatus OBJECT-TYPE
		SYNTAX  INTEGER {
				up(1),
				down(2)
				}
		ACCESS  read-write
		STATUS  mandatory
		DESCRIPTION
			"This variable is a duplicate of the ifAdminStatus
			of the SONET interface associated with this ATM
			card. It is used to enable / disable the ATM card."
		::= { ringswitchATMCardEntry 2 }
	ringswitchATMCardRemoteUNIVer OBJECT-TYPE
		SYNTAX  INTEGER {
				unknown         (1),
				uni-30          (2),
				uni-31          (3),
				uni-40          (4)
				}
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"The type of signalling supported by the ATM
			switch at the remote end of this interface."
		::= { ringswitchATMCardEntry 3 }
	ringswitchATMCardLECSAddress OBJECT-TYPE
		SYNTAX  NSAP
		ACCESS  read-write
		STATUS  mandatory
		DESCRIPTION
			"The LECS address that a LEC associated with this
			card will use.
			If the configured address fails or is set to 0 then
			the LEC must find the location of the LECS by either:
			using a defined ILMI procedure to determine the LECS
			address; using the well-known LECS address; or using
			the well-known PVC to the LECS.
			The LEC will try each of these three methods in turn
			(if configured to use them) until the LECS is
			located."
		::= { ringswitchATMCardEntry 4 }
	ringswitchATMCardLECSIlmi OBJECT-TYPE
		SYNTAX  INTEGER  {
				enable  (1),
				disable (2)
				}
		ACCESS  read-write
		STATUS  mandatory
		DESCRIPTION
			"This variable indicates whether or not a LEC can
			try to determine the LECS address using ILMI."
		::= { ringswitchATMCardEntry 5 }
	ringswitchATMCardLECSWka OBJECT-TYPE
		SYNTAX  INTEGER  {
				enable  (1),
				disable (2)
				}
		ACCESS  read-write
		STATUS  mandatory
		DESCRIPTION
			"This variable indicates whether or not a LEC can
			use the well-known LECS address to locate the LECS."
		::= { ringswitchATMCardEntry 6 }
	ringswitchATMCardLECSPvc OBJECT-TYPE
		SYNTAX  INTEGER  {
				enable  (1),
				disable (2)
				}
		ACCESS  read-write
		STATUS  mandatory
		DESCRIPTION
			"This variable indicates whether or not a LEC can
			use the well-known PVC to locate the LECS."
		::= { ringswitchATMCardEntry 7 }
	ringswitchATMCardActivePort OBJECT-TYPE
		SYNTAX  INTEGER  {
				port-a          (1),
				port-b          (2)
				}
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"The port that the ATM card is currently using."
		::= { ringswitchATMCardEntry 8 }
	ringswitchATMCardILMIStatus OBJECT-TYPE
		SYNTAX  INTEGER  {
				up(1),
				down(2)
				}
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"The current state of the ILMI signalling for
			this card."
		::= { ringswitchATMCardEntry 9 }
	ringswitchATMCardSignalStatus OBJECT-TYPE
		SYNTAX  INTEGER  {
				up(1),
				down(2)
				}
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"The current state of the ATM signalling for
			this card."
		::= { ringswitchATMCardEntry 10 }
	ringswitchATMCardHardWiredESI OBJECT-TYPE
		SYNTAX  ETHMacAddress
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"The Hard wired ESI (End Station Identifier)
			for the card."
		::= { ringswitchATMCardEntry 11 }
	ringswitchATMCardLocalESI OBJECT-TYPE
		SYNTAX  ETHMacAddress
		ACCESS  read-write
		STATUS  mandatory
		DESCRIPTION
			"The locally administered ESI (End Station Identifier)
			for the card. When a set request for this variable
			is accepted the new value will not take effect until
			the next time the ATM card is rebooted."
		::= { ringswitchATMCardEntry 12 }
	ringswitchATMCardPortB OBJECT-TYPE
		SYNTAX  INTEGER {
				enabled         (1),
				disabled        (2)
				}
		ACCESS  read-write
		STATUS  mandatory
		DESCRIPTION
			"The state of Port B - whether or not an attempt will
			be made to use Port B in the event of Port A failing."
		::= { ringswitchATMCardEntry 13 }
	ringswitchATMCardMode OBJECT-TYPE
		SYNTAX  INTEGER {
				sonet           (1),
				sdh             (2)
				}
		ACCESS  read-write
		STATUS  mandatory
		DESCRIPTION
			"The framing mode used by the front-end."
		::= { ringswitchATMCardEntry 14 }
	ringswitchATMCardSONETIfIndex OBJECT-TYPE
		SYNTAX  INTEGER
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"Each ATM card provides one SONET interface.
			This variable takes the same value as the ifIndex
			associated with this SONET interface."
		::= { ringswitchATMCardEntry 15 }
	ringswitchATMCardATMIfIndex OBJECT-TYPE
		SYNTAX  INTEGER
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"Each ATM card provides one ATM interface.
			This variable takes the same value as the ifIndex
			associated with this ATM interface."
		::= { ringswitchATMCardEntry 16 }
	ringswitchATMCardAAL5IfIndex OBJECT-TYPE
		SYNTAX  INTEGER
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"Each ATM card provides one AAL5 interface.
			This variable takes the same value as the ifIndex
			associated with this AAL5 interface."
		::= { ringswitchATMCardEntry 17 }
	ringswitchATMCardRowStatuses OBJECT-TYPE
		SYNTAX  OCTET STRING
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"An octet string containing the lecRowStatus for
			each LEC on this ATM card."
		::= { ringswitchATMCardEntry 18 }
	ringswitchATMCardMediaType OBJECT-TYPE
		SYNTAX  INTEGER {
				multi-mode-fiber  (1),
				single-mode-fiber (2)
				}
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"The media type of this ATM card."
		::= { ringswitchATMCardEntry 19 }
	ringswitchATMCardConfigUNIVer OBJECT-TYPE
		SYNTAX  INTEGER {
				autoConfig      (1),
				uni-30          (2),
				uni-31          (3),
				uni-40          (4)
				}
		ACCESS  read-write
		STATUS  mandatory
		DESCRIPTION
			"The type of signalling that is configured for
			the interface."
		::= { ringswitchATMCardEntry 20 }
	ringswitchATMCardActualUNIVer OBJECT-TYPE
		SYNTAX  INTEGER {
				unknown         (1),
				uni-30          (2),
				uni-31          (3),
				uni-40          (4)
				}
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"The type of signalling that is currently being
			used on the interface.  This may reflect a manually
			configured value for the signalling type, or
			may be determined by other means such as auto-
			configuration."
		::= { ringswitchATMCardEntry 21 }
	ringswitchATMCardEthLecSupport OBJECT-TYPE
		SYNTAX  INTEGER {
				yes (1),
				no  (2)
				}
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"Does this ATM card support Ethernet LEC."
		::= { ringswitchATMCardEntry 22 }
-- the ATM port group
	ringswitchATMPortTable OBJECT-TYPE
		SYNTAX  SEQUENCE OF RingswitchATMPortEntry
		ACCESS  not-accessible
		STATUS  mandatory
		DESCRIPTION
			"A table that contains information about each LEC
			in the Ringswitch."
		::= { ringswitchATM 2 }
	ringswitchATMPortEntry OBJECT-TYPE
		SYNTAX  RingswitchATMPortEntry
		ACCESS  not-accessible
		STATUS  mandatory
		DESCRIPTION
			"A list that contains information about each LEC
			in the Ringswitch."
		INDEX { ringswitchATMPortNum }
		::= { ringswitchATMPortTable 1 }
	RingswitchATMPortEntry ::=
			SEQUENCE        {
					ringswitchATMPortNum
						INTEGER,
					ringswitchATMPortFailReason
						INTEGER,
					ringswitchATMPortFailSecondary
						INTEGER,
					ringswitchATMPortRxDiscards
						INTEGER,
					ringswitchATMPortTxDiscards
						INTEGER,
					ringswitchATMPortTxQuotaDiscards
						INTEGER,
					ringswitchATMPortTxBUSBroadcasts
						INTEGER,
					ringswitchATMPortTxBUSUnknowns
						INTEGER,
					ringswitchATMPortRxBUSFiltered
						INTEGER,
					ringswitchATMPortIPXTypeETH
						INTEGER,
					ringswitchATMPortIPXTypeTRN
						INTEGER,
					ringswitchATMPortIPXEvenize
						INTEGER,
					ringswitchATMPortIPXBufLimit
						INTEGER,
					ringswitchATMPortIPXEnable
						INTEGER,
					ringswitchATMPortIPEnable
						INTEGER,
                                        ringswitchATMPortIPMulticastType
                                                INTEGER
					}
	ringswitchATMPortNum OBJECT-TYPE
		SYNTAX  INTEGER (1..65535)
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"The port number of the port for which this entry
			contains LEC information."
		::= { ringswitchATMPortEntry 1 }
	ringswitchATMPortFailReason OBJECT-TYPE
		SYNTAX  INTEGER {
				success                 (1),
				sig-ilmi-fail           (2),
				lecs-not-found          (3),
				config-failed           (4),
				les-connect-fail        (5),
				join-failed             (6),
				bus-not-found           (7),
				bus-connect-fail        (8),
				bus-didnt-connect       (9),
				duplicate-elan          (10),
				les-vc-dropped          (11),
				bus-vc-dropped          (12),
				rejoin-wrong-elan       (13),
				insufficient-memory     (14),
				already-open            (15),
				invalid-port            (16),
				unknown                 (17)
				}
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"The reason why this LEC failed to open."
		::= { ringswitchATMPortEntry 2 }
	ringswitchATMPortFailSecondary OBJECT-TYPE
		SYNTAX  INTEGER {
				none                    (1),
				timeout                 (2),
				undefined-error         (3),
				version-not-supported   (4),
				invalid-request-params  (5),
				dup-lan-destination     (6),
				dup-atm-address         (7),
				insuff-resources        (8),
				access-denied           (9),
				invalid-requester-id    (10),
				invalid-lan-destination (11),
				invalid-atm-address     (12),
				no-configuration        (13),
				le-configure-error      (14),
				insuff-information      (15)
				}
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"More information on why this LEC failed to open.
			Only valid if ringswitchATMFailReason is either
			config-failed(4) or join-failed(6)."
		::= { ringswitchATMPortEntry 3 }
	ringswitchATMPortRxDiscards OBJECT-TYPE
		SYNTAX  INTEGER
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			""
		::= { ringswitchATMPortEntry 4 }
	ringswitchATMPortTxDiscards OBJECT-TYPE
		SYNTAX  INTEGER
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			""
		::= { ringswitchATMPortEntry 5 }
	ringswitchATMPortTxQuotaDiscards OBJECT-TYPE
		SYNTAX  INTEGER
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			""
		::= { ringswitchATMPortEntry 6 }
	ringswitchATMPortTxBUSBroadcasts OBJECT-TYPE
		SYNTAX  INTEGER
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			""
		::= { ringswitchATMPortEntry 7 }
	ringswitchATMPortTxBUSUnknowns OBJECT-TYPE
		SYNTAX  INTEGER
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			""
		::= { ringswitchATMPortEntry 8 }
	ringswitchATMPortRxBUSFiltered OBJECT-TYPE
		SYNTAX  INTEGER
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			""
		::= { ringswitchATMPortEntry 9 }
	ringswitchATMPortIPXTypeETH OBJECT-TYPE
		SYNTAX  INTEGER {
				auto            (1),
				ieee8022        (2),
				snap            (3),
				ieee8023        (4),
				dix             (5)
				}
		ACCESS  read-write
		STATUS  mandatory
		DESCRIPTION
			"This variable specifies the encapsulation type on
			Ethernet for all IPX frames forwarded from onto
			an Ethernet LEC"
		::= { ringswitchATMPortEntry 10 }
	ringswitchATMPortIPXTypeTRN OBJECT-TYPE
		SYNTAX  INTEGER {
				auto            (1),
				ieee8022        (2),
				snap            (3)
				}
		ACCESS  read-write
		STATUS  mandatory
		DESCRIPTION
			"This variable specifies the encapsulation type on 
			TRN for all IPX frames forwarded from an Ethernet LEC"
		::= { ringswitchATMPortEntry 11 }
	ringswitchATMPortIPXEvenize OBJECT-TYPE
		SYNTAX  INTEGER {
				enable          (1),
				disable         (2)
				}
		ACCESS  read-write
		STATUS  mandatory
		DESCRIPTION
			"If this variable is set to enable then all IPX
			packets forwarded in either direction to be even 
			length by adding a zero padding byte if required"
		::= { ringswitchATMPortEntry 12 }
	ringswitchATMPortIPXBufLimit OBJECT-TYPE
		SYNTAX  INTEGER {
				enable          (1),
				disable         (2)
				}
		ACCESS  read-write
		STATUS  mandatory
		DESCRIPTION
			"If this variable is enabled then any NCP buffer size
			negotiation requests are intercepted and the 
			requested frame size is limit to something less than
			the ELAN's maximum size"
		::= { ringswitchATMPortEntry 13 }
	ringswitchATMPortIPXEnable OBJECT-TYPE
		SYNTAX  INTEGER {
				enable          (1),
				disable         (2)
				}
		ACCESS  read-write
		STATUS  mandatory
		DESCRIPTION
			"If this variable is enabled on an Ethernet LEC port 
			 then the LEC will do translational bridging of IPX 
			 frames, otherwise it will discard them.  It is 
			 ignored for Token Ring LECs"
		::= { ringswitchATMPortEntry 14 }
	ringswitchATMPortIPEnable OBJECT-TYPE
		SYNTAX  INTEGER {
				enable          (1),
				disable         (2)
				}
		ACCESS  read-write
		STATUS  mandatory
		DESCRIPTION
			"If this variable is enabled on an Ethernet LEC port 
			 then the LEC will do translational bridging of IP,
			 ARP and RARP frames, otherwise it will discard them. 
			 It is ignored for Token Ring LECs"
		::= { ringswitchATMPortEntry 15 }
    ringswitchATMPortIPMulticastType OBJECT-TYPE
		SYNTAX	INTEGER {
				broadcast (1),
				functional(2),
				group     (3)
				}
		ACCESS  read-write
		STATUS  mandatory
		DESCRIPTION
			"This variable specifies the type of MAC address to
			use as the destination for IP multicast frames on
			Token Ring. It is ignored for Token Ring LECs"
		::= { ringswitchATMPortEntry 16 }

----------------------------------------------------------------------------
-- the TLS card group
	ringswitchTLSCardTable OBJECT-TYPE
		SYNTAX	SEQUENCE OF RingswitchTLSCardEntry
		ACCESS	not-accessible
		STATUS	mandatory
		DESCRIPTION
			"A table that contains information about each TLS
			card in the Ringswitch."
		::= { ringswitchTLS 1 }
	ringswitchTLSCardEntry OBJECT-TYPE
		SYNTAX	RingswitchTLSCardEntry
		ACCESS	not-accessible
		STATUS	mandatory
		DESCRIPTION
			"A list that contains information about each TLS
			card in the Ringswitch."
		INDEX { ringswitchTLSCardNum }
		::= { ringswitchTLSCardTable 1 }
	RingswitchTLSCardEntry ::=
		SEQUENCE
                {
            ringswitchTLSCardNum                		INTEGER,
            ringswitchTLSCardRowStatuses        		OCTET STRING,
            ringswitchTLSCardRIPTTL             		INTEGER,
            ringswitchTLSCardRIPInterval        		INTEGER,
            ringswitchTLSCardRIPAging           		INTEGER,
            ringswitchTLSCardDefRoute           		IpAddress,
            ringswitchTLSCardDefRouteMetric     		INTEGER,
            ringswitchTLSCardRIPFlags           		INTEGER,
            ringswitchTLSCardOSPFRouterId       		IpAddress,
            ringswitchTLSCardOSPFFlags          		INTEGER,
            ringswitchTLSCardStatsCpuUsage      		INTEGER,
            ringswitchTLSCardStatsFreeMem       		INTEGER,
            ringswitchTLSCardStatsFramesFwd     		INTEGER,
            ringswitchTLSCardStatsMaxCpuUsage   		INTEGER,
            ringswitchTLSCardStatsMinFreeMem    		INTEGER,
            ringswitchTLSCardStatsMaxFramesFwd  		INTEGER,
            ringswitchTLSCardStatsResetState    		INTEGER,
            ringswitchTLSCardARPFlush           		INTEGER,
            ringswitchTLSCardBootPRASecondsThreshold 	        INTEGER,
            ringswitchTLSCardBootPRAHopsThreshold               INTEGER
		}
	ringswitchTLSCardNum OBJECT-TYPE
		SYNTAX	INTEGER (1..65535)
		ACCESS	read-only
		STATUS	mandatory
		DESCRIPTION
			"The slot number of the card for which this entry
			contains Ringswitch management information."
		::= { ringswitchTLSCardEntry 1 }
	ringswitchTLSCardRowStatuses OBJECT-TYPE
		SYNTAX	OCTET STRING
		ACCESS	read-only
		STATUS	mandatory
		DESCRIPTION
			"An octet string containing the legRowStatus for
			each LEG on this TLS card."
		::= { ringswitchTLSCardEntry 2 }
	ringswitchTLSCardRIPTTL OBJECT-TYPE
		SYNTAX	INTEGER (1..255)
		ACCESS	read-write
		STATUS	mandatory
		DESCRIPTION
			"An integer giving the value to use for the TTL
                         (Time to Live) field in RIP frames."
		::= { ringswitchTLSCardEntry 3 }
	ringswitchTLSCardRIPInterval OBJECT-TYPE
		SYNTAX	INTEGER (1..65535)
		ACCESS	read-write
		STATUS	mandatory
		DESCRIPTION
			"An integer giving the interval in seconds
                         between RIP anouncements."
		::= { ringswitchTLSCardEntry 4 }
	ringswitchTLSCardRIPAging OBJECT-TYPE
		SYNTAX	INTEGER (1..65535)
		ACCESS	read-write
		STATUS	mandatory
		DESCRIPTION
			"Time in seconds to age out routes learnt
                         through RIP."
		::= { ringswitchTLSCardEntry 5 }
	ringswitchTLSCardDefRoute OBJECT-TYPE
		SYNTAX	IpAddress
		ACCESS	read-write
		STATUS	mandatory
		DESCRIPTION
			"The IP address of a gateway to use as the default 
                         route."
		::= { ringswitchTLSCardEntry 6 }
	ringswitchTLSCardDefRouteMetric OBJECT-TYPE
		SYNTAX	INTEGER (0..16)
		ACCESS	read-write
		STATUS	mandatory
		DESCRIPTION
			"The metric to use for the default route."
		::= { ringswitchTLSCardEntry 7 }
	ringswitchTLSCardRIPFlags OBJECT-TYPE
		SYNTAX	INTEGER (0..65535)
		ACCESS	read-write
		STATUS	mandatory
		DESCRIPTION
			"RIP flags for the TLS card.  Bit 0 is set to 
                        indicate Ignore the Neighbours List. Bit 1 is set 
                        to indicate that routes learnt through OSPF should 
                        be redistributed through RIP."
		::= { ringswitchTLSCardEntry 8 }
	ringswitchTLSCardOSPFRouterId OBJECT-TYPE
		SYNTAX	IpAddress
		ACCESS	read-write
		STATUS	mandatory
		DESCRIPTION
			"The OSPF router Identifier."
		::= { ringswitchTLSCardEntry 9 }
	ringswitchTLSCardOSPFFlags OBJECT-TYPE
		SYNTAX	INTEGER (0..65535)
		ACCESS	read-write
		STATUS	mandatory
		DESCRIPTION
			"OSPF flags for the TLS card.  Bit 0 must be set
                         in an autonomous system border router and cleared
                         otherwise.  Bit 1 is not currently used. Bit 2 
                         indicates that RIP routes should be exported to 
                         OSPF. Bit 3 indicates that static routes should be 
                         exported to OSPF. Bit 15 is the master OSPF enable 
                         for the card."
		::= { ringswitchTLSCardEntry 10 }
	ringswitchTLSCardStatsCpuUsage OBJECT-TYPE
		SYNTAX	INTEGER (0..10000)
		ACCESS	read-only
		STATUS	mandatory
		DESCRIPTION
			"The estimated CPU usage on the TLS card in a second
                         measured in hundredths of a percent."
		::= { ringswitchTLSCardEntry 11 }
	ringswitchTLSCardStatsFreeMem OBJECT-TYPE
		SYNTAX	INTEGER
		ACCESS	read-only
		STATUS	mandatory
		DESCRIPTION
			"The amount of free memory in the heap on the TLS card
                         measured in kilobytes."
		::= { ringswitchTLSCardEntry 12 }
	ringswitchTLSCardStatsFramesFwd OBJECT-TYPE
		SYNTAX	INTEGER
		ACCESS	read-only
		STATUS	mandatory
		DESCRIPTION
			"The number of frames forwarded per second by the
                         fast forwarding engine on the TLS card."
		::= { ringswitchTLSCardEntry 13 }
	ringswitchTLSCardStatsMaxCpuUsage OBJECT-TYPE
		SYNTAX	INTEGER (0..10000)
		ACCESS	read-only
		STATUS	mandatory
		DESCRIPTION
			"The maximum value recorded for the CPU usage 
                         parameter."
		::= { ringswitchTLSCardEntry 14 }
	ringswitchTLSCardStatsMinFreeMem OBJECT-TYPE
		SYNTAX	INTEGER
		ACCESS	read-only
		STATUS	mandatory
		DESCRIPTION
			"The minimum value recorded for the CPU usage 
                         parameter."
		::= { ringswitchTLSCardEntry 15 }
	ringswitchTLSCardStatsMaxFramesFwd OBJECT-TYPE
		SYNTAX	INTEGER
		ACCESS	read-only
		STATUS	mandatory
		DESCRIPTION
			"The maximum value recorded for the forwarding 
                         parameter."
		::= { ringswitchTLSCardEntry 16 }
	ringswitchTLSCardStatsResetState OBJECT-TYPE
		SYNTAX	INTEGER
		ACCESS	read-write
		STATUS	mandatory
		DESCRIPTION
			"Write any value to this to reset the three maximum and
                         minimum values above.  Always reads as 1."
		::= { ringswitchTLSCardEntry 17 }
	ringswitchTLSCardARPFlush OBJECT-TYPE
		SYNTAX  INTEGER
		ACCESS  read-write
		STATUS  mandatory
		DESCRIPTION
			"Write any value to this to flush the TLS's ARP cache."
		::= { ringswitchTLSCardEntry 18 }
	ringswitchTLSCardBootPRASecondsThreshold OBJECT-TYPE
		SYNTAX  INTEGER
		ACCESS  read-write
		STATUS  mandatory
		DESCRIPTION
			"Length of time, in seconds, since the moment a 
                         BootP/DHCP client was booted after which the a 
                         clients BootP/DHCP requests will no longer be 
                         forwarded. Default value is 86,400."
		::= { ringswitchTLSCardEntry 19 }
	ringswitchTLSCardBootPRAHopsThreshold OBJECT-TYPE
		SYNTAX  INTEGER (1..16)
		ACCESS  read-write
		STATUS  mandatory
		DESCRIPTION
			"A maximum value for the hops field in received 
                        BootP/DHCP requests. Default value is 4."
		::= { ringswitchTLSCardEntry 20 }

-- the TLS port group
	ringswitchTLSPortTable OBJECT-TYPE
		SYNTAX	SEQUENCE OF RingswitchTLSPortEntry
		ACCESS	not-accessible
		STATUS	mandatory
		DESCRIPTION
			"A table that contains information about each LEG
			in the Ringswitch."
		::= { ringswitchTLS 2 }
	ringswitchTLSPortEntry OBJECT-TYPE
		SYNTAX	RingswitchTLSPortEntry
		ACCESS	not-accessible
		STATUS	mandatory
		DESCRIPTION
			"A list that contains information about each LEG
			in the Ringswitch."
		INDEX { ringswitchTLSPortNum }
		::= { ringswitchTLSPortTable 1 }
	RingswitchTLSPortEntry ::=
		SEQUENCE
                {
			ringswitchTLSPortNum           		INTEGER,
			ringswitchTLSPortName          		DisplayString ,
			ringswitchTLSPortMTU           		INTEGER,
			ringswitchTLSPortEnable        		INTEGER,
			ringswitchTLSPortIpAddress     		IpAddress,
			ringswitchTLSPortIpSubnetMask  		IpAddress,
			ringswitchTLSPortRIPRxType     		INTEGER,
			ringswitchTLSPortRIPTxType     		INTEGER,
			ringswitchTLSPortRIPFlags      		INTEGER,
			ringswitchTLSPortIfArray       		OCTET STRING,
			ringswitchTLSPortRIP2AuthType  		INTEGER,
			ringswitchTLSPortRIP2AuthKey   		OCTET STRING (SIZE(0..16)),
			ringswitchTLSPortOSPFEnable    		INTEGER,
			ringswitchTLSPortOSPFAreaId    		IpAddress,
			ringswitchTLSPortLegFlags      		INTEGER,
			ringswitchTLSPortOSPFAuthType  		INTEGER,
			ringswitchTLSPortOSPFAuthMD5KeyId 	INTEGER,
			ringswitchTLSPortOSPFAuthKey    	OCTET STRING (SIZE(0..16)),
			ringswitchTLSPortOSPFCost    		INTEGER,
			ringswitchTLSPortOSPFPriority  		INTEGER,
			ringswitchTLSPortOSPFHelloInterval	INTEGER,
			ringswitchTLSPortOSPFDeadInterval	INTEGER,
			ringswitchTLSPortBootPFlags		INTEGER
		}
	ringswitchTLSPortNum OBJECT-TYPE
		SYNTAX	INTEGER (1..65535)
		ACCESS	read-only
		STATUS	mandatory
		DESCRIPTION
			"The port number of the port for which this entry
			contains LEG information."
		::= { ringswitchTLSPortEntry 1 }
	ringswitchTLSPortName OBJECT-TYPE
		SYNTAX	DisplayString
		ACCESS	read-write
		STATUS	mandatory
		DESCRIPTION
			"The name that the administrator has given 
                         this subnet."
		::= { ringswitchTLSPortEntry 2 }
	ringswitchTLSPortMTU OBJECT-TYPE
		SYNTAX	INTEGER (64..18432)
		ACCESS	read-write
		STATUS	mandatory
		DESCRIPTION
			"The maximum frame size allowed on this subnet.  The
                         value must not exceed the maximum frame size of the
                         switch."
		::= { ringswitchTLSPortEntry 3 }
	ringswitchTLSPortEnable OBJECT-TYPE
		SYNTAX	INTEGER	{
				enable		(1),
				disable		(2)
				}
		ACCESS	read-write
		STATUS	mandatory
		DESCRIPTION
			"Enable a port to use it.  Disable a port to
                         configure it.  A newly created port will be
                         disabled."
		::= { ringswitchTLSPortEntry 4 }
	ringswitchTLSPortIpAddress OBJECT-TYPE
		SYNTAX	IpAddress
		ACCESS	read-write
		STATUS	mandatory
		DESCRIPTION
			"IP Address of the port.  Used in conjunction with
                         the IP subnet mask to define the subnet for the
                         port."
		::= { ringswitchTLSPortEntry 5 }
	ringswitchTLSPortIpSubnetMask OBJECT-TYPE
		SYNTAX	IpAddress
		ACCESS	read-write
		STATUS	mandatory
		DESCRIPTION
			"IP subnet mask for the port.  Used in conjunction
                         with the port IP address to define the subnet for
                         the port."
		::= { ringswitchTLSPortEntry 6 }
	ringswitchTLSPortRIPRxType OBJECT-TYPE
                SYNTAX   INTEGER { rip1 (1),
                                   rip2 (2),
                                   rip1OrRip2 (3),
                                   doNotReceive (4)
                                 }
		ACCESS	read-write
		STATUS	mandatory
		DESCRIPTION
                    "This indicates which version of RIP updates
                     are to be accepted.  Note that rip2 and
                     rip1OrRip2 implies reception of multicast
                     packets."
		::= { ringswitchTLSPortEntry 7 }
	ringswitchTLSPortRIPTxType OBJECT-TYPE
                SYNTAX   INTEGER { doNotSend (1),
                                   ripVersion1 (2),
                                   rip1Compatible (3),
                                   ripVersion2 (4)
                                 }
		ACCESS	read-write
		STATUS	mandatory
		DESCRIPTION
                    "What the router sends on this interface.
                     ripVersion1 implies sending RIP updates compliant
                     with  RFC  1058.   rip1Compatible implies
                     broadcasting RIP-2 updates using RFC 1058 route
                     subsumption rules.  ripVersion2 implies
                     multicasting RIP-2 updates."
		::= { ringswitchTLSPortEntry 8 }
	ringswitchTLSPortRIPFlags OBJECT-TYPE
                SYNTAX   INTEGER (0..65535)
		ACCESS	read-write
		STATUS	mandatory
		DESCRIPTION
                     "A bit mask of flags to control RIP operation.  By
                      default, all defined bits are set to 1.  Bit 0 (LSB)
                      says announce default routes, bit 1 says announce
                      host routes, bit 2 says announce static routes, bit 4
                      says learn default routes, bit 5 says learn host routes,
                      bit 8 enables route hold-down, bit 9 enables split 
                      horizon, bit 10 enables poison reverse, and bit 11 
                      enable route summarising."
		::= { ringswitchTLSPortEntry 9 }
	ringswitchTLSPortIfArray OBJECT-TYPE
		SYNTAX	OCTET STRING
		ACCESS	read-write
		STATUS	mandatory
		DESCRIPTION
			"An octet string containing every interface port
			 number in this subnet, one port (1-64) per octet."
		::= { ringswitchTLSPortEntry 10 }
	ringswitchTLSPortRIP2AuthType OBJECT-TYPE
                SYNTAX   INTEGER {
                                    noAuthentication (1),
                                    simplePassword (2),
                                    md5 (3)
                                 }
		ACCESS	read-write
		STATUS	mandatory
		DESCRIPTION
                    "The RIP-v2 authentification type.  Valid values are
                     noAuthentication, simplePassword or md5."
		::= { ringswitchTLSPortEntry 11 }
	ringswitchTLSPortRIP2AuthKey OBJECT-TYPE
		SYNTAX	OCTET STRING (SIZE(0..16))
		ACCESS	read-write
		STATUS	mandatory
		DESCRIPTION
                     "The value to be used as the Authentication Key
                     whenever the corresponding instance of
                     ringswitchTLSPortRIP2AuthTypee has a value other than
                     noAuthentication.   If a string shorter
                     than 16 octets is supplied, it will be left-
                     justified and padded to 16 octets, on the right,
                     with nulls (0x00).
                     Reading this object always results in an  OCTET
                     STRING of length zero."
		::= { ringswitchTLSPortEntry 12 }
	ringswitchTLSPortOSPFEnable OBJECT-TYPE
		SYNTAX	INTEGER	{
				enable		(1),
				disable		(2)
				}
		ACCESS	read-write
		STATUS	mandatory
		DESCRIPTION
			"Enable a port to use OSPF."
		::= { ringswitchTLSPortEntry 13 }
	ringswitchTLSPortOSPFAreaId OBJECT-TYPE
		SYNTAX	IpAddress
		ACCESS	read-write
		STATUS	mandatory
		DESCRIPTION
			"OSPF area identifier for the port."
		::= { ringswitchTLSPortEntry 14 }
	ringswitchTLSPortLegFlags OBJECT-TYPE
		SYNTAX	INTEGER	(0..65535)
		ACCESS	read-write
		STATUS	mandatory
		DESCRIPTION
			"Bits 1 and 0 Configure how to send IP multicast frames
                         over Token Ring.  00 means use broadcast.  01 means
                         use the functional address C000-0004-0000.  02 means
                         use the Group address as on Ethernet."
		::= { ringswitchTLSPortEntry 15 }
	ringswitchTLSPortOSPFAuthType OBJECT-TYPE
                SYNTAX   INTEGER {
                                    noAuthentication (1),
                                    simplePassword (2),
                                    md5 (3)
                                 }
		ACCESS	read-write
		STATUS	mandatory
		DESCRIPTION
                    "The OSPF authentification type.  Valid values are
                     noAuthentication, simplePassword or md5."
		::= { ringswitchTLSPortEntry 16 }
	ringswitchTLSPortOSPFAuthMD5KeyId OBJECT-TYPE
                SYNTAX   INTEGER (0 .. 255)
		ACCESS	read-write
		STATUS	mandatory
		DESCRIPTION
                    "The OSPF MD5 key id.  This is a byte value of the
                    MD5 key id.  It is ignored if
                    ringswitchTLSPortOSPFAuthType is not md5."
		::= { ringswitchTLSPortEntry 17 }
	ringswitchTLSPortOSPFAuthKey OBJECT-TYPE
		SYNTAX	OCTET STRING (SIZE(0..16))
		ACCESS	read-write
		STATUS	mandatory
		DESCRIPTION
                     "The value to be used as the Authentication Key
                     whenever the corresponding instance of
                     ringswitchTLSPortOSPFAuthType has a value other than
                     noAuthentication.   If a string shorter
                     than 16 octets is supplied, it will be left-
                     justified and padded to 16 octets, on the right,
                     with nulls (0x00).

                     Simple passwords use 8 characters.
                     MD5 passwords use 16 characters.

                     Reading this object always results in an  OCTET
                     STRING of length zero."
		::= { ringswitchTLSPortEntry 18 }
	ringswitchTLSPortOSPFCost OBJECT-TYPE
                SYNTAX   INTEGER (1 .. 65535)
		ACCESS	read-write
		STATUS	mandatory
		DESCRIPTION
                    "The OSPF cost of sending a packet
		     from this interface. Default value is 1.
		     A value of 0 is not allowed."
		::= { ringswitchTLSPortEntry 19 }
	ringswitchTLSPortOSPFPriority OBJECT-TYPE
                SYNTAX   INTEGER (0 .. 65535)
		ACCESS	read-write
		STATUS	mandatory
		DESCRIPTION
                    "The priority of this interface when
		     deciding which interface on the attached
	    	     network should become the designated router.
		     A value of 0 will prevent the interface
		     from ever becoming the designated router.
		     Default value is 1."
		::= { ringswitchTLSPortEntry 20 }
	ringswitchTLSPortOSPFHelloInterval OBJECT-TYPE
                SYNTAX   INTEGER
		ACCESS	read-write
		STATUS	mandatory
		DESCRIPTION
                    "The length of time in seconds between
		     sending OSPF hello packets from this interface.
		     Default value is 10. A value of 0 is not allowed."
		::= { ringswitchTLSPortEntry 21 }
	ringswitchTLSPortOSPFDeadInterval OBJECT-TYPE
                SYNTAX   INTEGER
		ACCESS	read-write
		STATUS	mandatory
		DESCRIPTION
                    "The length of time in seconds before
		     neighbors declare the router down after ceasing
		     to hear a hello packet. It is recommended that
		     this be set to 4 times the hello interval. 
		     Default value is 40. The dead interval must be 
		     longer than the hello interval."
		::= { ringswitchTLSPortEntry 22 }
	ringswitchTLSPortBootPFlags OBJECT-TYPE
		SYNTAX	INTEGER	{
				relay-agent-enable	(1),
				relay-agent-disable	(0)
				}
		ACCESS	read-write
		STATUS	mandatory
		DESCRIPTION
                    "Whether the interface forwards BootP and
		DHCP requests or not. Default is disabled."
		::= { ringswitchTLSPortEntry 23 }

-- The TLS static route group
	ringswitchTLSStaticRouteTable OBJECT-TYPE
		SYNTAX	SEQUENCE OF RingswitchTLSStaticRouteEntry
		ACCESS	not-accessible
		STATUS	mandatory
		DESCRIPTION
			"A table that contains a list of static routes."
		::= { ringswitchTLS 3 }
	ringswitchTLSStaticRouteEntry OBJECT-TYPE
		SYNTAX	RingswitchTLSStaticRouteEntry
		ACCESS	not-accessible
		STATUS	mandatory
		DESCRIPTION
			"An that contains information about one static
			route."
		INDEX { ringswitchTLSStaticRouteCardNum ,
                        ringswitchTLSStaticRouteIpAddress
                      }
		::= { ringswitchTLSStaticRouteTable 1 }
	RingswitchTLSStaticRouteEntry ::=
		SEQUENCE
                {
                        ringswitchTLSStaticRouteCardNum   INTEGER,
                        ringswitchTLSStaticRouteIpAddress IpAddress,
                        ringswitchTLSStaticRouteMask      IpAddress,
                        ringswitchTLSStaticRouteGateway   IpAddress,
                        ringswitchTLSStaticRouteMetric    INTEGER
		}
	ringswitchTLSStaticRouteCardNum OBJECT-TYPE
		SYNTAX	INTEGER
		ACCESS	read-only
		STATUS	mandatory
		DESCRIPTION
			"The TLS card number using a static route."
		::= { ringswitchTLSStaticRouteEntry 1 }
	ringswitchTLSStaticRouteIpAddress OBJECT-TYPE
		SYNTAX	IpAddress
		ACCESS	read-only
		STATUS	mandatory
		DESCRIPTION
			"The IP address of a static route."
		::= { ringswitchTLSStaticRouteEntry 2 }
	ringswitchTLSStaticRouteMask OBJECT-TYPE
		SYNTAX	IpAddress
		ACCESS	read-write
		STATUS	mandatory
		DESCRIPTION
			"The IP subnet mask to use in a static route."
		::= { ringswitchTLSStaticRouteEntry 3 }
	ringswitchTLSStaticRouteGateway OBJECT-TYPE
		SYNTAX	IpAddress
		ACCESS	read-write
		STATUS	mandatory
		DESCRIPTION
			"The IP address of a gateway to use in a static route.
                         This table is indexed by the TLS card and the IP
                         address of the route."
		::= { ringswitchTLSStaticRouteEntry 4 }
	ringswitchTLSStaticRouteMetric OBJECT-TYPE
		SYNTAX	INTEGER (0..16)
		ACCESS	read-write
		STATUS	mandatory
		DESCRIPTION
			"The metric to use for a static route.  Setting the
                         value to zero will delete the entry.  Setting it to a
                         non-zero value will cause the route to be registered,
                         so set this field after the gateway and mask."
		::= { ringswitchTLSStaticRouteEntry 5 }

-- the TLS RIP neighbours group
	ringswitchTLSRIPNeighbourTable OBJECT-TYPE
		SYNTAX	SEQUENCE OF RingswitchTLSRIPNeighbourEntry
		ACCESS	not-accessible
		STATUS	mandatory
		DESCRIPTION
			"A table that contains a list of static routes."
		::= { ringswitchTLS 4 }
	ringswitchTLSRIPNeighbourEntry OBJECT-TYPE
		SYNTAX	RingswitchTLSRIPNeighbourEntry
		ACCESS	not-accessible
		STATUS	mandatory
		DESCRIPTION
			"An that contains information about one static
			route."
		INDEX { ringswitchTLSRIPNeighbourCardNum ,
                        ringswitchTLSRIPNeighbourIpAddress
                      }
		::= { ringswitchTLSRIPNeighbourTable 1 }
	RingswitchTLSRIPNeighbourEntry ::=
		SEQUENCE
                {
                        ringswitchTLSRIPNeighbourCardNum   INTEGER,
                        ringswitchTLSRIPNeighbourIpAddress IpAddress,
                        ringswitchTLSRIPNeighbourEnable    INTEGER
		}
	ringswitchTLSRIPNeighbourCardNum OBJECT-TYPE
		SYNTAX	INTEGER
		ACCESS	read-only
		STATUS	mandatory
		DESCRIPTION
			"The TLS card number using a RIP Neighbour."
		::= { ringswitchTLSRIPNeighbourEntry 1 }
	ringswitchTLSRIPNeighbourIpAddress OBJECT-TYPE
		SYNTAX	IpAddress
		ACCESS	read-only
		STATUS	mandatory
		DESCRIPTION
			"The IP address of a RIP Neighbour."
		::= { ringswitchTLSRIPNeighbourEntry 2 }
	ringswitchTLSRIPNeighbourEnable OBJECT-TYPE
		SYNTAX	INTEGER	{
				enable		(1),
				disable		(2)
				}
		ACCESS	read-write
		STATUS	mandatory
		DESCRIPTION
                        "Setting to 1 marks the given IP address as a RIP 
                         neighbour. Setting to 2 removes the IP address from 
                         the list."
		::= { ringswitchTLSRIPNeighbourEntry 3 }

