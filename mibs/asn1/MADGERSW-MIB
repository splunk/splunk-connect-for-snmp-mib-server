-- the TLS OSPF areas group
	ringswitchTLSOSPFAreaTable OBJECT-TYPE
		SYNTAX	SEQUENCE OF RingswitchTLSOSPFAreaEntry
		ACCESS	not-accessible
		STATUS	mandatory
		DESCRIPTION
			"A table that contains information about each
			 OSPF area for a TLS."
		::= { ringswitchTLS 7 }
	ringswitchTLSOSPFAreaEntry OBJECT-TYPE
		SYNTAX	RingswitchTLSOSPFAreaEntry
		ACCESS	not-accessible
		STATUS	mandatory
		DESCRIPTION
			"An entry containing information about one OSPF area."
		INDEX { ringswitchTLSOSPFAreaId }
		::= { ringswitchTLSOSPFAreaTable 1 }
	RingswitchTLSOSPFAreaEntry ::=
		SEQUENCE
                {
			ringswitchTLSOSPFAreaCardNum      INTEGER,
			ringswitchTLSOSPFAreaId           IpAddress,
			ringswitchTLSOSPFAreaAddress      IpAddress,
			ringswitchTLSOSPFAreaAddressMask  IpAddress,
			ringswitchTLSOSPFAreaFlags        INTEGER
		}
	ringswitchTLSOSPFAreaCardNum OBJECT-TYPE
		SYNTAX	INTEGER
		ACCESS	read-only
		STATUS	mandatory
		DESCRIPTION
			"TLS card for OSPF area."
		::= { ringswitchTLSOSPFAreaEntry 1 }
	ringswitchTLSOSPFAreaId OBJECT-TYPE
		SYNTAX	IpAddress
		ACCESS	read-only
		STATUS	mandatory
		DESCRIPTION
			"OSPF area identifier."
		::= { ringswitchTLSOSPFAreaEntry 2 }
	ringswitchTLSOSPFAreaAddress OBJECT-TYPE
		SYNTAX	IpAddress
		ACCESS	read-only
		STATUS	mandatory
		DESCRIPTION
			"OSPF area address."
		::= { ringswitchTLSOSPFAreaEntry 3 }
	ringswitchTLSOSPFAreaAddressMask OBJECT-TYPE
		SYNTAX	IpAddress
		ACCESS	read-write
		STATUS	mandatory
		DESCRIPTION
			"OSPF area address mask."
		::= { ringswitchTLSOSPFAreaEntry 4 }
	ringswitchTLSOSPFAreaFlags OBJECT-TYPE
		SYNTAX	INTEGER	(0..32767)
		ACCESS	read-write
		STATUS	mandatory
		DESCRIPTION
			"OSPF area flags.  Bit 0 indicates address range 
                         active, Bit 1 indicates advertise address range, 
                         and bit 2 indicates external routing capability.
                         Bit 15 is a destroy area flag.  Setting this bit 
                         will cause the area to be deleted."
		::= { ringswitchTLSOSPFAreaEntry 5 }

-- The TLS BootP Relay Agent server group
	ringswitchTLSBootPRAServerTable OBJECT-TYPE
		SYNTAX	SEQUENCE OF RingswitchTLSBootPRAServerEntry
		ACCESS	not-accessible
		STATUS	mandatory
		DESCRIPTION
			"A table containing BootP/DHCP servers
		         that the BootP Relay Agent can forward BootP/DHCP
		         packets to. BootP/DHCP requests will be shared
		         amongst the servers in this table unless the
		         requests specify a particular server name."
		::= { ringswitchTLS 8 }
	ringswitchTLSBootPRAServerEntry OBJECT-TYPE
		SYNTAX	RingswitchTLSBootPRAServerEntry
		ACCESS	not-accessible
		STATUS	mandatory
		DESCRIPTION
			"Contains information about the BootP/DHCP
			 server."
		INDEX { ringswitchTLSBootPRAServerCardNum ,
                ringswitchTLSBootPRAServerIpAddress }
		::= { ringswitchTLSBootPRAServerTable 1 }
	RingswitchTLSBootPRAServerEntry ::=
		SEQUENCE
                {
			ringswitchTLSBootPRAServerCardNum   INTEGER,
                        ringswitchTLSBootPRAServerIpAddress IpAddress,
			ringswitchTLSBootPRAServerName	    OCTET STRING (SIZE (64)),
			ringswitchTLSBootPRAServerEnable    INTEGER
		}
	ringswitchTLSBootPRAServerCardNum OBJECT-TYPE
		SYNTAX	INTEGER
		ACCESS	read-only
		STATUS	mandatory
		DESCRIPTION
			"The TLS card number using this BootP/DHCP server."
		::= { ringswitchTLSBootPRAServerEntry 1 }
	ringswitchTLSBootPRAServerIpAddress OBJECT-TYPE
		SYNTAX	IpAddress
		ACCESS	read-only
		STATUS	mandatory
		DESCRIPTION
			"The IP address of this BootP/DHCP server."
		::= { ringswitchTLSBootPRAServerEntry 2 }
	ringswitchTLSBootPRAServerName OBJECT-TYPE
		SYNTAX	OCTET STRING (SIZE (64))
		ACCESS	read-write
		STATUS	mandatory
		DESCRIPTION
			"The name of the BootP/DHCP server. Clients
	 	        specifying this name in their BootP/DHCP requests
        		have their request frames sent on to this particular
        		server."
		::= { ringswitchTLSBootPRAServerEntry 3 }
	ringswitchTLSBootPRAServerEnable OBJECT-TYPE
		SYNTAX	INTEGER	{
				enable		(1),
				disable		(2)
				}
		ACCESS	read-write
		STATUS	mandatory
		DESCRIPTION
			"Setting to 1 adds a BootP Relay Agent server with 
                        this port and IP address to the list of BootP Relay 
                        Agent servers. Setting to 2 removes this BootP Relay 
                        Agent Server from the list."
		::= { ringswitchTLSBootPRAServerEntry 4 }

-- The TLS VRRP virtual router group
	ringswitchTLSVRRPVRTRTable OBJECT-TYPE
		SYNTAX	SEQUENCE OF RingswitchTLSVRRPVRTREntry
		ACCESS	not-accessible
		STATUS	mandatory
		DESCRIPTION
			"A table containing a list of VRRP virtual routers."
		::= { ringswitchTLS 9 }
	ringswitchTLSVRRPVRTREntry OBJECT-TYPE
		SYNTAX	RingswitchTLSVRRPVRTREntry
		ACCESS	not-accessible
		STATUS	mandatory
		DESCRIPTION
			"Contains information about the
			virtual router."
		INDEX { ringswitchTLSVRRPVRTRPortNum ,
			ringswitchTLSVRRPVRTRIpAddress
                      }
		::= { ringswitchTLSVRRPVRTRTable 1 }
	RingswitchTLSVRRPVRTREntry ::=
		SEQUENCE
                {
            ringswitchTLSVRRPVRTRPortNum        INTEGER,
            ringswitchTLSVRRPVRTRIpAddress      IpAddress,
            ringswitchTLSVRRPVRTRVRID           INTEGER,
			ringswitchTLSVRRPVRTRMacAddress     TRNMacAddress,
			ringswitchTLSVRRPVRTREnable   	    INTEGER,
			ringswitchTLSVRRPVRTRAdvertInterval INTEGER,
			ringswitchTLSVRRPVRTRPriority	    INTEGER,
			ringswitchTLSVRRPVRTRAuthType	    INTEGER,
			ringswitchTLSVRRPVRTRAuthKey	    OCTET STRING (SIZE (8)),
			ringswitchTLSVRRPVRTRAdvertDeadInterval INTEGER
				}
	ringswitchTLSVRRPVRTRPortNum OBJECT-TYPE
		SYNTAX	INTEGER
		ACCESS	read-only
		STATUS	mandatory
		DESCRIPTION
			"The TLS leg port that this virtual
		router is configured to use. There can be a
		maximum of 1 virtual router per leg port."
		::= { ringswitchTLSVRRPVRTREntry 1 }
	ringswitchTLSVRRPVRTRIpAddress OBJECT-TYPE
		SYNTAX	IpAddress
		ACCESS	read-only
		STATUS	mandatory
		DESCRIPTION
			"The IP address of this VRRP virtual
		router. This must be a unique IP address on
		the same subnet as the leg port that the
		virtual router has been	configured to use."
		::= { ringswitchTLSVRRPVRTREntry 2 }
	ringswitchTLSVRRPVRTRVRID OBJECT-TYPE
		SYNTAX	INTEGER (1 .. 255)
		ACCESS	read-write
		STATUS	mandatory
		DESCRIPTION
			"The VRRP virtual router ID.
		Only 1 virtual router with this ID can
		be used on any single Ringswitch."
		::= { ringswitchTLSVRRPVRTREntry 3 }
	ringswitchTLSVRRPVRTRMacAddress OBJECT-TYPE
		SYNTAX	TRNMacAddress
		ACCESS	read-write
		STATUS	mandatory
		DESCRIPTION
			"This is the MAC address that the port
		will use for this VRRP virtual router. It will
		normally be derived from the virtual router VRID
		using the formula 00-00-7A-00-80-<bitswapped VRID>."
		::= { ringswitchTLSVRRPVRTREntry 4 }
	ringswitchTLSVRRPVRTREnable OBJECT-TYPE
		SYNTAX	INTEGER	{
				enable		(1),
				disable		(2)
				}
		ACCESS	read-write
		STATUS	mandatory
		DESCRIPTION
			"Setting to 1 adds a VRRP Virtual Router with this
		port and IP address to the list of VRRP virtual routers.
		Setting to 2 removes this virtual router from the list."
		::= { ringswitchTLSVRRPVRTREntry 5 }
	ringswitchTLSVRRPVRTRAdvertInterval OBJECT-TYPE
		SYNTAX	INTEGER
		ACCESS	read-write
		STATUS	mandatory
		DESCRIPTION
			"The length of time in seconds between
		sending VRRP advertisements. Default is 1 second.
		The value 0 is not allowed."
		::= { ringswitchTLSVRRPVRTREntry 6 }
	ringswitchTLSVRRPVRTRPriority OBJECT-TYPE
		SYNTAX	INTEGER (0 .. 255)
		ACCESS	read-write
		STATUS	mandatory
		DESCRIPTION
			"Priority value used for electing the
		master router for a VRRP virtual router. Default
		value is 100. A value of 255 will force this node
		to act as the master for this virtual router. A
		value of 0 will force this node to act as a 
		backup."
		::= { ringswitchTLSVRRPVRTREntry 7 }
	ringswitchTLSVRRPVRTRAuthType OBJECT-TYPE
		SYNTAX	INTEGER	{
				none		(1),
				simple		(2)
				}
		ACCESS	read-write
		STATUS	mandatory
		DESCRIPTION
			"The type of authentication used
		by VRRP on this port."
		::= { ringswitchTLSVRRPVRTREntry 8 }
	ringswitchTLSVRRPVRTRAuthKey OBJECT-TYPE
		SYNTAX	OCTET STRING (SIZE (8))
		ACCESS	read-write
		STATUS	mandatory
		DESCRIPTION
			"The authentication key to be used
		if ringswitchTLSVRRPVRTRAuthType is set to
		simple."
		::= { ringswitchTLSVRRPVRTREntry 9 }
	ringswitchTLSVRRPVRTRAdvertDeadInterval OBJECT-TYPE
		SYNTAX	INTEGER
		ACCESS	read-write
		STATUS	mandatory
		DESCRIPTION
			"The length of time in seconds after which
        		if no VRRP advert has been received by the master
	        	VRRP router then it will change to backup mode.
		        Default is 4 seconds. The value 0 is not allowed.
        		It is recommended that this value be 4 times the
	        	value set as the AdvertInterval."
		::= { ringswitchTLSVRRPVRTREntry 10 }

----------------------------------------------------------------------------
-- the Ethernet port group

	ringswitchEthernetIfTable OBJECT-TYPE
		SYNTAX  SEQUENCE OF RingswitchEthernetIfEntry
		ACCESS  not-accessible
		STATUS  mandatory
		DESCRIPTION
			"This table contains Ethernet interface parameters 
                        and state variables applying to each ethernet port, 
                        and Ethernet-type interface parameters to configure 
                        translational bridging to Token Ring."
		::= { ringswitchEthernet 1}

	ringswitchEthernetIfEntry OBJECT-TYPE
		SYNTAX  RingswitchEthernetIfEntry
		ACCESS  not-accessible
		STATUS  mandatory
		DESCRIPTION
			"A list of Ethernet status and parameter values for 
                        a ethernet interface, plus a list of Ethernet-type 
                        interface parameters to configure translational 
                        bridging to Token-Ring"
		INDEX { ringswitchEthernetIfIndex }
		::= { ringswitchEthernetIfTable 1 }

	RingswitchEthernetIfEntry ::=
                SEQUENCE        {
                                  ringswitchEthernetIfIndex
                                        INTEGER,
                                  ringswitchEthernetIfIPXEthType
                                        INTEGER,
                                  ringswitchEthernetIfOperIPXEthType
                                        INTEGER,
                                  ringswitchEthernetIfIPXTRType
                                        INTEGER,
                                  ringswitchEthernetIfOperIPXTRType
                                        INTEGER,
                                  ringswitchEthernetIfIPXEnable
                                        INTEGER,
                                  ringswitchEthernetIfIPEnable
                                        INTEGER,
                                  ringswitchEthernetIfAdminSpeed
				        INTEGER,	
                                  ringswitchEthernetIfOperSpeed         
				        INTEGER,
                                  ringswitchEthernetIfAdminDuplexMode
				        INTEGER,
                                  ringswitchEthernetIfOperDuplexMode          
				        INTEGER,
                                  ringswitchEthernetIfIPMulticastType	
				        INTEGER,
                                  ringswitchEthernetIfCacheClear
				        INTEGER,
                                  ringswitchEthernetIfSPTEncaps
				        INTEGER,
                                  ringswitchEthernetIfPriorityTagging  
				        INTEGER,
                                  ringswitchEthernetIfPriorityTaggingVLANId   
				        INTEGER,
                                  ringswitchEthernetIfAdminIPXNetwork
                                        INTEGER,
                                  ringswitchEthernetIfOperIPXNetwork
				        INTEGER,
                                  ringswitchEthernetIfSTPBlockingTrapInfo
                                        INTEGER,
                                  ringswitchEthernetIfAdminFlowCtrl
                                        INTEGER,
                                  ringswitchEthernetIfOperFlowCtrl
                                        INTEGER,
                                  ringswitchEthernetIfAutoNegotiate
                                        INTEGER,
                                  ringswitchEthernetIfMediaType
                                        INTEGER,
                                  ringswitchEthernetIfAdminMaxFrameSize
                                        INTEGER,
                                  ringswitchEthernetIfOperMaxFrameSize
                                        INTEGER
                                }

	ringswitchEthernetIfIndex OBJECT-TYPE
		SYNTAX  INTEGER (1..65535)
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"The port number of the Ethernet port for which 
                        this entry contains Ethernet information."
		::= { ringswitchEthernetIfEntry 1 }

	ringswitchEthernetIfIPXEthType OBJECT-TYPE
		SYNTAX  INTEGER {
				auto            (1),
				ieee8022        (2),
				snap            (3),
				ieee8023        (4),
				dix             (5)
				}
		ACCESS  read-write
		STATUS  mandatory
		DESCRIPTION
			"This variable specifies the encapsulation type on
			 the Ethernet card for all forwarded IPX frames 
                         from a TRN."
		::= { ringswitchEthernetIfEntry 2 }

	ringswitchEthernetIfOperIPXEthType OBJECT-TYPE
		SYNTAX	INTEGER	{
				unknown (1),
				ieee8022(2),
				snap    (3),
				ieee8023(4),
				dix     (5)
				}
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION	
			"This variable specifies the actual encapsulation 
			type for all IPX frames forwarded onto Ethernet"
		::= { ringswitchEthernetIfEntry 3 }

	ringswitchEthernetIfIPXTRType OBJECT-TYPE
		SYNTAX  INTEGER {
				auto            (1),
				ieee8022        (2),
				snap            (3)
				}
		ACCESS  read-write
		STATUS  mandatory
		DESCRIPTION
			"This variable specifies the encapsulation type on 
			TRN for all IPX frames forwarded from an Ethernet port"
		::= { ringswitchEthernetIfEntry 4 }

	ringswitchEthernetIfOperIPXTRType OBJECT-TYPE
		SYNTAX	INTEGER	{
				unknown (1),
				ieee8022(2),
				snap    (3)
				}
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION	       			    		
			"This variable specifies the actual encapsulation 
			type for all IPX frames forwarded onto Token Ring"
		::= { ringswitchEthernetIfEntry 5 }

	ringswitchEthernetIfIPXEnable OBJECT-TYPE
		SYNTAX  INTEGER {
				enable          (1),
				disable         (2)
				}
		ACCESS  read-write
		STATUS  mandatory
		DESCRIPTION
			"If this variable is enabled on an Ethernet port 
			 then the port will do translational bridging of IPX 
			 frames, otherwise it will discard them.  It is 
			 ignored for Token Ring ports"
		::= { ringswitchEthernetIfEntry 6 }

	ringswitchEthernetIfIPEnable OBJECT-TYPE
		SYNTAX  INTEGER {
				enable          (1),
				disable         (2)
				}
		ACCESS  read-write
		STATUS  mandatory
		DESCRIPTION
			"If this variable is enabled on an Ethernet port 
			 then the port will do translational bridging of IP,
			 ARP and RARP frames, otherwise it will discard them. 
			 It is ignored for Token Ring ports"
		::= { ringswitchEthernetIfEntry 7 }

	ringswitchEthernetIfAdminSpeed OBJECT-TYPE
		SYNTAX  INTEGER {
				auto               (1),
				speed10Mbps        (2),
                                speed100Mbps       (3),
                                speed1000Mbps      (4)
				}
		ACCESS  read-write
		STATUS  mandatory
		DESCRIPTION
			"This variable specifies the speed that the Ethernet 
                         port will open at."
		::= { ringswitchEthernetIfEntry 8 }

	ringswitchEthernetIfOperSpeed OBJECT-TYPE
		SYNTAX  INTEGER {
				unknown  (1), 
				ten      (2),
				hundred  (3),
                                thousand (4)
				}
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"The current operational speed of the network to 
			which this interface is connected."
		::= { ringswitchEthernetIfEntry 9 }

	ringswitchEthernetIfAdminDuplexMode OBJECT-TYPE
		SYNTAX  INTEGER {
                                auto             (1),
                                half             (2),
                                full             (3)
				}
		ACCESS  read-write
		STATUS  mandatory
		DESCRIPTION
			"This variable specifies the duplex mode of the 
                         Ethernet port."
		::= { ringswitchEthernetIfEntry 10 }

	ringswitchEthernetIfOperDuplexMode OBJECT-TYPE
		SYNTAX  INTEGER {
				unknown         (1),
				half-duplex     (2),
				full-duplex     (3)
				}
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"The current operational mode of this interface."
		::= { ringswitchEthernetIfEntry 11 }

	ringswitchEthernetIfIPMulticastType OBJECT-TYPE
		SYNTAX	INTEGER {
				broadcast (1),
				functional(2),
				group     (3)
				}
		ACCESS  read-write
		STATUS  mandatory
		DESCRIPTION
			"This variable specifies the type of MAC address to
			use as the destination for IP multicast frames on
			Token Ring"
		::= { ringswitchEthernetIfEntry 12 }
					
	ringswitchEthernetIfCacheClear OBJECT-TYPE
		SYNTAX	INTEGER {
				clear (1)
				}
		ACCESS  write-only
		STATUS  mandatory
		DESCRIPTION
			"This variable clears the RIF cache"
		::= { ringswitchEthernetIfEntry 13 }
					
	ringswitchEthernetIfSPTEncaps OBJECT-TYPE  
		SYNTAX	INTEGER	{
				enabled	(1),
				disabled(2)
				}
		ACCESS  read-write
		STATUS  mandatory
		DESCRIPTION
			"This variable specifies whether BPDU's received
			from Ethernet are encapsulated when forwared onto
			Token Ring. Encapsulated BPDU's are de-encapsulated
			when forwarded to Ethernet"
		::= { ringswitchEthernetIfEntry 14 }

	ringswitchEthernetIfPriorityTagging OBJECT-TYPE  
		SYNTAX	INTEGER	{
				enabled	(1),
				disabled(2)
				}
		ACCESS  read-write
		STATUS  mandatory
		DESCRIPTION
                        "When this variable is enabled, frames onto ethernet
                        ring are priority tagged (using 802.1p) with the 
                        token ring priority"
		::= { ringswitchEthernetIfEntry 15 }

	ringswitchEthernetIfPriorityTaggingVLANId OBJECT-TYPE  
		SYNTAX	INTEGER	(0..4095)
		ACCESS  read-write
		STATUS  mandatory
		DESCRIPTION
                        "VLAN ID to be used when 
                        ringswitchEthernetIfPriorityTagging is enabled."
		::= { ringswitchEthernetIfEntry 16 }

	ringswitchEthernetIfAdminIPXNetwork OBJECT-TYPE
		SYNTAX	INTEGER
		ACCESS  read-write
		STATUS  mandatory
		DESCRIPTION
			"The IPX network number of the port. If this is
			zero then the IPX network number is learned 
			automatically."
		::= { ringswitchEthernetIfEntry 17 }

	ringswitchEthernetIfOperIPXNetwork OBJECT-TYPE
		SYNTAX	INTEGER
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION	    				
			"The actual IPX network number of the port."
		::= { ringswitchEthernetIfEntry 18 }

	ringswitchEthernetIfSTPBlockingTrapInfo OBJECT-TYPE
		SYNTAX	INTEGER {
                blocked   (1),
                unblocked (2)
                }
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"Trap set if circulating encapsulated BPDU frames 
                         are detected (indicating a 3rd party bridge) which 
                         cause the port to go into blocking mode. A trap is
			 also sent if the port becomes unblocked after 
                         previously being blocked."
		::= { ringswitchEthernetIfEntry 19 }

	ringswitchEthernetIfAdminFlowCtrl OBJECT-TYPE
		SYNTAX  INTEGER {
				not-applicable        (1),
				no-pause              (2),
                                tx-pause              (3),
                                rx-pause              (4),
                                tx-rx-pause           (5)
				}
		ACCESS  read-write
		STATUS  mandatory
		DESCRIPTION
			"This variable specifies the flow control abilities
                         that the Gigabit port will use. For 10/100 Ethernet 
                         ports this variable will always be not-applicable."
		::= { ringswitchEthernetIfEntry 20 }

	ringswitchEthernetIfOperFlowCtrl OBJECT-TYPE
		SYNTAX  INTEGER {
				unknown               (1),
				no-pause              (2),
                                tx-pause              (3),
                                rx-pause              (4),
                                tx-rx-pause           (5)
				}
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"The current flow control operational mode for the
                         Gigabit interface. For 10/100 Ethernet ports this 
                         variable will always be unknown."
		::= { ringswitchEthernetIfEntry 21 }

	ringswitchEthernetIfAutoNegotiate OBJECT-TYPE  
		SYNTAX	INTEGER	{
				enabled        (1),
				disabled       (2),
                                not-applicable (3)
				}
		ACCESS  read-write
		STATUS  mandatory
		DESCRIPTION
			"This variable specifies whether the Gigabit
                        interface will auto-negotiate the link properties
                        or whether it will force the link up. For 10/100
                        Ethernet interfaces this variable will always be
                        not applicable. Auto-negotiation for 10/100 Ethernet
                        is done by setting the speed or duplex mode to
                        auto."
		::= { ringswitchEthernetIfEntry 22 }

	ringswitchEthernetIfMediaType OBJECT-TYPE  
		SYNTAX	INTEGER	{
                                copper         (1),
				fibre-sx       (2),
				fibre-lx       (3)
				}
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"This variable specifies the media type of the 
                        10/100 Ethernet or Gigabit port."
		::= { ringswitchEthernetIfEntry 23 }

	ringswitchEthernetIfAdminMaxFrameSize OBJECT-TYPE
		SYNTAX	INTEGER
		ACCESS  read-write
		STATUS  mandatory
		DESCRIPTION	    				
			"The maximum frame size requested for the Ethernet
                        or Gigabit port. For 10/100 Ethernet this value can 
                        only be set to 1514. For Gigabit other larger values
                        are supported for jumbo frames. Any changes to this
                        value will not take place until the switch is 
                        rebooted. The max size available will be dependent
                        on the switch buffer size - 5K or 18K."
		::= { ringswitchEthernetIfEntry 24 }

	ringswitchEthernetIfOperMaxFrameSize OBJECT-TYPE
		SYNTAX	INTEGER
		ACCESS  read-write
		STATUS  mandatory
		DESCRIPTION	    				
			"The maximum frame size currently used on the Ethernet
                        or Gigabit port. For 10/100 Ethernet this value can 
                        only be 1514. For Gigabit other larger values
                        are supported for jumbo frames."
		::= { ringswitchEthernetIfEntry 25 }

        

-- the ringswitch ethernet translational bridge duplicate address group
	
	ringswitchEthernetXBDupAddrTable OBJECT-TYPE
		SYNTAX  SEQUENCE OF RingswitchEthernetXBDupAddrEntry
		ACCESS  not-accessible
		STATUS  mandatory
		DESCRIPTION
			"This table duplicate mac address information"
		::= { ringswitchEthernet 2 } 

	ringswitchEthernetXBDupAddrEntry OBJECT-TYPE
		SYNTAX  RingswitchEthernetXBDupAddrEntry
		ACCESS  not-accessible
		STATUS  mandatory
		DESCRIPTION
			"A list of duplicate mac addresses for each Ethernet port in the rsw"
		INDEX { ringswitchEthernetXBDupAddrIfIndex,
			ringswitchEthernetXBDupAddrIndex }
		::= { ringswitchEthernetXBDupAddrTable 1 }

	RingswitchEthernetXBDupAddrEntry
			::= SEQUENCE
			{		     		
			ringswitchEthernetXBDupAddrIfIndex    INTEGER,
			ringswitchEthernetXBDupAddrIndex      INTEGER,
			ringswitchEthernetXBDupAddrMacAddress TRNMacAddress
			}		 		
			
	ringswitchEthernetXBDupAddrIfIndex OBJECT-TYPE
		SYNTAX  INTEGER
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"The value of this object identifies the
			ethernet interface for which this entry
			contains management information.  The
			value of this object for a particular
			interface has the same value as the
			ifIndex object for the same interface."
		::= { ringswitchEthernetXBDupAddrEntry 1 }
			
	ringswitchEthernetXBDupAddrIndex OBJECT-TYPE
		SYNTAX  INTEGER (1..10)
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"An arbitrary index ..."
		::= { ringswitchEthernetXBDupAddrEntry 2 }

	ringswitchEthernetXBDupAddrMacAddress OBJECT-TYPE   
		SYNTAX	TRNMacAddress	
		ACCESS  read-write
		STATUS  mandatory
		DESCRIPTION
			""
		::= { ringswitchEthernetXBDupAddrEntry 3 }

-- the ringswitch ethernet to tokenring translational bridge statistics group

	ringswitchEthernetXBToTRNStatsTable OBJECT-TYPE
		SYNTAX  SEQUENCE OF RingswitchEthernetXBToTRNStatsEntry
		ACCESS  not-accessible
		STATUS  mandatory
		DESCRIPTION
			"This table contains information relating to the
			forwarding statistics that are maintained by each
			ethernet interface acting as a translational
			bridge to Token-ring."
		::= { ringswitchEthernet 3 } 

	ringswitchEthernetXBToTRNStatsEntry OBJECT-TYPE
		SYNTAX  RingswitchEthernetXBToTRNStatsEntry
		ACCESS  not-accessible
		STATUS  mandatory
		DESCRIPTION		      				
			"A list of information relating to the forwarding
			statistics of a single ethernet interface acting 
			as a translational bridge to Token-ring."
		INDEX { ringswitchEthernetXBToTRNStatsIfIndex }
		::= { ringswitchEthernetXBToTRNStatsTable 1 }
				
	RingswitchEthernetXBToTRNStatsEntry
		::= SEQUENCE
		{		     		
		ringswitchEthernetXBToTRNStatsIfIndex            INTEGER,
		ringswitchEthernetXBToTRNStatsDiscSptEncapsBlock INTEGER48,
		ringswitchEthernetXBToTRNStatsDiscFilter	 INTEGER48,
                ringswitchEthernetXBToTRNStatsDiscBPDU		 INTEGER48,
		ringswitchEthernetXBToTRNStatsDiscBPDUEncaps     INTEGER48,
		ringswitchEthernetXBToTRNStatsDiscTag8021qCfi    INTEGER48,
		ringswitchEthernetXBToTRNStatsDiscTag8021qVlan   INTEGER48,
		ringswitchEthernetXBToTRNStatsDiscBadRif         INTEGER48,
		ringswitchEthernetXBToTRNStatsDiscHopLimit       INTEGER48,
		ringswitchEthernetXBToTRNStatsDiscDuplicateRin   INTEGER48,
		ringswitchEthernetXBToTRNStatsDiscIPDisabled     INTEGER48,
		ringswitchEthernetXBToTRNStatsDiscARPDisabled    INTEGER48,
		ringswitchEthernetXBToTRNStatsDiscNCPCache       INTEGER48,
		ringswitchEthernetXBToTRNStatsDiscIPXDisabled    INTEGER48,
		ringswitchEthernetXBToTRNStatsDiscIPXTypeSnap    INTEGER48,
		ringswitchEthernetXBToTRNStatsDiscIPXType8022    INTEGER48,
		ringswitchEthernetXBToTRNStatsDiscIPXType8023    INTEGER48,
		ringswitchEthernetXBToTRNStatsDiscIPXTypeDix     INTEGER48,

		ringswitchEthernetXBToTRNStatsNoFakeBuffer       INTEGER48,
		ringswitchEthernetXBToTRNStatsCacheFull          INTEGER48,

		ringswitchEthernetXBToTRNStatsFwdIPFrames	 INTEGER48,
		ringswitchEthernetXBToTRNStatsFwdIPMulticast	 INTEGER48,
		ringswitchEthernetXBToTRNStatsFwdARPFrames	 INTEGER48,
		ringswitchEthernetXBToTRNStatsFwdIPXFrames	 INTEGER48,
		ringswitchEthernetXBToTRNStatsFwdAARPPh1Frames	 INTEGER48,
		ringswitchEthernetXBToTRNStatsFwdAARPPh2Frames	 INTEGER48,
		ringswitchEthernetXBToTRNStatsFwdOtherEthFrames	 INTEGER48,
		ringswitchEthernetXBToTRNStatsFwdZeroOUIFrames	 INTEGER48,
		ringswitchEthernetXBToTRNStatsFwdOtherDLCFrames	 INTEGER48,
		ringswitchEthernetXBToTRNStatsFwdBPDUEncaps	 INTEGER48,
		ringswitchEthernetXBToTRNStatsFwdBPDUDecaps	 INTEGER48
		}		 			
							
	ringswitchEthernetXBToTRNStatsIfIndex OBJECT-TYPE
		SYNTAX  INTEGER
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"The value of this object identifies the
			ethernet interface for which this entry
			contains management information.  The
			value of this object for a particular
			interface has the same value as the
			ifIndex object for the same interface."
		::= { ringswitchEthernetXBToTRNStatsEntry 1 }

	ringswitchEthernetXBToTRNStatsDiscSptEncapsBlock OBJECT-TYPE
		SYNTAX  INTEGER48
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION						
			"The number of received frames discarded by this 
			interface because the encasulated spanning tree 
			detected a 3rd party bridge loop and has forced 
			blocking. These frames will be counted in forwarding
			statistics even	though the frame is discarded."
		::= { ringswitchEthernetXBToTRNStatsEntry 2 }

	ringswitchEthernetXBToTRNStatsDiscFilter OBJECT-TYPE
		SYNTAX  INTEGER48
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION						
			"The number of received frames discarded by this 
			interface because the DA was known to be on Ethernet."
		::= { ringswitchEthernetXBToTRNStatsEntry 3 }

	ringswitchEthernetXBToTRNStatsDiscBPDU OBJECT-TYPE
		SYNTAX  INTEGER48
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"The number of BPDU's received from Ethernet 
			which have been discarded (i.e. not received)
			by this interface because they could not be   	
			encapsulated (e.g. because they are too big). Only 
			counted when encapsulated SPT is enabled."
		::= { ringswitchEthernetXBToTRNStatsEntry 4 }
	
	ringswitchEthernetXBToTRNStatsDiscBPDUEncaps OBJECT-TYPE
		SYNTAX  INTEGER48
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION					  	
			"The number of received encapsulated BPDU frames 
			discarded by this interface because they are invalid
			or duplicates. Only counted when encapsulated SPT is 
			enabled."
		::= { ringswitchEthernetXBToTRNStatsEntry 5 }

	ringswitchEthernetXBToTRNStatsDiscTag8021qCfi OBJECT-TYPE
		SYNTAX  INTEGER48
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION						
			"The number of received frames discarded by this 
			interface because they are 802.1Q encapsulated 
			Token Ring frames."
		::= { ringswitchEthernetXBToTRNStatsEntry 6 }

	ringswitchEthernetXBToTRNStatsDiscTag8021qVlan OBJECT-TYPE
		SYNTAX  INTEGER48
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION						
			"The number of received frames discarded by this 
			interface because they have an 802.1Q tag with the 
			wrong VLAN id. Only counted when ethernet tagging 
			is enabled."
		::= { ringswitchEthernetXBToTRNStatsEntry 7 }

	ringswitchEthernetXBToTRNStatsDiscBadRif OBJECT-TYPE
		SYNTAX  INTEGER48
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION						
			"The number of received frames discarded by this 
			interface because a non-cached RIF is invalid."
		::= { ringswitchEthernetXBToTRNStatsEntry 8 }

	ringswitchEthernetXBToTRNStatsDiscHopLimit OBJECT-TYPE
		SYNTAX  INTEGER48
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION						
			"The number of received frames discarded by this 
			interface because a non-cached RIF is longer than 
			the hop-count limit."
		::= { ringswitchEthernetXBToTRNStatsEntry 9 }

	ringswitchEthernetXBToTRNStatsDiscDuplicateRin OBJECT-TYPE
		SYNTAX  INTEGER48
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION						
			"The number of received frames discarded by this 
			interface because a non-cached RIF contains the 
			interface's ring number twice."
		::= { ringswitchEthernetXBToTRNStatsEntry 10 }

	ringswitchEthernetXBToTRNStatsDiscIPDisabled OBJECT-TYPE
		SYNTAX  INTEGER48
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION						
			"The number of received frames discarded by this 
			interface because IP forwarding is disabled."
		::= { ringswitchEthernetXBToTRNStatsEntry 11 }

	ringswitchEthernetXBToTRNStatsDiscARPDisabled OBJECT-TYPE
		SYNTAX  INTEGER48
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION	
			"The number of received ARP and RARP frames discarded 
			by this interface because IP forwarding is disabled."
		::= { ringswitchEthernetXBToTRNStatsEntry 12 }

	ringswitchEthernetXBToTRNStatsDiscNCPCache OBJECT-TYPE
		SYNTAX  INTEGER48
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION				    		
			"The number of received NCP requests discarded by 
			this interface because they could not be cached."
		::= { ringswitchEthernetXBToTRNStatsEntry 13 }

	ringswitchEthernetXBToTRNStatsDiscIPXDisabled OBJECT-TYPE
		SYNTAX  INTEGER48
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION						
			"The number of received frames discarded by this 
			interface because IPX forwarding is disabled."
		::= { ringswitchEthernetXBToTRNStatsEntry 14 }

	ringswitchEthernetXBToTRNStatsDiscIPXTypeSnap OBJECT-TYPE
		SYNTAX  INTEGER48
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION				 		
			"The number of received IPX SNAP frames discarded by 
			this interface because SNAP encapsulation is not 
			selected on this interface."
		::= { ringswitchEthernetXBToTRNStatsEntry 15 }

	ringswitchEthernetXBToTRNStatsDiscIPXType8022 OBJECT-TYPE
		SYNTAX  INTEGER48
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION	
			"The number of received 802.2 IPX frames discarded by 
			this interface because 802.2 encapsulation is not 
			selected on this interface."
		::= { ringswitchEthernetXBToTRNStatsEntry 16 }

	ringswitchEthernetXBToTRNStatsDiscIPXType8023 OBJECT-TYPE
		SYNTAX  INTEGER48
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION				  		
			"The number of received 802.3 IPX frames discarded by 
			this interface because 802.3 encapsulation is not 
			selected on this interface."
		::= { ringswitchEthernetXBToTRNStatsEntry 17 }

	ringswitchEthernetXBToTRNStatsDiscIPXTypeDix OBJECT-TYPE
		SYNTAX  INTEGER48
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"The number of received DIX IPX frames discarded by 
			this interface because DIX encapsulation is not 
			selected on this interface."
		::= { ringswitchEthernetXBToTRNStatsEntry 18 }

	ringswitchEthernetXBToTRNStatsNoFakeBuffer OBJECT-TYPE
		SYNTAX  INTEGER48
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"The number of ICMP can't fragment frames which could
			not be sent due to resource limitations."
		::= { ringswitchEthernetXBToTRNStatsEntry 19 }

	ringswitchEthernetXBToTRNStatsCacheFull OBJECT-TYPE
		SYNTAX  INTEGER48
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"The number of frames received by this interface 
			whose source address could not be cached because 
			the cache is full."
		::= { ringswitchEthernetXBToTRNStatsEntry 20 }

	ringswitchEthernetXBToTRNStatsFwdIPFrames OBJECT-TYPE
		SYNTAX  INTEGER48
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION						
			"The number of received IP frames forwarded to Token
			Ring by this interface."
		::= { ringswitchEthernetXBToTRNStatsEntry 21 }

	ringswitchEthernetXBToTRNStatsFwdIPMulticast OBJECT-TYPE
		SYNTAX  INTEGER48
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"The number of received IP multicast frames forwarded 
			to Token Ring by this interface. These are included 
			in the count of IP frames."
		::= { ringswitchEthernetXBToTRNStatsEntry 22 }

	ringswitchEthernetXBToTRNStatsFwdARPFrames OBJECT-TYPE
		SYNTAX  INTEGER48
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"The number of received ARP an RARP frames forwarded 
			to Token Ring by this interface."
		::= { ringswitchEthernetXBToTRNStatsEntry 23 }

	ringswitchEthernetXBToTRNStatsFwdIPXFrames OBJECT-TYPE
		SYNTAX  INTEGER48
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"The number of received IPX frames forwarded to Token
			Ring by this interface."
		::= { ringswitchEthernetXBToTRNStatsEntry 24 }

	ringswitchEthernetXBToTRNStatsFwdAARPPh1Frames OBJECT-TYPE
		SYNTAX  INTEGER48
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"The number of received Appletalk phase 1 (Ethertype)
			frames forwarded to Token Ring by this interface."
		::= { ringswitchEthernetXBToTRNStatsEntry 25 }

	ringswitchEthernetXBToTRNStatsFwdAARPPh2Frames OBJECT-TYPE
		SYNTAX  INTEGER48
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"The number of received Appletalk phase 2 (SNAP)
			frames forwarded to Token Ring by this interface."
		::= { ringswitchEthernetXBToTRNStatsEntry 26 }

	ringswitchEthernetXBToTRNStatsFwdOtherEthFrames OBJECT-TYPE
		SYNTAX  INTEGER48
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION				  		
			"The number of received Ethertype frames forwarded to 
			Token Ring by this interface which have not been 
			included in a more specific counter."
		::= { ringswitchEthernetXBToTRNStatsEntry 27 }

	ringswitchEthernetXBToTRNStatsFwdZeroOUIFrames OBJECT-TYPE
		SYNTAX  INTEGER48
		ACCESS  read-only
	        STATUS  deprecated
		DESCRIPTION
			"The number of received zero OUI SNAP frames that
			have been converted to 802.1H tunnel frames and 
			forwarded to Token Ring by this interface."
		::= { ringswitchEthernetXBToTRNStatsEntry 28 }

	ringswitchEthernetXBToTRNStatsFwdOtherDLCFrames OBJECT-TYPE
		SYNTAX  INTEGER48
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"The number of received non-zero OUI SNAP and other 
			DLC frames that have been forwarded to Token Ring 
			and which have not been included in a more specific 
			counter."
		::= { ringswitchEthernetXBToTRNStatsEntry 29 }

	ringswitchEthernetXBToTRNStatsFwdBPDUEncaps OBJECT-TYPE
		SYNTAX  INTEGER48
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"The number of received BPDU's which have been
			encapsulated before being forwarded to Token Ring.
			Only counted when encapsulated SPT is enabled."
		::= { ringswitchEthernetXBToTRNStatsEntry 30 }

	ringswitchEthernetXBToTRNStatsFwdBPDUDecaps OBJECT-TYPE
		SYNTAX  INTEGER48
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"The number of received BPDU's which have been
			de-encapsulated before being forwarded to Token Ring.
			Only counted when encapsulated SPT is enabled."
		::= { ringswitchEthernetXBToTRNStatsEntry 31 }

-- the ringswitch tokenring to ethernet translational bridge statistics group

	ringswitchEthernetXBFromTRNStatsTable OBJECT-TYPE
		SYNTAX  SEQUENCE OF RingswitchEthernetXBFromTRNStatsEntry
		ACCESS  not-accessible
		STATUS  mandatory
		DESCRIPTION
			"This table contains information relating to the
			forwarding statistics that are maintained by each
			ethernet interface acting as a translational
			bridge from Token-ring."
		::= { ringswitchEthernet 4 } 

	ringswitchEthernetXBFromTRNStatsEntry OBJECT-TYPE
		SYNTAX  RingswitchEthernetXBFromTRNStatsEntry
		ACCESS  not-accessible
		STATUS  mandatory
		DESCRIPTION		      				
			"A list of information relating to the forwarding
			statistics of a single ethernet interface acting 
			as a translational bridge from Token-ring."
		INDEX { ringswitchEthernetXBFromTRNStatsIfIndex }
		::= { ringswitchEthernetXBFromTRNStatsTable 1 }
				
	RingswitchEthernetXBFromTRNStatsEntry
		::= SEQUENCE
		{		     		
		ringswitchEthernetXBFromTRNStatsIfIndex              INTEGER,
		ringswitchEthernetXBFromTRNStatsDiscTooBig	         INTEGER48,
		ringswitchEthernetXBFromTRNStatsDiscSptEncapsBlock   INTEGER48,
		ringswitchEthernetXBFromTRNStatsDiscFilter	         INTEGER48,
		ringswitchEthernetXBFromTRNStatsDiscBPDU		     INTEGER48,
		ringswitchEthernetXBFromTRNStatsDiscBPDUEncaps	     INTEGER48,
		ringswitchEthernetXBFromTRNStatsDiscTag8021qCfi	     INTEGER48,
		ringswitchEthernetXBFromTRNStatsDiscTag8021qVlan	 INTEGER48,
		ringswitchEthernetXBFromTRNStatsDiscBadRIF	         INTEGER48,
		ringswitchEthernetXBFromTRNStatsDiscZeroRingNum	     INTEGER48,
		ringswitchEthernetXBFromTRNStatsDiscNonTermRIF	     INTEGER48,
		ringswitchEthernetXBFromTRNStatsDiscIPDisabled	     INTEGER48,
		ringswitchEthernetXBFromTRNStatsDiscARPDisabled	     INTEGER48,
		ringswitchEthernetXBFromTRNStatsDiscIPCantFrag	     INTEGER48,
		ringswitchEthernetXBFromTRNStatsDiscNCPCache	     INTEGER48,
		ringswitchEthernetXBFromTRNStatsDiscIPXDisabled	     INTEGER48,
		ringswitchEthernetXBFromTRNStatsDiscIPXTypeSnap	     INTEGER48,
		ringswitchEthernetXBFromTRNStatsDiscIPXType8022	     INTEGER48,

		ringswitchEthernetXBFromTRNStatsCacheFull    	     INTEGER48,
		ringswitchEthernetXBFromTRNStatsRIFCacheFull 	     INTEGER48,

		ringswitchEthernetXBFromTRNStatsFwdIPFrames   	     INTEGER48,
		ringswitchEthernetXBFromTRNStatsFwdIPFragmented	     INTEGER48,
		ringswitchEthernetXBFromTRNStatsFwdIPMulticast	     INTEGER48,
		ringswitchEthernetXBFromTRNStatsFwdARPFrames	     INTEGER48,
		ringswitchEthernetXBFromTRNStatsFwdIPXFrames	     INTEGER48,
		ringswitchEthernetXBFromTRNStatsFwdAARPPh1Frames	 INTEGER48,
		ringswitchEthernetXBFromTRNStatsFwdAARPPh2Frames	 INTEGER48,
		ringswitchEthernetXBFromTRNStatsFwdTunnel8021hFrames INTEGER48,
		ringswitchEthernetXBFromTRNStatsFwdZeroOUISnapFrames INTEGER48,
		ringswitchEthernetXBFromTRNStatsFwdOtherDLCFrames    INTEGER48,
		ringswitchEthernetXBFromTRNStatsFwdBPDUDecaps	     INTEGER48,
 		ringswitchEthernetXBFromTRNStatsFwdBPDUEncaps 	     INTEGER48
		}		 			
							
	ringswitchEthernetXBFromTRNStatsIfIndex OBJECT-TYPE
		SYNTAX  INTEGER
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"The value of this object identifies the
			ethernet interface for which this entry
			contains management information.  The
			value of this object for a particular
			interface has the same value as the
			ifIndex object for the same interface."
		::= { ringswitchEthernetXBFromTRNStatsEntry 1 }
						
	ringswitchEthernetXBFromTRNStatsDiscTooBig OBJECT-TYPE
		SYNTAX  INTEGER48
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"The number of frames received from Token Ring 
			which have been discarded (i.e. not transmitted)
			by this interface because after translation to 
			Ethernet they were bigger than 1514 bytes. These
			frames are counted by the forwarding counters even
			though they are discarded."
		::= { ringswitchEthernetXBFromTRNStatsEntry 2 }

	ringswitchEthernetXBFromTRNStatsDiscSptEncapsBlock OBJECT-TYPE
		SYNTAX  INTEGER48
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"The number of frames received from Token Ring 
			which have been discarded (i.e. not transmitted)
			by this interface because the encasulated spanning 
			tree detected a 3rd party bridge loop and has forced 
			blocking. These	frames are counted by the forwarding 
			counters even though they are discarded."
		::= { ringswitchEthernetXBFromTRNStatsEntry 3 }

	ringswitchEthernetXBFromTRNStatsDiscFilter OBJECT-TYPE
		SYNTAX  INTEGER48
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"The number of frames received from Token Ring 
			which have been discarded (i.e. not transmitted)
			by this interface because the destination MAC address
			was known to be on Token Ring."
		::= { ringswitchEthernetXBFromTRNStatsEntry 4 }

	ringswitchEthernetXBFromTRNStatsDiscBPDU OBJECT-TYPE
		SYNTAX  INTEGER48
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"The number of BPDU's received from Token Ring 
			which have been discarded (i.e. not transmitted)
			by this interface because they could not be   	
			encapsulated (e.g. because they are too big). Only 
			counted when encapsulated SPT is enabled."
		::= { ringswitchEthernetXBFromTRNStatsEntry 5 }

	ringswitchEthernetXBFromTRNStatsDiscBPDUEncaps OBJECT-TYPE
		SYNTAX  INTEGER48
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION				  		
			"The number of encapsulated BPDU's received from 
			Token Ring which have been discarded (i.e. not 
			transmitted) by this interface because they are 
			invalid or are ARE duplicates. Only counted when 
			encapsulated SPT is enabled."
		::= { ringswitchEthernetXBFromTRNStatsEntry 6 }

	ringswitchEthernetXBFromTRNStatsDiscTag8021qCfi OBJECT-TYPE
		SYNTAX  INTEGER48
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"The number of frames received from Token Ring 
			which have been discarded (i.e. not transmitted)
			by this interface because they are 802.1Q 
			encapsulated Ethernet frames."
		::= { ringswitchEthernetXBFromTRNStatsEntry 7 }

	ringswitchEthernetXBFromTRNStatsDiscTag8021qVlan OBJECT-TYPE
		SYNTAX  INTEGER48
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"The number of frames received from Token Ring 
			which have been discarded (i.e. not transmitted)
			by this interface because they have an 802.1Q tag
			with the wrong VLAN id. Only counted when Ethernet
			Tagging is enabled."
		::= { ringswitchEthernetXBFromTRNStatsEntry 8 }

	ringswitchEthernetXBFromTRNStatsDiscBadRIF OBJECT-TYPE
		SYNTAX  INTEGER48
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"The number of frames received from Token Ring 
			which have been discarded (i.e. not transmitted)
			by this interface because the RIF is invalid."
		::= { ringswitchEthernetXBFromTRNStatsEntry 9 }

	ringswitchEthernetXBFromTRNStatsDiscZeroRingNum OBJECT-TYPE
		SYNTAX  INTEGER48
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION			     			
			"The number of source-routed frames received from 
			Token Ring which have been discarded (i.e. not 
			transmitted) by this interface because we do not have
			a ring number for the interface."
		::= { ringswitchEthernetXBFromTRNStatsEntry 10 }

	ringswitchEthernetXBFromTRNStatsDiscNonTermRIF OBJECT-TYPE
		SYNTAX  INTEGER48
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION				   		
			"The number of specifically routed frames received 
			from Token Ring which have been discarded (i.e. not 
			transmitted) by this interface becuase the 
			interface's ring number is not last in the RIF."
		::= { ringswitchEthernetXBFromTRNStatsEntry 11 }

	ringswitchEthernetXBFromTRNStatsDiscIPDisabled OBJECT-TYPE
		SYNTAX  INTEGER48
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"The number of IP frames received from Token Ring 
			which have been discarded (i.e. not transmitted)
			by this interface because IP forwarding is disabled."
		::= { ringswitchEthernetXBFromTRNStatsEntry 12 }

	ringswitchEthernetXBFromTRNStatsDiscARPDisabled OBJECT-TYPE
		SYNTAX  INTEGER48
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION			   			
			"The number of ARP and RARP frames received from 
			Token Ring which have been discarded (i.e. not 
			transmitted) by this interface because IP forwarding 
			is disabled."
		::= { ringswitchEthernetXBFromTRNStatsEntry 13 }

	ringswitchEthernetXBFromTRNStatsDiscIPCantFrag OBJECT-TYPE
		SYNTAX  INTEGER48
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"The number of IP frames received from Token Ring 
			which have been discarded (i.e. not transmitted)
			by this interface because they need fragmenting but 
			the DONT-FRAG bit is set.  An ICMP frame will be sent
			back to the source."
		::= { ringswitchEthernetXBFromTRNStatsEntry 14 }

	ringswitchEthernetXBFromTRNStatsDiscNCPCache OBJECT-TYPE
		SYNTAX  INTEGER48
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION	
			"The number of NCP request frames received from Token 
			Ring which have been discarded (i.e. not transmitted)
			by this interface because they could not be cached."
		::= { ringswitchEthernetXBFromTRNStatsEntry 15 }

	ringswitchEthernetXBFromTRNStatsDiscIPXDisabled OBJECT-TYPE
		SYNTAX  INTEGER48
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"The number of IPX frames received from Token Ring 
			which have been discarded (i.e. not transmitted)
			by this interface because IPX forwarding is disabled."
		::= { ringswitchEthernetXBFromTRNStatsEntry 16 }

	ringswitchEthernetXBFromTRNStatsDiscIPXTypeSnap OBJECT-TYPE
		SYNTAX  INTEGER48
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION						
			"The number of IPX SNAP frames received from Token 
			Ring which have been discarded (i.e. not transmitted)
			by this interface becuase SNAP encapsulation has not
			been selected."
		::= { ringswitchEthernetXBFromTRNStatsEntry 17 }

	ringswitchEthernetXBFromTRNStatsDiscIPXType8022 OBJECT-TYPE
		SYNTAX  INTEGER48
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"The number of IPX 802.2 frames received from Token 
			Ring which have been discarded (i.e. not transmitted)
			by this interface becuase 802.2 encapsulation has not
			been selected."
		::= { ringswitchEthernetXBFromTRNStatsEntry 18 }

	ringswitchEthernetXBFromTRNStatsCacheFull OBJECT-TYPE
		SYNTAX  INTEGER48
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"The number of frames received from Token Ring whose
			source MAC address could not be cached because the 
			cache is full. The frame is still forwarded."
		::= { ringswitchEthernetXBFromTRNStatsEntry 19 }

	ringswitchEthernetXBFromTRNStatsRIFCacheFull OBJECT-TYPE
		SYNTAX  INTEGER48
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION			     			
			"The number of source-routed frames received from 
			Token Ring whose whose RIF could not be cached
			because the RIF cache is full.  The frame is still 
			forwarded."
		::= { ringswitchEthernetXBFromTRNStatsEntry 20 }

	ringswitchEthernetXBFromTRNStatsFwdIPFrames OBJECT-TYPE
		SYNTAX  INTEGER48
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION		  				
			"The number of IP frames received from Token Ring 
			which have been forwarded by this interface."
		::= { ringswitchEthernetXBFromTRNStatsEntry 21 }

	ringswitchEthernetXBFromTRNStatsFwdIPFragmented OBJECT-TYPE
		SYNTAX  INTEGER48
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"The number of IP frames received from Token Ring 
			which have been fragmented before being forwarded 
			by this interface. These are also included in the 
			count of IP frames."
		::= { ringswitchEthernetXBFromTRNStatsEntry 22 }

	ringswitchEthernetXBFromTRNStatsFwdIPMulticast OBJECT-TYPE
		SYNTAX  INTEGER48
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION			    			
			"The number of IP multicast frames received from 
			Token Ring that have been forwarded by this interface. 
			These are also included in the count of IP frames."
		::= { ringswitchEthernetXBFromTRNStatsEntry 23 }

	ringswitchEthernetXBFromTRNStatsFwdARPFrames OBJECT-TYPE
		SYNTAX  INTEGER48
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION			   			
			"The number of ARP and RARP frames received from 
			Token Ring which have been forwarded by this 
			interface."
		::= { ringswitchEthernetXBFromTRNStatsEntry 24 }

	ringswitchEthernetXBFromTRNStatsFwdIPXFrames OBJECT-TYPE
		SYNTAX  INTEGER48
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"The number of IPX frames received from Token Ring 
			which have been forwarded by this interface."
		::= { ringswitchEthernetXBFromTRNStatsEntry 25 }

	ringswitchEthernetXBFromTRNStatsFwdAARPPh1Frames OBJECT-TYPE
		SYNTAX  INTEGER48
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"The number of Appletalk phase 1 frames (802.1H 
			tunnel encapsulated) ARP frames received from Token 
			Ring which have been forwarded by this interface."
		::= { ringswitchEthernetXBFromTRNStatsEntry 26 }

	ringswitchEthernetXBFromTRNStatsFwdAARPPh2Frames OBJECT-TYPE
		SYNTAX  INTEGER48
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"The number of Appletalk phase 2 frames (SNAP
			encapsulated) ARP frames received from Token Ring 
			which have been forwarded by this interface."
		::= { ringswitchEthernetXBFromTRNStatsEntry 27 }

	ringswitchEthernetXBFromTRNStatsFwdTunnel8021hFrames OBJECT-TYPE
		SYNTAX  INTEGER48
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION		     		    		
			"The number of 802.1H tunnel frames received from 
			Token Ring which have been converted to zero OUI SNAP 
			frames before being forwarded by this interface."
		::= { ringswitchEthernetXBFromTRNStatsEntry 28 }

	ringswitchEthernetXBFromTRNStatsFwdZeroOUISnapFrames OBJECT-TYPE
		SYNTAX  INTEGER48
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION	
			"The number of other zero OUI SNAP frames received 
			from Token Ring which have been converted to 
			Ethertype frames before being forwarded by this 
			interface."
		::= { ringswitchEthernetXBFromTRNStatsEntry 29 }

	ringswitchEthernetXBFromTRNStatsFwdOtherDLCFrames OBJECT-TYPE
		SYNTAX  INTEGER48
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION					       	
			"The number of non-zero OUI SNAP and other DLC frames 
			received from Token Ring which have been forwarded 
			by this interface and which have not been included in
			a more specific counter."
		::= { ringswitchEthernetXBFromTRNStatsEntry 30 }

	ringswitchEthernetXBFromTRNStatsFwdBPDUDecaps OBJECT-TYPE
		SYNTAX  INTEGER48
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"The number of encapsulated BPDU's received from 
			Token Ring which have been de-encapsulated before
			being forwarded on this interface. Only counted when 
			encapsulated SPT is enabled."
		::= { ringswitchEthernetXBFromTRNStatsEntry 31 }
			      			
	ringswitchEthernetXBFromTRNStatsFwdBPDUEncaps OBJECT-TYPE
		SYNTAX  INTEGER48
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"The number of BPDU's received from Token Ring which 
			have been encapsulated before being forwarded on this
			interface. Only counted when encapsulated SPT is 
			enabled."
		::= { ringswitchEthernetXBFromTRNStatsEntry 32 }


-- the ringswitch gigabit extended physical statistics group

	ringswitchEthernetExtPhysStatsTable OBJECT-TYPE
		SYNTAX  SEQUENCE OF RingswitchEthernetExtPhysStatsEntry
		ACCESS  not-accessible
		STATUS  mandatory
		DESCRIPTION
			"This table contains physical layer statistics
                        associated with the gigabit interface that are
                        not available through the normal Ethernet physical
                        layer statistics."
		::= { ringswitchEthernet 5 } 

	ringswitchEthernetExtPhysStatsEntry OBJECT-TYPE
		SYNTAX  RingswitchEthernetExtPhysStatsEntry
		ACCESS  not-accessible
		STATUS  mandatory
		DESCRIPTION		      				
			"A list of information relating to the physical
			layer statistics of a single gigabit interface."
		INDEX { ringswitchEthernetExtPhysStatsIfIndex }
		::= { ringswitchEthernetExtPhysStatsTable 1 }
				
	RingswitchEthernetExtPhysStatsEntry
		::= SEQUENCE
		{		     		
		ringswitchEthernetExtPhysStatsIfIndex            INTEGER,
		ringswitchEthernetExtPhysStatsRxDataErrors	 INTEGER,
		ringswitchEthernetExtPhysStatsRxSequenceErrors	 INTEGER,
		ringswitchEthernetExtPhysStatsRxCarrierExtErrors INTEGER,
		ringswitchEthernetExtPhysStatsXonRxFrames	 INTEGER,
		ringswitchEthernetExtPhysStatsXonTxFrames	 INTEGER,
		ringswitchEthernetExtPhysStatsXoffRxFrames	 INTEGER,
		ringswitchEthernetExtPhysStatsXoffTxFrames	 INTEGER
		}		 			
							
	ringswitchEthernetExtPhysStatsIfIndex OBJECT-TYPE
		SYNTAX  INTEGER
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"The value of this object identifies the
			gigabitt interface for which this entry
			contains management information.  The
			value of this object for a particular
			interface has the same value as the
			ifIndex object for the same interface."
		::= { ringswitchEthernetExtPhysStatsEntry 1 }

	ringswitchEthernetExtPhysStatsRxDataErrors OBJECT-TYPE
		SYNTAX  INTEGER
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"The number of frames received with I-RX-ER
                        asserted by the front end."
		::= { ringswitchEthernetExtPhysStatsEntry 2 }

	ringswitchEthernetExtPhysStatsRxSequenceErrors OBJECT-TYPE
		SYNTAX  INTEGER
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"The number of sequence errors in the symbol
                        ordering received on the TBI interface."
		::= { ringswitchEthernetExtPhysStatsEntry 3 }

	ringswitchEthernetExtPhysStatsRxCarrierExtErrors OBJECT-TYPE
		SYNTAX  INTEGER
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"The number of receive data errors that were
                        detected during the carrier extension time of 
                        packet reception."
		::= { ringswitchEthernetExtPhysStatsEntry 4 }

	ringswitchEthernetExtPhysStatsXonRxFrames OBJECT-TYPE
		SYNTAX  INTEGER
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"The number of XON flow control frames received."
		::= { ringswitchEthernetExtPhysStatsEntry 5 }

	ringswitchEthernetExtPhysStatsXonTxFrames OBJECT-TYPE
		SYNTAX  INTEGER
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"Ther number of XON flow control frames transmitted."
		::= { ringswitchEthernetExtPhysStatsEntry 6 }

	ringswitchEthernetExtPhysStatsXoffRxFrames OBJECT-TYPE
		SYNTAX  INTEGER
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"The number of XOFF flow control frames received."
		::= { ringswitchEthernetExtPhysStatsEntry 7 }

	ringswitchEthernetExtPhysStatsXoffTxFrames OBJECT-TYPE
		SYNTAX  INTEGER
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"The number of XOFF flow control frames transmitted."
		::= { ringswitchEthernetExtPhysStatsEntry 8 }

----------------------------------------------------------------------------
-- the power supply group
	ringswitchPSUNumUnits OBJECT-TYPE
		SYNTAX  INTEGER
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"The number of power supply units in this Ring-
			switch."
		::= { ringswitchPSU 1 }
	ringswitchPSUTable      OBJECT-TYPE
		SYNTAX  SEQUENCE OF RingswitchPSUEntry
		ACCESS  not-accessible
		STATUS  mandatory
		DESCRIPTION
			"A table that contains information about each power
			supply unit in this Ringswitch."
		::= { ringswitchPSU 2 }
	ringswitchPSUEntry      OBJECT-TYPE
		SYNTAX  RingswitchPSUEntry
		ACCESS  not-accessible
		STATUS  mandatory
		DESCRIPTION ""
		INDEX { ringswitchPSUIndex }
		::= { ringswitchPSUTable 1 }
	RingswitchPSUEntry ::=
		SEQUENCE        {
				ringswitchPSUIndex       INTEGER,
				ringswitchPSUDescription DisplayString,
				ringswitchPSUStatus      INTEGER
				}
	ringswitchPSUIndex OBJECT-TYPE
		SYNTAX  INTEGER (1..65535)
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"The value of this object identifies the PSU
			for which this entry contains management
			information."
		::= { ringswitchPSUEntry 1 }
	ringswitchPSUDescription OBJECT-TYPE
		SYNTAX  DisplayString (SIZE (0..64))
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"A string containing a textual description for
			this power supply unit"
		::= { ringswitchPSUEntry 2 }
	ringswitchPSUStatus OBJECT-TYPE
		SYNTAX  INTEGER
			{
			unit-absent     (1),
			unit-broken     (2),
			unit-normal     (3)
			}
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"This variable gives the status
			of PSU1."
		::= { ringswitchPSUEntry 3 }
----------------------------------------------------------------------------
-- the fan group
	ringswitchFanNumUnits OBJECT-TYPE
		SYNTAX  INTEGER
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"The number of fans in this Ringswitch."
		::= { ringswitchFan 1 }
	ringswitchFanTable      OBJECT-TYPE
		SYNTAX  SEQUENCE OF RingswitchFanEntry
		ACCESS  not-accessible
		STATUS  mandatory
		DESCRIPTION
			"A table that contains information about each fan
			in this Ringswitch."
		::= { ringswitchFan 2 }
	ringswitchFanEntry      OBJECT-TYPE
		SYNTAX  RingswitchFanEntry
		ACCESS  not-accessible
		STATUS  mandatory
		DESCRIPTION ""
		INDEX { ringswitchFanIndex }
		::= { ringswitchFanTable 1 }
	RingswitchFanEntry ::=
		SEQUENCE        {
				ringswitchFanIndex       INTEGER,
				ringswitchFanDescription DisplayString,
				ringswitchFanSpeed       INTEGER
				}
	ringswitchFanIndex OBJECT-TYPE
		SYNTAX  INTEGER (1..65535)
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"The value of this object identifies the Fan
			for which this entry contains management
			information."
		::= { ringswitchFanEntry 1 }
	ringswitchFanDescription OBJECT-TYPE
		SYNTAX  DisplayString (SIZE (0..64))
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"A string containing a textual description for
			this Fan."
		::= { ringswitchFanEntry 2 }
	ringswitchFanSpeed OBJECT-TYPE
		SYNTAX  INTEGER (0..65535)
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"The number of revolutions per minute being
			reported by this fan. If this value is read as
			zero, then the fan is no longer operating."
		::= { ringswitchFanEntry 3 }
----------------------------------------------------------------------------
-- the Trap group
-- this group defines variables that need to be returned in the Ringswitch
-- specific traps. All of these variables are defined as not-accessible.
	ringswitchRouteDescrSegmentID OBJECT-TYPE
		SYNTAX  INTEGER (0..4095)
		ACCESS  not-accessible
		STATUS  mandatory
		DESCRIPTION
			"The LAN ID portion of the IEEE 802.5
			route descriptor that failed to register."
		::= { ringswitchTrap 1 }
	ringswitchRouteDescrBridgeNumber OBJECT-TYPE
		SYNTAX  INTEGER (0..15)
		ACCESS  not-accessible
		STATUS  mandatory
		DESCRIPTION
			"The Bridge Number portion of the IEEE 802.5 route
			descriptor that failed to register."
		::= { ringswitchTrap 2 }
	ringswitchVersionMismatch OBJECT-TYPE
		SYNTAX  INTEGER
			{
			boot-eprom-and-boot-flash(1)
			}
		ACCESS  not-accessible
		STATUS  mandatory
		DESCRIPTION
			"An enumerated value that describes which
			software components have a version mismatch."
		::= { ringswitchTrap 3 }
	ringswitchDownloadFailed OBJECT-TYPE
		SYNTAX  INTEGER
			{
			microcode(1),
			boot-code(2),
			fddi-code(3),
			atm-code(4)
			}
		ACCESS  not-accessible
		STATUS  mandatory
		DESCRIPTION
			"An enumerated value that describes which
			software component has had a failed 
			download that may compromise system operation
			at the next reboot."
		::= { ringswitchTrap 4 }
----------------------------------------------------------------------------
-- the Ringswitch filter group
	ringswitchFilterTable OBJECT-TYPE
		SYNTAX  SEQUENCE OF RingswitchFilterEntry
		ACCESS  not-accessible
		STATUS  mandatory
		DESCRIPTION
			"This table contains Ringswitch protocol filter
			information. There is one entry per filter."
		::= { ringswitchFilter 1 }
	ringswitchFilterEntry OBJECT-TYPE
		SYNTAX  RingswitchFilterEntry
		ACCESS  not-accessible
		STATUS  mandatory
		DESCRIPTION
			"A list of Ringswitch protocol filters."
		INDEX { ringswitchFilterIndex }
		::= { ringswitchFilterTable 1 }
	RingswitchFilterEntry ::= SEQUENCE
			{
			ringswitchFilterIndex           INTEGER,
			ringswitchFilterStatus          RingswitchRowStatus,
			ringswitchFilterName            DisplayString,
			ringswitchFilterData            OCTET STRING,
			ringswitchFilterExceptionType   INTEGER,
			ringswitchFilterType            INTEGER,
			ringswitchFilterForwardType     INTEGER
			}
	ringswitchFilterIndex OBJECT-TYPE
		SYNTAX  INTEGER
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"The index of this filter."
		::= { ringswitchFilterEntry 1 }
	ringswitchFilterStatus OBJECT-TYPE
		SYNTAX  RingswitchRowStatus
		ACCESS  read-write
		STATUS  mandatory
		DESCRIPTION
			"The filter status; see RingswitchRowStatus comments
			above for more details."
		::= { ringswitchFilterEntry 2 }
	ringswitchFilterName OBJECT-TYPE
		SYNTAX  DisplayString
		ACCESS  read-write
		STATUS  mandatory
		DESCRIPTION
			"The filter name."
		::= { ringswitchFilterEntry 3 }
	ringswitchFilterData OBJECT-TYPE
		SYNTAX  OCTET STRING (SIZE (0..64))
		ACCESS  read-write
		STATUS  mandatory
		DESCRIPTION
			"An octet string containing the filter definition.
			It consists of one or more sections in the format
				<length> <offset> <data1>...<datan>
			where the first byte is the length of this section
			in bytes (including the length and offset), and
			the second byte is the offset from the end of the
			routing information field.  The subsequent data
			bytes are compared with the data at the offset
			into broadcast frames received on each port on the
			Ringswitch."
		::= { ringswitchFilterEntry 4 }
	ringswitchFilterExceptionType OBJECT-TYPE
		SYNTAX  INTEGER {
				normal          (1),
				exception       (2)
				}
		ACCESS  read-write
		STATUS  mandatory
		DESCRIPTION
			"What this filter does when matching a frame: a
			value of 'normal' sends the frame to the ports
			on the row of the matrix corresponding to the
			input port; a value of 'exception' stops it going
			to those ports."
		::= { ringswitchFilterEntry 5 }
	ringswitchFilterType OBJECT-TYPE
		SYNTAX  INTEGER {
				normal          (1),
				match-all       (2)
				}
		ACCESS  read-write
		STATUS  mandatory
		DESCRIPTION
			"The filter type; for future expansion."
		::= { ringswitchFilterEntry 6 }
	ringswitchFilterForwardType OBJECT-TYPE
		SYNTAX  INTEGER {
				sr-only (1),
				tb-only (2),
				sr-tb   (3)
				}
		ACCESS  read-write
		STATUS  mandatory
		DESCRIPTION
			"The type of forwarding affected by the filter."
		::= { ringswitchFilterEntry 7 }
-- the Ringswitch filter matrix group
	ringswitchFilterMatrixTable OBJECT-TYPE
		SYNTAX  SEQUENCE OF RingswitchFilterMatrixEntry
		ACCESS  not-accessible
		STATUS  mandatory
		DESCRIPTION
			"This table contains Ringswitch protocol filter
			switching matrix information. There is one entry
			for each row of each switching matrix."
		::= { ringswitchFilter 2 }
	ringswitchFilterMatrixEntry OBJECT-TYPE
		SYNTAX  RingswitchFilterMatrixEntry
		ACCESS  not-accessible
		STATUS  mandatory
		DESCRIPTION
			"A list of Ringswitch protocol filter switching
			matrix rows."
		INDEX { ringswitchFilterMatrixIndex,
			ringswitchFilterMatrixRowIndex }
		::= { ringswitchFilterMatrixTable 1 }
	RingswitchFilterMatrixEntry ::= SEQUENCE
			{
			ringswitchFilterMatrixIndex     INTEGER,
			ringswitchFilterMatrixRowIndex  INTEGER,
			ringswitchFilterMatrixData      OCTET STRING
			}
	ringswitchFilterMatrixIndex OBJECT-TYPE
		SYNTAX  INTEGER
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"The index of this filter."
		::= { ringswitchFilterMatrixEntry 1 }
	ringswitchFilterMatrixRowIndex OBJECT-TYPE
		SYNTAX  INTEGER
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"The index of this row in this matrix."
		::= { ringswitchFilterMatrixEntry 2 }
	ringswitchFilterMatrixData OBJECT-TYPE
		SYNTAX  OCTET STRING
		ACCESS  read-write
		STATUS  mandatory
		DESCRIPTION
			"A single row of the matrix, with ports 1 to 8
			represented by the first octet (starting with the
			least significant bit), ports 9 to 16 by the next
			octet, and so on."
		::= { ringswitchFilterMatrixEntry 3 }
----------------------------------------------------------------------------
-- Madge Ringswitch specific traps
	fanPSSpeedFailed TRAP-TYPE
		ENTERPRISE ringswitch
		VARIABLES { ringswitchBasePSFanSpeed }
		DESCRIPTION
			"This trap is sent when the Power Supply fan has
			failed."
		::= 1
	fanExtSpeedFailed TRAP-TYPE
		ENTERPRISE ringswitch
		VARIABLES { ringswitchBaseExtFanSpeed }
		DESCRIPTION
			"This trap is sent when the external fan has
			failed."
		::= 2
	portFailed TRAP-TYPE
		ENTERPRISE ringswitch
		VARIABLES { ringswitchPortAdapterStatus }
		DESCRIPTION
			"This trap is sent when a port has auto-closed or
			failed to open."
		::= 3
	brTestFailed TRAP-TYPE
		ENTERPRISE ringswitch
		VARIABLES { ringswitchPortTestError }
		DESCRIPTION
			"This trap is sent when the bridge test on a port
			fails."
		::= 4
	elanRingnumFailed TRAP-TYPE
		ENTERPRISE ringswitch
		VARIABLES { ringswitchGenPortNum }
		DESCRIPTION
			"This trap is sent when the ELAN ring number for
			an ATM LEC is already configured on another port."
		::= 5
	routeDescriptorFailed TRAP-TYPE
		ENTERPRISE ringswitch
		VARIABLES { ringswitchGenPortNum,
			    ringswitchRouteDescrSegmentID,
			    ringswitchRouteDescrBridgeNumber }
		DESCRIPTION
			"This trap is sent when an ATM LEC has failed to
			register a route descriptor."
		::= 6
	fanSpeedFailed TRAP-TYPE
		ENTERPRISE ringswitch
		VARIABLES { ringswitchFanSpeed }
		DESCRIPTION
			"This trap is sent when a fan has failed."
		::= 7
	psuFailed TRAP-TYPE
		ENTERPRISE ringswitch
		VARIABLES { ringswitchPSUStatus }
		DESCRIPTION
			"This trap is sent when a PSU has broken."
		::= 8
	versionMismatch TRAP-TYPE
		ENTERPRISE ringswitch
		VARIABLES { ringswitchVersionMismatch }
		DESCRIPTION
			"This trap is sent when there is a version
			mismatch between some installed software
			components."
		::= 9
	genPortStatus TRAP-TYPE
		ENTERPRISE ringswitch
		VARIABLES { ringswitchGenPortLastStatus }
		DESCRIPTION
			"This trap is sent when an interesting
			status change occurs on a port."
		::= 10
	slotStatus TRAP-TYPE
		ENTERPRISE ringswitch
		VARIABLES { ringswitchSlotLastStatus }
		DESCRIPTION
			"This trap is sent when an interesting
			status change occurs on a slot."
		::= 11
	oneNodePerPortViolation TRAP-TYPE
		ENTERPRISE ringswitch
		VARIABLES { ringswitchTRGrpSwtchIfIndex }
		DESCRIPTION
			"Sent when two or more nodes are inserted in a 
			Group Switch port."
		::= 12
	grpSwitchPortDisabled TRAP-TYPE
		ENTERPRISE ringswitch
		VARIABLES { ringswitchTRGrpSwtchIfIndex,
			    ringswitchTRGrpSwtchPortIndex,
			    ringswitchTRGrpSwtchTrapInfoReason}
		DESCRIPTION
			"Sent when a group switch port is disabled."
		::= 13
	grpSwitchPortKilled TRAP-TYPE
		ENTERPRISE ringswitch
		VARIABLES { ringswitchTRGrpSwtchIfIndex,
			    ringswitchTRGrpSwtchPortIndex,
			    ringswitchTRGrpSwtchTrapInfoReason}
		DESCRIPTION
			"Sent when a group switch port is disabled by 
                         fault manager"
		::= 14
	downloadFailed TRAP-TYPE
		ENTERPRISE ringswitch
		VARIABLES { ringswitchDownloadFailed }
		DESCRIPTION
			"This trap is sent when a download
			operation has failed, and system operation
			may be compromised as the next reboot."
		::= 15
	grpSwitchPortFaultDetected TRAP-TYPE
		ENTERPRISE ringswitch
		VARIABLES { ringswitchTRGrpSwtchIfIndex,
			    ringswitchTRGrpSwtchPortIndex,
			    ringswitchTRGrpSwtchTrapInfoReason}
		DESCRIPTION
			"Sent when a group switch port causes a fault."
		::= 16
	fastFailoverWarning TRAP-TYPE
		ENTERPRISE ringswitch
		VARIABLES { ringswitchTRIfFastFailoverPrimaryPort,
			    ringswitchTRIfFastFailoverTrapInfoReason }
		DESCRIPTION
			"Sent when a fast failover switch port causes a 
                         warning"
		::= 17
     ethernetSTPBlockingTrap TRAP-TYPE
		ENTERPRISE ringswitch
		VARIABLES { ringswitchEthernetIfIndex,
			     ringswitchEthernetIfSTPBlockingTrapInfo}
		DESCRIPTION
			"Sent when a Ethernet port is forced into blocking mode
			 due to the presence of a 3rd party bridge. Also sent
			 when the port comes out of blocking"
		::= 18
	END

