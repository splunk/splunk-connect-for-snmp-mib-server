atmBaseMIB OBJECT IDENTIFIER ::= { mscPassportMIBs 63 }










-- Lp/n AtmResourceControl
-- The AtmResourceControl component is used to configure the usage 
-- of ATM-specific resources on a Logical Processor running ATM 
-- services. (This component is only permitted under Logical 
-- Processors which are linked to ATM card types.)

mscLpArc OBJECT IDENTIFIER ::= { mscLp 19 }



mscLpArcRowStatusTable OBJECT-TYPE
   SYNTAX     SEQUENCE OF MscLpArcRowStatusEntry
   ACCESS     not-accessible
   STATUS     obsolete
   DESCRIPTION
     "This entry controls the addition and deletion of 
      mscLpArc components."
::= { mscLpArc 1 }

mscLpArcRowStatusEntry OBJECT-TYPE
   SYNTAX     MscLpArcRowStatusEntry
   ACCESS     not-accessible
   STATUS     obsolete
   DESCRIPTION
     "A single entry in the table represents a single 
      mscLpArc component."
   INDEX {
      mscLpIndex,
      mscLpArcIndex }
::= { mscLpArcRowStatusTable 1 }

MscLpArcRowStatusEntry ::= SEQUENCE {
   mscLpArcRowStatus
      RowStatus,
   mscLpArcComponentName
      DisplayString,
   mscLpArcStorageType
      StorageType,
   mscLpArcIndex
      NonReplicated }

mscLpArcRowStatus OBJECT-TYPE
   SYNTAX     RowStatus 
   ACCESS     read-write
   STATUS     obsolete
   DESCRIPTION
     "This variable is used as the basis for SNMP naming of
      mscLpArc components.
      These components can be added and deleted."
::= { mscLpArcRowStatusEntry 1 }

