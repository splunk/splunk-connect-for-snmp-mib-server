--				apClrACLRangeTable(4)	      INTEGER	     R/W
--				apClrAddrFilterTbl(5)	      INTEGER	     R/W
--				apLdACLFrMUList(6)	      INTEGER	     R/W
--                           	apModemDialOut(7)             INTEGER        R/W
--                           	apModemHangUp(8)              INTEGER        R/W
--                           	apUpdateFirmware(9)           INTEGER        R/W
