--                              apACLViolations(1)            Counter        R
--				apSaveConfig(2)		      INTEGER	     R/W
--                           	apAccCtrlTable(3)
--                                  apACLEntry(1)
--                          		allowedMUIdx(1)       INTEGER        R
--                                      allowedMU(2)          PhysAddress    R/W
--				apACLRangeTable(4)	
--				    apACLRTEntry(1)
--					apACLRangeIdx(1)      INTEGER	     R
--                                      minMacAddr(2)         PhysAddress    R/W
--                                      maxMacAddr(3)         PhysAddress    R/W
