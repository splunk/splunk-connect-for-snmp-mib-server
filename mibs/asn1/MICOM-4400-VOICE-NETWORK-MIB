mcmVNETProfileCfgBckGrndNoiseLevel OBJECT-TYPE
	SYNTAX	INTEGER {
		neg25dbm0 (1),
		neg30dbm0 (2),
                neg35dbm0 (3),
                neg40dbm0 (4),
                neg45dbm0 (5)
		}
	ACCESS  read-write
	STATUS  mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = Signal input above this level is considered\
                  a voiced signal, below this level, the \
                  Voice Activity Detector (VAD), will  \
                  determine if it is a voiced signal or \
                  background noise.
		  (Operational);
	   HELP = Refer to the International \
                  Telecommunication Union(ITU-T) G.711 \
                  document for more information;
	   CAPABILITIES = ;"
	DEFVAL { neg30dbm0 }
	::= { mcmVNETProfileCfgEntry 32 }


mcmVNETProfileCfgSilenceHngOvrTime OBJECT-TYPE
	SYNTAX	INTEGER {
