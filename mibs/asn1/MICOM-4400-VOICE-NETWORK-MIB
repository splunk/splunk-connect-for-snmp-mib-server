--  the VNET Voice Switching Profile Configuration Table 
--  (Standard/Operational Parameters)

--  This table provides the VNET voice switching profile configuration parameters.
--  This table is used to configure a profile of voice channel switching configuration
--  characteristics; of which, a subset may be used for a particular voice channel
--  (of a specific interface type) to define its switching configuration. (Operational)

mcmVNETSwProfCfgTable OBJECT-TYPE
	SYNTAX  SEQUENCE OF McmVNETSwProfCfgEntry
	ACCESS  not-accessible
	STATUS  mandatory
	DESCRIPTION
	  "NAME = ; 
	   DESC = The VNET Voice Switching Profile Configuration \
		  table.(Operational);
	   HELP = ;
	   CAPABILITIES = NET_CFG, VPN_CFG;"
	::= { vnet-configuration 3 }

mcmVNETSwProfCfgEntry OBJECT-TYPE
	SYNTAX  McmVNETSwProfCfgEntry
	ACCESS  not-accessible
	STATUS  mandatory
	DESCRIPTION
	  "NAME = ; 
	   DESC = An entry in the VNET Voice Switching Profile \
		  Configuration table. (Operational);
	   HELP = ;
	   CAPABILITIES = ;"
	INDEX   { mcmVNETSwProfCfgProfileNum }
	::= { mcmVNETSwProfCfgTable 1 }

McmVNETSwProfCfgEntry ::= SEQUENCE {
	mcmVNETSwProfCfgProfileNum		INTEGER,
	mcmVNETSwProfCfgOutCallRestrict		INTEGER,
	mcmVNETSwProfCfgInCallRestrict		INTEGER,
	mcmVNETSwProfCfgCUGID			INTEGER,
	mcmVNETSwProfCfgRestrictClassCd		INTEGER,
	mcmVNETSwProfCfgAutoCallEntNum		INTEGER,
	mcmVNETSwProfCfgCallNegStrategy		INTEGER,
	mcmVNETSwProfCfgTransmitPriority	INTEGER,
	mcmVNETSwProfCfgAutoCallType		INTEGER,
	mcmVNETSwProfCfgAddServerSelect		INTEGER,
    mcmVNETSwProfCfgOutCallMode         INTEGER
	}

mcmVNETSwProfCfgProfileNum OBJECT-TYPE
	SYNTAX	INTEGER (1..32)
	ACCESS  read-only
	STATUS  mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = The value of this object indicates the profile \
		  number.  Used as an index to the VNET Voice \
		  Switching Profile Configuration table. \
		  (Operational);
	   HELP = ;
	   CAPABILITIES = ;"
	::= { mcmVNETSwProfCfgEntry 1 }

mcmVNETSwProfCfgOutCallRestrict OBJECT-TYPE
	SYNTAX	INTEGER {
		noRestriction (1),
		closedUserGroupCall (2),
		restrictionClassCall (3),
		bothCUGandRCCall (4),
		inhibitAll (5)
		}
	ACCESS  read-write
	STATUS  mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = The value of this object specifies how to \
		  handle outgoing calls for voice channel \
		  switching. (Operational);
	   HELP = 2: include CUG ID in outgoing call. \
		  3: include restriction class in outgoing call. \
		  4: include both CUG ID and restriction \
		  class in outgoing call. \
		  5: outgoing call is not allowed.;
	   CAPABILITIES = ;"
	DEFVAL { noRestriction }
	::= { mcmVNETSwProfCfgEntry 2 }

mcmVNETSwProfCfgInCallRestrict OBJECT-TYPE
	SYNTAX	INTEGER {
		noRestriction (1),
		checkCUGID (2),
		checkRestrictionClassCode (3),
		cUGandPublicClassInhibit (4),
		inhibitAll (5)
		}
	ACCESS  read-write
	STATUS  mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = The value of this object specifies how to \
		  handle incoming calls for voice channel \
		  switching. (Operational);
	   HELP = 2: check CUG ID in incoming call.  (Must \
		  match our CUG ID for call acceptance. \
		  3: check RCC in incoming call.  (Must be \
		  greater than or equal to our RCC for call \
		  acceptance. \
		  4: check both CUG and RCC in incoming \
		  call. \
		  5: incoming call is not allowed.;
	   CAPABILITIES = ;"
	DEFVAL { noRestriction }
	::= { mcmVNETSwProfCfgEntry 3 }

mcmVNETSwProfCfgCUGID OBJECT-TYPE
	SYNTAX	INTEGER (0..255)
	ACCESS  read-write
	STATUS  mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = The value of this object assigns the Closed \
		  User Group ID for a voice channel. (Operational);
	   HELP = ;
	   CAPABILITIES = ;"
	DEFVAL { 0 }
	::= { mcmVNETSwProfCfgEntry 4 }

mcmVNETSwProfCfgRestrictClassCd OBJECT-TYPE
	SYNTAX	INTEGER (0..255)
	ACCESS  read-write
	STATUS  mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = The value of this object assigns the Restriction \
		  Class Code for a voice channel. (Operational);
	   HELP = ;
	   CAPABILITIES = ;"
	DEFVAL { 0 }
	::= { mcmVNETSwProfCfgEntry 5 }

mcmVNETSwProfCfgAutoCallEntNum OBJECT-TYPE
	SYNTAX	INTEGER (0..32)
	ACCESS  read-write
	STATUS  mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = The value of this object provides a voice \
		  channel Auto Call destination number by \
		  specifying an index into the ingress table. \
		  (Operational);
	   HELP = 0: auto call disabled. \
		  1-32: index to the ingress table entry where \
		  the dial number is specified. \
                  The auto call for the ISDN BRI voice card is \
                  always disabled.;
	   CAPABILITIES = ;"
	DEFVAL { 0 }
	::= { mcmVNETSwProfCfgEntry 6 }

mcmVNETSwProfCfgCallNegStrategy OBJECT-TYPE
	SYNTAX	INTEGER {
		specific (1),
		bandwidth (2),
		delay (3)
		}
	ACCESS  read-write
	STATUS  mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = The value of this object specifies the preference \
		  for the voice algorithm when an outgoing call is \
		  made. (Operational);
	   HELP = specific: whatever is configured. \
		  bandwidth: bigger block size. \
		  delay: shorter block size.;
	   CAPABILITIES = ;"
	DEFVAL { specific }
	::= { mcmVNETSwProfCfgEntry 7 }

mcmVNETSwProfCfgTransmitPriority OBJECT-TYPE
	SYNTAX	INTEGER (0..15)
	ACCESS  read-write
	STATUS  mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = The value of this object assigns the priority \
		  of the voice data to be transmitted. (Operational);
	   HELP = 0: lowest priority. \
		  15: highest priority.;
	   CAPABILITIES = ;"
	DEFVAL { 11 }
	::= { mcmVNETSwProfCfgEntry 8 }

mcmVNETSwProfCfgAutoCallType OBJECT-TYPE
	SYNTAX	INTEGER {
                normal             (1),
                masterHootnHoller  (2),
                slaveHootnHoller   (3)
                }
	ACCESS  read-write
	STATUS  mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = Indicates the type of auto-call \
                  switching to be performed by the \
                  voice channel.;
	   HELP = ;
	   CAPABILITIES = ;"
	DEFVAL { normal }      
	::= { mcmVNETSwProfCfgEntry 9 }

mcmVNETSwProfCfgAddServerSelect OBJECT-TYPE
	SYNTAX	INTEGER {
                nas           (1),
                vncs          (2)
                }
	ACCESS  read-write
	STATUS  mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = Indicates which DNA server is used.;
	   HELP = ;
	   CAPABILITIES = ;"
	::= { mcmVNETSwProfCfgEntry 10 }

mcmVNETSwProfCfgOutCallMode OBJECT-TYPE
	SYNTAX	INTEGER {
                enblock-only (1),
                dynamic      (2)
                }
	ACCESS  read-write
	STATUS  mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = Indicates the type of call being made.;
	   HELP = ;
	   CAPABILITIES = ;"
	DEFVAL { dynamic }
	::= { mcmVNETSwProfCfgEntry 11 }


--  the VNET Voice Ingress Digit Filtering Configuration Table 
--  (Standard/Operational Parameters)

--  This table provides the VNET voice ingress digit filtering configuration 
--  parameters. (Operational)

mcmVNETIngressDigFilterTable OBJECT-TYPE
	SYNTAX  SEQUENCE OF McmVNETIngressDigFilterEntry
	ACCESS  not-accessible
	STATUS  mandatory
	DESCRIPTION
	  "NAME = ; 
	   DESC = The VNET Voice Ingress Digit Filtering \
		  Configuration table. (Operational);
	   HELP = ;
	   CAPABILITIES = NET_CFG, VPN_CFG;"
	::= { vnet-configuration 4 }

mcmVNETIngressDigFilterEntry OBJECT-TYPE
	SYNTAX  McmVNETIngressDigFilterEntry
	ACCESS  not-accessible
	STATUS  mandatory
	DESCRIPTION
	  "NAME = ; 
	   DESC = An entry in the VNET Voice Ingress Digit \
		  Filtering Configuration table. (Operational);
	   HELP = ;
	   CAPABILITIES = ;"
	INDEX   { mcmVNETIngressDigFilterIndex }
	::= { mcmVNETIngressDigFilterTable 1 }

McmVNETIngressDigFilterEntry ::= SEQUENCE {
	mcmVNETIngressDigFilterIndex		INTEGER,
	mcmVNETIngressDigFilterIngressNum	DisplayString,
	mcmVNETIngressDigFilterManipString	DisplayString
	}

mcmVNETIngressDigFilterIndex OBJECT-TYPE
	SYNTAX	INTEGER (1..32)
	ACCESS  read-only
	STATUS  mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = The value of this object indicates the index to \
		  the ingress table.  Used as an index to the VNET \
		  Voice Ingress Digit Filtering Configuration table. \
		  Operational);
	   HELP = ;
	   CAPABILITIES = ;"
	::= { mcmVNETIngressDigFilterEntry 1 }

mcmVNETIngressDigFilterIngressNum OBJECT-TYPE
	SYNTAX	DisplayString (SIZE (0..40))
	ACCESS  read-write
	STATUS  mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = The value of this object selects the digit \
		  pattern to be matched.  (Null terminated \
		  character string.) (Operational);
	   HELP = Valid characters are decimal digits, '#', \
		  '*', '+', '[' and ']' .;
	   CAPABILITIES = ;"
	DEFVAL { "" }
	::= { mcmVNETIngressDigFilterEntry 2 }

mcmVNETIngressDigFilterManipString OBJECT-TYPE
	SYNTAX	DisplayString (SIZE (0..40))
	ACCESS  read-write
	STATUS  mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = The value of this object selects commands \
		  to produce the ingress digits for speed dial, \
		  call redirect, and source ID insertion features. \
		  (Operational);
	   HELP = Valid characters are decimal digits, '#', \
		  '*', 'D', and 'I'.;
	   CAPABILITIES = ;"
	DEFVAL { "#" }
	::= { mcmVNETIngressDigFilterEntry 3 }


--  the VNET Voice Egress Digit Manipulation Configuration Table 
--  (Standard/Operational Parameters)

--  This table provides the VNET voice egress digit manipulation configuration 
--  parameters. (Operational)

mcmVNETEgressDigManipTable OBJECT-TYPE
	SYNTAX  SEQUENCE OF McmVNETEgressDigManipEntry
	ACCESS  not-accessible
	STATUS  mandatory
	DESCRIPTION
	  "NAME = ; 
	   DESC = The VNET Voice Egress Digit Manipulation \
		  Configuration table. (Operational);
	   HELP = ;
	   CAPABILITIES = NET_CFG, VPN_CFG;"
	::= { vnet-configuration 5 }

mcmVNETEgressDigManipEntry OBJECT-TYPE
	SYNTAX  McmVNETEgressDigManipEntry
	ACCESS  not-accessible
	STATUS  mandatory
	DESCRIPTION
	  "NAME = ; 
	   DESC = An entry in the VNET Voice Egress Digit \
		  Manipulation Configuration table. (Operational);
	   HELP = ;
	   CAPABILITIES = ;"
	INDEX   { mcmVNETEgressDigManipIndex }
	::= { mcmVNETEgressDigManipTable 1 }

McmVNETEgressDigManipEntry ::= SEQUENCE {
	mcmVNETEgressDigManipIndex		INTEGER,
	mcmVNETEgressDigManipPhoneNum		DisplayString,
	mcmVNETEgressDigManipString		DisplayString,
	mcmVNETEgressDigManipChIDList		DisplayString
	}

mcmVNETEgressDigManipIndex OBJECT-TYPE
	SYNTAX	INTEGER (1..32)
	ACCESS  read-only
	STATUS  mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = The value of this object indicates the index to \
		  the egress table.  Used as an index to the VNET \
		  Voice Egress Digit Manipulation Configuration \
		  table. Operational);
	   HELP = ;
	   CAPABILITIES = ;"
	::= { mcmVNETEgressDigManipEntry 1 }

mcmVNETEgressDigManipPhoneNum OBJECT-TYPE
	SYNTAX	DisplayString (SIZE (0..40))
	ACCESS  read-write
	STATUS  mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = The value of this object selects the phone \
		  number that voice channels are associated with. \
		  This is the number to be registered to the Address \
		  Server (RSA). (Operational);
	   HELP = Valid characters are decimal digits, '#', and '*'.;
	   CAPABILITIES = ;"
	DEFVAL { "" }
	::= { mcmVNETEgressDigManipEntry 2 }

mcmVNETEgressDigManipString OBJECT-TYPE
	SYNTAX	DisplayString (SIZE (0..40))
	ACCESS  read-write
	STATUS  mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = The value of this object selects commands \
		  to produce the egress digits for FXO, E&M, \
		  or FXS 3-Port interface.  The result is the \
		  number to be sent to the connected phone \
		  device. (Operational);
	   HELP = Valid characters are decimal digits, '#', '*', \
		  '+', 'D', 'H', and 'I'. The character 'H' is used \
		  used to trigger immediate dial handling by the PBX.;  
	   CAPABILITIES = ;"
	DEFVAL { "" }
	::= { mcmVNETEgressDigManipEntry 3 }

mcmVNETEgressDigManipChIDList OBJECT-TYPE
	SYNTAX	DisplayString (SIZE (0..70))
	ACCESS  read-write
	STATUS  mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = The value of this object selects voice channels \
		  associated with the phone number.  The valid \
		  string consists of the substring 'B1'~'B6', \
		  'C1'~'C12', 'D1'~'D12', and 'E1'~'E2'. \
		  (Operational);
	   HELP = Valid characters are decimal digits, 'B', 'C', \
		  'D', and 'E'.;
	   CAPABILITIES = ;"
	DEFVAL { "" }
	::= { mcmVNETEgressDigManipEntry 4 }



--  "Parallel" Configuration Parameter ("nvm") Groups/Tables


--  the VNET Voice Channel Configuration Table 
--  (Configuration Parameters)

--  This table provides the VNET voice channel configuration parameters
--  for identifying a voice channel with its corresponding voice profile. (Configuration)

nvmVNETChCfgTable OBJECT-TYPE
	SYNTAX  SEQUENCE OF NvmVNETChCfgEntry
	ACCESS  not-accessible
	STATUS  mandatory
	DESCRIPTION
	  "NAME = ; 
	   DESC = The VNET Voice Channel Configuration table. \
		  (Configuration);
	   HELP = ;
	   CAPABILITIES = NET_CFG, VPN_CFG;"
	::= { vnet-configuration 6 }

nvmVNETChCfgEntry OBJECT-TYPE
	SYNTAX  NvmVNETChCfgEntry
	ACCESS  not-accessible
	STATUS  mandatory
	DESCRIPTION
	  "NAME = ; 
	   DESC = An entry in the VNET Voice Channel Configuration \
		  table. (Configuration);
	   HELP = ;
	   CAPABILITIES = ;"
	INDEX   { nvmVNETChCfgLimID, nvmVNETChCfgChannelID }
	::= { nvmVNETChCfgTable 1 }

NvmVNETChCfgEntry ::= SEQUENCE {
	nvmVNETChCfgLimID			INTEGER,
	nvmVNETChCfgChannelID			INTEGER,
	nvmVNETChCfgVoiceProfile		INTEGER,
	nvmVNETChCfgSwitchingProfile		INTEGER,
	nvmVNETChCfgInterfaceType		INTEGER
	}

nvmVNETChCfgLimID OBJECT-TYPE
	SYNTAX	INTEGER {
		lim1 (2),
		lim2 (3),
		lim3 (4),
		lim4 (5),
		limB (6),
		limC (7),
		limD (8),
		limE (9)
		}
	ACCESS  read-only
	STATUS  mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = The card lim (logical interface module) number of a \
		  particular voice channel.  Used as an index to the \
		  VNET Voice Channel Configuration table. (Configuration);
	   HELP = ;
	   CAPABILITIES = ;"
	::= { nvmVNETChCfgEntry 1 }

nvmVNETChCfgChannelID OBJECT-TYPE
	SYNTAX	INTEGER (1..12)
	ACCESS  read-only
	STATUS  mandatory
	DESCRIPTION
  "NAME = ;
	   DESC = The position of the voice channel in the lim. \
		  Used as an index to the VNET Voice \
		  Channel Configuration table. (Configuration);
	   HELP = ;
	   CAPABILITIES = ;"
	::= { nvmVNETChCfgEntry 2 }

nvmVNETChCfgVoiceProfile OBJECT-TYPE
	SYNTAX	INTEGER (1..32)
	ACCESS  read-only
	STATUS  mandatory
	DESCRIPTION
	  "NAME = ; 
	   DESC = The value of this object indicates the profile \
		  to be used to configure the specified voice \
		  channel. (Configuration);
	   HELP = ;
	   CAPABILITIES = ;"
	DEFVAL { 1 }
	::= { nvmVNETChCfgEntry 3 }

nvmVNETChCfgSwitchingProfile OBJECT-TYPE
	SYNTAX	INTEGER (1..32)
	ACCESS  read-only
	STATUS  mandatory
	DESCRIPTION
	  "NAME = ; 
	   DESC = The value of this object indicates the profile \
		  to be used for a switching call. (Configuration);
	   HELP = ;
	   CAPABILITIES = ;"
	DEFVAL { 1 }
	::= { nvmVNETChCfgEntry 4 }

nvmVNETChCfgInterfaceType OBJECT-TYPE
	SYNTAX	INTEGER {
		not-available (1),
		enhanced-loop-start-FXS (2),
		enhanced-EandM (3),
		enhanced-loop-start-FXO (4),
		low-cost-loop-start-FXS (5),
		low-cost-EandM (6),
		low-cost-loop-start-FXO (7),
		ground-start-FXS (8),
		ground-start-FXO (9),
		three-port (10),
		dvm (11),
		bri (12)
		}
	ACCESS  read-only
	STATUS  mandatory
	DESCRIPTION
	  "NAME = ; 
	   DESC = The value of this object indicates the interface \
		  type of the specified voice channel. (Configuration);
	   HELP = Upon powerup, the interface type will not be \
		  available until the DSP software is loaded and its \
		  status is reported.;
	   CAPABILITIES = ;"
	::= { nvmVNETChCfgEntry 5 }


--  the VNET Voice Profile Configuration Table 
--  (Configuration Parameters)

--  This table provides the VNET voice profile configuration parameters.
--  This table is used to view a profile of voice channel configuration
--  characteristics; of which, a subset may be used for a particular voice
--  channel (of a specific interface type) to define its configuration. (Configuration)

nvmVNETProfileCfgTable OBJECT-TYPE
	SYNTAX  SEQUENCE OF NvmVNETProfileCfgEntry
	ACCESS  not-accessible
	STATUS  mandatory
	DESCRIPTION
	  "NAME = ; 
	   DESC = The VNET Voice Profile Configuration table. \
		  (Configuration);
	   HELP = ;
	   CAPABILITIES = NET_CFG, VPN_CFG;"
	::= { vnet-configuration 7 }

nvmVNETProfileCfgEntry OBJECT-TYPE
	SYNTAX  NvmVNETProfileCfgEntry
	ACCESS  not-accessible
	STATUS  mandatory
	DESCRIPTION
	  "NAME = ; 
	   DESC = An entry in the VNET Voice Profile Configuration \
		  table. (Configuration);
	   HELP = ;
	   CAPABILITIES = ;"
	INDEX   { nvmVNETProfileCfgProfileNum }
	::= { nvmVNETProfileCfgTable 1 }

NvmVNETProfileCfgEntry ::= SEQUENCE {
	nvmVNETProfileCfgProfileNum		INTEGER,
	nvmVNETProfileCfgMode			INTEGER,
	nvmVNETProfileCfgDigitizingRate		INTEGER,
	nvmVNETProfileCfgInputLevelGain		INTEGER,
	nvmVNETProfileCfgOutputLevelAttn	INTEGER,
	nvmVNETProfileCfgBusyOutMode		INTEGER,
	nvmVNETProfileCfgBandwidth		INTEGER,
	nvmVNETProfileCfgBackground		INTEGER,
	nvmVNETProfileCfgBRIPulseRate		INTEGER,
	nvmVNETProfileCfgEMSigFormat		INTEGER,
	nvmVNETProfileCfgFXSSigFormat		INTEGER,
	nvmVNETProfileCfgDVMSigFormat		INTEGER,
	nvmVNETProfileCfgNumberOfRings		INTEGER,
	nvmVNETProfileCfgEMAnalOper		INTEGER,
	nvmVNETProfileCfgRingingFreq		INTEGER,
	nvmVNETProfileCfgFaxDigRate		INTEGER,
	nvmVNETProfileCfgDiscSupervision	INTEGER,
	nvmVNETProfileCfgLineImpedance		INTEGER,
	nvmVNETProfileCfgMaxOutputLevel		INTEGER,
	nvmVNETProfileCfgRegenDelay		INTEGER,
	nvmVNETProfileCfgDialDigTimeLimit	INTEGER,
	nvmVNETProfileCfgMaxNumForDig		INTEGER,
	nvmVNETProfileCfgRegenFormat		INTEGER,
	nvmVNETProfileCfgCallProgTone		INTEGER,
	nvmVNETProfileCfgDTMFDetector		INTEGER,
	nvmVNETProfileCfgJitters		INTEGER,
	nvmVNETProfileCfgEchoCanceller		INTEGER,
	nvmVNETProfileCfgAutoGainControl	INTEGER,
	nvmVNETProfileCfgCompanderFormat	INTEGER,
	nvmVNETProfileCfgPremiumVoice		INTEGER,
	nvmVNETProfileCfgEndOfDialChar		INTEGER,
        nvmVNETProfileCfgBckGrndNoiseLevel      INTEGER,
        nvmVNETProfileCfgSilenceHngOvrTime      INTEGER,
        nvmVNETProfileCfgIdlePattern            INTEGER,
        nvmVNETProfileCfgEcanFilterLength       INTEGER,
        nvmVNETProfileCfgEcanErlImprovement     INTEGER,
        nvmVNETProfileCfgNoiseFloorOffset       INTEGER,
        nvmVNETProfileCfgDtmfGenBurstLength     INTEGER,
        nvmVNETProfileCfgDtmRegenBurstLength    INTEGER
	}

nvmVNETProfileCfgProfileNum OBJECT-TYPE
	SYNTAX	INTEGER (1..32)
	ACCESS  read-only
	STATUS  mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = The value of this object indicates the profile \
		  number.  Used as an index to the VNET Voice \
		  Profile Configuration table. (Configuration);
	   HELP = ;
	   CAPABILITIES = ;"
	::= { nvmVNETProfileCfgEntry 1 }

nvmVNETProfileCfgMode OBJECT-TYPE
	SYNTAX	INTEGER {
		voiceFax (1),
		voiceOnly (2)
		}
	ACCESS  read-only
	STATUS  mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = The value of this object selects the voice \
		  channel signal processing. (Configuration);
	   HELP = Can be used to set to either dual mode with \
		  both voice and fax, or just voice.;
	   CAPABILITIES = ;"
	DEFVAL { voiceFax }
	::= { nvmVNETProfileCfgEntry 2 }

nvmVNETProfileCfgDigitizingRate OBJECT-TYPE
	SYNTAX	INTEGER {
		rateG729 (1),
                rateG729-8k-40ms (2)
		}
	ACCESS  read-only
	STATUS  mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = The value of this object selects the voice \
		  digitizing algorithm. (Configuration);
	   HELP = Rate at which voice is digitized in \
		  designated in bits per second.;
	   CAPABILITIES = ;"
	DEFVAL { rateG729 }
	::= { nvmVNETProfileCfgEntry 3 }

nvmVNETProfileCfgInputLevelGain OBJECT-TYPE
	SYNTAX	INTEGER (-15..19)
	ACCESS  read-only
	STATUS  mandatory
	DESCRIPTION
	  "NAME = ; 
	   DESC = The value of this object selects the input \
		  signal gain in dB. (Configuration);
	   HELP = For DVM, max value is 8 dB.  If more than \
		  8 dB is configured, 8 dB will be used.\
                  The ISDN BRI voice card range is -15 to 15dB.;
	   CAPABILITIES = ;"
	DEFVAL { 0 }
	::= { nvmVNETProfileCfgEntry 4 }

nvmVNETProfileCfgOutputLevelAttn OBJECT-TYPE
	SYNTAX	INTEGER (-15..25)
	ACCESS  read-only
	STATUS  mandatory
	DESCRIPTION
	  "NAME = ; 
	   DESC = The value of this object selects the output \
		  signal attenuation in dB. (Configuration);
	   HELP = For DVM, max value is 8 dB.  If more than \
		  8 dB is configured, 8 dB will be used.;
	   CAPABILITIES = ;"
	DEFVAL { 0 }
	::= { nvmVNETProfileCfgEntry 5 }

nvmVNETProfileCfgBusyOutMode OBJECT-TYPE
	SYNTAX	INTEGER {
		systemControlled (1),
		forceOn (2),
		forceOff (3)
		}
	ACCESS  read-only
	STATUS  mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = The value of this object selects the voice \
		  channel busy out control type. (Configuration);
	   HELP = ;
	   CAPABILITIES = ;"
	DEFVAL { systemControlled }
	::= { nvmVNETProfileCfgEntry 6 }

nvmVNETProfileCfgBandwidth OBJECT-TYPE
	SYNTAX	INTEGER {
		voiceActivated (1),
		continuous (2)
		}
	ACCESS  read-only
	STATUS  mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = The value of this object selects the voice \
		  channel link bandwidth reservation style. \
		  (Configuration);
	   HELP = 2 is used for forced connects or PVCs only. \
		  1 is used for switched connects.;
	   CAPABILITIES = ;"
	DEFVAL { voiceActivated }
	::= { nvmVNETProfileCfgEntry 7 }

nvmVNETProfileCfgBackground OBJECT-TYPE
	SYNTAX	INTEGER {
		regenerated (1),
		silence (2)
		}
	ACCESS  read-only
	STATUS  mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = The value of this object selects the voice \
		  channel background noise generation type. \
		  (Configuration);
	   HELP = The ability of passing background noise or \ 
		  silencing the channel during idle periods. \
		  First option allows the background noise to be \
		  regenerated during the periods, whereas the \
		  second option suppresses background noise during \ 
		  idle periods.;
	   CAPABILITIES = ;"
	DEFVAL { regenerated }
	::= { nvmVNETProfileCfgEntry 8 }

nvmVNETProfileCfgBRIPulseRate OBJECT-TYPE
	SYNTAX	INTEGER {
		pulses10PerSec (1),
		pulses20PerSec (2),
		pulses125PerSec (3)
		}
	ACCESS  read-only
	STATUS  obsolete
	DESCRIPTION
	  "NAME = ;
	   DESC = The value of this object selects the voice \
		  channel pulse rate in pulses per second. \
		  (Configuration);
	   HELP = NOTE: This object is used by the Partner Card, \
                  not by the ISDN BRI voice card.;
	   CAPABILITIES = ;"
	DEFVAL { pulses10PerSec }
	::= { nvmVNETProfileCfgEntry 9 }

nvmVNETProfileCfgEMSigFormat OBJECT-TYPE
