	::= { mcmVNETChStatusEntry 8 }

mcmVNETChStatusRemoteChHwType OBJECT-TYPE
	SYNTAX	DisplayString (SIZE (0..15))
	ACCESS  read-only
	STATUS  mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = The value of this object indicates the remote \
		  voice channel hardware type (corresponding to \
		  the specified voice channel).;
	   HELP = This will be the NULL string if the specified \
		  voice channel is not connected.;
	   CAPABILITIES = ;"
	::= { mcmVNETChStatusEntry 9 }

