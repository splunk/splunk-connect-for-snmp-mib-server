	ENTERPRISE	micom-2vnet
	VARIABLES	{mcmSysAsciiTimeOfDay}
	DESCRIPTION
	  "NAME = ;
	   DESC = Indicates that the built image doesn't \
		  contain TUVM code.\
		  Severity Level: MAJOR.;
	   HELP = Access device code does not contain image for TUVM module. \
                  Please contact Sys Admin., or Field Personnel. ;"
	::= 7

mcmVNETNoDVMCodeImage TRAP-TYPE
	ENTERPRISE	micom-2vnet
	VARIABLES	{mcmSysAsciiTimeOfDay}
	DESCRIPTION
	  "NAME = ;
	   DESC = Indicates that the built image doesn't \
	          contain DVM code.\
                  Severity Level: MAJOR.;
	   HELP = Access device code does not contain image for DVM module. \
                  Please contact Sys Admin., or Field Personnel. ;"
	::= 8

