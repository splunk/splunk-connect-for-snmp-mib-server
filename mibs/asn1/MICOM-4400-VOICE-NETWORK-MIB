nvmVNETProfileCfgBckGrndNoiseLevel OBJECT-TYPE
	SYNTAX	INTEGER {
		neg25dbm0 (1),
		neg30dbm0 (2),
                neg35dbm0 (3),
                neg40dbm0 (4),
                neg45dbm0 (5)
		}
	ACCESS  read-only
	STATUS  mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = Signal input above this level is considered\
                  a voiced signal, below this level, the \
                  Voice Activity Detector (VAD), will  \
                  determine if it is a voiced signal or \
                  background noise.
		  (Configuration);
	   HELP = Refer to the International \
                  Telecommunication Union(ITU-T) G.711 \
                  document for more information;
	   CAPABILITIES = ;"
	DEFVAL { neg30dbm0 }
	::= { nvmVNETProfileCfgEntry 32 }

nvmVNETProfileCfgSilenceHngOvrTime OBJECT-TYPE
	SYNTAX	INTEGER {
		msec100 (1),
		msec200 (2),
                msec300 (3),
                msec400 (4),
                msec500 (5),
                msec600 (6),
                msec700 (7),
                msec800 (8)
		}
	ACCESS  read-only
	STATUS  mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = Indicates the time delay to switch \
                  from voice to silence.\
		  (Configuration);
	   HELP = ;
	   CAPABILITIES = ;"
	DEFVAL { msec300 }
	::= { nvmVNETProfileCfgEntry 33 }

nvmVNETProfileCfgIdlePattern OBJECT-TYPE
	SYNTAX	INTEGER ( 0..255 )
	ACCESS  read-only
	STATUS  mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = The value of this object represents the \
                  Idle pattern on the line when no call is up.\
                  Specific to a PBX. This is a permanent DSP \
                  configuration parameter;
	   HELP = This value can be used to detect that a call \
                  has finished and that the echo canceller should \
                  be reset and that fax mode should be terminated.;
	   CAPABILITIES = ;"
	DEFVAL { 255 }
	::= { nvmVNETProfileCfgEntry 34 }


nvmVNETProfileCfgEcanFilterLength OBJECT-TYPE
	SYNTAX	INTEGER {
				length128 (1),
				length256 (2)
				}
	ACCESS  read-only
	STATUS  mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = The value of this object specifies the filter \
                  length for echo canceller.  This is a permanent \
                  DSP configuration parameter.;
	   HELP = A length of 128 is equal to 16ms.\
                  A length of 256 is equal to 32ms.; 
----                  NOTE: The ISDN BRI voice card supports 32ms only;
	   CAPABILITIES = ;"
	DEFVAL { 256 }
	::= { nvmVNETProfileCfgEntry  35 }

nvmVNETProfileCfgEcanErlImprovement OBJECT-TYPE
	SYNTAX	INTEGER ( -18..0 )
	ACCESS  read-only
	STATUS  mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC =  The value of this object is used for echo \
                  canceller Er1 improvement. This is a DSP \
                  Dynamic configuration parameter.  \
                   A signal will be attenuated by this dB \
                  amount before being passed through the \
                  echo canceller. After the echo canceller, \
                  it will be amplified by this same dB amount, \
                  for a net gain of 0 dB. \
                   This attenuation amplicfication cycle reduces \
                  the resolution of the signal ( -18 ..0 ).;
	   HELP = ;
	   CAPABILITIES = ;"
	DEFVAL { 0 } 
	::= { nvmVNETProfileCfgEntry 36 }

nvmVNETProfileCfgNoiseFloorOffset OBJECT-TYPE
	SYNTAX	INTEGER ( 0..255 )
	ACCESS  read-only
	STATUS  mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = The value of this object will suppress the \
                  impulsive backround noise in order to achieve \
                  silence suppression savings.
	   HELP = ;
	   CAPABILITIES = ;"
	DEFVAL { 0 }
	::= { nvmVNETProfileCfgEntry 37 }

nvmVNETProfileCfgDtmfGenBurstLength OBJECT-TYPE
	SYNTAX	INTEGER {
                time50msec  (1),
                time60msec  (2),
                time70msec  (3),
                time100msec (4)
                }
	ACCESS  read-only
	STATUS  mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = DTMF burst generation in milliseconds.; 
	   HELP = ;
	   CAPABILITIES = ;"
	DEFVAL { 100 }
	::= { nvmVNETProfileCfgEntry 38 }

nvmVNETProfileCfgDtmRegenBurstLength OBJECT-TYPE
	SYNTAX	INTEGER {
                time50msec  (1),
                time60msec  (2),
                time70msec  (3),
                time100msec (4)
                }
	ACCESS  read-only
	STATUS  mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = DTMF burst Regeneration length in milliseconds.;
	   HELP = ;
	   CAPABILITIES = ;"
	DEFVAL { 100 }
	::= { nvmVNETProfileCfgEntry 39 }

--  the VNET Voice Switching Profile Configuration Table 
--  (Configuration Parameters)

--  This table provides the VNET voice switching profile configuration parameters.
--  This table is used to view a profile of voice channel switching configuration
--  characteristics; of which, a subset may be used for a particular voice channel
--  (of a specific interface type) to define its switching configuration. (Configuration)

nvmVNETSwProfCfgTable OBJECT-TYPE
	SYNTAX  SEQUENCE OF NvmVNETSwProfCfgEntry
	ACCESS  not-accessible
	STATUS  mandatory
	DESCRIPTION
	  "NAME = ; 
	   DESC = The VNET Voice Switching Profile Configuration \
		  table.(Configuration);
	   HELP = ;
	   CAPABILITIES = NET_CFG, VPN_CFG;"
	::= { vnet-configuration 8 }

nvmVNETSwProfCfgEntry OBJECT-TYPE
	SYNTAX  NvmVNETSwProfCfgEntry
	ACCESS  not-accessible
	STATUS  mandatory
	DESCRIPTION
	  "NAME = ; 
	   DESC = An entry in the VNET Voice Switching Profile \
		  Configuration table. (Configuration);
	   HELP = ;
	   CAPABILITIES = ;"
	INDEX   { nvmVNETSwProfCfgProfileNum }
	::= { nvmVNETSwProfCfgTable 1 }

NvmVNETSwProfCfgEntry ::= SEQUENCE {
	nvmVNETSwProfCfgProfileNum		INTEGER,
	nvmVNETSwProfCfgOutCallRestrict		INTEGER,
	nvmVNETSwProfCfgInCallRestrict		INTEGER,
	nvmVNETSwProfCfgCUGID			INTEGER,
	nvmVNETSwProfCfgRestrictClassCd		INTEGER,
	nvmVNETSwProfCfgAutoCallEntNum		INTEGER,
	nvmVNETSwProfCfgCallNegStrategy		INTEGER,
	nvmVNETSwProfCfgTransmitPriority	INTEGER,
	nvmVNETSwProfCfgAutoCallType		INTEGER,
    nvmVNETSwProfCfgAddServerSelect     INTEGER,
    nvmVNETSwProfCfgOutCallMode         INTEGER
	}

nvmVNETSwProfCfgProfileNum OBJECT-TYPE
	SYNTAX	INTEGER (1..32)
	ACCESS  read-only
	STATUS  mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = The value of this object indicates the profile \
		  number.  Used as an index to the VNET Voice \
		  Switching Profile Configuration table. \
		  (Configuration);
	   HELP = ;
	   CAPABILITIES = ;"
	::= { nvmVNETSwProfCfgEntry 1 }

nvmVNETSwProfCfgOutCallRestrict OBJECT-TYPE
	SYNTAX	INTEGER {
		noRestriction (1),
		closedUserGroupCall (2),
		restrictionClassCall (3),
		bothCUGandRCCall (4),
		inhibitAll (5)
		}
	ACCESS  read-only
	STATUS  mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = The value of this object specifies how to \
		  handle outgoing calls for voice channel \
		  switching. (Configuration);
	   HELP = 2: include CUG ID in outgoing call. \
		  3: include restriction class in outgoing call. \
		  4: include both CUG ID and restriction \
		  class in outgoing call. \
		  5: outgoing call is not allowed.;
	   CAPABILITIES = ;"
	DEFVAL { noRestriction }
	::= { nvmVNETSwProfCfgEntry 2 }

nvmVNETSwProfCfgInCallRestrict OBJECT-TYPE
	SYNTAX	INTEGER {
		noRestriction (1),
		checkCUGID (2),
		checkRestrictionClassCode (3),
		cUGandPublicClassInhibit (4),
		inhibitAll (5)
		}
	ACCESS  read-only
	STATUS  mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = The value of this object specifies how to \
		  handle incoming calls for voice channel \
		  switching. (Configuration);
	   HELP = 2: check CUG ID in incoming call.  (Must \
		  match our CUG ID for call acceptance. \
		  3: check RCC in incoming call.  (Must be \
		  greater than or equal to our RCC for call \
		  acceptance. \
		  4: check both CUG and RCC in incoming \
		  call. \
		  5: incoming call is not allowed.;
	   CAPABILITIES = ;"
	DEFVAL { noRestriction }
	::= { nvmVNETSwProfCfgEntry 3 }

nvmVNETSwProfCfgCUGID OBJECT-TYPE
	SYNTAX	INTEGER (0..255)
	ACCESS  read-only
	STATUS  mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = The value of this object assigns the Closed \
		  User Group ID for a voice channel. (Configuration);
	   HELP = ;
	   CAPABILITIES = ;"
	DEFVAL { 0 }
	::= { nvmVNETSwProfCfgEntry 4 }

nvmVNETSwProfCfgRestrictClassCd OBJECT-TYPE
	SYNTAX	INTEGER (0..255)
	ACCESS  read-only
	STATUS  mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = The value of this object assigns the Restriction \
		  Class Code for a voice channel. (Configuration);
	   HELP = ;
	   CAPABILITIES = ;"
	DEFVAL { 0 }
	::= { nvmVNETSwProfCfgEntry 5 }

nvmVNETSwProfCfgAutoCallEntNum OBJECT-TYPE
	SYNTAX	INTEGER (0..32)
	ACCESS  read-only
	STATUS  mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = The value of this object provides a voice \
		  channel Auto Call destination number by \
		  specifying an index into the ingress table. \
		  (Configuration);
	   HELP = 0: auto call disabled. \
		  1-32: index to the ingress table entry where \
		  the dial number is specified. \
                  The auto call for the ISDN BRI voice card is always \
                  disabled.;
	   CAPABILITIES = ;"
	DEFVAL { 0 }
	::= { nvmVNETSwProfCfgEntry 6 }

nvmVNETSwProfCfgCallNegStrategy OBJECT-TYPE
	SYNTAX	INTEGER {
		specific (1),
		bandwidth (2),
		delay (3)
		}
	ACCESS  read-only
	STATUS  mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = The value of this object specifies the preference \
		  for the voice algorithm when an outgoing call is \
		  made. (Configuration);
	   HELP = specific: whatever is configured. \
		  bandwidth: bigger block size. \
		  delay: shorter block size.;
	   CAPABILITIES = ;"
	DEFVAL { specific }
	::= { nvmVNETSwProfCfgEntry 7 }

nvmVNETSwProfCfgTransmitPriority OBJECT-TYPE
	SYNTAX	INTEGER (0..15)
	ACCESS  read-only
	STATUS  mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = The value of this object assigns the priority \
		  of the voice data to be transmitted. (Configuration);
	   HELP = 0: lowest priority. \
		  15: highest priority.;
	   CAPABILITIES = ;"
	DEFVAL { 11 }
	::= { nvmVNETSwProfCfgEntry 8 }

nvmVNETSwProfCfgAutoCallType OBJECT-TYPE
	SYNTAX	INTEGER {
                normal             (1),
                masterHootnHoller  (2),
                slaveHootnHoller   (3)
                }
	ACCESS  read-only
	STATUS  mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = Indicates the type of auto-call \
                  switching to be performed by the \
                  voice channel.;
	   HELP = ;
	   CAPABILITIES = ;"
	DEFVAL { normal }
	::= { nvmVNETSwProfCfgEntry 9 }

nvmVNETSwProfCfgAddServerSelect OBJECT-TYPE
	SYNTAX	INTEGER {
                nas      (1),
                vncs     (2)
                }
	ACCESS  read-only
	STATUS  mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = Indicates which DNA server is used.;
	   HELP = ;
	   CAPABILITIES = ;"
	::= { nvmVNETSwProfCfgEntry 10 }

nvmVNETSwProfCfgOutCallMode OBJECT-TYPE
	SYNTAX	INTEGER {
                enblock-only (1),
                dynamic      (2)
                }
	ACCESS  read-only
	STATUS  mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = Indicates the type of call being made.;
	   HELP = ;
	   CAPABILITIES = ;"
	::= { nvmVNETSwProfCfgEntry 11 }


