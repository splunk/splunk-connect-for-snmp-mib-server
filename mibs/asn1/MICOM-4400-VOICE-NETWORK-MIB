	SYNTAX	DisplayString (SIZE (0..40))
	ACCESS  read-only
	STATUS  mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = The value of this object selects the digit \
		  pattern to be matched.  (Null terminated \
		  character string.) (Configuration);
	   HELP = Valid characters are decimal digits, '#', \
		  '*', '+', '[' and ']' .;
	   CAPABILITIES = ;"
	DEFVAL { "" }
	::= { nvmVNETIngressDigFilterEntry 2 }

