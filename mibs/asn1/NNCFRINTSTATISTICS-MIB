--------------------------------------------------------------------------------
-- nncFrIntStatisticsObject 2: Stream Stats Interval Table
--------------------------------------------------------------------------------

  nncFrStrStatIntervalTable            OBJECT-TYPE
    SYNTAX        SEQUENCE OF NncFrStrStatIntervalEntry
    MAX-ACCESS    not-accessible
    STATUS        current
    DESCRIPTION  "The nncFrStrStatIntervalTable contains objects for
            monitoring the performance of a frame relay stream over
            M historical intervals of 1hr each."
  ::= { nncFrIntStatisticsObjects 2 }


  nncFrStrStatIntervalEntry            OBJECT-TYPE
    SYNTAX        NncFrStrStatIntervalEntry
    MAX-ACCESS    not-accessible
    STATUS        current
    DESCRIPTION  "An entry in the 1hr interval statistics table.  Each
            conceptual row contains interval statistics for a par-
            ticular interval on a particular stream."
    INDEX        { ifIndex,
              nncFrStrStatIntervalNumber }
  ::= { nncFrStrStatIntervalTable 1 }


  NncFrStrStatIntervalEntry ::=
    SEQUENCE {
        nncFrStrStatIntervalNumber                      INTEGER,
        nncFrStrStatIntervalState                       NncExtIntvlStateType,
        nncFrStrStatIntervalAbsoluteIntervalNumber      INTEGER,
        nncFrStrStatIntervalInOctets                    NncExtCounter64,
        nncFrStrStatIntervalOutOctets                   NncExtCounter64,
        nncFrStrStatIntervalInUCastPackets              NncExtCounter64,
        nncFrStrStatIntervalOutUCastPackets             NncExtCounter64,
        nncFrStrStatIntervalInDiscards                  NncExtCounter64,
        nncFrStrStatIntervalOutDiscards                 NncExtCounter64,
        nncFrStrStatIntervalInErrors                    NncExtCounter64,
        nncFrStrStatIntervalOutErrors                   NncExtCounter64,
        nncFrStrStatIntervalSigUserProtErrors           NncExtCounter64,
        nncFrStrStatIntervalSigNetProtErrors            NncExtCounter64,
        nncFrStrStatIntervalSigUserLinkRelErrors        NncExtCounter64,
        nncFrStrStatIntervalSigNetLinkRelErrors         NncExtCounter64,
        nncFrStrStatIntervalSigUserChanInactive         NncExtCounter64,
        nncFrStrStatIntervalSigNetChanInactive          NncExtCounter64,
        nncFrStrStatIntervalStSCAlarms                  Counter32,
        nncFrStrStatIntervalStTimeSC                    Gauge32,
        nncFrStrStatIntervalStMaxDurationRED            Counter32,
        nncFrStrStatIntervalStMCAlarms                  Counter32,
        nncFrStrStatIntervalStTimeMC                    Gauge32,
        nncFrStrStatIntervalOutLinkUtilization          Gauge32(0..100),
        nncFrStrStatIntervalInLinkUtilization           Gauge32(0..100),
        nncFrStrStatIntervalInInvdLength                NncExtCounter64,
        nncFrStrStatIntervalStLastErroredDLCI           Integer32(0..8388607),
        nncFrStrStatIntervalInDiscdOctetsCOS            NncExtCounter64,
        nncFrStrStatIntervalInDiscdCOSDESet             NncExtCounter64,
        nncFrStrStatIntervalInDiscdCOSDEClr             NncExtCounter64,
        nncFrStrStatIntervalInDiscdBadEncaps            NncExtCounter64,
        nncFrStrStatIntervalOutDiscdBadEncaps           NncExtCounter64,
        nncFrStrStatIntervalInDiscdUnsupEncaps          NncExtCounter64,
        nncFrStrStatIntervalOutDiscdUnsupEncaps         NncExtCounter64,
        nncFrStrStatIntervalOutDiscdDESet               NncExtCounter64,
        nncFrStrStatIntervalOutDiscdDEClr               NncExtCounter64,
        nncFrStrStatIntervalInDiscdDESet                NncExtCounter64,
        nncFrStrStatIntervalInDiscdDEClr                NncExtCounter64,
        nncFrStrStatIntervalStLMSigInvldField           NncExtCounter64,
        nncFrStrStatIntervalStLMSigUnsupMsgType         NncExtCounter64,
        nncFrStrStatIntervalStLMSigInvldEID             NncExtCounter64,
        nncFrStrStatIntervalStLMSigInvldIELen           NncExtCounter64,
        nncFrStrStatIntervalStLMSigInvldRepType         NncExtCounter64,
        nncFrStrStatIntervalStLMSigFrmWithNoIEs         NncExtCounter64,
        nncFrStrStatIntervalStUserSequenceErrs          NncExtCounter64,
        nncFrStrStatIntervalStNetSequenceErrs           NncExtCounter64,
        nncFrStrStatIntervalStLMUTimeoutsnT1            NncExtCounter64,
        nncFrStrStatIntervalStLMUStatusMsgsRcvd         NncExtCounter64,
        nncFrStrStatIntervalStLMUStatusENQMsgsSent      NncExtCounter64,
        nncFrStrStatIntervalStLMUAsyncStatusRcvd        NncExtCounter64,
        nncFrStrStatIntervalStLMNTimeoutsnT2            NncExtCounter64,
        nncFrStrStatIntervalStLMNStatusMsgsSent         NncExtCounter64,
        nncFrStrStatIntervalStLMNStatusENQMsgsRcvd      NncExtCounter64,
        nncFrStrStatIntervalStLMNAsyncStatusSent        NncExtCounter64,
        nncFrStrStatIntervalInCRCErrors                 NncExtCounter64,
        nncFrStrStatIntervalInNonIntegral               NncExtCounter64,
        nncFrStrStatIntervalInReservedDLCI              NncExtCounter64,
        nncFrStrStatIntervalInInvldEA                   NncExtCounter64,
        nncFrStrStatIntervalStFrmTooSmall               NncExtCounter64,
        nncFrStrStatIntervalInAborts                    NncExtCounter64,
        nncFrStrStatIntervalInSumOfDisagremnts          NncExtCounter64,
        nncFrStrStatIntervalInOverRuns                  NncExtCounter64,
        nncFrStrStatIntervalOutUnderRuns                NncExtCounter64,
        nncFrStrStatIntervalStIntervalDuration          Integer32,
        nncFrStrStatIntervalBestEffortPeakDelay         Gauge32(1..60000),
        nncFrStrStatIntervalCommittedPeakDelay          Gauge32(1..60000),
        nncFrStrStatIntervalLowDelayPeakDelay           Gauge32(1..60000),
        nncFrStrStatIntervalRealTimePeakDelay           Gauge32(1..60000),
        nncFrStrStatIntervalBestEffortAccDelay          NncExtCounter64,
        nncFrStrStatIntervalCommittedAccDelay           NncExtCounter64,
        nncFrStrStatIntervalLowDelayAccDelay            NncExtCounter64,
        nncFrStrStatIntervalRealTimeAccDelay            NncExtCounter64,
        nncFrStrStatIntervalBestEffortOutUCastPackets   NncExtCounter64,
        nncFrStrStatIntervalCommittedOutUCastPackets    NncExtCounter64,
        nncFrStrStatIntervalLowDelayOutUCastPackets     NncExtCounter64,
        nncFrStrStatIntervalRealTimeOutUCastPackets     NncExtCounter64,
        nncFrStrStatIntervalBestEffortUCastPacketsDEClr NncExtCounter64,
        nncFrStrStatIntervalCommittedUCastPacketsDEClr  NncExtCounter64,
        nncFrStrStatIntervalLowDelayUCastPacketsDEClr   NncExtCounter64,
        nncFrStrStatIntervalRealTimeUCastPacketsDEClr   NncExtCounter64,
        nncFrStrStatIntervalBestEffortDiscdDEClr        NncExtCounter64,
        nncFrStrStatIntervalCommittedDiscdDEClr         NncExtCounter64,
        nncFrStrStatIntervalLowDelayDiscdDEClr          NncExtCounter64,
        nncFrStrStatIntervalRealTimeDiscdDEClr          NncExtCounter64
    }

  nncFrStrStatIntervalNumber            OBJECT-TYPE
    SYNTAX        INTEGER (1..96)
    MAX-ACCESS    not-accessible
    STATUS        current
    DESCRIPTION  "The interval number (N) of the statistics in this row."
  ::= { nncFrStrStatIntervalEntry 1 }


