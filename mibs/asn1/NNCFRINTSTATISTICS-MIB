NNCFRINTSTATISTICS-MIB DEFINITIONS ::= BEGIN

    IMPORTS
        MODULE-IDENTITY, OBJECT-TYPE, OBJECT-IDENTITY,
            Integer32, Gauge32, Counter32
            FROM SNMPv2-SMI
        NncExtIntvlStateType
            FROM NNC-INTERVAL-STATISTICS-TC-MIB
        MODULE-COMPLIANCE, OBJECT-GROUP
            FROM SNMPv2-CONF
        ifIndex
            FROM IF-MIB
        nncExtensions, NncExtCounter64
            FROM NNCGNI0001-SMI;
            
    nncFrIntStatistics MODULE-IDENTITY
        LAST-UPDATED     "9803031200Z" 
        ORGANIZATION     "Newbridge Networks Corporation"
        CONTACT-INFO
                        "Newbridge Networks Corporation
                         Postal: 600 March Road
                             Kanata, Ontario
                             Canada K2K 2E6

                         Phone:  +1 613 591 3600
                         Fax:    +1 613 591 3680"
        
        DESCRIPTION  "This module contains definitions for
                 performance monitoring of Frame Relay Streams"
    
    ::= { nncExtensions 30 }


    nncFrIntStatisticsObjects OBJECT IDENTIFIER ::= { nncFrIntStatistics 1 }
    nncFrIntStatisticsTraps OBJECT IDENTIFIER ::= { nncFrIntStatistics 2 }
    nncFrIntStatisticsGroups OBJECT IDENTIFIER ::= { nncFrIntStatistics 3 }
    nncFrIntStatisticsCompliances OBJECT IDENTIFIER ::= { nncFrIntStatistics 4 }

--------------------------------------------------------------------------------
-- nncFrIntStatisticsObjects - Object Definitions
--
-- This MIB module contains the following objects:
--  1. FR Stream Stats Current Table
--  2. FR Stream Stats Interval Table
--  3. FR PVC Endpoint Stats Current Table
--  4. FR PVC Endpoint Stats Interval Table
--  5. nncFrDepthOfHistoricalStrata - depth of historical intervals
--  6. FR Stream BERT statistics Table
--
-- NOTE:
-- 1. For interval statistics tables, statistics are collected for 'M' intervals
-- of 1 hour each.  Presently, resource limitations on the card limit support
-- to the case M = 1.
--------------------------------------------------------------------------------

--------------------------------------------------------------------------------
-- nncFrIntStatisticsObject 1: Streams Stats Current Table
--------------------------------------------------------------------------------

  nncFrStrStatCurrentTable            OBJECT-TYPE
    SYNTAX        SEQUENCE OF NncFrStrStatCurrentEntry
    MAX-ACCESS    not-accessible
    STATUS        current
    DESCRIPTION  "The nncFrStrStatCurrentTable contains objects for
            monitoring the performance of a frame relay stream during
            the current 1hr interval."
  ::= { nncFrIntStatisticsObjects 1 }


  nncFrStrStatCurrentEntry            OBJECT-TYPE
    SYNTAX        NncFrStrStatCurrentEntry
    MAX-ACCESS    not-accessible
    STATUS        current
    DESCRIPTION  "An entry in the 1hr current statistics table.  Each
            conceptual row contains current interval statistics for
            a particular frame relay stream."
    INDEX        { ifIndex }
  ::= { nncFrStrStatCurrentTable 1 }


  NncFrStrStatCurrentEntry ::=
    SEQUENCE {
        nncFrStrStatCurrentState                       NncExtIntvlStateType,
        nncFrStrStatCurrentAbsoluteIntervalNumber      INTEGER,
        nncFrStrStatCurrentInOctets                    NncExtCounter64,
        nncFrStrStatCurrentOutOctets                   NncExtCounter64,
        nncFrStrStatCurrentInUCastPackets              NncExtCounter64,
        nncFrStrStatCurrentOutUCastPackets             NncExtCounter64,
        nncFrStrStatCurrentInDiscards                  NncExtCounter64,
        nncFrStrStatCurrentOutDiscards                 NncExtCounter64,
        nncFrStrStatCurrentInErrors                    NncExtCounter64,
        nncFrStrStatCurrentOutErrors                   NncExtCounter64,
        nncFrStrStatCurrentSigUserProtErrors           NncExtCounter64,
        nncFrStrStatCurrentSigNetProtErrors            NncExtCounter64,
        nncFrStrStatCurrentSigUserLinkRelErrors        NncExtCounter64,
        nncFrStrStatCurrentSigNetLinkRelErrors         NncExtCounter64,
        nncFrStrStatCurrentSigUserChanInactive         NncExtCounter64,
        nncFrStrStatCurrentSigNetChanInactive          NncExtCounter64,
        nncFrStrStatCurrentStSCAlarms                  Counter32,
        nncFrStrStatCurrentStTimeSC                    Gauge32,
        nncFrStrStatCurrentStMaxDurationRED            Counter32,
        nncFrStrStatCurrentStMCAlarms                  Counter32,
        nncFrStrStatCurrentStTimeMC                    Gauge32,
        nncFrStrStatCurrentOutLinkUtilization          Gauge32(0..100),
        nncFrStrStatCurrentInLinkUtilization           Gauge32(0..100),
        nncFrStrStatCurrentInInvdLength                NncExtCounter64,
        nncFrStrStatCurrentStLastErroredDLCI           Integer32(0..8388607),
        nncFrStrStatCurrentInDiscdOctetsCOS            NncExtCounter64,
        nncFrStrStatCurrentInDiscdCOSDESet             NncExtCounter64,
        nncFrStrStatCurrentInDiscdCOSDEClr             NncExtCounter64,
        nncFrStrStatCurrentInDiscdBadEncaps            NncExtCounter64,
        nncFrStrStatCurrentOutDiscdBadEncaps           NncExtCounter64,
        nncFrStrStatCurrentInDiscdUnsupEncaps          NncExtCounter64,
        nncFrStrStatCurrentOutDiscdUnsupEncaps         NncExtCounter64,
        nncFrStrStatCurrentOutDiscdDESet               NncExtCounter64,
        nncFrStrStatCurrentOutDiscdDEClr               NncExtCounter64,
        nncFrStrStatCurrentInDiscdDESet                NncExtCounter64,
        nncFrStrStatCurrentInDiscdDEClr                NncExtCounter64,
        nncFrStrStatCurrentStLMSigInvldField           NncExtCounter64,
        nncFrStrStatCurrentStLMSigUnsupMsgType         NncExtCounter64,
        nncFrStrStatCurrentStLMSigInvldEID             NncExtCounter64,
        nncFrStrStatCurrentStLMSigInvldIELen           NncExtCounter64,
        nncFrStrStatCurrentStLMSigInvldRepType         NncExtCounter64,
        nncFrStrStatCurrentStLMSigFrmWithNoIEs         NncExtCounter64,
        nncFrStrStatCurrentStUserSequenceErrs          NncExtCounter64,
        nncFrStrStatCurrentStNetSequenceErrs           NncExtCounter64,
        nncFrStrStatCurrentStLMUTimeoutsnT1            NncExtCounter64,
        nncFrStrStatCurrentStLMUStatusMsgsRcvd         NncExtCounter64,
        nncFrStrStatCurrentStLMUStatusENQMsgsSent      NncExtCounter64,
        nncFrStrStatCurrentStLMUAsyncStatusRcvd        NncExtCounter64,
        nncFrStrStatCurrentStLMNTimeoutsnT2            NncExtCounter64,
        nncFrStrStatCurrentStLMNStatusMsgsSent         NncExtCounter64,
        nncFrStrStatCurrentStLMNStatusENQMsgsRcvd      NncExtCounter64,
        nncFrStrStatCurrentStLMNAsyncStatusSent        NncExtCounter64,
        nncFrStrStatCurrentInCRCErrors                 NncExtCounter64,
        nncFrStrStatCurrentInNonIntegral               NncExtCounter64,
        nncFrStrStatCurrentInReservedDLCI              NncExtCounter64,
        nncFrStrStatCurrentInInvldEA                   NncExtCounter64,
        nncFrStrStatCurrentStFrmTooSmall               NncExtCounter64,
        nncFrStrStatCurrentInAborts                    NncExtCounter64,
        nncFrStrStatCurrentInSumOfDisagremnts          NncExtCounter64,
        nncFrStrStatCurrentInOverRuns                  NncExtCounter64,
        nncFrStrStatCurrentOutUnderRuns                NncExtCounter64,
        nncFrStrStatCurrentStIntervalDuration          Integer32,
        nncFrStrStatCurrentBestEffortPeakDelay         Gauge32(1..60000),
        nncFrStrStatCurrentCommittedPeakDelay          Gauge32(1..60000),
        nncFrStrStatCurrentLowDelayPeakDelay           Gauge32(1..60000),
        nncFrStrStatCurrentRealTimePeakDelay           Gauge32(1..60000),
        nncFrStrStatCurrentBestEffortAccDelay          NncExtCounter64,
        nncFrStrStatCurrentCommittedAccDelay           NncExtCounter64,
        nncFrStrStatCurrentLowDelayAccDelay            NncExtCounter64,
        nncFrStrStatCurrentRealTimeAccDelay            NncExtCounter64,
        nncFrStrStatCurrentBestEffortOutUCastPackets   NncExtCounter64,
        nncFrStrStatCurrentCommittedOutUCastPackets    NncExtCounter64,
        nncFrStrStatCurrentLowDelayOutUCastPackets     NncExtCounter64,
        nncFrStrStatCurrentRealTimeOutUCastPackets     NncExtCounter64,
        nncFrStrStatCurrentBestEffortUCastPacketsDEClr NncExtCounter64,
        nncFrStrStatCurrentCommittedUCastPacketsDEClr  NncExtCounter64,
        nncFrStrStatCurrentLowDelayUCastPacketsDEClr   NncExtCounter64,
        nncFrStrStatCurrentRealTimeUCastPacketsDEClr   NncExtCounter64,
        nncFrStrStatCurrentBestEffortDiscdDEClr        NncExtCounter64,
        nncFrStrStatCurrentCommittedDiscdDEClr         NncExtCounter64,
        nncFrStrStatCurrentLowDelayDiscdDEClr          NncExtCounter64,
        nncFrStrStatCurrentRealTimeDiscdDEClr          NncExtCounter64
    }


  nncFrStrStatCurrentState            OBJECT-TYPE
    SYNTAX        NncExtIntvlStateType
    MAX-ACCESS    read-only
    STATUS        current
    DESCRIPTION  "The state of the current interval. The object provides
            a status for those entries which have been reset by the
            user or have been subject to a wall-clock time change."
  ::= { nncFrStrStatCurrentEntry 1 }

  nncFrStrStatCurrentAbsoluteIntervalNumber    OBJECT-TYPE
    SYNTAX        INTEGER (0..96)
    MAX-ACCESS    read-only
    STATUS        current
    DESCRIPTION  "The absolute interval number of this interval."
  ::= { nncFrStrStatCurrentEntry 2 }

  nncFrStrStatCurrentInOctets            OBJECT-TYPE
    SYNTAX        NncExtCounter64
    MAX-ACCESS    read-only
    STATUS        current
    DESCRIPTION  "Number of bytes received on this stream."
  ::= { nncFrStrStatCurrentEntry 3 }


  nncFrStrStatCurrentOutOctets            OBJECT-TYPE
    SYNTAX        NncExtCounter64
    MAX-ACCESS    read-only
    STATUS        current
    DESCRIPTION  "Number of bytes transmitted on this stream."
  ::= { nncFrStrStatCurrentEntry 4 }


  nncFrStrStatCurrentInUCastPackets        OBJECT-TYPE
    SYNTAX        NncExtCounter64
    MAX-ACCESS    read-only
    STATUS        current
    DESCRIPTION  "Number of unerrored, unicast frames received." 
  ::= { nncFrStrStatCurrentEntry 5 }


  nncFrStrStatCurrentOutUCastPackets        OBJECT-TYPE
    SYNTAX        NncExtCounter64
    MAX-ACCESS    read-only
    STATUS        current
    DESCRIPTION  "Number of unerrored, unicast frames transmitted."
  ::= { nncFrStrStatCurrentEntry 6 }


  nncFrStrStatCurrentInDiscards            OBJECT-TYPE
    SYNTAX        NncExtCounter64
    MAX-ACCESS    read-only
    STATUS        current
    DESCRIPTION  "Number of incoming frames discarded due to these
            reasons: policing, congestion."
  ::= { nncFrStrStatCurrentEntry 7 }


  nncFrStrStatCurrentOutDiscards        OBJECT-TYPE
    SYNTAX        NncExtCounter64
    MAX-ACCESS    read-only
    STATUS        current
    DESCRIPTION  "Number of outgoing frames discarded due to these
            reasons: policing, congestion."
  ::= { nncFrStrStatCurrentEntry 8 }


  nncFrStrStatCurrentInErrors            OBJECT-TYPE
    SYNTAX        NncExtCounter64
    MAX-ACCESS    read-only
    STATUS        current
    DESCRIPTION  "Number of incoming frames discarded due to errors:
            invalid lengths, non-integral bytes, CRC errors, 
            bad encapsulation, invalid EA, reserved DLCI"
  ::= { nncFrStrStatCurrentEntry 9 }


  nncFrStrStatCurrentOutErrors            OBJECT-TYPE
    SYNTAX        NncExtCounter64
    MAX-ACCESS    read-only
    STATUS        current
    DESCRIPTION  "Number of outgoing frames discarded due to errors:
            eg. bad encapsulation."
  ::= { nncFrStrStatCurrentEntry 10 }


  nncFrStrStatCurrentSigUserProtErrors        OBJECT-TYPE
    SYNTAX        NncExtCounter64
    MAX-ACCESS    read-only
    STATUS        current
    DESCRIPTION  "Number of user-side local in-channel signalling
            protocol errors (protocol discriminator, message type,
            call reference & mandatory information eleement errors)
            for this uni/nni stream.  If the stream is not perfor-
            ming user-side procedures, this value is equal to
            noSuchName."
  ::= { nncFrStrStatCurrentEntry 11 }


  nncFrStrStatCurrentSigNetProtErrors        OBJECT-TYPE
    SYNTAX        NncExtCounter64
    MAX-ACCESS    read-only
    STATUS        current
    DESCRIPTION  "Number of network-side local in-channel signalling
            protocol errors (protocol discriminator, message type,
            call reference & mandatory information eleement errors)
            for this uni/nni stream.  If the stream is not perfor-
            ming network-side procedures, this value is equal to
            noSuchName."
  ::= { nncFrStrStatCurrentEntry 12 }


  nncFrStrStatCurrentSigUserLinkRelErrors    OBJECT-TYPE
    SYNTAX        NncExtCounter64
    MAX-ACCESS    read-only
    STATUS        current
    DESCRIPTION  "Number of user-side local in-channel signalling link
            reliability errors (non receipt of Enq/Status messages
            or invalid sequence numbers in a link integrity verifi-
            cation information element) for this stream.  If the
            stream is not performing user-side procedures, this
            value is equal to noSuchName."
  ::= { nncFrStrStatCurrentEntry 13 }


  nncFrStrStatCurrentSigNetLinkRelErrors    OBJECT-TYPE
    SYNTAX        NncExtCounter64
    MAX-ACCESS    read-only
    STATUS        current
    DESCRIPTION  "Number of network-side local in-channel signalling
            link reliability errors (non receipt of Enq/Status
            messages or invalid sequence numbers in a link integrity
            verification information element) for this stream.  If
            the stream is not performing network-side procedures,
            this value is equal to noSuchName."
  ::= { nncFrStrStatCurrentEntry 14 }


  nncFrStrStatCurrentSigUserChanInactive    OBJECT-TYPE
    SYNTAX        NncExtCounter64
    MAX-ACCESS    read-only
    STATUS        current
    DESCRIPTION  "Number of times the user-side channel was declared
            inactive (N2 errors in N3 events) for this stream.  If
            the stream is not performing user-side procedures, this
            value is equal to noSuchName."
  ::= { nncFrStrStatCurrentEntry 15 }


  nncFrStrStatCurrentSigNetChanInactive        OBJECT-TYPE
    SYNTAX        NncExtCounter64
    MAX-ACCESS    read-only
    STATUS        current
    DESCRIPTION  "Number of times the network-side channel was declared
            inactive (N2 errors in N3 events) for this stream.
            If the stream is not performing network-side procedures,
            this value is equal to noSuchName."
  ::= { nncFrStrStatCurrentEntry 16 }


  nncFrStrStatCurrentStSCAlarms OBJECT-TYPE
    SYNTAX        Counter32
    MAX-ACCESS    read-only
    STATUS        current
    DESCRIPTION  "Number of one second intervals for which the stream
            entered the severely congested state."
  ::= { nncFrStrStatCurrentEntry 17 }


  nncFrStrStatCurrentStTimeSC OBJECT-TYPE
    SYNTAX        Gauge32
    MAX-ACCESS    read-only
    STATUS        current
    DESCRIPTION  "Period of time the stream was in the severely congested state
            during the current interval (expressed in 10's of milliseconds).
            Divide into the entire length of the interval to get a ratio."
  ::= { nncFrStrStatCurrentEntry 18 }


  nncFrStrStatCurrentStMaxDurationRED OBJECT-TYPE
    SYNTAX        Counter32
    MAX-ACCESS    read-only
    STATUS        current
    DESCRIPTION  "Longest period of time the stream spent in the Red (severely
            congested) state, expressed in seconds."
  ::= { nncFrStrStatCurrentEntry 19 }


  nncFrStrStatCurrentStMCAlarms OBJECT-TYPE
    SYNTAX        Counter32
    MAX-ACCESS    read-only
    STATUS        current
    DESCRIPTION  "Number of one second intervals for which the stream
            entered the mildly congested state."
  ::= { nncFrStrStatCurrentEntry 20 }


  nncFrStrStatCurrentStTimeMC OBJECT-TYPE
    SYNTAX        Gauge32
    MAX-ACCESS    read-only
    STATUS        current
    DESCRIPTION  "Period of time the stream was in the mildly congested state
            during the current interval (expressed in 10's of milliseconds).
            Divide into the entire length of the interval to get a ratio."
  ::= { nncFrStrStatCurrentEntry 21 }


  nncFrStrStatCurrentOutLinkUtilization        OBJECT-TYPE
    SYNTAX        Gauge32(0..100)
    MAX-ACCESS    read-only
    STATUS        current
    DESCRIPTION  "The percentage utilization on the outgoing link
            during the current interval."
  ::= { nncFrStrStatCurrentEntry 22 }


  nncFrStrStatCurrentInLinkUtilization        OBJECT-TYPE
    SYNTAX        Gauge32(0..100)
    MAX-ACCESS    read-only
    STATUS        current
    DESCRIPTION  "The percentage utilization on the incoming link
            during the current interval."
  ::= { nncFrStrStatCurrentEntry 23 }


  nncFrStrStatCurrentInInvdLength OBJECT-TYPE
    SYNTAX        NncExtCounter64
    MAX-ACCESS    read-only
    STATUS        current
    DESCRIPTION  "Number of frames discarded because there was no data in
            the frame, or because the frame exceeded the maximum allowed
            length."
  ::= { nncFrStrStatCurrentEntry 24 }


  nncFrStrStatCurrentStLastErroredDLCI OBJECT-TYPE
    SYNTAX        Integer32(0..8388607)
    MAX-ACCESS    read-only
    STATUS        current
    DESCRIPTION  "DLCI of the most recent DLC to receive invalid frames
                 on this frame stream."
  ::= { nncFrStrStatCurrentEntry 25 }


  nncFrStrStatCurrentInDiscdOctetsCOS OBJECT-TYPE
    SYNTAX        NncExtCounter64
    MAX-ACCESS    read-only
    STATUS        current
    DESCRIPTION  "Number of ingress bytes discarded due to receive
            Class-of-Service procedures."
  ::= { nncFrStrStatCurrentEntry 26 }


  nncFrStrStatCurrentInDiscdCOSDESet OBJECT-TYPE
    SYNTAX        NncExtCounter64
    MAX-ACCESS    read-only
    STATUS        current
    DESCRIPTION  "Number of ingress frames discarded due to receive
            Class-of-Service procedures, in which the DE bit was set."
  ::= { nncFrStrStatCurrentEntry 27 }


  nncFrStrStatCurrentInDiscdCOSDEClr OBJECT-TYPE
    SYNTAX        NncExtCounter64
    MAX-ACCESS    read-only
    STATUS        current
    DESCRIPTION  "Number of ingress frames discarded due to receive
            Class-of-Service procedures, in which the DE bit was not set."
  ::= { nncFrStrStatCurrentEntry 28 }


  nncFrStrStatCurrentInDiscdBadEncaps        OBJECT-TYPE
    SYNTAX        NncExtCounter64
    MAX-ACCESS    read-only
    STATUS        current
    DESCRIPTION  "Number of ingress frames discarded due to having a bad
            RFC1490 encapsulation header."
  ::= { nncFrStrStatCurrentEntry 29 }


  nncFrStrStatCurrentOutDiscdBadEncaps        OBJECT-TYPE
    SYNTAX        NncExtCounter64
    MAX-ACCESS    read-only
    STATUS        current
    DESCRIPTION  "Number of egress frames discarded due to having a bad
            RFC1483 encapsulation header."
  ::= { nncFrStrStatCurrentEntry 30 }


  nncFrStrStatCurrentInDiscdUnsupEncaps        OBJECT-TYPE
    SYNTAX        NncExtCounter64
    MAX-ACCESS    read-only
    STATUS        current
    DESCRIPTION  "Number of ingress frames discarded due to having an
            unsupported RFC1490 encapsulation header."
  ::= { nncFrStrStatCurrentEntry 31 }


  nncFrStrStatCurrentOutDiscdUnsupEncaps        OBJECT-TYPE
    SYNTAX        NncExtCounter64
    MAX-ACCESS    read-only
    STATUS        current
    DESCRIPTION  "Number of egress frames discarded due to having an
            unsupported RFC1483 encapsulation header."
  ::= { nncFrStrStatCurrentEntry 32 }


  nncFrStrStatCurrentOutDiscdDESet OBJECT-TYPE
    SYNTAX        NncExtCounter64
    MAX-ACCESS    read-only
    STATUS        current
    DESCRIPTION  "Number of egress frames discarded due to transmit
            congestion procedures, in which the DE bit was set."
  ::= { nncFrStrStatCurrentEntry 33 }


  nncFrStrStatCurrentOutDiscdDEClr OBJECT-TYPE
    SYNTAX        NncExtCounter64
    MAX-ACCESS    read-only
    STATUS        current
    DESCRIPTION  "Number of egress frames discarded due to transmit
            congestion procedures, in which the DE bit was not set."
  ::= { nncFrStrStatCurrentEntry 34 }


  nncFrStrStatCurrentInDiscdDESet OBJECT-TYPE
    SYNTAX        NncExtCounter64
    MAX-ACCESS    read-only
    STATUS        current
    DESCRIPTION  "Number of ingress frames discarded due to receive
            congestion procedures, in which the DE bit was set."
  ::= { nncFrStrStatCurrentEntry 35 }


  nncFrStrStatCurrentInDiscdDEClr OBJECT-TYPE
    SYNTAX        NncExtCounter64
    MAX-ACCESS    read-only
    STATUS        current
    DESCRIPTION  "Number of ingress frames discarded due to receive
            congestion procedures, in which the DE bit was not set."
  ::= { nncFrStrStatCurrentEntry 36 }


  nncFrStrStatCurrentStLMSigInvldField        OBJECT-TYPE
    SYNTAX        NncExtCounter64
    MAX-ACCESS    read-only
    STATUS        current
    DESCRIPTION  "Number of Status signalling frames with incorrect values
            for the Unnumbered Information Frame field, Protocol
            Discrimator field, or Call Reference Field."
  ::= { nncFrStrStatCurrentEntry 37 }


  nncFrStrStatCurrentStLMSigUnsupMsgType        OBJECT-TYPE
    SYNTAX        NncExtCounter64
    MAX-ACCESS    read-only
    STATUS        current
    DESCRIPTION  "Number of Status signalling frames that are not a Status
            or Status Enquiry messsage (Unsupported message type)."
  ::= { nncFrStrStatCurrentEntry 38 }


  nncFrStrStatCurrentStLMSigInvldEID        OBJECT-TYPE
    SYNTAX        NncExtCounter64
    MAX-ACCESS    read-only
    STATUS        current
    DESCRIPTION  "Number of Status signalling frames with an IEID that is
            not a Report Type IEID, Keep Alive Sequence IEID, or a PVC
            Status IEID."
  ::= { nncFrStrStatCurrentEntry 39 }


  nncFrStrStatCurrentStLMSigInvldIELen        OBJECT-TYPE
    SYNTAX        NncExtCounter64
    MAX-ACCESS    read-only
    STATUS        current
    DESCRIPTION  "Number of Status signalling frames with an IE length
            field size that does not conform with the Frame Relay status
            signalling protocol."
  ::= { nncFrStrStatCurrentEntry 40 }


  nncFrStrStatCurrentStLMSigInvldRepType        OBJECT-TYPE
    SYNTAX        NncExtCounter64
    MAX-ACCESS    read-only
    STATUS        current
    DESCRIPTION  "Number of Status signalling frames with a Report Type IE
            that were not a Full Report Type or a Keep Alive Confirmation."
  ::= { nncFrStrStatCurrentEntry 41 }


  nncFrStrStatCurrentStLMSigFrmWithNoIEs        OBJECT-TYPE
    SYNTAX        NncExtCounter64
    MAX-ACCESS    read-only
    STATUS        current
    DESCRIPTION  "Number of Status signalling frames that terminate before
            the IE appears."
  ::= { nncFrStrStatCurrentEntry 42 }


  nncFrStrStatCurrentStUserSequenceErrs        OBJECT-TYPE
    SYNTAX        NncExtCounter64
    MAX-ACCESS    read-only
    STATUS        current
    DESCRIPTION  "Number of status signalling frames received with a Last
            Received Sequence number that is inconsistent with the expected
            value, the last transmitted Current Sequence Number.
            If the stream is not performing user-side procedures,
            this value is equal to noSuchName."
  ::= { nncFrStrStatCurrentEntry 43 }


  nncFrStrStatCurrentStNetSequenceErrs        OBJECT-TYPE
    SYNTAX        NncExtCounter64
    MAX-ACCESS    read-only
    STATUS        current
    DESCRIPTION  "Number of status signalling frames received with a
            Last Received Sequence number that is inconsistent with the
            expected value, the last transmitted Current Sequence Number.
            If the stream is not performing network-side procedures,
            this value is equal to noSuchName."
  ::= { nncFrStrStatCurrentEntry 44 }


  nncFrStrStatCurrentStLMUTimeoutsnT1 OBJECT-TYPE
    SYNTAX        NncExtCounter64
    MAX-ACCESS    read-only
    STATUS        current
    DESCRIPTION  "Number of times the link integrity verification interval
            has been exceeded without receiving a Status message.
            If the stream is not performing user-side procedures,
            this value is equal to noSuchName."
  ::= { nncFrStrStatCurrentEntry 45 }


  nncFrStrStatCurrentStLMUStatusMsgsRcvd OBJECT-TYPE
    SYNTAX        NncExtCounter64
    MAX-ACCESS    read-only
    STATUS        current
    DESCRIPTION  "Number of Status messages received.
            If the stream is not performing user-side procedures,
            this value is equal to noSuchName."
  ::= { nncFrStrStatCurrentEntry 46 }


  nncFrStrStatCurrentStLMUStatusENQMsgsSent OBJECT-TYPE
    SYNTAX        NncExtCounter64
    MAX-ACCESS    read-only
    STATUS        current
    DESCRIPTION  "Number of Status Enquiry messages sent.
            If the stream is not performing user-side procedures,
            this value is equal to noSuchName."
  ::= { nncFrStrStatCurrentEntry 47 }


  nncFrStrStatCurrentStLMUAsyncStatusRcvd OBJECT-TYPE
    SYNTAX        NncExtCounter64
    MAX-ACCESS    read-only
    STATUS        current
    DESCRIPTION  "Number of asynchronous Update Status messages received.
            If the stream is not performing user-side procedures,
            this value is equal to noSuchName."
  ::= { nncFrStrStatCurrentEntry 48 }


  nncFrStrStatCurrentStLMNTimeoutsnT2 OBJECT-TYPE
    SYNTAX        NncExtCounter64
    MAX-ACCESS    read-only
    STATUS        current
    DESCRIPTION  "Number of times the polling verification interval
            was exceeded without receivung a Keep Alive Sequence.
            If the stream is not performing network-side procedures,
            this value is equal to noSuchName."
  ::= { nncFrStrStatCurrentEntry 49 }


  nncFrStrStatCurrentStLMNStatusMsgsSent OBJECT-TYPE
    SYNTAX        NncExtCounter64
    MAX-ACCESS    read-only
    STATUS        current
    DESCRIPTION  "Number of Status messages sent.
            If the stream is not performing network-side procedures,
            this value is equal to noSuchName."
  ::= { nncFrStrStatCurrentEntry 50 }


  nncFrStrStatCurrentStLMNStatusENQMsgsRcvd OBJECT-TYPE
    SYNTAX        NncExtCounter64
    MAX-ACCESS    read-only
    STATUS        current
    DESCRIPTION  "Number of Status Enquiry messages received.
            If the stream is not performing network-side procedures,
            this value is equal to noSuchName."
  ::= { nncFrStrStatCurrentEntry 51 }


  nncFrStrStatCurrentStLMNAsyncStatusSent OBJECT-TYPE
    SYNTAX        NncExtCounter64
    MAX-ACCESS    read-only
    STATUS        current
    DESCRIPTION  "Number of asynchronous Update Status messages transmitted.
            If the stream is not performing network-side procedures,
            this value is equal to noSuchName."
  ::= { nncFrStrStatCurrentEntry 52 }


