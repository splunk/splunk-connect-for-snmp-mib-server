--------------------------------------------------------------------------------
-- nncFrIntStatisticsObject 3: Endpoint Stats Current Table
--------------------------------------------------------------------------------

  nncFrPVCEndptStatCurrentTable            OBJECT-TYPE
    SYNTAX        SEQUENCE OF NncFrPVCEndptStatCurrentEntry
    MAX-ACCESS    not-accessible
    STATUS        current
    DESCRIPTION  "The nncFrPVCEndptStatCurrentTable contains objects for
            monitoring performance of a frame relay endpoint during
            the current 1hr interval."
  ::= { nncFrIntStatisticsObjects 3 }


  nncFrPVCEndptStatCurrentEntry            OBJECT-TYPE
    SYNTAX        NncFrPVCEndptStatCurrentEntry
    MAX-ACCESS    not-accessible
    STATUS        current
    DESCRIPTION  "An entry in the 1hr current statistics table.  Each
            conceptual row contains current interval statistics for
            a particular PVC Endpoint."
    INDEX        { ifIndex,
              nncFrPVCEndptStatCurrentDLCINumber }
  ::= { nncFrPVCEndptStatCurrentTable 1 }


  NncFrPVCEndptStatCurrentEntry ::=
    SEQUENCE {
        nncFrPVCEndptStatCurrentDLCINumber               INTEGER,
        nncFrPVCEndptStatCurrentState                    NncExtIntvlStateType,
        nncFrPVCEndptStatCurrentAbsoluteIntervalNumber   INTEGER,
        nncFrPVCEndptStatCurrentInFrames                 NncExtCounter64,
        nncFrPVCEndptStatCurrentOutFrames                NncExtCounter64,
        nncFrPVCEndptStatCurrentInOctets                 NncExtCounter64,
        nncFrPVCEndptStatCurrentOutOctets                NncExtCounter64,
        nncFrPVCEndptStatCurrentInDiscards               NncExtCounter64,
        nncFrPVCEndptStatCurrentOutExcessFrames          NncExtCounter64,
        nncFrPVCEndptStatCurrentInDEFrames               NncExtCounter64,
        nncFrPVCEndptStatCurrentInCosTagDeFrames         NncExtCounter64,
        nncFrPVCEndptStatCurrentInOctetsDESet            NncExtCounter64,
        nncFrPVCEndptStatCurrentInFramesBECNSet          NncExtCounter64,
        nncFrPVCEndptStatCurrentInFramesFECNSet          NncExtCounter64,
        nncFrPVCEndptStatCurrentInInvdLength             NncExtCounter64,
        nncFrPVCEndptStatCurrentOutOctetsDESet           NncExtCounter64,
        nncFrPVCEndptStatCurrentOutFramesBECNSet         NncExtCounter64,
        nncFrPVCEndptStatCurrentOutFramesFECNSet         NncExtCounter64,
        nncFrPVCEndptStatCurrentOutFramesInRed           NncExtCounter64,
        nncFrPVCEndptStatCurrentInDiscdOctetsCOS         NncExtCounter64,
        nncFrPVCEndptStatCurrentInDiscdDESet             NncExtCounter64,
        nncFrPVCEndptStatCurrentInDiscdDEClr             NncExtCounter64,
        nncFrPVCEndptStatCurrentInDiscdBadEncaps         NncExtCounter64,
        nncFrPVCEndptStatCurrentInDiscdUnsupEncaps       NncExtCounter64,
        nncFrPVCEndptStatCurrentInDiscdCOSDESet          NncExtCounter64,
        nncFrPVCEndptStatCurrentInDiscdCOSDEClr          NncExtCounter64,
        nncFrPVCEndptStatCurrentOutDiscdDESet            NncExtCounter64,
        nncFrPVCEndptStatCurrentOutDiscdDEClr            NncExtCounter64,
        nncFrPVCEndptStatCurrentOutDiscdBadEncaps        NncExtCounter64,
        nncFrPVCEndptStatCurrentOutDiscdUnsupEncaps      NncExtCounter64,
        nncFrPVCEndptStatCurrentStReasDiscards           NncExtCounter64
    }


  nncFrPVCEndptStatCurrentDLCINumber        OBJECT-TYPE
    SYNTAX        INTEGER (0..1024)
    MAX-ACCESS    not-accessible
    STATUS        current
    DESCRIPTION  "The DLCI number of the stream to which statistics in
            this row belong."
  ::= { nncFrPVCEndptStatCurrentEntry 1 }


  nncFrPVCEndptStatCurrentState            OBJECT-TYPE
    SYNTAX        NncExtIntvlStateType
    MAX-ACCESS    read-only
    STATUS        current
    DESCRIPTION  "The state of the current interval. The object provides
            a status for those entries which have been reset by the
            user or have been subject to a wall-clock time change."
  ::= { nncFrPVCEndptStatCurrentEntry 2 }


  nncFrPVCEndptStatCurrentAbsoluteIntervalNumber    OBJECT-TYPE
    SYNTAX        INTEGER (0..96)
    MAX-ACCESS    not-accessible
    STATUS        current
    DESCRIPTION  "The absolute interval number of this interval."
  ::= { nncFrPVCEndptStatCurrentEntry 3 }


  nncFrPVCEndptStatCurrentInFrames        OBJECT-TYPE
    SYNTAX        NncExtCounter64
    MAX-ACCESS    read-only
    STATUS        current
    DESCRIPTION  "Number of frames received on this PVC Endpoint."
  ::= { nncFrPVCEndptStatCurrentEntry 4 }


  nncFrPVCEndptStatCurrentOutFrames        OBJECT-TYPE
    SYNTAX        NncExtCounter64
