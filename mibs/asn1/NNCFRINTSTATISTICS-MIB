--------------------------------------------------------------------------------
-- nncFrIntStatisticsObject 4: Endpoint Stats Interval Table
--------------------------------------------------------------------------------

  nncFrPVCEndptStatIntervalTable            OBJECT-TYPE
    SYNTAX        SEQUENCE OF NncFrPVCEndptStatIntervalEntry
    MAX-ACCESS    not-accessible
    STATUS        current
    DESCRIPTION  "The nncFrPVCEndptStatIntervalTable contains objects
            for monitoring performance of a frame relay endpoint
            over M 1hr historical intervals."
  ::= { nncFrIntStatisticsObjects 4 }


  nncFrPVCEndptStatIntervalEntry            OBJECT-TYPE
    SYNTAX        NncFrPVCEndptStatIntervalEntry
    MAX-ACCESS    not-accessible
    STATUS        current
    DESCRIPTION  "An entry in the 1hr interval statistics table.  Each
            conceptual row contains statistics for a particular PVC
            Endpoint and interval."
    INDEX        { ifIndex,
              nncFrPVCEndptStatIntervalDLCINumber,
              nncFrPVCEndptStatIntervalNumber }
  ::= { nncFrPVCEndptStatIntervalTable 1 }


  NncFrPVCEndptStatIntervalEntry ::=
    SEQUENCE {
        nncFrPVCEndptStatIntervalDLCINumber              INTEGER,
        nncFrPVCEndptStatIntervalNumber                  INTEGER,
        nncFrPVCEndptStatIntervalState                   NncExtIntvlStateType,
        nncFrPVCEndptStatIntervalAbsoluteIntervalNumber  INTEGER,
        nncFrPVCEndptStatIntervalInFrames                NncExtCounter64,
        nncFrPVCEndptStatIntervalOutFrames               NncExtCounter64,
        nncFrPVCEndptStatIntervalInOctets                NncExtCounter64,
        nncFrPVCEndptStatIntervalOutOctets               NncExtCounter64,
        nncFrPVCEndptStatIntervalInDiscards              NncExtCounter64,
        nncFrPVCEndptStatIntervalOutExcessFrames         NncExtCounter64,
        nncFrPVCEndptStatIntervalInDEFrames              NncExtCounter64,
        nncFrPVCEndptStatIntervalInCosTagDeFrames        NncExtCounter64,
        nncFrPVCEndptStatIntervalInOctetsDESet           NncExtCounter64,
        nncFrPVCEndptStatIntervalInFramesBECNSet         NncExtCounter64,
        nncFrPVCEndptStatIntervalInFramesFECNSet         NncExtCounter64,
        nncFrPVCEndptStatIntervalInInvdLength            NncExtCounter64,
        nncFrPVCEndptStatIntervalOutOctetsDESet          NncExtCounter64,
        nncFrPVCEndptStatIntervalOutFramesBECNSet        NncExtCounter64,
        nncFrPVCEndptStatIntervalOutFramesFECNSet        NncExtCounter64,
        nncFrPVCEndptStatIntervalOutFramesInRed          NncExtCounter64,
        nncFrPVCEndptStatIntervalInDiscdOctetsCOS        NncExtCounter64,
        nncFrPVCEndptStatIntervalInDiscdDESet            NncExtCounter64,
        nncFrPVCEndptStatIntervalInDiscdDEClr            NncExtCounter64,
        nncFrPVCEndptStatIntervalInDiscdBadEncaps        NncExtCounter64,
        nncFrPVCEndptStatIntervalInDiscdUnsupEncaps      NncExtCounter64,
        nncFrPVCEndptStatIntervalInDiscdCOSDESet         NncExtCounter64,
        nncFrPVCEndptStatIntervalInDiscdCOSDEClr         NncExtCounter64,
        nncFrPVCEndptStatIntervalOutDiscdDESet           NncExtCounter64,
        nncFrPVCEndptStatIntervalOutDiscdDEClr           NncExtCounter64,
        nncFrPVCEndptStatIntervalOutDiscdBadEncaps       NncExtCounter64,
        nncFrPVCEndptStatIntervalOutDiscdUnsupEncaps     NncExtCounter64,
        nncFrPVCEndptStatIntervalStReasDiscards          NncExtCounter64
    }


  nncFrPVCEndptStatIntervalDLCINumber            OBJECT-TYPE
    SYNTAX        INTEGER (0..1024)
    MAX-ACCESS    not-accessible
    STATUS        current
    DESCRIPTION  "The DLCI number of the stream to which statistics in
            this row belong."
  ::= { nncFrPVCEndptStatIntervalEntry 1 }

  nncFrPVCEndptStatIntervalNumber            OBJECT-TYPE
    SYNTAX        INTEGER (1..96)
    MAX-ACCESS    not-accessible
    STATUS        current
    DESCRIPTION  "The interval number (N) of the statistics in this row."
  ::= { nncFrPVCEndptStatIntervalEntry 2 }


  nncFrPVCEndptStatIntervalState            OBJECT-TYPE
    SYNTAX        NncExtIntvlStateType
    MAX-ACCESS    read-only
    STATUS        current
    DESCRIPTION  "The state of the interval represented by this row. The
            object provides a status for those entries which have
            been reset by the user or have been subject to a wall-
            clock time change."
  ::= { nncFrPVCEndptStatIntervalEntry 3 }


  nncFrPVCEndptStatIntervalAbsoluteIntervalNumber    OBJECT-TYPE
    SYNTAX        INTEGER (0..96)
    MAX-ACCESS    not-accessible
    STATUS        current
    DESCRIPTION  "The absolute interval number of this interval."
  ::= { nncFrPVCEndptStatIntervalEntry 4 }


  nncFrPVCEndptStatIntervalInFrames        OBJECT-TYPE
    SYNTAX        NncExtCounter64
    MAX-ACCESS    read-only
    STATUS        current
    DESCRIPTION  "Number of frames received on this PVC Endpoint."
  ::= { nncFrPVCEndptStatIntervalEntry 5 }


  nncFrPVCEndptStatIntervalOutFrames        OBJECT-TYPE
    SYNTAX        NncExtCounter64
    MAX-ACCESS    read-only
    STATUS        current
    DESCRIPTION  "Number of frames transmitted on this PVC Endpoint."
  ::= { nncFrPVCEndptStatIntervalEntry 6 }


  nncFrPVCEndptStatIntervalInOctets        OBJECT-TYPE
    SYNTAX        NncExtCounter64
    MAX-ACCESS    read-only
    STATUS        current
    DESCRIPTION  "Number of bytes received on this PVC Endpoint."
  ::= { nncFrPVCEndptStatIntervalEntry 7 }


  nncFrPVCEndptStatIntervalOutOctets        OBJECT-TYPE
    SYNTAX        NncExtCounter64
    MAX-ACCESS    read-only
    STATUS        current
    DESCRIPTION  "Number of bytes transmitted on this PVC Endpoint."
  ::= { nncFrPVCEndptStatIntervalEntry 8 }


  nncFrPVCEndptStatIntervalInDiscards        OBJECT-TYPE
    SYNTAX        NncExtCounter64
    MAX-ACCESS    read-only
    STATUS        current
    DESCRIPTION  "Number of frames received by the network (ingress)
            that were discarded due to traffic enforcement for this
            PVC Endpoint." 
  ::= { nncFrPVCEndptStatIntervalEntry 9 }


  nncFrPVCEndptStatIntervalOutExcessFrames    OBJECT-TYPE
    SYNTAX        NncExtCounter64
    MAX-ACCESS    read-only
    STATUS        current
    DESCRIPTION  "Number of frames transmitted by the network (egress)
            for this PVC Endpoint that were treated as excess
            traffic (the DE bit may be set to one)."
  ::= { nncFrPVCEndptStatIntervalEntry 10 }


  nncFrPVCEndptStatIntervalInDEFrames        OBJECT-TYPE
    SYNTAX        NncExtCounter64
    MAX-ACCESS    read-only
    STATUS        current
    DESCRIPTION  "Number of frames received by the network (ingress)
            with the DE bit set to (1) for the PVC Endpoint."
  ::= { nncFrPVCEndptStatIntervalEntry 11 }

 
  nncFrPVCEndptStatIntervalInCosTagDeFrames    OBJECT-TYPE
    SYNTAX      NncExtCounter64
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION "Number of frames received by the network (ingress)
            for this PVC Endpoint that were policed to be excess
            traffic (the DE bit was set to one) and transmitted."
  ::= { nncFrPVCEndptStatIntervalEntry 12 }


  nncFrPVCEndptStatIntervalInOctetsDESet OBJECT-TYPE
    SYNTAX      NncExtCounter64
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION  "Number of bytes received on this PVC Endpoint with the
            DE bit set."
  ::= { nncFrPVCEndptStatIntervalEntry 13 }


  nncFrPVCEndptStatIntervalInFramesBECNSet OBJECT-TYPE
    SYNTAX      NncExtCounter64
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION "Number of frames received by the network (ingress)
            for this PVC Endpoint that had the BECN bit set."
  ::= { nncFrPVCEndptStatIntervalEntry 14 }


  nncFrPVCEndptStatIntervalInFramesFECNSet OBJECT-TYPE
    SYNTAX      NncExtCounter64
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION "Number of frames received by the network (ingress)
            for this PVC Endpoint that had the FECN bit set."
  ::= { nncFrPVCEndptStatIntervalEntry 15 }


  nncFrPVCEndptStatIntervalInInvdLength OBJECT-TYPE
    SYNTAX      NncExtCounter64
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION "Number of frames received by the network (ingress)
            for this PVC Endpoint that were discarded because the frame
            had no data, or the data exceeded the maximum allowed length."
  ::= { nncFrPVCEndptStatIntervalEntry 16 }


  nncFrPVCEndptStatIntervalOutOctetsDESet OBJECT-TYPE
    SYNTAX      NncExtCounter64
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION  "Number of bytes transmitted on this PVC Endpoint with the
