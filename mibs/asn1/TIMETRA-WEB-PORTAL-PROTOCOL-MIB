    NOTIFICATIONS {
        tmnxWppPortalStatChanged,
        tmnxWppHostAuthenticationFailed
    }
    STATUS      current
    DESCRIPTION
        "The group of notifications supporting WPP
         on Alcatel-Lucent SROS series systems."
    ::= { tmnxWppGroups 100 }
    
