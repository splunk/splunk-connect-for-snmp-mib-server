                                     INTEGER
                                        }

        ctrlParamIndex OBJECT-TYPE
            SYNTAX      INTEGER
            ACCESS      read-only
            STATUS      mandatory
            DESCRIPTION
                "Index
                The position of this controller in relation to 'BASE'.
                 i.e.   BASE = 0
                        NMSC = 1
                        PS01 = 2
                        ...
                Applicable: All controller types"
            REFERENCE
                "ALL"
            ::={ ctrlParamEntry 1 }

        ctrlParamName OBJECT-TYPE
            SYNTAX      DisplayString (SIZE(4))
            ACCESS      read-write
            STATUS      mandatory
            DESCRIPTION
                "Name.
                The unique 4 character name by which this controller is
                known.
                Applicable: All controller types"
            REFERENCE
                "ALL"
            ::={ ctrlParamEntry 2 }

        ctrlParamType OBJECT-TYPE
            SYNTAX      INTEGER { x25(1),    mux(2),   null(3),
                                  epci(4),   hdlc(8),  qllc(9),
                                  xlan(10),  ilan(11), term(12),
                                  lan(13),   clus(14), mult(15),
                                  sdlc(16),  ip(17),   sru(18),
                                  v25b(19),  cons(20), nmsc(21),
                                  dlc(22),   map(23),  tlan(24),
                                  snmp(25),  nfrm(26), xfrm(27),
                                  tip(28),   rout(29), ppp(30),
                                  ipx(31),   tipx(32), esis(34),
                                  tesis(35), vfrm(36), smdx(37),
                                  dlcx(38), edlc(39) }
            ACCESS      read-write
            STATUS      mandatory
            DESCRIPTION
                "Type.
                The type of controller this is.
                Applicable: All controller types"
            REFERENCE
                "ALL"
            ::={ ctrlParamEntry 3 }

        ctrlParamSubType OBJECT-TYPE
            SYNTAX      INTEGER { none(1), sio(2), xio(3), xio-sio(4),
                                  generic(5), scc(17), vio(33) }
            ACCESS      read-write
            STATUS      mandatory
            DESCRIPTION
                "Sub-Type.
                The link between the controller and its associated hardware
                type.
                Applicable: X25, SDLC, QLLC, HDLC and NULL controller types"
            REFERENCE
                "NFRM VFRM PPP QLLC SDLC X25 NULL XFRM"
            ::= { ctrlParamEntry 4 }

        ctrlParamNoChannels OBJECT-TYPE
            SYNTAX      INTEGER (0..4096)
            ACCESS      read-write
            STATUS      mandatory
            DESCRIPTION
                "Number of channels.
