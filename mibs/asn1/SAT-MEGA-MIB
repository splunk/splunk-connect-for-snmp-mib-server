--      The Channel controllers

        chanParamTable OBJECT-TYPE
            SYNTAX      SEQUENCE OF ChanParamEntry
            ACCESS      not-accessible
            STATUS      mandatory
            DESCRIPTION
                "A table of channels associated with this Level 2
                controller."
            ::= { channel 3 }

        chanParamEntry OBJECT-TYPE
            SYNTAX      ChanParamEntry
            ACCESS      not-accessible
            STATUS      mandatory
            DESCRIPTION
                "An individual Channel."
            INDEX       { chanParamIndex }
            ::= { chanParamTable 1 }

        ChanParamEntry ::= SEQUENCE {   chanParamIndex
                                        INTEGER,
                                        chanParamName
                                        DisplayString,
                                        chanParamDRCMask
