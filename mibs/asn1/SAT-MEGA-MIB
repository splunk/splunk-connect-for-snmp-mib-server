            REFERENCE
                "CONS X25 NULL XLAN ILAN DLC HDLC"
            ::= { ctrlParamEntry 15 }

        ctrlParamKLevel3 OBJECT-TYPE
            SYNTAX      INTEGER (1..127)
            ACCESS      read-write
            STATUS      mandatory
            DESCRIPTION
                "KLevel 3.
                The level 3 window size.
                Applicable: X25, HDLC, CONS, ILAN, XLAN, DLC and NULL
