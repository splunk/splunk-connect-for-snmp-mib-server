            REFERENCE
                "CONS X25 NULL XFRM XLAN HDLC DLC ILAN"
            ::= { ctrlParamEntry 16 }

        ctrlParamLineGroup OBJECT-TYPE
            SYNTAX      INTEGER (0..15)
            ACCESS      read-write
            STATUS      mandatory
            DESCRIPTION
                "Line Group.
                The line group is used for load sharing. If two links
                have same (non-zero) line group then data may be shared
                between them.
                Applicable: X25, HDLC, CONS, ILAN, XLAN, DLC and NULL
                controller types"
            REFERENCE
                "CONS ILAN X25 NULL XFRM XLAN"
            ::= { ctrlParamEntry 17 }

        ctrlParamLCGN OBJECT-TYPE
            SYNTAX      INTEGER (0..15)
            ACCESS      read-write
            STATUS      mandatory
            DESCRIPTION
                "LCGN.
                The logical channel group number for this link.
                Applicable: X25, HDLC, CONS, ILAN, XLAN, DLC and NULL
                controller types"
            REFERENCE
                "CONS ILAN X25 NULL XFRM XLAN"
            ::= { ctrlParamEntry 18 }

        ctrlParamLCNOffset OBJECT-TYPE
            SYNTAX      INTEGER (0..4095)
            ACCESS      read-write
            STATUS      mandatory
            DESCRIPTION
                "LCN Offset.
                This parameter is used in conjunction with the LCGN. It's
                purpose is to shift the channel range by the appropriate
