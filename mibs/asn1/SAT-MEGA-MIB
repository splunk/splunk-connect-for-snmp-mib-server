                                     INTEGER,
                                     ctrlParamNewSerialInterface
                                     INTEGER,
                                     ctrlParamDlci
                                     INTEGER,
                                     ctrlParamMode
                                     INTEGER,
                                     ctrlParamLmi
                                     INTEGER,
                                     ctrlParamKeepAliveTimer
                                     INTEGER,
                                     ctrlParamFullStatusInterval
                                     INTEGER,
