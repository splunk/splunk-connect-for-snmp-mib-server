                                        chanParamHelpNumber
                                        INTEGER,
                                        chanParamNewsNumber
                                        INTEGER,
                                        chanParamCallPriority
                                        INTEGER,
                                        chanParamResourcePriority
                                        INTEGER,
                                        chanParamPadEnable
                                        INTEGER,
                                        chanParamPadRecall
                                        INTEGER,
                                        chanParamEcho
                                        INTEGER,
                                        chanParamDataForwarding
                                        INTEGER,
                                        chanParamDelay
                                        INTEGER,
                                        chanParamDRI
                                        INTEGER,
                                        chanParamPadSignals
                                        INTEGER,
                                        chanParamBreak
                                        INTEGER,
                                        chanParamPadding
                                        INTEGER,
                                        chanParamFold
                                        INTEGER,
                                        chanParamDRO
                                        INTEGER,
                                        chanParamLFI
                                        INTEGER,
                                        chanParamLFPad
                                        INTEGER,
                                        chanParamEdit
                                        INTEGER,
                                        chanParamCharDel
                                        INTEGER,
                                        chanParamBufferDel
                                        INTEGER,
                                        chanParamDisp
                                        INTEGER,
                                        chanParamEditServiceSignals
                                        INTEGER,
                                        chanParamEchoMask
                                        INTEGER,
                                        chanParamParityTreatment
                                        INTEGER,
                                        chanParamPageWait
                                        INTEGER,
                                        chanParamForce
                                        INTEGER,
                                        chanParamBreakChar
                                        INTEGER,
                                        chanParamNetworkParity
                                        INTEGER,
                                        chanParamEscapeDelay
                                        INTEGER,
                                        chanParamStationAddress
                                        INTEGER,
                                        chanParamProvideXID
                                        INTEGER,
                                        chanParamMode
                                        INTEGER,
                                        chanParamPortMatch
                                        INTEGER,
                                        chanParamReverseTelnetPort
                                        INTEGER,
                                        chanParamBroadcastFilterTimer
                                        INTEGER,
                                        chanParamInhibitLearning
                                        INTEGER,
                                        chanParamRetainSession
                                        INTEGER,
                                        chanParamPrinterType
                                        INTEGER,
                                        chanParamScreenPrinter
                                        INTEGER,
                                        chanParamLocalPrinter
                                        INTEGER,
                                        chanParamBnnDsap
                                        INTEGER,
                                        chanParamBnnSsap
                                        INTEGER,
                                        chanParamBnnPuTyp
                                        INTEGER,
                                        chanParamPriority
                                        INTEGER        }

        chanParamIndex OBJECT-TYPE
            SYNTAX      INTEGER
            ACCESS      read-only
            STATUS      mandatory
            DESCRIPTION
                "Index.
                The position of this channel in the controller channel
                Table.
                Applicable: All controller types"
          REFERENCE
                "ALL"
          ::={ chanParamEntry 1 }

        chanParamName OBJECT-TYPE
            SYNTAX      DisplayString (SIZE(4))
            ACCESS      read-write
            STATUS      mandatory
            DESCRIPTION
