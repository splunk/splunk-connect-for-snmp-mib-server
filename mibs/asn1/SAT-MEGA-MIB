                                     ctrlParamAddressGroup
                                     INTEGER,
                                     ctrlParamFrameSequence
                                     INTEGER,
                                     ctrlParamCallTimeOut
                                     INTEGER,
                                     ctrlParamErrorThreshold
                                     INTEGER,
                                     ctrlParamLoopBarPriority
                                     INTEGER,
                                     ctrlParamExtendedCallMgmt
                                     INTEGER,
                                     ctrlParamOptionalTimers
                                     INTEGER,
                                     ctrlParamOptionalFlags
                                     INTEGER,
                                     ctrlParamTransportLevelType
                                     INTEGER,
                                     ctrlParamTransportClass
                                     INTEGER,
                                     ctrlParamDefaultWindowSize
                                     INTEGER,
                                     ctrlParamDisableFlowControl
                                     INTEGER,
                                     ctrlParamTSAPFormat
                                     INTEGER,
                                     ctrlParamMaximumTPDUSize
                                     INTEGER,
                                     ctrlParamTTRTime
                                     INTEGER,
                                     ctrlParamTWRTime
                                     INTEGER,
                                     ctrlParamDestinationAddress
                                     DisplayString,
                                     ctrlParamDSAP
                                     INTEGER,
                                     ctrlParamSSAP
                                     INTEGER,
                                     ctrlParamSDLCOptions
                                     INTEGER,
                                     ctrlParamPrimaryStation
                                     INTEGER,
                                     ctrlParamIdleDetectTime
                                     INTEGER,
                                     ctrlParamPollInterval
                                     INTEGER,
                                     ctrlParamDuplex
                                     INTEGER,
                                     ctrlParamTransmitDelay
                                     INTEGER,
