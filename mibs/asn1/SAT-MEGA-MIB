                Applicable: All controller types"
            REFERENCE
                "ALL"
            ::= { ctrlParamEntry 5 }

        ctrlParamSpeed OBJECT-TYPE
            SYNTAX      INTEGER { speed-2mbps(19),    speed-1mbps(18),
                                  speed-512kbps(17),  speed-256kbps(16),
                                  speed-128kbps(15),  speed-64kbps(14),
                                  speed-12-75bps(13), speed-19200bps(12),
                                  speed-9600bps(11),  speed-7200bps(10),
                                  speed-4800bps(9),   speed-3600bps(8),
                                  speed-2400bps(7),   speed-2000bps(6),
