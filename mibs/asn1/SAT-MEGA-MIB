        billFileSize OBJECT-TYPE
            SYNTAX      INTEGER
            ACCESS      read-only
            STATUS      mandatory
            DESCRIPTION
                "Bill File Size.
                The number of entries configured into the BILL file."
            ::= { billFile 1 }

        billTable OBJECT-TYPE
            SYNTAX      SEQUENCE OF BillEntry
            ACCESS      not-accessible
            STATUS      mandatory
            DESCRIPTION
                "A table of 'Bill' entries."
            ::= { billFile 2 }

