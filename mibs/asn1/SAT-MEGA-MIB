--      The Controller configuration section
        ctrlParamTable OBJECT-TYPE
            SYNTAX      SEQUENCE OF CtrlParamEntry
            ACCESS      not-accessible
            STATUS      mandatory
            DESCRIPTION
                "The controller configuration table."
            ::= { controller 4 }

        ctrlParamEntry OBJECT-TYPE
            SYNTAX      CtrlParamEntry
            ACCESS      not-accessible
            STATUS      mandatory
            DESCRIPTION
                "An individual controller configuration."
            INDEX       { ctrlParamIndex }
            ::= { ctrlParamTable 1 }

        CtrlParamEntry ::= SEQUENCE {ctrlParamIndex
                                     INTEGER,
                                     ctrlParamName
                                     DisplayString,
                                     ctrlParamType
                                     INTEGER,
                                     ctrlParamSubType
                                     INTEGER,
                                     ctrlParamNoChannels
                                     INTEGER,
                                     ctrlParamSpeed
                                     INTEGER,
