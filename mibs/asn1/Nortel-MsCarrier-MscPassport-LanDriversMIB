mscLpIlsFwdrTestRowStatusTable OBJECT-TYPE
   SYNTAX     SEQUENCE OF MscLpIlsFwdrTestRowStatusEntry
   ACCESS     not-accessible
   STATUS     mandatory
   DESCRIPTION
     "This entry controls the addition and deletion of 
      mscLpIlsFwdrTest components."
::= { mscLpIlsFwdrTest 1 }

mscLpIlsFwdrTestRowStatusEntry OBJECT-TYPE
   SYNTAX     MscLpIlsFwdrTestRowStatusEntry
   ACCESS     not-accessible
   STATUS     mandatory
   DESCRIPTION
     "A single entry in the table represents a single 
      mscLpIlsFwdrTest component."
   INDEX {
      mscLpIndex,
      mscLpIlsFwdrIndex,
      mscLpIlsFwdrTestIndex }
::= { mscLpIlsFwdrTestRowStatusTable 1 }

MscLpIlsFwdrTestRowStatusEntry ::= SEQUENCE {
   mscLpIlsFwdrTestRowStatus
      RowStatus,
   mscLpIlsFwdrTestComponentName
      DisplayString,
   mscLpIlsFwdrTestStorageType
      StorageType,
   mscLpIlsFwdrTestIndex
      NonReplicated }

mscLpIlsFwdrTestRowStatus OBJECT-TYPE
   SYNTAX     RowStatus 
   ACCESS     read-only
   STATUS     mandatory
   DESCRIPTION
     "This variable is used as the basis for SNMP naming of
      mscLpIlsFwdrTest components.
      These components cannot be added nor deleted."
::= { mscLpIlsFwdrTestRowStatusEntry 1 }

mscLpIlsFwdrTestComponentName OBJECT-TYPE
   SYNTAX     DisplayString 
   ACCESS     read-only
   STATUS     mandatory
   DESCRIPTION
     "This variable provides the component's string name for
      use with the ASCII Console Interface"
::= { mscLpIlsFwdrTestRowStatusEntry 2 }

mscLpIlsFwdrTestStorageType OBJECT-TYPE
   SYNTAX     StorageType 
   ACCESS     read-only
   STATUS     mandatory
   DESCRIPTION
     "This variable represents the storage type value for the
      mscLpIlsFwdrTest tables."
::= { mscLpIlsFwdrTestRowStatusEntry 4 }

mscLpIlsFwdrTestIndex OBJECT-TYPE
   SYNTAX     NonReplicated 
   ACCESS     not-accessible
   STATUS     mandatory
   DESCRIPTION
     "This variable represents the index for the mscLpIlsFwdrTest tables."
::= { mscLpIlsFwdrTestRowStatusEntry 10 }

mscLpIlsFwdrTestPTOTable OBJECT-TYPE
   SYNTAX     SEQUENCE OF MscLpIlsFwdrTestPTOEntry
   ACCESS     not-accessible
   STATUS     mandatory
   DESCRIPTION
     "This group contains the operational data for the LanPortTest 
      component."
::= { mscLpIlsFwdrTest 10 }

mscLpIlsFwdrTestPTOEntry OBJECT-TYPE
   SYNTAX     MscLpIlsFwdrTestPTOEntry
   ACCESS     not-accessible
   STATUS     mandatory
   DESCRIPTION
     "An entry in the mscLpIlsFwdrTestPTOTable."
   INDEX {
      mscLpIndex,
      mscLpIlsFwdrIndex,
      mscLpIlsFwdrTestIndex }
::= { mscLpIlsFwdrTestPTOTable 1 }

MscLpIlsFwdrTestPTOEntry ::= SEQUENCE {
   mscLpIlsFwdrTestType
      INTEGER,
   mscLpIlsFwdrTestFrmSize
      Unsigned32,
   mscLpIlsFwdrTestDuration
      Unsigned32 }

mscLpIlsFwdrTestType OBJECT-TYPE
   SYNTAX     INTEGER  {
                 onCard(0),
                 normal(1),
                 wrapA(257),
                 wrapB(258),
                 thruA(259),
                 thruB(260),
                 extWrapA(263),
                 extWrapB(264),
                 extThruA(265),
                 extThruB(266),
                 extWrapAB(267),
                 extWrapBA(268) }
   ACCESS     read-write
   STATUS     mandatory
   DESCRIPTION
     "This attribute is the type of test to be run. The available test types 
      include the following: card test sets up an internal loop on the board 
      and runs a bit error rate test; manual test assumes the line is looped 
      back externally and a bit error rate test is performed; localLoop test 
      loops back the local modem and performs a bit error rate test; 
      remoteLoop test loops back the remote modem (or CSU) and 
      performs a bit error rate test; externalLoop test loops back the line 
      to the far end; payloadLoop test only loops back the payload data 
      received from the incoming line; Note that localLoop, remoteLoop 
      and payloadLoop are not supported by all function processors."
::= { mscLpIlsFwdrTestPTOEntry 1 }

mscLpIlsFwdrTestFrmSize OBJECT-TYPE
   SYNTAX     Unsigned32 (4..4096)
   ACCESS     read-write
   STATUS     mandatory
   DESCRIPTION
     "This attribute defines the size of the test data frames in number of 
      bytes."
   DEFVAL     { 1024 }
::= { mscLpIlsFwdrTestPTOEntry 2 }

mscLpIlsFwdrTestDuration OBJECT-TYPE
   SYNTAX     Unsigned32 (1..30240)
   ACCESS     read-write
   STATUS     mandatory
   DESCRIPTION
     "This attribute is the duration (in minutes) that test data will be 
      transmitted. In the case of an externalLoop, this is the time that the 
      loop is in effect. Note that dataStartDelay does not apply to the 
      externalLoop."
   DEFVAL     { 1 }
::= { mscLpIlsFwdrTestPTOEntry 3 }

mscLpIlsFwdrTestResultsTable OBJECT-TYPE
   SYNTAX     SEQUENCE OF MscLpIlsFwdrTestResultsEntry
   ACCESS     not-accessible
   STATUS     mandatory
   DESCRIPTION
     "This group contains all the statistics related attributes of the Test 
      component."
::= { mscLpIlsFwdrTest 11 }

mscLpIlsFwdrTestResultsEntry OBJECT-TYPE
   SYNTAX     MscLpIlsFwdrTestResultsEntry
   ACCESS     not-accessible
   STATUS     mandatory
   DESCRIPTION
     "An entry in the mscLpIlsFwdrTestResultsTable."
   INDEX {
      mscLpIndex,
      mscLpIlsFwdrIndex,
      mscLpIlsFwdrTestIndex }
::= { mscLpIlsFwdrTestResultsTable 1 }

MscLpIlsFwdrTestResultsEntry ::= SEQUENCE {
   mscLpIlsFwdrTestElapsedTime
      Counter32,
   mscLpIlsFwdrTestTimeRemaining
      Unsigned32,
   mscLpIlsFwdrTestCauseOfTermination
      INTEGER,
   mscLpIlsFwdrTestFrmTx
      PassportCounter64,
   mscLpIlsFwdrTestBitsTx
      PassportCounter64,
   mscLpIlsFwdrTestFrmRx
      PassportCounter64,
   mscLpIlsFwdrTestBitsRx
      PassportCounter64,
   mscLpIlsFwdrTestErroredFrmRx
      PassportCounter64 }

mscLpIlsFwdrTestElapsedTime OBJECT-TYPE
   SYNTAX     Counter32 
   ACCESS     read-only
   STATUS     mandatory
   DESCRIPTION
     "This attribute is the number of minutes elapsed since the test started 
      for an externalLoop test. For a data test, this is the time since test 
      data was first transmitted."
::= { mscLpIlsFwdrTestResultsEntry 4 }

mscLpIlsFwdrTestTimeRemaining OBJECT-TYPE
   SYNTAX     Unsigned32 (0..4294967295)
   ACCESS     read-only
   STATUS     mandatory
   DESCRIPTION
     "This attribute is the number of minutes remaining before the test is 
      to be stopped according to the duration attribute."
   DEFVAL     { 0 }
::= { mscLpIlsFwdrTestResultsEntry 5 }

mscLpIlsFwdrTestCauseOfTermination OBJECT-TYPE
   SYNTAX     INTEGER  {
                 testTimeExpired(0),
                 stoppedByOperator(1),
                 unknown(2),
                 neverStarted(3),
                 testRunning(4) }
   ACCESS     read-only
   STATUS     mandatory
   DESCRIPTION
     "This attribute records the reason the test is terminated."
   DEFVAL     { neverStarted }
::= { mscLpIlsFwdrTestResultsEntry 6 }

mscLpIlsFwdrTestFrmTx OBJECT-TYPE
   SYNTAX     PassportCounter64 
   ACCESS     read-only
   STATUS     mandatory
   DESCRIPTION
     "This attribute is the total number of frames sent during the test 
      period. This counter is not expected to reach its maximum value."
::= { mscLpIlsFwdrTestResultsEntry 7 }

mscLpIlsFwdrTestBitsTx OBJECT-TYPE
   SYNTAX     PassportCounter64 
   ACCESS     read-only
   STATUS     mandatory
   DESCRIPTION
     "This attribute is the total number of bits sent during the test period. 
      This counter is not expected to reach its maximum value."
::= { mscLpIlsFwdrTestResultsEntry 8 }

mscLpIlsFwdrTestFrmRx OBJECT-TYPE
   SYNTAX     PassportCounter64 
   ACCESS     read-only
   STATUS     mandatory
   DESCRIPTION
     "This attribute is the total number of frames received during the test 
      period. This counter is not expected to reach its maximum value. 
      The counter will wrap around to 0 if it does reach its maximum 
      value."
::= { mscLpIlsFwdrTestResultsEntry 9 }

mscLpIlsFwdrTestBitsRx OBJECT-TYPE
   SYNTAX     PassportCounter64 
   ACCESS     read-only
   STATUS     mandatory
   DESCRIPTION
     "This attribute is the total number of bits received during the test 
      period. This counter is not expected to reach its maximum value. 
      The counter will wrap around to 0 if it does reach its maximum 
      value."
::= { mscLpIlsFwdrTestResultsEntry 10 }

mscLpIlsFwdrTestErroredFrmRx OBJECT-TYPE
   SYNTAX     PassportCounter64 
   ACCESS     read-only
   STATUS     mandatory
   DESCRIPTION
     "This attribute is the total number of errored frames received during 
      the test period. This counter is not expected to reach its maximum 
      value. The counter will wrap around to 0 if it does reach its 
      maximum value."
::= { mscLpIlsFwdrTestResultsEntry 11 }


-- Lp/n Ethernet100BaseT/n
-- This component defines an instance of a 100BaseT Ethernet station 
-- attached to a bus. One instance of this component must be created for 
-- each hardware interface.

mscLpEth100 OBJECT IDENTIFIER ::= { mscLp 25 }



mscLpEth100RowStatusTable OBJECT-TYPE
   SYNTAX     SEQUENCE OF MscLpEth100RowStatusEntry
   ACCESS     not-accessible
   STATUS     mandatory
   DESCRIPTION
     "This entry controls the addition and deletion of 
      mscLpEth100 components."
::= { mscLpEth100 1 }

mscLpEth100RowStatusEntry OBJECT-TYPE
   SYNTAX     MscLpEth100RowStatusEntry
   ACCESS     not-accessible
   STATUS     mandatory
   DESCRIPTION
     "A single entry in the table represents a single 
      mscLpEth100 component."
   INDEX {
      mscLpIndex,
      mscLpEth100Index }
::= { mscLpEth100RowStatusTable 1 }

MscLpEth100RowStatusEntry ::= SEQUENCE {
   mscLpEth100RowStatus
      RowStatus,
   mscLpEth100ComponentName
      DisplayString,
   mscLpEth100StorageType
      StorageType,
   mscLpEth100Index
      Integer32 }

mscLpEth100RowStatus OBJECT-TYPE
   SYNTAX     RowStatus 
   ACCESS     read-write
   STATUS     mandatory
   DESCRIPTION
     "This variable is used as the basis for SNMP naming of
      mscLpEth100 components.
      These components can be added and deleted."
::= { mscLpEth100RowStatusEntry 1 }

