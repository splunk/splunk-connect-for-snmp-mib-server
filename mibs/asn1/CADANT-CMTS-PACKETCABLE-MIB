CADANT-CMTS-PACKETCABLE-MIB DEFINITIONS ::= BEGIN


IMPORTS
    MODULE-IDENTITY,
    OBJECT-TYPE,
    Integer32,
    IpAddress,
    Unsigned32,
    Counter32
        FROM SNMPv2-SMI

    TEXTUAL-CONVENTION,
    RowStatus,
    TruthValue,
    DisplayString,
    TimeStamp
        FROM SNMPv2-TC

    ifIndex,
    InterfaceIndex
        FROM IF-MIB
    IfDirection
        FROM DOCS-QOS3-MIB

    cadCable
        FROM CADANT-PRODUCTS-MIB

    CardId,
    AdminState
        FROM CADANT-TC

    cadOvSysCpuStatus
        FROM CADANT-CMTS-PROCESS-MIB

    cadIfCmtsCardNumber
        FROM CADANT-CMTS-DOWNCHANNEL-MIB;

cadPCMIB   MODULE-IDENTITY
        

