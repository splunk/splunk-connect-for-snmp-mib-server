        SYNTAX  SEQUENCE OF ImaDs1Entry
        ACCESS  not-accessible
        STATUS  mandatory
        DESCRIPTION
            "A table of physical layer configuration for the DS1
         interface"
        ::= { atmImaDs1Objects 1 }

    imaDs1Entry  OBJECT-TYPE
        SYNTAX  ImaDs1Entry
        ACCESS  not-accessible
        STATUS  mandatory
        DESCRIPTION
          "A entry in the table, containing information about the
           physical layer of a DS1 interface"
        INDEX  { imaDs1GroupIndex, imaDs1Index }
        ::= { imaDs1Table 1 }
    
    ImaDs1Entry ::= SEQUENCE {
	imaDs1Index 		  INTEGER,
	imaDs1GroupIndex 	  INTEGER,
	imaDs1ConfLoopback        INTEGER,
      	imaDs1ConfTxClockSelect   INTEGER,
