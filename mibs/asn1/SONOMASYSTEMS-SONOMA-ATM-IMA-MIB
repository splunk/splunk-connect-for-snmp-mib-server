      	imaDs1ConfFrameFormat     INTEGER,
      	imaDs1ConfCableLength     INTEGER,
	imaDs1ConfTxTimingRefLink INTEGER,
	imaDs1ConfScramble        INTEGER,
      	imaDs1StatsNoSignals      Counter,
      	imaDs1StatsAISDetects     Counter,
      	imaDs1StatsYelAlarmCount  Counter,
      	imaDs1StatsLCVErrors      Counter,
      	imaDs1StatsPCVErrors      Counter,
      	imaDs1StatsMOSErrors      Counter,
      	imaDs1StatsSyncLossCount  Counter,
      	imaDs1StatsHECErrors      Counter,
      	imaDs1StatsSignalLoss     INTEGER,
      	imaDs1StatsAISDetect      INTEGER,
      	imaDs1StatsYellowAlarm    INTEGER,
      	imaDs1StatsSyncLoss       INTEGER,
      	imaDs1StatsTxClockLoss    INTEGER,
      	imaDs1StatsErrSec         Counter,
      	imaDs1StatsLineErrSec     Counter,
      	imaDs1StatsErrFreeSec     Counter,
      	imaDs1StatsBurstErrSec    Counter,
      	imaDs1StatsSevErrSec      Counter,
      	imaDs1StatsConSevErrSec   Counter,
      	imaDs1StatsFailedSignal   Counter,
      	imaDs1StatsUnavailSec     Counter,
      	imaDs1StatsSevErrFramSec  Counter,
      	imaDs1StatsClearCounters  INTEGER,
	imaDs1DebugAddr           INTEGER,
	imaDs1DebugReadValue      INTEGER,
	imaDs1DebugWriteValue     INTEGER,
	imaDs1DebugRead           INTEGER,
	imaDs1PortNumber          INTEGER
    }

