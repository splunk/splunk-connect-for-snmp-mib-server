            wfWcpCircuitSearchDepth
                INTEGER, 
	    wfWcpCircuitCompressorOutBytes
    		Counter,
    	    wfWcpCircuitDecompressorOutBytes
    		Counter
        }

    wfWcpCircuitDelete OBJECT-TYPE
        SYNTAX	INTEGER {
    		    created(1),
    		    deleted(2)
    		}
