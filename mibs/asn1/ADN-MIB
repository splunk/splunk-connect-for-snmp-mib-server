    SYNTAX  INTEGER (-2147483648..2147483647)
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The area number part of adnAtmUniVcCconnectLastErrorNodeID."
    ::= { adnAtmUniVcCconnectEntry  34 }

