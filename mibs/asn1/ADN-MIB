        adnIsnaRouteIpAddrs
            IpAddress,

        adnIsnaRouteIpMask
            IpAddress
    }

adnIsnaRouteifIndex OBJECT-TYPE
    SYNTAX  INTEGER (-2147483648..2147483647)
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "This object is mapped to adnIsnaIcifIndex in the 
     adnIsnaIcTable."
    ::= { adnIsnaRouteIpEntry  1 }

adnIsnaRouteDlci OBJECT-TYPE
    SYNTAX  INTEGER (-2147483648..2147483647)
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "This object is mapped to adnIsnaIcDlci in the 
      adnIsnaIcTable."
    ::= { adnIsnaRouteIpEntry  2 }

adnIsnaRouteIpAddrs OBJECT-TYPE
    SYNTAX  IpAddress
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "Defines the IP address of any host that needs to communicate
     with the HSS for network management purposes.  This field is
     used internally by the internal subscriber subsystem to link
     IP addresses to internal subscriber subsystem data 
     link connection identifiers (DLCI). At a minimum, the IP 
     address of the A1100 NMS must be entered.  Any other hosts 
     using applications that would access the HSS need their IP
     address entered here."
    ::= { adnIsnaRouteIpEntry  3 }

adnIsnaRouteIpMask OBJECT-TYPE
    SYNTAX  IpAddress
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "Defines which bits of the IP address are examined in 
     reading the IP address of any host that needs to 
     communicate with the HSS.  Bit fields in the mask that
     are set to '1' cause the address digit to be significant, 
     bit fields that are set to '0' cause the digit to be 
     ignored."
    ::= { adnIsnaRouteIpEntry  4 }

adnIsomMIB OBJECT IDENTIFIER ::= { adnProprietary  7 }

adnIsomVcTable OBJECT-TYPE
    SYNTAX  SEQUENCE OF AdnIsomVcEntry
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
           "VC table extensions that serve as alternates to the PVC 
	 end-point table in RFC1604, which is not sufficient for 
	 the HSS network. Additionally, the Network Management 
         System needs to know the identity of the owner of a given
         ADN proprietary interface and this is done by utilizing 
         object adnIfSubType=iss of adnIfTable."
    ::= { adnIsomMIB  2 }

adnIsomVcEntry OBJECT-TYPE
    SYNTAX  AdnIsomVcEntry
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
           "VC table entry."
    INDEX   { adnIsomVcifIndex,  adnIsomVcOutDlci }
    ::= { adnIsomVcTable  1 }

AdnIsomVcEntry ::=
    SEQUENCE {
        adnIsomVcifIndex
            INTEGER (-2147483648..2147483647),

        adnIsomVcOutDlci
            INTEGER (-2147483648..2147483647),

        adnIsomVcAdminState
            INTEGER,

        adnIsomVcOperState
            INTEGER,

        adnIsomVcTime
            AdnNodeTimeStamp
    }

adnIsomVcifIndex OBJECT-TYPE
    SYNTAX  INTEGER (-2147483648..2147483647)
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "This is the ifIndex of the internal subscriber line 
         carrying this internal connection."
    ::= { adnIsomVcEntry  1 }

adnIsomVcOutDlci OBJECT-TYPE
    SYNTAX  INTEGER (-2147483648..2147483647)
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "DLCI (Data Link Connection Identifier) for this internal 
         connection in the direction from the internal subscriber 
         on the OM (operations and maintenance) processor towards
         the FRS (frame relay service) subsystem on the LT (line
         termination) processor."
    ::= { adnIsomVcEntry  2 }

adnIsomVcAdminState OBJECT-TYPE
    SYNTAX  INTEGER {
    up(1),
    down(2)
}
    ACCESS  read-write
    STATUS  mandatory
    DESCRIPTION
           "Defines the desired state of this internal subscriber 
         connection."
    DEFVAL  { up }
    ::= { adnIsomVcEntry  3 }

adnIsomVcOperState OBJECT-TYPE
    SYNTAX  INTEGER {
    up(1),
    down(2)
}
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "Defines the current state of this internal subscriber
         connection."
    ::= { adnIsomVcEntry  4 }

adnIsomVcTime OBJECT-TYPE
    SYNTAX  AdnNodeTimeStamp
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "Defines the time this internal subscriber connection was
         initialized."
    ::= { adnIsomVcEntry  5 }

adnIsomTrapIpTable OBJECT-TYPE
    SYNTAX  SEQUENCE OF AdnIsomTrapIpEntry
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
           "This table contains a list of Network Management System 
         IP (internet protocol) addresses to which an 
         adnHSSCuDownTrap should be sent in the event that the 
         internal subscriber subsystem detects that the CU 
         (control unit) has gone down.  The addresses in this 
         table must match the entries in the CU relating to 
         recipients of the adnNodeResync trap."
    ::= { adnIsomMIB  3 }

adnIsomTrapIpEntry OBJECT-TYPE
    SYNTAX  AdnIsomTrapIpEntry
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
           "One or several IP host address entries mapped to a single
	 DLCI as referenced by ifIndex, adnIsomVcOutDlci, and 
         adnIsomTrapIpAddrs."
    INDEX   { adnIsomTrapifIndex,  adnIsomTrapDlci,  adnIsomTrapIpAddrs }
    ::= { adnIsomTrapIpTable  1 }

AdnIsomTrapIpEntry ::=
    SEQUENCE {
        adnIsomTrapifIndex
            INTEGER (-2147483648..2147483647),

        adnIsomTrapDlci
            INTEGER (-2147483648..2147483647),

        adnIsomTrapIpAddrs
            IpAddress
    }

adnIsomTrapifIndex OBJECT-TYPE
    SYNTAX  INTEGER (-2147483648..2147483647)
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "This object is mapped to 'adnIsomVcifIndex' of 
         'adnIsomVcTable' table, and is defined similar to the 
         'adnIsomVcifIndex' entry contained in the adnIsomVcTable 
         table.  Used to address the DLCI that carries SNMP CU down 
         traps to a defined NMS host."
    ::= { adnIsomTrapIpEntry  1 }

adnIsomTrapDlci OBJECT-TYPE
    SYNTAX  INTEGER (-2147483648..2147483647)
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "Mapped to 'adnIsomVcOutDlci' of 'adnIsomVcTable' table,
         and is defined similar to 'adnIsomVcOutDlci' entry 
         contained in the adnIsomVcTable table.  Used to address
         the DLCI that carries SNMP CU down traps to a defined 
         (IP host) NMS."
    ::= { adnIsomTrapIpEntry  2 }

adnIsomTrapIpAddrs OBJECT-TYPE
    SYNTAX  IpAddress
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "Full 4-byte NMS IP address that the Internal Subscriber 
         Operation and maintenance  subsystem uses to send a
         SNMP CU down trap in the event the node supervision 
         subsystem reports a CU down condition."
    ::= { adnIsomTrapIpEntry  3 }

