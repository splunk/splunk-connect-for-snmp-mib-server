        adnBrdFlashChassisIndex
            INTEGER (-2147483648..2147483647),

        adnBrdFlashSlotIndex
            INTEGER (-2147483648..2147483647),

        adnBrdFlashSize
            INTEGER (-2147483648..2147483647),

        adnBrdFlashVolume1
            DisplayString,

        adnBrdFlashVolume2
            DisplayString,

        adnBrdFlashVolume3
            DisplayString,

        adnBrdFlashVolume4
            DisplayString
    }

