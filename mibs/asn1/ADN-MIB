adnsonetMediumTable OBJECT-TYPE
    SYNTAX  SEQUENCE OF AdnSonetMediumEntry
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
           "The ADN SONET/SDH Medium table."
    ::= { adnsonetMedium  1 }

adnsonetMediumEntry OBJECT-TYPE
    SYNTAX  AdnSonetMediumEntry
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
           "An entry in the ADN SONET/SDH Medium table."
    INDEX   { ifIndex }
    ::= { adnsonetMediumTable  1 }

AdnSonetMediumEntry ::=
    SEQUENCE {
        adnsonetMediumStatus
            INTEGER (-2147483648..2147483647),

        adnsonetMediumLaserDegradation
            Counter,

        adnsonetMediumLaserTransmitSignalOut
            Counter,

        adnsonetMediumAppliqueFailure
            Counter,

        adnsonetMediumConfigLineClockMode
            INTEGER,

        adnsonetMediumConfigLaserPower
            INTEGER
    }

adnsonetMediumStatus OBJECT-TYPE
    SYNTAX  INTEGER (-2147483648..2147483647)
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "This variable indicates the status
                 of the interface.
                 It is a bit map represented as a sum;
                 therefore, it can represent multiple
                 defects simultaneously.
                 The adnsonetMediumNoDefect bit should
                 be set only if no other bit is set.

                 The bit positions are:
                       1   adnsonetMediumNoDefect
                       2   adnsonetMediumLaserDegradation
	               4   adnsonetMediumLaserTransmitSignalOut
		       8   adnsonetMediumAppliqueFailure (obsolete)
		       
		 Common Name: Medium State"
    ::= { adnsonetMediumEntry  1 }

adnsonetMediumLaserDegradation OBJECT-TYPE
    SYNTAX  Counter	-- UNITS milliseconds

    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "Duration of laser degradation.
		  Counter #23 of the ATM core, only 
                  available on BBS1Bx board with HAS1B
                  applique and on BBTS1C board."
    ::= { adnsonetMediumEntry  2 }

adnsonetMediumLaserTransmitSignalOut OBJECT-TYPE
    SYNTAX  Counter	-- UNITS milliseconds

    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "Duration of transmit signal out of
                  range.
		  Counter #24 of the ATM core, only 
                  available on BBS1Bx board with HAS1B
                  applique and on BBTS1C board."
    ::= { adnsonetMediumEntry  3 }

adnsonetMediumAppliqueFailure OBJECT-TYPE
    SYNTAX  Counter	-- UNITS milliseconds

    ACCESS  read-only
    STATUS  obsolete
    DESCRIPTION
           "Duration of applique board failure.
		  Counter #27 of the ATM core."
    ::= { adnsonetMediumEntry  4 }

adnsonetMediumConfigLineClockMode OBJECT-TYPE
    SYNTAX  INTEGER {
    adnsonetMediumLocalTiming(1),
    adnsonetMediumLoopTiming(2),
    adnsonetMediumThroughTiming(3)
}
    ACCESS  read-write
    STATUS  mandatory
    DESCRIPTION
           "The transmit clock mode of the line:
		   - local timing: the transmit clock is
                     generated by the local board,
		   - loop timing: the transmit clock is
                     the one received from the remote 
                     equipment,
		   - through timing: the transmit clock is
                     derived from the recovered receive 
                     clock of another interface of the HSS.

		  Common Name: Transmit Clock Mode"
    ::= { adnsonetMediumEntry  5 }

adnsonetMediumConfigLaserPower OBJECT-TYPE
    SYNTAX  INTEGER {
    adnsonetMediumLaserON(1),
    adnsonetMediumLaserOFF(2),
    adnsonetMediumLaserNS(3),
    adnsonetMediumLaserONALS(4)
}
    ACCESS  read-write
    STATUS  mandatory
    DESCRIPTION
           "The administrative status of the laser LED 
                  on the applique:
		   - LaserON: laser LED in service with
		     automatic laser shutdown (ALS) function
		     disabled,
		   - LaserOFF: laser LED out of service,
		   - LaserNS: laser LED not supported by
		     the applique,
		   - LaserONALS: laser LED in service with
		     ALS function enabled.
		       
		  Common Name: Laser Power"
    ::= { adnsonetMediumEntry  6 }

adnsonetSectionTable OBJECT-TYPE
    SYNTAX  SEQUENCE OF AdnSonetSectionEntry
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
           "The ADN SONET/SDH Section Current table."
    ::= { adnsonetSection  1 }

adnsonetSectionEntry OBJECT-TYPE
    SYNTAX  AdnSonetSectionEntry
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
           "An entry in the ADN SONET/SDH Section Current table."
    INDEX   { ifIndex }
    ::= { adnsonetSectionTable  1 }

AdnSonetSectionEntry ::=
    SEQUENCE {
        adnsonetSectionStatus
            INTEGER (-2147483648..2147483647),

        adnsonetSectionLOS
            Counter,

        adnsonetSectionLOF
            Counter,

        adnsonetSectionOOF
            Counter
    }

adnsonetSectionStatus OBJECT-TYPE
    SYNTAX  INTEGER (-2147483648..2147483647)
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "This variable indicates the status
                 of the interface.
                 It is a bit map represented as a sum;
                 therefore, it can represent multiple
                 defects simultaneously.
                 The adnsonetSectionNoDefect bit should
                 be set only if no other bit is set.

                 The bit positions are:
                       1   adnsonetSectionNoDefect
                       2   adnsonetSectionLOS
                       4   adnsonetSectionLOF
		       8   adnsonetSectionOOF
		       
		 Common Name: Section State"
    ::= { adnsonetSectionEntry  1 }

adnsonetSectionLOS OBJECT-TYPE
    SYNTAX  Counter	-- UNITS milliseconds

    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "Duration of loss-of-signal (LOS).
		  Counter #0 of the ATM core."
    ::= { adnsonetSectionEntry  2 }

adnsonetSectionLOF OBJECT-TYPE
    SYNTAX  Counter	-- UNITS milliseconds

    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "Duration of loss-of-frame (LOF).
		  Counter #7 of the ATM core."
    ::= { adnsonetSectionEntry  3 }

adnsonetSectionOOF OBJECT-TYPE
    SYNTAX  Counter
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "Number of out-of-frame (OOF) events.
		  Counter #29 of the ATM core."
    ::= { adnsonetSectionEntry  4 }

adnsonetLineTable OBJECT-TYPE
    SYNTAX  SEQUENCE OF AdnSonetLineEntry
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
           "The ADN SONET/SDH Line Current table."
    ::= { adnsonetLine  1 }

adnsonetLineEntry OBJECT-TYPE
    SYNTAX  AdnSonetLineEntry
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
           "An entry in the ADN SONET/SDH Line Current table."
    INDEX   { ifIndex }
    ::= { adnsonetLineTable  1 }

AdnSonetLineEntry ::=
    SEQUENCE {
        adnsonetLineStatus
            INTEGER (-2147483648..2147483647),

        adnsonetLineAIS
            Counter,

        adnsonetLineRcvRDI
            Counter,

        adnsonetLineBIP
            Counter,

        adnsonetLineConfigLoopback
            INTEGER,

        adnsonetLineConfigRDI
            INTEGER,

        adnsonetLineConfigAIS
            INTEGER
    }

adnsonetLineStatus OBJECT-TYPE
    SYNTAX  INTEGER (-2147483648..2147483647)
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "This variable indicates the status
                 of the interface.
                 It is a bit map represented as a sum;
                 therefore, it can represent multiple
                 defects simultaneously.
                 The adnsonetLineNoDefect bit should
                 be set only if no other bit is set.

                 The bit positions are:
                       1   adnsonetLineNoDefect
                       2   adnsonetLineAIS
                       4   adnsonetLineRDI
	               8   adnsonetLineBIP  
                      16   adnsonetLineHighBIP  
		       
		 Common Name: Line State"
    ::= { adnsonetLineEntry  1 }

adnsonetLineAIS OBJECT-TYPE
    SYNTAX  Counter	-- UNITS milliseconds

    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "Duration of alarm indication signal (AIS).
		  Counter #2 of the ATM core."
    ::= { adnsonetLineEntry  2 }

adnsonetLineRcvRDI OBJECT-TYPE
    SYNTAX  Counter	-- UNITS milliseconds

    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "Duration of remote defect indication (RDI) 
                 received from the remote equipement.
	         For a Synchronous Digital Hierarchy (SDH)
                 line, the RDI signal is known as Multiplexing
                 Section Far End Receive Failure (MS-FERF).
		 Counter #3 of the ATM core."
    ::= { adnsonetLineEntry  3 }

adnsonetLineBIP OBJECT-TYPE
    SYNTAX  Counter
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "Number of 24-bit interleaved parity
                  (BIP-24) errors for STS-3c or STM-1.
                  Number of 96-bit interleaved parity
                  (BIP-96) errors for STS-12c or STM-4.
		  Counter #28 of the ATM core."
    ::= { adnsonetLineEntry  4 }

adnsonetLineConfigLoopback OBJECT-TYPE
    SYNTAX  INTEGER {
    adnsonetLineLoopbackON(1),
    adnsonetLineLoopbackOFF(2)
}
    ACCESS  read-write
    STATUS  mandatory
    DESCRIPTION
           "This variable indicates if line loopback
                  is activated or deactivated. 
                  When activated, received frames are 
                  retransmitted on the line.  
		  Common Name: Line Loopback"
    ::= { adnsonetLineEntry  5 }

adnsonetLineConfigRDI OBJECT-TYPE
    SYNTAX  INTEGER {
    adnsonetLineRDION(1),
    adnsonetLineRDIOFF(2)
}
    ACCESS  read-write
    STATUS  mandatory
    DESCRIPTION
           "This variable indicates if a permanent 
                  remote defect indication (RDI) or Far End
                  Receive Failure (FERF) is sent on the line.
                  It is a configuration option that should
                  be used only for maintenance purposes.
 		  Common Name: Forcing a Line Remote Defect 
 		  Indication"
    ::= { adnsonetLineEntry  6 }

adnsonetLineConfigAIS OBJECT-TYPE
    SYNTAX  INTEGER {
    adnsonetLineAISON(1),
    adnsonetLineAISOFF(2)
}
    ACCESS  read-write
    STATUS  mandatory
    DESCRIPTION
           "This variable indicates if a permanent
                  alarm indication signal (AIS) can be sent
                  on the line.
                  For STM-1 interfaces, this option can't be
                  activated (AIS off).
                  For STS-3c, STS-12c, and STM-4 interfaces,
                  this option can be activated
                  (AIS off or AIS on).
                  It is a configuration option that should
                  be used only for maintenance purposes.
		  Common Name: Forcing a Line Alarm Indication
		  Signal"
    ::= { adnsonetLineEntry  7 }

adnsonetPathTable OBJECT-TYPE
    SYNTAX  SEQUENCE OF AdnSonetPathEntry
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
           "The ADN SONET/SDH Path Current table."
    ::= { adnsonetPath  1 }

adnsonetPathEntry OBJECT-TYPE
    SYNTAX  AdnSonetPathEntry
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
           "An entry in the ADN SONET/SDH Path Current table."
    INDEX   { ifIndex }
    ::= { adnsonetPathTable  1 }

AdnSonetPathEntry ::=
    SEQUENCE {
        adnsonetPathWidth
            INTEGER,

        adnsonetPathStatus
            INTEGER (-2147483648..2147483647),

        adnsonetPathLOP
            Counter,

        adnsonetPathAIS
            Counter,

        adnsonetPathRcvRDI
            Counter,

        adnsonetPathBIP8
            Counter,

        adnsonetPathFEBE
            Counter,

        adnsonetPathConfigRDI
            INTEGER,

        adnsonetPathConfigAIS
            INTEGER
    }

adnsonetPathWidth OBJECT-TYPE
    SYNTAX  INTEGER {
    sts3cSTM1(1),
    sts12cSTM4(2)
}
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The type of Synchronous Optical 
                 Network/Synchronous Digital Hierarchy 
                 (SONET/SDH) path."
    ::= { adnsonetPathEntry  1 }

adnsonetPathStatus OBJECT-TYPE
    SYNTAX  INTEGER (-2147483648..2147483647)
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "This variable indicates the status
                 of the interface.
                 It is a bit map represented as a sum;
                 therefore, it can represent multiple
                 defects simultaneously.
                 The adnsonetPathNoDefect bit should
                 be set only if no other bit is set.

                 The bit positions are:
                   1   adnsonetLineNoDefect
                   2   adnsonetPathSTSLOP
                   4   adnsonetPathSTSAIS
                   8   adnsonetPathSTSRDI
                  16   adnsonetPathUnequipped *
                  32   adnsonetPathSignalLabelMismatch *
                  64   adnsonetPathBIP8
                 128   adnsonetPathFEBE

		 * not provided by HSS, but the value is 
		   required to ensure compatibility with 
		   the RFC sonetPathStatus."
    ::= { adnsonetPathEntry  2 }

adnsonetPathLOP OBJECT-TYPE
    SYNTAX  Counter	-- UNITS milliseconds

    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "Duration of loss-of-pointer (LOP).
	         Counter #12 of the ATM core."
    ::= { adnsonetPathEntry  3 }

adnsonetPathAIS OBJECT-TYPE
    SYNTAX  Counter	-- UNITS milliseconds

    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "Duration of alarm indication signal
                   (AIS) at path level.
	           Counter #8 of the ATM core."
    ::= { adnsonetPathEntry  4 }

adnsonetPathRcvRDI OBJECT-TYPE
    SYNTAX  Counter	-- UNITS milliseconds

    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "Duration of remote defect indication
		   (RDI) received from the remote
		   equipment at path level.
		   For Synchronous Digital Hierarchy (SDH),
		   this signal is known as a path Far End 
		   Receive Failure (FERF) event.
	           Counter #10 of the ATM core."
    ::= { adnsonetPathEntry  5 }

adnsonetPathBIP8 OBJECT-TYPE
    SYNTAX  Counter
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "Number of 8-bit interleaved parity
                   (BIP-8) errors.
	           Counter #11 of the ATM core."
    ::= { adnsonetPathEntry  6 }

adnsonetPathFEBE OBJECT-TYPE
    SYNTAX  Counter
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "Number of far end block errors (FEBE)
                   received from the remote equipment at
                   path level.
	           Counter #9 of the ATM core."
    ::= { adnsonetPathEntry  7 }

adnsonetPathConfigRDI OBJECT-TYPE
    SYNTAX  INTEGER {
    adnsonetPathRDION(1),
    adnsonetPathRDIOFF(2)
}
    ACCESS  read-write
    STATUS  mandatory
    DESCRIPTION
           "This variable indicates if a permanent
                  remote defect indication (RDI) is sent 
                  on the path.
                  For Synchronous Digital Hierarchy (SDH),
                  it indicates if a permanent Far End Receive
                  Failure (FERF) can be sent on the path.
                  This is a configuration option that should
                  be used only for maintenance purposes.
		  Common Name: Forcing a Path Remote Defect
		  Indication"
    ::= { adnsonetPathEntry  8 }

adnsonetPathConfigAIS OBJECT-TYPE
    SYNTAX  INTEGER {
    adnsonetPathAISON(1),
    adnsonetPathAISOFF(2)
}
    ACCESS  read-write
    STATUS  mandatory
    DESCRIPTION
           "This variable indicates if a permanent
                  alarm indication signal (AIS) is sent 
                  on the path.
                  This is a configuration option that should
                  be used only for maintenance purposes.
		  Common Name: Forcing a Path Alarm Indication
		  Signal"
    ::= { adnsonetPathEntry  9 }

