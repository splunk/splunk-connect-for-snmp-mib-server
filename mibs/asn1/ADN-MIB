adnUCNodeErrorsNmgsTooLongSll OBJECT IDENTIFIER ::= { adnUCNodeErrorsNmgs  4 }

adnUCNodeErrorsNmgsNullInmpSend OBJECT IDENTIFIER ::= { adnUCNodeErrorsNmgs  10 }

adnUCNodeErrorsNmgsInmpSend OBJECT IDENTIFIER ::= { adnUCNodeErrorsNmgs  11 }

adnUCNodeErrorsNmgsControlBlock OBJECT IDENTIFIER ::= { adnUCNodeErrorsNmgs  12 }

adnUCNodeErrorsNmgsPfxiAccess OBJECT IDENTIFIER ::= { adnUCNodeErrorsNmgs  13 }

adnUCNodeErrorsNmgsStatusRequest OBJECT IDENTIFIER ::= { adnUCNodeErrorsNmgs  14 }

adnUCNodeErrorsNmgsNoFullConfig OBJECT IDENTIFIER ::= { adnUCNodeErrorsNmgs  15 }

