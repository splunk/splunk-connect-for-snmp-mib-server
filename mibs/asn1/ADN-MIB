adnAnTable OBJECT-TYPE
    SYNTAX  SEQUENCE OF AdnAnEntry
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
           "HSS routing services address-to-node mapping
	table on the control unit. 
	The address-to-node table records the node(s) in the network
	that supports the specified address. 
	This table is used only by the frame relay type of service."
    ::= { adnRoutesMIB  18 }

adnAnEntry OBJECT-TYPE
    SYNTAX  AdnAnEntry
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
           "Each entry (row) in the adnAnTable that maps
	an address supported in the network to the node(s) where
	it is supported. An address may be supported multiple
	times."
    INDEX   { adnAnNumPlanIndex,  adnAnAdIndex,  adnAnNodeIndex }
    ::= { adnAnTable  1 }

AdnAnEntry ::=
    SEQUENCE {
        adnAnNumPlanIndex
            AdnRoutesNumPlan,

        adnAnAdIndex
            OCTET STRING,

        adnAnNodeIndex
            AdnRoutingID,

        adnAnRank
            INTEGER,

        adnAnNodeMacNum
            INTEGER (-2147483648..2147483647),

        adnAnNodeAreaNum
            INTEGER (-2147483648..2147483647),

        adnAnPartialAddrFlag
            INTEGER,

        adnAnAddrLength
            INTEGER
    }

adnAnNumPlanIndex OBJECT-TYPE
    SYNTAX  AdnRoutesNumPlan
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
           "Type of address; for example, X.121."
    ::= { adnAnEntry  1 }

adnAnAdIndex OBJECT-TYPE
    SYNTAX  OCTET STRING
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
           "Target address, maintained as ASCII decimal."
    ::= { adnAnEntry  2 }

adnAnNodeIndex OBJECT-TYPE
    SYNTAX  AdnRoutingID
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "Node ID for a specific node which supports the
	target address.  Since a single target address
	may be supported by more than one node (several
	service-gateway nodes for instance), there
	should be a separate entry for each one."
    ::= { adnAnEntry  3 }

adnAnRank OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "Rank (cost order) of the node ID to reach the address. 
	 The rank of the entry with the lowest cost is 1."
    ::= { adnAnEntry  4 }

adnAnNodeMacNum OBJECT-TYPE
    SYNTAX  INTEGER (-2147483648..2147483647)
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The machine number part of adnAnNodeIndex,
         the routing node ID of the node at the remote
         endpoint."
    ::= { adnAnEntry  5 }

adnAnNodeAreaNum OBJECT-TYPE
    SYNTAX  INTEGER (-2147483648..2147483647)
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The area number part of adnAnNodeIndex,
         the routing area number of the node at the
         remote endpoint."
    ::= { adnAnEntry  6 }

adnAnPartialAddrFlag OBJECT-TYPE
    SYNTAX  INTEGER {
    yes(1),
    no(2)
}
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "When a sequence of addresses is homing through 
	 a single gateway (Gateway 1), a user can enter 
	 a partial address by setting Partial Address Flag 
	 to `YES' to support the entire range of addresses.  
	 E.g.: 31103600 to 31103699 are going to Gateway 1. 
	 User can enter 311036 to Gateway 1 with 
	 adnAnPartialAddrFlag `yes'. "
    ::= { adnAnEntry  7 }

adnAnAddrLength OBJECT-TYPE
    SYNTAX  INTEGER (-2147483648..2147483647)
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           " The number of digits in the Address that is 
	  supported by network."
    ::= { adnAnEntry  8 }

