adnDumEnable OBJECT-TYPE
    SYNTAX  AdnEnableDisabled
    ACCESS  read-write
    STATUS  mandatory
    DESCRIPTION
           "Enables and disables all Disk Utilization
                Management processing."
    DEFVAL  { enabled }
    ::= { adnDumGroup  1 }

adnDumReduceExcessDiskStorage OBJECT-TYPE
    SYNTAX  AdnEnableDisabled
    ACCESS  read-write
    STATUS  mandatory
    DESCRIPTION
           "Enables and disables all methods to reduce excessive
                disk storage in Disk Utilization Management managed
                filesystems. Methods of reduction consist of those
                defined by adnDumCyclicallyReduceExcess and 
                adnDumImmediatelyReduceExcess. The Disk Utilization
                Management's managed filesystems are also defined in
                the adnDumGroup."
    DEFVAL  { enabled }
    ::= { adnDumGroup  2 }

adnDumCyclicallyReduceExcess OBJECT-TYPE
    SYNTAX  AdnEnableDisabled
    ACCESS  read-write
    STATUS  mandatory
    DESCRIPTION
           "Enables and disables the cyclical reduction of excess
                disk storage in Disk Utilization Management's managed
                filesytems. Disk Utilization Management's reduction
                process is an adjunct of its monitoring process.
                The periodicity of both processes is defined by
                adnDumMonitorInterval. The Disk Utilization Management's
                managed filesystems are also defined in adnDumGroup."
    DEFVAL  { enabled }
    ::= { adnDumGroup  3 }

adnDumImmediatelyReduceExcess OBJECT-TYPE
    SYNTAX  AdnEnableDisabled
    ACCESS  read-write
    STATUS  mandatory
    DESCRIPTION
           "Enables and disables the immediate reduction of
                excessive disk storage in Disk Utilization Management's
                managed filesystems. Independent of its monitoring
                cycle, Disk Utilization Management immediately reduces
                excessive disk storage. Once the excess storage is
                reduced, Disk Utilization Management effectively
                sets this object to disabled.
                The Disk Utilization Management's managed filesystems
                are also defined in the adnDumGroup."
    DEFVAL  { disabled }
    ::= { adnDumGroup  4 }

adnDumFlagThresholdViolations OBJECT-TYPE
    SYNTAX  AdnEnableDisabled
    ACCESS  read-write
    STATUS  mandatory
    DESCRIPTION
           "Enables and disables Disk Utilization Management's
                notification to the Network Management System of
                threshold violations from Disk Utilization
                Management's managed filesystems. Disk Utilization
                Management notifies the Network Management System
                via a Simple Network Management Protocol trap. The
                Disk Utilization Management's managed filesystems
                are also defined in the adnDumGroup."
    DEFVAL  { enabled }
    ::= { adnDumGroup  5 }

adnDumFlagFileReady OBJECT-TYPE
    SYNTAX  AdnEnableDisabled
    ACCESS  read-write
    STATUS  mandatory
    DESCRIPTION
           "Enables and disables Disk Utilization Management's
                notification to the Network Management System of 
                overdue file retrievals from Disk Utilization
                Management's managed filesystems. If file retrievals
                are not completed within the limit, 
                adnDumFileRetrievalTimeLimit, Disk Utilization
                Management considers the retrievals overdue and
                notifies the Network Management System via a Simple
                Network Management Protocol trap. The Disk
                Utilization Management's managed filesystems are
                also defined in the adnDumGroup."
    DEFVAL  { enabled }
    ::= { adnDumGroup  6 }

adnDumLogEvents OBJECT-TYPE
    SYNTAX  AdnEnableDisabled
    ACCESS  read-write
    STATUS  mandatory
    DESCRIPTION
           "Enables and disables Disk Utilization Management's
                 maintenance of a history log file in which Disk
                 Utilization Management events are recorded. The
                 history log file is in an ASCII format."
    DEFVAL  { enabled }
    ::= { adnDumGroup  7 }

adnDumMonitorInterval OBJECT-TYPE
    SYNTAX  INTEGER	-- UNITS seconds

    ACCESS  read-write
    STATUS  mandatory
    DESCRIPTION
           "Defines the period of time between disk monitoring
                 by Disk Utilization Management. The unit of time for
                 this interval is seconds."
    DEFVAL  { 300 }
    ::= { adnDumGroup  8 }

adnDumFileRetrievalTimeLimit OBJECT-TYPE
    SYNTAX  INTEGER	-- UNITS seconds

    ACCESS  read-write
    STATUS  mandatory
    DESCRIPTION
           "Defines the time period that Disk Utilization
                 Management allows the Network Management System
                 to retrieve files from Disk Utilization
                 Management's managed filesystems, before 
                 determining that a reminder trap is required. 
                 The unit of time for this interval is seconds."
    DEFVAL  { 360 }
    ::= { adnDumGroup  9 }

adnDumDirectoryTable OBJECT-TYPE
    SYNTAX  SEQUENCE OF AdnDumDirectoryEntry
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
           "The Disk Utilization Management Directory Table
                 defines the directories and subdirectories on
                 which the filesytems that Disk Utilization
                 Management manages are mounted. By default, 
                 the only filesystem monitored for disk 
                 utilization is /hss (which also contains
                 /to_nms)."
    ::= { adnDumGroup  10 }

adnDumDirectoryEntry OBJECT-TYPE
    SYNTAX  AdnDumDirectoryEntry
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
           "An entry in the Disk Utilization Management
                 Directory Table that defines one directory
                 and any subdirectories under the directory."
    INDEX   { adnDumDirectoryIndex }
    ::= { adnDumDirectoryTable  1 }

AdnDumDirectoryEntry ::=
    SEQUENCE {
        adnDumDirectoryIndex
            INTEGER,

        adnDumDirectoryPathname
            DisplayString,

        adnDumDirectoryPercentUsage
            AdnPercent,

        adnDumDirectoryLevelUsage
            INTEGER,

        adnDumUsageThresholds
            INTEGER
    }

adnDumDirectoryIndex OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
           "Index to a table of directories on which
                 Disk Utilization Management managed
                 filesystems have been mounted."
    ::= { adnDumDirectoryEntry  1 }

adnDumDirectoryPathname OBJECT-TYPE
    SYNTAX  DisplayString
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The complete pathname to the directory on which
                 a managed filesystem is mounted. The complete
                 pathname starts with and includes the root
                 directory. By default, the directory /hss is
                 monitored. This includes the directory 
                 /to_nms."
    ::= { adnDumDirectoryEntry  2 }

adnDumDirectoryPercentUsage OBJECT-TYPE
    SYNTAX  AdnPercent
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The percent of the aggregate disk storage that is
                 used in the directory on which the filesystem is
                 mounted."
    DEFVAL  { 100 }
    ::= { adnDumDirectoryEntry  3 }

adnDumDirectoryLevelUsage OBJECT-TYPE
    SYNTAX  INTEGER {
    belowLow(1),
    lowToMedium(2),
    mediumToHigh(3),
    abovehigh(4)
}
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The level of usage of aggregate disk storage with 
                 respect to the three usage thresholds, low, medium
                 and high, in the directory on which the filesystem
                 is mounted."
    DEFVAL  { 1 }
    ::= { adnDumDirectoryEntry  4 }

adnDumUsageThresholds OBJECT-TYPE
    SYNTAX  INTEGER {
    low60med70high80(1),
    low70med80high90(2),
    low85med90high95(3),
    low90med95high99(4)
}
    ACCESS  read-write
    STATUS  mandatory
    DESCRIPTION
           "A set of low, medium and  high limits to the amount
                 of the aggregate disk storage that is utilized in a
                 managed filesystem. Each value represents a percentage
                 of the whole amount that is allocated to the 
                 particular filesystem."
    DEFVAL  { low70med80high90 }
    ::= { adnDumDirectoryEntry  5 }

adnDumSubdirectoryTable OBJECT-TYPE
    SYNTAX  SEQUENCE OF AdnDumSubdirectoryEntry
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
           "The Disk Utilization Management Subdirectory
                 Table defines the subdirectories under a Disk
                 Utilization Management directory. A filesytem
                 that Disk Utilization Management manages is
                 mounted on the Disk Utilization Management
                 directory. By default, the subdirectories 
                 under /hss which are monitored are:
                 /hss/to_nms/perf  (first priority is to delete
                                   old historical stats files.)
                 /hss/to_nms/fault (second priority is to delete
                                   old alarm files.)
                 /hss/to_nms/acct  (third priority is to delete
                                   old accounting files, but only 
                                   if the quota for accoutning is
                                   less than the default value 
                                   of 100 percent.)"
    ::= { adnDumGroup  11 }

adnDumSubdirectoryEntry OBJECT-TYPE
    SYNTAX  AdnDumSubdirectoryEntry
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
           "An entry in the Disk Utilization Management
                 Subdirectory Table that defines one subdirectories
                 under a Disk Utilization Management directory."
    INDEX   { adnDumDirectoryIndex,  adnDumSubdirectoryIndex }
    ::= { adnDumSubdirectoryTable  1 }

AdnDumSubdirectoryEntry ::=
    SEQUENCE {
        adnDumDirectoryIndex
            INTEGER,

        adnDumSubdirectoryIndex
            INTEGER,

        adnDumSubdirectoryPathname
            DisplayString,

        adnDumSubdirectoryQuota
            AdnPercent
    }

adnDumSubdirectoryIndex OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
           "Index to a table of subdirectories under a
                 directory on which a Disk Utilization
                 Management's managed filesystem has been
                 mounted. The value of the index is directly
                 related to order in which the subdirectories
                 are targeted for storage reduction. The first
                 subdirectory in the table of subdirectories
                 that Disk Utilization Management targets for
                 storage reduction has index 1"
    ::= { adnDumSubdirectoryEntry  1 }

adnDumSubdirectoryPathname OBJECT-TYPE
    SYNTAX  DisplayString
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The complete pathname to the subdirectory of
                 a Disk Utilization Management directory. The
                 complete pathname starts with and includes the
                 root directory."
    ::= { adnDumSubdirectoryEntry  2 }

adnDumSubdirectoryQuota OBJECT-TYPE
    SYNTAX  AdnPercent
    ACCESS  read-write
    STATUS  mandatory
    DESCRIPTION
           "The percent of the aggregate disk storage that 
                 is allocated to the subdirectory of the 
                 filesystem on which the parent directory is 
                 mounted. The default quota for the 
                 /hss/to_nms/perf subdirectory is 10 percent,
                 meaning that on a first pass of the 
                 reduction cycle, old statistics files will 
                 be deleted until the /hss/to_nms/perf 
                 subdirectory is using no more than 10 
                 percent of the total partition size. The 
                 default quota for the /hss/to_nms/fault 
                 subdirectory is 10 percent.
                 The default quota for the /hss/to_nms/acct 
                 subdirectory is 100 percent, meaning that 
                 no accounting files will be deleted by this 
                 process."
    DEFVAL  { 100 }
    ::= { adnDumSubdirectoryEntry  3 }

adnUpdateCntlMIB OBJECT IDENTIFIER ::= { adnProprietary  12 }

adnUCNodeGroup OBJECT IDENTIFIER ::= { adnUpdateCntlMIB  1 }

adnUpdateCntl OBJECT IDENTIFIER ::= { adnUpdateCntlMIB  2 }

adnVerInfoGroup OBJECT IDENTIFIER ::= { adnUpdateCntlMIB  3 }

adnUCNodeAdminCommand OBJECT-TYPE
    SYNTAX  INTEGER {
    null(1),
    install(2),
    activate(3),
    commit(4),
    deactivate(5),
    withdraw(6),
    confirm-link(7),
    qInstallActivate(8),
    qDeactivateWithdraw(9),
    qConfirmChange(10),
    qRebaseline(11)
}
    ACCESS  read-write
    STATUS  mandatory
    DESCRIPTION
           "The NMS sets a command value into this object,as follows:
	
		NULL       No action to take place.
		install	   Places newly-transferred files in a read-
			   only partition.
		activate   HSS sends code/configuration to 
			   subsystems.
		commit	   HSS declares the activated code/
			   configuration to be in committed state. 
		deactivate Changes the state of the HSS by reverting
			   to the committed code and configuration.
		withdraw   Deletes installed files.
                confirm-link  Set by the NMS following receipt of 
                              successful Activate trap.
		qInstallActivate 	Places newly-transferred Quick
					configuration change files 
					in a secured partition and 
					sends configuration to subsytems.
		qDeactivateWithdraw     Undoes Quick configuration changes
					and deletes installed files.
		qConfirmChange		Confirms a Quick configuration
					change and sets it as an extension 
					of the committed version.
		qRebaseline		Install and commit a new set of 
					configuration files to synchronize
					the NMS and HSS after several
					Quick transactions.
	Common Name: Administrative Command"
    ::= { adnUCNodeGroup  1 }

adnUCNodeOperCommand OBJECT-TYPE
    SYNTAX  INTEGER {
    other(1),
    install(2),
    activate(3),
    commit(4),
    deactivate(5),
    withdraw(6),
    none(7),
    qInstallActivate(8),
    qDeactivateWithdraw(9),
    qConfirmChange(10),
    qRebaseline(11)
}
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "This object indicates the current or last command
	accepted and processed by the HSS.
        Common Name: Oper Command"
    ::= { adnUCNodeGroup  2 }

adnUCNodeOperStatus OBJECT-TYPE
    SYNTAX  INTEGER {
    in-progress(1),
    success(2),
    failed(3),
    warning(4),
    rollback-in-progress(5),
    initial(6)
}
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "This object notifies the NMS of the results of the last 
	command, or if it is still in progress.
        Common Name: Oper Command Status"
    ::= { adnUCNodeGroup  3 }

adnUCNodeState OBJECT-TYPE
    SYNTAX  INTEGER {
    committed(0),
    activateInProgress(1),
    uncommitted(2),
    activateInProgressSuspended(3),
    loadingCommittedSuspended(4),
    loadingCommitted(5),
    qConfirmed(6),
    qActivateInProgress(7),
    qUnconfirmed(8),
    qLoadingConfirmed(9),
    qRebaselineInProgress(10)
}
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "This object notifies the NMS of the `state' of code/
	configuration update processing. The values are:
        committed -- The node is running the committed 
                     software version.
        activateInProgress -- An Activate command is in 
                     progress.
        uncommitted -- The node is running the uncommitted
                     software version.
        activateInProgressSuspended -- An Activate command 
                     is in progress.  The processing of
                     the Activate is suspended while the
                     Control Unit board waits for a 
                     confirmation that any coupler boards 
                     that have been reset have gone down,
                     before Activate processing continues.
        loadingCommittedSuspended -- The node is in the 
                     process of loading the committed 
                     software version, due to a Deactivate
                     command or an automatic rollback.  
                     The processing of the deactivation is
                     suspended while the Control Unit board 
                     waits for a confirmation that any 
                     coupler boards that have been reset 
                     have gone down, before deactivation 
                     processing continues.
        loadingCommitted -- The node is in the process of
                     loading the committed software version,
                     due to a Deactivate command or an 
                     automatic rollback.
        qConfirmed -- The node is running a confirmed Quick
		     configuration change on top of the 
		     committed version.
        qActivateInProgress -- A Quick Install&Activate 
		     command is in progress.
        qUnconfirmed -- The node is running the unconfirmed
                     Quick configuration.
        qLoadingConfirmed -- The node is in the process of
                     loading the confirmed Quick configuration,
                     due to a Quick Deactivate&Withdraw command 
		     or an automatic rollback.
	qRebaselineInProgress -- A Quick Rebaseline command is
		     in progress.
	Common name: Update Control Status"
    ::= { adnUCNodeGroup  4 }

adnUCNodeErrorType OBJECT-TYPE
    SYNTAX  INTEGER {
    no-err(0),
    config(1),
    software(2)
}
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "This object notifies the NMS of the type of error detected
	by the HSS configuration management.
	no-err   -- There is no error
	config   -- Error occurred in configuration file management
	software -- Error occurred in firmware or software 
	            management.
        Common Name: Update Error Type"
    ::= { adnUCNodeGroup  5 }

adnUCNodeErrorStatus OBJECT-TYPE
    SYNTAX  INTEGER {
    noError(0),
    tooBig(1),
    noSuchName(2),
    badValue(3),
    readOnly(4),
    genErr(5),
    noAccess(6),
    wrongType(7),
    wrongLength(8),
    wrongEncoding(9),
    wrongValue(10),
    noCreation(11),
    inconsistentValue(12),
    resourceUnavailable(13),
    commitFailed(14),
    undoFailed(15),
    authorizationError(16),
    notWritable(17),
    inconsistentName(18)
}
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "This object notifies the NMS of the exact error detected 
	by the HSS configuration management.  For configuration table 
	errors, this is the SNMP error code defined in RFC 1448.
	For software or	firmware management errors, the value is 
	genErr(5).
        Common Name: Update Error Status"
    ::= { adnUCNodeGroup  6 }

adnUCNodeErrorGroup OBJECT-TYPE
    SYNTAX  INTEGER {
    noError(0),
    sfas(1),
    nmgs(2),
    routes(3),
    cme(4),
    frs(5),
    isna(6),
    isom(7),
    llce(8),
    franpCm(9),
    franpSi(10),
