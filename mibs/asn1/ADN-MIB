        adnCMBfvcadnFrVCRemNodeAreaNum
            INTEGER (-2147483648..2147483647),

        adnCMBfvcfrPVCEndptDLCIIndex
            INTEGER (-2147483648..2147483647),

        adnCMBfvcfrPVCEndptInMaxFrameSize
