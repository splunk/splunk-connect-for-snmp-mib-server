    other(1),
    core-complement(2)
}
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
           "Index field.  The value of a Control Unit service."
    ::= { adnCuCodeUpdateCntlEntry  1 }

adnCuCodeOperStatus OBJECT-TYPE
    SYNTAX  INTEGER {
    in-progress(1),
    success(2),
    failed(3),
    pending(4)
}
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "This object notifies the NMS of the results of the last 
	command, or whether it is still in progress."
    ::= { adnCuCodeUpdateCntlEntry  2 }

adnCuCodeState OBJECT-TYPE
    SYNTAX  INTEGER {
    committed(1),
    uncommitted(2)
}
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "This object notifies the NMS of the `state' of Control
	Unit code update processing."
    ::= { adnCuCodeUpdateCntlEntry  3 }

adnCuConfigUpdateCntlTable OBJECT-TYPE
    SYNTAX  SEQUENCE OF AdnCuConfigUpdateCntlEntry
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
           "The Configuration Update Control table contains objects 
	for Control Unit Configuration operational status between
	the HSS and the NMS."
    ::= { adnUpdateCntl  3 }

adnCuConfigUpdateCntlEntry OBJECT-TYPE
    SYNTAX  AdnCuConfigUpdateCntlEntry
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
           "An entry in the Control Unit Configuration Update Control
	table."
    INDEX   { adnCuConfigApplIdIndex }
    ::= { adnCuConfigUpdateCntlTable  1 }

AdnCuConfigUpdateCntlEntry ::=
    SEQUENCE {
        adnCuConfigApplIdIndex
            INTEGER,

        adnCuConfigOperStatus
            INTEGER,

        adnCuConfigState
            INTEGER,

        adnCuConfigFileName
            OCTET STRING,

        adnCuConfigNextFileName
            OCTET STRING
    }

adnCuConfigApplIdIndex OBJECT-TYPE
    SYNTAX  INTEGER {
    sfas-id(1),
    nmgs-id(2),
    msup-id(3),
    iss-id(4),
    pmd-id(5),
    routes-id(6),
    mibmgt-id(7),
    iprt-id(8)
}
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
           "Index field - Control Unit application IDs (e.g. NMGS, 
	SFAS, Main Supervisor, ....)"
    ::= { adnCuConfigUpdateCntlEntry  1 }

adnCuConfigOperStatus OBJECT-TYPE
    SYNTAX  INTEGER {
    in-progress(1),
    success(2),
    failed(3),
    pending(4)
}
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "This object notifies the NMS of the results of the last 
	command, or whether it is still in progress."
    ::= { adnCuConfigUpdateCntlEntry  2 }

adnCuConfigState OBJECT-TYPE
    SYNTAX  INTEGER {
    committed(0),
    actInProgWaitingForLoadInd(1),
    actInProgMiddleTransaction(2),
    actInProgEndTransaction(3),
    uncommitted(4),
    loadingCommittedNextFile(5),
    loadingCommittedMiddleTrans(6),
    loadingCommittedEndTrans(7)
}
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "This object notifies the NMS of the `state' of Control
	Unit configuration update processing. The values mean:
        committed -- This CU application is not currently 
                     loading configuration tables.  Its 
                     configuration tables are from the 
                     committed version.
        actInProgWaitingForLoadInd -- An Activate command
                     is in progress, and for this CU 
                     application, the state machine is 
                     waiting for an indication that a request
                     has been received to start loading the
                     tables.
        actInProgMiddleTransaction -- An Activate command
                     is in progress, and for this CU 
                     application, the state machine is 
                     waiting for the response to a middle 
                     message within a configuration 
                     transaction.
        actInProgEndTransaction -- An Activate command
                     is in progress, and for this CU 
                     application, the state machine is 
                     waiting for the response to a null 
                     message that signals the end of a 
                     a configuration transaction.
        uncommitted -- This CU application is using
                     uncommitted configuration tables.
        loadingCommittedNextFile -- This CU application 
                     is the the process of loading 
                     uncommitted configuration tables.
                     The state machine is waiting for a 
                     response to a null message that 
                     signals the end of a transaction, 
                     and will then send the next 
                     configuration file, which will be the
                     rollback file.
        loadingCommittedMiddleTrans -- This CU application
                     is the the process of loading
                     committed configuration tables.
                     The state machine is waiting for a
                     response to a middle message within
                     the configuration transaction.
        loadingCommittedEndTrans -- This CU application
                     is the the process of loading
                     committed configuration tables.
                     The state machine is waiting for a
                     response to a null message that 
                     signals the end of a transaction."
    ::= { adnCuConfigUpdateCntlEntry  3 }

adnCuConfigFileName OBJECT-TYPE
    SYNTAX  OCTET STRING
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "This object identifies the configuration file to be used 
	for this Control Unit subsystem."
    ::= { adnCuConfigUpdateCntlEntry  4 }

adnCuConfigNextFileName OBJECT-TYPE
    SYNTAX  OCTET STRING
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "This object identifies the next configuration file to be
	used for this Control Unit subsystem, if there is an automatic
	rollback in progress from uncommitted configuration to 
	committed configuration.  The value of this object is not 
	significant if there is currently no rollback in progress."
    ::= { adnCuConfigUpdateCntlEntry  5 }

adnProcCodeUpdateCntlTable OBJECT-TYPE
    SYNTAX  SEQUENCE OF AdnProcCodeUpdateCntlEntry
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
           "The Update Control table contains objects for processor 
	code operational status between the HSS and the NMS."
    ::= { adnUpdateCntl  4 }

adnProcCodeUpdateCntlEntry OBJECT-TYPE
    SYNTAX  AdnProcCodeUpdateCntlEntry
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
           "An entry in the processor code portion of the Update 
	Control table."
    INDEX   { adnProcCodeChassisIndex,  adnProcCodeSlotIndex,  adnProcCodeProcessorIndex }
    ::= { adnProcCodeUpdateCntlTable  1 }

AdnProcCodeUpdateCntlEntry ::=
    SEQUENCE {
        adnProcCodeChassisIndex
            INTEGER (-2147483648..2147483647),

        adnProcCodeSlotIndex
            INTEGER (-2147483648..2147483647),

        adnProcCodeProcessorIndex
            INTEGER (-2147483648..2147483647),

        adnProcCodeOperStatus
            INTEGER,

        adnProcCodeState
            INTEGER
    }

adnProcCodeChassisIndex OBJECT-TYPE
    SYNTAX  INTEGER (-2147483648..2147483647)
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
           "Index field that identifies the chassis. Chassis are 
	numbered 1-7. The main chassis (i.e., the one with the 
	active Control Unit) in a multi-chassis node is chassis
	number 1. In a single-chassis node, the chassis number is 1.
	This number is provided in hardware by a HLCID card which 
	is plugged into the midplane."
    ::= { adnProcCodeUpdateCntlEntry  1 }

adnProcCodeSlotIndex OBJECT-TYPE
    SYNTAX  INTEGER (-2147483648..2147483647)
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
           "Index field that identifies the slot. Slots within a 
	chassis are numbered 1-12. The first slot on the left when 
	facing the front of the chassis is number 1. This number is
	provided in hardware through hard-wiring on the midplane. 
	Note that the HBSAC board always has a special slot 
	identifier of 30, but this slot number is never used."
    ::= { adnProcCodeUpdateCntlEntry  2 }

adnProcCodeProcessorIndex OBJECT-TYPE
    SYNTAX  INTEGER (-2147483648..2147483647)
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
           "Index field that identifies the processor on a coupler 
	board. Processors on a coupler are numbered 0-3. The number
	of processors varies depending on the type of coupler."
    ::= { adnProcCodeUpdateCntlEntry  3 }

adnProcCodeOperStatus OBJECT-TYPE
    SYNTAX  INTEGER {
    in-progress(1),
    success(2),
    failed(3),
    pending(4)
}
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "This object notifies the NMS of the results of the last 
	command, or whether it is still in progress."
    ::= { adnProcCodeUpdateCntlEntry  4 }

adnProcCodeState OBJECT-TYPE
    SYNTAX  INTEGER {
    committed(0),
    actInProgWaitForDownMsg(1),
    actInProgWaitForReloadReq(2),
    actInProgWaitForSwCompleteMsg(3),
    uncommitted(4),
    loadCommittedWaitForDownMsg(5),
    loadCommittedWaitForCompleteMsg(6),
    pending(7),
    actInProgWaitForFwareCompleteMsg(8),
    actInProgWaitForFlashCompleteMsg(9),
    actInProgWaitForLoadRequest(10)
}
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "This object notifies the NMS of the `state' of processor 
	code update processing. The values are:
        committed -- This processor is not in the process of
                     loading, and the software for this 
                     processor is the committed version.
        actInProgWaitForDownMsg -- An Activate command
                     is in progress, this processor has
                     been reset, and the state machine is
                     waiting for a message confirming that
                     the processor is now down.
        actInProgWaitForReloadReq -- An Activate command
                     is in progress, this processor has
                     already loaded at least once and then
                     started to reload.  The state machine
                     is waiting for another load request.
        actInProgWaitForSwCompleteMsg -- An Activate command
                     is in progress, and the state machine
                     is waiting for a message to indicate 
                     that the software portion of the load 
                     has completed.
        uncommitted -- This processor is not in the process 
                     of loading.  It is currently running
                     uncommitted software.
        loadCommittedWaitForDownMsg -- This processor is
                     in the process of loading committed 
                     software.  The processor has been reset,
                     and the state machine is waiting for a 
                     message confirming that the processor 
                     is down.
        loadCommittedWaitForCompleteMsg -- This processor
                     is in the process of loading committed
                     software.  The state machine is waiting
                     for a message to indicate that the load
                     has completed.
        pending -- An Activate command is in progress, and 
                     this processor is currently down.  When
                     the processor comes up, it will be 
                     loaded with the uncommitted software 
                     version.
        actInProgWaitForFwareCompleteMsg -- An Activate 
                     command is in progress, and the state
                     machine is waiting for a message to
                     indicate that the firmware portion
                     of the load has completed.
        actInProgWaitForFlashCompleteMsg -- An Activate 
                     command is in progress, and the state
                     machine is waiting for a message to
                     indicate that the update of code into
                     flash memory has completed.
        actInProgWaitForLoadRequest -- An Activate command
                     is in progress, and this processor
                     has been reset.  The state machine is
                     waiting for a request to start the 
                     loading process."
    ::= { adnProcCodeUpdateCntlEntry  5 }

adnBrdConfigUpdateCntlTable OBJECT-TYPE
    SYNTAX  SEQUENCE OF AdnBrdConfigUpdateCntlEntry
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
           "The board configuration portion of the Update Control 
	table contains objects for board configuration operational
	status between the HSS and the NMS."
    ::= { adnUpdateCntl  5 }

