    SYNTAX  INTEGER (-2147483648..2147483647)
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "This is the most recent clearing diagnostic code that
         was received from the network for this VC."
    ::= { adnFrVCEntry  43 }

adnFrVCLastClrCauseTX OBJECT-TYPE
    SYNTAX  INTEGER (-2147483648..2147483647)
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "This is the most recent clearing cause that was
         transmitted to the network for this VC."
    ::= { adnFrVCEntry  44 }

adnFrVCLastClrDiagTX OBJECT-TYPE
    SYNTAX  INTEGER (-2147483648..2147483647)
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "This is the most recent clearing diagnostic code that
         was transmitted to the network for this VC."
    ::= { adnFrVCEntry  45 }

adnFrVCRemoteifindex OBJECT-TYPE
    SYNTAX  INTEGER (-2147483648..2147483647)
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "This is the ifIndex of the interface on the node
          at the remote end of the VC. Together the objects
          adnFrVCRemNodeID, ifIndex, adnFrVCRemoteifindex,
          frPVCEndptDLCIIndex, adnFrVCRemDlci, and
          adnFrVCUniqueConnID uniquely identify VCs for
          accounting and statistics purposes."
    ::= { adnFrVCEntry  46 }

adnFrPVCEndptXmitdSigStatus OBJECT-TYPE
    SYNTAX  INTEGER {
    deleted(1),
    active(2),
    inactive(3),
    none(4)
}
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The value of this object identifies the PVC
             status sent via the local in-channel signaling
             procedures for this PVC end-point.  If the logical port
             is performing only user-side procedures, or if the interface
	     is inoperative, then this value should be 'none(4)'."
    ::= { adnFrVCEntry  47 }

adnFrVCInBc OBJECT-TYPE
    SYNTAX  INTEGER (-2147483648..2147483647)
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "This specifies the committed burst size (Bc) parameter 
        (measured in bits) for this PVC end-point in the ingress 
        direction (into the frame relay network).  This is a 
        measure of the exclusively allocated trunk bandwidth 
        required for the user's committed data.
        Common Name: Ingress Trunk Bc"
    DEFVAL  { 64000 }
    ::= { adnFrVCEntry  48 }

adnFrVCOutBc OBJECT-TYPE
    SYNTAX  INTEGER (-2147483648..2147483647)
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "This specifies the committed burst size (Bc) parameter 
        (measured in bits) for this PVC end-point in the egress 
        direction (out of the frame relay network).  This is a 
        measure of the exclusively allocated trunk bandwidth 
        required for the user's committed data.
        Common Name: Egress Trunk Bc"
    DEFVAL  { 64000 }
    ::= { adnFrVCEntry  49 }

adnFrVCRemNodeMacNum OBJECT-TYPE
    SYNTAX  INTEGER (-2147483648..2147483647)
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The machine number part of adnFrVCRemNodeID, the routing
         node ID of the node at the remote endpoint of this frame
         relay VC."
    ::= { adnFrVCEntry  50 }

adnFrVCRemNodeAreaNum OBJECT-TYPE
    SYNTAX  INTEGER (-2147483648..2147483647)
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The area number part of adnFrVCRemNodeID, the routing
         node ID of the node at the remote endpoint of this frame
         relay VC."
    ::= { adnFrVCEntry  51 }

adnFrVCDelaySensitive OBJECT-TYPE
    SYNTAX  TruthValue
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "This field indicates whether the VC is delay sensitive.
          If either this field or adnFrChanDelaySensitive is set to
          'true(1)', then path selection for the VC will be based on
          the criterion (delay, monetary cost, hops). Otherwise, path
          selection will be based on the criterion
          (monetary cost, delay, hops)."
    DEFVAL  { false }
    ::= { adnFrVCEntry  52 }

adnFrVCCos OBJECT-TYPE
    SYNTAX  AdnFrCos
    ACCESS  read-write
    STATUS  mandatory
    DESCRIPTION
           "This indicates the Class of Service (COS) for the PVC.
          SVCs and calls received through a TPX Gateway use the
          COS defined in adnFrChanDefaultCos."
    DEFVAL  { cosNrt }
    ::= { adnFrVCEntry  53 }

