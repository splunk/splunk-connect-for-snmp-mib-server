    SYNTAX  SEQUENCE OF AdnLapFlowEntry
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
           "This table defines the objects recorded by
		LAP (link access procedure) to provide information about
		the traffic flow through the interface."
    ::= { adnLapMIB  3 }

adnLapFlowEntry OBJECT-TYPE
    SYNTAX  AdnLapFlowEntry
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
           "This contains information regarding the effects of
		flow controls in LAP (link access procedure)."
    INDEX   { ifIndex,  adnLapDLCIIndex }
    ::= { adnLapFlowTable  1 }

AdnLapFlowEntry ::=
    SEQUENCE {
        adnLapFlowStateChanges
            Counter,

        adnLapFlowChangeReason
            INTEGER,

        adnLapFlowCurrentMode
            INTEGER,

        adnLapFlowBusyDefers
            Counter,

        adnLapFlowRejOutPkts
            Counter,

        adnLapFlowRejInPkts
            Counter,

        adnLapFlowT200Timeouts
            Counter,

        adnLapFlowT203Timeouts
            Counter,

        adnLapFlowLinkResets
            Counter,

        adnLapFlowNsErrorsIns
            Counter,

        adnLapFlowNrErrorsIns
            Counter,

        adnLapFlowFrmrSent
            OCTET STRING,

        adnLapFlowFrmrReceived
            OCTET STRING
    }

adnLapFlowStateChanges OBJECT-TYPE
    SYNTAX  Counter
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The number of LAP (link access procedure) state changes,
                including resets."
    ::= { adnLapFlowEntry  2 }

adnLapFlowChangeReason OBJECT-TYPE
    SYNTAX  INTEGER {
    notStarted(1),
    abmeEntered(2),
    abmeReset(3),
    dmReceived(4),
    dmSent(5),
    discReceived(6),
    frmrReceived(8),
    frmrSent(9),
    n200Timeout(10),
    other(11)
}
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The reason for the most recent incrementing
		of adnlapFlowStateChanges."
    ::= { adnLapFlowEntry  3 }

adnLapFlowCurrentMode OBJECT-TYPE
    SYNTAX  INTEGER {
    other(1),
    disconnected(2),
    linkSetup(3),
    frameReject(4),
    informationTransfer(6),
    rejFrameSent(7),
    waitingAcknowledgement(8),
    stationBusy(9),
    remoteStationBusy(10),
    bothStationsBusy(11),
    waitingAckStationBusy(12),
    waitingAckRemoteBusy(13),
    waitingAckBothBusy(14),
    rejFrameSentRemoteBusy(15),
    xidFrameSent(16),
    error(17)
}
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The current condition of the session."
    ::= { adnLapFlowEntry  4 }

adnLapFlowBusyDefers OBJECT-TYPE
    SYNTAX  Counter
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The number of times this device was unable
		to transmit a frame due to a perceived
		remote busy condition.  Busy conditions can
		result from the receipt of an RNR (receive not ready) 
		from the remote device, the lack of valid sequence
		number space (window saturation), or other
		conditions."
    ::= { adnLapFlowEntry  5 }

adnLapFlowRejOutPkts OBJECT-TYPE
    SYNTAX  Counter
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The number of REJ (reject) frames sent by
		this station."
    ::= { adnLapFlowEntry  6 }

adnLapFlowRejInPkts OBJECT-TYPE
    SYNTAX  Counter
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The number of REJ (reject) frames received
		by this station."
    ::= { adnLapFlowEntry  7 }

adnLapFlowT200Timeouts OBJECT-TYPE
    SYNTAX  Counter
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The number of times a transmission was resent
                because the T200 timer expired."
    ::= { adnLapFlowEntry  8 }

adnLapFlowT203Timeouts OBJECT-TYPE
    SYNTAX  Counter
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The number of times a transmission was resent
                because the T203 timer expired."
    ::= { adnLapFlowEntry  9 }

adnLapFlowLinkResets OBJECT-TYPE
    SYNTAX  Counter
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The number of times a SABME (set asynchronous balanced
                mode extended) was received while this interface was in
                data transfer state."
    ::= { adnLapFlowEntry  10 }

adnLapFlowNsErrorsIns OBJECT-TYPE
    SYNTAX  Counter
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The number of Ns (send sequence number) errors that were 
		detected on this interface while in data transfer state."
    ::= { adnLapFlowEntry  11 }

adnLapFlowNrErrorsIns OBJECT-TYPE
    SYNTAX  Counter
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The number of Nr (receive sequence number) errors 
		that were detected on this interface while in data 
                transfer state."
    ::= { adnLapFlowEntry  12 }

adnLapFlowFrmrSent OBJECT-TYPE
    SYNTAX  OCTET STRING
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The information field of the FRMR (frame reject) most
		recently sent.  If no FRMR has been sent
		(the normal case) or the information is not
		available, this will be an OCTET STRING of
		zero length."
    ::= { adnLapFlowEntry  13 }

adnLapFlowFrmrReceived OBJECT-TYPE
    SYNTAX  OCTET STRING
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The information field of the FRMR (frame reject) most
		recently received.  If no FRMR has been
		received (the normal case) or the
		information is not available, this will be an
		OCTET STRING of zero length."
    ::= { adnLapFlowEntry  14 }

adnDs1 OBJECT IDENTIFIER ::= { adnTransmission  18 }

adnDsx1ConfigTable OBJECT-TYPE
    SYNTAX  SEQUENCE OF AdnDsx1ConfigEntry
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
           "This table is the extension of the
            dsx1ConfigTable described in the DS1/E1 
            standard MIB."
    ::= { adnDs1  1 }

adnDsx1ConfigEntry OBJECT-TYPE
    SYNTAX  AdnDsx1ConfigEntry
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
           "An entry in the DS1
            Configuration table extension."
    INDEX   { dsx1LineIndex }
    ::= { adnDsx1ConfigTable  1 }

AdnDsx1ConfigEntry ::=
    SEQUENCE {
        adnDsx1LineType
            INTEGER,

        adnDsx1LineLength
            INTEGER,

        adnDsx1LineStatus
            INTEGER (-2147483648..2147483647),

        adnDsx1MsAisTest
            INTEGER,

        adnDsx1MsFerfTest
            INTEGER,

        adnDsx1RemLoopbackTest
            INTEGER,

        adnDsx1LineHWStatus
            INTEGER (-2147483648..2147483647),

        adnDsx1StatType
            INTEGER,

        adnDsx1LinePerfStatus
            INTEGER (-2147483648..2147483647)
    }

adnDsx1LineType OBJECT-TYPE
    SYNTAX  INTEGER {
    noQual(1),
    dsx1E1Unframed(2)
}
    ACCESS  read-write
    STATUS  mandatory
    DESCRIPTION
           "The field is used to qualify the dsx1LineType.
            noQual          - No qualification.
            e1Unframed      - E1 with no framing"
    ::= { adnDsx1ConfigEntry  1 }

adnDsx1LineLength OBJECT-TYPE
    SYNTAX  INTEGER	-- UNITS meters

    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The length of the DS1 line in meters. This
        object provides information for line build out
        circuitry if it exists and can profitably use the
        information.  Accuracy should be within 30 meters.
        For many short lines and for lines that cannot use
        this information, the value zero is appropriate.

        Line build out circuitry sets electrical power levels
        in the transmitter, and sensitivity in the receiver.

        The line length means the distance to the nearest
        electrical regeneration point (which could be a simple
        repeater, or a CSU or DACS/multiplexer, or even a
        direct connection to another switch, etc.

        There is a possibility of increased noise (or even
        total signal loss) if the length is not set properly."
    ::= { adnDsx1ConfigEntry  2 }

adnDsx1LineStatus OBJECT-TYPE
    SYNTAX  INTEGER (-2147483648..2147483647)
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "This variable indicates the status
            of the line interface.
            It is a bit map represented as a sum;
            therefore, it can represent multiple
            defects simultaneously.
            The adnDsx1NoAlarm bit should be set
            only if no other bit is set.

            The bit positions and their corresponding
            ATM core counter numbers # are:

    #     Bit     Value 
    ------------------------------------------------------
      
    .      1        noAlarm        
    .                No alarm present
           
    21     2        HBERError      
    .                High bit error rate (HBER) 
   
    23     4        RLBError       
    .                Loopback requested by the remote equipment 

    30     8        LCMAError       
    .                Loss of CRC multiframe alignment 

      Common Name: Line Status"
    ::= { adnDsx1ConfigEntry  3 }

adnDsx1MsAisTest OBJECT-TYPE
    SYNTAX  INTEGER {
    on(1),
    off(2)
}
    ACCESS  read-write
    STATUS  mandatory
    DESCRIPTION
           "This variable is used to send a constant alarm
            indication signal (AIS) to the remote equipment.
            This object is used only for testing and debugging.
            This object is available for ATM T1/E1 only.
            Common Name: Forcing a Line Alarm Indication Signal"
    ::= { adnDsx1ConfigEntry  4 }

adnDsx1MsFerfTest OBJECT-TYPE
    SYNTAX  INTEGER {
    on(1),
    off(2)
}
    ACCESS  read-write
    STATUS  mandatory
    DESCRIPTION
           "This variable is used to send a constant yellow
            alarm signal at the frame level to the remote equipment.
            This object is used only for testing and debugging.
            This object is available for ATM T1/E1 only.
            Common Name: Forcing a Line FERF Signal"
    ::= { adnDsx1ConfigEntry  5 }

adnDsx1RemLoopbackTest OBJECT-TYPE
    SYNTAX  INTEGER {
    lineNoLoop(1),
    lineLoop(3)
}
    ACCESS  read-write
    STATUS  mandatory
    DESCRIPTION
           "This variable is used to force a loopback of
            the line of the remote equipment"
    ::= { adnDsx1ConfigEntry  6 }

adnDsx1LineHWStatus OBJECT-TYPE
    SYNTAX  INTEGER (-2147483648..2147483647)
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "This variable indicates the hardware
            status of a line.
            The given status value corresponds to
            the last test initiated by setting the
            nominator ifTestType with adnIfTest-
            TypeLineHW test identifier.
            If the result of the test, given by the
            nominator ifTestResult, is different
            than 'success' or 'failed', the given  
            status value is not significant.
            It is a bit map represented as a sum;
            therefore, it can represent multiple
            defects simultaneously.
            The adnDsx1LineHWNoDefect bit should
            be set only if no other bit is set.
            If the status is not significant, all
            the bits are reset.
            
            The bit positions are: 
               1   lineHWNoDefect
               2   BGPHT chip failure
               4   not used
               8   CBD chip failure on line side
               8   framer chip failure on the input side
               16  framer chip failure on line side"
    ::= { adnDsx1ConfigEntry  7 }

adnDsx1StatType OBJECT-TYPE
    SYNTAX  INTEGER {
    noStatistics(1),
    ansi(2),
    itu(3)
}
    ACCESS  read-write
    STATUS  mandatory
    DESCRIPTION
           "The field displays the standard used for computing
         the statistics counters.
            noStatistics  : no statistics are available on the 
                                 ds1/E1 interface
            ansi          : ANSI T1.231 statistics are activated
            itu           : ITU G.826   statistics are activated"
    ::= { adnDsx1ConfigEntry  8 }

adnDsx1LinePerfStatus OBJECT-TYPE
    SYNTAX  INTEGER (-2147483648..2147483647)
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "This variable indicates the performance status of the 
           interface.  It is a bit map represented as a sum; 
           therefore, it can represent multiple status
           simultaneously.
           - The NoDefect bit should be set only if no other 
             bit is set.
           - The UnavailablePeriod indicates that the
             interface has entered an unavailable period.
           - The CurrentCrossingThreshold bit indicates that
             one of the current statistics counter has
             reached its threshold.
           - The DayIntervalCrossingThreshold bit indicates that
             one of the Day Interval statistics counter has
             reached its threshold.

           The bit positions are:
      
             1     noDefect
             2     unavailablePeriod
             4     currentCrossingThreshold
             8     dayIntervalCrossingThreshold"
    ::= { adnDsx1ConfigEntry  9 }

adnDsx1CounterTable OBJECT-TYPE
    SYNTAX  SEQUENCE OF AdnDsx1CounterEntry
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
           "The DS1/E1 ADN Counter table."
    ::= { adnDs1  2 }

adnDsx1CounterEntry OBJECT-TYPE
    SYNTAX  AdnDsx1CounterEntry
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
           "An entry in the DS1/E1 ADN Counter table ."
    INDEX   { dsx1LineIndex }
    ::= { adnDsx1CounterTable  1 }

AdnDsx1CounterEntry ::=
    SEQUENCE {
        adnDsx1LosCounter
            Counter,

        adnDsx1AisCounter
            Counter,

        adnDsx1FerfCounter
            Counter,

        adnDsx1FeCounter
            Counter,

        adnDsx1LofCounter
            Counter,

        adnDsx1FebeCounter
            Counter,

        adnDsx1PcvCounter
            Counter,

        adnDsx1Ts16AisCounter
            Counter,

        adnDsx1RsaCounter
            Counter,

        adnDsx1LsmaCounter
            Counter,

        adnDsx1LcvCounter
            Counter,

        adnDsx1LcmaCounter
            Counter,

        adnDsx1SefCounter
            Counter,

        adnDsx1ExzCounter
            Counter
    }

adnDsx1LosCounter OBJECT-TYPE
    SYNTAX  Counter	-- UNITS millisecond

    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "Duration of loss-of-signal (LOS).
                 Counter #0 of the ATM core, only 
                 available on ATM-T1, ATM-E1, CES-T1
                 and CES-E1 interfaces."
    ::= { adnDsx1CounterEntry  1 }

adnDsx1AisCounter OBJECT-TYPE
    SYNTAX  Counter	-- UNITS millisecond

    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "Duration of alarm indication signal 
                 (AIS).
                 Counter #2 of the ATM core, only 
                 available on ATM-T1, ATM-E1, CES-T1
                 and CES-E1 interfaces."
    ::= { adnDsx1CounterEntry  2 }

adnDsx1FerfCounter OBJECT-TYPE
    SYNTAX  Counter	-- UNITS millisecond

    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "Duration of far end received failure
                 (FERF) received from the remote 
                 equipment.
                 FERF is also called remote alarm
                 indication (RAI) or received yellow
                 alarm.
                 Counter #3 of the ATM core, only 
                 available on ATM-T1, ATM-E1, CES-T1
                 and CES-E1-Framed interfaces."
    ::= { adnDsx1CounterEntry  3 }

adnDsx1FeCounter OBJECT-TYPE
    SYNTAX  Counter
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "Number of frame bit errors (FE).
                 Counter #5 of the ATM core, only 
                 available on ATM-E1, CES-T1
                 and CES-E1-Framed interfaces."
    ::= { adnDsx1CounterEntry  4 }

adnDsx1LofCounter OBJECT-TYPE
    SYNTAX  Counter	-- UNITS millisecond

    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "Duration of loss-of-frame (LOF).
                 Counter #7 of the ATM core, only 
                 available on ATM-T1, ATM-E1, CES-T1
                 and CES-E1-Framed interfaces."
    ::= { adnDsx1CounterEntry  5 }

adnDsx1FebeCounter OBJECT-TYPE
    SYNTAX  Counter
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "Number of far end block errors (FEBE).
                 Counter #9 of the ATM core, only 
                 available on ATM-E1 and CES-E1-CRC
                 interfaces."
    ::= { adnDsx1CounterEntry  6 }

adnDsx1PcvCounter OBJECT-TYPE
    SYNTAX  Counter
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "Path Coding Violation (PCV) Error Event.
                 A Path Coding Violation error event is :
                 - a frame synchronization bit error in the T1-SF
                   and E1 no CRC formats,
                 - a CRC-6 error in the T1-ESF format,
                 - a CRC-4 error in the E1 CRC formats.

                 Counter #11 of the ATM core, only 
                 available on ATM-T1, ATM-E1, CES-T1
                 and CES-E1-CRC interfaces."
    ::= { adnDsx1CounterEntry  7 }

adnDsx1Ts16AisCounter OBJECT-TYPE
    SYNTAX  Counter	-- UNITS millisecond

    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "Duration of Alarm Indication Signal in
                 time slot 16 signalling channel (TS16AIS).
                 Counter #17 of the ATM core, only 
                 available on CES-E1-Multiframed interfaces."
    ::= { adnDsx1CounterEntry  8 }

adnDsx1RsaCounter OBJECT-TYPE
    SYNTAX  Counter	-- UNITS millisecond

    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "Duration of Remote Signalling Multiframe Alarm (RSA).
                 Counter #18 of the ATM core, only 
                 available on CES-E1-Multiframed interfaces."
    ::= { adnDsx1CounterEntry  9 }

adnDsx1LsmaCounter OBJECT-TYPE
    SYNTAX  Counter	-- UNITS millisecond

    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "Duration of Loss of Signalling Multiframe Alignment
                 (LSMA). Counter #19 of the ATM core, only 
