adnFrStatVCTable OBJECT-TYPE
    SYNTAX  SEQUENCE OF AdnFrStatVCEntry
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
           "This table constitutes the FRS (Frame Relay Subscriber)
         connection-level statistics record. None of the objects 
         in this table can ever be obtained via an SNMP GET 
         command. These objects are only visible in a statistics 
         record.  Most of these objects duplicate objects in other 
         tables. They are collected here to allow the statistics 
         record to be sent as a single table to conserve space."
    ::= { adnFrnetservMIB  7 }

adnFrStatVCEntry OBJECT-TYPE
    SYNTAX  AdnFrStatVCEntry
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
           "One instance of the FRS (Frame Relay Service) connection 
         statistics record"
    INDEX   { ifIndex,  frPVCEndptDLCIIndex }
    ::= { adnFrStatVCTable  1 }

AdnFrStatVCEntry ::=
    SEQUENCE {
        adnFrStatVCifName
            DisplayString,

        adnFrStatVCadnFrVCRemoteifindex
            INTEGER (-2147483648..2147483647),

        adnFrStatVCadnFrVCRemDlci
            INTEGER (-2147483648..2147483647),

        adnFrStatVCadnFrVCUniqueConnID
            INTEGER (-2147483648..2147483647),

        adnFrStatVCadnFrVCRemNodeID
            AdnRoutingID,

        adnFrStatVCfrLportNumPlan
            INTEGER,

        adnFrStatVCifPhysAddress
            PhysAddress,

        adnFrStatVCadnFrVCRemNumPlan
            INTEGER,

        adnFrStatVCadnFrVCRemPhysAddress
            PhysAddress,

        adnFrStatVCadnFrVCSourceDest
            INTEGER,

        adnFrStatVCadnFrVCNumRelinks
            INTEGER (-2147483648..2147483647),

        adnFrStatVCfrPVCEndptInCIR
            INTEGER (-2147483648..2147483647),

        adnFrStatVCadnFrVCInBc
            INTEGER (-2147483648..2147483647),

        adnFrStatVCadnFrVCInBcs
            INTEGER (-2147483648..2147483647),

        adnFrStatVCfrPVCEndptInBe
            INTEGER (-2147483648..2147483647),

        adnFrStatVCfrPVCEndptOutCIR
            INTEGER (-2147483648..2147483647),

        adnFrStatVCadnFrVCOutBc
            INTEGER (-2147483648..2147483647),

        adnFrStatVCadnFrVCOutBcs
            INTEGER (-2147483648..2147483647),

        adnFrStatVCfrPVCEndptOutBe
            INTEGER (-2147483648..2147483647),

        adnFrStatVCadnFrVCISR
            INTEGER (-2147483648..2147483647),

        adnFrStatVCadnFrVCUserOrNet
            INTEGER,

        adnFrStatVCfrAccountPVCSegmentSize
            INTEGER (-2147483648..2147483647),

        adnFrStatVCadnFrVCType
            INTEGER,

        adnFrStatVCadnNodeTime
            AdnNodeTimeTicks,

        adnFrStatVCadnFrVCAdminState
            INTEGER,

        adnFrStatVCadnFrVCOperState
            INTEGER,

        adnFrStatVCadnFrVCFrameIN
            INTEGER (-2147483648..2147483647),

        adnFrStatVCadnFrVCFrameOUT
            INTEGER (-2147483648..2147483647),

        adnFrStatVCadnFrVCSubFrameIN
            INTEGER (-2147483648..2147483647),

        adnFrStatVCadnFrVCSubFrameOUT
            INTEGER (-2147483648..2147483647),

        adnFrStatVCadnFrVCBeInDiscards
            INTEGER (-2147483648..2147483647),

        adnFrStatVCadnFrVCBcsInDiscards
            INTEGER (-2147483648..2147483647),

        adnFrStatVCadnFrVCBcInDiscards
            INTEGER (-2147483648..2147483647),

        adnFrStatVCadnFrVCBeOutDiscards
            INTEGER (-2147483648..2147483647),

        adnFrStatVCadnFrVCBcsOutDiscards
            INTEGER (-2147483648..2147483647),

        adnFrStatVCadnFrVCBcOutDiscards
            INTEGER (-2147483648..2147483647),

        adnFrStatVCadnFrVCDiscardLisIn
            INTEGER (-2147483648..2147483647),

        adnFrStatVCadnFrVCInCongsState
            AdnCongestLevels,

        adnFrStatVCfrPVCEndptDLCIIndex
            INTEGER (-2147483648..2147483647),

        adnFrStatVCadnFrVCNumNodes
            INTEGER (-2147483648..2147483647),

        adnFrStatVCfrPVCEndptInOctets
            INTEGER (-2147483648..2147483647),

        adnFrStatVCfrPVCEndptOutOctets
            INTEGER (-2147483648..2147483647),

        adnFrStatVCadnFrVCDescr
            DisplayString
    }

