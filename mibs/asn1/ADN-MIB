adnRs232 OBJECT IDENTIFIER ::= { adnTransmission  33 }

adnRs232InSigTable OBJECT-TYPE
    SYNTAX  SEQUENCE OF AdnRs232InSigEntry
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
           "A list of port input control signal entries,
           implemented and visible to the software on the port,
           and useful to monitor."
    ::= { adnRs232  1 }

adnRs232InSigEntry OBJECT-TYPE
    SYNTAX  AdnRs232InSigEntry
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
           "Input control signal status for a hardware port."
    INDEX   { adnRs232InSigPortIndex,  adnRs232InSigName }
    ::= { adnRs232InSigTable  1 }

AdnRs232InSigEntry ::=
    SEQUENCE {
        adnRs232InSigPortIndex
            InterfaceIndex,

        adnRs232InSigName
            INTEGER,

        adnRs232InSigState
            INTEGER,

        adnRs232InSigChanges
            Counter,

        adnRs232InSigDuration
            Counter
    }

adnRs232InSigPortIndex OBJECT-TYPE
    SYNTAX  InterfaceIndex
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The value of adnRs232PortIndex for the port to which
           this entry belongs."
    ::= { adnRs232InSigEntry  1 }

adnRs232InSigName OBJECT-TYPE
    SYNTAX  INTEGER {
    tm(1),
    lal(2),
    rdl(3),
    x21cont(4),
    x21ind(5),
    cts(6),
    dsr(7),
    dcd(8)
}
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "Identification of a hardware input signal, as follows:

               tm        Test Mode 
               lal       Local Loop 
               rdl       Remote Loop 
               x21cont   X.21 Control
               x21ind    X.21 Indication
               cts       Clear To Send
               dsr       Data Set Ready
               dcd       Received Line Signal Detector

            The cts, dsr and dcd signals are repeated from RFC 1659
            to provide an index to object adnRs232InSigDuration.
           "
    REFERENCE
           "Identification of a hardware input signal, as follows:

               tm        Test Mode 
               lal       Local Loop 
               rdl       Remote Loop 
               x21cont   X.21 Control
               x21ind    X.21 Indication
               cts       Clear To Send
               dsr       Data Set Ready
               dcd       Received Line Signal Detector

            The cts, dsr and dcd signals are repeated from RFC 1659
            to provide an index to object adnRs232InSigDuration.
           "
    ::= { adnRs232InSigEntry  2 }

adnRs232InSigState OBJECT-TYPE
    SYNTAX  INTEGER {
    none(1),
    on(2),
    off(3)
}
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The current input signal state."
    ::= { adnRs232InSigEntry  3 }

adnRs232InSigChanges OBJECT-TYPE
    SYNTAX  Counter
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The number of times the input signal has changed from
           'on' to 'off' or from 'off' to 'on'.
           'NoSuchInstance' is returned in case of the HBCEU board."
    ::= { adnRs232InSigEntry  4 }

adnRs232InSigDuration OBJECT-TYPE
    SYNTAX  Counter	-- UNITS milliseconds

    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The duration the input signal has been in the 'on'
            state for the tm(1), cts(6), dsr(7), dcd(8) signals
            of the HBCEU board.
            The duration the input signal has been in the 'off'
            state for the x21ind(5) signal of the HBCEU board."
    ::= { adnRs232InSigEntry  5 }

adnRs232OutSigTable OBJECT-TYPE
    SYNTAX  SEQUENCE OF AdnRs232OutSigEntry
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
           "A list of port output control signal entries,
           implemented and visible to the software on the port,
           and useful to monitor."
    ::= { adnRs232  2 }

adnRs232OutSigEntry OBJECT-TYPE
    SYNTAX  AdnRs232OutSigEntry
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
           "Output control signal status for a hardware port."
    INDEX   { adnRs232OutSigPortIndex,  adnRs232OutSigName }
    ::= { adnRs232OutSigTable  1 }

AdnRs232OutSigEntry ::=
    SEQUENCE {
        adnRs232OutSigPortIndex
            InterfaceIndex,

        adnRs232OutSigName
            INTEGER,

        adnRs232OutSigState
            INTEGER,

        adnRs232OutSigChanges
            Counter
    }

adnRs232OutSigPortIndex OBJECT-TYPE
    SYNTAX  InterfaceIndex
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The value of adnRs232PortIndex for the port to which
           this entry belongs."
    ::= { adnRs232OutSigEntry  1 }

adnRs232OutSigName OBJECT-TYPE
    SYNTAX  INTEGER {
    tm(1),
    lal(2),
    rdl(3),
    x21cont(4),
    x21ind(5),
    cts(6),
    dsr(7),
    dcd(8)
}
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "Identification of a hardware output signal, as follows:

               tm        Test Mode 
               lal       Local Loop 
               rdl       Remote Loop 
               x21cont   X.21 Control
               x21ind    X.21 Indication
               cts       Clear To Send
               dsr       Data Set Ready
               dcd       Received Line Signal Detector

            The cts, dsr and dcd signals are repeated from RFC 1659
            for consistency with object adnRs232InSigName.
            The cts, dsr and dcd signals aren't used in 
            adnRs232OutSigTable.
           "
    REFERENCE
           "Identification of a hardware output signal, as follows:

               tm        Test Mode 
               lal       Local Loop 
               rdl       Remote Loop 
               x21cont   X.21 Control
               x21ind    X.21 Indication
               cts       Clear To Send
               dsr       Data Set Ready
               dcd       Received Line Signal Detector

            The cts, dsr and dcd signals are repeated from RFC 1659
            for consistency with object adnRs232InSigName.
            The cts, dsr and dcd signals aren't used in 
            adnRs232OutSigTable.
           "
    ::= { adnRs232OutSigEntry  2 }

adnRs232OutSigState OBJECT-TYPE
    SYNTAX  INTEGER {
    none(1),
    on(2),
    off(3)
}
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The current output signal state."
    ::= { adnRs232OutSigEntry  3 }

adnRs232OutSigChanges OBJECT-TYPE
    SYNTAX  Counter
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The number of times the output signal has changed from
           'on' to 'off' or from 'off' to 'on'.
           'NoSuchInstance' is returned in case of the HBCEU board."
    ::= { adnRs232OutSigEntry  4 }

