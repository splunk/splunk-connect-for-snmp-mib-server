adnAtmBoardTraCounterEntry OBJECT-TYPE
    SYNTAX  AdnAtmBoardTraCounterEntry
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
           "An entry in the ATM boards traffic counter table."
    INDEX   { adnEquipType,  adnEquipIndex }
    ::= { adnAtmBoardTraCounterTable  1 }

AdnAtmBoardTraCounterEntry ::=
    SEQUENCE {
        adnAtmBoardTraHMP0ObsCounter
            Counter,

        adnAtmBoardTraHMP1ObsCounter
            Counter,

        adnAtmBoardTraFPTMICICounter
            Gauge,

        adnAtmBoardTraMOBSCDCounter
            Counter,

        adnAtmBoardTraLOBSCDCounter
            Counter,

        adnAtmBoardTraMOBEFCICounter
            Counter,

        adnAtmBoardTraLOBEFCICounter
            Counter,

        adnAtmBoardTraMOBTPDCounter
            Counter,

        adnAtmBoardTraLOBTPDCounter
            Counter,

        adnAtmBoardTraHOBObsCounter
            Counter,

        adnAtmBoardTraMOBObsCounter
            Counter,

        adnAtmBoardTraLOBObsCounter
            Counter
    }

