    SYNTAX  INTEGER (-2147483648..2147483647)
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The chassis number detecting the error.
        This nominator, which is read only, is  not  significant if
        adnAtmUniVcCconnectCauseError is equal to 'notSignificant'."
    ::= { adnAtmUniVcCconnectEntry  35 }

adnAtmUniVcCconnectLastSlot OBJECT-TYPE
    SYNTAX  INTEGER (-2147483648..2147483647)
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The slot number of the board detecting the error.
        This nominator, which is read only, is  not  significant if
        adnAtmUniVcCconnectCauseError is equal to 'notSignificant'."
    ::= { adnAtmUniVcCconnectEntry  36 }

adnAtmUniVcCconnectPvcDescr OBJECT-TYPE
    SYNTAX  DisplayString
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "This object contains the Permanent Virtual Connection (PVC)
        description inserted in each charging ticket generated  for
        this Virtual Channel (VC) cross-connection.
	This object is significant only if the type of the  Virtual
	Channel Link (VCL), specified  by adnAtmVclType, is  set to
	'pvc'."
    ::= { adnAtmUniVcCconnectEntry  37 }

adnAtmCniVcCconnectTable OBJECT-TYPE
    SYNTAX  SEQUENCE OF AdnAtmCniVcCconnectEntry
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
           "The  Virtual  Channel  (VC)  cross-connect  table  for  the
        inter-node ATM interfaces (Cell  alcatel  Network  protocol
        Interfaces (CNIs))."
    ::= { adnAtmMIBObjects  11 }

adnAtmCniVcCconnectEntry OBJECT-TYPE
    SYNTAX  AdnAtmCniVcCconnectEntry
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
           "An entry in  the  Cell  alcatel  Network protocol Interface
        (CNI) Virtual Channel (VC)  cross-connect  table. There  is
        one entry for each VC cross-connection on a CNI."
    INDEX   { ifIndex,  atmVclVpi,  atmVclVci,  adnAtmCniVcCconnectIndex }
    ::= { adnAtmCniVcCconnectTable  1 }

AdnAtmCniVcCconnectEntry ::=
    SEQUENCE {
        adnAtmCniVcCconnectIndex
            INTEGER,

        adnAtmCniVcCconnectOutIfIndex
            InterfaceIndex,

        adnAtmCniVcCconnectOutVpi
            INTEGER,

        adnAtmCniVcCconnectOutVci
            INTEGER,

        adnAtmCniVcCconnectOutCcnIndex
            INTEGER,

        adnAtmCniVcCconnectAdminStatus
            AdnAtmAdminStatus,

        adnAtmCniVcCconnectOperStatus
            AdnAtmCniOperStatus,

        adnAtmCniVcCconnectSourceDest
            AdnAtmCniSourceDest,

        adnAtmCniVcCconnectStatistics
            INTEGER,

        adnAtmCniVcCconnectDirectedPath
            AdnAtmDirectedPath
    }

adnAtmCniVcCconnectIndex OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
           "The index of the cross-connection for this Virtual  Channel
        Connection (VCC) on  this  interface. In  a  point-to-point
        cross-connection, the index equals 0."
    ::= { adnAtmCniVcCconnectEntry  1 }

adnAtmCniVcCconnectOutIfIndex OBJECT-TYPE
    SYNTAX  InterfaceIndex
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The ifIndex of the output ATM interface from this node."
    ::= { adnAtmCniVcCconnectEntry  2 }

adnAtmCniVcCconnectOutVpi OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The Virtual Path Identifier (VPI) of this connection on the
        output ATM interface from this  node. In  conjunction  with
        adnAtmCniVcCconnectOutIfIndex, this VPI  can  be  used  for
        path tracing."
    ::= { adnAtmCniVcCconnectEntry  3 }

adnAtmCniVcCconnectOutVci OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The Virtual Channel Identifier (VCI) of this node's  output
        interface. This cross-connection exits this node on another
        VCI. This     value     can      be      combined      with
        adnAtmCniVcCconnectOutIfIndex and adnAtmCniVcCconnectOutVpi
        and used for call tracing."
    ::= { adnAtmCniVcCconnectEntry  4 }

adnAtmCniVcCconnectOutCcnIndex OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The    cross-connection   index   of   the   Virtual   Path
        Identifier/Virtual Channel  Identifier  (VPI/VCI)  of  this
        node's output interface. This value can be  used  for  call
        tracing  if  combined  with  adnAtmCniVcCconnectOutIfIndex,
        adnAtmCniVcCconnectOutVpi, and adnAtmCniVcCconnectOutVci."
    ::= { adnAtmCniVcCconnectEntry  5 }

adnAtmCniVcCconnectAdminStatus OBJECT-TYPE
    SYNTAX  AdnAtmAdminStatus
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The current administrative status of this  Virtual  Channel
        (VC) cross-connection. The  'up' and 'down' states indicate
        whether   or   not   this   ATM   VC   cross-connection  is
        operational."
    ::= { adnAtmCniVcCconnectEntry  6 }

adnAtmCniVcCconnectOperStatus OBJECT-TYPE
    SYNTAX  AdnAtmCniOperStatus
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The current Virtual Channel Connection (VCC)  call  attempt
        state."
    ::= { adnAtmCniVcCconnectEntry  7 }

adnAtmCniVcCconnectSourceDest OBJECT-TYPE
    SYNTAX  AdnAtmCniSourceDest
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "Whether this is the  source  or  destination  side  of  the
        cross-connection."
    ::= { adnAtmCniVcCconnectEntry  8 }

adnAtmCniVcCconnectStatistics OBJECT-TYPE
    SYNTAX  INTEGER {
    enabled(1),
    disabled(2),
    useInterfaceValue(3)
}
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "Whether statistics are enabled  for  this  Virtual  Channel
        (VC) cross-connection.
        This object is significant only if the type of the  Virtual
        Channel Link (VCL), specified  by adnAtmVclType, is set  to
        'pvc'; otherwise, the  statistics  status  is  defined  per
        interface.
        If  set  to  'useInterfaceValue', the  value  defined   per
        interface is used.
        Common Name: Statistics"
    ::= { adnAtmCniVcCconnectEntry  9 }

adnAtmCniVcCconnectDirectedPath OBJECT-TYPE
    SYNTAX  AdnAtmDirectedPath
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "Whether  this  Virtual  Channel  Connection  (VCC)  uses  a
        directed path."
    ::= { adnAtmCniVcCconnectEntry  10 }

adnAtmVpcDirRoutTable OBJECT-TYPE
    SYNTAX  SEQUENCE OF AdnAtmVpcDirRoutEntry
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
           "This table contains  the  directed  path, used  to  set  up
        point-to-point permanent Virtual Path  Connections  (VPCs).
        A directed path is composed of a list  of  node  identifier
        and outgoing line  identifiers. This  table  is  configured
        only  in  the  origin  node  of  the  cross-connection. The
        path between the origin and the destination node  could  be
        partly or completely described by the list, but it is  only
        used   if   adnAtmUniVpCconnectDirectedPath   is   set   to
        'enabled'."
    ::= { adnAtmMIBObjects  12 }

adnAtmVpcDirRoutEntry OBJECT-TYPE
    SYNTAX  AdnAtmVpcDirRoutEntry
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
           "An entry in the adnAtmVpcDirRoutTable. There is  one  entry
        for each element of each list specifing a directed path."
    INDEX   { ifIndex,  atmVplVpi,  adnAtmUniVpCconnectIndex,  adnAtmVpcDirRoutIndex }
    ::= { adnAtmVpcDirRoutTable  1 }

AdnAtmVpcDirRoutEntry ::=
    SEQUENCE {
        adnAtmVpcDirRoutIndex
            INTEGER,

        adnAtmVpcDirRoutNodeId
            AdnRoutingID,

        adnAtmVpcDirRoutOutChassis
            INTEGER,

        adnAtmVpcDirRoutOutSlot
            INTEGER,

        adnAtmVpcDirRoutOutLine
            INTEGER (-2147483648..2147483647),

        adnAtmVpcDirRoutFlag
            AdnAtmDirRoutFlag,

        adnAtmVpcDirRoutNodeMacNum
            INTEGER (-2147483648..2147483647),

        adnAtmVpcDirRoutNodeAreaNum
            INTEGER (-2147483648..2147483647)
    }

adnAtmVpcDirRoutIndex OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
           "The index of an element in the list of  this  Virtual  Path
        Connection (VPC)."
    ::= { adnAtmVpcDirRoutEntry  1 }

adnAtmVpcDirRoutNodeId OBJECT-TYPE
    SYNTAX  AdnRoutingID
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The identifier of the next node to be  used  to  reach  the
        destination node, for this Virtual Path Connection (VPC)."
    ::= { adnAtmVpcDirRoutEntry  2 }

adnAtmVpcDirRoutOutChassis OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The chassis number, within the current node, supporting the
        line used   to   reach   the   next   node, specified    by
        adnAtmVpcDirRoutNodeId."
    ::= { adnAtmVpcDirRoutEntry  3 }

adnAtmVpcDirRoutOutSlot OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The slot number, within the  current  node, supporting  the
        line  used   to   reach   the   next   node, specified   by
        adnAtmVpcDirRoutNodeId."
    ::= { adnAtmVpcDirRoutEntry  4 }

adnAtmVpcDirRoutOutLine OBJECT-TYPE
    SYNTAX  INTEGER (-2147483648..2147483647)
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The line number, within the current node, used to reach the
        next node, specified by adnAtmVpcDirRoutNodeId."
    ::= { adnAtmVpcDirRoutEntry  5 }

adnAtmVpcDirRoutFlag OBJECT-TYPE
    SYNTAX  AdnAtmDirRoutFlag
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "Whether only the outgoing line specified must  be  used  to
        reach the next node or not."
    ::= { adnAtmVpcDirRoutEntry  6 }

adnAtmVpcDirRoutNodeMacNum OBJECT-TYPE
    SYNTAX  INTEGER (-2147483648..2147483647)
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The machine number part of adnAtmVpcDirRoutNodeID."
    ::= { adnAtmVpcDirRoutEntry  7 }

adnAtmVpcDirRoutNodeAreaNum OBJECT-TYPE
    SYNTAX  INTEGER (-2147483648..2147483647)
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The area number part of adnAtmVpcDirRoutNodeID."
    ::= { adnAtmVpcDirRoutEntry  8 }

adnAtmVccDirRoutTable OBJECT-TYPE
    SYNTAX  SEQUENCE OF AdnAtmVccDirRoutEntry
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
           "This table contains  the  directed  path, used  to  set  up
        point-to-point permanent Virtual Channel Connections (VPCs).
        A directed path is composed of a list  of  node  identifier
        and outgoing line  identifiers. This  table  is  configured
        only  in  the  origin  node  of  the  cross-connection. The
        path between the origin and the destination node  could  be
