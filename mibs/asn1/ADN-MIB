    SYNTAX  AdnSonetIntervalEntry
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
           "An entry in the ADN SONET/SDH Interval table."
    INDEX   { ifIndex,  adnsonetIntervalNumber }
    ::= { adnsonetIntervalTable  1 }

AdnSonetIntervalEntry ::=
    SEQUENCE {
        adnsonetIntervalNumber
            INTEGER (-2147483648..2147483647),

        adnsonetIntervalESs
            Gauge,

        adnsonetIntervalSESs
            Gauge,

        adnsonetIntervalBBEs
            Gauge,

        adnsonetIntervalUASs
            Gauge
    }

adnsonetIntervalNumber OBJECT-TYPE
    SYNTAX  INTEGER (-2147483648..2147483647)
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
           "A number which identifies the interval for 
                 which the set of statistics is available.
                 The previous 24-hour period is divided into
                 96 15-minute intervals.
                 The interval identified by 1 is the most 
                 recently completed 15-minute interval and the 
                 interval identified by N is the interval
                 immediately preceding the one identified
                 by N-1."
    ::= { adnsonetIntervalEntry  1 }

adnsonetIntervalESs OBJECT-TYPE
    SYNTAX  Gauge
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "Number of errored seconds (ES) encountered
                during a particular 15-minute interval."
    ::= { adnsonetIntervalEntry  2 }

adnsonetIntervalSESs OBJECT-TYPE
    SYNTAX  Gauge
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "Number of severely errored seconds (SES) 
                encountered during a particular 15-minute 
                interval."
    ::= { adnsonetIntervalEntry  3 }

adnsonetIntervalBBEs OBJECT-TYPE
    SYNTAX  Gauge
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "Number of background errored blocks (BBE) 
                encountered during a particular 15-minute 
                interval."
    ::= { adnsonetIntervalEntry  4 }

adnsonetIntervalUASs OBJECT-TYPE
    SYNTAX  Gauge
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "Number of unavailable seconds (UAS) 
                encountered in a particular 15-minute interval."
    ::= { adnsonetIntervalEntry  5 }

