adnIpfwdsMIB OBJECT IDENTIFIER ::= { adnProprietary  24 }

adnIpIfStackTable OBJECT-TYPE
    SYNTAX  SEQUENCE OF AdnIpIfStackEntry
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
           "The table containing information on the relationship
               between an IP interface and its correponding AAL5 connections
	       (or indices). Each of these multiplexed entities correspond
	       to a conceptual row in the ifTable."
    ::= { adnIpfwdsMIB  1 }

adnIpIfStackEntry OBJECT-TYPE
    SYNTAX  AdnIpIfStackEntry
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
           "Information on a particular relationship between
		IP interface and its corresponding AAL5 connections.
		Each of the multiplexed ifIndex has a conceptual
		row in the ifTable."
    INDEX   { adnIpIfStackHigherLayer,  adnIpIfStackLowerLayer }
    ::= { adnIpIfStackTable  1 }

AdnIpIfStackEntry ::=
    SEQUENCE {
        adnIpIfStackHigherLayer
            INTEGER (-2147483648..2147483647),

        adnIpIfStackLowerLayer
            INTEGER (-2147483648..2147483647),

        adnIpIfStackStatus
            RowStatus
    }

adnIpIfStackHigherLayer OBJECT-TYPE
    SYNTAX  INTEGER (-2147483648..2147483647)
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
           "The value of ifIndex corresponding to the IP interface
		which runs on 'top of' an AAL5 connection identified
		by a corresponding instance of adnIpIfStackLowerLayer."
    ::= { adnIpIfStackEntry  1 }

adnIpIfStackLowerLayer OBJECT-TYPE
    SYNTAX  INTEGER (-2147483648..2147483647)
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
           "The value of ifIndex corresponding to an AAL5 connection
		which runs 'below' the IP interface identified by the
		correponding instance of adnIpIfStackHigherLayer."
    ::= { adnIpIfStackEntry  2 }

adnIpIfStackStatus OBJECT-TYPE
    SYNTAX  RowStatus
    ACCESS  read-write
    STATUS  mandatory
    DESCRIPTION
           "The status of the relationship between two sub-
               layers.

               Changing the value of this object from 'active' to
               'notInService' or 'destroy' will likely have
               consequences up and down a connection. "
    ::= { adnIpIfStackEntry  3 }

