    DESCRIPTION
           "Exit from frame relay data discarded at the channel
            level because of network congestion."
    ::= 8

adnMIBTrapVarsEquipStatusText OBJECT-TYPE
    SYNTAX  DisplayString
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
           "If present in an adnEquipStatusChange trap, this object
             contains diagnostic text describing the reason for the
             equipment status change."
    ::= { adnTrapVars  1 }

adnTrapCategory OBJECT-TYPE
    SYNTAX  INTEGER {
    crucial(1),
    administrative(2),
    critical(3),
    major(4),
    minor(5),
    shtr(6),
    diagnostic(7),
    cleared(8)
}
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
           "For the trap currently being sent, category of the trap
         that occurs as a variable-binding of an SNMP trap PDU."
    ::= { adnTrapVars  2 }

adnTrapFrVCDLCIIndex OBJECT-TYPE
    SYNTAX  INTEGER (-2147483648..2147483647)
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
           "This object defines the index of the DLCI of the local
        end of the VC."
    ::= { adnTrapVars  3 }

