    ::= { adnAtmMIBObjects  4 }

adnAtmSaalEntry OBJECT-TYPE
    SYNTAX  AdnAtmSaalEntry
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
           "An entry in the adnAtmSaalTable. There  is  one  entry  for
        each ATM Adaptation Layer  (AAL)  connection  on  an    ATM
        interface."
    INDEX   { ifIndex,  adnAtmSaalVpi,  adnAtmSaalVci }
    ::= { adnAtmSaalTable  1 }

AdnAtmSaalEntry ::=
    SEQUENCE {
        adnAtmSaalVpi
            INTEGER,

        adnAtmSaalVci
            INTEGER,

        adnAtmSaalConnectType
            INTEGER,

        adnAtmSaalBandwidth
            INTEGER,

        adnAtmSaalMaxCC
            INTEGER,

        adnAtmSaalMaxPD
            INTEGER,

        adnAtmSaalMaxSTAT
            INTEGER,

        adnAtmSaalCCTimer
            INTEGER,

        adnAtmSaalNoRespTimer
            INTEGER,

        adnAtmSaalPollTimer
            INTEGER,

        adnAtmSaalKeepAliveTimer
            INTEGER,

        adnAtmSaalIdleTimer
            INTEGER,

        adnAtmSaalMaxLength
            INTEGER,

        adnAtmSaalAdminStatus
            AdnAtmAdminStatus,

        adnAtmSaalOperStatus
            AdnAtmOperStatus
    }

adnAtmSaalVpi OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
           "The Virtual Path Identifier  (VPI)  value  of  the  Virtual
        Channel   Connection  (VCC)   for   this   Signalling   ATM
        Adaptation Layer (SAAL) connection."
    ::= { adnAtmSaalEntry  1 }

adnAtmSaalVci OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
           "The Virtual Channel Identifier (VCI) value of  the  Virtual
        Channel Connection (VCC) for this Signalling ATM Adaptation
        Layer (SAAL) connection."
    ::= { adnAtmSaalEntry  2 }

adnAtmSaalConnectType OBJECT-TYPE
    SYNTAX  INTEGER {
    other(1),
    signalling(2),
    routing(3)
}
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The type of the ATM Adaptation Layer (AAL) connection."
    ::= { adnAtmSaalEntry  3 }

adnAtmSaalBandwidth OBJECT-TYPE
    SYNTAX  INTEGER	-- UNITS bits/sec

    ACCESS  read-only
    STATUS  deprecated
    DESCRIPTION
           "The bandwidth  of  the  Virtual  Channel  Connection  (VCC)
        supporting this ATM Adaptation Layer (AAL) connection."
    ::= { adnAtmSaalEntry  4 }

adnAtmSaalMaxCC OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The maximum number of times  an  unacknowledged  Connection
        Control (CC) Protocol Data Unit (PDU)  can  be  resent  for
        this Signalling ATM Adaptation Layer (SAAL) connection. The
        connection is aborted when this value is exceeded."
    REFERENCE
           "The maximum number of times  an  unacknowledged  Connection
        Control (CC) Protocol Data Unit (PDU)  can  be  resent  for
        this Signalling ATM Adaptation Layer (SAAL) connection. The
        connection is aborted when this value is exceeded."
    ::= { adnAtmSaalEntry  5 }

adnAtmSaalMaxPD OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The maximum number of Sequenced  Data  (SD)  Protocol  Data
        Units (PDUs) that can be sent before a polling PDU for this
        Signalling ATM Adaptation Layer (SAAL) connection."
    REFERENCE
           "The maximum number of Sequenced  Data  (SD)  Protocol  Data
        Units (PDUs) that can be sent before a polling PDU for this
        Signalling ATM Adaptation Layer (SAAL) connection."
    ::= { adnAtmSaalEntry  6 }

adnAtmSaalMaxSTAT OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The maximum number of status parameters that can be sent in
        a   STATus   (STAT)  Protocol  Data  Unit  (PDU)  for  this
        Signalling  ATM  Adaptation  Layer  (SAAL)  connection. The
        value must be an odd number."
    REFERENCE
           "The maximum number of status parameters that can be sent in
        a   STATus   (STAT)  Protocol  Data  Unit  (PDU)  for  this
        Signalling  ATM  Adaptation  Layer  (SAAL)  connection. The
        value must be an odd number."
    ::= { adnAtmSaalEntry  7 }

adnAtmSaalCCTimer OBJECT-TYPE
    SYNTAX  INTEGER	-- UNITS milliseconds

    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The length (in milliseconds) of the Connection Control (CC)
        Protocol Data Unit (PDU)  timer  for  this  Signalling  ATM
        Adaptation Layer (SAAL) connection. If this timer  expires,
        the CC PDU is resent."
    REFERENCE
           "The length (in milliseconds) of the Connection Control (CC)
        Protocol Data Unit (PDU)  timer  for  this  Signalling  ATM
        Adaptation Layer (SAAL) connection. If this timer  expires,
        the CC PDU is resent."
    ::= { adnAtmSaalEntry  8 }

adnAtmSaalNoRespTimer OBJECT-TYPE
    SYNTAX  INTEGER	-- UNITS seconds

    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The  time  interval (in  seconds)  between  STATtus  (STAT)
        Protocol Data Units (PDUs) received for this Signalling ATM
        Adaptation Layer (SAAL) connection. If this timer  expires,
        the connection is aborted."
    REFERENCE
           "The  time  interval (in  seconds)  between  STATtus  (STAT)
        Protocol Data Units (PDUs) received for this Signalling ATM
        Adaptation Layer (SAAL) connection. If this timer  expires,
        the connection is aborted."
    ::= { adnAtmSaalEntry  9 }

adnAtmSaalPollTimer OBJECT-TYPE
    SYNTAX  INTEGER	-- UNITS milliseconds

    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The time interval (in milliseconds)  between  transmissions
        of POLLing (POLL) Protocol Data  Units  (PDUs)  during  the
        data transfer phase  for  this  Signalling  ATM  Adaptation
        Layer (SAAL) connection. If this timer expires, a  POLL PDU
        is sent."
    REFERENCE
           "The time interval (in milliseconds)  between  transmissions
        of POLLing (POLL) Protocol Data  Units  (PDUs)  during  the
        data transfer phase  for  this  Signalling  ATM  Adaptation
        Layer (SAAL) connection. If this timer expires, a  POLL PDU
        is sent."
    ::= { adnAtmSaalEntry  10 }

adnAtmSaalKeepAliveTimer OBJECT-TYPE
    SYNTAX  INTEGER	-- UNITS milliseconds

    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The amount of time (in milliseconds) allocated for  waiting
        for the STATus (STAT) Protocol Data Unit (PDU) solicited by
        the final POLLing (POLL) PDU when  activity  ends  on  this
        Signalling ATM Adaptation Layer (SAAL) connection."
    REFERENCE
           "The amount of time (in milliseconds) allocated for  waiting
        for the STATus (STAT) Protocol Data Unit (PDU) solicited by
        the final POLLing (POLL) PDU when  activity  ends  on  this
        Signalling ATM Adaptation Layer (SAAL) connection."
    ::= { adnAtmSaalEntry  11 }

adnAtmSaalIdleTimer OBJECT-TYPE
    SYNTAX  INTEGER	-- UNITS seconds

    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The time interval (in  seconds)  between  transmissions  of
        POLLing   (POLL)  Protocol  Data  Units  (PDUs)  when   the
        connection is no longer in the data transfer phase."
    REFERENCE
           "The time interval (in  seconds)  between  transmissions  of
        POLLing   (POLL)  Protocol  Data  Units  (PDUs)  when   the
        connection is no longer in the data transfer phase."
    ::= { adnAtmSaalEntry  12 }

adnAtmSaalMaxLength OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The  maximum  data  length  of  the  information  field  in
        Protocol   Data  Units  (PDUs)  for  this  Signalling   ATM
        Adaptation Layer (SAAL) connection."
    ::= { adnAtmSaalEntry  13 }

adnAtmSaalAdminStatus OBJECT-TYPE
    SYNTAX  AdnAtmAdminStatus
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The current administrative status of  this  Signalling  ATM
        Adaptation Layer (SAAL) connection."
    ::= { adnAtmSaalEntry  14 }

adnAtmSaalOperStatus OBJECT-TYPE
    SYNTAX  AdnAtmOperStatus
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The current  operational  status  of  this  Signalling  ATM
        Adaptation Layer (SAAL) connection."
    ::= { adnAtmSaalEntry  15 }

adnAtmSigTable OBJECT-TYPE
    SYNTAX  SEQUENCE OF AdnAtmSigEntry
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
           "This table describes the signalling entity  controlling  an
        ATM interface."
    ::= { adnAtmMIBObjects  5 }

adnAtmSigEntry OBJECT-TYPE
    SYNTAX  AdnAtmSigEntry
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
           "An entry in the adnAtmSigTable. There is one entry for each
        Virtual  Channel  Connection  (VCC)  controlling   an   ATM
        interface."
    INDEX   { ifIndex,  adnAtmSigVpi,  adnAtmSigVci }
    ::= { adnAtmSigTable  1 }

AdnAtmSigEntry ::=
    SEQUENCE {
        adnAtmSigVpi
            INTEGER,

        adnAtmSigVci
            INTEGER,

        adnAtmSigProtocol
            INTEGER,

        adnAtmSigT301
            INTEGER,

        adnAtmSigT302
            INTEGER,

        adnAtmSigT303
            INTEGER,

        adnAtmSigT304
            INTEGER,

        adnAtmSigT306
            INTEGER,

        adnAtmSigT308
            INTEGER,

        adnAtmSigT309
            INTEGER,

        adnAtmSigT310
            INTEGER,

        adnAtmSigT313
            INTEGER,

        adnAtmSigT316
            INTEGER,

        adnAtmSigT317
            INTEGER,

        adnAtmSigT322
            INTEGER,

        adnAtmSigT398
            INTEGER,

        adnAtmSigT399
            INTEGER,

        adnAtmSigT331
            INTEGER,

        adnAtmSigT397
            INTEGER,

        adnAtmSigUserNetFlag
            INTEGER,

        adnAtmSigCallProc
            AdnEnableDisabled,

        adnAtmSigBrSendComp
            AdnEnableDisabled,

        adnAtmSigSetupScreening
            INTEGER,

        adnAtmSigDdi
            AdnEnableDisabled,

        adnAtmSigMsn
            AdnEnableDisabled,

        adnAtmSigSvccReconnect
            AdnEnableDisabled,

        adnAtmSigCallBarring
            INTEGER,

        adnAtmSigClip
            INTEGER,

        adnAtmSigClir
            AdnAtmSigPIUpdate,

        adnAtmSigColp
            INTEGER,

        adnAtmSigColr
            AdnAtmSigPIUpdate,

        adnAtmSigSub
            AdnEnableDisabled,

        adnAtmSigUus
            AdnEnableDisabled,

        adnAtmSigVpiVciAlloc
            AdnEnableDisabled,

        adnAtmSigCniCheckingState
            INTEGER,

        adnAtmSigSetupAddr
            INTEGER,

        adnAtmSigConnectScreening
            INTEGER
    }

adnAtmSigVpi OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
           "The value of the Virtual  Path  Identifier  (VPI)  for  the
        Virtual Channel Connection (VCC) supporting this signalling
        entity."
    ::= { adnAtmSigEntry  1 }

adnAtmSigVci OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
           "The value of the Virtual Channel Identifier (VCI)  for  the
        Virtual Channel Connection (VCC) supporting this signalling
        entity."
    ::= { adnAtmSigEntry  2 }

adnAtmSigProtocol OBJECT-TYPE
    SYNTAX  INTEGER {
    other(1),
    q2931(2),
    atmfv30(3),
    atmfv31(4),
    canpv10(5),
    atmfv40(6),
    iisp(7),
    canpv11(8),
    canpv20(9)
}
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The signalling protocol type."
    REFERENCE
           "The signalling protocol type."
    ::= { adnAtmSigEntry  3 }

adnAtmSigT301 OBJECT-TYPE
    SYNTAX  INTEGER	-- UNITS seconds

    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The length (in seconds) of the timer started  upon  receipt
        of an Alerting message. This timer is stopped upon  receipt
        of  a  Connect   message. When   the   timer   expires, the
        connection is released."
    REFERENCE
           "The length (in seconds) of the timer started  upon  receipt
        of an Alerting message. This timer is stopped upon  receipt
        of  a  Connect   message. When   the   timer   expires, the
        connection is released."
    ::= { adnAtmSigEntry  4 }

adnAtmSigT302 OBJECT-TYPE
    SYNTAX  INTEGER	-- UNITS seconds

    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "This timer, is not implemented.

        The length  (in  seconds)  of  the   timer   started   upon
        transmission of a Setup acknowledge message  and  restarted
        on the receipt of any Information message not containing  a
        sending complete indication. This  timer  is  stopped  upon
        receipt  of  any  of  the  following  messages: Information
        message containing a sending complete indication, Alerting,
        or Connect request message. When the timer expires, a  Call
        proceeding, Alerting  or  Connect   message  is    sent, as
        appropriate if sufficient information has been received."
    REFERENCE
           "This timer, is not implemented.

        The length  (in  seconds)  of  the   timer   started   upon
        transmission of a Setup acknowledge message  and  restarted
        on the receipt of any Information message not containing  a
        sending complete indication. This  timer  is  stopped  upon
        receipt  of  any  of  the  following  messages: Information
        message containing a sending complete indication, Alerting,
        or Connect request message. When the timer expires, a  Call
        proceeding, Alerting  or  Connect   message  is    sent, as
        appropriate if sufficient information has been received."
    ::= { adnAtmSigEntry  5 }

adnAtmSigT303 OBJECT-TYPE
    SYNTAX  INTEGER	-- UNITS seconds

    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The  length (in  seconds)  of  the   timer   started   upon
        transmission of a Setup message. The timer is stopped  upon
        receipt  of  any  of  the   following   messages: Alerting,
        Connect, Call Proceeding, or  Release  Complete. The  first
        time this timer expires, the Setup message  is  resent. The
        second time it expires, the connection is cleared."
    REFERENCE
           "The  length (in  seconds)  of  the   timer   started   upon
        transmission of a Setup message. The timer is stopped  upon
        receipt  of  any  of  the   following   messages: Alerting,
        Connect, Call Proceeding, or  Release  Complete. The  first
        time this timer expires, the Setup message  is  resent. The
        second time it expires, the connection is cleared."
    ::= { adnAtmSigEntry  6 }

adnAtmSigT304 OBJECT-TYPE
    SYNTAX  INTEGER	-- UNITS seconds

    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "This timer is not implemented.

        The length (in seconds) of the timer started  upon  receipt
        of a Setup acknowledge  message  and  restarted  when  each
        Information message is sent. This  timer  is  stopped  upon
        receipt  of  an   Alerting, Connect, or   Call   proceeding
        message. When  the   timer   expires, the   connection   is
        released."
    REFERENCE
           "This timer is not implemented.

        The length (in seconds) of the timer started  upon  receipt
        of a Setup acknowledge  message  and  restarted  when  each
        Information message is sent. This  timer  is  stopped  upon
        receipt  of  an   Alerting, Connect, or   Call   proceeding
        message. When  the   timer   expires, the   connection   is
        released."
    ::= { adnAtmSigEntry  7 }

adnAtmSigT306 OBJECT-TYPE
    SYNTAX  INTEGER	-- UNITS seconds

    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "This timer is not implemented.
  
        The  length  (in  seconds)  of  the  timer   started   upon
        transmission of a Release message. This  timer  is  stopped
        upon receipt of a Release complete message. When this timer
        expires, a Release complete message is sent."
    REFERENCE
           "This timer is not implemented.
  
        The  length  (in  seconds)  of  the  timer   started   upon
        transmission of a Release message. This  timer  is  stopped
        upon receipt of a Release complete message. When this timer
        expires, a Release complete message is sent."
    ::= { adnAtmSigEntry  8 }

adnAtmSigT308 OBJECT-TYPE
    SYNTAX  INTEGER	-- UNITS seconds

    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The  length  (in seconds)  of  the   timer   started   upon
        transmission of a Release message.  The  timer  is  stopped
        upon receipt of a Release or Release complete  message. The
        first time  this  timer  expires, the  Release  message  is
        resent.  The second  time  it  expires, the  connection  is
        considered as cleared."
    REFERENCE
           "The  length  (in seconds)  of  the   timer   started   upon
        transmission of a Release message.  The  timer  is  stopped
        upon receipt of a Release or Release complete  message. The
        first time  this  timer  expires, the  Release  message  is
        resent.  The second  time  it  expires, the  connection  is
        considered as cleared."
    ::= { adnAtmSigEntry  9 }

adnAtmSigT309 OBJECT-TYPE
    SYNTAX  INTEGER	-- UNITS seconds

    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The length (in seconds) of the timer started  upon  receipt
        of a Signalling ATM Adaptation Layer  (SAAL)  disconnection
        indication. The timer is stopped upon  receipt  of  a  SAAL
        reconnection   indication. If   the   timer   expires,  all
        connections carried  by  the  faulty  SAAL  connection  are
        released."
    REFERENCE
           "The length (in seconds) of the timer started  upon  receipt
        of a Signalling ATM Adaptation Layer  (SAAL)  disconnection
        indication. The timer is stopped upon  receipt  of  a  SAAL
        reconnection   indication. If   the   timer   expires,  all
        connections carried  by  the  faulty  SAAL  connection  are
        released."
    ::= { adnAtmSigEntry  10 }

adnAtmSigT310 OBJECT-TYPE
    SYNTAX  INTEGER	-- UNITS seconds

    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The length (in seconds) of the timer started  upon  receipt
        of a Call proceeding message.  The timer  is  stopped  upon
        receipt of a Connect  or  Release  message.  If  the  timer
        expires, the connection is released."
    REFERENCE
           "The length (in seconds) of the timer started  upon  receipt
        of a Call proceeding message.  The timer  is  stopped  upon
        receipt of a Connect  or  Release  message.  If  the  timer
        expires, the connection is released."
    ::= { adnAtmSigEntry  11 }

adnAtmSigT313 OBJECT-TYPE
    SYNTAX  INTEGER	-- UNITS seconds

    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The  length  (in  seconds)  of  the  timer   started   upon
        transmission of a Connect message.  The  timer  is  stopped
        upon receipt of  a  Connect  acknowledge  message.  If  the
        timer expires, the connection is  cleared.  This  timer  is
        only applicable on the user side of the interface."
    REFERENCE
           "The  length  (in  seconds)  of  the  timer   started   upon
        transmission of a Connect message.  The  timer  is  stopped
        upon receipt of  a  Connect  acknowledge  message.  If  the
        timer expires, the connection is  cleared.  This  timer  is
        only applicable on the user side of the interface."
    ::= { adnAtmSigEntry  12 }

adnAtmSigT316 OBJECT-TYPE
    SYNTAX  INTEGER	-- UNITS seconds

    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The  length  (in  seconds)  of  the  timer   started   upon
        transmission of a Restart  message. The  timer  is  stopped
        upon receipt of a Restart Acknowledge message. If the timer
        expires, the Restart message may be resent several times."
    REFERENCE
           "The  length  (in  seconds)  of  the  timer   started   upon
        transmission of a Restart  message. The  timer  is  stopped
        upon receipt of a Restart Acknowledge message. If the timer
        expires, the Restart message may be resent several times."
    ::= { adnAtmSigEntry  13 }

adnAtmSigT317 OBJECT-TYPE
    SYNTAX  INTEGER	-- UNITS seconds

    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The length (in seconds) of the timer started  upon  receipt
        of  a  Restart  message. The  timer  is  stopped  when  the
        internal clearing of called references is completed. If the
        timer expires, a maintenance message is sent."
    REFERENCE
           "The length (in seconds) of the timer started  upon  receipt
        of  a  Restart  message. The  timer  is  stopped  when  the
        internal clearing of called references is completed. If the
        timer expires, a maintenance message is sent."
    ::= { adnAtmSigEntry  14 }

adnAtmSigT322 OBJECT-TYPE
    SYNTAX  INTEGER	-- UNITS seconds

    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The  length  (in seconds)  of  the  timer   started    upon
        transmission of a  Status  enquiry  message. The  timer  is
        stopped  upon  receipt  of  a  Status, Release, or  Release
        complete message. If the timer expires, the Status  enquiry
        message may be resent several times."
    REFERENCE
           "The  length  (in seconds)  of  the  timer   started    upon
        transmission of a  Status  enquiry  message. The  timer  is
        stopped  upon  receipt  of  a  Status, Release, or  Release
        complete message. If the timer expires, the Status  enquiry
        message may be resent several times."
    ::= { adnAtmSigEntry  15 }

adnAtmSigT398 OBJECT-TYPE
    SYNTAX  INTEGER	-- UNITS seconds

    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The  length  (in  seconds)  of  the  timer  started    upon
        transmission of a Drop party message. The timer is  stopped
        upon receipt of a Drop party acknowledge, Add party reject,
        or Release message.  If the timer  expires, the  connection
        is considered as cleared."
    REFERENCE
           "The  length  (in  seconds)  of  the  timer  started    upon
        transmission of a Drop party message. The timer is  stopped
        upon receipt of a Drop party acknowledge, Add party reject,
        or Release message.  If the timer  expires, the  connection
        is considered as cleared."
    ::= { adnAtmSigEntry  16 }

adnAtmSigT399 OBJECT-TYPE
    SYNTAX  INTEGER	-- UNITS seconds

    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The  length  (in  seconds)  of  the  timer  started    upon
        transmission of an Add party message. The timer is  stopped
        upon receipt of an Add party acknowledge, Add party reject,
        or Release message. If the timer expires, the connection is
        cleared."
    REFERENCE
           "The  length  (in  seconds)  of  the  timer  started    upon
        transmission of an Add party message. The timer is  stopped
        upon receipt of an Add party acknowledge, Add party reject,
        or Release message. If the timer expires, the connection is
        cleared."
    ::= { adnAtmSigEntry  17 }

adnAtmSigT331 OBJECT-TYPE
    SYNTAX  INTEGER	-- UNITS seconds

    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "This timer is not implemented.

        The  length  (in  seconds)  of  the  timer  started    upon
        transmission of an Leaf setup request message. The timer is
        stopped upon receipt of a Setup, Add party, or  Leaf  setup
        failure message.  The first time  this  timer  expires, the
        leaf may  optionally  retransmit  the  Leaf  setup  request
        message. The second time (or the first time if  the  resent
        is not chosen) it expires, the connection is cleared."
    REFERENCE
           "This timer is not implemented.

        The  length  (in  seconds)  of  the  timer  started    upon
        transmission of an Leaf setup request message. The timer is
        stopped upon receipt of a Setup, Add party, or  Leaf  setup
        failure message.  The first time  this  timer  expires, the
        leaf may  optionally  retransmit  the  Leaf  setup  request
        message. The second time (or the first time if  the  resent
        is not chosen) it expires, the connection is cleared."
    ::= { adnAtmSigEntry  18 }

adnAtmSigT397 OBJECT-TYPE
    SYNTAX  INTEGER	-- UNITS seconds

    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "This timer is not implemented.
  
        The length (in seconds) of the timer started  upon  receipt
        of an Alerting, or Add party alerting  message. This  timer
        is stopped upon receipt of an Alerting acknowledge message.
        When the timer expires, the connection is released."
    REFERENCE
           "This timer is not implemented.
  
        The length (in seconds) of the timer started  upon  receipt
        of an Alerting, or Add party alerting  message. This  timer
        is stopped upon receipt of an Alerting acknowledge message.
        When the timer expires, the connection is released."
    ::= { adnAtmSigEntry  19 }

adnAtmSigUserNetFlag OBJECT-TYPE
    SYNTAX  INTEGER {
    user(1),
    network(2)
}
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "Whether the interface follows the user or network  side  of
        the protocol recommendations.
        Common Names: User or Network Side,
                      User/Network"
    ::= { adnAtmSigEntry  20 }

