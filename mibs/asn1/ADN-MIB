            INTEGER (-2147483648..2147483647),

        adnCMBfvcadnFrVCLastClrDiagTX
            INTEGER (-2147483648..2147483647),

        adnCMBfvcadnFrVCRemoteifindex
            INTEGER (-2147483648..2147483647),

        adnCMBfvcadnFrPVCEndptXmitdSigStatus
            INTEGER (-2147483648..2147483647),

        adnCMBfvcadnFrVCInBc
            INTEGER (-2147483648..2147483647),

