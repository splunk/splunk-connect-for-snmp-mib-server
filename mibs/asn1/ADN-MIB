adnAtmUniVpCconnectMaxHops OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The maximum allowed number of hops of this Virtual Path (VP)
        cross-connection.
        If set to 65535, the value defined at node level is used.
        Common Name: Number of hops"
    ::= { adnAtmUniVpCconnectEntry  35 }

adnAtmUniVpCconnectPvcDescr OBJECT-TYPE
    SYNTAX  DisplayString
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "This object contains the Permanent Virtual Connection (PVC)
        description inserted in each charging ticket generated  for
        this Virtual Path (VP) cross-connection.
	This object is significant only if the type of the  Virtual
	Path Link  (VPL), specified  by  adnAtmVplType, is  set  to
	'pvc'."
    ::= { adnAtmUniVpCconnectEntry  36 }

adnAtmCniVpCconnectTable OBJECT-TYPE
    SYNTAX  SEQUENCE OF AdnAtmCniVpCconnectEntry
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
           "The Virtual Path (VP) cross-connect table on the inter-node
        ATM interfaces (Cell alcatel  Network  protocol  Interfaces
        (CNIs))."
    ::= { adnAtmMIBObjects  9 }

adnAtmCniVpCconnectEntry OBJECT-TYPE
    SYNTAX  AdnAtmCniVpCconnectEntry
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
           "An entry in the Cell  alcatel  Network  protocol  Interface
        (CNI) Virtual Path (VP) cross-connect table. There  is  one
        entry for each VP cross-connection on a CNI."
    INDEX   { ifIndex,  atmVplVpi,  adnAtmCniVpCconnectIndex }
    ::= { adnAtmCniVpCconnectTable  1 }

AdnAtmCniVpCconnectEntry ::=
    SEQUENCE {
        adnAtmCniVpCconnectIndex
            INTEGER,

        adnAtmCniVpCconnectOutIfIndex
            InterfaceIndex,

        adnAtmCniVpCconnectOutVpi
            INTEGER,

        adnAtmCniVpCconnectOutCcnIndex
            INTEGER,

        adnAtmCniVpCconnectAdminStatus
            AdnAtmAdminStatus,

        adnAtmCniVpCconnectOperStatus
            AdnAtmCniOperStatus,

        adnAtmCniVpCconnectSourceDest
            AdnAtmCniSourceDest,

        adnAtmCniVpCconnectStatistics
            INTEGER,

        adnAtmCniVpCconnectDirectedPath
            AdnAtmDirectedPath
    }

adnAtmCniVpCconnectIndex OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
           "The index of the  cross-connection  for  the  Virtual  Path
        Connection (VPC) on  this  interface. In  a  point-to-point
        cross-connection, the index equals 0."
    ::= { adnAtmCniVpCconnectEntry  1 }

adnAtmCniVpCconnectOutIfIndex OBJECT-TYPE
    SYNTAX  InterfaceIndex
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The ifIndex of this node's output ATM interface."
    ::= { adnAtmCniVpCconnectEntry  2 }

adnAtmCniVpCconnectOutVpi OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The Virtual Path Identifier (VPI) of this connection on the
        output ATM interface from this  node. In  conjunction  with
        adnAtmCniVpCconnectOutIfIndex, this can be  used  for  path
        tracing."
    ::= { adnAtmCniVpCconnectEntry  3 }

adnAtmCniVpCconnectOutCcnIndex OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The cross-connection  index  of  the  output  Virtual  Path
        Identifier (VPI) of this node's output  ATM interface. This
        index can be used  with  adnAtmCniVpCconnectOutIfIndex  and
        adnAtmCniVpCconnectOutVpi for call tracing."
    ::= { adnAtmCniVpCconnectEntry  4 }

adnAtmCniVpCconnectAdminStatus OBJECT-TYPE
    SYNTAX  AdnAtmAdminStatus
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The current administrative status of this Virtual Path (VP)
        cross-connection. The  'up'  and  'down'  states   indicate
        whether or not this ATM VP cross-connection is operational."
    ::= { adnAtmCniVpCconnectEntry  5 }

adnAtmCniVpCconnectOperStatus OBJECT-TYPE
    SYNTAX  AdnAtmCniOperStatus
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The current Virtual  Path  Connection  (VPC)  call  attempt
        state."
    ::= { adnAtmCniVpCconnectEntry  6 }

adnAtmCniVpCconnectSourceDest OBJECT-TYPE
    SYNTAX  AdnAtmCniSourceDest
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "Whether this is the  source  or  destination  side  of  the
        cross-connection."
    ::= { adnAtmCniVpCconnectEntry  7 }

adnAtmCniVpCconnectStatistics OBJECT-TYPE
    SYNTAX  INTEGER {
    enabled(1),
    disabled(2),
    useInterfaceValue(3)
}
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "Whether statistics are enabled for this Virtual  Path  (VP)
        cross-connection.
        This object is significant only if the type of the  Virtual
        Path Link  (VPL), specified  by  adnAtmVplType, is  set  to
        'pvc'; otherwise, the  statistics  status  is  defined  per
        interface.
        If  set  to  'useInterfaceValue', the  value  defined   per
        interface is used.
        Common Name: Statistics"
    ::= { adnAtmCniVpCconnectEntry  8 }

adnAtmCniVpCconnectDirectedPath OBJECT-TYPE
    SYNTAX  AdnAtmDirectedPath
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "Whether this Virtual Path Connection (VPC) uses a  directed
        path."
    ::= { adnAtmCniVpCconnectEntry  9 }

adnAtmUniVcCconnectTable OBJECT-TYPE
    SYNTAX  SEQUENCE OF AdnAtmUniVcCconnectEntry
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
           "The  ATM  User  Network  Interface  (UNI)  Virtual  Channel
        cross-connect table."
    ::= { adnAtmMIBObjects  10 }

adnAtmUniVcCconnectEntry OBJECT-TYPE
    SYNTAX  AdnAtmUniVcCconnectEntry
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
           "An entry in the ATM User Network  Interface  (UNI)  Virtual
        Channel (VC) cross-connect table. There is  one  entry  for
        each UNI VC cross-connection. An entry describes  both  the
        local cross-connection and the end-to-end cross-connection."
    INDEX   { ifIndex,  atmVclVpi,  atmVclVci,  adnAtmUniVcCconnectIndex }
    ::= { adnAtmUniVcCconnectTable  1 }

AdnAtmUniVcCconnectEntry ::=
    SEQUENCE {
        adnAtmUniVcCconnectIndex
            INTEGER,

        adnAtmUniVcCconnectOutIfIndex
            InterfaceIndex,

        adnAtmUniVcCconnectOutVpi
            INTEGER,

        adnAtmUniVcCconnectOutVci
            INTEGER,

        adnAtmUniVcCconnectOutCcnIndex
            INTEGER,

        adnAtmUniVcCconnectAdminStatus
            AdnAtmAdminStatus,

        adnAtmUniVcCconnectOperStatus
            AdnAtmUniOperStatus,

        adnAtmUniVcCconnectSourceDest
            AdnAtmUniSourceDest,

        adnAtmUniVcCconnectStatistics
            INTEGER,

        adnAtmUniVcCconnectDirectedPath
            AdnAtmDirectedPath,

        adnAtmUniVcCconnectRetryAttempts
            Counter,

        adnAtmUniVcCconnectFastRetry
            INTEGER (-2147483648..2147483647),

        adnAtmUniVcCconnectSlowRetry
            INTEGER (-2147483648..2147483647),

        adnAtmUniVcCconnectRemNumPlan
            AdnAtmNumPlan,

        adnAtmUniVcCconnectRemPhysAddress
            PhysAddress,

        adnAtmUniVcCconnectRemVpi
            INTEGER,

        adnAtmUniVcCconnectRemVci
            INTEGER,

        adnAtmUniVcCconnectRemCcnIndex
            INTEGER,

        adnAtmUniVcCconnectRemRoutingNodeId
            AdnRoutingID,

        adnAtmUniVcCconnectCumulNodes
            INTEGER,

        adnAtmUniVcCconnectMaxTransitDelay
            INTEGER,

        adnAtmUniVcCconnectCharging
            INTEGER,

        adnAtmUniVcCconnectCumulTD
            INTEGER,

        adnAtmUniVcCconnectCumulMonetaryCst
            INTEGER,

        adnAtmUniVcCconnectCauseError
            INTEGER,

        adnAtmUniVcCconnectLastCanpCause
            INTEGER,

        adnAtmUniVcCconnectLastErrorNodeID
            AdnRoutingID,

        adnAtmUniVcCconnectLastLocation
            INTEGER,

        adnAtmUniVcCconnectLastCause
            INTEGER,

        adnAtmUniVcCconnectLastDiag
            INTEGER,

        adnAtmUniVcCconnectRemNodeMacNum
            INTEGER (-2147483648..2147483647),

        adnAtmUniVcCconnectRemNodeAreaNum
            INTEGER (-2147483648..2147483647),

        adnAtmUniVcCconnectErrorNodeMacNum
            INTEGER (-2147483648..2147483647),

        adnAtmUniVcCconnectErrorNodeAreaNum
            INTEGER (-2147483648..2147483647),

        adnAtmUniVcCconnectLastChassis
            INTEGER (-2147483648..2147483647),

        adnAtmUniVcCconnectLastSlot
            INTEGER (-2147483648..2147483647),

        adnAtmUniVcCconnectPvcDescr
            DisplayString
    }

adnAtmUniVcCconnectIndex OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
           "The index of the cross-connection for the  Virtual  Channel
        Connection (VCC) on  this  interface. In  a  point-to-point
        cross-connection, the index  equals  zero. In  a  point-to-
        -multipoint cross-connection, there is  a  different  index
        for each leaf.
        Common Name: Cross-connection Index"
    ::= { adnAtmUniVcCconnectEntry  1 }

adnAtmUniVcCconnectOutIfIndex OBJECT-TYPE
    SYNTAX  InterfaceIndex
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The ifIndex of this node's output ATM interface."
    ::= { adnAtmUniVcCconnectEntry  2 }

adnAtmUniVcCconnectOutVpi OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The Virtual Path Identifier (VPI) of this connection on the
        output ATM interface from this  node. In  conjunction  with
        adnAtmUniVcCconnectOutIfIndex, this VPI  can  be  used  for
        path tracing."
    ::= { adnAtmUniVcCconnectEntry  3 }

adnAtmUniVcCconnectOutVci OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The Virtual Channel Identifier (VCI) of this node's  output
        interface. This cross-connection exits the node on  another
        VCI. This value can be combined with
        adnAtmUniVcCconnectOutIfIndex and 
        adnAtmUniVcCconnectOutVpi and used for call tracing."
    ::= { adnAtmUniVcCconnectEntry  4 }

adnAtmUniVcCconnectOutCcnIndex OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The cross-connection  index  of  the  output  Virtual  Path
        Identifier/Virtual Channel  Identifier  (VPI/VCI)  of  this
        node's output interface. This value can  be  combined  with
        adnAtmUniVcCconnectOutIfIndex,   adnAtmUniVcCconnectOutVpi,
        and adnAtmUniVcCconnectOutVci and used for call tracing."
    ::= { adnAtmUniVcCconnectEntry  5 }

adnAtmUniVcCconnectAdminStatus OBJECT-TYPE
    SYNTAX  AdnAtmAdminStatus
    ACCESS  read-write
    STATUS  mandatory
    DESCRIPTION
           "The current administrative status of this  Virtual  Channel
        (VC) cross-connection. The 'up' and 'down'  states  specify
        whether or not this ATM VC cross-connection is operational."
    ::= { adnAtmUniVcCconnectEntry  6 }

adnAtmUniVcCconnectOperStatus OBJECT-TYPE
    SYNTAX  AdnAtmUniOperStatus
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The current Virtual Channel Connection (VCC)  call  attempt
        state."
    ::= { adnAtmUniVcCconnectEntry  7 }

adnAtmUniVcCconnectSourceDest OBJECT-TYPE
    SYNTAX  AdnAtmUniSourceDest
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "Whether this is the  source  or  destination  side  of  the
        virtual cross-connection."
    ::= { adnAtmUniVcCconnectEntry  8 }

adnAtmUniVcCconnectStatistics OBJECT-TYPE
    SYNTAX  INTEGER {
    enabled(1),
    disabled(2),
    useInterfaceValue(3)
}
    ACCESS  read-write
    STATUS  mandatory
    DESCRIPTION
           "Whether statistics are enabled  for  this  Virtual  Channel
        (VC) cross-connection.
        This object is significant only if the type of the  Virtual
        Channel Link (VCL), specified  by adnAtmVclType, is set  to
        'pvc'; otherwise, the  statistics  status  is  defined  per
        interface.
        If  set  to  'useInterfaceValue', the  value  defined   per
        interface is used.
        Common Name: Statistics"
    ::= { adnAtmUniVcCconnectEntry  9 }

adnAtmUniVcCconnectDirectedPath OBJECT-TYPE
    SYNTAX  AdnAtmDirectedPath
    ACCESS  read-write
    STATUS  mandatory
    DESCRIPTION
           "Whether  this  Virtual  Channel  Connection  (VCC)  uses  a
        directed path."
    ::= { adnAtmUniVcCconnectEntry  10 }

adnAtmUniVcCconnectRetryAttempts OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The number of call  attempts  to  make  at  the  fast  rate
        (adnAtmUniVcCconnectFastRetry)  before  slowing  the   call
        attempt rate to adnAtmUniVcCconnectSlowRetry.
        adnAtmUniVcCconnectOperStatus specifies the current attempt
        state. This    object    is     significant     only     if
        adnAtmUniVcCconnectSourceDest is 'source', adnAtmVclType is
        'pvc', and adnAtmVclModeType is 'automatic'.
        Common Name: Retry Attempts at Fast Rate"
    ::= { adnAtmUniVcCconnectEntry  11 }

adnAtmUniVcCconnectFastRetry OBJECT-TYPE
    SYNTAX  INTEGER	-- UNITS seconds

    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "This timer specifies the amount of  time  to  wait  between
        failed Permanent Virtual Connection (PVC) setup attempts at
        the fast rate. After  making  attempts  at  this  rate  the
        number of times specified  by
        adnAtmUniVcCconnectCRetryAttempts, the   rate   slows    to
        adnAtmUniVcCconnectSlowRetry. This is significant  only  if
        adnAtmUniVcCconnectSourceDest is 'source', adnAtmVclType is
        'pvc', and adnAtmVclModeType is 'automatic'.
        Common Name: Wait Time between Fast Retries"
    ::= { adnAtmUniVcCconnectEntry  12 }

adnAtmUniVcCconnectSlowRetry OBJECT-TYPE
    SYNTAX  INTEGER	-- UNITS minutes

    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "This timer specifies the amount of  time  to  wait  between
        failed Permanent Virtual Connection (PVC) setup attempts at
        the slow rate. After making attempts at the
        adnAtmUniVcCconnectFastRetry  rate  the  number  of   times
        specified  by  adnAtmUniVcCconnectRetryAttempts, the   rate
        slows to this rate. This  object  is  significant  only  if
        adnAtmUniVcCconnectSourceDest is 'source', adnAtmVclType is
        'pvc', and adnAtmVclModeType is 'automatic'.
        Common Name: Wait Time between Slow Retries"
    ::= { adnAtmUniVcCconnectEntry  13 }

adnAtmUniVcCconnectRemNumPlan OBJECT-TYPE
    SYNTAX  AdnAtmNumPlan
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The numbering plan of the  remote  User  Network  Interface
        (UNI)   for    the    current    Virtual    Channel    (VC)
        cross-connection.
        Common Names: Remote Numbering Plan,
                      Rem Num Plan"
    REFERENCE
           "The numbering plan of the  remote  User  Network  Interface
        (UNI)   for    the    current    Virtual    Channel    (VC)
        cross-connection.
        Common Names: Remote Numbering Plan,
                      Rem Num Plan"
    ::= { adnAtmUniVcCconnectEntry  14 }

adnAtmUniVcCconnectRemPhysAddress OBJECT-TYPE
    SYNTAX  PhysAddress
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The   address   of   the   current  Virtual   Channel  (VC)
        cross-connection's remote  User  Network  Interface/Network
        Network Interface (UNI/NNI).
        Common Name: Remote Address"
    ::= { adnAtmUniVcCconnectEntry  15 }

adnAtmUniVcCconnectRemVpi OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The  Virtual Path Identifier (VPI) of the  current  Virtual
        Channel  (VC)  cross-connection's   remote   User   Network
        Interface/Network Network Interface (UNI/NNI).
        Common Name: Remote Virtual Path Identifier (VPI)"
    ::= { adnAtmUniVcCconnectEntry  16 }

adnAtmUniVcCconnectRemVci OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The Virtual Channel Identifier (VCI) value of  the  current
        Virtual Channel (VC) cross-connection's remote User Network
        Interface/Network Network Interface (UNI/NNI).
        Common Name: Remote Virtual Channel Identifier (VCI)"
    ::= { adnAtmUniVcCconnectEntry  17 }

adnAtmUniVcCconnectRemCcnIndex OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The  index  value  of  the  current  Virtual  Channel  (VC)
        cross-connection's remote  User  Network  Interface/Network
        Network Interface (UNI/NNI).
        Common Name: Remote UNI Cross-Connection Index"
    ::= { adnAtmUniVcCconnectEntry  18 }

adnAtmUniVcCconnectRemRoutingNodeId OBJECT-TYPE
    SYNTAX  AdnRoutingID
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The identifier of  the  node  connecting  the  remote  User
        Network Interface/Network Network Interface  (UNI/NNI)  for
        the current Virtual Channel (VC) cross-connection.
        Common Name: Remote Node Identifier"
    ::= { adnAtmUniVcCconnectEntry  19 }

adnAtmUniVcCconnectCumulNodes OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The    number    of   nodes     in    the    connection. If
        adnAtmUniVcCconnectOperStatus   is  'callup', this  is  the
        current nodes count; otherwise, it is the nodes  count  for
        the last connection.
        Common Name: Number of Nodes"
    ::= { adnAtmUniVcCconnectEntry  20 }

adnAtmUniVcCconnectMaxTransitDelay OBJECT-TYPE
    SYNTAX  INTEGER	-- UNITS milliseconds

    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The maximum allowed transit delay of this  Virtual  Channel
        (VC) cross-connection.
        This object is significant only if the type of the  Virtual
        Channel Link (VCL), specified by adnAtmVplType, is  set  to
        'pvc'.
        In case of 'svc', the maximum transit delay is defined  per
        interface or could be defined  in  an  Information  Element
        If set to 0, the value defined per interface is used."
    ::= { adnAtmUniVcCconnectEntry  21 }

adnAtmUniVcCconnectCharging OBJECT-TYPE
    SYNTAX  INTEGER {
    other(1),
    enabled(2),
    disabled(3),
    useInterfaceValue(4)
}
    ACCESS  read-write
    STATUS  mandatory
    DESCRIPTION
           "Whether charging is enabled for this Virtual  Channel  (VC)
        cross-connection.
        This object is significant only if the type of the  Virtual
        Channel Link (VCL), specified by adnAtmVclType, is  set  to
        'pvc'; otherwise, the  charging  status  is   defined   per
        interface.
        If  set  to  'useInterfaceValue', the  value  defined   per
        interface is used."
    ::= { adnAtmUniVcCconnectEntry  22 }

adnAtmUniVcCconnectCumulTD OBJECT-TYPE
    SYNTAX  INTEGER	-- UNITS milliseconds

    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The  current  cumulative  transit  delay  of  this  Virtual
        Channel (VC) cross-connection."
    ::= { adnAtmUniVcCconnectEntry  23 }

adnAtmUniVcCconnectCumulMonetaryCst OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The  current  cumulative  monetary  cost  of  this  Virtual
        Channel (VC) cross-connection."
    ::= { adnAtmUniVcCconnectEntry  24 }

adnAtmUniVcCconnectCauseError OBJECT-TYPE
    SYNTAX  INTEGER {
    notSignificant(1),
    maxCallAttempts(2),
    configurationError(3)
}
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "Global cause of the cross-connect failure."
    ::= { adnAtmUniVcCconnectEntry  25 }

adnAtmUniVcCconnectLastCanpCause OBJECT-TYPE
    SYNTAX  INTEGER (-2147483648..2147483647)
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "Last Cell Alcatel Network Protocol (CANP) cause.
        This nominator, which is read only, is  not  significant if
        adnAtmUniVcCconnectCauseError is equal to 'notSignificant'."
    ::= { adnAtmUniVcCconnectEntry  26 }

adnAtmUniVcCconnectLastErrorNodeID OBJECT-TYPE
    SYNTAX  AdnRoutingID
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The identifier of the node detecting the error.
        This nominator, which is read only, is  not  significant if
        adnAtmUniVcCconnectCauseError is equal to 'notSignificant'."
    ::= { adnAtmUniVcCconnectEntry  27 }

adnAtmUniVcCconnectLastLocation OBJECT-TYPE
    SYNTAX  INTEGER (-2147483648..2147483647)
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "ATM forum location value in the cause information element.
        This nominator, which is read only, is  not  significant if
        adnAtmUniVcCconnectCauseError is equal to 'notSignificant'."
    REFERENCE
           "ATM forum location value in the cause information element.
        This nominator, which is read only, is  not  significant if
        adnAtmUniVcCconnectCauseError is equal to 'notSignificant'."
    ::= { adnAtmUniVcCconnectEntry  28 }

adnAtmUniVcCconnectLastCause OBJECT-TYPE
    SYNTAX  INTEGER (-2147483648..2147483647)
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "ATM forum cause value in the cause information element.
        This nominator, which is read only, is  not  significant if
        adnAtmUniVcCconnectCauseError is equal to 'notsignificant'."
    REFERENCE
           "ATM forum cause value in the cause information element.
        This nominator, which is read only, is  not  significant if
        adnAtmUniVcCconnectCauseError is equal to 'notsignificant'."
    ::= { adnAtmUniVcCconnectEntry  29 }

adnAtmUniVcCconnectLastDiag OBJECT-TYPE
    SYNTAX  INTEGER (-2147483648..2147483647)
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "ATM forum diagnostic value in the cause information element.
        This nominator, which is read only, is  not  significant if
        adnAtmUniVcCconnectCauseError is equal to 'notsignificant'."
    REFERENCE
           "ATM forum diagnostic value in the cause information element.
        This nominator, which is read only, is  not  significant if
        adnAtmUniVcCconnectCauseError is equal to 'notsignificant'."
    ::= { adnAtmUniVcCconnectEntry  30 }

adnAtmUniVcCconnectRemNodeMacNum OBJECT-TYPE
    SYNTAX  INTEGER (-2147483648..2147483647)
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The machine number part of
        adnAtmUniVpCconnectRemRoutingNodeID."
    ::= { adnAtmUniVcCconnectEntry  31 }

adnAtmUniVcCconnectRemNodeAreaNum OBJECT-TYPE
    SYNTAX  INTEGER (-2147483648..2147483647)
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The area number part of adnAtmUniVcCconnectRemRoutingNodeID."
    ::= { adnAtmUniVcCconnectEntry  32 }

