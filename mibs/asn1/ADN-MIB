adnAeiPortTraCounterTable OBJECT-TYPE
    SYNTAX  SEQUENCE OF AdnAeiPortTraCounterEntry
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
           "This table is the ATM electrical interface (AEI) port
        traffic counters table.
        The counters are classified according to the board
        type. Some are specific to the broadband ATM
        switching element (BBASE) board also called the
        matrix board."
    ::= { adnNodeSupervisionMIB  24 }

adnAeiPortTraCounterEntry OBJECT-TYPE
    SYNTAX  AdnAeiPortTraCounterEntry
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
           "An entry in the ATM electrical interface (AEI) port
        traffic counters table."
    INDEX   { adnEquipType,  adnEquipIndex }
    ::= { adnAeiPortTraCounterTable  1 }

AdnAeiPortTraCounterEntry ::=
    SEQUENCE {
        adnAeiPortTraPTPSetRqCounter
            Counter,

        adnAeiPortTraPTMSetRqCounter
            Counter,

        adnAeiPortTraPTPSetRjBCounter
            Counter,

        adnAeiPortTraPTMSetRjBCounter
            Counter,

        adnAeiPortTraPTPSetRjCCounter
            Counter,

        adnAeiPortTraPTMSetRjCCounter
            Counter,

        adnAeiPortTraPTPSetTrCounter
            Counter,

        adnAeiPortTraPTMSetTrCounter
            Counter,

        adnAeiPortTraPTPCnxCounter
            Gauge,

        adnAeiPortTraPTMCnxCounter
            Gauge,

        adnAeiPortTraEFCCCounter
            Counter,

        adnAeiPortTraPTPBWICIRqCounter
            Counter,

        adnAeiPortTraPTMBWICIRqCounter
            Counter,

        adnAeiPortTraPTPBWRjCounter
            Counter,

        adnAeiPortTraPTMBWRjCounter
            Counter,

        adnAeiPortTraPTPICIRjCounter
            Counter,

        adnAeiPortTraPTMICIRjCounter
            Counter,

        adnAeiPortTraPeakBWCounter
            Gauge,

        adnAeiPortTraFPTPICICounter
            Gauge,

        adnAeiPortTraMPriorBWCounter
            Gauge,

        adnAeiPortTraInCellsCounter
            Counter,

        adnAeiPortTraOutCellsCounter
            Counter,

        adnAeiPortTraEBCCCounter
            Counter
    }

