adnLapMIB OBJECT IDENTIFIER ::= { adnProtocols  2 }

adnLapParamTable OBJECT-TYPE
    SYNTAX  SEQUENCE OF AdnLapParamEntry
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
           "This table contains configurable LAP (link access
                        procedure) parameters."
    ::= { adnLapMIB  1 }

adnLapParamEntry OBJECT-TYPE
    SYNTAX  AdnLapParamEntry
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
           "Configured parameter values for a specific LAP (link access
                procedure)."
    INDEX   { ifIndex,  adnLapDLCIIndex }
    ::= { adnLapParamTable  1 }

AdnLapParamEntry ::=
    SEQUENCE {
        adnLapDLCIIndex
            INTEGER (-2147483648..2147483647),

        adnLapParamStationType
            INTEGER,

        adnLapParamN201FrameSize
            INTEGER (-2147483648..2147483647),

        adnLapParamKWindowSize
            INTEGER,

        adnLapParamN200RxmitCount
            INTEGER,

        adnLapParamT200AckTimer
            INTEGER (-2147483648..2147483647),

        adnLapParamT203IdleTimer
            INTEGER (-2147483648..2147483647),

        adnLapParamProtocolID
            INTEGER,

        adnLapParamAlarmsEnable
            INTEGER,

        adnLapParamPerfEnable
            INTEGER
    }

adnLapDLCIIndex OBJECT-TYPE
    SYNTAX  INTEGER (-2147483648..2147483647)
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
           "DLCI (data link connection identifier) index."
    ::= { adnLapParamEntry  1 }

adnLapParamStationType OBJECT-TYPE
    SYNTAX  INTEGER {
    other(1),
    network(2),
    user(3),
    layer-3-selects(4)
}
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "Identifies the desired station type of this interface."
    DEFVAL  { layer-3-selects }
    ::= { adnLapParamEntry  2 }

adnLapParamN201FrameSize OBJECT-TYPE
    SYNTAX  INTEGER (-2147483648..2147483647)	-- UNITS bits

    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The default maximum N201 information field size 
		(in number of bits) for a frame the DCE/remote
		DTE exchanges with this DTE.  This excludes
		flags and 0 bits inserted for transparency."
    DEFVAL  { 32768 }
    ::= { adnLapParamEntry  3 }

adnLapParamKWindowSize OBJECT-TYPE
    SYNTAX  INTEGER	-- UNITS PDUs

    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The default window size for this
		interface.  This is the maximum number of
		unacknowledged sequenced PDUs (protocol data units) that may
		be outstanding from this DTE at any one time."
    DEFVAL  { 4 }
    ::= { adnLapParamEntry  4 }

adnLapParamN200RxmitCount OBJECT-TYPE
    SYNTAX  INTEGER	-- UNITS PDU retransmissions

    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The default N200 retry counter for this
		interface.  This specifies the number of
		times a PDU (protocol data unit) will be 
                resent after timer T200 expires without 
                acknowledging the PDU."
    DEFVAL  { 3 }
    ::= { adnLapParamEntry  5 }

adnLapParamT200AckTimer OBJECT-TYPE
    SYNTAX  INTEGER (-2147483648..2147483647)	-- UNITS milliseconds

    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The default T200 timer for this interface.
		This specifies the maximum time (in milliseconds)
		to wait for acknowledgment of a PDU (protocol data unit)."
    DEFVAL  { 1000 }
    ::= { adnLapParamEntry  6 }

adnLapParamT203IdleTimer OBJECT-TYPE
    SYNTAX  INTEGER (-2147483648..2147483647)	-- UNITS milliseconds

    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The T203 timer for this interface.  This
		specifies the maximum time (in milliseconds)
		to allow without frames being exchanged on
		the data link.  A value of '2147483647'
		indicates that no idle timer is being kept."
    DEFVAL  { 10000 }
    ::= { adnLapParamEntry  7 }

adnLapParamProtocolID OBJECT-TYPE
    SYNTAX  INTEGER {
    other(1),
    lapdQ921(2),
    lapfQ922(3),
    layer-3-selects(4)
}
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "This identifies the protocol to be used on this interface.
		Common name: Protocol"
    DEFVAL  { layer-3-selects }
    ::= { adnLapParamEntry  8 }

