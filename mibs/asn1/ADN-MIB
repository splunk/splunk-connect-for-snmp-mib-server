adnFrVCTable OBJECT-TYPE
    SYNTAX  SEQUENCE OF AdnFrVCEntry
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
           "These are the PVC table extensions to the PVC End-Point
        table in RFC1604. This table is used for PVCs and SVCs."
    ::= { adnFrnetservMIB  2 }

adnFrVCEntry OBJECT-TYPE
    SYNTAX  AdnFrVCEntry
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
           "VC table entry."
    INDEX   { ifIndex,  frPVCEndptDLCIIndex }
    ::= { adnFrVCTable  1 }

AdnFrVCEntry ::=
    SEQUENCE {
        adnFrVCRemNodeID
            AdnRoutingID,

        adnFrVCRemNumPlan
            INTEGER,

        adnFrVCRemPhysAddress
            PhysAddress,

        adnFrVCRemDlci
            INTEGER (-2147483648..2147483647),

        adnFrVCAdminState
            INTEGER,

        adnFrVCOperState
            INTEGER,

        adnFrVCTime
            AdnNodeTimeStamp,

        adnFrVCInBcs
            INTEGER (-2147483648..2147483647),

        adnFrVCOutBcs
            INTEGER (-2147483648..2147483647),

        adnFrVCUserOrNet
            INTEGER,

        adnFrVCLisDiscard
            INTEGER,

        adnFrVCRetryAttempts
            INTEGER (-2147483648..2147483647),

        adnFrVCFastRetry
            INTEGER (-2147483648..2147483647),

        adnFrVCSlowRetry
            INTEGER (-2147483648..2147483647),

        adnFrVCForcedPath
            AdnEnableDisabled,

        adnFrVCSourceDest
            INTEGER,

        adnFrVCFrameIN
            Counter,

        adnFrVCFrameOUT
            Counter,

        adnFrVCSubFrameIN
            Counter,

        adnFrVCSubFrameOUT
            Counter,

        adnFrVCBeInDiscards
            Counter,

        adnFrVCBcsInDiscards
            Counter,

        adnFrVCBcInDiscards
            Counter,

        adnFrVCBeOutDiscards
            Counter,

        adnFrVCBcsOutDiscards
            Counter,

        adnFrVCBcOutDiscards
            Counter,

        adnFrVCInCongsState
            AdnCongestLevels,

        adnFrVCDiscardLisIn
            Counter,

        adnFrVCNumRelinks
            Counter,

        adnFrVCNumNodes
            INTEGER (-2147483648..2147483647),

        adnFrVCType
            INTEGER,

        adnFrVCOutifIndex
            InterfaceIndex,

        adnFrVCOutDlci
            INTEGER (-2147483648..2147483647),

        adnFrVCISR
            INTEGER (-2147483648..2147483647),

        adnFrVCAttemptState
            INTEGER,

        adnFrVCStatistics
            INTEGER,

        adnFrVCAccounting
            INTEGER,

        adnFrVCUniqueConnID
            INTEGER (-2147483648..2147483647),

        adnFrVCLastClrCauseRX
            INTEGER (-2147483648..2147483647),

        adnFrVCLastClrDiagRX
            INTEGER (-2147483648..2147483647),

        adnFrVCLastClrCauseTX
            INTEGER (-2147483648..2147483647),

        adnFrVCLastClrDiagTX
            INTEGER (-2147483648..2147483647),

        adnFrVCRemoteifindex
            INTEGER (-2147483648..2147483647),

        adnFrPVCEndptXmitdSigStatus
            INTEGER (-2147483648..2147483647),

        adnFrVCInBc
            INTEGER (-2147483648..2147483647),

        adnFrVCOutBc
            INTEGER (-2147483648..2147483647),

        adnFrVCRemNodeMacNum
            INTEGER (-2147483648..2147483647),

        adnFrVCRemNodeAreaNum
            INTEGER (-2147483648..2147483647),

        adnFrVCDelaySensitive
            TruthValue,

        adnFrVCCos
            AdnFrCos,

        adnFrVCEndptDLCIIndex
            INTEGER (-2147483648..2147483647),

        adnFrVCEndptInMaxFrameSize
            INTEGER (-2147483648..2147483647),

        adnFrVCEndptInBc
            INTEGER (-2147483648..2147483647),

        adnFrVCEndptInBe
            INTEGER (-2147483648..2147483647),

        adnFrVCEndptInCIR
            INTEGER (-2147483648..2147483647),

        adnFrVCEndptOutMaxFrameSize
            INTEGER (-2147483648..2147483647),

        adnFrVCEndptOutBc
            INTEGER (-2147483648..2147483647),

        adnFrVCEndptOutBe
            INTEGER (-2147483648..2147483647),

        adnFrVCEndptOutCIR
            INTEGER (-2147483648..2147483647),

        adnFrVCEndptConnectIdentifier
            INTEGER (-2147483648..2147483647),

        adnFrVCEndptRowStatus
            RowStatus,

        adnFrVCEndptRcvdSigStatus
            INTEGER,

        adnFrVCEndptInFrames
            Counter,

        adnFrVCEndptOutFrames
            Counter,

        adnFrVCEndptInDEFrames
            Counter,

        adnFrVCEndptInExcessFrames
            Counter,

        adnFrVCEndptOutExcessFrames
            Counter,

        adnFrVCEndptInDiscards
            Counter,

        adnFrVCEndptInOctets
            Counter,

        adnFrVCEndptOutOctets
            Counter,

        adnFrVCBwAllocated
            INTEGER (-2147483648..2147483647),

        adnFrVCDescr
            DisplayString
    }

adnFrVCRemNodeID OBJECT-TYPE
    SYNTAX  AdnRoutingID
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "This is the routing node ID of the destination node for
        this VC.  It is an integer that ROUTES (HSS routing 
        application) uses to route this call to the proper 
        destination node. 
        Common Name: Remote Node ID"
    ::= { adnFrVCEntry  1 }

adnFrVCRemNumPlan OBJECT-TYPE
    SYNTAX  INTEGER {
    other(1),
    e164(2),
    x121(3),
    none(4)
}
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "This is the numbering plan of the interface
         on the node at the remote end of the VC.
         Common Name: Remote Numbering Plan"
    DEFVAL  { e164 }
    ::= { adnFrVCEntry  2 }

adnFrVCRemPhysAddress OBJECT-TYPE
    SYNTAX  PhysAddress
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "This is the address of the interface on the
         node at the remote end of the VC.  The numbering
         plan for that interface is given by the value of
         adnFrVCRemNumPlan.
         Common Name: Remote address"
    ::= { adnFrVCEntry  3 }

adnFrVCRemDlci OBJECT-TYPE
    SYNTAX  INTEGER (-2147483648..2147483647)
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "This is the DLCI (data link connection identifier) of
         this VC on the interface on the node at the remote
         end of the VC.
         Common Name: Remote DLCI"
    ::= { adnFrVCEntry  4 }

adnFrVCAdminState OBJECT-TYPE
    SYNTAX  INTEGER {
    up(1),
    down(2)
}
    ACCESS  read-write
    STATUS  mandatory
    DESCRIPTION
           "The desired state of this call.
        Common Name: Status Administrative, Administrative"
    DEFVAL  { up }
    ::= { adnFrVCEntry  5 }

adnFrVCOperState OBJECT-TYPE
    SYNTAX  INTEGER {
    up(1),
    down(2)
}
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The current state of this call.
        Common Name: Status Operational, Operational"
    ::= { adnFrVCEntry  6 }

adnFrVCTime OBJECT-TYPE
    SYNTAX  AdnNodeTimeStamp
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The time the VC was initialized. For a PVC, this is the
        time the PVC was configured.
        Common Name: Last Change"
    ::= { adnFrVCEntry  7 }

adnFrVCInBcs OBJECT-TYPE
    SYNTAX  INTEGER (-2147483648..2147483647)
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "This specifies the Bcs (statistically-committed data rate)
        for ingress data.
        Common Name: Ingress Bcs"
    DEFVAL  { 64000 }
    ::= { adnFrVCEntry  8 }

adnFrVCOutBcs OBJECT-TYPE
    SYNTAX  INTEGER (-2147483648..2147483647)
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "This specifies the Bcs (statistically-committed data rate)
        for egress data.
        Common Name: Egress Bcs"
    DEFVAL  { 64000 }
    ::= { adnFrVCEntry  9 }

adnFrVCUserOrNet OBJECT-TYPE
    SYNTAX  INTEGER {
    usercall(1),
    networkcall(2)
}
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "This indicates if this is a User or Network call. The
        priority given to each type of call's data in the network
        differs.
        Common Name: Type of Call"
    DEFVAL  { usercall }
    ::= { adnFrVCEntry  10 }

adnFrVCLisDiscard OBJECT-TYPE
    SYNTAX  INTEGER {
    discard(1),
    nodiscard(2)
}
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "This object indicates whether FRS should discard ingress
        data if the LIS (Local In-channel Signalling) status for
        this PVC is inactive.  If this is 'discard(1)' and the
        local or remote LIS status is down, then ingress data is
        discarded.  If this is 'nodiscard(2)', then regardless of
        LIS status, the ingress data is not discarded.
        Common Name: LIS Discard Action"
    DEFVAL  { discard }
    ::= { adnFrVCEntry  11 }

adnFrVCRetryAttempts OBJECT-TYPE
    SYNTAX  INTEGER (-2147483648..2147483647)
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "This specifies the number of PVC call attempts to make
         at the fast rate (adnFrVCFastRetry) before slowing
         the call attempt rate to adnFrVCSlowRetry.  This only
         has significance if adnFrVCSourceDest is 'source(1)'.
         The current attempt state is in adnFrVCAttemptState.
        Common Name: Fast Rate Retry"
    DEFVAL  { 20 }
    ::= { adnFrVCEntry  12 }

adnFrVCFastRetry OBJECT-TYPE
    SYNTAX  INTEGER (-2147483648..2147483647)	-- UNITS seconds

    ACCESS  read-write
    STATUS  mandatory
    DESCRIPTION
           "This timer specifies the amount of time to wait between
        failed PVC setup attempts. After attempting this rate for
        adnFrVCRetryAttempts times, the rate will be slowed to
        adnFrVCSlowRetry. This only has significance if
        adnFrVCSourceDest is 'source(1)'.  
        Common Name: Fast Rate Retry Timer"
    DEFVAL  { 30 }
    ::= { adnFrVCEntry  13 }

adnFrVCSlowRetry OBJECT-TYPE
    SYNTAX  INTEGER (-2147483648..2147483647)	-- UNITS seconds

    ACCESS  read-write
    STATUS  mandatory
    DESCRIPTION
           "This timer specifies the amount of time to wait
        between failed PVC setup attempts. After attempting the
        adnFrVCFastRetry rate for adnFrVCRetryAttempts times,
        the rate will be slowed to adnFrVCSlowRetry.  This only
        has significance if adnFrVCSourceDest is 'source(1)'.
        Common Name: Slow Rate Retry Timer"
    DEFVAL  { 255 }
    ::= { adnFrVCEntry  14 }

adnFrVCForcedPath OBJECT-TYPE
    SYNTAX  AdnEnableDisabled
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "This specifies that this PVC has a forced path
        specified in the adnFrForceRoute table. If this object
        is set to 'enabled(1)' and there is no adnFrForceEntry
        for this PVC, then the PVC will be treated as
        if adnFrVCForcedPath has the value 'disabled(2)'.
        Common Name: Forced Path"
    DEFVAL  { disabled }
    ::= { adnFrVCEntry  15 }

adnFrVCSourceDest OBJECT-TYPE
    SYNTAX  INTEGER {
    source(1),
    destination(2)
}
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "This object indicates the call direction of this PVC.
        If it has the value 'source(1)', the FRS (frame relay
        Subscriber) subsystem will initiate the call. If it has the
        value 'destination(2)', FRS will wait for an incoming call
        request.
        Common Names: Source or Destination, Src/Dest"
    ::= { adnFrVCEntry  16 }

adnFrVCFrameIN OBJECT-TYPE
    SYNTAX  Counter
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "This counts the number of frames forwarded to the
        network for this VC. Note that the value of
        frPVCEndptInFrames from RFC1604 gives the frames received
        from the user. This counter does not include the
        'dummy' frames generated by congestion control. It
        does include the DQ (data qualified) frames, used to
        exchange status information with the FRS subsystem at the
        remote end of a call.
        Common Name: Frame In"
    ::= { adnFrVCEntry  17 }

adnFrVCFrameOUT OBJECT-TYPE
    SYNTAX  Counter
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "This counts the number of frames received from the
        network for this VC. Note that the value of
        frPVCEndptOutFrames from RFC1604 gives the frames sent
        to the user. This counter does not include the number of
        'dummy' frames generated by congestion control. It does
        include the DQ (data qualified) frames. 
        Common Name: Frame Out"
    ::= { adnFrVCEntry  18 }

adnFrVCSubFrameIN OBJECT-TYPE
    SYNTAX  Counter
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "This counts the number of sub-frames forwarded to the
        network for this VC. Note that the value of
        frAccountPVCInSegments from RFC1604 gives the number of
        sub-frames received from the user.
        Common Name: Sub Frame In"
    ::= { adnFrVCEntry  19 }

adnFrVCSubFrameOUT OBJECT-TYPE
    SYNTAX  Counter
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "This counts the number of sub-frames received from the
        network for this VC. Note that the value of
        frAccountPVCOutSegments from RFC1604 gives the
        number of sub-frames sent to the user.
        Common Name: Sub Frames Out"
    ::= { adnFrVCEntry  20 }

adnFrVCBeInDiscards OBJECT-TYPE
    SYNTAX  Counter
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "This indicates the number of ingress Be (excessive burst 
        data rate) frames that were discarded at the interface due 
        to network congestion.
        Common Name: Be Frame In Discards"
    ::= { adnFrVCEntry  21 }

adnFrVCBcsInDiscards OBJECT-TYPE
    SYNTAX  Counter
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "This indicates the number of ingress Bcs (statistically-
        committed data rate) frames that were discarded at the 
        interface due to network congestion.
        Common Name: Bcs Frame In Discards"
    ::= { adnFrVCEntry  22 }

adnFrVCBcInDiscards OBJECT-TYPE
    SYNTAX  Counter
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "This indicates the number of ingress Bc (committed data 
        rate) frames that were discarded at the interface due to 
        network congestion.
        Common Name: Bc Frame In Discards"
    ::= { adnFrVCEntry  23 }

adnFrVCBeOutDiscards OBJECT-TYPE
    SYNTAX  Counter
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "This indicates the number of egress Be (excessive burst data
        rate) frames that were discarded at the interface due to 
        network congestion.
        Common Name: Be Frame Out Discards"
    ::= { adnFrVCEntry  24 }

adnFrVCBcsOutDiscards OBJECT-TYPE
    SYNTAX  Counter
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "This indicates the number of egress Bcs (statistically-
        committed data rate) frames that were discarded at the 
        interface due to network congestion.
        Common Name: Bcs Frame Out Discards"
    ::= { adnFrVCEntry  25 }

adnFrVCBcOutDiscards OBJECT-TYPE
    SYNTAX  Counter
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "This indicates the number of egress Bc (committed data rate)
        frames that were discarded at the interface due to network 
        congestion.
        Common Name: Bc Frame Out Discards"
    ::= { adnFrVCEntry  26 }

adnFrVCInCongsState OBJECT-TYPE
    SYNTAX  AdnCongestLevels
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "This indicates the congestion level of this VC in the
        ingress direction. Depending on the congestion level, some
        frames from the user may be discarded. Frames are discarded
        according to a priority scheme.  Note that an idle VC may
        also be classified as congested due to restricted internal
        resources.
        Common Name: Ingress Congestion Level"
    ::= { adnFrVCEntry  27 }

adnFrVCDiscardLisIn OBJECT-TYPE
    SYNTAX  Counter
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "This indicates the number of ingress frames that were
         discarded because LIS (local in-channel signalling)
         declared the PVC inactive. This value will increment only if
         adnFrVCLisDiscard is set to 'discard(1)'.
         Common Name: Frames In Discarded (LIS down)"
    ::= { adnFrVCEntry  28 }

adnFrVCNumRelinks OBJECT-TYPE
    SYNTAX  Counter
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "Indicates the number of times the call was
        successfully connected.
        Common Name: # of Successful Links"
    ::= { adnFrVCEntry  29 }

adnFrVCNumNodes OBJECT-TYPE
    SYNTAX  INTEGER (-2147483648..2147483647)
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "This indicates the number of nodes in the VC connection
        If adnFrVCOperState is 'up(1)', then this gives the
        current hop count. If the adnFrVCOperState is not
        'up(1)', this this gives the hop count of the last
        connection."
    ::= { adnFrVCEntry  30 }

adnFrVCType OBJECT-TYPE
    SYNTAX  INTEGER {
    other(1),
    pvc(2),
    svc(3),
    mspvc(4)
}
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "This specifies the type of this connection: other, 
         pvc ( permanent virtual connection ), svc ( switched
         virtual connection ) or mspvc ( multi-source
         permanent virtual connection ).
        Common Name: Type"
    DEFVAL  { pvc }
    ::= { adnFrVCEntry  31 }

adnFrVCOutifIndex OBJECT-TYPE
    SYNTAX  InterfaceIndex
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "This is the ifIndex of the interface on which this VC
        exits this node away from this VC endpoint.  It is
        usually a trunk interface."
    ::= { adnFrVCEntry  32 }

adnFrVCOutDlci OBJECT-TYPE
    SYNTAX  INTEGER (-2147483648..2147483647)
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "This is the DLCI (data link connection identifier) of 
        the VC on the interface on which it exits this node away 
        from this VC endpoint.  This can be combined with 
        adnFrVCOutifIndex and used for call tracing."
    ::= { adnFrVCEntry  33 }

adnFrVCISR OBJECT-TYPE
    SYNTAX  INTEGER (-2147483648..2147483647)
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "This is the initial submission rate (ISR) for ingress
        traffic. When the VC is determined to be idle in the
        ingress direction, the submission information rate
        (SIR) is set to the the ISR.
        Common Name: Initial Submission Rate"
    DEFVAL  { 64000 }
    ::= { adnFrVCEntry  34 }

adnFrVCAttemptState OBJECT-TYPE
    SYNTAX  INTEGER {
    fast(1),
    slow(2),
    rxcall(3),
    callup(4)
}
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "This contains the current VC call attempt state. This
        will start out as 'fast(1)' or 'rxcall(3)' depending on
        adnFrVCSourceDest. If adnFrVCSourceDest has the value
        'source(1)', and the call attempt fails adnFrVCRetryAttempt 
        times, then the call will be attempted at the slow rate 
        (adnFrVCSlowRetry) and this object will be set to 'slow(2)'.
        Common Name: Attempt State"
    ::= { adnFrVCEntry  35 }

adnFrVCStatistics OBJECT-TYPE
    SYNTAX  INTEGER {
    enabled(1),
    disabled(2),
    useChannelValue(3)
}
    ACCESS  read-write
    STATUS  mandatory
    DESCRIPTION
           "This indicates whether statistics are enabled for
        this VC.  If set to 'useChannelValue(3)', this VC uses
        the value specified by adnFrChanStatistics.
        Common Name: Statistics"
    DEFVAL  { useChannelValue }
    ::= { adnFrVCEntry  36 }

adnFrVCAccounting OBJECT-TYPE
    SYNTAX  INTEGER {
    enabled(1),
    disabled(2),
    useChannelValue(3)
}
    ACCESS  read-write
    STATUS  mandatory
    DESCRIPTION
           "This indicates whether accounting is enabled for this
        VC.  If it has the value 'useChannelValue(3)', this VC
        uses the value specified by adnFrChanAccounting.
        Common Name: Accounting"
    DEFVAL  { useChannelValue }
    ::= { adnFrVCEntry  37 }

adnFrVCUniqueConnID OBJECT-TYPE
    SYNTAX  INTEGER (-2147483648..2147483647)
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "This is a unique connection identifier for this VC.
        This value is exchanged with the remote FRS (Frame Relay
        Subscriber) at call setup time.  Both the local and the 
        remote ends of the VC will report this value in the 
        accounting record."
    ::= { adnFrVCEntry  40 }

adnFrVCLastClrCauseRX OBJECT-TYPE
    SYNTAX  INTEGER (-2147483648..2147483647)
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "This is the most recent clearing cause that was
         received from the network for this VC."
    ::= { adnFrVCEntry  42 }

