adnClockSourceDown TRAP-TYPE
    ENTERPRISE  { adnTrapsCommon }
    VARIABLES   { adnNodeTime,  adnTrapCategory,  ifIndex,  ifName }
    DESCRIPTION
           "This trap indicates that a line configured to be
             a clock source has gone down."
    ::= 16

adnFrVCSlowPoll TRAP-TYPE
    ENTERPRISE  { adnTrapsFrameRelay }
    VARIABLES   { adnNodeTime,  adnTrapCategory,  ifIndex,  ifName,  frPVCEndptDLCIIndex,  adnFrVCLastClrCauseRX,  adnFrVCLastClrDiagRX,  adnFrVCRemNodeID,  adnFrVCCos }
    DESCRIPTION
           "This trap is sent every time a PVC enters the slow
           poll state."
    ::= 1

adnFrVCExitSlowPoll TRAP-TYPE
    ENTERPRISE  { adnTrapsFrameRelay }
    VARIABLES   { adnNodeTime,  adnTrapCategory,  ifIndex,  ifName,  frPVCEndptDLCIIndex,  adnFrVCCos }
    DESCRIPTION
           " This trap indicates the return to a normal
             PVC call setup attempt polling interval, after the call
             was in the slow poll state."
    ::= 2

adnFrVCDown TRAP-TYPE
    ENTERPRISE  { adnTrapsFrameRelay }
    VARIABLES   { adnNodeTime,  adnTrapCategory,  ifIndex,  ifName,  frPVCEndptDLCIIndex,  adnFrVCAdminState,  adnFrVCOperState,  adnFrVCCos }
    DESCRIPTION
           "Indicates that the PVC has been set to
                'down' by an NMS command."
    ::= 3

adnFrVCUp TRAP-TYPE
    ENTERPRISE  { adnTrapsFrameRelay }
    VARIABLES   { adnNodeTime,  adnTrapCategory,  ifIndex,  ifName,  frPVCEndptDLCIIndex,  adnFrVCAdminState,  adnFrVCOperState,  adnFrVCCos }
    DESCRIPTION
           "Indicates that the switch will attempt to make calls
            or accept calls for a particular PVC.  The NMS has
            allowed the PVC to be put into service."
    ::= 4

adnFrDiscardOn TRAP-TYPE
    ENTERPRISE  { adnTrapsFrameRelay }
    VARIABLES   { adnNodeTime,  adnTrapCategory,  ifIndex,  ifName,  adnTrapFrVCDLCIIndex,  adnTrapFrCongsState,  adnTrapFrIfType,  adnTrapFrCongOrigin,  adnTrapFrVCCos }
    DESCRIPTION
           "Indicates frame relay data is being discarded
            because of network congestion."
    ::= 5

adnFrDiscardOff TRAP-TYPE
    ENTERPRISE  { adnTrapsFrameRelay }
