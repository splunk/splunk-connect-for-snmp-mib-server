    }

adnAtmInterfaceLowLayerIfIndex OBJECT-TYPE
    SYNTAX  InterfaceIndex
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The ifIndex of the logical or physical line supporting this
        ATM interface. The line is considered to be the lower layer
        of the ATM interface.
        Common Name: Lower Layer Interface Index"
    ::= { adnAtmInterfaceConfEntry  1 }

adnAtmInterfaceType OBJECT-TYPE
    SYNTAX  INTEGER {
    externalUni(1),
    nni(2),
    internalUniSig(3),
    virtualUni(4),
    physicalCni(5),
    routlogicalCni(6),
    nonroutlogicalCni(7),
    nonroutlogicalCniendtoend(8),
    internalUni(9),
    tdpAccess(10),
    tdpTrunk(11)
}
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The type of ATM interface:

        - 'externalUni': On an High Speed Coupler (HSC) line

        - 'nni': On a HSC line, connected to  a  Virtual  Path (VP)
          switched network

        - 'internalUniSig': On   a   Cell  Interconnection  Gateway
          (CIG) internal line used for special connections such  as
          routing, signalling and ILMI

        - 'virtualUni': Logical  interface  supporting  the Virtual
          Path Connection (VPC) used to support  the  logical  Cell
          alcatel Network protocol Interfaces (CNIs)

        - 'physicalCni': Inter-node physical  interface (on  a  HSC
          line)

        - 'routlogicalCni': Routable logical  CNI, supported  by  a
          logical line (inter-node VPC)

        - 'nonroutlogicalCni': Non-routable logical CNI, known only
          by path select

        - 'nonroutlogicalCniendtoend': Non-routable   logical  CNI;
          end-to-end identical to previous one but  disallowed  for
          transit Virtual Channel Connections (VCCs)

        - 'internalUni': On a  board  internal  line  (e.g. Circuit
          Emulation Service (CES) board).

        - 'tdpAccess': Tag  Distribution   Protocol  (TDP)  capable
          access. It can be a regular Tag Edge Router  (TER)  or  a
          Tag Switch Controller (TSC). It is supported  by  an  ATM
          logical line

        - 'tdpTrunk': TDP capable trunk. It is supported by an  ATM
          logical line.

        Common Name: Interface Type"
    ::= { adnAtmInterfaceConfEntry  2 }

adnAtmInterfaceMaxFillingRate OBJECT-TYPE
    SYNTAX  INTEGER	-- UNITS percentage

    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The maximum filling rate on this ATM interface. This  value
        is the percentage of data that can be loaded on  the  line.
        For example, if the ifspeed is 100000 Mbits and the maximum
        filling rate is 80%, the total  bandwidth  allowed  on  the
        line is 80000  Mbits. When  the  maximum  filling  rate  is
        reached, no more calls are accepted on the ATM interface.
        Common Name: Filling Rate (%)"
    ::= { adnAtmInterfaceConfEntry  3 }

adnAtmInterfaceBWThreshold1 OBJECT-TYPE
    SYNTAX  INTEGER	-- UNITS percentage

    ACCESS  read-only
    STATUS  deprecated
    DESCRIPTION
           "The ATM interface's higher  bandwidth  threshold, used  for
        path  selection  on  the  Cell  alcatel  Network   protocol
        Interface (CNI) to calculate  actual  available  bandwidth.
        The value, expressed as a percentage, is  significant  only
        if this is a CNI; otherwise, it is 0.
        Common Name: Higher Bandwidth Threshold"
    ::= { adnAtmInterfaceConfEntry  4 }

adnAtmInterfaceBWThreshold2 OBJECT-TYPE
    SYNTAX  INTEGER	-- UNITS percentage

    ACCESS  read-only
    STATUS  deprecated
    DESCRIPTION
           "The ATM interface's medium  bandwidth  threshold, used  for
        path  selection  on  the  Cell  alcatel  Network   protocol
        Interface (CNI) to calculate  actual  available  bandwidth.
        The value, expressed as a percentage, is  significant  only
        if this is a CNI; otherwise, it is 0.
        Common Name: Medium Bandwidth Threshold"
    ::= { adnAtmInterfaceConfEntry  5 }

adnAtmInterfaceBWThreshold3 OBJECT-TYPE
    SYNTAX  INTEGER	-- UNITS percentage

    ACCESS  read-only
    STATUS  deprecated
    DESCRIPTION
           "The ATM interface's  lower  bandwidth  threshold, used  for
        path  selection  on  the  Cell  alcatel  Network   protocol
        Interface (CNI) to calculate  actual  available  bandwidth.
        The value, expressed as a percentage, is  significant  only
        if this is a CNI; otherwise, it is 0.
        Common Name: Lower Bandwidth Threshold"
    ::= { adnAtmInterfaceConfEntry  6 }

adnAtmInterfaceMaxPtpVcs OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The maximum number of  point-to-point  Virtual  Connections
        (VCs) (Virtual Path Connections (VPCs) and Virtual  Channel
        Connections (VCCs)) on this ATM interface.
        Common Name: Max Point-to-Point Virtual Connections"
    ::= { adnAtmInterfaceConfEntry  7 }

adnAtmInterfaceMaxPtMCc OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  read-only
    STATUS  deprecated
    DESCRIPTION
           "The maximum number of point-to-multipoint cross-connections
        on this ATM interface.
        Common Name: Max Point-to-Multipoint Cross Connections"
    ::= { adnAtmInterfaceConfEntry  8 }

adnAtmInterfaceMaxPvcc OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  read-only
    STATUS  deprecated
    DESCRIPTION
           "The maximum number of permanent Virtual Channel Connections
        (VCCs) on this ATM interface.
        Common Names: Max Permanent VCCs,
                      Max P-VCCs"
    ::= { adnAtmInterfaceConfEntry  9 }

