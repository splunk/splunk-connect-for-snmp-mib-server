adnAtmInterfaceAllowedAtc OBJECT-TYPE
    SYNTAX  INTEGER {
    other(1),
    anyAtc(2),
    onlyUbr1(3)
}
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "Allowed ATM Transfer Capabilities (ATC) for a Virtual  Path
        Connection  (VPC)  or  Virtual  Channel  Connection   (VCC)
        supported by the ATM interface:

        - 'anyAtc':   The ATM interface is able to support any ATC

        - 'ubr1Only': The ATM interface can only support Unspecified 
                      Bit Rate 1 (UBR1) ATC."
    ::= { adnAtmInterfaceConfEntry  49 }

adnAtmVpiVccConfTable OBJECT-TYPE
    SYNTAX  SEQUENCE OF AdnAtmVpiVccConfEntry
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
           "This  table  describes  the  characteristics  of  the   ATM
        interface's Virtual Path Identifiers  (VPIs)  dedicated  to
        Virtual Channel Connections (VCCs)."
    ::= { adnAtmMIBObjects  3 }

adnAtmVpiVccConfEntry OBJECT-TYPE
    SYNTAX  AdnAtmVpiVccConfEntry
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
           "An entry in the adnAtmVpiVccConfTable. There is  one  entry
        for each Virtual Path Identifier (VPI) dedicated to Virtual
        Channel Connections (VCCs) on an ATM interface."
    INDEX   { ifIndex,  adnAtmVpiVccIndex }
    ::= { adnAtmVpiVccConfTable  1 }

AdnAtmVpiVccConfEntry ::=
    SEQUENCE {
        adnAtmVpiVccIndex
            INTEGER,

        adnAtmVpiVccTrmBandwidth
            INTEGER,

        adnAtmVpiVccRcvBandwidth
            INTEGER,

        adnAtmVpiVccMaxFillingRate
            INTEGER,

        adnAtmVpiVccMaxVcc
            INTEGER,

        adnAtmVpiVccMaxPVccs
            INTEGER,

        adnAtmVpiVccPvccLowVci
            INTEGER,

        adnAtmVpiVccPvccHighVci
            INTEGER,

        adnAtmVpiVccMaxSVccs
            INTEGER,

        adnAtmVpiVccSvccAllocOrd
            AdnAtmAllocOrder,

        adnAtmVpiVccSvccLowVci
            INTEGER,

        adnAtmVpiVccSvccHighVci
            INTEGER
    }

adnAtmVpiVccIndex OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
           "The value of the Virtual Path Identifier (VPI) dedicated to
        Virtual Channel Connections (VCCs) on the ATM interface."
    ::= { adnAtmVpiVccConfEntry  1 }

adnAtmVpiVccTrmBandwidth OBJECT-TYPE
    SYNTAX  INTEGER	-- UNITS bit/secs

    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The bandwidth limitation  (if  any)  of  the  Virtual  Path
        Identifier (VPI) dedicated to Virtual  Channel  Connections
        (VCCs) in the transmit direction.
        If both adnAtmVpiVccTrmBandwidth and adnAtmVpiVccRcvBandwidth
        are set to zero, the bandwidth limitation is the  interface
        bandwidth."
    ::= { adnAtmVpiVccConfEntry  2 }

adnAtmVpiVccRcvBandwidth OBJECT-TYPE
    SYNTAX  INTEGER	-- UNITS bit/secs

    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The bandwidth limitation  (if  any)  of  the  Virtual  Path
        Identifier (VPI) dedicated to Virtual  Channel  Connections
        (VCCs)    in     the     receiving   direction. If     both
        adnAtmVpiVccRcvBandwidth and  adnAtmVpiVccTrmBandwidth  are
        '0', the bandwidth limitation is the interface bandwidth."
    ::= { adnAtmVpiVccConfEntry  3 }

adnAtmVpiVccMaxFillingRate OBJECT-TYPE
    SYNTAX  INTEGER	-- UNITS percentage

    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The bandwidth filling rate of the Virtual  Path  Identifier
        (VPI) dedicated to Virtual Channel Connections (VCCs)."
    ::= { adnAtmVpiVccConfEntry  4 }

