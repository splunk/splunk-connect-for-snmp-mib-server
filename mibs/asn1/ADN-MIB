            Counter,

        adnsonetTCCorrectedHEC
            Counter,

        adnsonetTCUnCorrectedHEC
            Counter,

        adnsonetTCUpBufferOverflow
            Counter,

        adnsonetTCDownBufferOverflow
            Counter,

        adnsonetTCETtoLTLoss
            Counter,

        adnsonetTCConfigCRD
            INTEGER
    }

adnsonetTCStatus OBJECT-TYPE
    SYNTAX  INTEGER (-2147483648..2147483647)
    ACCESS  read-only
    STATUS  obsolete
    DESCRIPTION
           "This variable indicates the status
                 of the interface.
                 It is a bit map represented as a sum;
                 therefore, it can represent multiple
                 defects simultaneously.
                 The adnsonetTCNoDefect bit should
                 be set only if no other bit is set.

                 The bit positions are:
                   1   adnsonetTCNoDefect
                   2   adnsonetTCOCD
                   4   adnsonetTCCorrectedHEC
                   8   adnsonetTCUnCorrectedHEC
                  16   adnsonetTCUpBufferOverflow
                  32   adnsonetTCDownBufferOverflow
                  64   adnsonetTCETtoLTLoss"
    ::= { adnsonetTCEntry  1 }

adnsonetTCOCD OBJECT-TYPE
    SYNTAX  Counter	-- UNITS milliseconds

    ACCESS  read-only
    STATUS  obsolete
    DESCRIPTION
           "Duration of out-of-cell delineation
                 (OCD). 
	         Counter #16 of the ATM core."
    ::= { adnsonetTCEntry  2 }

adnsonetTCCorrectedHEC OBJECT-TYPE
    SYNTAX  Counter	-- UNITS cells

    ACCESS  read-only
    STATUS  obsolete
    DESCRIPTION
           "Number of cells corrected through Header
                 Error Control (HEC) mechanisms.
	         Counter #17 of the ATM core."
    ::= { adnsonetTCEntry  3 }

adnsonetTCUnCorrectedHEC OBJECT-TYPE
    SYNTAX  Counter	-- UNITS cells

    ACCESS  read-only
    STATUS  obsolete
    DESCRIPTION
           "Number of cells discarded through Header
                  Error Control (HEC) mechanisms.
	          Counter #18 of the ATM core."
    ::= { adnsonetTCEntry  4 }

adnsonetTCUpBufferOverflow OBJECT-TYPE
    SYNTAX  Counter
    ACCESS  read-only
    STATUS  obsolete
    DESCRIPTION
           "Number of upward (from remote equipment)
                cell buffer overflow events.
	        Counter #22 of the ATM core."
    ::= { adnsonetTCEntry  5 }

adnsonetTCDownBufferOverflow OBJECT-TYPE
    SYNTAX  Counter
    ACCESS  read-only
    STATUS  obsolete
    DESCRIPTION
           "Number of downward (to remote equipment)
		cell buffer overflow events.
	        Counter #26 of the ATM core."
    ::= { adnsonetTCEntry  6 }

adnsonetTCETtoLTLoss OBJECT-TYPE
    SYNTAX  Counter	-- UNITS milliseconds

    ACCESS  read-only
    STATUS  obsolete
    DESCRIPTION
           "Duration of loss of ATM signal from 
                exchange termination processor (ET) to
                linetermination processor (LT).
	        Counter #25 of the ATM core."
    ::= { adnsonetTCEntry  7 }

adnsonetTCConfigCRD OBJECT-TYPE
    SYNTAX  INTEGER {
    adnsonetTCUnassignedCell(1),
    adnsonetTCIdleCell(2)
}
    ACCESS  read-write
    STATUS  obsolete
    DESCRIPTION
           "The type of cell rate decoupling (CRD) mode
                  selected for the line:
                    - unassigned cells (ANSI)
                    - idle cells (ITU).
                  Common Name: Cell Rate Decoupling"
    ::= { adnsonetTCEntry  8 }

