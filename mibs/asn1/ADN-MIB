adnPmdDumpFileReady TRAP-TYPE
    ENTERPRISE  { adnTrapsCommon }
    VARIABLES   { adnNodeTime,  adnTrapCategory,  adnPmdDumpFilePathName,  adnPmdNodeSurveillance,  adnPmdDumpExecutionReport,  adnPmdFrequencyCount,  adnPmdTargetIdentification }
    DESCRIPTION
           "The main PMD application sends this trap to the
                 NMS to indicate that a PMD file is ready for collection.
                 The full name of the PMD file is given. If
                 adnPmdDumpExecutionReport has the value `alreadyBuilt',
                 a PMD file already exists for the same fault, and the
                 requested PMD has not been built. This fault has occurred
                 for the adnPmdFrequencyCount time(s).
                 adnPmdNodeSurveillance is set if the file has to be 
                 picked up by the NMS. This variable can be modified only
                 by the NMS.
                 adnPmdTargetIdentification indicates the chassis, slot
                 and processor where the PMD has been built."
    ::= 11

adnPmdFailed TRAP-TYPE
    ENTERPRISE  { adnTrapsCommon }
    VARIABLES   { adnNodeTime,  adnTrapCategory,  adnPmdDumpExecutionReport,  adnPmdFailedDiagnostic,  adnPmdNodeSurveillance,  adnPmdTargetIdentification }
    DESCRIPTION
           "The main PMD application sends this trap to the NMS to
                 indicate that a PMD has failed. If the PMD started and
                 failed during execution, adnPmdDumpExecutionReport
                 returns an error code of 'aborted'. If the PMD never
                 started, then adnPmdDumpExecutionReport returns error
                 code 'nok' (not okay).
                 adnPmdNodeSurveillance is set if the file has to be
                 picked up by the NMS. This variable can be modified only
                 by the NMS. This variable is not relevant for the trap
                 adnPmdFailed because the file has not been built.
                 adnPmdTargetIdentification indicates the chassis, slot
                 and processor where the PMD has failed."
    ::= 12

adnPmdMscCrashIndication TRAP-TYPE
    ENTERPRISE  { adnTrapsCommon }
    VARIABLES   { adnNodeTime,  adnTrapCategory,  adnPmdMscCrashClass,  adnPmdMscCrashOrigin,  adnPmdMscCrashTime,  adnPmdTargetIdentification }
    DESCRIPTION
           "The main PMD sub-system sends this trap to the NMS to
                 indicate that a crash occurred on a MSC (Medium Speed
                 Coupler).
                 This trap is only a crash quick report. To get more
                 information, the operator has to check the PMD file (if
                 it has been authorized to be created : see adnPmdMib)."
    ::= 20

adnPmdDumpFilePathName OBJECT-TYPE
    SYNTAX  DisplayString
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
           "PMD pathname built in the NMS transfer directory."
    ::= { adnTrapVars  21 }

adnPmdDumpExecutionReport OBJECT-TYPE
    SYNTAX  INTEGER {
    ok(1),
    aborted(2),
    alreadyBuilt(3),
    nok(4)
}
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
           "If the PMD has actually been built, it is ok(1). If it
                 has not been built because the filter mechanism knows that
                 a PMD was previously built for the same fault, it is
                 alreadyBuilt(3), and the file name is filled with the
                 known value."
    ::= { adnTrapVars  22 }

adnPmdFailedDiagnostic OBJECT-TYPE
    SYNTAX  INTEGER {
    other(1),
    badMessageReceived(2),
    noDataToDump(3),
    softwareInconsistency(4),
    cannotOpenFile(5),
    writeError(6),
    unixMoveError(7),
    unixCompressError(8),
    nectarMaintUnav(9),
    nectarMaintErrorReport(10),
    timeOut(11)
}
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
           "The diagnostic is sent to explain why the PMD
                 failed. The significance of each value is
                 described in the document 3CB00572AAAAUBZZA."
    REFERENCE
           "The diagnostic is sent to explain why the PMD
                 failed. The significance of each value is
                 described in the document 3CB00572AAAAUBZZA."
    ::= { adnTrapVars  23 }

adnPmdFrequencyCount OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
           "This variable indicates how often a particular
                 fault is occurring."
    ::= { adnTrapVars  24 }

adnPmdTargetIdentification OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
           "This variable indicates the processor experiencing      
                 the fault.
                 The variable is expressed with 5 digits.
                 The first one gives the chassis number,
                 the next two digits give the slot number and
                 the two last digits give the processor number.
                 So, the value CSSPP means that the PMD has been
                 generated by the processor PP of the slot SS of
                 the chassis C."
    ::= { adnTrapVars  25 }

adnPmdMscCrashClass OBJECT-TYPE
    SYNTAX  INTEGER {
    unknown(-1),
    stop(0),
    str(1),
    system(2),
    other(3)
}
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
           "This field contains the class of the crash:
                 - unknown(-1): the processor has been reset by an
                 .              external cause.
                 - stop(0)    : the reset has been asked by an process.
                 - str(1)     : a fatal str occurred.
                 - system(2)  : an exception or a system panic 
                 .              occurred.
                 - other(3)   : other."
    ::= { adnTrapVars  34 }

adnPmdMscCrashOrigin OBJECT-TYPE
    SYNTAX  DisplayString
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
           "This field depends on the adnPmdMscCrashClass.
                 If adnPmdMscCrashClass is 'str' then the str
                 name is given.
                 If adnPmdMscCrashClass is 'stop' the process
                 name is given.
                 If adnPmdMscCrashClass is 'system' the name of
                 running process when the fault occurred is given."
    ::= { adnTrapVars  35 }

adnPmdMscCrashTime OBJECT-TYPE
    SYNTAX  AdnNodeTimeTicks
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
           "This field is used to give the time the crash
                 occurred at the MSC (Medium Speed Coupler) level."
    ::= { adnTrapVars  36 }

