adnAtmTCTable OBJECT-TYPE
    SYNTAX  SEQUENCE OF AdnAtmTCEntry
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
           "The ADN Transmission Convergence (TC) 
                extension table."
    ::= { adnAtmMIBObjects  50 }

adnAtmTCEntry OBJECT-TYPE
    SYNTAX  AdnAtmTCEntry
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
           "An entry in the ADN Transmission 
                Convergence (TC) extension table."
    INDEX   { ifIndex }
    ::= { adnAtmTCTable  1 }

AdnAtmTCEntry ::=
    SEQUENCE {
        adnAtmTCAlarmState
            INTEGER (-2147483648..2147483647),

        adnAtmTCScramblerConfig
            INTEGER,

        adnAtmTCCellRateDecConfig
            INTEGER,

        adnAtmTCMappingConfig
            INTEGER,

        adnAtmTCLossOfCellDelCounter
            Counter,

        adnAtmTCCorrectedHecCounter
            Counter,

        adnAtmTCUncorrectedHecCounter
            Counter,

        adnAtmTCUpwardOverflowCounter
            Counter,

        adnAtmTCDownwardOverflowCounter
            Counter,

        adnAtmTCLossOfAtmSignalCounter
            Counter
    }

adnAtmTCAlarmState OBJECT-TYPE
    SYNTAX  INTEGER (-2147483648..2147483647)
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "This variable indicates the state
            of alarms of the Transmission and
            Convergence part of ATM layer.
            It includes information given by
            atmTCAlarmState.
            It is a bit map represented as a sum;
            therefore, it can represent multiple
            defects simultaneously.
            The adnAtmTCNoAlarm bit should
            be set only if no other bit is set.

            The bit positions and their corresponding
            ATM core counter numbers # are:

    #     Bit     Value 
    ------------------------------------------------------
      
    .      1        adnAtmTCNoAlarm        
    .                No alarm present
      
    17     2        adnAtmTCCorrectedHec
    .                Excessive number of cells corrected
    .                through Header Error Control (HEC)
    
    18     4         adnAtmTCUncorrectedHec
    .                Excessive number of cells deleted
    .                through Header Error Control (HEC)
    
    22     8        adnAtmTCUpwardOverflow
    .                Upwards cell buffer overflow
   
    26     16       adnAtmTCDownwardOverflow
    .                Downwards cell buffer overflow
    
    25     32       adnAtmTCLossOfAtmSignalCounter
    .	             Loss of ATM signal from the ET part
    .	             to the LT part of the board"
    ::= { adnAtmTCEntry  1 }

adnAtmTCScramblerConfig OBJECT-TYPE
    SYNTAX  INTEGER {
    adnAtmTCScramblerOn(1),
    adnAtmTCScramblerOff(2)
}
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "Defines if ATM scrambler is active or not."
    ::= { adnAtmTCEntry  2 }

adnAtmTCCellRateDecConfig OBJECT-TYPE
    SYNTAX  INTEGER {
    adnAtmTCUnassignedCell(1),
    adnAtmTCIdleCell(2),
    adnAtmTCNoCRD(3)
}
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The type of cell rate decoupling (CRD) mode
                  selected for the ATM TC layer:
                    - unassigned cells (ANSI),
                    - idle cells (ITU),
                    - no CRD for an ATM TC layer 
                      over an ATM internal line.
                  Common Name: Cell Rate Decoupling"
    ::= { adnAtmTCEntry  3 }

adnAtmTCMappingConfig OBJECT-TYPE
    SYNTAX  INTEGER {
    adnAtmTCOtherMapping(1),
    adnAtmTCPlcpMapping(2),
    adnAtmTCDirectMapping(3)
}
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The type of mapping of ATM layer
                  on physical layer:
                    - other mapping : the mapping of the
                      ATM layer is none of the following
                      mappings,
                    - PLCP mapping : the ATM layer is
                      mapped on Physical Layer Convergence
                      Protocol layer (PLCP),
                    - direct mapping : the ATM layer is
                      directly mapped on transmission layer."
    ::= { adnAtmTCEntry  4 }

adnAtmTCLossOfCellDelCounter OBJECT-TYPE
    SYNTAX  Counter	-- UNITS milliseconds

    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "Duration of loss-of-cell-delineation
                 (LOCD).
                 Counter #16 of the ATM core, only 
                 available on ATM-SONET, ATM-SDH,
                 ATM-DS3-DM, ATM-E3, ATM-T1 and ATM-E1
                 interfaces."
    ::= { adnAtmTCEntry  5 }

adnAtmTCCorrectedHecCounter OBJECT-TYPE
    SYNTAX  Counter	-- UNITS cells

    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "Number of cells corrected through Header
                 Error Control (HEC) mechanisms.
                 Counter #17 of the ATM core, only 
                 available on ATM interfaces over lines  
                 on BBTP3C, BBE3B, BBE3B3,
                 BBS1B, BBS1B3, BBS1B2, BBS1B4,
                 BBS4B or BBS4B2 board."
    ::= { adnAtmTCEntry  6 }

adnAtmTCUncorrectedHecCounter OBJECT-TYPE
    SYNTAX  Counter	-- UNITS cells

    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "Number of cells discarded through Header
                 Error Control (HEC) mechanisms.
                 Counter #18 of the ATM core, only 
                 available on ATM-SONET, ATM-SDH, ATM-DS3,
                 ATM-E3, ATM-T1 and ATM-E1 interfaces."
    ::= { adnAtmTCEntry  7 }

adnAtmTCUpwardOverflowCounter OBJECT-TYPE
    SYNTAX  Counter
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "Number of upward cell buffer (output
                 buffer) overflow events.
                 Counter #22 of the ATM core, only 
                 available on ATM interfaces over lines  
                 on BBT1B, BBE1B, BBE3B, BBE3B3,
                 BBT3B, BBT3B2, BBT3B3, BBT3B4,
                 BBS1B, BBS1B3, BBS1B2, BBS1B4,
                 BBS4B or BBS4B2 board."
    ::= { adnAtmTCEntry  8 }

adnAtmTCDownwardOverflowCounter OBJECT-TYPE
    SYNTAX  Counter
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "Number of downward cell buffer (input
                 buffer) overflow events.
                 Counter #26 of the ATM core, only 
                 available on ATM-SONET, ATM-SDH, ATM-DS3,
                 and ATM-E3 interfaces."
    ::= { adnAtmTCEntry  9 }

adnAtmTCLossOfAtmSignalCounter OBJECT-TYPE
    SYNTAX  Counter	-- UNITS milliseconds

    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "Duration of loss of ATM signal from 
                 the Exchange Termination processor
                 (ET) to the Line Termination
                 processor (LT).
                 Counter #31 of the ATM core, only 
                 available on ATM interfaces over lines  
                 on BBS1B, BBS1B3, BBS1B2, or BBS1B4 board."
    ::= { adnAtmTCEntry  10 }

adnAtmDs3PlcpTable OBJECT-TYPE
    SYNTAX  SEQUENCE OF AdnAtmDs3PlcpEntry
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
           "The ADN DS3 Physical Layer Convergence
                Protocol (PLCP) extension table."
    ::= { adnAtmMIBObjects  51 }

adnAtmDs3PlcpEntry OBJECT-TYPE
    SYNTAX  AdnAtmDs3PlcpEntry
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
           "An entry in the ADN DS3 Physical Layer
                Convergence Protocol (PLCP) extension
                table."
    INDEX   { ifIndex }
    ::= { adnAtmDs3PlcpTable  1 }

AdnAtmDs3PlcpEntry ::=
    SEQUENCE {
        adnAtmDs3PlcpFerfCounter
            Counter,

        adnAtmDs3PlcpLofCounter
            Counter,

        adnAtmDs3PlcpFebeCounter
            Counter,

        adnAtmDs3PlcpBip8Counter
            Counter,

        adnAtmDs3PlcpOofCounter
            Counter
    }

adnAtmDs3PlcpFerfCounter OBJECT-TYPE
    SYNTAX  Counter	-- UNITS 450-milliseconds periods

    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "Duration of far end received 
                 failure (FERF) received from the
                 remote equipment.
                 FERF is also called remote alarm
                 indication (RAI) or received yellow
                 alarm.
                 Counter #10 of the ATM core, only 
                 available on DS3-PLCP interfaces."
    ::= { adnAtmDs3PlcpEntry  1 }

adnAtmDs3PlcpLofCounter OBJECT-TYPE
    SYNTAX  Counter	-- UNITS 450-milliseconds periods

    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "Duration of loss-of-frame (LOF).
                 Counter #12 of the ATM core, only 
                 available on DS3-PLCP interfaces."
    ::= { adnAtmDs3PlcpEntry  2 }

adnAtmDs3PlcpFebeCounter OBJECT-TYPE
    SYNTAX  Counter
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "Number of far end block errors (FEBE)
                 received from the remote equipment.
                 Counter #9 of the ATM core, only 
                 available on DS3-PLCP interfaces."
    ::= { adnAtmDs3PlcpEntry  3 }

adnAtmDs3PlcpBip8Counter OBJECT-TYPE
    SYNTAX  Counter
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "Number of 8-bits interleaved parity
                 (BiP-8) errors.
                 received from the remote equipment.
                 Counter #11 of the ATM core, only 
                 available on DS3-PLCP interfaces."
    ::= { adnAtmDs3PlcpEntry  4 }

adnAtmDs3PlcpOofCounter OBJECT-TYPE
    SYNTAX  Counter
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "Number of out-of-frame (OOF) events.
                 Counter #20 of the ATM core, only 
                 available on DS3-PLCP interfaces."
    ::= { adnAtmDs3PlcpEntry  5 }

adnAtmBoardErCounterTable OBJECT-TYPE
    SYNTAX  SEQUENCE OF AdnAtmBoardErCounterEntry
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
           "This table is the ATM boards error counter table.
        Each counter corresponds to a defect number
        identified by a bit set in the adnProcessorStatus
        of the adnProcessorTable.
        The counters are classified according to the board
        type. Some are specific to the broadband ATM
        switching element (BBASE) board also called the
        matrix board."
    ::= { adnNodeSupervisionMIB  21 }

adnAtmBoardErCounterEntry OBJECT-TYPE
    SYNTAX  AdnAtmBoardErCounterEntry
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
           "An entry in the ATM boards error counter table."
    INDEX   { adnEquipType,  adnEquipIndex }
    ::= { adnAtmBoardErCounterTable  1 }

