adnCalcMonTable OBJECT-TYPE
    SYNTAX  SEQUENCE OF AdnCalcMonEntry
    ACCESS  not-accessible
    STATUS  deprecated
    DESCRIPTION
           "General parameters used by the routing
	services for node-to-path table calculation."
    ::= { adnRoutesMIB  1 }

adnCalcMonEntry OBJECT-TYPE
    SYNTAX  AdnCalcMonEntry
    ACCESS  not-accessible
    STATUS  deprecated
    DESCRIPTION
           "Each entry (row) of the adnCalcMonTable 
	 provides the general parameters used by '
	 the routing services for node-to-path 
	 table calculation for a specific type of 
	 service and criterion."
    INDEX   { adntosIndex,  adncriterionIndex }
    ::= { adnCalcMonTable  1 }

AdnCalcMonEntry ::=
    SEQUENCE {
        adntosIndex
            INTEGER,

        adncriterionIndex
            INTEGER,

        adnMaxPath
            INTEGER,

        adnMaxCost
            INTEGER (-2147483648..2147483647),

        adnMinTimeBetCalc
            INTEGER (-2147483648..2147483647)
    }

adntosIndex OBJECT-TYPE
    SYNTAX  INTEGER {
    other(1),
    fr(2),
    atmVc(3),
    atmVp(4)
}	-- UNITS Type of service

    ACCESS  not-accessible
    STATUS  deprecated
    DESCRIPTION
           "Type of service: frame relay, ATM virtual 
	 channel, ATM virtual path, etc."
    ::= { adnCalcMonEntry  1 }

adncriterionIndex OBJECT-TYPE
    SYNTAX  INTEGER {
    other(1),
    hop(2),
    delay(3)
}	-- UNITS Criterion index

    ACCESS  not-accessible
    STATUS  deprecated
    DESCRIPTION
           "Rule used to generate a routing table 
	 based on the link cost metrics."
    ::= { adnCalcMonEntry  2 }

adnMaxPath OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  read-only
    STATUS  deprecated
    DESCRIPTION
           "Maximum number of paths per node in the
	node-to-path routing table for a specific 
	type of service and criterion."
    DEFVAL  { 5 }
    ::= { adnCalcMonEntry  3 }

adnMaxCost OBJECT-TYPE
    SYNTAX  INTEGER (-2147483648..2147483647)
    ACCESS  read-only
    STATUS  deprecated
    DESCRIPTION
           "Maximum cost of a path to an arbitrary 
	 destination node for a type of service 
	 and criterion. If the criterion is the 
	 number of hops, maximum number of hops.
	 The default value is defined for a criterion 
	 equal to the number of hops."
    DEFVAL  { 10 }
    ::= { adnCalcMonEntry  4 }

adnMinTimeBetCalc OBJECT-TYPE
    SYNTAX  INTEGER (-2147483648..2147483647)	-- UNITS seconds

    ACCESS  read-write
    STATUS  deprecated
    DESCRIPTION
           "Minimum time between successive route 
	 calculations for a specific type of 
	 service and criterion and identifies 
	 the minimum time between completion 
	 of routing services initialization 
	 and the first routing calculation."
    DEFVAL  { 30 }
    ::= { adnCalcMonEntry  5 }

adnMessMonTable OBJECT-TYPE
    SYNTAX  SEQUENCE OF AdnMessMonEntry
    ACCESS  not-accessible
    STATUS  deprecated
    DESCRIPTION
           "General parameters used by the routing
	services for routing message handling."
    ::= { adnRoutesMIB  2 }

