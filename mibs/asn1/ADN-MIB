    SYNTAX  INTEGER (-2147483648..2147483647)
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
           "Equal to the DLCI (data link connection identifier) value
         for the frame relay trunk or FRS (frame relay subsystem)
         local DLCI."
    ::= { adnCongMgmtVCEntry  1 }

adnCongMgmtVCBcInDiscards OBJECT-TYPE
    SYNTAX  Counter
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "Number of Bc (committed data rate) sub-frames that were 
         discarded for ingress traffic (TPX interworking
         function, frame relay switching interface, or frame relay
         subsystem) due to ISM (internal switching mechanism) 
         queue congestion."
    ::= { adnCongMgmtVCEntry  2 }

adnCongMgmtVCBcsInDiscards OBJECT-TYPE
    SYNTAX  Counter
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "Number of Bcs (statistically committed data rate) 
         sub-frames that were discarded for ingress traffic
         (TPX interworking function, frame relay switching  
         interface, or frame relay subsystem) due to
         ISM (internal switching mechanism) queue congestion."
    ::= { adnCongMgmtVCEntry  3 }

adnCongMgmtVCBeInDiscards OBJECT-TYPE
    SYNTAX  Counter
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "Number of Be (excess burst data rate) sub-frames that
         were discarded for ingress traffic (TPX interworking 
         function, frame relay switching interface, or frame relay
         subsystem) due to ISM (internal switching mechanism) 
         queue congestion."
    ::= { adnCongMgmtVCEntry  4 }

adnCongMgmtVCInBcSubfrmSent OBJECT-TYPE
    SYNTAX  Counter
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "Number of Bc (committed data rate) sub-frames sent to the
         ISM (internal switching mechanism)."
    ::= { adnCongMgmtVCEntry  5 }

adnCongMgmtVCInBcsSubfrmSent OBJECT-TYPE
    SYNTAX  Counter
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "Number of Bcs (statistically committed data rate)
         sub-frames sent to the ISM (internal switching mechanism)."
    ::= { adnCongMgmtVCEntry  6 }

