    SYNTAX  AdnEnableDisabled
    ACCESS  read-write
    STATUS  mandatory
    DESCRIPTION
           "This indicates if accounting is enabled for the VCs on
        this channel.
        Common Name: Accounting"
    DEFVAL  { enabled }
    ::= { adnFrChanEntry  2 }

adnFrChanStatistics OBJECT-TYPE
    SYNTAX  AdnEnableDisabled
    ACCESS  read-write
    STATUS  mandatory
    DESCRIPTION
           "This indicates if statistics are enabled for this
        channel. When enabled, performance information is
        reported to the NMS.
        Common Name: Statistics"
    DEFVAL  { enabled }
    ::= { adnFrChanEntry  3 }

