    SYNTAX  INTEGER (-2147483648..2147483647)	-- UNITS seconds

    ACCESS  read-only
    STATUS  deprecated
    DESCRIPTION
           "Minimum time between self-generated 
	 link state messages for the node."
    DEFVAL  { 2 }
    ::= { adnMessMonEntry  1 }

adnbroadcastLSU OBJECT-TYPE
    SYNTAX  INTEGER {
    other(1),
    active(2),
    inactive(3)
}
    ACCESS  read-write
    STATUS  deprecated
    DESCRIPTION
           "If active, a trigger for a one shot link 
	 state update broadcast for this node. The 
	 status is reset to inactive when done."
    DEFVAL  { 3 }
    ::= { adnMessMonEntry  2 }

adnLSUbroadcastfreez OBJECT-TYPE
    SYNTAX  INTEGER {
    other(1),
    active(2),
    inactive(3)
}
    ACCESS  read-write
    STATUS  deprecated
    DESCRIPTION
           "Status of the link state update processing. 
	When the status is active, no link 
        state update for the node is broadcast for a
        period of time."
    DEFVAL  { 3 }
    ::= { adnMessMonEntry  3 }

adnTblDistfreez OBJECT-TYPE
    SYNTAX  INTEGER {
    other(1),
    active(2),
    inactive(3)
}
    ACCESS  read-write
    STATUS  deprecated
    DESCRIPTION
           "Status of the table distribution processor. 
	 When the status is active, the table 
	 distributions sent to a coupler card are frozen."
    DEFVAL  { 3 }
    ::= { adnMessMonEntry  4 }

