    ::= { adnAtmSigEntry  33 }

adnAtmSigVpiVciAlloc OBJECT-TYPE
    SYNTAX  AdnEnableDisabled
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "Whether the VPi and VCi number are inserted  in  the  Setup
        request message. This object is significant only  when  the
        switch  follows  the   network   side   of   the   protocol
        recommendations.
        This object is significant  only  if  adnAtmSigProtocol  is
        'atmfv40'.
        The not significant value is 'disabled'."
    ::= { adnAtmSigEntry  34 }

adnAtmSigCniCheckingState OBJECT-TYPE
    SYNTAX  INTEGER {
    notSignificant(1),
    ok(2),
    nokUserNet(3),
    nokCanp(4),
    nokSaal(5),
    nokConf(6),
    notYetPerformed(65535)
}
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The current status of the validity checking phase which  is
        performed when setting an ATM trunk into service.
        This  read-only  nominator  is  significant  only  if   the
        associated ATM interface is a trunk.
        Common Name: CNI validity checking"
    ::= { adnAtmSigEntry  35 }

adnAtmSigSetupAddr OBJECT-TYPE
    SYNTAX  INTEGER {
    notSignificant(1),
    calledPartyNumber(2),
    calledPartySubAddress(3),
    both(4)
}
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "This nominator defines which addressing Information Element
        (IE) is used by the switch to forward a Setup received.

        - 'calledPartyNumber': The called party number is  used  to
          forward the call, this is the normal case

        - 'calledPartySubAddress': The called party  subaddress  is
          used to forward the call as defined in E191  standard. In
          this case, the switch replaces the  called  party  number
          received by the called party subaddress and  forwards the
          call with it. If no called party subaddress is  received,
          the call is rejected

        - 'both': Depending on the content of the  Setup  received,
          the call is forwarded either with the:

          .  called party  subaddress  (if  present  in  the  Setup
             received)

          .  called party number if the call  party  subaddress  is
             not present in the Setup received."
    REFERENCE
           "This nominator defines which addressing Information Element
        (IE) is used by the switch to forward a Setup received.

        - 'calledPartyNumber': The called party number is  used  to
          forward the call, this is the normal case

        - 'calledPartySubAddress': The called party  subaddress  is
          used to forward the call as defined in E191  standard. In
          this case, the switch replaces the  called  party  number
          received by the called party subaddress and  forwards the
          call with it. If no called party subaddress is  received,
          the call is rejected

        - 'both': Depending on the content of the  Setup  received,
          the call is forwarded either with the:

          .  called party  subaddress  (if  present  in  the  Setup
             received)

          .  called party number if the call  party  subaddress  is
             not present in the Setup received."
    ::= { adnAtmSigEntry  36 }

adnAtmSigConnectScreening OBJECT-TYPE
    SYNTAX  AdnAtmSigScreening
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "Whether the called party number Information Element (IE) of
        a   Connect   message   is   checked   according   to   the
        administrative address associated with the interface"
    ::= { adnAtmSigEntry  37 }

adnAtmVplTable OBJECT-TYPE
    SYNTAX  SEQUENCE OF AdnAtmVplEntry
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
           "This is an extension of the Virtual Path (VP)  link  table,
        described in RFC 1695."
    ::= { adnAtmMIBObjects  6 }

adnAtmVplEntry OBJECT-TYPE
    SYNTAX  AdnAtmVplEntry
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
           "An entry in the extension to the  Virtual  Path  (VP)  link
        table. There is one entry for each VP link."
    INDEX   { ifIndex,  atmVplVpi }
    ::= { adnAtmVplTable  1 }

AdnAtmVplEntry ::=
    SEQUENCE {
        adnAtmVplTopology
            INTEGER,

        adnAtmVplTrafficType
            INTEGER,

        adnAtmVplType
            AdnAtmVlType,

        adnAtmVplTransferCapability
            AdnAtmVlTrfCapability,

        adnAtmVplTrafficDescrType
            INTEGER,

        adnAtmVplTrmPcrAggregate
            INTEGER,

        adnAtmVplTrmPcrClpZero
            INTEGER,

        adnAtmVplTrmScrAggregate
            INTEGER,

        adnAtmVplTrmScrClpZero
            INTEGER,

        adnAtmVplTrmMbsAggregate
            INTEGER,

        adnAtmVplTrmMbsClpZero
            INTEGER,

        adnAtmVplTrmCdvTol
            INTEGER,

        adnAtmVplTrmBwWeighting
            INTEGER,

        adnAtmVplRcvPcrAggregate
            INTEGER,

        adnAtmVplRcvPcrClpZero
            INTEGER,

        adnAtmVplRcvScrAggregate
            INTEGER,

        adnAtmVplRcvScrClpZero
            INTEGER,

        adnAtmVplRcvMbsAggregate
            INTEGER,

        adnAtmVplRcvMbsClpZero
            INTEGER,

        adnAtmVplRcvCdvTol
            INTEGER,

        adnAtmVplRcvBwWeighting
            INTEGER,

        adnAtmVplPolicing
            AdnAtmVlPolicing,

        adnAtmVplPriority
            INTEGER,

        adnAtmVplModeType
            INTEGER,

        adnAtmVplTrmAISStatus
            AdnAtmVlAISStatus,

        adnAtmVplTrmTrafficDescrType
            AdnAtmVlTrfDescrType,

        adnAtmVplTrmTagging
            AdnEnableDisabled,

        adnAtmVplTrmPacketDiscard
            AdnEnableDisabled,

        adnAtmVplRcvAISStatus
            AdnAtmVlAISStatus,

        adnAtmVplRcvTrafficDescrType
            AdnAtmVlTrfDescrType,

        adnAtmVplRcvTagging
            AdnEnableDisabled,

        adnAtmVplRcvPacketDiscard
            AdnEnableDisabled,

        adnAtmVplInternodeType
            INTEGER
    }

adnAtmVplTopology OBJECT-TYPE
    SYNTAX  INTEGER {
    pointToPoint(1),
    pointToMultipoint(2),
    pointToMultipointRoot(3),
    pointToMultipointLeaf(4)
}
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The topology type of this Virtual Path Link (VPL).
        Common Name: Topology"
    ::= { adnAtmVplEntry  1 }

adnAtmVplTrafficType OBJECT-TYPE
    SYNTAX  INTEGER {
    userTraffic(1),
    networkTraffic(2)
}
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The type of traffic  carried  on  this  Virtual  Path  Link
        (VPL).
        Common Name: Traffic Type"
    ::= { adnAtmVplEntry  2 }

adnAtmVplType OBJECT-TYPE
    SYNTAX  AdnAtmVlType
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The type of this Virtual Path Link (VPL).
        Common Name: VPL Type"
    ::= { adnAtmVplEntry  3 }

adnAtmVplTransferCapability OBJECT-TYPE
    SYNTAX  AdnAtmVlTrfCapability
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The ATM Transfer Capabilities (ATC) offered on this Virtual
        Path Link (VPL)."
    ::= { adnAtmVplEntry  4 }

adnAtmVplTrafficDescrType OBJECT-TYPE
    SYNTAX  INTEGER {
    adnAtmTrafficDescPcrCdv(1),
    adnAtmTrafficDescPcrCdvScrMbs(2)
}
    ACCESS  read-only
    STATUS  deprecated
    DESCRIPTION
           "The type of Virtual Path Link (VPL) traffic  descriptor  in
        the transmit  and  receiving  direction. Depending  on  the
        value, certain traffic parameters must be initialized:

        - 'adnAtmTrafficDescPcrCdv':

          The following traffic parameters must be initialized:

          adnAtmVplTrmPcrAggregate, adnAtmVplRcvPcrAggregate,
          adnAtmVplTrmCdvTol,       adnAtmVplRcvCdvTol,
          adnAtmVplTrmBwWeighting,  adnAtmVplRcvBwWeighting,

        - 'adnAtmTrafficDescPcrCdvScrMbs':

          The following traffic parameters must be initialized:

          adnAtmVplTrmPcrAggregate, adnAtmVplRcvPcrAggregate,
          adnAtmVplTrmScrAggregate, adnAtmVplRcvScrAggregate,
          adnAtmVplTrmMbsAggregate, adnAtmVplRcvMbsAggregate,
          adnAtmVplTrmCdvTol,       adnAtmVplRcvCdvTol,
          adnAtmVplTrmBwWeighting,  adnAtmVplRcvBwWeighting."
    ::= { adnAtmVplEntry  5 }

adnAtmVplTrmPcrAggregate OBJECT-TYPE
    SYNTAX  INTEGER	-- UNITS cells/sec

    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The Peak Cell Rate (PCR) for  the  aggregate  traffic (Cell
        Loss Priority (CLP) 0+1), in the transmit direction (switch
        point of view), of this Virtual Path Link (VPL).
        Common Names: Transmit Peak Cell Rate (PCR) Aggregate,
                      Transmit PCR Aggregate"
    ::= { adnAtmVplEntry  6 }

