        adnAtmBoardHSCStatus
            INTEGER (-2147483648..2147483647),

        adnAtmBoardErQRP0RAMPCounter
            Counter,

        adnAtmBoardErQRP0FIFOCounter
            Counter,

        adnAtmBoardErQRP1RAMPCounter
            Counter,

        adnAtmBoardErQRP1FIFOCounter
