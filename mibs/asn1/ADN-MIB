adnAtmAccTable OBJECT-TYPE
    SYNTAX  SEQUENCE OF AdnAtmCharEntry
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
           "This table constitutes the ATM charging ticket. None
	of the objects in this table can be obtained via a GET
	command. These objects are visible only in a charging
	ticket.
	Most of these objects duplicate objects in other tables. 
	They are concatenated here to allow the charging ticket
	to be sent as a single INMP/PFXI table to conserve space."
    ::= { adnAtmMIB  2 }

adnAtmAccEntry OBJECT-TYPE
    SYNTAX  AdnAtmCharEntry
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
           "One instance of the ATM charging ticket."
    INDEX   { ifIndex }
    ::= { adnAtmAccTable  1 }

AdnAtmCharEntry ::=
    SEQUENCE {
        adnAtmAccEquipType
            INTEGER,

        adnAtmAccTicketVersion
            INTEGER,

        adnAtmAccLocalNodeId
            INTEGER,

        adnAtmAccLocalChassis
            INTEGER,

        adnAtmAccLocalSlot
            INTEGER,

        adnAtmAccLocalLine
            INTEGER,

        adnAtmAccRemoteNodeId
            INTEGER,

        adnAtmAccRemoteChassis
            INTEGER,

        adnAtmAccRemoteSlot
            INTEGER,

        adnAtmAccRemoteLine
            INTEGER,

        adnAtmAccCallTerm
            INTEGER,

        adnAtmAccInterfaceType
            INTEGER,

        adnAtmAccConnectionType
            INTEGER,

        adnAtmAccConnectionTopology
            INTEGER,

        adnAtmAccSourceDest
            INTEGER,

        adnAtmAccCallingPartyNumPlan
            INTEGER,

        adnAtmAccCallingPartyLength
            INTEGER,

        adnAtmAccCallingPartyNumber
            OCTET STRING,

        adnAtmAccCalledPartyNumPlan
            INTEGER,

        adnAtmAccCalledPartyLength
            INTEGER,

        adnAtmAccCalledPartyNumber
            OCTET STRING,

        adnAtmAccLocalVpi
            INTEGER,

        adnAtmAccLocalVci
            INTEGER,

        adnAtmAccRemVpi
            INTEGER,

        adnAtmAccRemVci
            INTEGER,

        adnAtmAccConnUniqueID
            INTEGER,

        adnAtmAccConnStartTime
            INTEGER,

        adnAtmAccConnDuration
            INTEGER (-2147483648..2147483647),

        adnAtmAccReqTransitDelay
            INTEGER,

        adnAtmAccCumulTransitDelay
            INTEGER,

        adnAtmAccTransferCapability
            INTEGER,

        adnAtmAccTrmTrafficDescrType
            INTEGER,

        adnAtmAccTrmParam1
            INTEGER,

        adnAtmAccTrmParam2
            INTEGER,

        adnAtmAccTrmParam3
            INTEGER,

        adnAtmAccTrmParam4
            INTEGER,

        adnAtmAccTrmTagging
            INTEGER,

        adnAtmAccTrmPacketDiscard
            INTEGER,

        adnAtmAccRcvTrafficDescrType
            INTEGER,

        adnAtmAccRcvParam1
            INTEGER,

        adnAtmAccRcvParam2
            INTEGER,

        adnAtmAccRcvParam3
            INTEGER,

        adnAtmAccRcvParam4
            INTEGER,

        adnAtmAccRcvTagging
            INTEGER,

        adnAtmAccRcvPacketDiscard
            INTEGER,

        adnAtmAccClearingCause
            INTEGER,

        adnAtmAccClearingDiag
            INTEGER (-2147483648..2147483647),

        adnAtmAccVcDescriptor
            OCTET STRING
    }

adnAtmAccEquipType OBJECT-TYPE
    SYNTAX  INTEGER {
    other(1),
    hss700(2),
    hss1000(3)
}
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
           "This is the type of the equipment which generates
	 this charging ticket.
         hss700  : Alcatel 1100 HSS Series 400/700,
         hss1000 : Alcatel 1100 HSS Series 1000.
         Common Name : Equipment Type."
    ::= { adnAtmAccEntry  1 }

adnAtmAccTicketVersion OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
           "This is the identifier of the version (format) of this ATM 
         charging ticket.
         This object is numbered per type of equipment listed in
         adnAtmAccEquipType.
         adnAtmAccTicketVersion = 1 is the first value for 
	 Alcatel 1100 HSS Series 400/700.
         Common Name : Charging Ticket Version."
    ::= { adnAtmAccEntry  2 }

adnAtmAccLocalNodeId OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
           "This is the identifier of the node which generates this 
         charging ticket.
         For HSS, the format of the ADN Node Routing ID is used :
         Bits 31-23 (9 bits)     reserved for future use
         Bits 22-16 ( 7 bits)    area identifier
         Bits 15-0  (16 bits)    machine identifier
         Common Name : Local Node Identifier."
    ::= { adnAtmAccEntry  3 }

adnAtmAccLocalChassis OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
           "This is the chassis number where the interface is located.
         Common Name : Local Chassis"
    ::= { adnAtmAccEntry  4 }

adnAtmAccLocalSlot OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
           "This is the slot number of the interface in the chassis.
         Common Name : Local Slot"
    ::= { adnAtmAccEntry  5 }

adnAtmAccLocalLine OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
           "This is the line number in the slot.
         Common Name : Local Line"
    ::= { adnAtmAccEntry  6 }

adnAtmAccRemoteNodeId OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
           "This is the identifier of the node which supports the
	 connection at the remote side.
         For HSS, the format of the ADN Node Routing ID is used :
         Bits 31-23 (9 bits)     reserved for future use
         Bits 22-16 ( 7 bits)    area identifier
         Bits 15-0  (16 bits)    machine identifier
         Common Name : Remote Node Identifier."
    ::= { adnAtmAccEntry  7 }

adnAtmAccRemoteChassis OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
           "This is the chassis number where the remote interface is
	 located.
         Common Name : Remote Chassis."
    ::= { adnAtmAccEntry  8 }

adnAtmAccRemoteSlot OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
           "This is the slot number of the interface in the remote
	 chassis.
         Common Name : Remote Slot"
    ::= { adnAtmAccEntry  9 }

adnAtmAccRemoteLine OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
           "This is the line number in the remote slot.
         Common Name : Remote Line"
    ::= { adnAtmAccEntry  10 }

adnAtmAccCallTerm OBJECT-TYPE
    SYNTAX  INTEGER {
    other(1),
    terminated(2),
    intermediate(3)
}
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
           "This object indicates a terminated ticket or an intermediate
         charging ticket :
         - terminated   : PVC has been de-configured or disrupted
                          SVC has been cleared (user or network cause)
         - intermediate : periodic collection of PVCs and long SVCs
                          operator initiated collection of PVCs and
	 SVCs.
         Intermediate tickets are for future use.
         
         Common Name : Call Terminated / not Terminated."
    ::= { adnAtmAccEntry  11 }

adnAtmAccInterfaceType OBJECT-TYPE
    SYNTAX  INTEGER {
    externalUni(1),
    nni(2),
    internalUniSig(3),
    virtualUni(4),
    physicalCni(5),
    routlogicalCni(6),
    nonroutlogicalCni(7),
    nonroutlogicalCniendtoend(8),
    internalUni(9)
}
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
           "Type of the ATM interface on which generates this charging
	 ticket.
         Common Name : ATM Interface Type."
    ::= { adnAtmAccEntry  12 }

adnAtmAccConnectionType OBJECT-TYPE
    SYNTAX  INTEGER {
    pvc(1),
    svc(2),
    internalVc(3)
}
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
           "This is the connection type : PVC versus SVC.
         Only the following values can be provided in a HSS charging
	 ticket :
         - pvc, - svc.
         Common Name : Connection Type."
    ::= { adnAtmAccEntry  13 }

adnAtmAccConnectionTopology OBJECT-TYPE
    SYNTAX  INTEGER {
    pointToPoint(1),
    pointToMultipointRoot(3),
    pointToMultipointLeaf(4)
}
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
           "This object describes the characteristics of the connection 
	 at the location where this charging ticket is generated :
         Only the following values can be provided in a HSS charging
	 ticket :
         - pointTopoint.
         Common Name : Connection Topology."
    ::= { adnAtmAccEntry  14 }

adnAtmAccSourceDest OBJECT-TYPE
    SYNTAX  INTEGER {
    source(1),
    destination(2),
    transit(3)
}
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
           "This object indicates the location of the node which 
	 generates this charging ticket :
         - source      : the node receives the SET-UP message from 
                         the calling user.
         - destination : the node transmits the SET-UP message to 
                         the called user.
         - transit     : this value is reserved for the 1000 HSS
	 Series 1000 system (this system may generate charging
	 ticket on trunks for observation purposes).
         Common Name : Source or Destination."
    ::= { adnAtmAccEntry  15 }

adnAtmAccCallingPartyNumPlan OBJECT-TYPE
    SYNTAX  INTEGER {
    private(1),
    nsapE164(2),
    nativeE164(3),
    other(4),
    notSignificant(5)
}
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
           "This is the type of numbering plan of the calling party
	 number.
         Common Name : Numbering Plan, Num Plan."
    ::= { adnAtmAccEntry  16 }

adnAtmAccCallingPartyLength OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
           "This is the number of significant digits in object 
         adnAtmAccCallingParty.
         Common Name : Length of Calling Party Number."
    ::= { adnAtmAccEntry  17 }

