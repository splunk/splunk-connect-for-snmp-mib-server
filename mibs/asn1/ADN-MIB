adnCvsiTscChnlState OBJECT-TYPE
    SYNTAX  INTEGER {
    down(0),
    up(1)
}
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "This entry represents the status of the Tag Switch 
         Controller (TSC) channel. This channel connects 
         the HSS with the Tag Switch Controller (TSC).
         Common Name: TSC Channel State"
    ::= { adnCvsiSwitchInfoGroup  1 }

adnCvsiActiveSesId OBJECT-TYPE
    SYNTAX  INTEGER (-2147483648..2147483647)
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "This object indicates the active session ID of the
         CVSI slave in the HSS. This value changes whenever 
         the slave is restarted, powered up, switched over 
         to or manually reset. The session IDs are used by the 
         Tag Switch Controller (TSC) to detect the loss of 
         synchronization. 
         Common Name: Active Session ID"
    ::= { adnCvsiSwitchInfoGroup  2 }

adnCvsiPwrUpSesId OBJECT-TYPE
    SYNTAX  INTEGER (-2147483648..2147483647)
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "This object indicates the power-up session ID of the
         CVSI slave in the HSS. This value reflects the 
         session ID which was assigned when this slave was
         powered up. This value may be used by the Tag Switch 
         Controller (TSC) for detecting a more serious 
         synchronization loss.
         Common Name: PowerUp Session ID"
    ::= { adnCvsiSwitchInfoGroup  3 }

adnCvsiSlaveId OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The slave ID corresponding to this instance of CVSI 
         slave in HSS. This value is relevant if we have 
         multiple slaves for each Tag Switch Controller (TSC). 
         Common Name: Salve ID"
    ::= { adnCvsiSwitchInfoGroup  4 }

adnCvsiSwitchId OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The switch ID for the HSS in this Tag Switching Router
         (TSR).
         Common Name: Switch ID"
    ::= { adnCvsiSwitchInfoGroup  5 }

adnCvsiControllerId OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The controller ID for the Tag Switch Controller (TSC) 
         in this Tag Switching Router (TSR).
         Common Name: Controller ID"
    ::= { adnCvsiSwitchInfoGroup  6 }

adnCvsiVersionId OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The version of the CVSI code running in this Tag 
         Switching Router (TSR).
         Common Name: Version ID"
    ::= { adnCvsiSwitchInfoGroup  7 }

adnCvsiMaxTrapWindow OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The maximum number of traps which can be pending
         in the CVSI slave in the HSS. The traps will be
         pending if either the Tag Switch controller (TSC) 
         hasn't acknowledged them, or they could not be 
         sent for some reason. 
         Common Name: Max Trap Window"
    ::= { adnCvsiSwitchInfoGroup  8 }

adnCvsiCurrTrapWindow OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The number of pending traps in the CVSI slave in the
         HSS. The traps will be pending if either the Tag 
         Switch Controller (TSC) hasn't acknowledged them, or 
         they could not be sent for some reason. 
         Common Name: Current Trap Window"
    ::= { adnCvsiSwitchInfoGroup  9 }

adnCvsiMaxCmdWindow OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The maximum number of commands which can be pending
         in the CVSI slave in the HSS. A command will be
         pending if it has not been processed by the CVSI 
         slave. This value is conveyed to the Tag Switch 
         controller.
         Common Name: Max Cmd Window"
    ::= { adnCvsiSwitchInfoGroup  10 }

adnCvsiCurrCmdWindow OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The number of pending commands in the CVSI slave in the
         HSS. A command will be pending if it has not been 
         processed by the CVSI slave. 
         Common Name: Current Cmd Window"
    ::= { adnCvsiSwitchInfoGroup  11 }

adnCvsiSpacingState OBJECT-TYPE
    SYNTAX  AdnEnableDisabled
    ACCESS  read-write
    STATUS  mandatory
    DESCRIPTION
           "Indicates whether traffic spacing is performed at all
	 TDP interfaces.  Spacing is used to reduce the peak to 
	 peak Cell Delay Variation (CDV) or to smoooth out bursty 
	 traffic coming into the network or to perform rate 
	 reduction.
         Common Name: Spacing State"
    DEFVAL  { enabled }
    ::= { adnCvsiSwitchInfoGroup  12 }

adnCvsiSpacingRate OBJECT-TYPE
    SYNTAX  INTEGER (-2147483648..2147483647)	-- UNITS cells/sec

    ACCESS  read-write
    STATUS  mandatory
    DESCRIPTION
           "The rate at which traffic will be spaced if 
	 adnCvsiSpacingState is enabled.  This rate is
	 specified in cells per second.
         Common Name: Spacing Rate"
    DEFVAL  { ''H }
    ::= { adnCvsiSwitchInfoGroup  13 }

adnCvsiTscIfIndex OBJECT-TYPE
    SYNTAX  InterfaceIndex
    ACCESS  read-write
    STATUS  mandatory
    DESCRIPTION
           "The ifIndex of the TSC interface for this switch. 
         Common Name: TSC ifIndex "
    ::= { adnCvsiSwitchInfoGroup  14 }

adnTdpIfTable OBJECT-TYPE
    SYNTAX  SEQUENCE OF AdnTdpIfEntry
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
           "This table describes the TDP Interface. This table 
         contains proprietary TDP interface configuration 
         parameters. There is one entry per TDP interface. 
         A TDP interface is a logical inter-node ATM interface 
         or an ATM interface Connected to the Tag Switch 
         Controller (TSC)."
    ::= { adnCvsiMIB  2 }

adnTdpIfEntry OBJECT-TYPE
    SYNTAX  AdnTdpIfEntry
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
           "An entry in the TDP interface table. There is one 
         entry per TDP interface."
    INDEX   { ifIndex }
    ::= { adnTdpIfTable  1 }

AdnTdpIfEntry ::=
    SEQUENCE {
        adnTdpIfLogicalNum
            INTEGER (-2147483648..2147483647),

        adnTdpIfPhyDesc
            OCTET STRING,

        adnTdpIfType
            INTEGER,

        adnTdpIfState
            INTEGER,

        adnTdpIfTrapSent
            INTEGER,

        adnTdpIfPhyIfIndex
            InterfaceIndex,

        adnTdpIfPhyVpi
            INTEGER (-2147483648..2147483647),

        adnTdpIfIuniIfIndex
            InterfaceIndex,

        adnTdpIfVciMax
            INTEGER (-2147483648..2147483647),

        adnTdpIfVciMin
            INTEGER (-2147483648..2147483647),

        adnTdpIfMaxTDPConn
            INTEGER (-2147483648..2147483647),

        adnTdpIfAvailConnEgress
            INTEGER (-2147483648..2147483647),

        adnTdpIfAvailConnIn
            INTEGER (-2147483648..2147483647),

        adnTdpIfMaxCellRateEgress
            INTEGER (-2147483648..2147483647),

        adnTdpIfMaxCellRateIn
            INTEGER (-2147483648..2147483647),

        adnTdpIfAvailCellRateEgress
            INTEGER (-2147483648..2147483647),

        adnTdpIfAvailCellRateIn
            INTEGER (-2147483648..2147483647),

        adnTdpIfMaxCtdEgress
            INTEGER (-2147483648..2147483647),

        adnTdpIfMaxCtdIn
            INTEGER (-2147483648..2147483647),

        adnTdpIfMaxCdvEgress
            INTEGER (-2147483648..2147483647),

        adnTdpIfMaxCdvIn
            INTEGER (-2147483648..2147483647),

        adnTdpIfRemoteLogicalNum
            INTEGER (-2147483648..2147483647),

        adnTdpIfRemoteNodeId
            AdnRoutingID,

        adnTdpIfPolicingState
            AdnEnableDisabled,

        adnTdpIfPCR
            INTEGER (-2147483648..2147483647),

        adnTdpIfABW
            INTEGER (-2147483648..2147483647),

        adnTdpIfRemNodeMacNum
            INTEGER (-2147483648..2147483647),

        adnTdpIfRemNodeAreaNum
            INTEGER (-2147483648..2147483647)
    }

adnTdpIfLogicalNum OBJECT-TYPE
    SYNTAX  INTEGER (-2147483648..2147483647)
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "This is the logical interface number of this TDP 
          interface used by the Tag Switch Controller (TSC).
         Common Name: Logical If Number"
    ::= { adnTdpIfEntry  1 }

adnTdpIfPhyDesc OBJECT-TYPE
    SYNTAX  DisplayString
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The description of the interface. It is the 20 byte 
         physical descriptor of the interface provided to the 
         Tag Switch Controller (TSC).
         Common Name: Interface Physical Desc"
    ::= { adnTdpIfEntry  2 }

adnTdpIfType OBJECT-TYPE
    SYNTAX  INTEGER {
    tsc(1),
    tdpAccess(2),
    tdpTrunk(3)
}
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "This object provides the type of the interface 
         The following are the possible values it can hold:
           1 - Tag switch controller (TSC) interface, it 
               connects the HSS to the Tag Switch Controller
               (TSC). 
           2 - Tag distribution protocol (TDP) access interface,
               it connects HSS to the Tag Edge Router (TER). 
           3 - Tag distribution protocol (TDP) trunk interace,
               it is the trunk interface for the tagged data.
         Common Name: Interface Type"
    ::= { adnTdpIfEntry  3 }

adnTdpIfState OBJECT-TYPE
    SYNTAX  INTEGER {
    pending(1),
    congested(2),
    opened(3),
    up(4),
    down(5)
}
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "This object provides the state of the interface 
         The following are the possible values it can hold:
           1 - Interface Pending, the interface is being set up
           2 - Interface Congested, the interface is congested
           3 - Interface Opened, the interface is up
           4 - Interface Up
           5 - Interface Down
         Common Name: Interface State"
    ::= { adnTdpIfEntry  4 }

adnTdpIfTrapSent OBJECT-TYPE
    SYNTAX  TruthValue
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "This object tells whether a trap has been sent to the 
         Tag Switch controller The following are the possible values 
         it can hold.  A trap has to be sent when an interface 
         is brought up or down, This object tells whether a trap 
         has been sent after this event. The interface status 
         transition will be based on the the TDP interface 
         status (adnTdpIfState).
         Common Name: Trap Sent"
    ::= { adnTdpIfEntry  5 }

adnTdpIfPhyIfIndex OBJECT-TYPE
    SYNTAX  InterfaceIndex
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The ifIndex of the physical line supporting this TDP 
         interface.
         Common Name: Physical IfIndex"
    ::= { adnTdpIfEntry  6 }

adnTdpIfPhyVpi OBJECT-TYPE
    SYNTAX  INTEGER (-2147483648..2147483647)
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The Virtual Path Identifier (VPI) associated with the 
         physical line supporting this TDP interface.
         Common Name: Physical VPI"
    ::= { adnTdpIfEntry  7 }

adnTdpIfIuniIfIndex OBJECT-TYPE
    SYNTAX  InterfaceIndex
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The ifIndex of the internal UNI (I-UNI) on the Cell 
         Interconnection Gateway (CIG) associated with this TDP 
         interface. An internal UNI is used for the CVSI connection 
         between the Tag Switch Controller (TSC) and HSS. It is zero 
         if this is not a TSC interface (i.e if adnTdpIfType is
         not equal to 'tsc').
         Common Name: IUNI IfIndex"
    ::= { adnTdpIfEntry  8 }

adnTdpIfVciMax OBJECT-TYPE
    SYNTAX  INTEGER (-2147483648..2147483647)
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The maximum VCI value allowed on this interface.
         Common Name: Max VCI"
    ::= { adnTdpIfEntry  9 }

adnTdpIfVciMin OBJECT-TYPE
    SYNTAX  INTEGER (-2147483648..2147483647)
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The minimum VCI value allowed on this interface.
         Common Name: Min VCI"
    ::= { adnTdpIfEntry  10 }

adnTdpIfMaxTDPConn OBJECT-TYPE
    SYNTAX  INTEGER (-2147483648..2147483647)
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The maximum number of Tag Distribution protocol (TDP)
         connections allowed on this interface.
         Common Name: Max Connections"
    ::= { adnTdpIfEntry  11 }

adnTdpIfAvailConnEgress OBJECT-TYPE
    SYNTAX  INTEGER (-2147483648..2147483647)
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The number of available Tag Distribution Protocol 
         (TDP) egress connections.
         The egress direction means the direction of outwards 
         traffic flow from the interface.
         Common Name: Available Egress Connections"
    ::= { adnTdpIfEntry  12 }

adnTdpIfAvailConnIn OBJECT-TYPE
    SYNTAX  INTEGER (-2147483648..2147483647)
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The number of available Tag Distribution Protocol 
         (TDP) ingress connections.
         The ingress direction means the direction of inwards 
         traffic flow into the interface.
         Common Name: Available Ingress Connections"
    ::= { adnTdpIfEntry  13 }

adnTdpIfMaxCellRateEgress OBJECT-TYPE
    SYNTAX  INTEGER (-2147483648..2147483647)	-- UNITS cells/sec

    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The maximum egress cell rate available. The cell rate 
         is measured in Cells/Second. This value is configured 
         when an interface is created. 
         The egress direction means the direction of outwards 
         traffic flow from the interface.
         A value of -1 indicates that the field is not used. 
         Common Name: Max Egress Cell Rate"
    ::= { adnTdpIfEntry  14 }

adnTdpIfMaxCellRateIn OBJECT-TYPE
    SYNTAX  INTEGER (-2147483648..2147483647)	-- UNITS cells/sec

    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The maximum ingress cell rate available. The cell rate 
         is measured in Cells/Second. This value is configured 
         when an interface is created. 
         The ingress direction means the direction of inwards 
         traffic flow into the interface.
         A value of -1 indicates that the field is not used. 
         Common Name: Max Ingress Cell Rate"
    ::= { adnTdpIfEntry  15 }

adnTdpIfAvailCellRateEgress OBJECT-TYPE
    SYNTAX  INTEGER (-2147483648..2147483647)	-- UNITS cells/sec

    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The available egress cell rate. The cell rate is 
         measured in Cells/Second. 
         The egress direction means the direction of outwards 
         traffic flow from the interface.
         A value of -1 indicates that the field is not used. 
         Common Name: Available Egress Cell Rate"
    ::= { adnTdpIfEntry  16 }

adnTdpIfAvailCellRateIn OBJECT-TYPE
    SYNTAX  INTEGER (-2147483648..2147483647)	-- UNITS cells/sec

    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The available ingress cell rate. 
         The cell rate is measured in Cells/Second. 
         The ingress direction means the direction of inwards 
         traffic flow into the interface.
         A value of -1 indicates that the field is not used. 
         Common Name: Available Ingress Cell Rate"
    ::= { adnTdpIfEntry  17 }

adnTdpIfMaxCtdEgress OBJECT-TYPE
    SYNTAX  INTEGER (-2147483648..2147483647)	-- UNITS milliseconds

    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The maximum Cell Transfer Delay (CTD) for egress. This 
         value is configured when an interface is created. 
         The egress direction means the direction of outwards 
         traffic flow from the interface.
         A value of -1 indicates that the field is not used. 
         Common Name: Max Egress CTD"
    ::= { adnTdpIfEntry  18 }

adnTdpIfMaxCtdIn OBJECT-TYPE
    SYNTAX  INTEGER (-2147483648..2147483647)	-- UNITS milliseconds

    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The maximum Cell Transfer Delay (CTD) for ingress. This 
         value is configured when an interface is created. 
         The ingress direction means the direction of inwards 
         traffic flow into the interface.
         A value of -1 indicates that the field is not used. 
         Common Name: Max Ingress CTD"
    ::= { adnTdpIfEntry  19 }

adnTdpIfMaxCdvEgress OBJECT-TYPE
    SYNTAX  INTEGER (-2147483648..2147483647)	-- UNITS microseconds

    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The maximum Cell Delay Variation (CDV) for egress. This 
         value is configured when an interface is created. 
         The egress direction means the direction of outwards 
         traffic flow from the interface.
         A value of -1 indicates that the field is not used. 
         Common Name: Max Egress CDV"
    ::= { adnTdpIfEntry  20 }

adnTdpIfMaxCdvIn OBJECT-TYPE
    SYNTAX  INTEGER (-2147483648..2147483647)	-- UNITS microseconds

    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The maximum Cell Delay Variation (CDV) for ingress. This 
         value is configured when an interface is created. 
         The ingress direction means the direction of inwards 
         traffic flow into the interface.
         A value of -1 indicates that the field is not used. 
         Common Name: Max Ingress CDV"
    ::= { adnTdpIfEntry  21 }

adnTdpIfRemoteLogicalNum OBJECT-TYPE
    SYNTAX  INTEGER (-2147483648..2147483647)
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "This entry represents the logical interface number
         of the remote side of the TDP trunk. 
         This is the logical interface number of remote TDP 
         interface used by the Tag Switch Controller (TSC).
         Common Name: Remote Logical Interface Number"
    ::= { adnTdpIfEntry  22 }

adnTdpIfRemoteNodeId OBJECT-TYPE
    SYNTAX  AdnRoutingID
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "This entry represents the node ID of the remote 
         side HSS node of the TDP trunk. 
         Common Name: Remote Node ID"
    ::= { adnTdpIfEntry  23 }

adnTdpIfPolicingState OBJECT-TYPE
    SYNTAX  AdnEnableDisabled
    ACCESS  read-write
    STATUS  mandatory
    DESCRIPTION
           "Indicates whether traffic policing is performed on
	 this  TDP interface.  When policing is
	 disabled then no traffic spacing will be done at
	 the TDP interface no matter what the value of
	 adnCvsiSpacingState is.
         Common Name: Policing"
    DEFVAL  { enabled }
    ::= { adnTdpIfEntry  24 }

adnTdpIfPCR OBJECT-TYPE
    SYNTAX  INTEGER (-2147483648..2147483647)	-- UNITS cells/sec

    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The Peak Cell Rate (PCR) allowed for traffic through 
         this interface. This rate is specified in 
         cells per second.
         Common Name: PCR"
    ::= { adnTdpIfEntry  25 }

adnTdpIfABW OBJECT-TYPE
    SYNTAX  INTEGER (-2147483648..2147483647)	-- UNITS cells/sec

    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The bandwidth, measured in cells per second, 
	 allocated to this TDP interface.
         Common Name: Allocated Bandwidth"
    ::= { adnTdpIfEntry  26 }

adnTdpIfRemNodeMacNum OBJECT-TYPE
    SYNTAX  INTEGER (-2147483648..2147483647)
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The machine number part of
        adnTdpIfRemoteNodeId."
    ::= { adnTdpIfEntry  27 }

adnTdpIfRemNodeAreaNum OBJECT-TYPE
    SYNTAX  INTEGER (-2147483648..2147483647)
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The area number part of
        adnTdpIfRemoteNodeId."
    ::= { adnTdpIfEntry  28 }

adnTdpIfLookupTable OBJECT-TYPE
    SYNTAX  SEQUENCE OF AdnTdpIfLookupEntry
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
           "This table is a lookup table where a CVSI logical 
         interface number is mapped to the corresponding 
         ATM logical interface ifIndex. The ATM logical
         interface ifIndex is the index of the
         adnTdpIfTable."
    ::= { adnCvsiMIB  3 }

adnTdpIfLookupEntry OBJECT-TYPE
    SYNTAX  AdnTdpIfLookupEntry
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
           "An entry in the TDP Interface lookup Table. 
         There is one entry per TDP interface."
    INDEX   { adnTdpIfLogicalNum }
    ::= { adnTdpIfLookupTable  1 }

AdnTdpIfLookupEntry ::=
    SEQUENCE {
        adnTdpIfIndex
            InterfaceIndex
    }

adnTdpIfIndex OBJECT-TYPE
    SYNTAX  InterfaceIndex
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The ATM logical interface ifIndex corresponding 
         to the given logical interface number. This 
         ifIndex is the index of the adnTdpIfTable.
         Common Name: IfIndex"
    ::= { adnTdpIfLookupEntry  1 }

adnAtmTdpConnTable OBJECT-TYPE
    SYNTAX  SEQUENCE OF AdnAtmTdpConnEntry
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
           "This table describes the TDP connections in the HSS. 
         There is one entry per TDP connection in the HSS."
    ::= { adnCvsiMIB  4 }

adnAtmTdpConnEntry OBJECT-TYPE
    SYNTAX  AdnAtmTdpConnEntry
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
           "An entry in the CVSI HSS Connection Table. 
         There is one entry per TDP connection in the HSS."
    INDEX   { ifIndex,  adnAtmTdpConnVpi,  adnAtmTdpConnVci,  adnAtmTdpConnCconnectIndex }
    ::= { adnAtmTdpConnTable  1 }

AdnAtmTdpConnEntry ::=
    SEQUENCE {
        adnAtmTdpConnVpi
            INTEGER,

        adnAtmTdpConnVci
            INTEGER,

        adnAtmTdpConnCconnectIndex
            INTEGER,

        adnAtmTdpConnPriSec
            INTEGER,

        adnAtmTdpConnOutIfIndex
            InterfaceIndex,

        adnAtmTdpConnOutVpi
            INTEGER,

        adnAtmTdpConnOutVci
            INTEGER,

        adnAtmTdpConnOutCconnectIndex
            INTEGER,

        adnAtmTdpConnState
            INTEGER,

        adnAtmTdpConnForwCellRate
            INTEGER (-2147483648..2147483647),

        adnAtmTdpConnForwPCR
            INTEGER (-2147483648..2147483647),

        adnAtmTdpConnForwDelay
            INTEGER (-2147483648..2147483647),

        adnAtmTdpConnForwDelayVariation
            INTEGER (-2147483648..2147483647),

        adnAtmTdpConnRevCellRate
            INTEGER (-2147483648..2147483647),

        adnAtmTdpConnRevPCR
            INTEGER (-2147483648..2147483647),

        adnAtmTdpConnRevDelay
            INTEGER (-2147483648..2147483647),

        adnAtmTdpConnRevDelayVariation
            INTEGER (-2147483648..2147483647),

        adnAtmTdpConnPriPersEPFlag
            INTEGER,

        adnAtmTdpConnSecPersEPFlag
            INTEGER,

        adnAtmTdpConnForwSCR
            INTEGER (-2147483648..2147483647),

        adnAtmTdpConnForwMBS
            INTEGER (-2147483648..2147483647),

        adnAtmTdpConnRevSCR
            INTEGER (-2147483648..2147483647),

        adnAtmTdpConnRevMBS
            INTEGER (-2147483648..2147483647),

        adnAtmTdpConnForwServType
            INTEGER,

        adnAtmTdpConnRevServType
            INTEGER
    }

adnAtmTdpConnVpi OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The VPI of the ATM cross connection for this TDP
         connection. This is also an index of the table.
         Common Name: VPI"
    ::= { adnAtmTdpConnEntry  1 }

adnAtmTdpConnVci OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The VCI of the ATM cross connection for this TDP
         connection. This is also an index of the table.
         Common Name: VCI"
    ::= { adnAtmTdpConnEntry  2 }

adnAtmTdpConnCconnectIndex OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The index of the cross-connection for the  Virtual  Channel
        Connection (VCC) on  this  interface. In  a  point-to-point
        cross-connection, the index  equals  zero. In  a  point-to-
        -multipoint cross-connection, there is  a  different  index
        for each leaf.
        Common Name: Cross Connect Index"
    ::= { adnAtmTdpConnEntry  3 }

adnAtmTdpConnPriSec OBJECT-TYPE
    SYNTAX  INTEGER {
    primary(1),
    secondary(2)
}
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "This object tells whether this end of the TDP connection
         index is Primary or Secondary.
         Common Name: Pri/Sec"
    ::= { adnAtmTdpConnEntry  4 }

adnAtmTdpConnOutIfIndex OBJECT-TYPE
    SYNTAX  InterfaceIndex
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The ifIndex of this node's output ATM logical interface.
         Common Name: Out IfIndex"
    ::= { adnAtmTdpConnEntry  5 }

adnAtmTdpConnOutVpi OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The Virtual Path Identifier (VPI) of this connection on the
        output ATM interface from this  node. In  conjunction  with
        adnAtmTdpConnOutIfIndex, this VPI  can  be  used  for
        path tracing.
        Common Name: Out VPI"
    ::= { adnAtmTdpConnEntry  6 }

adnAtmTdpConnOutVci OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The Virtual Connection Identifier (VCI) of this connection 
        on the output ATM interface from this  node. In  conjunction  
        with adnAtmTdpConnOutIfIndex, this VCI  can  be  used  for
        path tracing.
        Common Name: Out VCI"
    ::= { adnAtmTdpConnEntry  7 }

adnAtmTdpConnOutCconnectIndex OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The index of the cross-connection for the  Virtual  Channel
        Connection (VCC) on  the output ATM interface from this node.
        In  a  point-to-point cross-connection, the index  equals  
        zero. In  a  point-to-multipoint cross-connection, there is  
        a  different  index for each leaf.
        Common Name: Out Cross Connect Index"
    ::= { adnAtmTdpConnEntry  8 }

adnAtmTdpConnState OBJECT-TYPE
    SYNTAX  INTEGER {
    reserved(1),
    active(2)
}
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "This object provides the state of the connection 
         The following are the possible values it can hold.
           1 - Connection Reserved- Created but not activated
           2 - Connection Active  - Active state
         Common Name: Conn State"
    ::= { adnAtmTdpConnEntry  9 }

adnAtmTdpConnForwCellRate OBJECT-TYPE
    SYNTAX  INTEGER (-2147483648..2147483647)	-- UNITS cells/sec

    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The forward requested cell rate for this connection. 
         This value is configured when the connection is created.
         Forward direction is the direction traffic from the 
         primary to the secondary interface.
         A value of -1 indicates that the field is not used. 
         Common Name: Forward Cell Rate"
    ::= { adnAtmTdpConnEntry  10 }

adnAtmTdpConnForwPCR OBJECT-TYPE
    SYNTAX  INTEGER (-2147483648..2147483647)	-- UNITS cells/sec

    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The forward peak cell rate (PCR) for the connection. 
         This value is configured when the connection is created.
         Forward direction is the direction traffic from the 
         primary to the secondary interface.
         A value of -1 indicates that the field is not used. 
         Common Name: Forward PCR"
    ::= { adnAtmTdpConnEntry  11 }

adnAtmTdpConnForwDelay OBJECT-TYPE
    SYNTAX  INTEGER (-2147483648..2147483647)	-- UNITS milliseconds

    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The maximum forward delay allowed for this connection. 
         This value is configured when the connection is created. 
         Forward direction is the direction traffic from the 
         primary to the secondary interface.
         A value of -1 indicates that the field is not used. 
         Common Name: Forward Delay"
    ::= { adnAtmTdpConnEntry  12 }

adnAtmTdpConnForwDelayVariation OBJECT-TYPE
    SYNTAX  INTEGER (-2147483648..2147483647)	-- UNITS microseconds

    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The maximum Cell Delay Variation (CDV) in the forward 
         direction allowed for this connection. This value is 
         configured when the connection is created. Forward 
         direction is the direction traffic from the primary to 
         the secondary interface.
         A value of -1 indicates that the field is not used. 
         Common Name: Forward CDV"
    ::= { adnAtmTdpConnEntry  13 }

adnAtmTdpConnRevCellRate OBJECT-TYPE
    SYNTAX  INTEGER (-2147483648..2147483647)	-- UNITS cells/sec

    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The reverse requested cell rate for this connection. 
         This value is configured when the connection is created.
         Reverse direction is the direction of traffic from 
         the secondary to the primary interface.
         A value of -1 indicates that the field is not used. 
         Common Name: Reverse Cell Rate"
    ::= { adnAtmTdpConnEntry  14 }

adnAtmTdpConnRevPCR OBJECT-TYPE
    SYNTAX  INTEGER (-2147483648..2147483647)	-- UNITS cells/sec

    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The reverse peak cell rate (PCR) for this connection. 
         This value is configured when the connection is created.
         Reverse direction is the direction of traffic from  
         the secondary to the primary interface.
         A value of -1 indicates that the field is not used. 
         Common Name: Reverse PCR"
    ::= { adnAtmTdpConnEntry  15 }

adnAtmTdpConnRevDelay OBJECT-TYPE
    SYNTAX  INTEGER (-2147483648..2147483647)	-- UNITS milliseconds

    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The maximum reverse delay allowed for this connection. 
         This value is configured when the connection is created. 
         Reverse direction is the direction of traffic from  
         the secondary to the primary interface.
         A value of -1 indicates that the field is not used. 
         Common Name: Reverse Delay"
    ::= { adnAtmTdpConnEntry  16 }

adnAtmTdpConnRevDelayVariation OBJECT-TYPE
    SYNTAX  INTEGER (-2147483648..2147483647)	-- UNITS microseconds

    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The maximum Cell Delay Variation (CDV) in the reverse 
         direction allowed for this connection. This value is 
         configured when the connection is created. Reverse 
         direction is the direction of traffic from the secondary 
         to the primary interface.
         A value of -1 indicates that the field is not used. 
         Common Name: Reverse CDV"
    ::= { adnAtmTdpConnEntry  17 }

adnAtmTdpConnPriPersEPFlag OBJECT-TYPE
    SYNTAX  TruthValue
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "This object reflects the primary Persistent End point 
         flag. This flag indicates that this part of connection 
         is the first leg of a tag distribution protocol (TDP) 
         connection. The first leg of the connection terminates 
         at a tag edge router (TER).
         Common Name: Primary Endpoint Flag"
    ::= { adnAtmTdpConnEntry  18 }

adnAtmTdpConnSecPersEPFlag OBJECT-TYPE
    SYNTAX  TruthValue
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "This object reflects the secondary Persistent End point 
         flag. This flag indicates that this part of connection 
         is last leg of a tag distribution protocol (TDP) 
         connection. The last leg of the connection terminates 
         into a tag edge router (TER).
         Common Name: Secondry Endpoint Flag"
    ::= { adnAtmTdpConnEntry  19 }

adnAtmTdpConnForwSCR OBJECT-TYPE
    SYNTAX  INTEGER (-2147483648..2147483647)	-- UNITS cells/sec

    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The forward sustainable cell rate (SCR) for this 
         connection. Forward direction is the direction traffic 
         from the primary to the secondary interface.
         A value of -1 indicates that the field is not used. 
         Common Name: Forward SCR"
    ::= { adnAtmTdpConnEntry  20 }

adnAtmTdpConnForwMBS OBJECT-TYPE
    SYNTAX  INTEGER (-2147483648..2147483647)
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The forward maximum burst size (MBS) for this connection. 
         Forward direction is the direction of traffic from the 
         primary to the secondary interface.
         A value of -1 indicates that the field is not used. 
         Common Name: Forward MBS"
    ::= { adnAtmTdpConnEntry  21 }

adnAtmTdpConnRevSCR OBJECT-TYPE
    SYNTAX  INTEGER (-2147483648..2147483647)	-- UNITS cells/sec

    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The Reverse sustainable cell rate (SCR) for this 
         connection. Reverse direction is the direction traffic 
         from the secondary to the primary interface.
         A value of -1 indicates that the field is not used. 
         Common Name: Reverse SCR"
    ::= { adnAtmTdpConnEntry  22 }

adnAtmTdpConnRevMBS OBJECT-TYPE
    SYNTAX  INTEGER (-2147483648..2147483647)
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The reverse maximum burst size (MBS) for this connection. 
         Reverse direction is the direction of traffic from the 
         secondary to the primary interface.
         A value of -1 indicates that the field is not used. 
         Common Name: Reverse MBS"
    ::= { adnAtmTdpConnEntry  23 }

adnAtmTdpConnForwServType OBJECT-TYPE
    SYNTAX  AdnCvsiCos
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The forward service type for a connection. It is a value 
         which specifies what class of service has been requested 
         for this connection in forward direction. 
         Forward direction is the direction traffic from the 
         primary to the secondary interface.
         Common Name: Forward Service Type"
    ::= { adnAtmTdpConnEntry  24 }

adnAtmTdpConnRevServType OBJECT-TYPE
    SYNTAX  AdnCvsiCos
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The reverse service type for a connection. It is a value 
         which specifies what class of service has been requested 
         for this connection in reverse direction. 
         Reverse direction is the direction of traffic from  
         the secondary to the primary interface.
         Common Name: Reverse Service Type"
    ::= { adnAtmTdpConnEntry  25 }

adnIprtMIB OBJECT IDENTIFIER ::= { adnProprietary  23 }

adnIpRouteNodeTable OBJECT-TYPE
    SYNTAX  SEQUENCE OF AdnIpRouteNodeEntry
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
           "This is the IP Routing Configuration Table"
    ::= { adnIprtMIB  1 }

adnIpRouteNodeEntry OBJECT-TYPE
    SYNTAX  AdnIpRouteNodeEntry
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
           "The IP Route Node Entry defines IP Routing Node Table Entries."
    INDEX   { adnIpRouteConfigIndex }
    ::= { adnIpRouteNodeTable  1 }

AdnIpRouteNodeEntry ::=
    SEQUENCE {
        adnIpRouteConfigIndex
            INTEGER (-2147483648..2147483647),

        adnIpRouteConfigFilePtr
            DisplayString,

        adnIpRouteConfigAdminState
            INTEGER (-2147483648..2147483647),

        adnIpRouteConfigOperState
            INTEGER (-2147483648..2147483647)
    }

adnIpRouteConfigIndex OBJECT-TYPE
    SYNTAX  INTEGER (-2147483648..2147483647)
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "This object defines the Index into this table. This index will
     represent only one instance of any object in this table."
    ::= { adnIpRouteNodeEntry  1 }

adnIpRouteConfigFilePtr OBJECT-TYPE
    SYNTAX  DisplayString
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "This object defines the pointer to an ASCII string that 
     points to the IP routing Configuration path/file on the HSS CU disk."
    ::= { adnIpRouteNodeEntry  2 }

adnIpRouteConfigAdminState OBJECT-TYPE
    SYNTAX  INTEGER {
    enabled(1),
    disabled(2)
}
    ACCESS  read-write
    STATUS  mandatory
    DESCRIPTION
           "This object indicates the desired state of the IP Routing (GateD) 
     component. There are typically only two selection states, Enabled(1) or
     Disabled(2)."
    ::= { adnIpRouteNodeEntry  3 }

adnIpRouteConfigOperState OBJECT-TYPE
    SYNTAX  INTEGER {
    up(1),
    down(2)
}
    ACCESS  read-write
    STATUS  mandatory
    DESCRIPTION
           "This object indicates the current state of the IP Routing (GateD) 
     component. There are typically only two states possible, Up(1) or
     Down(2)."
    ::= { adnIpRouteNodeEntry  4 }

END
