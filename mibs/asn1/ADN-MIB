adnUCNodeErrorsAtmsupValue OBJECT IDENTIFIER ::= { adnUCNodeErrorsAtmsup  11 }

adnCESBufferUnderBegin TRAP-TYPE
    ENTERPRISE  { adnTrapsATM }
    VARIABLES   { adnNodeTime,  adnTrapCategory,  atmfCESCbrIndex,  ifName }
    DESCRIPTION
           " This TRAP indicates that a cell starvation condition, 
          confirmed after a persistency check, has begun on the
          Circuit Emulation Service connection.
          A cell starvation condition means that no more ATM cells 
          are received on the ATM connection supporting the Circuit
          Emulation Service connection which means an underflow in 
          the received cells buffer
          The persistency check consists in waiting a certain time 
          (default value is 2.5 s) before sending the alarm "
    ::= 19

adnCESBufferUnderEnd TRAP-TYPE
    ENTERPRISE  { adnTrapsATM }
    VARIABLES   { adnNodeTime,  adnTrapCategory,  atmfCESCbrIndex,  ifName }
    DESCRIPTION
           " This TRAP indicates that a cell starvation condition 
          has ended on the Circuit Emulation Service connection.
          A cell starvation condition means that no more ATM cells 
          are received on the ATM connection supporting the Circuit
          Emulation Service connection which means an underflow in 
          the received cells buffer"
    ::= 20

adnCES OBJECT IDENTIFIER ::= { adnProprietary  20 }

adnCESConfTable OBJECT-TYPE
    SYNTAX  SEQUENCE OF AdnCESConfEntry
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
           "A list of extra parameters associated to a 
            Circuit Emulation Service connection.
           "
    ::= { adnCES  1 }

adnCESConfEntry OBJECT-TYPE
    SYNTAX  AdnCESConfEntry
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
           "Extra parameters for describing a 
            Circuit Emulation Service connection.
           "
    INDEX   { atmfCESCbrIndex }
    ::= { adnCESConfTable  1 }

AdnCESConfEntry ::=
    SEQUENCE {
        adnCESClockMode
            INTEGER,

        adnCESTrunkCondData
            INTEGER,

        adnCESCondSignal
            INTEGER,

        adnCESUpDownTrapEnable
            INTEGER
    }

adnCESClockMode OBJECT-TYPE
    SYNTAX  INTEGER {
    useAtmfClockMode(1),
    looptiming(2)
}
    ACCESS  read-write
    STATUS  mandatory
    DESCRIPTION
           "It defines the synchronization mode associated 
            to the Circuit Emulation Service connection:
             - use the value defined in atmfCESCbrClockMode
             - loop timing: proprietary mode used when the 
               line is in loop timing mode
           "
    ::= { adnCESConfEntry  1 }

adnCESTrunkCondData OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  read-write
    STATUS  mandatory
    DESCRIPTION
           "It specifies the pattern to be sent in the data 
            timeslot when trunk conditioning (fault condition)
            on Constant Bit Rate interface is activated. 
            It only applies to structured data transfer 
            service.
           "
    ::= { adnCESConfEntry  2 }

adnCESCondSignal OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  read-write
    STATUS  mandatory
    DESCRIPTION
           "It specifies the pattern to be sent in the 
            signalling nibble (ABCD bits of CAS signalling)
            when trunk conditioning (fault condition) on 
            Constant Bit Rate interface is activated. 
            It only applies to structured data transfer 
            service.
           "
    ::= { adnCESConfEntry  3 }

adnCESUpDownTrapEnable OBJECT-TYPE
    SYNTAX  INTEGER {
    enabled(1),
    disabled(2)
}
    ACCESS  read-write
    STATUS  mandatory
    DESCRIPTION
           " It indicates whether Circuit Emulation Service
             trap should be generated on the Constant Bit
             Rate interface.  
           "
    ::= { adnCESConfEntry  4 }

