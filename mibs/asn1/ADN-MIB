adnFrForceRoute OBJECT-TYPE
    SYNTAX  SEQUENCE OF AdnFrForceEntry
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
           "This table describes the forced path 
        to be used for this PVC."
    ::= { adnFrnetservMIB  4 }

adnFrForceEntry OBJECT-TYPE
    SYNTAX  AdnFrForceEntry
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
           "This is one entry of the Forced Path table for the specified
         PVC."
    INDEX   { ifIndex,  frPVCEndptDLCIIndex,  adnFrForceHop }
    ::= { adnFrForceRoute  1 }

AdnFrForceEntry ::=
    SEQUENCE {
        adnFrForceHop
            INTEGER (-2147483648..2147483647),

        adnFrForceNode
            AdnRoutingID,

        adnFrForceFlag
            INTEGER,

        adnFrForceOutifindex
            INTEGER (-2147483648..2147483647),

        adnFrFrcNodeMacNum
            INTEGER (-2147483648..2147483647),

        adnFrFrcNodeAreaNum
            INTEGER (-2147483648..2147483647)
    }

adnFrForceHop OBJECT-TYPE
    SYNTAX  INTEGER (-2147483648..2147483647)
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "This is the hop indicator. The value is '1' in the
         source node and increments for each hop through the
         network."
    ::= { adnFrForceEntry  1 }

adnFrForceNode OBJECT-TYPE
    SYNTAX  AdnRoutingID
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "This is the routing node ID of the next node in this 
         forced path. It is required for every entry
         in the Forced Path table."
    ::= { adnFrForceEntry  2 }

adnFrForceFlag OBJECT-TYPE
    SYNTAX  INTEGER {
    preferred(1),
    required(2)
}
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "This flag indicates if this is the preferred or
         required path.  If this flag is set to 'preferred(1)',
         and the interface designated by adnFrForceOutifindex
         is not available, then alternate interfaces are tried.  
         If this is set to 'required(2)', and the designated 
         interface is not available, then no additional 
         interfaces are attempted and the call is cleared."
    DEFVAL  { required }
    ::= { adnFrForceEntry  3 }

adnFrForceOutifindex OBJECT-TYPE
    SYNTAX  INTEGER (-2147483648..2147483647)
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "This specifies the ifIndex of the interface that the 
         call should use when exiting this node. If adnFrForceFlag 
         has the value 'required(2)', then this interface MUST be 
         used. If adnFrForceFlag is set to 'preferred(1), an 
         alternate interface can be used to get to the next node
         identified by adnFrForceNode if this interface is 
         unavailable."
    ::= { adnFrForceEntry  4 }

adnFrFrcNodeMacNum OBJECT-TYPE
    SYNTAX  INTEGER (-2147483648..2147483647)
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The machine number part of the adnFrForceNode, the routing 
         node ID of the next node in this forced path."
    ::= { adnFrForceEntry  5 }

adnFrFrcNodeAreaNum OBJECT-TYPE
    SYNTAX  INTEGER (-2147483648..2147483647)
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The area number part of the adnFrForceNode, the routing 
         node ID of the next node in this forced path."
    ::= { adnFrForceEntry  6 }

adnFrAccountVCTable OBJECT-TYPE
    SYNTAX  SEQUENCE OF AdnFrAccountVCEntry
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
           "This is the Accounting MIB table. These are the
         counters that are reported in the FRS
         (Frame Relay Subscriber) accounting record."
    ::= { adnFrnetservMIB  5 }

adnFrAccountVCEntry OBJECT-TYPE
    SYNTAX  AdnFrAccountVCEntry
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
           "One instance of the FRS (Frame Relay Subscriber) accounting 
         counters."
    INDEX   { ifIndex,  frPVCEndptDLCIIndex,  adnFrAccountTariff }
    ::= { adnFrAccountVCTable  1 }

AdnFrAccountVCEntry ::=
    SEQUENCE {
        adnFrAccountTariff
            INTEGER (-2147483648..2147483647),

        adnFrAccountInSegments
            Counter,

        adnFrAccountOutSegments
            Counter,

        adnFrAccountInFrames
            Counter,

        adnFrAccountOutFrames
            Counter,

        adnFrAccountInExcessFrames
            Counter,

        adnFrAccountOutExcessFrames
            Counter
    }

adnFrAccountTariff OBJECT-TYPE
    SYNTAX  INTEGER (-2147483648..2147483647)
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "This indicates the tariff period for these counters.
         A tariff period is a designated time period during
         which specified billing rates apply"
    ::= { adnFrAccountVCEntry  1 }

adnFrAccountInSegments OBJECT-TYPE
    SYNTAX  Counter
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "This indicates the number of ingress sub-frames in this
         reporting period for this VC."
    ::= { adnFrAccountVCEntry  2 }

adnFrAccountOutSegments OBJECT-TYPE
    SYNTAX  Counter
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "This indicates the number of egress sub-frames in this
         reporting period for this VC."
    ::= { adnFrAccountVCEntry  3 }

adnFrAccountInFrames OBJECT-TYPE
    SYNTAX  Counter
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "This indicates the number of ingress frames in this 
         reporting period for this VC."
    ::= { adnFrAccountVCEntry  4 }

adnFrAccountOutFrames OBJECT-TYPE
    SYNTAX  Counter
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "This indicates the number of egress frames in this reporting
         period for this VC."
    ::= { adnFrAccountVCEntry  5 }

adnFrAccountInExcessFrames OBJECT-TYPE
    SYNTAX  Counter
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "This indicates the number of ingress excess frames in this
         reporting period for this VC."
    ::= { adnFrAccountVCEntry  6 }

adnFrAccountOutExcessFrames OBJECT-TYPE
    SYNTAX  Counter
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "This indicates the number of egress excess frames in
         this reporting period for this VC."
    ::= { adnFrAccountVCEntry  7 }

adnFrAccVCTable OBJECT-TYPE
    SYNTAX  SEQUENCE OF AdnFrAccVCEntry
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
           "This table constitutes the FRS (Frame Relay Subscriber)
         accounting record. None of the objects in this table 
         can be obtained via an SNMP GET command. These objects 
         are visible only in an accounting record.  Most of these 
         objects duplicate objects in other tables. They are 
         collected here to allow the accounting record to be sent
         as a single table to conserve space."
    ::= { adnFrnetservMIB  6 }

adnFrAccVCEntry OBJECT-TYPE
    SYNTAX  AdnFrAccVCEntry
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
           "One instance of the FRS (Frame Relay Subscriber) accounting 
         record."
    INDEX   { ifIndex,  frPVCEndptDLCIIndex }
    ::= { adnFrAccVCTable  1 }

AdnFrAccVCEntry ::=
    SEQUENCE {
        adnFrAccVCifName
            DisplayString,

        adnFrAccVCfrPVCEndptDLCIIndex
            INTEGER (-2147483648..2147483647),

        adnFrAccVCadnFrVCRemDlci
            INTEGER (-2147483648..2147483647),

        adnFrAccVCadnFrVCUniqueConnID
            INTEGER (-2147483648..2147483647),

        adnFrAccVCadnFrVCRemNodeID
            AdnRoutingID,

        adnFrAccVCfrLportNumPlan
            INTEGER,

        adnFrAccVCifPhysAddress
            PhysAddress,

        adnFrAccVCadnFrVCRemNumPlan
            INTEGER,

        adnFrAccVCadnFrVCRemPhysAddress
            PhysAddress,

        adnFrAccVCadnFrVCSourceDest
            INTEGER,

        adnFrAccVCadnFrVCNumRelinks
            INTEGER (-2147483648..2147483647),

        adnFrAccVCfrPVCEndptInCIR
            INTEGER (-2147483648..2147483647),

        adnFrAccVCadnFrVCInBc
            INTEGER (-2147483648..2147483647),

        adnFrAccVCadnFrVCInBcs
            INTEGER (-2147483648..2147483647),

        adnFrAccVCfrPVCEndptInBe
            INTEGER (-2147483648..2147483647),

        adnFrAccVCfrPVCEndptOutCIR
            INTEGER (-2147483648..2147483647),

        adnFrAccVCadnFrVCOutBc
            INTEGER (-2147483648..2147483647),

        adnFrAccVCadnFrVCOutBcs
            INTEGER (-2147483648..2147483647),

        adnFrAccVCfrPVCEndptOutBe
            INTEGER (-2147483648..2147483647),

        adnFrAccVCadnFrVCUserOrNet
            INTEGER,

        adnFrAccVCfrAccountPVCSegmentSize
            INTEGER (-2147483648..2147483647),

        adnFrAccVCadnFrVCType
            INTEGER,

        adnFrAccVCTimeEndAcct
            AdnNodeTimeStamp,

        adnFrAccVCTimeStartAcct
            AdnNodeTimeStamp,

        adnFrAccVCCallTerm
            INTEGER,

        adnFrAccVCadnFrChanLineType
            INTEGER,

        adnFrAccVCadnFrVCNumNodes
            INTEGER (-2147483648..2147483647),

        adnFrAccVCTableInSegments
            INTEGER (-2147483648..2147483647),

        adnFrAccVCTableInFrames
            INTEGER (-2147483648..2147483647),

        adnFrAccVCTableInExcessFrame
            INTEGER (-2147483648..2147483647),

        adnFrAccVCTableOutSegments
            INTEGER (-2147483648..2147483647),

        adnFrAccVCTableOutFrames
            INTEGER (-2147483648..2147483647),

        adnFrAccVCTableOutExcessFrame
            INTEGER (-2147483648..2147483647),

        adnFrAccVCPVCRegroomed
            TruthValue,

        adnFrAccVCSVCCallingPartyNum
            PhysAddress,

        adnFrAccVCSVCCalledPartyNum
            PhysAddress,

        adnFrAccVCSVCCallEstablished
            TruthValue,

        adnFrAccVCSVCQ933ClearingCause
            INTEGER (-2147483648..2147483647),

        adnFrAccVCSVCQ933ClearingLocation
            INTEGER (-2147483648..2147483647),

        adnFrAccVCCos
            AdnFrCos,

        adnFrAccVCadnFrVCLocalNodeID
            AdnRoutingID,

        adnFrAccVCadnFrVCDescr
            DisplayString,

        adnFrAccVCadnFrVCLocalifIndex
            INTEGER (-2147483648..2147483647),

        adnFrAccVCadnFrVCRemoteifIndex
            InterfaceIndex,

        adnFrAccVCadnFrVCEndptInDiscards
            INTEGER (-2147483648..2147483647),

        adnFrAccVCifSpeed
            Gauge,

        adnFrAccVCadnNodeTime
            AdnNodeTimeTicks
    }

