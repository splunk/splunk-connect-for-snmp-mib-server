            Counter,

        adnAtmBoardErHMPLHPCCounter
            Counter,

        adnAtmBoardErHMP0SYNCCounter
            Counter,

        adnAtmBoardErHMP1SYNCCounter
            Counter,

        adnAtmBoardErUFOFCounter
            Counter,

        adnAtmBoardErUFRFCounter
            Counter,

        adnAtmBoardErNFOFCounter
            Counter,

        adnAtmBoardErPHTRAMPCounter
            Counter,

        adnAtmBoardErNFRFCounter
            Counter,

        adnAtmBoardErNAVCCounter
            Counter,

        adnAtmBoardErNAVPCounter
            Counter,

        adnAtmBoardErRBOFCounter
