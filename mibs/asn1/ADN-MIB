    SYNTAX  INTEGER {
    frs(1),
    franp(2),
    iwf(3)
}
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
           "Type of interface affected."
    ::= { adnTrapVars  5 }

adnTrapFrCongOrigin OBJECT-TYPE
    SYNTAX  INTEGER {
    ingress-vc(1),
    egress-vc(2),
    ingress-channel(3),
    egress-channel(4),
    egress-cuf(5)
}
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
           "Origin of the congestion trap."
    ::= { adnTrapVars  6 }

adnTrapReason OBJECT-TYPE
    SYNTAX  INTEGER {
    atm-line(1),
    fr-line(2),
    lis-link(3),
    lapf-lapd-link(4),
    nms-initiated(5),
    atm-interface(6),
    xfr-line(7),
    xcm-link(8),
    x25-gateway(9),
    x75-gateway(10),
    x25-link(11)
}
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
           "The reason for sending this trap (usually, the type
         of the cause object).

         atm-line: ATM line,
         fr-line: Frame Relay line,
         lis-link: Local Inchannel Signalling (LIS) link,
         lapf-lapd-link: LAPF/LAPD protocol,
         nms-initiated: NMS-initiated via SNMP SET command,
         atm-interface: ATM interface,
         xfr-line: Alcatel protocol for Frame Relay PSX trunk,
         xcm-link: Alcatel protocol for X.25 PSX trunk,
         x25-gateway: X.25 link to other (non ACX) X.25 network,
         x75-gateway: X.75 link to other X.25 network, 
         x25-link: PSX-ACX trunk (ACX is an X.25 PSX subscriber)."
    ::= { adnTrapVars  7 }

adnAtmInterfaceCauseError OBJECT-TYPE
    SYNTAX  INTEGER {
    adnAtmInterfaceBadUserNetFlg(1)
}
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
           "Cause of the ATM interface error trap."
    ::= { adnTrapVars  20 }

adnTrapFrVCCos OBJECT-TYPE
    SYNTAX  AdnFrCos
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
           "Class of Service for Frame Relay."
    ::= { adnTrapVars  67 }

adnFlashUpdateStatus TRAP-TYPE
    ENTERPRISE  { adnTrapsCommon }
    VARIABLES   { adnNodeTime,  adnTrapCategory,  adnBrdFlashChassisIndex,  adnBrdFlashSlotIndex,  adnBrdFlashUpdateErrorCode }
    DESCRIPTION
           "This trap is sent for each board whose on-line or off-line
            flash update fails."
    ::= 24

adnBrdFlashUpdateErrorCode OBJECT-TYPE
    SYNTAX  INTEGER {
    msupTimeout(1),
    flashWriteError(2),
    offlineFlashUpdateError(3)
}
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
           "This object identifies a specific error for flash update.
         Values are:
         msupTimeout - on-line flash update failed due to a Main
                           Supervisor timeout.
         flashWriteError - on-line flash update failed due to a
                           flash write error.
         offlineFlashUpdateError - off-line flash update failed."
    ::= { adnTrapVars  68 }

adnFlashBuildStatus TRAP-TYPE
    ENTERPRISE  { adnTrapsCommon }
    VARIABLES   { adnNodeTime,  adnTrapCategory,  adnBrdFlashChassisIndex,  adnBrdFlashSlotIndex,  adnBrdFlashBuildErrorCode }
    DESCRIPTION
           "This trap is sent for each board whose flash volume build fails."
    ::= 25

adnBrdFlashBuildErrorCode OBJECT-TYPE
    SYNTAX  INTEGER {
    flashVolumeWriteError(1),
    openDescriptionFileError(2),
    descriptionFileSyntaxError(3),
    flashComponentFileMissing(4),
    nectarFileManagementError(5),
    otherError(6)
}
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
           "This object identifies a specific error for flash build.
         flashVolumeWriteError - an error occured while creating or writing
                                 the flash volume file on the Control Unit.
                                 This may be a disk space problem.
         openDescriptionFileError - the flash description file can not be
                                 opened. This may be due to a permission
                                 problem or to the absence of this file on
                                 the Control Unit.
         descriptionFileSyntaxError - there is some syntax error within
                                 the description file.
         flashComponentFileMissing - a file to be gathered in the flash volume
                                 can not be found on the Control Unit.
         nectarFileManagementError - a Nectar File Management error occured.
         otherError - any error other than the above ones."
    ::= { adnTrapVars  69 }

adnDumThresholdViolation TRAP-TYPE
    ENTERPRISE  { adnTrapsCommon }
    VARIABLES   { adnNodeTime,  adnTrapCategory,  adnDumDirectoryPathname,  adnDumDirectoryPercentUsage,  adnDumDirectoryLevelUsage }
    DESCRIPTION
           "This trap indicates that one of the thresholds for
            disk utilization in the managed filesystem under Disk
            Utilization Management in the Node Management
            Subsystem was violated."
    ::= 26

adnDumFileReady TRAP-TYPE
    ENTERPRISE  { adnTrapsCommon }
    VARIABLES   { adnNodeTime,  adnTrapCategory }
    DESCRIPTION
           "This trap indicates that a file that the Network
            Management System normally retrieves from a managed
            node via FTP services is ready to be transferred to
            the Network Management System."
    ::= 27

adnIfAssocTable OBJECT-TYPE
    SYNTAX  SEQUENCE OF AdnIfAssocEntry
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
           "A list of point to point associations between the PSX/ACX internal 
         link interfaces."
    ::= { adnProprietary  19 }

adnIfAssocEntry OBJECT-TYPE
    SYNTAX  AdnIfAssocEntry
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
           "An association entry containing ifIndex of the first interface,
          adnNodeIndex of the first node, ifIndex of the associated 
          interface, adnNodeIndex of the associated node and the type
          of protocol used on these interfaces."
    INDEX   { adnFirstIfIndex }
    ::= { adnIfAssocTable  1 }

AdnIfAssocEntry ::=
    SEQUENCE {
        adnFirstIfIndex
            INTEGER,

        adnFirstNodeIndex
            INTEGER,

        adnAssocIfIndex
            INTEGER,

        adnAssocNodeIndex
            INTEGER,

        adnProtocolType
            INTEGER
    }

adnFirstIfIndex OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The value of this object corresponds to the ifIndex of the first 
           interface."
    ::= { adnIfAssocEntry  1 }

adnFirstNodeIndex OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The value of this object corresponds to the adnNodeIndex of the 
           first node."
    ::= { adnIfAssocEntry  2 }

adnAssocIfIndex OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The value of this object corresponds to the ifIndex of the 
           associated interface, this value is not significant in the case
           where adnProtocolType is g25 or x75. In that case a GET request
           on this OID will be replied with the response noSuchname into  
	   error-status field."
    ::= { adnIfAssocEntry  3 }

adnAssocNodeIndex OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The value of this object corresponds to the adnNodeIndex of the 
           associated node, this value is not significant in the case
           where adnProtocolType is g25 or x75. In that case a GET request
           on this OID will be replied with the response noSuchname into  
	   error-status field."
    ::= { adnIfAssocEntry  4 }

adnProtocolType OBJECT-TYPE
    SYNTAX  INTEGER {
    xfr(1),
    xcm(2),
    x25(3),
    g25(4),
    x75(5)
}
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The value of this object represents the protocol used on  
           interfaces. xfr and xcm protocols are  ALCATEL proprietary 
           protocols that are used for PSX and ACX internal links.
           x25 is a standard protocol that is used for connecting  an  
           ACX configured as a concentrator to a PSX. 
           g25 is an ALCATEL proprietary protocol that is used for 
	   connecting an ALCATEL 1100 NETWORK to another packet switching
	   network via an X25 gateway link. x75 is a standard protocol         
           used for connecting an ALCATEL 1100 NETWORK to another packet 
	   switching network via an X75 gateway link."
    ::= { adnIfAssocEntry  5 }

