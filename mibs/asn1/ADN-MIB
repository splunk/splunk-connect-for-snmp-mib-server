adnAtmCconnectDown TRAP-TYPE
    ENTERPRISE  { adnTrapsATM }
    VARIABLES   { adnNodeTime,  adnTrapCategory,  ifName,  ifIndex,  adnAtmVpi,  adnAtmVci,  adnAtmCconnectIndex,  adnAtmCconnectAdminStatus,  adnAtmCconnectOperStatus,  adnAtmCcCauseError,  adnAtmCconnectLastCanpCause,  adnAtmCconnectLastErrorNodeID,  adnAtmCconnectLastLocation,  adnAtmCconnectLastCause,  adnAtmCconnectLastDiag,  adnAtmCconnectLastMachine,  adnAtmCconnectLastArea,  adnAtmCconnectLastChassis,  adnAtmCconnectLastSlot }
