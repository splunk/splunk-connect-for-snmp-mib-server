    SYNTAX  INTEGER (-2147483648..2147483647)	-- UNITS bits per second

    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "This is the sum of committed ingress bandwidth for Class
         of Service NRT.
         Common Name: Non-Real Time Data Bc Commit Ingress"
    ::= { adnFrtChanStatsEntry  16 }

adnFrtVCTable OBJECT-TYPE
    SYNTAX  SEQUENCE OF AdnFrtVCEntry
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
           "This is the VC table for FRANP (Frame Relay Alcatel
         Network Protocol) trunk protocol.
	 This table is owned by FRANP Signalling software module."
    ::= { adnFranpTrunkServiceMIB  3 }

adnFrtVCEntry OBJECT-TYPE
    SYNTAX  AdnFrtVCEntry
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
           "VC table entry."
    INDEX   { ifIndex,  adnFrtVCDLCIIndex }
    ::= { adnFrtVCTable  1 }

AdnFrtVCEntry ::=
    SEQUENCE {
        adnFrtVCDLCIIndex
            INTEGER (-2147483648..2147483647),

        adnFrtVCSetupDir
            INTEGER,

        adnFrtVCAdminState
            INTEGER,

        adnFrtVCOperState
            INTEGER,

        adnFrtVCForcedPath
            INTEGER,

        adnFrtVCTrafficType
            INTEGER,

        adnFrtVCState
            INTEGER,

        adnFrtVCRemoteDlci
            INTEGER (-2147483648..2147483647),

        adnFrtVCRemoteDlciLen
            INTEGER (-2147483648..2147483647),

        adnFrtVCRemoteifIndex
            InterfaceIndex,

        adnFrtVCAdjacentNodeID
            AdnRoutingID,

        adnFrtVCAdjacentifIndex
            InterfaceIndex,

        adnFrtVCBcForward
            INTEGER (-2147483648..2147483647),

        adnFrtVCBcBackward
            INTEGER (-2147483648..2147483647),

        adnFrtVCBeForward
            INTEGER (-2147483648..2147483647),

        adnFrtVCBeBackward
            INTEGER (-2147483648..2147483647),

        adnFrtVCBcsForward
            INTEGER (-2147483648..2147483647),

        adnFrtVCBcsBackward
            INTEGER (-2147483648..2147483647),

        adnFrtVCCirForward
            INTEGER (-2147483648..2147483647),

        adnFrtVCCirBackward
            INTEGER (-2147483648..2147483647),

        adnFrtVCDelayCostRem
            INTEGER (-2147483648..2147483647),

        adnFrtVCMonetaryCostRem
            INTEGER (-2147483648..2147483647),

        adnFrtVCHopCostRem
            INTEGER (-2147483648..2147483647),

        adnFrtVCRoutingMode
            INTEGER,

        adnFrtVCPathSelectMode
            INTEGER,

        adnFrtVCRtsCritID
            INTEGER,

        adnFrtVCType
            INTEGER,

        adnFrtVCCos
            AdnFrCos
    }

adnFrtVCDLCIIndex OBJECT-TYPE
    SYNTAX  INTEGER (-2147483648..2147483647)
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
           "The value of this object is equal to the DLCI
         (data link connection identifier) value for
         the frame relay trunk. 
	 Common Name: Local DLCI"
    ::= { adnFrtVCEntry  1 }

adnFrtVCSetupDir OBJECT-TYPE
    SYNTAX  INTEGER {
    ingress(1),
    egress(2)
}
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The direction of the setup message. It can be either
	 entering the node (ingress) or exiting the node (egress)."
    ::= { adnFrtVCEntry  2 }

adnFrtVCAdminState OBJECT-TYPE
    SYNTAX  INTEGER {
    up(1),
    down(2)
}
    ACCESS  read-write
    STATUS  mandatory
    DESCRIPTION
           "The desired state of the call.  
	 Common Names: Administrative, Status Administrative"
    ::= { adnFrtVCEntry  3 }

adnFrtVCOperState OBJECT-TYPE
    SYNTAX  INTEGER {
    up(1),
    down(2)
}
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The current state of the call. 
	 Common Names: Operational, Status Operational"
    ::= { adnFrtVCEntry  4 }

adnFrtVCForcedPath OBJECT-TYPE
    SYNTAX  AdnEnableDisabled
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "This specifies that this VC must take a forced path 
         through the network."
    ::= { adnFrtVCEntry  5 }

adnFrtVCTrafficType OBJECT-TYPE
    SYNTAX  INTEGER {
    usercall(1),
    networkcall(2)
}
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "This indicates if this is a user or network call. The
         priority given to each type of call's data in the network
         differs: user call priority = customer traffic; network 
         call priority = network traffic such as alarms, 
         accounting, and statistics."
    ::= { adnFrtVCEntry  6 }

adnFrtVCState OBJECT-TYPE
    SYNTAX  AdnFrtVCStates
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "This defines the states that are implemented and 
         supported for the Frame Relay Alcatel Network Protocol 
         signalling interface on the trunk."
    ::= { adnFrtVCEntry  7 }

adnFrtVCRemoteDlci OBJECT-TYPE
    SYNTAX  INTEGER (-2147483648..2147483647)
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "This is the ingress DLCI (data link connection identifier)
         associated with the VC. 
	 Common Name: Remote DLCI"
    ::= { adnFrtVCEntry  8 }

adnFrtVCRemoteDlciLen OBJECT-TYPE
    SYNTAX  INTEGER {
    twoOctets10Bits(1),
    threeOctets10Bits(2),
    threeOctets16Bits(3),
    fourOctets17Bits(4),
    fourOctets23Bits(5)
}
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "This is the length of the ingress VC's DLCI (data link
         connection identifier)."
    ::= { adnFrtVCEntry  9 }

adnFrtVCRemoteifIndex OBJECT-TYPE
    SYNTAX  InterfaceIndex
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "This is the ifIndex of the interface on the remote node
	 on which this VC is carried. This ifIndex has no local
	 significance."
    ::= { adnFrtVCEntry  10 }

adnFrtVCAdjacentNodeID OBJECT-TYPE
    SYNTAX  AdnRoutingID
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "This is the routing node ID of the adjacent node to 
	 which the channel carrying this VC is connected."
    ::= { adnFrtVCEntry  11 }

adnFrtVCAdjacentifIndex OBJECT-TYPE
    SYNTAX  InterfaceIndex
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "This is the ifIndex of the interface on the adjacent node
	 on which this VC is carried. This ifIndex has no local
	 significance."
    ::= { adnFrtVCEntry  12 }

adnFrtVCBcForward OBJECT-TYPE
    SYNTAX  INTEGER (-2147483648..2147483647)
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "This specifies the committed data rate, in bits per  
         second, in the forward direction. Note that forward 
         data may be ingress or egress depending on the setting
         of adnFrtVCSetupDir."
    ::= { adnFrtVCEntry  13 }

adnFrtVCBcBackward OBJECT-TYPE
    SYNTAX  INTEGER (-2147483648..2147483647)
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "This specifies the committed data rate, in bits per 
         second, in the backward direction. Note that backward 
         data may be ingress or egress depending on the setting 
         of adnFrtVCSetupDir."
    ::= { adnFrtVCEntry  14 }

adnFrtVCBeForward OBJECT-TYPE
    SYNTAX  INTEGER (-2147483648..2147483647)
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "This specifies the excessive burst data rate, in bits 
         per second, in the forward direction. Note that forward 
         data may be ingress or egress depending on the setting 
         of adnFrtVCSetupDir."
    ::= { adnFrtVCEntry  15 }

adnFrtVCBeBackward OBJECT-TYPE
    SYNTAX  INTEGER (-2147483648..2147483647)
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "This specifies the excessive burst data rate, in bits 
         per second, in the backward direction. Note that  
         backward data may be ingress or egress depending on the
         setting of adnFrtVCSetupDir."
    ::= { adnFrtVCEntry  16 }

adnFrtVCBcsForward OBJECT-TYPE
    SYNTAX  INTEGER (-2147483648..2147483647)
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "This specifies the statistically committed data rate, in 
         bits per second, in the forward direction. Note that 
         forward data may be ingress or egress depending on the 
         setting of adnFrtVCSetupDir."
    ::= { adnFrtVCEntry  17 }

adnFrtVCBcsBackward OBJECT-TYPE
    SYNTAX  INTEGER (-2147483648..2147483647)
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "This specifies the statistically committed data rate, in
         bits per second, in the backward direction. Note that 
         backward data may be ingress or egress depending on the 
         setting of adnFrtVCSetupDir."
    ::= { adnFrtVCEntry  18 }

adnFrtVCCirForward OBJECT-TYPE
    SYNTAX  INTEGER (-2147483648..2147483647)
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "This specifies the committed information rate, in bits
         per second, in the forward direction. Note that forward
         data may be ingress or egress depending on the setting
         of adnFrtVCSetupDir."
    ::= { adnFrtVCEntry  19 }

adnFrtVCCirBackward OBJECT-TYPE
    SYNTAX  INTEGER (-2147483648..2147483647)
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "This specifies the committed information rate, in bits
         per second, in the backward direction. Note that backward
         data may be ingress or egress depending on the setting
         of adnFrtVCSetupDir."
    ::= { adnFrtVCEntry  20 }

adnFrtVCDelayCostRem OBJECT-TYPE
    SYNTAX  INTEGER (-2147483648..2147483647)	-- UNITS milliseconds

    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The delay cost (in milliseconds) allowed 
	 between this node and the destination node for
	 setting up this PVC.
	 Common Name: Remaining Delay Cost"
    DEFVAL  { 65535 }
    ::= { adnFrtVCEntry  21 }

adnFrtVCMonetaryCostRem OBJECT-TYPE
    SYNTAX  INTEGER (-2147483648..2147483647)
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The monetary cost allowed between this 
	 node and the destination node for setting
	 up this VC. 
         Common Name: Remaining Monetary Cost"
    DEFVAL  { 0 }
    ::= { adnFrtVCEntry  22 }

adnFrtVCHopCostRem OBJECT-TYPE
    SYNTAX  INTEGER (-2147483648..2147483647)
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The number of hops allowed between this 
	 node and the destination node for setting 
	 up this VC.
	 Common Name: Remaining Hop Cost"
    DEFVAL  { 10 }
    ::= { adnFrtVCEntry  23 }

adnFrtVCRoutingMode OBJECT-TYPE
    SYNTAX  INTEGER {
    bestEffort(1),
    considerCost(2)
}
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The routing mode used for routing this VC.
	 When best effort is used, routing is done
	 without regards to the cost requirement.
	 When best effort is not used, routing must
	 take into account the cost requirement.
	Common Name: Routing Mode"
    DEFVAL  { bestEffort }
    ::= { adnFrtVCEntry  24 }

adnFrtVCPathSelectMode OBJECT-TYPE
    SYNTAX  INTEGER {
    bestFit(1),
    bestAvailable(2)
}
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The mode the path selection function must
	 use to select a path for this VC.
         Best available service uses the lowest cost 
	 path(s) which meet the cost requirement as 
	 the primary path.  With this approach, the
	 first calls set up in the network potentially
	 receive a higher quality service than calls
	 which happen to arrive later.
	 Best fit service uses the highest cost path(s) 
	 which meet the cost requirement as the primary 
	 path(s).  This approach uses the least 
	 required resources to satisfy the cost 
	 function and conserves the higher quality of
	 service for those calls which may require
	 them.
	 Common Name: Path Selection Mode"
    DEFVAL  { bestAvailable }
    ::= { adnFrtVCEntry  25 }

adnFrtVCRtsCritID OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "The index into adnRtsCritSmryTable
	 of the routing criteria applied for
	 this VC.
	 Common Name: Routing Criteria ID"
    DEFVAL  { 1 }
    ::= { adnFrtVCEntry  26 }

adnFrtVCType OBJECT-TYPE
    SYNTAX  INTEGER {
    pvc(1),
    svc(2)
}
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "This indicates the type of VC for the call."
    ::= { adnFrtVCEntry  27 }

adnFrtVCCos OBJECT-TYPE
    SYNTAX  AdnFrCos
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "This indicates the Class of Service for the call."
    ::= { adnFrtVCEntry  28 }

