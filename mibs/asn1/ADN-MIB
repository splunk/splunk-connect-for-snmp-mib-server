    ::= { adnBrdConfigUpdateCntlTable  1 }

AdnBrdConfigUpdateCntlEntry ::=
    SEQUENCE {
        adnBrdConfigChassisIndex
            INTEGER (-2147483648..2147483647),

        adnBrdConfigSlotIndex
            INTEGER (-2147483648..2147483647),

        adnBrdConfigOperStatus
            INTEGER,

        adnBrdConfigState
            INTEGER,

        adnBrdConfigFileName
            OCTET STRING,

        adnBrdConfigNextFileName
            OCTET STRING,

        adnBrdConfigNoCfgOrFullCfg
            INTEGER
    }

adnBrdConfigChassisIndex OBJECT-TYPE
    SYNTAX  INTEGER (-2147483648..2147483647)
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
           "Index field that identifies the chassis. Chassis are 
	numbered 1-7. The main chassis (i.e., the one with the 
	active Control Unit) in a multi-chassis node is chassis 
	number 1. In a single-chassis node, the chassis number is 1.
	This number is provided in hardware by a HLCID card which 
	is plugged into the midplane."
    ::= { adnBrdConfigUpdateCntlEntry  1 }

adnBrdConfigSlotIndex OBJECT-TYPE
    SYNTAX  INTEGER (-2147483648..2147483647)
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
           "Index field that identifies the slot. Slots within a 
	chassis are numbered 1-12. The first slot on the left when 
	facing the front of the chassis is number 1. This number is
	provided in hardware through hard-wiring on the midplane. 
	Note that the HBSAC board always has a special slot
	identifier of 30, but this slot number is never used."
    ::= { adnBrdConfigUpdateCntlEntry  2 }

adnBrdConfigOperStatus OBJECT-TYPE
    SYNTAX  INTEGER {
    in-progress(1),
    success(2),
    failed(3),
    pending(4)
}
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "This object notifies the NMS of the results of the last 
	command, or whether it is still in progress."
    ::= { adnBrdConfigUpdateCntlEntry  3 }

adnBrdConfigState OBJECT-TYPE
    SYNTAX  INTEGER {
    committed(0),
    actInProgWaitingForLoadInd(1),
    actInProgMiddleTransaction(2),
    actInProgEndTransaction(3),
    uncommitted(4),
    loadingCommittedNextFile(5),
    loadingCommittedMiddleTrans(6),
    loadingCommittedEndTrans(7),
    pending(8)
}
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "This object notifies the NMS of the `state' of 
	configuration update processing. The values mean:
        committed -- This board is not currently 
                     loading configuration tables.  Its
                     configuration tables are from the
                     committed version.
        actInProgWaitingForLoadInd -- An Activate command
                     is in progress.  For this board,
                     the state machine is waiting for
                     an indication that all requests
                     to load tables have been received, 
                     so that the loading can begin.
        actInProgMiddleTransaction -- An Activate command
                     is in progress, and for this board,
                     the state machine is waiting for
                     the response to a middle message
                     within a configuration transaction.
        actInProgEndTransaction -- An Activate command
                     is in progress, and for this board,
                     the state machine is waiting for the
                     response to a null message that 
                     signals the end of a configuration 
                     transaction.
        uncommitted -- This board is using uncommitted
                     configuration tables.
        loadingCommittedNextFile -- This board is in the
                     process of loading uncommitted
                     configuration tables.
                     The state machine is waiting for a
                     response to a null message that
                     signals the end of a transaction,
                     and will then send the next
                     configuration file, which will be the
                     rollback file.
        loadingCommittedMiddleTrans -- This board is in
                     the process of loading committed
                     configuration tables.  The state 
                     machine is waiting for a response
                     to a middle message within the 
                     configuration transaction.
        loadingCommittedEndTrans -- This board is in
                     the process of loading committed
                     configuration tables.  The state
                     machine is waiting for a response
                     to a null message that signals
                     the end of a transaction.
        pending -- An Activate command is in progress, 
                     and this board is currently down.  
                     When the board comes up, it will be
                     loaded with the uncommitted 
                     configuration file."
    ::= { adnBrdConfigUpdateCntlEntry  4 }

adnBrdConfigFileName OBJECT-TYPE
    SYNTAX  OCTET STRING
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "This object identifies the configuration file being used 
	for this board."
    ::= { adnBrdConfigUpdateCntlEntry  5 }

adnBrdConfigNextFileName OBJECT-TYPE
    SYNTAX  OCTET STRING
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "This object identifies the next configuration file to be
	used for this board, if there is an automatic rollback
	in progress from uncommitted configuration to committed
	configuration.  The value of this object is not significant
	if there is currently no rollback in progress."
    ::= { adnBrdConfigUpdateCntlEntry  6 }

adnBrdConfigNoCfgOrFullCfg OBJECT-TYPE
    SYNTAX  INTEGER {
    noConfig(1),
    fullConfig(2),
    deletedConfig(3)
}
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
           "This object identifies whether a Parameteric File Xfer
        Interface (PFXI) configuration file exists for this
        chassis and slot. The values mean:
            noConfig      -- no configuration file
            fullConfig    -- full configuration file
            deletedConfig -- the board has been deleted.
        For some board types (such as High Speed Couplers), the
        configuration parameters are managed entirely by the 
        Control Unit, not by the software running on the board 
        itself.  In these cases there will be no configuration 
        file.  When a coupler board has been deleted from the 
        uncommitted version, but still exists in the committed
        version, the value of this object is set to deletedConfig."
    ::= { adnBrdConfigUpdateCntlEntry  7 }

adnBrdFlashContentListTable OBJECT-TYPE
    SYNTAX  SEQUENCE OF AdnBrdFlashContentListEntry
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
           "The board flash content list is a portion of the 
        Update Control group which contains information 
        about the contents of a board's flash memory."
    ::= { adnUpdateCntl  6 }

adnBrdFlashContentListEntry OBJECT-TYPE
    SYNTAX  AdnBrdFlashContentListEntry
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
