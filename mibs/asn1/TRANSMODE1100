--*****************************************************************************************
--*****************************************************************************************
-- 26xx Interface slots
--*****************************************************************************************
--*****************************************************************************************

	trm26xx	OBJECT IDENTIFIER
		::=  {trmSubrackUnits 7 }
		

--*****************************************************************************************
-- 26xx Interface units Performance management
--*****************************************************************************************

	trm26xxPM	OBJECT IDENTIFIER
		::=  {trm26xx 1 }


	trm26xxTable	OBJECT-TYPE
		SYNTAX		SEQUENCE  OF  Trm26xxEntry
		MAX-ACCESS	not-accessible
		STATUS		current
		DESCRIPTION	
			"A list of 26xx units in the subrack(s)."
			::=  {trm26xxPM  1}

	trm26xxEntry	OBJECT-TYPE
		SYNTAX		Trm26xxEntry
		MAX-ACCESS	not-accessible
		STATUS		current
		DESCRIPTION	
			"A set of parameters that describe a 26xx unit."
		INDEX		{trm26xxRackNumber,trm26xxSlotNumber}
		::=  {trm26xxTable 1}

	Trm26xxEntry  ::=  SEQUENCE {
		trm26xxRackNumber	RackNumber,
		trm26xxSlotNumber	BoardNumber,
		trm26xxStatus  	DisplayString,
		trm26xxTemperature	INTEGER,
		trm26xxTxMode		TxMode75,
		trm26xxSwReset  	INTEGER,
		trm26xxOpticalPowerPIN1	INTEGER,
		trm26xxOpticalPowerPIN2	INTEGER,
		trm26xxLogOpticalPower  INTEGER,
		trm26xxTemperatureTEC1	INTEGER,
		trm26xxTemperatureTEC2	INTEGER,
		trm26xxVoltageTEC1	INTEGER,
		trm26xxVoltageTEC2	INTEGER,
		trm26xxCurrentTEC1	INTEGER,
		trm26xxCurrentTEC2	INTEGER,
		trm26xxCurrentBIAS1	INTEGER,
		trm26xxCurrentBIAS2	INTEGER,
		trm26xxVoltageBIAS1	INTEGER,
		trm26xxVoltageBIAS2	INTEGER
		}

	trm26xxRackNumber	OBJECT-TYPE	
		SYNTAX			RackNumber
		MAX-ACCESS		not-accessible
		STATUS			current
		DESCRIPTION		"This parameter indicates in what rack the unit is
					situated in."
		::=  {trm26xxEntry  1}

	trm26xxSlotNumber	OBJECT-TYPE	
		SYNTAX			BoardNumber
		MAX-ACCESS		not-accessible
		STATUS			current
		DESCRIPTION		"This parameter indicates in what slot number the
					unit is situated in. Can be slot 1 - 8."
		::=  {trm26xxEntry  2}

	trm26xxStatus	OBJECT-TYPE	
		SYNTAX			DisplayString
		MAX-ACCESS		read-only
		STATUS			current
		DESCRIPTION		"This parameter indicates the status of the optical 
					input for the line interface. It corresponds to the
					client Rx LED status of the unit.
					Can be Receiving or Not receiving."
		::=  {trm26xxEntry  3}

	trm26xxTemperature	OBJECT-TYPE	
		SYNTAX			INTEGER
		MAX-ACCESS		read-only
		STATUS			current
