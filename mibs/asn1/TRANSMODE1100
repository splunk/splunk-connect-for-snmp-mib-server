	trm75xx	OBJECT IDENTIFIER
		::=  {trmSubrackUnits 3 }
		

--*****************************************************************************************
-- 70YY/ZZ and 75YY/ZZ Interface units Performance management
--*****************************************************************************************

	trm75xxPM	OBJECT IDENTIFIER
		::=  {trm75xx 1 }


	trm75xxTable	OBJECT-TYPE
		SYNTAX		SEQUENCE  OF  Trm75xxEntry
		MAX-ACCESS	not-accessible
		STATUS		current
		DESCRIPTION	
			"A list of 70 & 75 units in the subrack(s)."
			::=  {trm75xxPM  1}

	trm75xxEntry	OBJECT-TYPE
		SYNTAX		Trm75xxEntry
		MAX-ACCESS	not-accessible
		STATUS		current
		DESCRIPTION	
			"A set of parameters that describe a 70 or a 75
			unit."
		INDEX		{trm75xxRackNumber,trm75xxSlotNumber}
		::=  {trm75xxTable 1}

	Trm75xxEntry  ::=  SEQUENCE {
		trm75xxRackNumber	RackNumber,
		trm75xxSlotNumber	BoardNumber,
		trm75xxRxLine  	DisplayString,
		trm75xxEstProtLine	DisplayString,
		trm75xxEstFibRateLine	DisplayString,
		trm75xxTxLine  	DisplayString,
		trm75xxTxMode	TxMode75,
		trm75xxWavelengthLine	DisplayString,
		trm75xxRxClient  DisplayString,
		trm75xxEstProtClient	DisplayString,
		trm75xxEstFibRateClient	DisplayString,
		trm75xxTxClient  DisplayString,
		trm75xxTxModeClient	TxMode75,
		trm75xxIDStringClient	DisplayString,
		trm75xxIPAddressClient	DisplayString,
		trm75xxWavelengthClient	DisplayString,
		trm75xxSpeedLimit	SpeedLimit75
		}

	trm75xxRackNumber	OBJECT-TYPE	
		SYNTAX			RackNumber
		MAX-ACCESS		not-accessible
		STATUS			current
		DESCRIPTION		"This parameter indicates in what rack the unit is
					situated in."
		::=  {trm75xxEntry  1}

	trm75xxSlotNumber	OBJECT-TYPE	
		SYNTAX			BoardNumber
		MAX-ACCESS		not-accessible
		STATUS			current
		DESCRIPTION		"This parameter indicates in what slot number the
					unit is situated in. Can be slot 1 - 8."
		::=  {trm75xxEntry  2}

	trm75xxRxLine	OBJECT-TYPE	
		SYNTAX			DisplayString
		MAX-ACCESS		read-only
		STATUS			current
		DESCRIPTION		"This parameter indicates the status of the optical 
					input for the line interface. It corresponds to the
					client Rx LED status of the unit.
					Can be Receiving or Not receiving."
		::=  {trm75xxEntry  3}

	trm75xxEstProtLine	OBJECT-TYPE	
		SYNTAX			DisplayString
		MAX-ACCESS		read-only
		STATUS			current
		DESCRIPTION		"This parameter indicates the Estimated Protocol for
					the line interface input."
		::=  {trm75xxEntry  4}

	trm75xxEstFibRateLine	OBJECT-TYPE
		SYNTAX			DisplayString
		MAX-ACCESS		read-only
		STATUS			current
		DESCRIPTION		"This parameter indicates the Estimated Fiber Rate
					for the line interface input in Mbit/s."
		::=  {trm75xxEntry  5}

	trm75xxTxLine	OBJECT-TYPE	
		SYNTAX			DisplayString
		MAX-ACCESS		read-only
		STATUS			current
		DESCRIPTION		"This parameter indicates the status of the optical
					output power for the line interface. It corresponds
					to the line Tx LED status of the unit.
					Can be on or off."
		::=  {trm75xxEntry  6}
	
	trm75xxTxMode	OBJECT-TYPE	
		SYNTAX			TxMode75
		MAX-ACCESS		read-write
		STATUS			current
		DESCRIPTION		"This parameter indicates the mode of the optical
					transmitter for line and client TX. 
					Can be set to normal(1) or disabled(2).
					normal  (1): Optical output when receiving signal.
					disabled(2): Optical output is OFF."
		::=  {trm75xxEntry  7}


	trm75xxWavelengthLine	OBJECT-TYPE
		SYNTAX			DisplayString 
		MAX-ACCESS		read-only
		STATUS			current
		DESCRIPTION		"This parameter indicates the nominal wavelength for
					the line interface transiever."
		::=  {trm75xxEntry  8}


	trm75xxRxClient	OBJECT-TYPE
		SYNTAX			DisplayString
		MAX-ACCESS		read-only
		STATUS			current
		DESCRIPTION		"This parameter indicates the status of the optical 
					input for the client interface. It corresponds to the
					client Rx LED status of the unit.
					Can be Receiving or Not receiving."
		::=  {trm75xxEntry  9}

	trm75xxEstProtClient	OBJECT-TYPE	
		SYNTAX			DisplayString
		MAX-ACCESS		read-only
		STATUS			current
		DESCRIPTION		"This parameter indicates the Estimated Protocol for
					the client interface input."
		::=  {trm75xxEntry  10}

	trm75xxEstFibRateClient	OBJECT-TYPE
		SYNTAX			DisplayString
		MAX-ACCESS		read-only
		STATUS			current
		DESCRIPTION		"This parameter indicates the Estimated Fiber Rate
					for the client interface input in Mbit/s."
		::=  {trm75xxEntry  11}

	trm75xxTxClient	OBJECT-TYPE	
		SYNTAX			DisplayString
		MAX-ACCESS		read-only
		STATUS			current
		DESCRIPTION		"This parameter indicates the status of the optical
					output power for the client interface. It corresponds
					to the client Tx LED status of the unit.
					Can be on or off."
		::=  {trm75xxEntry  12}

	trm75xxTxModeClient	OBJECT-TYPE	
		SYNTAX			TxMode75
		MAX-ACCESS		read-only
		STATUS			obsolete
		DESCRIPTION		"This object is obsolete and not used anymore."
		::=  {trm75xxEntry  13}

	trm75xxIDStringClient	OBJECT-TYPE	
		SYNTAX			DisplayString
		MAX-ACCESS		read-write
		STATUS			current
		DESCRIPTION		
			"This parameter identifies the equipment(ID String) 
			connected to the client interface.
			Maximum number of characters is 20."
		::=  {trm75xxEntry  14 }


	trm75xxIPAddressClient	OBJECT-TYPE	
		SYNTAX			DisplayString
		MAX-ACCESS		read-write
		STATUS			current
		DESCRIPTION		
			"This parameter indentifies the equipment(IP Address)
			connected to the client interface."
		::=  {trm75xxEntry  15 }


	trm75xxWavelengthClient	OBJECT-TYPE
		SYNTAX			DisplayString 
		MAX-ACCESS		read-only
		STATUS			current
		DESCRIPTION		"This parameter indicates the nominal wavelength for
					the client interface transiever."
		::=  {trm75xxEntry  16}

	trm75xxSpeedLimit	OBJECT-TYPE
		SYNTAX			SpeedLimit75
		MAX-ACCESS		read-write
		STATUS			current
		DESCRIPTION		"This parameter indicates the speed limit set for this unit
					and is used to set a new speed limit. 
					It generates an alarm and disables Line and Client Tx if the
					Client bitrate is higher than a predefined protocol.
					The alarm is ceased and Tx is enabled again at a lower value
					(hysteresis).
					Speed Limit is deactivated if trm75xxTxMode is disabled(2).
					Speed Limit can be set in 3 modes to allow different protocols:
					noLimit(1): Any protocol is allowed.
					lowLimit(2): 100Base-FX, STM1/OC3, ESCON & Video 270Mb.
					mediumLimit(3): All protocols above + STM4/OC-12."
		::=  {trm75xxEntry  17}


--*****************************************************************************************
-- 70YY/ZZ and 75YY/ZZ Interface units fault management
--*****************************************************************************************
		
	trm75xxFM	OBJECT IDENTIFIER
		::=  {trm75xx 2 }
		
	trm75xxTraps	OBJECT IDENTIFIER ::= { trm75xxFM 0 }

	trm75xxTrapGlitchLine	NOTIFICATION-TYPE
		OBJECTS {
			trmAlarmActiveName,
			trmAlarmActiveSeverity,
			trmAlarmActiveActTime,
			trmAlarmActiveUnit,
	        	trmAlarmActiveRackNumber,
	        	trmAlarmActiveSlotNumber,
	        	trmAlarmActiveSerialNumber
	        }
		STATUS			current
		DESCRIPTION		
				"This trap is sent when a unit experiences a split second
				optical	input power glitch at the line interface."
		::=  {  trm75xxTraps  1 }

	trm75xxTrapLopLine	NOTIFICATION-TYPE
		OBJECTS {
			trmAlarmActiveName,
			trmAlarmActiveSeverity,
			trmAlarmActiveActTime,
			trmAlarmActiveUnit,
	        	trmAlarmActiveRackNumber,
	        	trmAlarmActiveSlotNumber,
	        	trmAlarmActiveSerialNumber
	        }
		STATUS			current
		DESCRIPTION		
				"This trap is sent when the optical input power is
				lost at the line interface."
		::=  {  trm75xxTraps  2 }

	trm75xxTrapTxLine	NOTIFICATION-TYPE
		OBJECTS {
			trmAlarmActiveName,
			trmAlarmActiveSeverity,
			trmAlarmActiveActTime,
			trmAlarmActiveUnit,
	        	trmAlarmActiveRackNumber,
	        	trmAlarmActiveSlotNumber,
	        	trmAlarmActiveSerialNumber
	        }
		STATUS			current
		DESCRIPTION		
				"This trap is sent when a unit fails to generate
				optical output power at Tx line interface."
		::=  {  trm75xxTraps  3 }

	trm75xxTrapTxDisableLine	NOTIFICATION-TYPE
		OBJECTS {
