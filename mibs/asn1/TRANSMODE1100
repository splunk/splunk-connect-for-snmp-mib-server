				encoding is used."
		::=  {trm6001Entry  9 }

--*****************************************************************************************
-- NMB - fault management
--*****************************************************************************************

	trm6001FM	OBJECT IDENTIFIER
		::=  {trm6001 2 }

	trm6001Traps	OBJECT IDENTIFIER ::= { trm6001FM 0 }

	trm6001TrapExternal1	NOTIFICATION-TYPE
		OBJECTS {
			trmAlarmActiveName,
			trmAlarmActiveSeverity,
			trmAlarmActiveActTime,
			trmAlarmActiveUnit,
	        	trmAlarmActiveRackNumber,
	        	trmAlarmActiveSlotNumber,
	        	trmAlarmActiveSerialNumber
	        }
		STATUS			current
		DESCRIPTION		
				"This trap is sent when a External 1 alarm is 
				generated at the NMB 6001 unit."
		::=  {  trm6001Traps  1  }

	trm6001TrapExternal2	NOTIFICATION-TYPE
		OBJECTS {
			trmAlarmActiveName,
			trmAlarmActiveSeverity,
			trmAlarmActiveActTime,
			trmAlarmActiveUnit,
	        	trmAlarmActiveRackNumber,
	        	trmAlarmActiveSlotNumber,
	        	trmAlarmActiveSerialNumber
	        }
		STATUS			current
		DESCRIPTION		
				"This trap is sent when a External 2 alarm is 
				generated at the NMB 6001 unit."
		::=  {  trm6001Traps  2  }

	trm6001TrapExternal3	NOTIFICATION-TYPE
		OBJECTS {
			trmAlarmActiveName,
			trmAlarmActiveSeverity,
			trmAlarmActiveActTime,
			trmAlarmActiveUnit,
	        	trmAlarmActiveRackNumber,
	        	trmAlarmActiveSlotNumber,
	        	trmAlarmActiveSerialNumber
	        }
		STATUS			current
		DESCRIPTION		
				"This trap is sent when a External 3 alarm is 
				generated at the NMB 6001 unit."
		::=  {  trm6001Traps  3  }


	trm6001CascadeFailure	NOTIFICATION-TYPE
		OBJECTS {
			trmAlarmActiveName,
			trmAlarmActiveSeverity,
			trmAlarmActiveActTime,
			trmAlarmActiveUnit,
	        	trmAlarmActiveRackNumber,
	        	trmAlarmActiveSlotNumber,
	        	trmAlarmActiveSerialNumber
	        }
		STATUS			current
		DESCRIPTION		
				"This trap is sent when a attempt to enable Cascading 
				has failed or connection is lost with the cascading rack."
		::=  {  trm6001Traps  4  }

--*****************************************************************************************
--*****************************************************************************************
-- Power
--*****************************************************************************************
--*****************************************************************************************

	trm9xxx	OBJECT IDENTIFIER
		::=  { trmSubrackUnits 2 }


--*****************************************************************************************
-- Power -  performance management 
--*****************************************************************************************


	trm9xxxPM	OBJECT IDENTIFIER
		::=  {trm9xxx 1 }


	trm9xxxTable	OBJECT-TYPE
		SYNTAX		SEQUENCE  OF  Trm9xxxEntry
		MAX-ACCESS	not-accessible
		STATUS		current
		DESCRIPTION	
			"A list of power supply units in the subrack(s)."
			::=  {trm9xxxPM  1}

	trm9xxxEntry	OBJECT-TYPE
		SYNTAX		Trm9xxxEntry
		MAX-ACCESS	not-accessible
		STATUS		current
		DESCRIPTION	
			"A set of parameters that describe a power supply
			unit."
		INDEX		{trm9xxxRackNumber,trm9xxxSlotNumber}
		::=  {trm9xxxTable 1}

	Trm9xxxEntry  ::=  SEQUENCE {
		trm9xxxRackNumber	  RackNumber,
		trm9xxxSlotNumber  SlotNumberPS,
		trm9xxxPresent Present,
		trm9xxxType  PowerType,
		trm9xxxStatus  PowerStatus
		}

	trm9xxxRackNumber	OBJECT-TYPE	
		SYNTAX			RackNumber
		MAX-ACCESS		not-accessible
		STATUS			current
		DESCRIPTION		
			"This object indicates in what rack the power
			supply is situated in."
		::=  {trm9xxxEntry  1 }


	trm9xxxSlotNumber	OBJECT-TYPE	
		SYNTAX			SlotNumberPS
		MAX-ACCESS		not-accessible
		STATUS			current
		DESCRIPTION		
			"This object indicates in what slot number the
			power supply is situated in. Can be slot 10 or 
			11."
		::=  {trm9xxxEntry  2 }


	trm9xxxPresent	OBJECT-TYPE	
		SYNTAX			Present
		MAX-ACCESS		read-only
		STATUS			current
		DESCRIPTION		
			"This parameter indicates if the power supply is
			present or not. 
			yes(1) means present and no(2) means not present."
		::=  {trm9xxxEntry  3 }


	trm9xxxType	OBJECT-TYPE	
		SYNTAX			PowerType 
		MAX-ACCESS		read-only
		STATUS			current
		DESCRIPTION		
			"This parameter describes what kind of power supply
			this unit is. 
			Power9048(2) means a power supply with input pover
			48 VDC, Power9220(3) means 100-240 VAC. If the type
			is unknown this parameter is unknown(1)."
		::=  {trm9xxxEntry  4 }

	trm9xxxStatus	OBJECT-TYPE	
		SYNTAX			PowerStatus 
		MAX-ACCESS		read-only
		STATUS			current
		DESCRIPTION		
			"This parameter indicates the status of the power
			supply. off(1) means that the power supply not is
			operating and on(2) means that it is operating."
		::=  {trm9xxxEntry  5 }

--*****************************************************************************************
-- Power - fault management
--*****************************************************************************************


	trm9xxxFM	OBJECT IDENTIFIER
		::=  {trm9xxx 2 }

	trm9xxxTraps	OBJECT IDENTIFIER ::= { trm9xxxFM 0 }

	trm9xxxTrapFailure	NOTIFICATION-TYPE
		OBJECTS {
			trmAlarmActiveName,
			trmAlarmActiveSeverity,
			trmAlarmActiveActTime,
			trmAlarmActiveUnit,
	        	trmAlarmActiveRackNumber,
	        	trmAlarmActiveSlotNumber,
	        	trmAlarmActiveSerialNumber
	        }
		STATUS			current
		DESCRIPTION		
				"This trap indicates that a fault was detected on
				a power supply or the input power is removed."
		::=  {  trm9xxxTraps  1 }

	trm9xxxTrapMissing	NOTIFICATION-TYPE
		OBJECTS {
			trmAlarmActiveName,
			trmAlarmActiveSeverity,
			trmAlarmActiveActTime,
			trmAlarmActiveUnit,
	        	trmAlarmActiveRackNumber,
	        	trmAlarmActiveSlotNumber,
	        	trmAlarmActiveSerialNumber
	        }
		STATUS			current
		DESCRIPTION		
				"This trap is sent when a power supply is missing."
		::=  {  trm9xxxTraps  2 }
		
