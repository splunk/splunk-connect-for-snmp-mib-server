--*****************************************************************************************
--*****************************************************************************************
-- Mux
--*****************************************************************************************
--*****************************************************************************************

	trm803x	OBJECT IDENTIFIER
		::=  { trmSubrackUnits 5 }


--*****************************************************************************************
-- Mux -  performance management 
--*****************************************************************************************


	trm803xPM	OBJECT IDENTIFIER
		::=  {trm803x 1 }


	trm803xTable	OBJECT-TYPE
		SYNTAX		SEQUENCE  OF  Trm803xEntry
		MAX-ACCESS	not-accessible
		STATUS		current
		DESCRIPTION	
			"A list of 803x units."
			::=  {trm803xPM  1}

	trm803xEntry	OBJECT-TYPE
		SYNTAX		Trm803xEntry
		MAX-ACCESS	not-accessible
		STATUS		current
		DESCRIPTION	
			"A set of parameters that describe a 803x unit."
		INDEX		{trm803xRackNumber,trm803xSlotNumber}
		::=  {trm803xTable 1}

	Trm803xEntry  ::=  SEQUENCE {
		trm803xRackNumber	  RackNumber,
		trm803xSlotNumber  INTEGER,
		trm803xMuxType	MuxType,
		trm803xIDStringLine DisplayString,
		trm803xIPAddressLine DisplayString
		}

	trm803xRackNumber	OBJECT-TYPE	
		SYNTAX			RackNumber
		MAX-ACCESS		not-accessible
		STATUS			current
		DESCRIPTION		
			"This object indicates in what rack the 803x
			unit is situated in."
		::=  {trm803xEntry  1 }


	trm803xSlotNumber	OBJECT-TYPE	
		SYNTAX			INTEGER
		MAX-ACCESS		not-accessible
		STATUS			current
		DESCRIPTION		
			"This object indicates in what slot number the
			803x unitis situated in. Can only be slot 12."
		::=  {trm803xEntry  2 }

	trm803xMuxType	OBJECT-TYPE	
		SYNTAX			MuxType
		MAX-ACCESS		read-only
		STATUS			current
		DESCRIPTION		
			"This parameter indicates what Mux type this is. 
			Can be none(1), mux8030(2), mux8031(3), mux8032(4),
			mux8033(5) or mux8034(6)."
		::=  {trm803xEntry  3 }

	trm803xIDStringLine	OBJECT-TYPE	
		SYNTAX			DisplayString
		MAX-ACCESS		read-write
		STATUS			current
		DESCRIPTION		
			"This parameter identifies the equipment connected to
			the line interface.
			The ID string can be used to indentify a fiber pair/link.
			Maximum number of characters is 20."
		::=  {trm803xEntry  4 }

	trm803xIPAddressLine	OBJECT-TYPE	
		SYNTAX			DisplayString
		MAX-ACCESS		read-write
		STATUS			current
		DESCRIPTION		
			"This parameter identifies the equipment connected to
			the line interface.
			The IP address can be used to indentify the corresponding
			NMB unit."
		::=  {trm803xEntry  5 }



