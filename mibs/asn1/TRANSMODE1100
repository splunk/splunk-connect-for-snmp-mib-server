		::=  {trmAlarmExternalEntry  3 }

-- The AlarmLog group
--
-- The Alarm group is a collection of objects for monitoring of
-- history alarms in the system.
	trmAlarmLogTable	OBJECT-TYPE
		SYNTAX		SEQUENCE  OF  TrmAlarmLogEntry
		MAX-ACCESS	not-accessible
		STATUS		current
		DESCRIPTION	
			"A table of history alarms in the system. The number of
			entries are as many as there are acknowledged alarms in
			the system."
		::=  {trmAlarmLog  1}

	trmAlarmLogEntry	OBJECT-TYPE
		SYNTAX		TrmAlarmLogEntry
		MAX-ACCESS	not-accessible
		STATUS		current
		DESCRIPTION	
			"A set of parameters that describe a history alarm.
			A row in this table can not be created or deleted
			by SNMP operations on columns of the table."
		INDEX		{trmAlarmLogIndex }
		::=  {trmAlarmLogTable 1}

	TrmAlarmLogEntry  ::=  SEQUENCE {
		trmAlarmLogIndex 		INTEGER,
		trmAlarmLogRackNumber  RackNumber,
		trmAlarmLogSlotNumber  SlotNumber,
		trmAlarmLogName	DisplayString,
		trmAlarmLogSeverity	PerceivedSeverity,
		trmAlarmLogUnit	DisplayString,
		trmAlarmLogSerialNumber	DisplayString,
		trmAlarmLogActTime	DisplayString,
		trmAlarmLogDeactTime	DisplayString,
		trmAlarmLogAckTime	DisplayString,
		trmAlarmLogAckUser	DisplayString
		}


	trmAlarmLogIndex	OBJECT-TYPE
		SYNTAX		INTEGER  (1 .. 2147483647 ) 
		MAX-ACCESS	not-accessible
		STATUS		current
		DESCRIPTION	
				"The index for this alarm entry."
		::=  {trmAlarmLogEntry  1 }


	trmAlarmLogRackNumber	OBJECT-TYPE	
		SYNTAX			RackNumber
		MAX-ACCESS		read-only
		STATUS			current
		DESCRIPTION		
				"The rack number where the alarm occured."
		::=  {trmAlarmLogEntry  2 }


	trmAlarmLogSlotNumber	OBJECT-TYPE	
		SYNTAX			SlotNumber
		MAX-ACCESS		read-only
		STATUS			current
		DESCRIPTION		
				"The slot number where the alarm occured."
		::=  {trmAlarmLogEntry  3 }


	trmAlarmLogName	OBJECT-TYPE	
		SYNTAX			DisplayString
		MAX-ACCESS		read-only
		STATUS			current
		DESCRIPTION		
				"The alarm description."
		::=  {trmAlarmLogEntry  4 }


	trmAlarmLogSeverity	OBJECT-TYPE	
		SYNTAX			PerceivedSeverity
		MAX-ACCESS		read-only
		STATUS			current
		DESCRIPTION		
				"The alarm severity. Can be indeterminate (0) ,
				critical (1) , major (2) , minor (3) or 
				warning (4)."
		::=  {trmAlarmLogEntry  5 }


	trmAlarmLogUnit	OBJECT-TYPE	
		SYNTAX			DisplayString
		MAX-ACCESS		read-only
		STATUS			current
		DESCRIPTION		
				"The type, version and index for the unit associated
				with the alarm."
		::=  {trmAlarmLogEntry  6 }


	trmAlarmLogSerialNumber	OBJECT-TYPE	
		SYNTAX			DisplayString
		MAX-ACCESS		read-only
		STATUS			current
		DESCRIPTION		
				"The serial number for the unit associated with the
				alarm."
		::=  {trmAlarmLogEntry  7 }


	trmAlarmLogActTime	OBJECT-TYPE	
		SYNTAX			DisplayString
		MAX-ACCESS		read-only
		STATUS			current
		DESCRIPTION		
				"The time the alarm was generated."
		::=  {trmAlarmLogEntry  8 }
	

	trmAlarmLogDeactTime	OBJECT-TYPE	
		SYNTAX			DisplayString
		MAX-ACCESS		read-only
		STATUS			current
		DESCRIPTION		
				"The time the alarm was deactivated."
		::=  {trmAlarmLogEntry  9 }


	trmAlarmLogAckTime	OBJECT-TYPE	
		SYNTAX			DisplayString
		MAX-ACCESS		read-only
		STATUS			current
		DESCRIPTION		
				"This time the alarm was acknowledged."
		::=  {trmAlarmLogEntry  10 }


	trmAlarmLogAckUser	OBJECT-TYPE	
		SYNTAX			DisplayString
		MAX-ACCESS		read-only
		STATUS			current
		DESCRIPTION		
				"The user that acknowledged the alarm."
		::=  {trmAlarmLogEntry  11 }


-- The AlarmActive group
--
-- The AlarmActive group is a collection of objects for monitoring of
-- active alarms in the system.

	trmAlarmActiveGeneral	OBJECT IDENTIFIER
		::=  {trmAlarmActive  1 }

	trmAlarmActiveCounter	OBJECT-TYPE	
		SYNTAX			INTEGER  (-1 .. 32768 )
		MAX-ACCESS		read-only
		STATUS			current
		DESCRIPTION	"Indicates the total number of active alarms in the
				system."
		::=  {trmAlarmActiveGeneral  1 }

	trmAlarmActiveAcknowledgeAllDeact	OBJECT-TYPE	
		SYNTAX			AlarmAcknowledge
		MAX-ACCESS		read-write
		STATUS			current
		DESCRIPTION		
				"Acknowledges all alarm that is not acknowledged and
				is deactivated.
				To acknowledge, a SetRequest-PDU with a yes(1) value
				encoding is used."
		::=  {trmAlarmActiveGeneral  2 }

	trmAlarmActiveAcknowledgeAll	OBJECT-TYPE	
		SYNTAX			AlarmAcknowledge
		MAX-ACCESS		read-write
		STATUS			current
		DESCRIPTION		
				"Acknowledgedges all alarms that is not acknowledged.
				To acknowledge, a SetRequest-PDU with a yes(1) value
				encoding is used."
		::=  {trmAlarmActiveGeneral  3 }


	trmAlarmActiveTable	OBJECT-TYPE
		SYNTAX		SEQUENCE  OF  TrmAlarmActiveEntry
		MAX-ACCESS	not-accessible
		STATUS		current
		DESCRIPTION	"A table of active alarms in the system. With active
				alarm the meaning is a not acknowledged alarm. The
				number of entries are as many as there are not
				acknowledged alarms in the system."
		::=  {trmAlarmActive  2}

	trmAlarmActiveEntry	OBJECT-TYPE
		SYNTAX		TrmAlarmActiveEntry
		MAX-ACCESS	not-accessible
		STATUS		current
		DESCRIPTION	
			"An entry in the table of active alarms in the 
			system.
			A row in this table can not be created or deleted
			by SNMP operations on columns of the table."
		INDEX		{trmAlarmActiveIndex }
		::=  {trmAlarmActiveTable 1}

	TrmAlarmActiveEntry  ::=  SEQUENCE {
		trmAlarmActiveIndex 		INTEGER,
		trmAlarmActiveRackNumber  RackNumber,
		trmAlarmActiveSlotNumber  SlotNumber,
		trmAlarmActiveName	DisplayString,
		trmAlarmActiveSeverity	PerceivedSeverity,
		trmAlarmActiveUnit	DisplayString,
		trmAlarmActiveSerialNumber	DisplayString,
		trmAlarmActiveActTime	DisplayString,
		trmAlarmActiveDeactTime	DisplayString,
		trmAlarmActiveAcknowledge	AlarmAcknowledge
		}

	trmAlarmActiveIndex	OBJECT-TYPE
		SYNTAX		INTEGER  (1 .. 2147483647 ) 
		MAX-ACCESS	not-accessible
		STATUS		current
		DESCRIPTION	
				"The index for this alarm entry."
		::=  {trmAlarmActiveEntry  1 }


	trmAlarmActiveRackNumber	OBJECT-TYPE	
		SYNTAX			RackNumber
		MAX-ACCESS		read-only
		STATUS			current
		DESCRIPTION		
				"The rack number where the alarm occured."
		::=  {trmAlarmActiveEntry  2 }


	trmAlarmActiveSlotNumber	OBJECT-TYPE	
		SYNTAX			SlotNumber
		MAX-ACCESS		read-only
		STATUS			current
		DESCRIPTION		
				"The slot number where the alarm occured."
		::=  {trmAlarmActiveEntry  3 }


	trmAlarmActiveName	OBJECT-TYPE	
		SYNTAX			DisplayString
		MAX-ACCESS		read-only
		STATUS			current
		DESCRIPTION		
				"The alarm description"
		::=  {trmAlarmActiveEntry  4 }


	trmAlarmActiveSeverity	OBJECT-TYPE	
		SYNTAX			PerceivedSeverity
		MAX-ACCESS		read-only
		STATUS			current
		DESCRIPTION		
				"The alarm severity. Can be indeterminate (0) ,
				critical (1) , major (2) , minor (3) or 
				warning (4)."
		::=  {trmAlarmActiveEntry  5 }


	trmAlarmActiveUnit	OBJECT-TYPE	
		SYNTAX			DisplayString
		MAX-ACCESS		read-only
		STATUS			current
		DESCRIPTION		
				"The type, version and index of the unit associated
				with the alarm."
		::=  {trmAlarmActiveEntry  6 }


	trmAlarmActiveSerialNumber	OBJECT-TYPE	
		SYNTAX			DisplayString
		MAX-ACCESS		read-only
		STATUS			current
		DESCRIPTION		
				"The serial number of the unit associated with the
				alarm."
		::=  {trmAlarmActiveEntry  7 }


	trmAlarmActiveActTime	OBJECT-TYPE	
		SYNTAX			DisplayString
		MAX-ACCESS		read-only
		STATUS			current
		DESCRIPTION		
				"The time the alarm was generated."
		::=  {trmAlarmActiveEntry  8 }

	trmAlarmActiveDeactTime	OBJECT-TYPE	
		SYNTAX			DisplayString
		MAX-ACCESS		read-only
		STATUS			current
		DESCRIPTION		
				"The time the alarm was deactivated."
		::=  {trmAlarmActiveEntry  9 }

	trmAlarmActiveAcknowledge	OBJECT-TYPE	
		SYNTAX			AlarmAcknowledge
		MAX-ACCESS		read-write
		STATUS			current
		DESCRIPTION		
				"This oid is used to acknowledge a alarm. To
				acknowledge a alarm, a SetRequest-PDU with a acknowledge(1)
				value encoding is used."
		::=  {trmAlarmActiveEntry  10 }



