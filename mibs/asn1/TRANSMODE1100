	        	trmAlarmActiveRackNumber,
	        	trmAlarmActiveSlotNumber,
	        	trmAlarmActiveSerialNumber
	        }
		STATUS			current
		DESCRIPTION		
				"This trap is sent when the optical input power is
				lost at the client interface."
		::=  {  trm75xxTraps  6 }

	trm75xxTrapTxClient	NOTIFICATION-TYPE
		OBJECTS {
			trmAlarmActiveName,
			trmAlarmActiveSeverity,
			trmAlarmActiveActTime,
			trmAlarmActiveUnit,
	        	trmAlarmActiveRackNumber,
	        	trmAlarmActiveSlotNumber,
	        	trmAlarmActiveSerialNumber
	        }
		STATUS			current
		DESCRIPTION		
				"This trap is sent when a unit fails to generate
				optical output power at Tx client."
		::=  {  trm75xxTraps  7 }

	trm75xxTrapTxDisableClient	NOTIFICATION-TYPE
		OBJECTS {
			trmAlarmActiveName,
			trmAlarmActiveSeverity,
			trmAlarmActiveActTime,
			trmAlarmActiveUnit,
	        	trmAlarmActiveRackNumber,
