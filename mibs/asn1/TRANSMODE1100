--*****************************************************************************************
--*****************************************************************************************
-- General Subrack fault management 
--*****************************************************************************************
--*****************************************************************************************

	trmSubrackFM 	OBJECT IDENTIFIER
		::=  {trmSubrack 3 }


	trmSubrackTraps	OBJECT IDENTIFIER ::= { trmSubrackFM 0 }

	trmTrapBmiLost	NOTIFICATION-TYPE
		OBJECTS {
			trmAlarmActiveName,
			trmAlarmActiveSeverity,
			trmAlarmActiveActTime,
			trmAlarmActiveUnit,
	        	trmAlarmActiveRackNumber,
	        	trmAlarmActiveSlotNumber,
	        	trmAlarmActiveSerialNumber
	        }
		STATUS			current
		DESCRIPTION		
				"This trap is sent when communication with a unit
				is lost."
		::=  {  trmSubrackTraps 1 }


	trmTrapBmiError	NOTIFICATION-TYPE
		OBJECTS {
			trmAlarmActiveName,
			trmAlarmActiveSeverity,
			trmAlarmActiveActTime,
			trmAlarmActiveUnit,
	        	trmAlarmActiveRackNumber,
	        	trmAlarmActiveSlotNumber,
	        	trmAlarmActiveSerialNumber
	        }
		STATUS			current
		DESCRIPTION		
				"This trap is sent when a internal BMI error occur."
		::=  {  trmSubrackTraps 2 }

	trmTrapHighTemp	NOTIFICATION-TYPE
		OBJECTS {
			trmAlarmActiveName,
			trmAlarmActiveSeverity,
			trmAlarmActiveActTime,
			trmAlarmActiveUnit,
	        	trmAlarmActiveRackNumber,
	        	trmAlarmActiveSlotNumber,
	        	trmAlarmActiveSerialNumber
	        }
		STATUS			current
		DESCRIPTION		
				"This trap is sent when a high temperature alarm
				occurs on the NMB 6001 unit or a unit with 
				temperature measurement."
		::=  {  trmSubrackTraps  3  }

	trmTrapLowTemp	NOTIFICATION-TYPE
		OBJECTS {
			trmAlarmActiveName,
			trmAlarmActiveSeverity,
			trmAlarmActiveActTime,
			trmAlarmActiveUnit,
	        	trmAlarmActiveRackNumber,
	        	trmAlarmActiveSlotNumber,
	        	trmAlarmActiveSerialNumber
	        }
		STATUS			current
		DESCRIPTION		
				"This trap is sent when a low temperature alarm
				occurs on the NMB 6001 unit or a unit with 
				temperature measurement."
		::=  {  trmSubrackTraps  4  }

	trmTrapVeryHighTemp	NOTIFICATION-TYPE
		OBJECTS {
			trmAlarmActiveName,
			trmAlarmActiveSeverity,
			trmAlarmActiveActTime,
			trmAlarmActiveUnit,
	        	trmAlarmActiveRackNumber,
	        	trmAlarmActiveSlotNumber,
	        	trmAlarmActiveSerialNumber
	        }
		STATUS			current
		DESCRIPTION		
				"This trap is sent when a very high temperature
				alarm occurs on the NMB 6001 unit or a unit with 
				temperature measurement."
		::=  {  trmSubrackTraps  5  }

	trmTrapSpeedLimitExceeded	NOTIFICATION-TYPE
		OBJECTS {
			trmAlarmActiveName,
			trmAlarmActiveSeverity,
			trmAlarmActiveActTime,
			trmAlarmActiveUnit,
	        	trmAlarmActiveRackNumber,
	        	trmAlarmActiveSlotNumber,
	        	trmAlarmActiveSerialNumber
	        }
		STATUS			current
		DESCRIPTION		
				"This trap is sent if the Client bitrate is higher than a 
				predefined protocol.
				When the alarm is activated Line and Client Tx is disabled. 
				The alarm is ceased and Tx is enabled again at a lower value
				(hysteresis).
				Speed Limit is deactivated if Tx mode for the unit is
				disabled(2) or forcedon(3)"
		::=  {  trmSubrackTraps  6  }


