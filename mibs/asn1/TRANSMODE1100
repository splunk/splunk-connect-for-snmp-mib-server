--*****************************************************************************************
-- 5300/5500 Subrack units fault management
--*****************************************************************************************


	trm53005500FM 	OBJECT IDENTIFIER
		::=  {trm53005500 2 }

	trm53005500Traps	OBJECT IDENTIFIER ::= { trm53005500FM 0 }

	trm53005500TrapRxLinkdownLine	NOTIFICATION-TYPE
		OBJECTS {
			trmAlarmActiveName,
			trmAlarmActiveSeverity,
			trmAlarmActiveActTime,
			trmAlarmActiveUnit,
	        	trmAlarmActiveRackNumber,
	        	trmAlarmActiveSlotNumber,
	        	trmAlarmActiveSerialNumber
	        }
		STATUS			current
		DESCRIPTION		
				"This trap is sent when the Line Rx signal is out
				of sync or has been lost."
		::=  {  trm53005500Traps  1 }

	trm53005500TrapExternalsyncFailureLine	NOTIFICATION-TYPE
		OBJECTS {
			trmAlarmActiveName,
			trmAlarmActiveSeverity,
			trmAlarmActiveActTime,
			trmAlarmActiveUnit,
	        	trmAlarmActiveRackNumber,
	        	trmAlarmActiveSlotNumber,
	        	trmAlarmActiveSerialNumber
	        }
		STATUS			current
		DESCRIPTION		
				"This trap is sent when the Line external sync signal
				is lost or is out of sync."
		::=  {  trm53005500Traps  2 }

	trm53005500TrapSfpMissingLine	NOTIFICATION-TYPE
		OBJECTS {
			trmAlarmActiveName,
			trmAlarmActiveSeverity,
			trmAlarmActiveActTime,
			trmAlarmActiveUnit,
	        	trmAlarmActiveRackNumber,
	        	trmAlarmActiveSlotNumber,
	        	trmAlarmActiveSerialNumber
	        }
		STATUS			current
		DESCRIPTION		
				"This trap is sent when the Line SFP transiever not is mounted."
		::=  {  trm53005500Traps  3 }

	trm53005500TrapSfpBadTypeLine	NOTIFICATION-TYPE
		OBJECTS {
			trmAlarmActiveName,
			trmAlarmActiveSeverity,
			trmAlarmActiveActTime,
			trmAlarmActiveUnit,
	        	trmAlarmActiveRackNumber,
	        	trmAlarmActiveSlotNumber,
	        	trmAlarmActiveSerialNumber
	        }
		STATUS			current
		DESCRIPTION		
				"This trap is sent when a invalid Line SFP is used."
		::=  {  trm53005500Traps  4 }

	trm53005500TrapSfpFaultyLine	NOTIFICATION-TYPE
		OBJECTS {
			trmAlarmActiveName,
			trmAlarmActiveSeverity,
			trmAlarmActiveActTime,
			trmAlarmActiveUnit,
