		MAX-ACCESS		read-only
		STATUS			current
		DESCRIPTION		"This parameter indicates the Estimated Fiber Rate
					for the client interface Rx in Mbit/s."
		::=  {trm76xxEntry  22}


	trm76xxTxClient	OBJECT-TYPE	
		SYNTAX			DisplayString
		MAX-ACCESS		read-only
		STATUS			current
		DESCRIPTION		"This parameter indicates the status of the optical
					output power for the client interface. It corresponds
					to the client Tx LED status of the unit.
					Can be on or off."
		::=  {trm76xxEntry  23}


	trm76xxIDStringClient	OBJECT-TYPE	
		SYNTAX			DisplayString
		MAX-ACCESS		read-write
		STATUS			current
		DESCRIPTION		
			"This parameter identifies the equipment(ID String) 
			connected to the client interface.
			Maximum number of characters is 20."
		::=  {trm76xxEntry  24 }


	trm76xxIPAddressClient	OBJECT-TYPE	
		SYNTAX			DisplayString
		MAX-ACCESS		read-write
		STATUS			current
		DESCRIPTION		
			"This parameter indentifies the equipment(IP Address)
			connected to the client interface."
		::=  {trm76xxEntry  25 }


	trm76xxWavelengthClient	OBJECT-TYPE	
		SYNTAX			DisplayString
		MAX-ACCESS		read-only
		STATUS			current
		DESCRIPTION		"This parameter indicates the nominal wavelength for
					the client interface transiever."
		::=  {trm76xxEntry  26}

	trm76xxOpticalInPowClient	OBJECT-TYPE	
		SYNTAX			INTEGER
		MAX-ACCESS		read-only
		STATUS			current
		DESCRIPTION		"This parameter indicates the Optical input power
					for the client interface. When the Optical input power
					is to low to measure this parameter is -500 (-50 dBm). If
					the Optical output power is to high to measure this
					parameter is -20 (-2 dBm).
					If this parameter is invalid -1 (-0.1 dBm) is returned.
					Divide this value by 10 to show optical input power
					in dBm."
		::=  {trm76xxEntry  27}

	trm76xxOpticalOutPowClient	OBJECT-TYPE	
		SYNTAX			INTEGER
		MAX-ACCESS		read-only
		STATUS			current
		DESCRIPTION		"This parameter indicates the optical output power
					for the client interface. If data not is available yet
					this parameter is -1(-0.1 dBm). If this parameter is 
					-100 (-10 dBm) the output power is equal too or less
					than -100 (-10 dBm).
					Divide this value by 10 to show the optical output
					power in dBm."
		::=  {trm76xxEntry  28}

	trm76xxLaserBiasCurClient	OBJECT-TYPE	
		SYNTAX			INTEGER
		MAX-ACCESS		read-only
		STATUS			current
		DESCRIPTION		"This parameter indicates the laser bias current for
					the client interface. If this parameter is 5450(545 mA)
					the laser bias current is more or equal to 545 mA.
					Divide this value by 10 to show the laser bias
					current in mA."
		::=  {trm76xxEntry  29}

--*****************************************************************************************
-- 76YY/ZZ Subrack units fault management
--*****************************************************************************************


	trm76xxFM 	OBJECT IDENTIFIER
		::=  {trm76xx 2 }

	trm76xxTraps	OBJECT IDENTIFIER ::= { trm76xxFM 0 }

	trm76xxTrapCDRAutoModeRangeChangeLine	NOTIFICATION-TYPE
		OBJECTS {
			trmAlarmActiveName,
			trmAlarmActiveSeverity,
			trmAlarmActiveActTime,
			trmAlarmActiveUnit,
	        	trmAlarmActiveRackNumber,
	        	trmAlarmActiveSlotNumber,
	        	trmAlarmActiveSerialNumber
	        }
		STATUS			current
		DESCRIPTION		
				"This trap is sent when a client equipment might be
				using a protocol not specified for auto mode or it
				might have changed the protocol. If auto mode is
				used consider goto manual mode instead.
				This trap concerns the regeneration at the Tx line
				output."
		::=  {  trm76xxTraps  1 }

	trm76xxTrapRxGlitchLine	NOTIFICATION-TYPE
		OBJECTS {
			trmAlarmActiveName,
			trmAlarmActiveSeverity,
			trmAlarmActiveActTime,
			trmAlarmActiveUnit,
	        	trmAlarmActiveRackNumber,
	        	trmAlarmActiveSlotNumber,
	        	trmAlarmActiveSerialNumber
	        }
		STATUS			current
		DESCRIPTION		
				"This trap is sent when a unit experiences a split second
				optical	input power glitch at the line interface."
		::=  {  trm76xxTraps  2 }

	trm76xxTrapLopLine	NOTIFICATION-TYPE
		OBJECTS {
			trmAlarmActiveName,
			trmAlarmActiveSeverity,
			trmAlarmActiveActTime,
			trmAlarmActiveUnit,
	        	trmAlarmActiveRackNumber,
	        	trmAlarmActiveSlotNumber,
	        	trmAlarmActiveSerialNumber
	        }
		STATUS			current
		DESCRIPTION		
				"This trap is sent when the optical input power	is
				lost at the line interface."
		::=  {  trm76xxTraps  3 }

	trm76xxTrapRxHighPowerLine	NOTIFICATION-TYPE
		OBJECTS {
			trmAlarmActiveName,
			trmAlarmActiveSeverity,
			trmAlarmActiveActTime,
			trmAlarmActiveUnit,
	        	trmAlarmActiveRackNumber,
	        	trmAlarmActiveSlotNumber,
	        	trmAlarmActiveSerialNumber
	        }
		STATUS			current
		DESCRIPTION		
				"This trap is sent when the optical input power for 
				line is higher than trmOpticalInputPowerHighPINLine
				or trmOpticalInputPowerHighAPDLine."
		::=  {  trm76xxTraps  4 }

	trm76xxTrapRxLowPowerLine	NOTIFICATION-TYPE
		OBJECTS {
			trmAlarmActiveName,
			trmAlarmActiveSeverity,
			trmAlarmActiveActTime,
			trmAlarmActiveUnit,
	        	trmAlarmActiveRackNumber,
	        	trmAlarmActiveSlotNumber,
	        	trmAlarmActiveSerialNumber
	        }
		STATUS			current
		DESCRIPTION		
				"This trap is sent when the optical input power for
				line is lower than trmOpticalInputPowerLowPINLine or
				trmOpticalInputPowerLowAPDLine."
		::=  {  trm76xxTraps  5 }

	trm76xxTrapLoLLine	NOTIFICATION-TYPE
		OBJECTS {
			trmAlarmActiveName,
			trmAlarmActiveSeverity,
			trmAlarmActiveActTime,
			trmAlarmActiveUnit,
	        	trmAlarmActiveRackNumber,
	        	trmAlarmActiveSlotNumber,
