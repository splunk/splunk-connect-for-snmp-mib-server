			trmAlarmActiveUnit,
	        	trmAlarmActiveRackNumber,
	        	trmAlarmActiveSlotNumber,
	        	trmAlarmActiveSerialNumber
	        }
		STATUS			current
		DESCRIPTION		
				"This trap is sent when the optical output power
				for the line interface is disabled."
		::=  {  trm75xxTraps  4 }


	trm75xxTrapGlitchClient	NOTIFICATION-TYPE
		OBJECTS {
			trmAlarmActiveName,
			trmAlarmActiveSeverity,
			trmAlarmActiveActTime,
			trmAlarmActiveUnit,
	        	trmAlarmActiveRackNumber,
	        	trmAlarmActiveSlotNumber,
	        	trmAlarmActiveSerialNumber
	        }
		STATUS			current
		DESCRIPTION		
				"This trap is sent when a unit experiences a split second
				optical	input power glitch at the client interface."
		::=  {  trm75xxTraps  5 }

	trm75xxTrapLopClient	NOTIFICATION-TYPE
		OBJECTS {
			trmAlarmActiveName,
