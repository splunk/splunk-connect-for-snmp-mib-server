	trmOpticalInputPowerHighAPDLine2	OBJECT-TYPE	
		SYNTAX			INTEGER
		MAX-ACCESS		read-write
		STATUS			current
		DESCRIPTION		
			"Optical input power high alarm level for client interface APDs.
			Applicable for 76xx/15 units(repeaters).
			Value displayed and set is dBm/10."
		::=  {trmAlarmGeneral  15 }

	trmOpticalInputPowerLowAPDLine2	OBJECT-TYPE	
		SYNTAX			INTEGER
		MAX-ACCESS		read-write
		STATUS			current
		DESCRIPTION		
