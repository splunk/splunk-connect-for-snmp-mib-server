-- 25xx Subrack units fault management
--*****************************************************************************************


	trm25xxFM 	OBJECT IDENTIFIER
		::=  {trm25xx 2 }

	trm25xxTraps	OBJECT IDENTIFIER ::= { trm25xxFM 0 }

	trm25xxTrapWorkingLineFailure	NOTIFICATION-TYPE
		OBJECTS {
			trmAlarmActiveName,
			trmAlarmActiveSeverity,
			trmAlarmActiveActTime,
			trmAlarmActiveUnit,
	        	trmAlarmActiveRackNumber,
	        	trmAlarmActiveSlotNumber,
	        	trmAlarmActiveSerialNumber
	        }
		STATUS			current
		DESCRIPTION		
				"This trap is sent when the Working Line goes down."
		::=  {  trm25xxTraps  1 }

	trm25xxTrapProtectingLineFailure	NOTIFICATION-TYPE
		OBJECTS {
			trmAlarmActiveName,
			trmAlarmActiveSeverity,
			trmAlarmActiveActTime,
			trmAlarmActiveUnit,
	        	trmAlarmActiveRackNumber,
	        	trmAlarmActiveSlotNumber,
	        	trmAlarmActiveSerialNumber
	        }
		STATUS			current
		DESCRIPTION		
				"This trap is sent when the Protecting Line goes down."
		::=  {  trm25xxTraps  2 }

	trm25xxTrapWorkingAndProtectingLineFailure	NOTIFICATION-TYPE
		OBJECTS {
			trmAlarmActiveName,
			trmAlarmActiveSeverity,
			trmAlarmActiveActTime,
			trmAlarmActiveUnit,
	        	trmAlarmActiveRackNumber,
	        	trmAlarmActiveSlotNumber,
	        	trmAlarmActiveSerialNumber
	        }
		STATUS			current
		DESCRIPTION		
				"This trap is sent when both the Working and Protecting
				Line goes down."
		::=  {  trm25xxTraps  3 }


