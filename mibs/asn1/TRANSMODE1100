	trmSubrackList	OBJECT IDENTIFIER
		::=  {trmSubrack 1 }
		
	trmSubrackListTable	OBJECT-TYPE
		SYNTAX		SEQUENCE  OF  TrmSubrackListEntry
		MAX-ACCESS	not-accessible
		STATUS		current
		DESCRIPTION	
			"A list of units present in the subrack(s)."
			::=  {trmSubrackList  1}

	trmSubrackListEntry	OBJECT-TYPE
		SYNTAX		TrmSubrackListEntry
		MAX-ACCESS	not-accessible
		STATUS		current
		DESCRIPTION	
			"A set of parameters that describe a unit."
		INDEX		{trmListRackNumber,trmListSlotNumber }
		::=  {trmSubrackListTable 1}

	TrmSubrackListEntry  ::=  SEQUENCE {
		trmListRackNumber  RackNumber,
		trmListSlotNumber  SlotNumber,
		trmListUnitPresent Present,
		trmListProductNumber  DisplayString,
		trmListProductDescription DisplayString,
		trmListHwRevision  DisplayString,
		trmListSerialNumber DisplayString,
		trmListManufacturingDate DisplayString,
		trmListSoftwareProdNo DisplayString,
		trmListSwVersion DisplayString
		}

	trmListRackNumber	OBJECT-TYPE	
		SYNTAX			RackNumber
		MAX-ACCESS		not-accessible
		STATUS			current
		DESCRIPTION		
			"This object indicates in what rack the unit is
			situated in."
		::=  {trmSubrackListEntry  1 }


	trmListSlotNumber	OBJECT-TYPE	
		SYNTAX			SlotNumber
		MAX-ACCESS		not-accessible
		STATUS			current
		DESCRIPTION		
			"This object indicates in what slot the unit is
			situated in. Can be slot 1-12."
		::=  {trmSubrackListEntry  2 }


	trmListUnitPresent	OBJECT-TYPE	
		SYNTAX			Present
		MAX-ACCESS		read-only
		STATUS			current
		DESCRIPTION		
			"This object indicates if the unit is present
			yes(1) or not no(2)."
		::=  {trmSubrackListEntry  3 }


	trmListProductNumber	OBJECT-TYPE	
		SYNTAX			DisplayString
		MAX-ACCESS		read-only
		STATUS			current
		DESCRIPTION		
			"The Product number of the unit (XXYY/ZZ) where:
			Board type = XX
			Board version = YY
			Board index = ZZ."
		::=  {trmSubrackListEntry  4 }

	trmListProductDescription	OBJECT-TYPE	
		SYNTAX			DisplayString
		MAX-ACCESS		read-only
		STATUS			current
		DESCRIPTION		
			"A product description of the unit."
		::=  {trmSubrackListEntry  5 }


	trmListHwRevision	OBJECT-TYPE	
		SYNTAX			DisplayString
		MAX-ACCESS		read-only
		STATUS			current
		DESCRIPTION		
			"The hardware revision of the unit."
		::=  {trmSubrackListEntry  6 }


	trmListSerialNumber	OBJECT-TYPE	
		SYNTAX			DisplayString
		MAX-ACCESS		read-only
		STATUS			current
		DESCRIPTION		
			"The serial number of the unit."
		::=  {trmSubrackListEntry  7}


	trmListManufacturingDate	OBJECT-TYPE	
		SYNTAX			DisplayString
		MAX-ACCESS		read-only
		STATUS			current
		DESCRIPTION		
			"The manufacturing date of the unit. Format is
			Year:Week."
		::=  {trmSubrackListEntry  8}


	trmListSoftwareProdNo	OBJECT-TYPE	
		SYNTAX			DisplayString
		MAX-ACCESS		read-only
		STATUS			current
		DESCRIPTION		
			"The software product number of the software on this unit."
		::=  {trmSubrackListEntry  9}


	trmListSwVersion	OBJECT-TYPE	
		SYNTAX			DisplayString
		MAX-ACCESS		read-only
		STATUS			current
		DESCRIPTION		
			"Revision number of the software on this unit."
		::=  {trmSubrackListEntry  10}


