-- Alarm Configuration

	trmAlarmGeneral	OBJECT IDENTIFIER
		::=  {trmAlarmConfig  1 }
		
	trmAutoAcknowledge	OBJECT-TYPE	
		SYNTAX			INTEGER { on(1), off(2) }
		MAX-ACCESS		read-write
		STATUS			current
		DESCRIPTION		
				"Indicates if alarms should be automatically
				acknowledged or not. Can be on(1) or off(2)."
		::=  {trmAlarmGeneral  1 }

	trmClearAlarmLog	OBJECT-TYPE	
		SYNTAX			INTEGER { clear(1) }
		MAX-ACCESS		read-write
		STATUS			current
		DESCRIPTION		
				"Used to delete all history alarms. To delete all
				history alarms, a SetRequest-PDU with a clear(1)
				value encoding is used."
		::=  {trmAlarmGeneral  2 }

	trmTemperature	OBJECT-TYPE	
		SYNTAX			INTEGER { celsius(1), farenheit(2) }
		MAX-ACCESS		read-write
		STATUS			current
		DESCRIPTION		
				"Indicates the temperature unit used in the system.
				Can be celsius(1) or farenheit(2)."
		::=  {trmAlarmGeneral  3 }

	trmAlarmTemperatureHigh	OBJECT-TYPE	
		SYNTAX			INTEGER  (-32767 .. 32768 ) 
		MAX-ACCESS		read-write
		STATUS			current
		DESCRIPTION		
			"High temperature level. This parameter indicates
			at what level a High temperature alarm will occur
			in the system. 
			See trmTemperature if deg. Celsius or
			Farenheit is used."
		::=  {trmAlarmGeneral  4 }

	trmAlarmTemperatureHighHyst	OBJECT-TYPE	
		SYNTAX			INTEGER  (-32767 .. 32768 ) 
		MAX-ACCESS		read-write
		STATUS			current
		DESCRIPTION		
			"High termperature hysteresis. This parameter is
			used together with trmAlarmTempemperatureHigh to
			decide when a High temperature alarm will cease.
			The alarm will cease at 
			trmAlarmTemperatureHigh - trmAlarmTemperatureHighHyst.
			See trmTemperature if deg. Celsius or
			Farenheit is used."
		::=  {trmAlarmGeneral  5 }

	trmAlarmTemperatureLow	OBJECT-TYPE	
		SYNTAX			INTEGER  (-32767 .. 32768 ) 
		MAX-ACCESS		read-write
		STATUS			current
		DESCRIPTION		
			"Low temperature level. This parameter indicates
			at what level a Low temperature alarm will occur
			in the system. 
			See trmTemperature if deg. Celsius or
			Farenheit is used."
		::=  {trmAlarmGeneral  6 }

	trmAlarmTemperatureLowHyst	OBJECT-TYPE	
		SYNTAX			INTEGER  (-32767 .. 32768 ) 
		MAX-ACCESS		read-write
		STATUS			current
		DESCRIPTION		
			"Low termperature hysteresis. This parameter is
			used together with trmAlarmTemperatureLow to decide
			when a Low temperature alarm will cease. The alarm will
			cease at trmAlarmTemperatureLow - 
			trmAlarmTemperatureLowHyst.
			See trmTemperature if deg. Celsius or Farenheit is used."
		::=  {trmAlarmGeneral  7 }

	trmPluginReset	OBJECT-TYPE	
		SYNTAX			INTEGER { reset(1) }
		MAX-ACCESS		read-write
		STATUS			current
		DESCRIPTION		
				"Used to reset all the plug in units. To reset all
				units, a SetRequest-PDU with a reset(1) value
				encoding is used."
		::=  {trmAlarmGeneral  8 }

