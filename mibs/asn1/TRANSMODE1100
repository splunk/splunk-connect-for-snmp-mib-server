--*****************************************************************************************
--*****************************************************************************************
-- 25xx Interface units
--*****************************************************************************************
--*****************************************************************************************

	trm25xx 	OBJECT IDENTIFIER
		::=  {trmSubrackUnits 8 }

--*****************************************************************************************
-- 25YY/ZZ Interface units performance management
--*****************************************************************************************

	trm25xxPM 	OBJECT IDENTIFIER
		::=  {trm25xx 1 }

	trm25xxTable	OBJECT-TYPE
		SYNTAX		SEQUENCE  OF  Trm25xxEntry
		MAX-ACCESS	not-accessible
		STATUS		current
		DESCRIPTION	"A list of 25xx in the subrack(s)."
		::=  {trm25xxPM  1}

	trm25xxEntry	OBJECT-TYPE
		SYNTAX		Trm25xxEntry
		MAX-ACCESS	not-accessible
		STATUS		current
		DESCRIPTION	"A set of parameters that describe the Subrack
				units"
		INDEX		{trm25xxRackNumber,trm25xxSlotNumber }
		::=  {trm25xxTable 1}

	Trm25xxEntry  ::=  SEQUENCE {
		trm25xxRackNumber	RackNumber,
		trm25xxSlotNumber	BoardNumber,
		trm25xxWorkingLineStatus	DisplayString,
		trm25xxProtectingLineStatus	DisplayString,
		trm25xxTemperature	INTEGER,
		trm25xxSwReset		INTEGER,

		trm25xxRxLine		DisplayString,
		trm25xxOpticalInPowLine	INTEGER,
		trm25xxSpeedLimit	SpeedLimit76,
		trm25xxEstProtLine	DisplayString,
		trm25xxEstFibRateLine	DisplayString,

		trm25xxTxLine		DisplayString,
		trm25xxOpticalOutPowLine	INTEGER,
		trm25xxProdOutPowLine	INTEGER,
		trm25xxLaserBiasCurLine	INTEGER,
		trm25xxProdLaserBiasCurLine	INTEGER,

		trm25xxCDR		CDRMode,
		trm25xxCustomFibRate	INTEGER,
		trm25xxTxMode		TxMode76
		}


	trm25xxRackNumber	OBJECT-TYPE	
		SYNTAX			RackNumber
		MAX-ACCESS		not-accessible
		STATUS			current
		DESCRIPTION		"This parameter indicates in what rack the unit is 
					situated in."
		::=  {trm25xxEntry  1}


	trm25xxSlotNumber	OBJECT-TYPE	
		SYNTAX			BoardNumber
		MAX-ACCESS		not-accessible
		STATUS			current
		DESCRIPTION		"This parameter indicates in what slot number the 
					unit is situated in. Can be any of slot 1 - 8."
		::=  {trm25xxEntry  2}


	trm25xxWorkingLineStatus	OBJECT-TYPE	
		SYNTAX			DisplayString
		MAX-ACCESS		read-only
		STATUS			current
		DESCRIPTION		
			"This parameter indicates the Working Line status."
		::=  {trm25xxEntry  3 }


	trm25xxProtectingLineStatus	OBJECT-TYPE	
		SYNTAX			DisplayString
		MAX-ACCESS		read-only
		STATUS			current
		DESCRIPTION		
			"This parameter indicates the Protecting Line status."
		::=  {trm25xxEntry  4 }


	trm25xxTemperature	OBJECT-TYPE	
		SYNTAX			INTEGER
		MAX-ACCESS		read-only
		STATUS			current
		DESCRIPTION		"This parameter indicates the temperature measured
					by the unit in deg. C or deg. F. See 
					trmSubrackConfigTemp if deg. Celsius or Farenheit
					is used."
		::=  {trm25xxEntry  5}


	trm25xxSwReset	OBJECT-TYPE
		SYNTAX			INTEGER { reset(1) }
		MAX-ACCESS		read-write
		STATUS			current
		DESCRIPTION		
					"Used to reset the  unit. To reset the unit, a SetRequest-PDU 
					with a reset(1) value encoding is used."
		::=  {trm25xxEntry  6}


	trm25xxRxLine	OBJECT-TYPE	
		SYNTAX			DisplayString
		MAX-ACCESS		read-only
		STATUS			current
		DESCRIPTION		"This parameter indicates the status of the optical 
					input for the line Subrack. It corresponds to the
					client Rx LED status of the unit.
					Can be Receiving or Not receiving."
		::=  {trm25xxEntry  7}


	trm25xxOpticalInPowLine	OBJECT-TYPE	
		SYNTAX			INTEGER
		MAX-ACCESS		read-only
		STATUS			current
		DESCRIPTION		"This parameter indicates the Optical input power
					for the line Subrack. When the Optical input power
					is to low to measure this parameter is -500 (-50 dBm). If
					the Optical output power is to high to measure this
					parameter is -20 (-2 dBm).
					If this parameter is invalid -1 (-0.1 dBm) is returned.
					Divide this value by 10 to show optical input power
					in dBm."
		::=  {trm25xxEntry  8 }


	trm25xxSpeedLimit	OBJECT-TYPE
		SYNTAX			SpeedLimit76
		MAX-ACCESS		read-write
		STATUS			current
		DESCRIPTION		"This parameter indicates the speed limit set for this unit
					and is used to set a new speed limit. 
					It generates an alarm and disables Line and Client Tx if the
					Client bitrate is higher than a predefined protocol.
					The alarm is ceased and Tx is enabled again at a lower value
					(hysteresis).
					Speed Limit is deactivated if trm25xxTxMode is disabled(2) or
					forcedon(3).
					Speed Limit can be set in 4 modes to allow different protocols:
					noLimit(1): Any protocol is allowed.
					lowLimit(2): 100Base-FX, STM1/OC3, ESCON & Video 270Mb.
					mediumLimit(3): All protocols above + STM4/OC-12.
					highLimit(4): All protocols above + Fibre channel & 
					Gigabit Ethernet."
		::=  {trm25xxEntry  9}


	trm25xxEstProtLine	OBJECT-TYPE	
		SYNTAX			DisplayString
		MAX-ACCESS		read-only
		STATUS			current
		DESCRIPTION		"This parameter indicates the Estimated Protocol for
					the line Subrack input."
		::=  {trm25xxEntry  10}


	trm25xxEstFibRateLine	OBJECT-TYPE	
		SYNTAX			DisplayString
		MAX-ACCESS		read-only
		STATUS			current
		DESCRIPTION		"This parameter indicates the Estimated Fiber Rate
					for the line Subrack input in Mbit/s."
		::=  {trm25xxEntry  11}


	trm25xxTxLine	OBJECT-TYPE	
		SYNTAX			DisplayString
		MAX-ACCESS		read-only
		STATUS			current
		DESCRIPTION		"This parameter indicates the status of the optical
					output power for the line Subrack. It corresponds
					to the line Tx LED status of the unit.
					Can be on or off."
		::=  {trm25xxEntry  12}


	trm25xxOpticalOutPowLine	OBJECT-TYPE	
		SYNTAX			INTEGER
		MAX-ACCESS		read-only
		STATUS			current
		DESCRIPTION		"This parameter indicates the optical output power
					for the line Subrack. If data not is available yet
					this parameter is -1(-0.1 dBm). If this parameter is 
					-100 (-10 dBm) the output power is equal too or less
					than -100 (-10 dBm).
					Divide this value by 10 to show the optical output
					power in dBm."
		::=  {trm25xxEntry  13}


	trm25xxProdOutPowLine	OBJECT-TYPE	
		SYNTAX			INTEGER
		MAX-ACCESS		read-only
		STATUS			current
		DESCRIPTION		"This parameter indicates the production output
					power for the line Subrack. If data not is valid
					yet this parameter is -1 (-0.1 dBm).
					Divide this value by 10 to show the production
					output power in dBm."
		::=  {trm25xxEntry  14}


	trm25xxLaserBiasCurLine	OBJECT-TYPE	
		SYNTAX			INTEGER
		MAX-ACCESS		read-only
		STATUS			current
		DESCRIPTION		"This parameter indicates the laser bias current for
					the line Subrack. If this parameter is 5450(545 mA)
					the laser bias current is more or equal to 545 mA.
					Divide this value by 10 to show the laser bias
					current in mA."
		::=  {trm25xxEntry  15}


	trm25xxProdLaserBiasCurLine	OBJECT-TYPE	
		SYNTAX			INTEGER
		MAX-ACCESS		read-only
		STATUS			current
		DESCRIPTION		"This parameter indicates the production laser bias
					current for the line Subrack. 
					Divide this value by 10 to show the laser bias
					current in mA."
		::=  {trm25xxEntry  16}


	trm25xxCDR	OBJECT-TYPE	
		SYNTAX			CDRMode
		MAX-ACCESS		read-write
		STATUS			current
		DESCRIPTION		"This parameter indicates the working mode of the CDR 
					clock recovery circuit.
					Can be auto3R(1), bypass2R(2), custom(3),fddiOR100Base-FX(4),
					stm1orOC-3(5), video270Mbps(6), stm4orOC-12(7), 
					gigabitEthernet(8), fibreChannel(9), fibreChannelx2(10),
					stm16orOC-48(11) or gigabitEthernetx2(12). custom(3) can not
					be set, it just indicates if a custom protocol is set with
					oid trm25xxCustomFibRate."
		::=  {trm25xxEntry  17}


	trm25xxCustomFibRate	OBJECT-TYPE	
		SYNTAX			INTEGER
		MAX-ACCESS		read-write
		STATUS			current
		DESCRIPTION		"This parameter is used to set the working mode of the
					CDR clock recovery circuit(Custom mode).
					Custom mode can be set between 30000 and 2700000 Kbit/s."
		::=  {trm25xxEntry  18}


	trm25xxTxMode	OBJECT-TYPE
		SYNTAX			TxMode76
		MAX-ACCESS		read-write
		STATUS			current
		DESCRIPTION		"This parameter indicates the mode of the optical
					transmitter for line and client TX. 
					Can be set to normal(1), disabled(2) or forcedon(3).
					normal  (1): Optical output when receiving signal.
					disabled(2): Optical output is OFF.
					forcedon(3): Optical output is ON.
					(Normally, forcedon(3) is used for test purpose only)"
		::=  {trm25xxEntry  19}




