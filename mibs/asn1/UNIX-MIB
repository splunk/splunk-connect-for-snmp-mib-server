		installedSwVersion
			OCTET STRING,
 		installedSwVendorPatches
			OCTET STRING,
		installedSwUserPatches
			OCTET STRING,
		installedSwLicInstalled
			OCTET STRING
	}


	installedSwIndex OBJECT-TYPE
		SYNTAX OCTET STRING
		ACCESS read-only
		STATUS mandatory
		DESCRIPTION
			"The index"
	::= { installedSwEntry 1 }

	installedSwName	OBJECT-TYPE
		SYNTAX OCTET STRING
		ACCESS read-only
		STATUS mandatory
		DESCRIPTION
			"The name of the installedSw"
	::= { installedSwEntry 2 }

	installedSwVendorAuthor	OBJECT-TYPE
		SYNTAX OCTET STRING
		ACCESS read-only
		STATUS mandatory
		DESCRIPTION
			"The vendor authorization of the installedSw"
	::= { installedSwEntry 3 }

	installedSwVersion	OBJECT-TYPE
		SYNTAX OCTET STRING
		ACCESS read-only
		STATUS mandatory
		DESCRIPTION
			"The version of the installedSw" 
	::= { installedSwEntry 4 }

	installedSwVendorPatches	OBJECT-TYPE
		SYNTAX OCTET STRING
		ACCESS read-only
		STATUS mandatory
		DESCRIPTION
			"The vendor patches list of the installedSw "
	::= { installedSwEntry 5 }

	installedSwUserPatches	OBJECT-TYPE
		SYNTAX OCTET STRING
		ACCESS read-only
		STATUS mandatory
		DESCRIPTION
			"The user patches of the installedSw"
	::= { installedSwEntry 6 }

	installedSwLicInstalled	OBJECT-TYPE
		SYNTAX OCTET STRING
		ACCESS read-only
		STATUS mandatory
		DESCRIPTION
			"The license of the installedSw"
	::= { installedSwEntry 7 }

--
-- The   license table
--
	
	licenseTable OBJECT-TYPE
		SYNTAX SEQUENCE OF LicenseEntry
		ACCESS not-accessible
		STATUS mandatory
		DESCRIPTION
			"This table describes the license"
	::= { host 14 }

	licenseEntry OBJECT-TYPE
		SYNTAX LicenseEntry
		ACCESS not-accessible
		STATUS mandatory
		DESCRIPTION
			"An entry in the license table"
		INDEX { licenseIndex }
	::= { licenseTable 1 }

	LicenseEntry ::= SEQUENCE {
		licenseIndex
			INTEGER,
		licensePoductName
			OCTET STRING,
		licenseVendorAuthor
			OCTET STRING,
		licenseExpirationDate
			OCTET STRING,
 		licenseVersion
			OCTET STRING,
		licenseCapacity
			OCTET STRING,
		licenseAuthorization
			OCTET STRING
	}

	licenseIndex	OBJECT-TYPE
		SYNTAX OCTET STRING
		ACCESS read-only
		STATUS mandatory
		DESCRIPTION
			"The index"
	::= { licenseEntry 1 }

	licenseProductName	OBJECT-TYPE
		SYNTAX OCTET STRING
		ACCESS read-only
		STATUS mandatory
		DESCRIPTION
			"The product name of the license"
	::= { licenseEntry 2 }

	licenseVendorAuthor	OBJECT-TYPE
		SYNTAX OCTET STRING
		ACCESS read-only
		STATUS mandatory
		DESCRIPTION
			"The vendor authorization of the license"
	::= { licenseEntry 3 }

	licenseExpirationDate	OBJECT-TYPE
		SYNTAX OCTET STRING
		ACCESS read-only
		STATUS mandatory
		DESCRIPTION
			"The Expiry date of the license" 
	::= { licenseEntry 4 }

	licenseVersion	OBJECT-TYPE
		SYNTAX OCTET STRING
		ACCESS read-only
		STATUS mandatory
		DESCRIPTION
			"The Version of the license "
	::= { licenseEntry 5 }

	licenseCapacity	OBJECT-TYPE
		SYNTAX OCTET STRING
		ACCESS read-only
		STATUS mandatory
		DESCRIPTION
			"The capacity of the license"
	::= { licenseEntry 6 }

	licenseAuthorization	OBJECT-TYPE
		SYNTAX OCTET STRING
		ACCESS read-only
		STATUS mandatory
		DESCRIPTION
			"The authorization of the license"
	::= { licenseEntry 7 }

--
-- The filesystem table
--
	
	filesystemTable	OBJECT-TYPE
		SYNTAX SEQUENCE OF FilesystemEntry
		ACCESS not-accessible
		STATUS mandatory
		DESCRIPTION
			"This table describes the filesystem"
	::= { host 15 }

	filesystemEntry OBJECT-TYPE
		SYNTAX FilesystemEntry
		ACCESS not-accessible
		STATUS mandatory
		DESCRIPTION
			"An entry in the filesystem table"
		INDEX { filesystemIndex }
	::= { filesystemTable 1 }

	FilesystemEntry ::= SEQUENCE {
		filesystemIndex
			INTEGER,
		filesystemOsType
			OCTET STRING,
		filesystemName
			OCTET STRING,
		filesystemHost
			OCTET STRING,
 		filesystemFreeCapacity
			INTEGER,
		filesystemFormattedCap
			INTEGER
	}

	filesystemIndex OBJECT-TYPE
		SYNTAX INTEGER
		ACCESS read-only
		STATUS mandatory
		DESCRIPTION
			"The Os type of the filesystem"
	::= { filesystemEntry 1 }

	filesystemName	OBJECT-TYPE
		SYNTAX OCTET STRING
		ACCESS read-only
		STATUS mandatory
		DESCRIPTION
			"The name of the filesystem"
	::= { filesystemEntry 2 }

	filesystemFreeCapacity	OBJECT-TYPE
		SYNTAX OCTET STRING
		ACCESS read-only
		STATUS mandatory
		DESCRIPTION
			"The free capatity of the filesystem "
	::= { filesystemEntry 3 }

	filesystemFormattedCap	OBJECT-TYPE
		SYNTAX INTEGER
		ACCESS read-only
		STATUS mandatory
		DESCRIPTION
			"The formatted capacity of the filesystem"
	::= { filesystemEntry 4 }


--
-- The mountInfo table
--
	
	mountInfoTable	OBJECT-TYPE
		SYNTAX SEQUENCE OF MountInfoEntry
		ACCESS not-accessible
		STATUS mandatory
		DESCRIPTION
			"This table describes the mountInfo"
	::= { host 16 }

	mountInfoEntry OBJECT-TYPE
		SYNTAX MountInfoEntry
		ACCESS not-accessible
		STATUS mandatory
		DESCRIPTION
			"An entry in the mountInfo table"
		INDEX { mountInfoIndex }
	::= { mountInfoTable 1 }

	MountInfoEntry ::= SEQUENCE {
		mountInfoIndex
			INTEGER,
		mountInfoFilesystemName
			OCTET STRING,
		mountInfoDeviceName
			OCTET STRING,
		mountInfoMountPoint
			OCTET STRING,
 		mountInfoMountType
			OCTET STRING,
		mountInfoRelVolNum
			INTEGER
	}

	mountInfoIndex	OBJECT-TYPE
		SYNTAX INTEGER
		ACCESS read-only
		STATUS mandatory
		DESCRIPTION
			"The file system name of the mountInfo"
	::= { mountInfoEntry 1 }

	mountInfoFileSystemName	OBJECT-TYPE
		SYNTAX OCTET STRING
		ACCESS read-only
		STATUS mandatory
		DESCRIPTION
			"The file system name of the mountInfo"
	::= { mountInfoEntry 2 }

	mountInfoDeviceName	OBJECT-TYPE
		SYNTAX OCTET STRING
		ACCESS read-only
		STATUS mandatory
		DESCRIPTION
			"The Device name of the mountInfo"
	::= { mountInfoEntry 3 }

	mountInfoMountPoint	OBJECT-TYPE
		SYNTAX OCTET STRING
		ACCESS read-only
		STATUS mandatory
		DESCRIPTION
			"The mount point of the mountInfo" 
	::= { mountInfoEntry 4 }

	mountInfoMountType	OBJECT-TYPE
		SYNTAX OCTET STRING
		ACCESS read-only
		STATUS mandatory
		DESCRIPTION
			"The Mount type list of the mountInfo "
	::= { mountInfoEntry 4 }

	mountInfoRelVolNum	OBJECT-TYPE
		SYNTAX INTEGER
		ACCESS read-only
		STATUS mandatory
		DESCRIPTION
			"The rel volume number of the mountInfo"
	::= { mountInfoEntry 6 }
END
