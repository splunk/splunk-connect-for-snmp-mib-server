			"An entry in the installedSw table"
		INDEX { installedSwIndex }
	::= { installedSwTable 1 }

	InstalledSwEntry ::= SEQUENCE {
		installedSwIndex
			INTEGER,
		installedSwName
			OCTET STRING,
		installedSwVendorAuthor
