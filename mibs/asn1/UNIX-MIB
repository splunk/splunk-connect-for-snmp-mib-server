	installedSwTable OBJECT-TYPE
		SYNTAX SEQUENCE OF InstalledSwEntry
		ACCESS not-accessible
		STATUS mandatory
		DESCRIPTION
			"This table describes the installedSw"
	::= { host 13 }

	installedSwEntry OBJECT-TYPE
		SYNTAX InstalledSwEntry
		ACCESS not-accessible
		STATUS mandatory
