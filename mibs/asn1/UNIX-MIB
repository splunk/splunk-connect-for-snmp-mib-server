	diskPartitionSize OBJECT-TYPE
		SYNTAX INTEGER
		ACCESS read-only
		STATUS mandatory
		DESCRIPTION
			"The size of the disk partition"
	::= { diskPartitionEntry 4 }

	diskPartitionDeviceName OBJECT-TYPE
