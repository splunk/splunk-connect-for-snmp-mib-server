	ENTERPRISE  cxr
    	VARIABLES   { shelfIndex, slotIndex , portIndex }
   	DESCRIPTION "User changed config."
    	::= 15
 resetbyATZ TRAP-TYPE
	ENTERPRISE  cxr
    	VARIABLES   { shelfIndex, slotIndex , portIndex }
   	DESCRIPTION "Reset by ATZ"
    	::= 16
 llActive TRAP-TYPE
	ENTERPRISE  cxr
    	VARIABLES   { shelfIndex, slotIndex , portIndex }
   	DESCRIPTION "LL active"
    	::= 17
