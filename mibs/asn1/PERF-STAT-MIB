 --
 -- sitaraPerfTraf - statistics for traffic monitors 
 --

 sitaraPerfTrafTable OBJECT-TYPE
     SYNTAX      SEQUENCE OF SitaraPerfTrafEntry
     MAX-ACCESS  not-accessible
     STATUS      current
     DESCRIPTION
             "The sitaraPerfTrafTable contains statistics for each traffic monitor instance."
     ::= { sitaraPerfTraf 1 }

 sitaraPerfTrafEntry OBJECT-TYPE
     SYNTAX      SitaraPerfTrafEntry
     MAX-ACCESS  not-accessible
     STATUS      current
     DESCRIPTION
             "Statistics about a particular traffic monitor instance for the most
	      recently completed interval."
     INDEX { sitaraCfgSysMonIndex }
     ::= { sitaraPerfTrafTable 1 }

 SitaraPerfTrafEntry ::=
     SEQUENCE {
         sitaraPerfTrafIndex                Unsigned32,
         sitaraPerfTrafName                 DisplayString,
         sitaraPerfTrafTime                 TimeTicks,
         sitaraPerfTrafInterval             INTEGER,
         sitaraPerfTrafPktCnt               Gauge32,
         sitaraPerfTrafByteCnt              Gauge32
     }

 sitaraPerfTrafIndex OBJECT-TYPE
     SYNTAX      Unsigned32
     MAX-ACCESS  not-accessible
     STATUS      current
     DESCRIPTION
             "A unique value for each sitaraPerfTraf instance. This table is indexed
              by sitaraCfgSysMonIndex."
     ::= { sitaraPerfTrafEntry 1 }

 sitaraPerfTrafName OBJECT-TYPE
     SYNTAX      DisplayString
     MAX-ACCESS  read-create
     STATUS      current
     DESCRIPTION
             "Indicates the name of the sitaraPerfTraf instance."
     ::= { sitaraPerfTrafEntry 2 }

 sitaraPerfTrafTime OBJECT-TYPE
     SYNTAX      TimeStamp
     MAX-ACCESS  read-only
     STATUS      current
     DESCRIPTION
             "Value of sysUptime when traffic monitoring for this instance began."
     ::= { sitaraPerfTrafEntry 3 }

 sitaraPerfTrafInterval OBJECT-TYPE
     SYNTAX      INTEGER(1..3600)
     MAX-ACCESS  read-only
     STATUS      current
     DESCRIPTION
             "Polling interval, in seconds, for this monitoring instance."
     DEFVAL   { 60 }
     ::= { sitaraPerfTrafEntry 4 }

 sitaraPerfTrafPktCnt OBJECT-TYPE
     SYNTAX      Gauge32 
     MAX-ACCESS  read-only
     STATUS      current
     DESCRIPTION
             "The number of packets sent during this interval.  
	      This is the delta value of packets sent since the last sampling interval."
     ::= { sitaraPerfTrafEntry 5 }

 sitaraPerfTrafByteCnt OBJECT-TYPE
     SYNTAX      Gauge32 
     MAX-ACCESS  read-only
     STATUS      current
     DESCRIPTION
             "The number of bytes sent during this interval.  
	      This is the delta value of bytes sent since the last sampling interval."
     ::= { sitaraPerfTrafEntry 6 }

 --
 -- sitaraPerfPlcy - statistics for policies
 --
 -- sitaraPerfPlcyTable - each row defined in this table describes the type
 -- of monitoring to be done for a given link, group, and class .
 --

 sitaraPerfPlcyNextIndex   OBJECT-TYPE
       SYNTAX  Unsigned32
       MAX-ACCESS  read-only
       STATUS  current
       DESCRIPTION
       "Next available index in the sitaraPerfPlcyTable."
       ::= { sitaraPerfPlcy 1 }

 sitaraPerfPlcyTable OBJECT-TYPE
     SYNTAX      SEQUENCE OF SitaraPerfPlcyEntry
     MAX-ACCESS  not-accessible
     STATUS      current
     DESCRIPTION
             "The sitaraPerfPlcyTable contains an entry for each policy instance."
     ::= { sitaraPerfPlcy 2 }

 sitaraPerfPlcyEntry OBJECT-TYPE
     SYNTAX      SitaraPerfPlcyEntry
     MAX-ACCESS  not-accessible
     STATUS      current
     DESCRIPTION
             "Statistics about a particular policy instance for the most
 	      recently completed interval."
     INDEX { sitaraPerfPlcyIndex }
     ::= { sitaraPerfPlcyTable 1 }

 SitaraPerfPlcyEntry ::=
     SEQUENCE {
         sitaraPerfPlcyIndex                Unsigned32,
         sitaraPerfPlcyTime                 TimeTicks,
         sitaraPerfPlcyInterval             INTEGER,
         sitaraPerfPlcyAlias                PolicyName,
         sitaraPerfPlcyType                 PolicyType,
         sitaraPerfPlcyLanBandWidth         Gauge32,
         sitaraPerfPlcyLanBurst             Gauge32,
         sitaraPerfPlcyWanBandWidth         Gauge32,
         sitaraPerfPlcyWanBurst             Gauge32,
         sitaraPerfPlcyPktCnt               Counter32,
         sitaraPerfPlcyPktCntDrop           Counter32,
         sitaraPerfPlcyPktCntBurst          Counter32,
         sitaraPerfPlcyByteCnt              Counter32,
         sitaraPerfPlcyByteCntDrop          Counter32,
         sitaraPerfPlcyQueuePkts            Counter32,
         sitaraPerfPlcyHandle               Unsigned32,
         sitaraPerfPlcyCbqName              CbqName,
         sitaraPerfPlcyAvgIdleTime          INTEGER,
	 sitaraPerfPlcyCustomerId	    PolicyCustomerId,
	 sitaraPerfPlcyOwner                SitaraOwnerString,
	 sitaraPerfPlcyPathName             PolicyPathName,
	 sitaraPerfPlcyDirection            PolicyDirection,
	 sitaraPerfPlcyStatus		    INTEGER,
	 sitaraPerfPlcyTimeStamp	    TimeTicks,
         sitaraPerfPlcyBandWidth  	    Gauge32,
         sitaraPerfPlcyBurst  	    	    Gauge32,
         sitaraPerfPlcyByteCntBurst         Counter64,
         sitaraPerfPlcyAvgQueSize           Counter32
     }


