 sitaraPerfPlcyOwner OBJECT-TYPE
        SYNTAX      SitaraOwnerString 
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
		"The owner name of this class."
		::= { sitaraPerfPlcyEntry 20 }

 sitaraPerfPlcyPathName OBJECT-TYPE
       SYNTAX      PolicyPathName
       MAX-ACCESS  read-only
       STATUS      current
       DESCRIPTION
       "Path Name gives the full specification of this link/Group/Class
       information.  See the PolicyPathName textual convention."
     		::= { sitaraPerfPlcyEntry 21 }

 sitaraPerfPlcyDirection  OBJECT-TYPE
       SYNTAX      PolicyDirection
       MAX-ACCESS  read-only
       STATUS      current
       DESCRIPTION
       "The policy group traffic direction object indicates the
       directionality of this flow. Based on the value of this object,
       the LAN and WAN bandwidth paramerts will be filled in. If this
       is a bi-directional flow, values for both the LAN and WAN
       objects should be supplied."
     		::= { sitaraPerfPlcyEntry 22 }

 sitaraPerfPlcyStatus OBJECT-TYPE
     SYNTAX      INTEGER 
		{
		active (1),
		inActive (2)
		}
     MAX-ACCESS  read-only
     STATUS      current
     DESCRIPTION
             " This specifies whether this Policy is active in the Current Schedule"
     ::= { sitaraPerfPlcyEntry 23 }
 
 sitaraPerfPlcyTimeStamp	   OBJECT-TYPE
     SYNTAX      TimeTicks
     MAX-ACCESS  read-only
     STATUS      current
     DESCRIPTION
             "TimeStamp is the SysupTime in number of ticks 
              at the time of reporting."
     ::= { sitaraPerfPlcyEntry 24 }

 sitaraPerfPlcyBandWidth OBJECT-TYPE
        SYNTAX      Gauge32
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
        "Link BandWidth Utilization in the Wan direction.  It is currently unused."
        ::= { sitaraPerfPlcyEntry 25 }

 sitaraPerfPlcyBurst OBJECT-TYPE
        SYNTAX      Gauge32
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
        "Link BandWidth Utilization in the Wan direction.  It is currently unused."
        ::= { sitaraPerfPlcyEntry 26 }

 sitaraPerfPlcyByteCntBurst OBJECT-TYPE
     SYNTAX      Counter64 
     MAX-ACCESS  read-only
     STATUS      current
     DESCRIPTION
             "The number of bytes sent as burst during this period for this class."
     ::= { sitaraPerfPlcyEntry 27 }

 sitaraPerfPlcyAvgQueSize     OBJECT-TYPE
     SYNTAX      Counter32
     MAX-ACCESS  read-only
     STATUS      current
     DESCRIPTION
             "The average of the number of packets queued so far for this class."
     ::= { sitaraPerfPlcyEntry 28 }






 --
 -- sitaraPerfCach - statistics for the web cache (SQUID)
 --
 -- The MIB objects for cache statistics are contained within the SQUID Mib
 -- are not reiterated in this MIB module.
 --

 --
 -- sitaraPerfNotifns - for Sitara Performance Monitoring MIB Module
 --
 --

 sitaraPerfNotifnsMemoryUtil NOTIFICATION-TYPE
    OBJECTS     {
		 alarmIndex,
		 alarmInterval,
		 alarmVariable,
		 alarmValue,
		 alarmRisingThreshold,
		 alarmFallingThreshold,
                 sitaraPerfSysMemoryUsage
                }
    STATUS      current
    DESCRIPTION
        "sitaraPerfNotifnsMemoryUtil notification is generated when the
         system memory utilization exceeds the configured threshold for
         system memory utilization as a percentage.
         sitaraPerfNotifnsMemoryUtil notification is maskable by
         setting the corresponding bit in the sitaraPerfAlarmMasks."
    ::= {  sitaraPerfNotifns  1 }

 sitaraPerfNotifnsCPU NOTIFICATION-TYPE
    OBJECTS     {
		 alarmIndex,
		 alarmInterval,
		 alarmVariable,
		 alarmValue,
		 alarmRisingThreshold,
		 alarmFallingThreshold,
                 sitaraPerfSysCPUUsage
                }
    STATUS      current
    DESCRIPTION
        "sitaraPerfNotifnsCPU notification is generated when the system
         CPU utilization exceeds the configured threshold for CPU
         utilization as a percentage. sitaraPerfNotifnsCPU
         notification is maskable by setting the corresponding bit
         in the sitaraPerfAlarmMasks."
    ::= {  sitaraPerfNotifns  2 }

