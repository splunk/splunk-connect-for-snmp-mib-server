 PERF-STAT-MIB DEFINITIONS ::= BEGIN

 IMPORTS
     MODULE-IDENTITY, OBJECT-TYPE, Unsigned32, Integer32, Gauge32, Counter32, Counter64,
     NOTIFICATION-TYPE, enterprises, IpAddress, TimeTicks
         FROM SNMPv2-SMI
     TEXTUAL-CONVENTION, DisplayString, TruthValue, RowStatus, 
     DateAndTime, TimeStamp
         FROM SNMPv2-TC
     alarmInterval, alarmValue, alarmIndex, alarmRisingThreshold, 
     alarmVariable, alarmFallingThreshold
	 FROM RMON-MIB
     MODULE-COMPLIANCE, OBJECT-GROUP
         FROM SNMPv2-CONF;

    sitara                      OBJECT IDENTIFIER ::= { enterprises 2302 }

    sitaraCfg                   OBJECT IDENTIFIER ::= { sitara 1 }

    sitaraCfgPlcy               OBJECT IDENTIFIER ::= { sitaraCfg 1 }

    sitaraCfgSysGenlObjs        OBJECT IDENTIFIER ::= { sitaraCfgSys 2 }

    sitaraCfgSys                OBJECT IDENTIFIER ::= { sitaraCfg 2 }

    sitaraCfgSysMon             OBJECT IDENTIFIER ::= { sitaraCfgSys 6 }

    sitaraPerf                   OBJECT IDENTIFIER ::= { sitara 2 }

    sitaraPerfSys                OBJECT IDENTIFIER ::= { sitaraPerf 2 }

    sitaraPerfTraf               OBJECT IDENTIFIER ::= { sitaraPerf 3 }

    sitaraPerfPlcy               OBJECT IDENTIFIER ::= { sitaraPerf 4 }

    sitaraPerfCach               OBJECT IDENTIFIER ::= { sitaraPerf 5 }

    sitaraPerfNotifns            OBJECT IDENTIFIER ::= { sitaraPerf 6 }


 sitaraPerfMonMIB MODULE-IDENTITY
     LAST-UPDATED "200106151400Z"
     ORGANIZATION "Sitara, Incorporated"
     CONTACT-INFO
             "     Postal Address:
                   Sitara Networks
                   52 Second Avenue
                   Suite 200
                   Waltham MA. 02451
                   USA

                   Tel:     +1 781 487 5900
                   E-mail:  info@sitaranetworks.com
                   WWW:     http://www.sitaranetworks.com"
     DESCRIPTION
             "This MIB module provides Sitara QOSWorks performance monitoring capabilities."

     REVISION  "200106151400Z"
     DESCRIPTION
             "This Version of MIB consists of the Objects supported in
              QosDirector 1.5/QosWorks 1.10/QosArray 1.3.  
              Status of the Objects/Notification, supported only in the  
              QosWorks 1.7/1.8/1.9, are not supported any more is changed to obsolete"
             

     ::= { sitaraPerfSys 1 }

 --
 -- Textual Conventions
 --

 FilterType ::= TEXTUAL-CONVENTION
     STATUS      current
     DESCRIPTION
	  "This TC defines a traffic filter type.  It describes how TCP/IP
	   addresses and port numbers in a particular instance of a traffic
	   monitor are used to filter traffic."
     SYNTAX      INTEGER {
                   none                (1), -- No address or port specified
                   sourceAddr          (2), -- Filter source address
                   sourcePort          (3), -- Filter source port number
                   destinationAddr     (4), -- Filter destination address
                   destinationPort     (5), -- Filter destination port number
                   bothPorts           (6)  -- Filter both port numbers
		 }

 FilterDirection ::= TEXTUAL-CONVENTION
     STATUS      current
     DESCRIPTION
             "This TC defines the directions of traffic flows."
     SYNTAX      INTEGER {
                   inbound             (1), -- WAN to LAN
                   outbound            (2)  -- LAN to WAN
		 }

 PolicyName ::= TEXTUAL-CONVENTION
     STATUS       current
     DESCRIPTION  " [A-Za-z0-9_ + && not 'default'  (UTF-8 ever?))]
                   'default' A policy pathname."
     SYNTAX       OCTET STRING (SIZE (1..64))  

 CbqName ::= TEXTUAL-CONVENTION
     STATUS       current
     DESCRIPTION  "[Cc]_[0-9]{1,8}
		   This Tc represents an internal name used by CBQ to 
		   identify a policy being monitored."  
     SYNTAX       OCTET STRING (SIZE (3..10))  

 PolicyType ::= TEXTUAL-CONVENTION
     STATUS       current
     DESCRIPTION
             "Describes the type of a policy for which statistics are 
              being collected."
     SYNTAX	 INTEGER {
                      class(0),            -- class statistics
                      group(1),             -- group statistics
                      link(2)             -- link statistics
		 }
PolicyPathName ::= TEXTUAL-CONVENTION
    STATUS       current
    DESCRIPTION  "[A-Za-z0-9 _ / ] 'default' Name  is reserved for
                 internal use. Path is the full expression of the
                 part of the policy when an object with this textual
                 convention is used. A path must be unique, within a
                 schedule and is to be read and understood very much
                 like a directory structure such as /usr/var where var
                 is contained in usr. The format for path is
                 l_LinkAlias/g_GroupAlias/c_ClassAlias/f_FilterAlias.
                 At any point in this hierarchy, there is one and only
                 one unique path within a schedule."
    SYNTAX       OCTET STRING (SIZE (1..512))

PolicyCustomerId ::= TEXTUAL-CONVENTION
    STATUS       current
    DESCRIPTION  "[A-Za-z0-9 _  not 'default' or any reserved Policy name
                  like LINK, END , VERSION etc]. The 'default' alias  is
                  reserved for internal use. For example, a Customer ID can be
                  combination of Alpha numeric value GoldUser4527"
    SYNTAX       OCTET STRING (SIZE (1..30))
 
 PolicyDirection ::= TEXTUAL-CONVENTION
     STATUS      current
     DESCRIPTION
             "This TC defines the directions of traffic flows."
     SYNTAX      INTEGER {
                   inbound             (1), -- to LAN
                   outbound            (2)  -- to WAN
		 }

 SitaraOwnerString ::= TEXTUAL-CONVENTION
     STATUS current
     DESCRIPTION
         "This data type is used to model an administratively
         assigned name of the owner of a resource. Implementations
         must accept values composed of well-formed NVT ASCII
         sequences. In addition, implementations should accept
         values composed of well-formed UTF-8 sequences.

         It is suggested that this name contain one or more of
         the following: IP address, management station name,
         network manager's name, location, or phone number.
         In some cases the agent itself will be the owner of
         an entry.  In these cases, this string shall be set
         to a string starting with 'monitor'.

         SNMP access control is articulated entirely in terms
         of the contents of MIB views; access to a particular
         SNMP object instance depends only upon its presence
         or absence in a particular MIB view and never upon
         its value or the value of related object instances.
         Thus, objects of this type afford resolution of
         resource contention only among cooperating
         managers; they realize no access control function
         with respect to uncooperative parties."
     SYNTAX OCTET STRING (SIZE (0..127))

 -- End of TCs


