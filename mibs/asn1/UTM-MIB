        ::= {utmOsiLpapEntry 11}
 
 
 
 
 
-----------------------------------------------
-- Data objects for trap control
-----------------------------------------------
 
 
utmTrapControl                OBJECT IDENTIFIER ::= { sniUTM 4 }
 
 
 
utmTrapCtrlCheckCaseNOW   OBJECT-TYPE
        SYNTAX  INTEGER
        ACCESS  read-write
        STATUS  mandatory
        DESCRIPTION "Any set request for this object will start
                     an immediate trap case detection for all traps
                     (independent from any timer)"
        ::= {utmTrapControl 1}
 
utmTrapCtrlAbortedPeriod   OBJECT-TYPE
        SYNTAX  INTEGER
        ACCESS  read-write
        STATUS  mandatory
        DESCRIPTION "Break time in seconds between two trap checks;
                     '-1' means no trap case detection"
        ::= {utmTrapControl 2}
 
utmTrapCtrlPENDERPeriod   OBJECT-TYPE
        SYNTAX  INTEGER
        ACCESS  read-write
        STATUS  mandatory
        DESCRIPTION "Break time in seconds between two trap checks;
                     '-1' means no trap case detection"
        ::= {utmTrapControl 3}
 
utmTrapCtrlDeadlockPeriod   OBJECT-TYPE
        SYNTAX  INTEGER
        ACCESS  read-write
        STATUS  mandatory
        DESCRIPTION "Break time in seconds between two trap checks;
                     '-1' means no trap case detection"
        ::= {utmTrapControl 4}
 
utmTrapCtrlSecurViolPeriod OBJECT-TYPE
        SYNTAX  INTEGER
        ACCESS  read-write
        STATUS  mandatory
        DESCRIPTION "Break time in seconds between two trap checks;
                     '-1' means no trap case detection"
        ::= {utmTrapControl 5}
 
utmTrapCtrlSecurViolLimit OBJECT-TYPE
        SYNTAX  INTEGER
        ACCESS  read-write
        STATUS  mandatory
        DESCRIPTION "Necessary number of user security violations (since 
                     application start or since last security trap) 
                     before sending a new trap"
        ::= {utmTrapControl 6}
 
utmTrapCtrlConnLossPeriod OBJECT-TYPE
        SYNTAX  INTEGER
        ACCESS  read-write
        STATUS  mandatory
        DESCRIPTION "Break time in seconds between two trap checks;
                     '-1' means no trap case detection"
        ::= {utmTrapControl 7}
 
utmTrapCtrlConnLossLimit OBJECT-TYPE
        SYNTAX  INTEGER
        ACCESS  read-write
        STATUS  mandatory
        DESCRIPTION "Necessary number of unconnected partner 
                     applications before sending a trap; 
                     -1 means maximum (no connected partners)"
        ::= {utmTrapControl 8}
 
utmTrapCtrlConnUsersPeriod   OBJECT-TYPE
        SYNTAX  INTEGER
        ACCESS  read-write
        STATUS  mandatory
        DESCRIPTION "Break time in seconds between two trap checks;
                     '-1' means no trap case detection"
        ::= {utmTrapControl 9}
 
utmTrapCtrlConnUsersLimit   OBJECT-TYPE
        SYNTAX  INTEGER
        ACCESS  read-write
        STATUS  mandatory
        DESCRIPTION "Necessary number of connected users before
                     sending a trap; -1 means the generated maximum 
                     of allowed user connections"
        ::= {utmTrapControl 10}
 
utmTrapCtrlActiveTasksPeriod   OBJECT-TYPE
        SYNTAX  INTEGER
        ACCESS  read-write
        STATUS  mandatory
        DESCRIPTION "Break time in seconds between two trap checks;
                     '-1' means no trap case detection"
        ::= {utmTrapControl 11}
 
utmTrapCtrlActiveTasksLimit   OBJECT-TYPE
        SYNTAX  INTEGER
        ACCESS  read-write
        STATUS  mandatory
        DESCRIPTION "Necessary number of active tasks before 
                     sending a trap; -1 means the actual maximum
                     of allowed tasks"
        ::= {utmTrapControl 12}
 
utmTrapCtrlCacheHitPeriod   OBJECT-TYPE
        SYNTAX  INTEGER
        ACCESS  read-write
        STATUS  mandatory
        DESCRIPTION "Break time in seconds between two trap checks;
                     '-1' means no trap case detection"
        ::= {utmTrapControl 13}
 
utmTrapCtrlCacheHitLimit   OBJECT-TYPE
        SYNTAX  INTEGER
        ACCESS  read-write
        STATUS  mandatory
        DESCRIPTION "Minimum rate (in percent) of cache hits before
                     sending a trap"
        ::= {utmTrapControl 14}
 
utmTrapCtrlPoolSizePeriod   OBJECT-TYPE
        SYNTAX  INTEGER
        ACCESS  read-write
        STATUS  mandatory
        DESCRIPTION "Break time in seconds between two trap checks;
                     '-1' means no trap case detection"
        ::= {utmTrapControl 15}
 
utmTrapCtrlPoolSizeLimit   OBJECT-TYPE
        SYNTAX  INTEGER
        ACCESS  read-write
        STATUS  mandatory
        DESCRIPTION "Maximum pool size (in percent) before sending
                     a trap"
        ::= {utmTrapControl 16}
 
utmTrapCtrlTclWaitMsgsPeriod   OBJECT-TYPE
        SYNTAX  INTEGER
        ACCESS  read-write
        STATUS  mandatory
        DESCRIPTION "Break time in seconds between two trap checks;
                     '-1' means no trap case detection"
        ::= {utmTrapControl 17}
 
utmTrapCtrlTclWaitMsgsLimit   OBJECT-TYPE
        SYNTAX  INTEGER
        ACCESS  read-write
        STATUS  mandatory
        DESCRIPTION "Necessary number of waiting messages for a 
                     TAC class before sending a trap"
        ::= {utmTrapControl 18}
 
utmTrapCtrlLtermWaitOutMsgsPeriod   OBJECT-TYPE
        SYNTAX  INTEGER
        ACCESS  read-write
        STATUS  mandatory
        DESCRIPTION "Break time in seconds between two trap checks;
                     '-1' means no trap case detection"
        ::= {utmTrapControl 19}
 
utmTrapCtrlLtermWaitOutMsgsLimit   OBJECT-TYPE
        SYNTAX  INTEGER
        ACCESS  read-write
        STATUS  mandatory
        DESCRIPTION "Necessary number of messages waiting to be output 
                     on a logical terminal before sending a trap"
        ::= {utmTrapControl 20}
 
 
 
 
 
 
 
 
 
 
 
 
-- UTM  Traps
--
 
utmApplAborted   TRAP-TYPE
              ENTERPRISE utmApplicationData
              DESCRIPTION "Application terminated"
      ::= 1
 
utmApplPENDER   TRAP-TYPE
              ENTERPRISE utmApplicationData
              DESCRIPTION "Termination with PEND ER"
      ::= 2
 
utmApplDeadlock   TRAP-TYPE
              ENTERPRISE utmApplicationData
              DESCRIPTION "Detected and resolved deadlock"
      ::= 3
 
utmApplUserSecurityViolations   TRAP-TYPE
              ENTERPRISE utmApplicationData
              DESCRIPTION "User security violations"
      ::= 4
 
utmApplConnectionLosses   TRAP-TYPE
              ENTERPRISE utmApplicationData
              DESCRIPTION "Connection losses to partner
                           applications"
      ::= 5
 
utmApplConnectedUsers   TRAP-TYPE
              ENTERPRISE utmApplicationData
              DESCRIPTION "Connected users"
      ::= 6
 
utmApplActiveTasks   TRAP-TYPE
              ENTERPRISE utmApplicationData
              DESCRIPTION "Active tasks"
      ::= 7
 
utmApplCacheHits   TRAP-TYPE
              ENTERPRISE utmApplicationData
              DESCRIPTION "Cache hit rate"
      ::= 8
 
utmApplPoolSize   TRAP-TYPE
              ENTERPRISE utmApplicationData
              DESCRIPTION "Page pool size"
      ::= 9
 
utmApplTacclassWaitMsgs   TRAP-TYPE
              ENTERPRISE utmApplicationData
              DESCRIPTION "Unprocessed messages for TAC classes"
      ::= 10
 
utmApplLtermWaitOutpMsgs   TRAP-TYPE
              ENTERPRISE utmApplicationData
              DESCRIPTION "Messages waiting to be output on
                           logical terminals"
      ::= 11
 
 
END
 
