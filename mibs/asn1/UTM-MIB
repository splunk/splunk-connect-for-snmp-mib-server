utmTacTabNum    OBJECT-TYPE
        SYNTAX  INTEGER
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION "The number of entries in the table utmTacTable"
        ::= {utmTacData  1}
 
utmTacTable   OBJECT-TYPE
        SYNTAX  SEQUENCE OF UtmTacEntry
        ACCESS  not-accessible
        STATUS  mandatory
        DESCRIPTION "Table including infos about all transaction codes"
        ::= {utmTacData  2}
 
utmTacEntry   OBJECT-TYPE
        SYNTAX  UtmTacEntry
        ACCESS  not-accessible
        STATUS  mandatory
        DESCRIPTION "An entry in the table"
        INDEX {utmTacIndex}
        ::= {utmTacTable 1}
 
UtmTacEntry ::=
        SEQUENCE
        {
          utmTacIndex               INTEGER,
          utmTacName                DisplayString,
          utmTacLock                INTEGER,
          utmTacStatus              INTEGER,
          utmTacTcl                 INTEGER,
          utmTacInq                 INTEGER,
          utmTacUsed                INTEGER,
          utmTacError               INTEGER,
          utmTacDbcnt               INTEGER,
          utmTacElap                INTEGER,
          utmTacDbElap              INTEGER,
          utmTacCpu                 INTEGER
        }
 
utmTacIndex   OBJECT-TYPE
        SYNTAX  INTEGER
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION "A unique value for each entry, its value
                     ranges between 1 and the value of utmTacTabNum"
        ::= {utmTacEntry 1}
 
utmTacName   OBJECT-TYPE
        SYNTAX  DisplayString
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION "Name of the chosen transaction code"
        ::= {utmTacEntry  2}
 
utmTacLock   OBJECT-TYPE
        SYNTAX  INTEGER
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION "Lock of the transaction code (0 through 255)"
        ::= {utmTacEntry  3}
 
utmTacStatus   OBJECT-TYPE
        SYNTAX  INTEGER {
			  on   (1),
			  off  (2),
			  halt (3)
                        }
        ACCESS  read-write
        STATUS  mandatory
	DESCRIPTION "The status of the transaction code is either 'on', 'off'
		     or locked ('halt')"
        ::= {utmTacEntry  4}
 
utmTacTcl   OBJECT-TYPE
        SYNTAX  INTEGER
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION "TAC class of this transaction code"
        ::= {utmTacEntry  5}
 
utmTacInq   OBJECT-TYPE
        SYNTAX  INTEGER
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION "Number of messages still to be processed by the program
                     unit run designated by transaction code"
        ::= {utmTacEntry  6}
 
utmTacUsed   OBJECT-TYPE
        SYNTAX  INTEGER
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION "Number of program unit runs with this transaction code
                     processed since the initial start of the application
                     (only for asynchronous TAC's)"
        ::= {utmTacEntry  7}
 
utmTacError   OBJECT-TYPE
        SYNTAX  INTEGER
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION "Number of program unit runs with this transaction code
                     terminated with errors since the initial start of
                     the application"
        ::= {utmTacEntry  8}
 
utmTacDbcnt   OBJECT-TYPE
        SYNTAX  INTEGER
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION "Always '0' if XA-interface is used"
        ::= {utmTacEntry  9}
 
utmTacElap   OBJECT-TYPE
        SYNTAX  INTEGER
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION "Average runtime in milliseconds of program units
                     with this transaction code"
        ::= {utmTacEntry  10}
 
utmTacDbElap   OBJECT-TYPE
        SYNTAX  INTEGER
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION "Average time in milliseconds spent processing the
                     database calls in the program unit runs with this
                     transaction code"
        ::= {utmTacEntry  11}
 
utmTacCpu   OBJECT-TYPE
        SYNTAX  INTEGER
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION "Average CPU time in milliseconds spent in the UTM program run for
                     processing this transaction code"
        ::= {utmTacEntry  12}
 
 
