        DESCRIPTION "An entry in the table"
	INDEX {utmLtacIndex}
	::= {utmLtacTable 1}
 
UtmLtacEntry ::=
        SEQUENCE
        {
          utmLtacIndex              INTEGER,
          utmLtacName               DisplayString,
          utmLtacLock               INTEGER,
          utmLtacStatus             INTEGER,
          utmLtacRtac               DisplayString,
          utmLtacLpap               DisplayString,
          utmLtacAccessWait         INTEGER,
          utmLtacReplyWait          INTEGER,
          utmLtacUsed               INTEGER
        }
 
utmLtacIndex   OBJECT-TYPE
        SYNTAX  INTEGER
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION "A unique value for each entry, its value
                     ranges between 1 and the value of utmLtacTabNum"
        ::= {utmLtacEntry 1}
 
utmLtacName   OBJECT-TYPE
        SYNTAX  DisplayString
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION "Name of the LTAC"
        ::= {utmLtacEntry 2}
 
utmLtacLock   OBJECT-TYPE
        SYNTAX  INTEGER
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION "Lock of the remote converstation"
        ::= {utmLtacEntry 3}
 
utmLtacStatus   OBJECT-TYPE
        SYNTAX  INTEGER  {
                           on  (1),
                           off (2)
                         }
        ACCESS  read-write
        STATUS  mandatory
        DESCRIPTION "The LTAC transaction code is either locked (OFF) or 
                     unlocked (ON)"
        ::= {utmLtacEntry 4}
 
utmLtacRtac   OBJECT-TYPE
        SYNTAX  DisplayString
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION "Name of the transaction code in a remote application"
        ::= {utmLtacEntry 5}
 
utmLtacLpap   OBJECT-TYPE
        SYNTAX  DisplayString
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION "Name of the remote application in the local 
                     application"
        ::= {utmLtacEntry 6}
 
utmLtacAccessWait   OBJECT-TYPE
        SYNTAX  INTEGER
        ACCESS  read-write
        STATUS  mandatory
        DESCRIPTION "Time in seconds spent waiting for a session or 
                     association to be reserved"
        ::= {utmLtacEntry 7}
 
utmLtacReplyWait   OBJECT-TYPE
        SYNTAX  INTEGER
        ACCESS  read-write
        STATUS  mandatory
        DESCRIPTION "Time in seconds spent waiting for response from 
                     receiving partner"
        ::= {utmLtacEntry 8}
 
utmLtacUsed   OBJECT-TYPE
        SYNTAX  INTEGER
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION "Number of jobs issued to this LTAC since the start of 
                     the application"
        ::= {utmLtacEntry 9}
 
 
