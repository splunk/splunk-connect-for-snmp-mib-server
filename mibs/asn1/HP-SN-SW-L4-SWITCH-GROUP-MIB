                INTEGER,
    snL4WebCacheTotalConnections
                INTEGER,
        snL4WebCacheTxPkts
                Counter,
        snL4WebCacheRxPkts
                Counter,
        snL4WebCacheTxOctets
                Counter,
        snL4WebCacheRxOctets
                Counter,
    snL4WebCachePortState
                WebCacheState
        }

