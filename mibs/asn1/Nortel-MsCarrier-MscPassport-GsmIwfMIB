   mscGsmBcV42bisRowStatus
      RowStatus,
   mscGsmBcV42bisComponentName
      DisplayString,
   mscGsmBcV42bisStorageType
      StorageType,
   mscGsmBcV42bisIndex
      NonReplicated }

