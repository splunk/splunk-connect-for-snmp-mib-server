		sheSyncSourceAvailabilityInterfaceNumber OBJECT-TYPE
			SYNTAX Unsigned32 (1..72)
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"The number of the physical interface of the hardware unit.
				The interfaces are numbered vendor specific starting with 1 in ascending order.
				Synchronization interfaces may not be contained in the interface table."
			::= { sheSyncSourceAvailabilityEntry 4 }

		
		sheSyncSourceAvailabilityTypeSelectionPossible OBJECT-TYPE
			SYNTAX TruthValue
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"This status parameter shows, if the type of this clock source entry is fix or could be modified 
				by configuration. 
				
				This is a new parameter with R2.8 maintenance, where a new power module was introduced,
				which supports a configurable input selector to choose the SourceClockType out of this range:
				sheBitsSyncSource or 
				sheT3SyncSource (default) or 
				sheE1SyncSource.
				
				Only if clock sources with parameter TypeSelectionPossible == TRUE are available in the system,
				the new controlRequest commands for type switching could be used."
			DEFVAL { false }
			::= { sheSyncSourceAvailabilityEntry 5 }

		
		sheSyncConformance OBJECT IDENTIFIER ::= { sheSyncObjects 6 }

		
		sheSyncCompliances OBJECT IDENTIFIER ::= { sheSyncConformance 1 }

		
--  this module
		sheSyncCompliance MODULE-COMPLIANCE
			STATUS current
			DESCRIPTION 
				"Description."
			MODULE -- this module
				MANDATORY-GROUPS { sheSyncConfigGroup, sheSyncConfigControlGroup, sheSyncNotificationsGroup }
			::= { sheSyncCompliances 1 }

		
		sheSyncGroups OBJECT IDENTIFIER ::= { sheSyncConformance 2 }

		
		sheSyncConfigGroup OBJECT-GROUP
			OBJECTS { sheSyncActiveClockSource, sheSyncWaitTimeToRestore, sheSyncSourcePriority, sheSyncSourceAlarmState, sheSyncSourceAlarmSeverityIndex, 
				sheSyncMode, sheSyncSourceSSMLevel, sheSyncSourceAvailabilityIndex, sheSyncSourceAvailabilityType, sheSyncSourceAvailabilityHwUnitIndex, 
				sheSyncLocalClockSSMLevel, sheSyncSourceAvailabilityTypeSelectionPossible, sheSyncSourceAvailabilityInterfaceNumber, sheSyncSourceSelectedAvailabilityIndex, sheSyncSourceSSMLevelRec
				 }
			STATUS current
			DESCRIPTION 
				"Description."
			::= { sheSyncGroups 1 }

		
		sheSyncConfigControlGroup OBJECT-GROUP
			OBJECTS { sheSyncCgControlReq, sheSyncCgControlStatus, sheSyncCgControlReqResult, sheSyncCgControlTimeStamp, sheSyncCgSourcePriority, 
				sheSyncCgSourceAlarmSeverityIndex, sheSyncCgSourceWaitTimeToRestore, sheSyncCgMode, sheSyncCgSourceSSMLevel, sheSyncCgSourceAvailabilityIndex
				 }
			STATUS current
			DESCRIPTION 
				"Description."
			::= { sheSyncGroups 2 }

		
		sheSyncNotificationsGroup NOTIFICATION-GROUP
			NOTIFICATIONS { sheSyncControlResult, sheSyncClockSourceModified, sheSyncWaitTimeToRestoreModified, sheSyncActiveClockSourceChanged, sheSyncClockSourceAlarm, 
				sheSyncUsedSyncSourcesModified, sheSyncGlobalsModified, sheSyncAvailabilityTableUpdated, sheSyncClockNoSSMInfoAlarm, sheSyncAvailableSyncSourceTypeModified, 
				sheSyncSourceModifiedReqResult }
			STATUS current
			DESCRIPTION 
				"Description."
			::= { sheSyncGroups 3 }

		
		sheSyncObsoleteGroup OBJECT-GROUP
			OBJECTS { sheSyncSourceType, sheSyncSourceHwUnitIndex, sheSyncSourceInterfaceNumber }
			STATUS obsolete
			DESCRIPTION 
				"This parameters are not longer supported by SHE R3.0."
			::= { sheSyncGroups 4 }

		
	
	END

--
-- she-sync-mib.mib
