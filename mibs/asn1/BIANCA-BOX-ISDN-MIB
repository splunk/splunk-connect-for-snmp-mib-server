	::= { isdnCreditsEntry 17 }
	
	isdnCreditsMaxCurrentInCon OBJECT-TYPE
	    SYNTAX  INTEGER
	    ACCESS  read-write
	    STATUS  mandatory

	    DESCRIPTION
		"Maximum number of currently allowed incoming connections.
		 -1 disables this limit."
	::= { isdnCreditsEntry 18 }

	isdnCreditsMaxCurrentOutCon OBJECT-TYPE
