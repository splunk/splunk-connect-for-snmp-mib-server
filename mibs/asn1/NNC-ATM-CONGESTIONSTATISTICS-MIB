  nncAtmCongPointInfoGroup OBJECT-GROUP 
    OBJECTS {
      entPhysicalEntityInfoIndex,
      nncAtmCongPointInfoIndex,
      nncAtmCongPointHardwareType
    }
    STATUS      current
    DESCRIPTION 
      "An object providing various information for a congestion point."
  ::= { nncAtmCongStatsGroups 1 }
  
  nncAtmCongStatsRawGroup OBJECT-GROUP 
    OBJECTS {
      entPhysicalEntityRawIndex,
      nncAtmCongPointRawIndex,
      nncAtmCongStatsQOSRawIndex,
      nncAtmCongStatsTotalCellsArrived,
      nncAtmCongStatsTotalCellsDiscarded,
      nncAtmCongStatsClp0CellsDiscarded,
      nncAtmCongStatsLowCongestedSeconds,
      nncAtmCongStatsMediumCongestedSeconds,
      nncAtmCongStatsSevereCongestedSeconds,
      nncAtmCongStatsTotalSeconds,
      nncAtmCongStatsCurrentCongestionState,
      nncAtmCongStatsAlarmStatus,
      nncAtmCongStatsVirtualBandwidthAvail
    }
    STATUS      current
    DESCRIPTION 
      "A collection of objects providing raw congestion statistics per quality
       of service."
  ::= { nncAtmCongStatsGroups 2 }

  nncAtmCongStatsCurrentShortIntvlGroup OBJECT-GROUP 
