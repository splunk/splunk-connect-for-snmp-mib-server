          -- the Physical Layer Interface Administrative table

          aix1PhysAdminTable OBJECT-TYPE
              SYNTAX     SEQUENCE OF PhysAdminEntry
              ACCESS     not-accessible
              STATUS     mandatory
              DESCRIPTION
                  "The table of administrative elements which apply
                  to an individual X.25 link at the physical layer."
              ::= { aiX1Phys 1 }

          aix1PhysAdminEntry OBJECT-TYPE
              SYNTAX     PhysAdminEntry
              ACCESS     not-accessible
              STATUS     mandatory
              DESCRIPTION
                  "The addressing information for one of this X.25
                  link's physical layer administrative objects."
              INDEX   { aix1PhysAdminIndex }
              ::= { aix1PhysAdminTable 1 }

          PhysAdminEntry ::=
              SEQUENCE {
                  aix1PhysAdminIndex
                      IfIndexType,
                  aix1PhysAdminInterFrameDelayStatus
                      INTEGER,
                  aix1PhysAdminInterFrameDelayInterval
                      INTEGER,
                  aix1PhysAdminT1LineBuildOut
                      INTEGER
              }


    aix1PhysAdminIndex OBJECT-TYPE
        SYNTAX     IfIndexType
        ACCESS     read-only
        STATUS     mandatory
        DESCRIPTION
            "The ifIndex value of the physical port associated with
            these objects."
        ::= { aix1PhysAdminEntry 1 }
    
    aix1PhysAdminInterFrameDelayStatus OBJECT-TYPE
        SYNTAX     INTEGER {
                         ifdDisabled(1),
                         ifdEnabled(2)
                        }
        ACCESS     read-write
        STATUS     mandatory
        DESCRIPTION
            "Enables and disables a delay period between transmitted
            frames."
        DEFVAL     { ifdDisabled }
        ::= { aix1PhysAdminEntry 2 }

    aix1PhysAdminInterFrameDelayInterval OBJECT-TYPE
        SYNTAX     INTEGER(0..'7fffffff'h)
        ACCESS     read-write
        STATUS     mandatory
        DESCRIPTION
            "Sets the interframe delay interval.  This value is
            effective only if aix1PhysAdminInterFrameDelayStatus is
            set to ifdEnabled(2).  This value is expressed in
            microseconds, with 100 microsecond granularity (it's
            truncated to the nearest 100 microseconds)."
        DEFVAL     { 100 }
        ::= { aix1PhysAdminEntry 3 }

    aix1PhysAdminT1LineBuildOut OBJECT-TYPE
        SYNTAX     INTEGER {
                         lbo0to133(1),
                         lbo133to266(2),
                         lbo266to399(3),
                         lbo399to533(4),
                         lbo533to655(5)
                        }
        ACCESS     read-write
        STATUS     mandatory
        DESCRIPTION
            "The T1 line build out value.  The specified syntax
            represents ranges of line length expressed in feet."
        DEFVAL     { lbo133to266 }
        ::= { aix1PhysAdminEntry 4 }


