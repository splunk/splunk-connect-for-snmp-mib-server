              SYNTAX     SEQUENCE OF PktStatEntry
              ACCESS     not-accessible
              STATUS     mandatory
              DESCRIPTION
                      "The table of statistical elements which apply
                      to an individual X.25 link at the packet layer."
              ::= { aiX1Pkt 3 }

          aix1PktStatEntry OBJECT-TYPE
              SYNTAX     PktStatEntry
              ACCESS     not-accessible
              STATUS     mandatory
              DESCRIPTION
                      "The addressing information for one of this
                      X.25 link's packet layer statistical objects."
              INDEX   { aix1PktStatIndex }
              ::= { aix1PktStatTable 1 }

          PktStatEntry ::=
              SEQUENCE {
                  aix1PktStatIndex
                      IfIndexType,
                  aix1PktStatInCharsPerSec80s
                      Counter32,
                  aix1PktStatOutCharsPerSec80s
                      Counter32
              }


    aix1PktStatIndex OBJECT-TYPE
        SYNTAX     IfIndexType
        ACCESS     read-only
        STATUS     mandatory
        DESCRIPTION
            "The ifIndex value of the packet layer entity associated
            with these objects."
        ::= { aix1PktStatEntry 1 }

    aix1PktStatInCharsPerSec80s OBJECT-TYPE
        SYNTAX     Counter32
        ACCESS     read-only
        STATUS     mandatory
        DESCRIPTION
            "The average number of data characters (characters in the
            data field of X.25 data packets) received from the network
            per second over the last 80 seconds by this packet layer
            entity."
        ::= { aix1PktStatEntry 2 }

    aix1PktStatOutCharsPerSec80s OBJECT-TYPE
        SYNTAX     Counter32
        ACCESS     read-only
        STATUS     mandatory
        DESCRIPTION
            "The average number of data characters (characters in the
            data field of X.25 data packets) transmitted to the network
            per second over the last 80 seconds by this packet layer
            entity."
        ::= { aix1PktStatEntry 3 }


