              SEQUENCE {
                  aix1PhysStatIndex
                      IfIndexType,
                  aix1PhysStatTxAborts
                      Counter32
              }


    aix1PhysStatIndex OBJECT-TYPE
        SYNTAX     IfIndexType
        ACCESS     read-only
        STATUS     mandatory
        DESCRIPTION
            "The ifIndex value of the physical port associated with
            these objects."
        ::= { aix1PhysStatEntry 1 }

    aix1PhysStatTxAborts OBJECT-TYPE
        SYNTAX     Counter32
        ACCESS     read-only
        STATUS     mandatory
        DESCRIPTION
            "The number of abort sequences transmitted on the serial
            interfaces.  This is a proprietary object because the
            RS232-like MIB (RFC 1659) doesn't define a similar object!
            RFC 1659 defines a counter for receive aborts, but not
            transmit aborts."
        ::= { aix1PhysStatEntry 2 }


          -- the Virtual Connection Circuit table

          aix1CircuitTable OBJECT-TYPE
              SYNTAX     SEQUENCE OF CircuitEntry
              ACCESS     not-accessible
              STATUS     mandatory
              DESCRIPTION
                  "The table of objects which maps logical channel
                  number and packet level ifIndex number to  Virtual
                  Connection(VC) number."
              ::= { aiX1VC 1 }

          aix1CircuitEntry OBJECT-TYPE
              SYNTAX     CircuitEntry
              ACCESS     not-accessible
              STATUS     mandatory
              DESCRIPTION
                  "The addressing information for the VC Circuit table."
              INDEX   { aix1CircuitLinkId,aix1CircuitChannelNumber }
              ::= { aix1CircuitTable 1 }

          CircuitEntry ::=
              SEQUENCE {
                  aix1CircuitLinkId
                      IfIndexType,
                  aix1CircuitChannelNumber
                      INTEGER,
                  aix1CircuitState
                      INTEGER,
                  aix1CircuitConType
                      INTEGER,
                  aix1CircuitBCNumber
                      INTEGER,
                  aix1CircuitUserData
                      DisplayString,
                  aix1CircuitRemoteBasePort
                      INTEGER,
                  aix1CircuitRemoteVCNumber
                      INTEGER,
                  aix1CircuitPktsSent
                      Counter32,
                  aix1CircuitPktsRcvd
                      Counter32,
                  aix1CircuitOperPVCType
                      INTEGER,
                  aix1CircuitOperPVCTimer
                      INTEGER
              }

 
    aix1CircuitLinkId OBJECT-TYPE
        SYNTAX     IfIndexType
        ACCESS     read-only
        STATUS     mandatory
        DESCRIPTION
            "The ifindex value of the packet layer entity."
        ::= { aix1CircuitEntry 1 }

    aix1CircuitChannelNumber OBJECT-TYPE
        SYNTAX     INTEGER (0..4095)
        ACCESS     read-only
        STATUS     mandatory
        DESCRIPTION
            "X.25 logical channel number."
        ::= { aix1CircuitEntry 2 }

    aix1CircuitState OBJECT-TYPE
        SYNTAX     INTEGER {
                         idle(1),
                         active(2),
                         csCall(3),
                         csDisco(4),
                         csCancel(5),
                         csReject(6),
                         csCol(7),
                         csUncol(8),
                         npCall(9),
                         npDisco(10),
                         npCancel(11),
                         npReject(12),
                         npCol(13),
                         npUncol(14),
                         trapped(15),
                         csSuspend(16)
                        }
        ACCESS     read-only
        STATUS     mandatory
        DESCRIPTION
            "The call processing state of the local Virtual Connection.
            This is not a standard packet level state.  This is the
            call processing state of the AI version 7 system firmware
            that mediates between the X.25 protocol stack and AISwitch
            internal connection mechanisms."
        ::= { aix1CircuitEntry 3 }

    aix1CircuitConType OBJECT-TYPE
        SYNTAX     INTEGER {
                         lmux(1),
                         rmux(2),
                         debug(5),
                         background(6),
                         pvc(11),
                         svc(12)
                        }
        ACCESS     read-only
        STATUS     mandatory
        DESCRIPTION
            "The type of connection currently supported by the VC."
        ::= { aix1CircuitEntry 4 }

    aix1CircuitBCNumber OBJECT-TYPE
        SYNTAX     INTEGER (0..31)
        ACCESS     read-only
        STATUS     mandatory
        DESCRIPTION
            "The number of the Bus Connection with which this VC is
            associated."
        ::= { aix1CircuitEntry 5 }

    aix1CircuitUserData OBJECT-TYPE
        SYNTAX     DisplayString (SIZE (0..16))
        ACCESS     read-only
        STATUS     mandatory
        DESCRIPTION
            "The user data that was sent or received in the call
            request."
        ::= { aix1CircuitEntry 6 }

    aix1CircuitRemoteBasePort OBJECT-TYPE
        SYNTAX     INTEGER (-1..256)
        ACCESS     read-only
        STATUS     mandatory
        DESCRIPTION
            "The baseport of the card to which the SLC is connected
            across the backplane.  This value is meaningful only when
            this VC is multiplexed to the other SLC.  If not, the value
            returned is -1."
        ::= { aix1CircuitEntry 7 }

    aix1CircuitRemoteVCNumber OBJECT-TYPE
        SYNTAX     INTEGER(0..'ffff'h)
        ACCESS     read-only
        STATUS     mandatory
        DESCRIPTION
            "The Virtual Connection number of the VC on the remote SLC
            to which this VC is connected.  This value is meaningful
            only on multiplexed VCs."
        ::= { aix1CircuitEntry 8 }

    aix1CircuitPktsSent OBJECT-TYPE
        SYNTAX     Counter32
        ACCESS     read-only
        STATUS     mandatory
        DESCRIPTION
            "The number of packets sent to the packet level by the AI
            version 7 system software.  This counts all kinds of packets,
            not just data packets."
        ::= { aix1CircuitEntry 9 }

    aix1CircuitPktsRcvd OBJECT-TYPE
        SYNTAX     Counter32
        ACCESS     read-only
        STATUS     mandatory
        DESCRIPTION
            "The number of packets received from the packet level by the
            AI version 7 system software.  This counts all kinds of
            packets, not just data packets."
        ::= { aix1CircuitEntry 10 }

    aix1CircuitOperPVCType OBJECT-TYPE
        SYNTAX     INTEGER {
                         none(1),
                         active (2),
                         connectOnActivity(3),
                         passive (4)
                        }
        ACCESS     read-write
        STATUS     mandatory
        DESCRIPTION
            "Defines the relationship between a PVC and the 
            connection-oriented AISwitch TDM backplane.  The three types
            represented are defined as follows:
              Active:  initiates connections across the backplane at an
                       interval defined by the timer.
              Passive: waits to respond to a connection attempt across
                       the backplane.  Uses timer to wait for reset
                       confirm from network.  Accepts backplane
                       connection if it receives reset confirmation in
                       time.
              Connect on Activity:
                       initiates connection across backplane upon the
                       receipt of data from the network.  Forwards
                       received data once connection is established.
                       Disconnects backplane connection if a timer
                       interval passes with no data activity in either
                       direction.

            This object will return the value none(1) if the value of
            aix1CircuitConType for this VC is not pvc(11).

            This object reflects the current operational value of this
            parameter.  Note that unlike most operational objects, it is
            writable.  If the value of this object is changed, it modifies
            the current behavior of the board, but is not saved in
            nonvolatile memory."
        DEFVAL     { active }
        ::= { aix1CircuitEntry 11 }

    aix1CircuitOperPVCTimer OBJECT-TYPE
        SYNTAX     INTEGER(0..600)
        ACCESS     read-write
        STATUS     mandatory
        DESCRIPTION
            "Defines the timer interval, in seconds used to manage the
            PVC's relationship with an internal backplane connection.
            The timer is used for different purposes depending on the
            value of aix1VCPVCType as described above.

            This object will return the value 0 if the VC in question
            is not associated with a PVC.

            This object reflects the current operational value of this
            parameter.  Note that unlike most operational objects, it is
            writable. If the value of this object is changed, it modifies
            the current behavior of the board, but is not saved in
            nonvolatile memory."
        DEFVAL     { 15 }
        ::= { aix1CircuitEntry 12 }


        -- the Virtual Connection table

          aix1VCTable OBJECT-TYPE
              SYNTAX     SEQUENCE OF VCEntry
              ACCESS     not-accessible
              STATUS     mandatory
              DESCRIPTION
                  "The table of objects containing information pertaining
                  to Virtual Connections(VC)."
              ::= { aiX1VC 2 }

          aix1VCEntry OBJECT-TYPE
              SYNTAX     VCEntry
              ACCESS     not-accessible
              STATUS     mandatory
              DESCRIPTION
                      "The addressing information for the VC table."
              INDEX   { aix1VCVCNumber }
              ::= { aix1VCTable 1 }

          VCEntry ::=
              SEQUENCE {
                  aix1VCVCNumber
                      INTEGER(0..287),
                  aix1VCState
                      INTEGER,
                  aix1VCConType
                      INTEGER,
                  aix1VCBCNumber
                      INTEGER,
                  aix1VCUserData
                      DisplayString,
                  aix1VCRemoteBasePort
                      INTEGER,
                  aix1VCRemoteVCNumber
                      INTEGER,
                  aix1VCPktsSent
                      Counter32,
                  aix1VCPktsRcvd
                      Counter32,
                  aix1VCOperPVCType
                      INTEGER,
                  aix1VCOperPVCTimer
                      INTEGER
              }


    aix1VCVCNumber OBJECT-TYPE
        SYNTAX     INTEGER (0..287)
        ACCESS     read-only
        STATUS     mandatory
        DESCRIPTION
            "The local Virtual Connection number."
        ::= { aix1VCEntry 1 }

    aix1VCState OBJECT-TYPE
        SYNTAX     INTEGER {
                         idle(1),
                         active(2),
                         csCall(3),
                         csDisco(4),
                         csCancel(5),
                         csReject(6),
                         csCol(7),
                         csUncol(8),
                         npCall(9),
                         npDisco(10),
                         npCancel(11),
                         npReject(12),
                         npCol(13),
                         npUncol(14),
                         trapped(15),
                         csSuspend(16)
                        }
        ACCESS     read-only
        STATUS     mandatory
        DESCRIPTION
            "The call processing state of the local Virtual Connection.
            This is not a standard packet level state.  This is the call
            call processing state of the AI version 7 system firmware
            that mediates between the X.25 protocol stack and AISwitch
            internal connection mechanisms."
        ::= { aix1VCEntry 2 }

    aix1VCConType OBJECT-TYPE
        SYNTAX     INTEGER {
                         lmux(1),
                         rmux(2),
                         debug(5),
                         background(6),
                         pvc(11),
                         svc(12)
                        }
        ACCESS     read-only
        STATUS     mandatory
        DESCRIPTION
            "The type of connection currently supported by the VC."
        ::= { aix1VCEntry 3 }

    aix1VCBCNumber OBJECT-TYPE
        SYNTAX     INTEGER (0..31)
        ACCESS     read-only
        STATUS     mandatory
        DESCRIPTION
            "The number of the Bus Connection with which this VC is
            associated."
        ::= { aix1VCEntry 4 }

    aix1VCUserData OBJECT-TYPE
        SYNTAX     DisplayString (SIZE (0..16))
        ACCESS     read-only
        STATUS     mandatory
    DESCRIPTION
            "The user data that was sent or received in the call request."
        ::= { aix1VCEntry 5 }

    aix1VCRemoteBasePort OBJECT-TYPE
        SYNTAX     INTEGER (-1..256)
        ACCESS     read-only
        STATUS     mandatory
        DESCRIPTION
            "The baseport of the card to which the SLC is connected
            across the backplane.  This value is meaningful only when
            this VC is multiplexed to the other SLC.  If not, the value
            returned is -1."
        ::= { aix1VCEntry 6 }

    aix1VCRemoteVCNumber OBJECT-TYPE
        SYNTAX     INTEGER(0..'ffff'h)
        ACCESS     read-only
        STATUS     mandatory
        DESCRIPTION
            "The Virtual Connection number of the VC on the remote SLC
            to which this VC is connected.  This value is meaningful
            only on multiplexed VCs."
        ::= { aix1VCEntry 7 }

    aix1VCPktsSent OBJECT-TYPE
        SYNTAX     Counter32
        ACCESS     read-only
        STATUS     mandatory
        DESCRIPTION
            "The number of packets sent to the packet level by the AI
            version 7 system software.  This counts all kinds of packets,
            not just data packets."
        ::= { aix1VCEntry 8 }

    aix1VCPktsRcvd OBJECT-TYPE
        SYNTAX     Counter32
        ACCESS     read-only
        STATUS     mandatory
        DESCRIPTION
            "The number of packets received from the packet level by
            the AI version 7 system software.  This counts all kinds
            of packets, not just data packets."
        ::= { aix1VCEntry 9 }

    aix1VCOperPVCType OBJECT-TYPE
        SYNTAX     INTEGER {
                         none(1),
                         active(2),
                         connectOnActivity(3),
                         passive(4)
                        }
        ACCESS     read-write
        STATUS     mandatory
        DESCRIPTION
            "Defines the relationship between a PVC and the 
            connection-oriented AISwitch TDM backplane.  The three
            types represented are defined as follows:
              Active:  initiates connections across the backplane at
                       an interval defined by the timer.
              Passive: waits to respond to a connection attempt across
                       the backplane.  Uses timer to wait for reset
                       confirm from network.  Accepts backplane
                       connection if it receives reset confirmation in
                       time.
              Connect on Activity:
                       initiates connection across backplane upon
                       receipt of data from network.  Forwards received
                       data once connection is established.  Disconnects
                       backplane connection if a timer interval passes
                       with no data activity in either direction.

            This object will return the value none(1) if the value of
            aix1VCConType for this VC is not pvc(11).

            This object reflects the current operational value of this
            parameter.  Note that unlike most operational objects, it is
            writable.  If the value of this object is changed, it
            modifies the current behavior of the board, but is not saved
            in nonvolatile memory."
        DEFVAL     { active }
        ::= { aix1VCEntry 10 }

    aix1VCOperPVCTimer OBJECT-TYPE
        SYNTAX     INTEGER(0..600)
        ACCESS     read-write
        STATUS     mandatory
        DESCRIPTION
            "Defines the timer interval, in seconds used to manage the
            PVC's relationship with an internal backplane connection.
            The timer is used for different purposes depending on the
            value of aix1VCPVCType as described above.

            This object will return the value 0 if the VC in question
            question is not associated with a PVC.

            This object reflects the current operational value of this
            parameter.  Note that unlike most operational objects, it is
            writable.  If the value of this object is changed, it
            modifies the current behavior of the board, but is not saved
            in nonvolatile memory."
        DEFVAL     { 15 }
        ::= { aix1VCEntry 11 }


          -- the PVC Admin table

          aix1AdminPVCTable OBJECT-TYPE
              SYNTAX     SEQUENCE OF AdminPVCEntry
              ACCESS     not-accessible
              STATUS     mandatory
              DESCRIPTION
                  "The table of objects which control attributes of PVCs
                  configured, but not necessarily in effect."
              ::= { aiX1VC 3 }

          aix1AdminPVCEntry OBJECT-TYPE
              SYNTAX     AdminPVCEntry
              ACCESS     not-accessible
              STATUS     mandatory
              DESCRIPTION
                  "The addressing information for the PVC Admin table."
              INDEX   { aix1AdminPVCLinkId,aix1AdminPVCChannelNumber }
              ::= { aix1AdminPVCTable 1 }

          AdminPVCEntry ::=
              SEQUENCE {
                  aix1AdminPVCLinkId
                      IfIndexType,
                  aix1AdminPVCChannelNumber
                      INTEGER,
                  aix1AdminPVCType
                      INTEGER,
                  aix1AdminPVCTimer
                      INTEGER
              }


    aix1AdminPVCLinkId OBJECT-TYPE
        SYNTAX     IfIndexType
        ACCESS     read-only
        STATUS     mandatory
        DESCRIPTION
            "The ifindex value of the packet layer entity."
        ::= { aix1AdminPVCEntry 1 }

    aix1AdminPVCChannelNumber OBJECT-TYPE
        SYNTAX     INTEGER (0..4095)
        ACCESS     read-only
        STATUS     mandatory
        DESCRIPTION
            "X.25 logical channel number."
        ::= { aix1AdminPVCEntry 2 }

    aix1AdminPVCType OBJECT-TYPE
        SYNTAX     INTEGER {
                         none(1),
                         active(2),
                         connectOnActivity(3),
                         passive(4)
                        }
        ACCESS     read-write
        STATUS     mandatory
        DESCRIPTION
            "Defines the relationship between a PVC and the
            connection-oriented AISwitch TDM backplane.  The three
            types represent are defined as follows:
              Active:  initiates connections across the backplane at
                       an interval defined by the timer.
              Passive: waits to respond to a connection attempt across
                       the backplane.  Uses timer to wait for reset
                       confirm from network.  Accepts the backplane
                       connection if it receives reset confirmation in
                       time.
              Connect on Activity:
                       initiates connection across backplane upon
                       receipt of data from network.  Forwards received
                       data once connection is established.  Disconnects
                       backplane connection if a timer interval passes
                       with no data activity in either direction.

            This object will return the value none(1) if
            aix1AdminPVCChannelNumber doesn't correspond to a channel in
            the range specified by the combination of x25admnNumberPVCs
            and aix1PktAdminPVCOffset for the link specified by
            aix1AdminPVCLinkID.

            This object is settable, but does not reflect the mandatory
            operational value of this parameter."
        DEFVAL     { active }
        ::= { aix1AdminPVCEntry 3 }

    aix1AdminPVCTimer OBJECT-TYPE
        SYNTAX     INTEGER(0..600)
        ACCESS     read-write
        STATUS     mandatory
        DESCRIPTION
            "Defines the timer interval, in seconds used to manage the
            PVC's relationship with an internal backplane connection.
            The timer is used for different purposes depending on the
            value of aix1VCPVCType as described above.

            This object will return the value 0 if
            aix1AdminPVCChannelNumber doesn't correspond to a channel in
            the range specified by the combination of x25admnNumberPVCs
            and aix1PktAdminPVCOffset for the link specified by
            aix1AdminPVCLinkID.

            This object is settable, but does not reflect the mandatory
            operational value of this parameter."
        DEFVAL     { 15 }
        ::= { aix1AdminPVCEntry 4 }

END
