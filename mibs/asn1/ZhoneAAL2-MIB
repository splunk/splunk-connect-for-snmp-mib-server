        -- 1.3.6.1.4.1.5504.4.2.1.2.1.23
        aal2CidCompletedCalls OBJECT-TYPE
            SYNTAX Counter32
            MAX-ACCESS read-only
            STATUS current
            DESCRIPTION
                "Total number of completed calls made on the CID."
            ::= { aal2CidEntry 23 }

        
-- 
        -- 1.3.6.1.4.1.5504.4.2.1.2.1.24
        aal2CidBlockedCallsNoBandwidth OBJECT-TYPE
            SYNTAX Counter32
            MAX-ACCESS read-only
