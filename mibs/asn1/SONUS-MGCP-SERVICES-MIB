    OBJECTS         { sonusIuaAsName, sonusEventDescription, 
                      sonusEventClass, sonusEventLevel }
    STATUS          current
    DESCRIPTION     
            "This trap indicates the IUA AS is inactive."
    ::= { sonusMgcpServicesMIBNotificationsPrefix 6 }

