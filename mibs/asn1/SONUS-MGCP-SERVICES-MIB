    OBJECTS         { sonusIuaAsName, sonusEventDescription, 
                      sonusEventClass, sonusEventLevel }
    STATUS          current
    DESCRIPTION     
            "This trap indicates the IUA AS is in pending state."
    ::= { sonusMgcpServicesMIBNotificationsPrefix 8 }

sonusIuaAspDownNotification NOTIFICATION-TYPE
    OBJECTS         { sonusIuaAspName, sonusEventDescription, 
                      sonusEventClass, sonusEventLevel }
    STATUS          current
    DESCRIPTION     
            "This trap indicates the IUA ASP is down."
    ::= { sonusMgcpServicesMIBNotificationsPrefix 9 }

sonusIuaAspUpNotification NOTIFICATION-TYPE
    OBJECTS         { sonusIuaAspName, sonusEventDescription, 
                      sonusEventClass, sonusEventLevel }
    STATUS          current
    DESCRIPTION     
            "This trap indicates the IUA ASP is up."
    ::= { sonusMgcpServicesMIBNotificationsPrefix 10 }

