sonusIuaAspStatusTable OBJECT-TYPE
    SYNTAX          SEQUENCE OF SonusIuaAspStatusEntry
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION     
            "This table specifies the status of IUA ASPs for the GSX node."
    ::= { sonusMgcpServicesMIBObjects 16 }

sonusIuaAspStatusEntry OBJECT-TYPE
    SYNTAX          SonusIuaAspStatusEntry
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION     
            ""
    INDEX           { sonusIuaAspStatusIndex }
    ::= { sonusIuaAspStatusTable 1 }

SonusIuaAspStatusEntry ::=
    SEQUENCE {
        sonusIuaAspStatusIndex          Integer32,
        sonusIuaAspStatusNumConnections Integer32,
        sonusIuaAspStatusNumAppServers  Integer32,
        sonusIuaAspStatusStatus         INTEGER
    }

sonusIuaAspStatusIndex OBJECT-TYPE
    SYNTAX          Integer32
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "Identifies the target IUA Application Server Process entry."
    ::= { sonusIuaAspStatusEntry 1 }

sonusIuaAspStatusNumConnections OBJECT-TYPE
    SYNTAX          Integer32
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "The number of IP addresses bound to this Application Server Process"
    ::= { sonusIuaAspStatusEntry 2 }

sonusIuaAspStatusNumAppServers OBJECT-TYPE
    SYNTAX          Integer32
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "The number of Application Servers bound to this Application Server Process"
    ::= { sonusIuaAspStatusEntry 3 }

sonusIuaAspStatusStatus OBJECT-TYPE
    SYNTAX          INTEGER { down(1), up(2) }
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     
            "The status of this Application Server Process.  Active indicates this Application Server Process is actively acting active."
    ::= { sonusIuaAspStatusEntry 4 }

