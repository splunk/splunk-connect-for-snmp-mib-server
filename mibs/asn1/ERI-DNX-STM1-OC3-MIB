dnxStm1Oc3Enterprise OBJECT-IDENTITY
      STATUS current
      DESCRIPTION "ERI DNX Stm1Oc3 Device Enterprise for Alarms/Events"
    ::= { dnxStm1Oc3 0 }

optDevConfigTrap NOTIFICATION-TYPE
        OBJECTS {
            trapSequence,
            optDevCfgAddr,
            optDevCmdStatus
            }
        STATUS current
        DESCRIPTION
              	"This trap is used to notify a NMS that a user has updated 
              	the configuration for a given Stm1Oc3 Device entry."
        ::= { dnxStm1Oc3Enterprise 1 }

optT1E1ConfigTrap NOTIFICATION-TYPE
        OBJECTS {
            trapSequence,
            optT1E1CfgLinkAddr,
            optT1E1CfgCmdStatus
            }
        STATUS current
        DESCRIPTION
              	"This trap is used to notify a NMS that a user has updated 
              	the configuration for a given Stm1Oc3 T1/E1 Link entry."
        ::= { dnxStm1Oc3Enterprise 2 }
                
END

