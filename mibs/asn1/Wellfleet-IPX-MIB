    		OCTET STRING,
    	    wfIpxSapNetLvlAction
    		INTEGER,
    	    wfIpxSapNetLvlIntf
    		OCTET STRING,
    	    wfIpxSapNetLvlIndex
    		INTEGER
        }

    wfIpxSapNetLvlDelete OBJECT-TYPE
        SYNTAX	INTEGER {
    		    created(1),
    		    deleted(2)
    		}
        ACCESS	read-write
