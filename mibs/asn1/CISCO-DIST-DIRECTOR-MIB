--        portion(7),
--        dfpAvailability(8),
--        routeMap(9)
--        }
    ACCESS not-accessible
    STATUS mandatory
    DESCRIPTION
        "The type of a metric in the profile."
    ::= { cddGeneralMetricProfEntry 2 }

