    MICOMBRGEXT DEFINITIONS ::= BEGIN

    IMPORTS
	     micom-oscar 
		     FROM MICOM-OSCAR-MIB
             OBJECT-TYPE
                     FROM RFC-1212;



          MacAddress ::= OCTET STRING (SIZE (6))    -- a 6 octet address
                                                    -- in the
                                                    -- "canonical"
                                                    -- order
          -- defined by IEEE 802.1a, i.e., as if it were transmitted
          -- least significant bit first, even though 802.5 (in
          -- contrast to other n802.x protocols) requires MAC
          -- addresses to be transmitted most significant bit first.
          --

          Timeout ::= INTEGER -- a STP timer in units of 1/100 seconds

          -- To convert a Timeout value into a value in units of
          -- 1/256 seconds, the following algorithm should be used:
          --
          --      b  = floor( (n * 256) / 100)
          --
          -- where:
          --      floor   =  quotient [ignore remainder]
          --      n is the value in 1/100 second units
          --      b is the value in 1/256 second units
          --
          -- To convert the value from 1/256 second units back to
          -- 1/100 seconds, the following algorithm should be used:
          --
          --      n = ceiling( (b * 100) / 256)
          --
          -- where:
          --      ceiling =  quotient [if remainder is 0], or
          --                 quotient + 1 [if remainder is non-zero]
          --      n is the value in 1/100 second units
          --      b is the value in 1/256 second units
          --
          -- Note: it is important that the arithmetic operations are
          -- done in the order specified (i.e., multiply first, divide
          -- second).


    --  This MIB module uses the extended OBJECT-TYPE macro as
    --  defined in [13]


    --  this is the MIB module for MICOM Bridge objects


   mcmBrg    OBJECT IDENTIFIER ::= { micom-oscar 6 }


 -- MICOM Bridge Parameters Group

 -- The implementation of this group is mandatory for all RLR systems. 
 -- This group consists of objects pertaining to the Bridge System
 -- parameters.

   mcmBrgGlobalParamGroup    OBJECT IDENTIFIER ::= { mcmBrg 1 }

mcmBrgIPBridged OBJECT-TYPE
	SYNTAX	INTEGER { bridged(1), routed(2) }
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = IP is being bridged or Routed.;
	   HELP = Is IP being Bridged or Routed?;
	   CAPABILITIES = NET_CFG, VPN_CFG;"
	::= { mcmBrgGlobalParamGroup 1 }

mcmBrgNumInterfaces OBJECT-TYPE
	SYNTAX	INTEGER
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = The number of ports(ethernet & wan) \
                  that the access device has.;
	   HELP = The number of ports this bridge has.;
	   CAPABILITIES = NET_CFG, VPN_CFG;"
	::= { mcmBrgGlobalParamGroup 2 }

mcmBrgSpanEnable OBJECT-TYPE
	SYNTAX	INTEGER { enabled(1), disabled(2) }
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = The current state of the Spanning \
                  tree Algorithm on the access device.;
	   HELP = Spanning-Tree Algorithm Enabled?;
	   CAPABILITIES = NET_CFG, VPN_CFG;"
	::= { mcmBrgGlobalParamGroup 3 }

 
mcmBrgSpoofEnable OBJECT-TYPE
        SYNTAX  INTEGER { enabled(1), disabled(2) }
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION
          "NAME = ;
           DESC = IP-ARP spoofing -when enabled, access device \
                  intercepts most ARP bcasts and handle them locally \
                  to reduce the amount of bcast pkts fwded across WAN.;
           HELP = IP-ARP Spoofing Enabled?;
           CAPABILITIES = NET_CFG, VPN_CFG;"
        ::= { mcmBrgGlobalParamGroup 4 }
 

mcmBrgAgeTime OBJECT-TYPE
        SYNTAX  INTEGER (10..1000000)
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION
          "NAME = ;
           DESC = Aging time for dynamically learned devices \
                  in forwarding Table.;
           HELP = The timeout period in seconds for aging \
                  out dynamically learned forwarding \
                  information. 802.1D-1990 recommends \
                  a default of 300 seconds. Mirror object \
                  of dot1dTpAgingTime.;
           CAPABILITIES = NET_CFG, VPN_CFG;"
        ::= { mcmBrgGlobalParamGroup 5 }



   mcmBrgMiscParamGroup    OBJECT IDENTIFIER ::= { mcmBrg 2 }

 

mcmBrgDebugEnable OBJECT-TYPE
	SYNTAX	INTEGER { enabled(1), disabled(2) }
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = The access device capability to generate controlled \
                  number of debugging messages to CLI for \
                  the purpose of debugging some bridge \
                  status.; 
	   HELP = Bridge Debugging Enabled?;
	   CAPABILITIES = NET_CFG, VPN_DISP;"
	::= { mcmBrgMiscParamGroup 1 }

mcmBrgSpanDebugEnable OBJECT-TYPE
	SYNTAX	INTEGER { enabled(1), disabled(2) }
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = The access device capability to debug spanning \
                  tree algorithm.;
	   HELP = Spanning-Tree Debugging Enabled?;
	   CAPABILITIES = NET_CFG, VPN_DISP;"
	::= { mcmBrgMiscParamGroup 2 }


mcmBrgSpoofCacheAge OBJECT-TYPE
        SYNTAX  INTEGER(60..1800)
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = The aging time for entries in the ARP Spoof\
                  cache table to specified by the cache timeout \
                  to make room for active entries.;
	   HELP = The Aging Time for the ARP Spoof Cache.;
	   CAPABILITIES = NET_CFG, VPN_CFG;"
	::= { mcmBrgMiscParamGroup 3 }

mcmBrgSpoofThresholdAge OBJECT-TYPE
        SYNTAX  INTEGER(10..120)
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = The Aging Time for the ARP Spoofcache Threshold \
                  in seconds.(60 .. 1800 sec).;
	   HELP = The Aging Time for the ARP Spoof Threshold.;
	   CAPABILITIES = NET_CFG, VPN_CFG;"
	::= { mcmBrgMiscParamGroup 4 }

mcmBrgSpoofThresholdCount OBJECT-TYPE
        SYNTAX  INTEGER(5..30)
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = The ARP Spoof Threshold count- allows \
                  the max number of consecutive outstanding \
                  ARP request per destination node.;
	   HELP = The ARP Spoof Threshold count.;
	   CAPABILITIES = NET_CFG, VPN_CFG;"
	::= { mcmBrgMiscParamGroup 5 }



	-- MICOM Bridge Port Config Table.

mcmBrgConfPortTable OBJECT-TYPE
              SYNTAX  SEQUENCE OF McmBrgConfPortEntry
              ACCESS  not-accessible
              STATUS  mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = A config Table that contains port-specific \
                  information for the access device Bridge \
				  Configuration information.;
	   HELP = ;
	   CAPABILITIES = NET_CFG, VPN_CFG;"
              ::= { mcmBrg 3 }

          mcmBrgConfPortEntry OBJECT-TYPE
              SYNTAX  McmBrgConfPortEntry
              ACCESS  not-accessible
              STATUS  mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = Configuration parameter information maintained by \
                  each Port that are being used by the access \ 
                  device.;
	   HELP = ;
	   CAPABILITIES = ;"
              INDEX   { mcmBrgConfPortIndex }
              ::= { mcmBrgConfPortTable 1 }

          McmBrgConfPortEntry ::=
              SEQUENCE {
                  mcmBrgConfPortIndex
                      INTEGER,
                  mcmBrgConfPortPPA
                      INTEGER,
                  mcmBrgConfPortType
                      INTEGER,
                  mcmBrgConfPortMacFilterFlag
                      INTEGER,
                  mcmBrgConfPortEtFilterFlag
                      INTEGER,
                  mcmBrgConfPortSapFilterFlag
                      INTEGER,
                  mcmBrgConfPortMacInclExcl
                      INTEGER,
                  mcmBrgConfPortEtInclExcl
                      INTEGER,
                  mcmBrgConfPortSapInclExcl
                      INTEGER
              }

mcmBrgConfPortIndex OBJECT-TYPE
	SYNTAX	INTEGER
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = The Port Index to which this entry pertains.;
	   HELP = ;
	   CAPABILITIES = ;"
	::= { mcmBrgConfPortEntry 1 }

mcmBrgConfPortPPA OBJECT-TYPE
	SYNTAX	INTEGER
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC =  The Protocol Point of Access to which \
                  this entry pertains.;
	   HELP = ;
	   CAPABILITIES = ;"
	::= { mcmBrgConfPortEntry 2 }

mcmBrgConfPortType OBJECT-TYPE
	SYNTAX	INTEGER {
		ethernet(1),
		wan(2),
		frameRelay(3),
		internal(4)
	}
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = The Type of the interface this port \
                  is attached to.;
	   HELP = ;
	   CAPABILITIES = ;"
	::= { mcmBrgConfPortEntry 3 }


mcmBrgConfPortMacFilterFlag OBJECT-TYPE
	SYNTAX	INTEGER { enabled(1), disabled(2) }
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = When MAC address filtering is needed on a \
                  specific port, enable MAC filter Flag. \
                  Otherwise, disable the MAC Filetr Flag.;
	   HELP = Is MAC Filtering enabled?;
	   CAPABILITIES = ;"
	::= { mcmBrgConfPortEntry 4 }

mcmBrgConfPortEtFilterFlag OBJECT-TYPE
	SYNTAX	INTEGER { enabled(1), disabled(2) }
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = When E-type filtering is needed on a specific \
                  port, enable E-type filter Flag. Otherwise, \
                  disable E-type Filter Flag.;
	   HELP = Is E-type Filtering enabled?;
	   CAPABILITIES = ;"
	::= { mcmBrgConfPortEntry 5 }

mcmBrgConfPortSapFilterFlag OBJECT-TYPE
	SYNTAX	INTEGER { enabled(1), disabled(2) }
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = When SAP filtering is needed on a specific \
                  port, enable SAP filter flag. Otherwise, \
                  disable the SAP filter flag.;
	   HELP = Is SAP Filtering enabled?;
	   CAPABILITIES = ;"
	::= { mcmBrgConfPortEntry 6 }

mcmBrgConfPortMacInclExcl OBJECT-TYPE
	SYNTAX	INTEGER { inclusive(1), exclusive(2) }
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = If Inclusive MAC filtering is set, only \
                  addresses in the table are filtered/discarded.\
                  If Exclusive, allow only addresses in the \
                  table to be accepted/learned.;
	   HELP = Inclusive or Exclusive Mac Filtering?;
	   CAPABILITIES = ;"
	::= { mcmBrgConfPortEntry 7 }

mcmBrgConfPortEtInclExcl OBJECT-TYPE
	SYNTAX	INTEGER { inclusive(1), exclusive(2) }
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC =  If Inclusive E-Type filtering is set, only \
                   E-Type in the table are filtered/discarded.\
                   If Exclusive, allow only E-Type in the \ 
                   table to be accepted/learned.;
	   HELP = Inclusive or Exclusive Et Filtering?;
	   CAPABILITIES = ;"
	::= { mcmBrgConfPortEntry 8 }

mcmBrgConfPortSapInclExcl OBJECT-TYPE
	SYNTAX	INTEGER { inclusive(1), exclusive(2) }
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = If Inclusive Sap Type filtering is set, only \
                  Sap Types in the Table are filtered/discarded. \
                  If Exclusive, allow only Sap Types in the \ 
                  table to be accepted/learned.;
	   HELP = Inclusive or Exclusive Sap Filtering?;
	   CAPABILITIES = ;"
	::= { mcmBrgConfPortEntry 9 }


	-- MICOM Bridge MAC Filter Table.

mcmBrgMacFilterTable OBJECT-TYPE
              SYNTAX  SEQUENCE OF McmBrgMacFilterEntry
              ACCESS  not-accessible
              STATUS  mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = A table that contains port-specific \
                  information for the access device MAC Filter \
                  info including the source & destination.;
	   HELP = ;
	   CAPABILITIES = NET_CFG, VPN_CFG;"
              ::= { mcmBrg 4 }

          mcmBrgMacFilterEntry OBJECT-TYPE
              SYNTAX  McmBrgMacFilterEntry
              ACCESS  not-accessible
              STATUS  mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = MAC Filter parameter information maintained by \
                  every port that the access device uses.;
	   HELP = ;
	   CAPABILITIES = ;"
              INDEX   { mcmBrgMacFilterPortIndex, mcmBrgMacFilterNumber }
              ::= { mcmBrgMacFilterTable 1 }

          McmBrgMacFilterEntry ::=
              SEQUENCE {
                  mcmBrgMacFilterPortIndex
                      INTEGER,
                  mcmBrgMacFilterNumber
                      INTEGER,
                  mcmBrgMacAddress
                      MacAddress,
                  mcmBrgMacType
                      INTEGER
	}



mcmBrgMacFilterPortIndex OBJECT-TYPE
        SYNTAX  INTEGER(1..16)
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = The Port Index particular to this entry.;
	   HELP = ;
	   CAPABILITIES = ;"
	::= { mcmBrgMacFilterEntry 1 }

mcmBrgMacFilterNumber OBJECT-TYPE
        SYNTAX  INTEGER(1..32)
        ACCESS  read-write
        STATUS  mandatory
        DESCRIPTION
          "NAME = ;
           DESC = The Filter number associated with the entry.;
           HELP = ;
           CAPABILITIES = ;"
        ::= { mcmBrgMacFilterEntry 2 }
 

mcmBrgMacAddress OBJECT-TYPE
	SYNTAX	MacAddress
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = The MAC address(HW address) that needs to be \
                  filtered.;
	   HELP = ;
	   CAPABILITIES = ;"
	::= { mcmBrgMacFilterEntry 3 }

mcmBrgMacType OBJECT-TYPE
	SYNTAX	INTEGER { 
		source(1),
		destination(2),
		invalid(3) 
	}
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = Filter based on source MAC address or \
                  destination MAC address.;
	   HELP = The type of the MAC Address. By setting \
		  it to invalid	one can invalidate the \
		  entry.;
	   CAPABILITIES = ;"
	::= { mcmBrgMacFilterEntry 4 }

	-- MICOM Bridge E-Type Filter Table.

mcmBrgEtFilterTable OBJECT-TYPE
              SYNTAX  SEQUENCE OF McmBrgEtFilterEntry
              ACCESS  not-accessible
              STATUS  mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = A table that contains port-specific \
                  information for the access device \
				  E-type Filter.;
	   HELP = 
	   CAPABILITIES = NET_CFG, VPN_CFG;"
              ::= { mcmBrg 5 }

          mcmBrgEtFilterEntry OBJECT-TYPE
              SYNTAX  McmBrgEtFilterEntry
              ACCESS  not-accessible
              STATUS  mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = A list of information maintained by \
                  each port about the access device \
				  E-type Filter parameters for that port.;
	   HELP = 
	   CAPABILITIES = ;"
              INDEX   { mcmBrgEtFilterPortIndex , mcmBrgEtFilterNumber }
              ::= { mcmBrgEtFilterTable 1 }

          McmBrgEtFilterEntry ::=
              SEQUENCE {
                  mcmBrgEtFilterPortIndex
                      INTEGER,
                  mcmBrgEtFilterNumber
                      INTEGER,
                  mcmBrgEtFilterStatus
                      INTEGER,
                  mcmBrgEtFilterEType
                      INTEGER,
                  mcmBrgEtFilterUpperRange
                      INTEGER,
                  mcmBrgEtFilterLowerRange
                      INTEGER
	}

mcmBrgEtFilterPortIndex OBJECT-TYPE
        SYNTAX  INTEGER(1..16) 
        ACCESS  read-write
        STATUS  mandatory
        DESCRIPTION
          "NAME = ;
           DESC = The port Index particular to this port.;
           HELP = ;
           CAPABILITIES = ;"
        ::= { mcmBrgEtFilterEntry 1 }
 


mcmBrgEtFilterNumber OBJECT-TYPE
        SYNTAX  INTEGER(1..32)
        ACCESS  read-write
        STATUS  mandatory
        DESCRIPTION
          "NAME = ;
           DESC =  The Filter number associated with the \
                   port Index.; 
           HELP = ;
           CAPABILITIES = ;"
        ::= { mcmBrgEtFilterEntry 2 }

mcmBrgEtFilterStatus OBJECT-TYPE
	SYNTAX	INTEGER { singular(1), range(2), invalid(3) }
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = An indication as to whether the filter \^M
               is specified as a single e-type or as a range of \
               as a range of e-types.; ;
	   HELP = 
           HELP = Setting the value tp invalid, has the effect of \
                  the entry,;
	   CAPABILITIES = ;"
	::= { mcmBrgEtFilterEntry 3 }

mcmBrgEtFilterEType OBJECT-TYPE
        SYNTAX  INTEGER (1536..65535)
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = The singular e-type to be filtered.;
           HELP = The value of this object is valid only \
		  if the mcmBrgEtFilterStatus (above) \
		  is singular.;
	   CAPABILITIES = ;"
	::= { mcmBrgEtFilterEntry 4 }

mcmBrgEtFilterUpperRange OBJECT-TYPE
	SYNTAX	INTEGER (1536..65535)
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = 
           DESC = The Upper Range of the E-type to be \
              filtered. Range is between  0 - 65535.; 
           HELP = This object is valid only if the \
                  mcmBrgEtFilterStatus (above) is range.;
	   CAPABILITIES = ;"
	::= { mcmBrgEtFilterEntry 5 }

mcmBrgEtFilterLowerRange OBJECT-TYPE
	SYNTAX	INTEGER (1536..65535)
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = The lower Range of the E-type to be \
                  filtered. Range is between 0 - 65535.;
           HELP = This object is valid only \
                  if the mcmBrgEtFilterStatus (above) \
                  is range.;
	   CAPABILITIES = ;"
	::= { mcmBrgEtFilterEntry 6 }


	-- MICOM Bridge SAP Filter Table.

