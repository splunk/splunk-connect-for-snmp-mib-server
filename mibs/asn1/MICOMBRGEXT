	  "NAME = ;
       DESC = This object was obsoleted as of release \
	      3.0; and is included here only to support \
	      backwards compatibility for software \
	      versions prior to release 3.0. \
	      When set to reset, the spanning Tree port \
          counters will be zeroed out.;
           HELP = ;
	   CAPABILITIES = ;"
        ::= { mcmBrgCounterZero 1 }

mcmBrgTpCounterZero OBJECT-TYPE
        SYNTAX  INTEGER { reset(1) }
	ACCESS	write-only
        STATUS  obsolete
	DESCRIPTION
	  "NAME = ;
       DESC = This object was obsoleted as of release \
	      3.0; and is included here only to support \
	      backwards compatibility for software \
	      versions prior to release 3.0. \
	      When set to reset, the Transparent port \
          counters will be zeroed out.;
	   HELP = ;
	   CAPABILITIES = ;"
        ::= { mcmBrgCounterZero 2 }

END

