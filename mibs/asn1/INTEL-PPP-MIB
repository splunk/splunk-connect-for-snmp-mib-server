    SYNTAX INTEGER {
        disabled(1),
        enabled(2)
        }
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION "Using old BACP id's to support remote device."
    ::= { pppLinkBacpEntry 23 }

pppLinkIpcpTable OBJECT-TYPE
    SYNTAX SEQUENCE OF PppLinkIpcpEntry
    ACCESS not-accessible
    STATUS mandatory
    DESCRIPTION "IPCP (IP Control Protocol) information table. Values are
    first valid when protocol has state OPENED. Not used for multilink
    slave links (pppLinkProtoStatePppLinkType = multilinkslave)."
    ::= { pppLink 12 }

pppLinkIpcpEntry OBJECT-TYPE
    SYNTAX PppLinkIpcpEntry
    ACCESS not-accessible
    STATUS mandatory
    DESCRIPTION ""
    INDEX { pppLinkIpcpIfIndex }
    ::= { pppLinkIpcpTable 1 }

PppLinkIpcpEntry ::= SEQUENCE {
    pppLinkIpcpIfIndex
    INTEGER,
    pppLinkIpcpIpAddressLocal
    IpAddress,
    pppLinkIpcpIpAddressRemote
    IpAddress
}

pppLinkIpcpIfIndex OBJECT-TYPE
    SYNTAX INTEGER
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION "Interface index"
    ::= { pppLinkIpcpEntry 1 }

pppLinkIpcpIpAddressLocal OBJECT-TYPE
    SYNTAX IpAddress
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION "Local IP address."
    ::= { pppLinkIpcpEntry 2 }

pppLinkIpcpIpAddressRemote OBJECT-TYPE
    SYNTAX IpAddress
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION "Remote IP Address"
    ::= { pppLinkIpcpEntry 3 }

