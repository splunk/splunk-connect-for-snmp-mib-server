	Counter,
	pppLinkBacpCallRespTx
	Counter,
	pppLinkBacpCallRespRx
	Counter,
	pppLinkBacpCallbackReqTx
	Counter,
	pppLinkBacpCallbackReqRx
	Counter,
	pppLinkBacpCallbackRespTx
	Counter,
	pppLinkBacpCallbackRespRx
	Counter,
	pppLinkBacpLinkDropReqTx
	Counter,
	pppLinkBacpLinkDropReqRx
	Counter,
	pppLinkBacpLinkDropRespTx
	Counter,
	pppLinkBacpLinkDropRespRx
	Counter,
	pppLinkBacpCallStatusIndTx
	Counter,
	pppLinkBacpCallStatusIndRx
	Counter,
	pppLinkBacpCallStatusRespTx
	Counter,
	pppLinkBacpCallStatusRespRx
	Counter,
	pppLinkBacpNoFreeSlavesFound
	Counter,
	pppLinkBacpMaxBundleSizeRejects
	Counter,
	pppLinkBacpChannelLackDisconnects
	Counter,
	pppLinkBacpOldProtocolIdsUsed
	INTEGER
}

