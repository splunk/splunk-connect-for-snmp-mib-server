        Counter,
	pppLinkMlMasterLinkAddThreshold
	Counter,
	pppLinkMlMasterSamplingTime
	Counter,
	pppLinkMlMasterSlavesWithBOD
	INTEGER,
	pppLinkMlMasterMeasuringMode
	INTEGER
}

