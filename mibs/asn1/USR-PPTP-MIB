USR-PPTP-MIB DEFINITIONS ::= BEGIN
	 IMPORTS
         Counter32, Integer32, IpAddress, 
         OBJECT-IDENTITY, OBJECT-TYPE, MODULE-IDENTITY
                FROM SNMPv2-SMI
    	 RowStatus, TruthValue, TEXTUAL-CONVENTION, DisplayString
                FROM SNMPv2-TC
         common, UsrConfigStatus, UsrDisplayString
         	FROM USR-DEFINITIONS-MIB;
 usrPptp MODULE-IDENTITY
         LAST-UPDATED "200204011700Z"
         ORGANIZATION "CommWorks Corporation, a 3Com Company"
         CONTACT-INFO "Customer Support"
         DESCRIPTION
                 "The commWorks enterprise defined MIB objects
                   for point-to-point tunneling."
         ::= { common 56 }
   MilliSeconds ::=  TEXTUAL-CONVENTION
          DISPLAY-HINT    "d-3"
          STATUS          current
          DESCRIPTION     
              " A period of time measured in units of .001 of seconds
              when used in conjunction with the DISPLAY-HINT will
              show seconds and fractions of second with a resolution
              of .001 of a second."
       SYNTAX          INTEGER (0..2147483646)      
   usrPPTPGlobal  OBJECT-IDENTITY
           STATUS          current
           DESCRIPTION
              "The root of the PPTP global Object-Identifiers."
           ::= { usrPptp 1 }
   usrPPTPConfigTable OBJECT-TYPE
           SYNTAX          SEQUENCE OF UsrPPTPConfigEntry
           MAX-ACCESS      not-accessible
           STATUS          current
           DESCRIPTION
              ""
           ::= { usrPPTPGlobal 1 }
   usrPptpConfigEntry OBJECT-TYPE
           SYNTAX          UsrPPTPConfigEntry
           MAX-ACCESS      not-accessible
           STATUS          current
           DESCRIPTION
              "."
           INDEX           { usrPptpConfigIndex }
           ::= { usrPPTPConfigTable 1 }
   UsrPPTPConfigEntry ::= SEQUENCE {
           usrPptpConfigIndex
                   Integer32,
           usrPptpMaxNumberOfSessions
                   Integer32,
           usrPptpMaxNumberOfTunnels
                   Integer32,
           usrPptpControlTunnelSeekDescriptors
                   Integer32,
           usrPptpUseDataTunnelFlowControl
                   TruthValue,
           usrPptpDataTunnelDelayedAckTimeout
                   MilliSeconds,
           usrPptpDataTunnelReassemblyTimeout
                   MilliSeconds,
           usrPptpDataChannelRecvPacketWindow
                   Integer32,
           usrPptpHelloInactivityHoldTime
                   Integer32,
           usrPptpEchoReplyTimeoutSeconds
                   Integer32,
           usrPptpLoggingLevel
                   INTEGER,
           usrPptpLoadBalancing
                   TruthValue,
           usrPptpDialOut
                    UsrDisplayString,
    	   usrPptpPnsFlag 
                    INTEGER 
       }
   usrPptpConfigIndex     OBJECT-TYPE
           SYNTAX          Integer32
           MAX-ACCESS      read-only
           STATUS          current
           DESCRIPTION
              " A unique value for each pptp protocol stack. "
           ::= { usrPptpConfigEntry 1 }
   usrPptpMaxNumberOfSessions OBJECT-TYPE
           SYNTAX          Integer32
           MAX-ACCESS      read-write
           STATUS          current
           DESCRIPTION
               "Defines the number of simultaneous active sessions
               that the PPTP stack will support."
           DEFVAL          { 0 }
              --CONFIGURABLE
           ::= { usrPptpConfigEntry 2 }
   usrPptpMaxNumberOfTunnels OBJECT-TYPE
           SYNTAX          Integer32
           MAX-ACCESS      read-write
           STATUS          current
           DESCRIPTION
               "Defines the number of simultaneous active tunnels
               that the PPTP stack will support. Remember that sessions
               are multiplexed within a single tunnel, therefore this
               object contains the number of active PPTP tunnel
               sessions which are to be supported per tunnel."
           DEFVAL          { 0 }
               --CONFIGURABLE
           ::= { usrPptpConfigEntry 3 }
   usrPptpControlTunnelSeekDescriptors  OBJECT-TYPE
           SYNTAX          Integer32
           MAX-ACCESS      read-write
           STATUS          current
           DESCRIPTION
              "Defines the number of tunnel terminators which the PPTP
              stack is to be able to simultaneous connect to. Once a
              tunnel is in the established state, the seek descriptor
              is no longer used."
           DEFVAL          { 8 }
              --CONFIGURABLE
           ::= { usrPptpConfigEntry 4 }
    usrPptpUseDataTunnelFlowControl  OBJECT-TYPE
           SYNTAX          TruthValue
           MAX-ACCESS      read-write
           STATUS          current
           DESCRIPTION
              "This object determines whether or not the PPTP stack is
              to use flow control on the data tunnel."
           DEFVAL          { false }
              --CONFIGURABLE
           ::= { usrPptpConfigEntry 5 }
    usrPptpDataTunnelDelayedAckTimeout  OBJECT-TYPE
           SYNTAX          MilliSeconds
           MAX-ACCESS      read-write
           STATUS          current
           DESCRIPTION
              "This object defines the number of milliseconds which the
              PPTP stack will wait  to send and ACK to it's peer in the
              case where there are no data or control packets to piggy-
              back the ACK to.  The default value for this object is 0
              which results in immediate acknowledgment when there are
              no data or control packets pending."
             DEFVAL          { 0 }
            --CONFIGURABLE
           ::= { usrPptpConfigEntry 6 }
     usrPptpDataTunnelReassemblyTimeout  OBJECT-TYPE
           SYNTAX          MilliSeconds
           MAX-ACCESS      read-write
           STATUS          current
           DESCRIPTION
              "This object defines the number of milliseconds that the
              PPTP stack will use to determine the window to use before
              reassembling out of order packets. A low value increases
              the chance of out-of-sequence packets to be lost (which
              MAY causes the PPP decompression engine to reset), a high
              value increases the time period where the PPTP stack
              processes packets which were received out of order
              (especially in the case of a packet which was lost within
              the network).  The default value for this object is zero
              (0) which may result in all out of sequence packets being
              lost."
           DEFVAL          { 0 }
           --CONFIGURABLE
           ::= { usrPptpConfigEntry 7 }
   usrPptpDataChannelRecvPacketWindow  OBJECT-TYPE
           SYNTAX          Integer32
           MAX-ACCESS      read-write
           STATUS          current
           DESCRIPTION
              "This object defines the Data Channel receive window
              size which is transmitted to the PPTP peers."
           DEFVAL          { 7 }
           --CONFIGURABLE
           ::= { usrPptpConfigEntry 8 }
    usrPptpHelloInactivityHoldTime    OBJECT-TYPE
           SYNTAX          Integer32
           MAX-ACCESS      read-write
           STATUS          current
           DESCRIPTION
              "This object defines the length of the inactivity timer
              in seconds which causes the PPTP stack to transmit a
              keep-alive packet (known as Hello)."
           DEFVAL          { 0 }
            --CONFIGURABLE
           ::= { usrPptpConfigEntry 9 }
    usrPptpEchoReplyTimeoutSeconds  OBJECT-TYPE
           SYNTAX          Integer32
           MAX-ACCESS      read-write
           STATUS          current
           DESCRIPTION
              "This object defines the number of seconds which the PPTP
              stack must wait until a time-out occurs in receiving a
              response to the Hello  message."
           DEFVAL          { 0 }
              --CONFIGURABLE
           ::= { usrPptpConfigEntry 10 }
     usrPptpLoggingLevel  OBJECT-TYPE
           SYNTAX          INTEGER {
               logDisabled(1),
               logControlPackets(2),
               logControlAndDataPacketHeaders(3),
               logControlAndDataPackets(4)
           }
           MAX-ACCESS      read-write
           STATUS          current
           DESCRIPTION
              "This object defines the logging level which the PPTP
              protocol stack will use."
           DEFVAL          { logDisabled }
                --CONFIGURABLE
           ::= { usrPptpConfigEntry 11 }
   usrPptpLoadBalancing  OBJECT-TYPE
           SYNTAX          TruthValue
           MAX-ACCESS      read-write
           STATUS          current
           DESCRIPTION
              "Defines If Load Balancing is enabled or not."
           DEFVAL          { true }
             --CONFIGURABLE 
           ::= { usrPptpConfigEntry 12 }
usrPptpDialOut OBJECT-TYPE
        SYNTAX  UsrDisplayString (SIZE(0..32))
        MAX-ACCESS  read-write
        STATUS  current
        DESCRIPTION
                "This variable is set through the CLI when the user
                 manually issues a PPTP dial-out."
        ::= { usrPptpConfigEntry 13 }
 usrPptpPnsFlag OBJECT-TYPE
      SYNTAX   INTEGER { enabled(1), disabled(2) }
      MAX-ACCESS   read-write 
      STATUS   current
      DESCRIPTION
             "Setting this object to a value of enabled(1) will enable      
              the PPTP pns enable feature. Setting this object to
              disabled(2) will disable the PPTP PNS feature."
     --CONFIGURABLE 
 ::= { usrPptpConfigEntry 14 }
usrPPTPStatsTable OBJECT-TYPE
           SYNTAX          SEQUENCE OF UsrPPTPStatsEntry 
           MAX-ACCESS      not-accessible
           STATUS          current
           DESCRIPTION
              ""
           ::= { usrPPTPGlobal 2 }
usrPptpStatsEntry OBJECT-TYPE
           SYNTAX          UsrPPTPStatsEntry 
           MAX-ACCESS      not-accessible
           STATUS          current
           DESCRIPTION
              "."
           INDEX           { usrPptpStatsIndex }
           ::= {usrPPTPStatsTable 1 }
UsrPPTPStatsEntry ::= SEQUENCE {
           usrPptpStatsIndex
                   Integer32,
           usrPptpNumActiveTunnels
                   Integer32,
           usrPptpStatsNumActiveSessions
                   Integer32,
           usrPptpStatsMalformedPackets
                   Counter32,
           usrPptpStatsControlTunnelRecvPackets
                   Counter32,
           usrPptpStatsProcessedControlTunnelRecvPackets
                   Counter32,
           usrPptpStatsControlTunnelSendPackets
                   Counter32,
           usrPptpStatsDataTunnelRecvPackets
                   Counter32,
           usrPptpStatsDataTunnelWithdataRecvPackets
                   Counter32,
           usrPptpStatsDataTunnelWithoutdataRecvPackets
                   Counter32,
           usrPptpStatsProcessedDataTunnelRecvPackets
                   Counter32,
           usrPptpStatsInSequenceDataTunnelRecvPackets
                   Counter32,
           usrPptpStatsOutSequenceDataTunnelRecvPackets
                   Counter32,
           usrPptpStatsInOrderDataTunnelRecvPackets
                   Counter32,
           usrPptpStatsOutOfOrderDataTunnelRecvPackets
                   Counter32,
           usrPptpStatsFlowDiscardedDataTunnelRecvPackets
                   Counter32,
           usrPptpStatsOutOfOrderDiscardedDataTunnelRecvPackets
                   Counter32,
           usrPptpStatsDataTunnelSendPackets
                   Counter32,
           usrPptpStatsDataTunnelWithdataSendPackets
                   Counter32,
           usrPptpStatsDataTunnelWithoutdataSendPackets
                   Counter32,
           usrPptpStatsDataTunnelFlowControlTimeouts
                   Counter32,
           usrPptpStatsLocalDataTunnelFlowControlEnables
                   Counter32,
           usrPptpStatsRemoteDataTunnelFlowControlEnables
                   Counter32,
           usrPptpStatsDataTunnelReassemblyTimeouts
                   Counter32 }
 usrPptpStatsIndex          OBJECT-TYPE
           SYNTAX          Integer32
           MAX-ACCESS      read-only
           STATUS          current
           DESCRIPTION
              " Should be the same value as pptpConfigIndex to
              reference the same logical pptp protocol stack. "
           ::= { usrPptpStatsEntry 1 }
 usrPptpNumActiveTunnels  OBJECT-TYPE
           SYNTAX          Integer32
           MAX-ACCESS      read-only
           STATUS          current
           DESCRIPTION
              "This object returns the number of PPTP tunnels
              that are currently active. "
           ::= { usrPptpStatsEntry 2 }
 usrPptpStatsNumActiveSessions  OBJECT-TYPE
           SYNTAX          Integer32
           MAX-ACCESS      read-only
           STATUS          current
           DESCRIPTION
              "This object returns the number of PPTP sessions
              that are currently active. "
           ::= { usrPptpStatsEntry 3 }
 usrPptpStatsMalformedPackets    OBJECT-TYPE
           SYNTAX          Counter32
           MAX-ACCESS      read-only
           STATUS          current
           DESCRIPTION
              "This object returns a count of the number of malformed
              packets received by this PPTP stack since it was last
              initialized.   "
           ::= { usrPptpStatsEntry 4 }
  usrPptpStatsControlTunnelRecvPackets   OBJECT-TYPE
           SYNTAX          Counter32
           MAX-ACCESS      read-only
           STATUS          current
           DESCRIPTION
              " This object returns a count of the number of control
              packets received by this PPTP stack since it was last
              initialized."
           ::= { usrPptpStatsEntry 5 }
 usrPptpStatsProcessedControlTunnelRecvPackets   OBJECT-TYPE
           SYNTAX          Counter32
           MAX-ACCESS      read-only
           STATUS          current
           DESCRIPTION
              " This object returns a count of the number of received
              control packets that were processed by this PPTP stack
              since it was last initialized."
           ::= { usrPptpStatsEntry 6 }
 usrPptpStatsControlTunnelSendPackets   OBJECT-TYPE
           SYNTAX          Counter32
           MAX-ACCESS      read-only
           STATUS          current
           DESCRIPTION
              " This object returns a count of the number of control
              packets that were transmitted by this PPTP stack since
              it was last initialized. "
           ::= { usrPptpStatsEntry 7 }
  usrPptpStatsDataTunnelRecvPackets     OBJECT-TYPE
           SYNTAX          Counter32
           MAX-ACCESS      read-only
           STATUS          current
           DESCRIPTION
              " This object returns a count of the number of data
              packets that were received by this PPTP stack since
              it was last initialized. "
           ::= { usrPptpStatsEntry 8 }
   usrPptpStatsDataTunnelWithdataRecvPackets     OBJECT-TYPE
           SYNTAX          Counter32
           MAX-ACCESS      read-only
           STATUS          current
           DESCRIPTION
              " This object returns a count of the number of data
              packets that were received with data by this PPTP stack
              since it was last initialized. "
           ::= { usrPptpStatsEntry 9 }
   usrPptpStatsDataTunnelWithoutdataRecvPackets    OBJECT-TYPE
           SYNTAX          Counter32
           MAX-ACCESS      read-only
           STATUS          current
           DESCRIPTION
              " This object returns a count of the number of Zero
              Length data packets that were received by this PPTP
              stack since it was last initialized. "
           ::= { usrPptpStatsEntry 10 }
    usrPptpStatsProcessedDataTunnelRecvPackets  OBJECT-TYPE
           SYNTAX          Counter32
           MAX-ACCESS      read-only
           STATUS          current
           DESCRIPTION
              " This object returns a count of the number of data
              packets that were received and processed by this PPTP
              stack since it was last initialized."
           ::= { usrPptpStatsEntry 11 }
    usrPptpStatsInSequenceDataTunnelRecvPackets    OBJECT-TYPE
           SYNTAX          Counter32
           MAX-ACCESS      read-only
           STATUS          current
           DESCRIPTION
              " This object returns a count of the number of data
              packets that were received in sequence by this PPTP
              stack since it was last initialized. "
           ::= { usrPptpStatsEntry 12 }
     usrPptpStatsOutSequenceDataTunnelRecvPackets    OBJECT-TYPE
           SYNTAX          Counter32
           MAX-ACCESS      read-only
           STATUS          current
           DESCRIPTION
              " This object returns a count of the number of data
              packets that were received out of sequence by this
              PPTP stack since it was last initialized."
           ::= { usrPptpStatsEntry 13 }
     usrPptpStatsInOrderDataTunnelRecvPackets   OBJECT-TYPE
           SYNTAX          Counter32
           MAX-ACCESS      read-only
           STATUS          current
           DESCRIPTION
              " This object returns a count of the number of data
              packets that were received in order by this PPTP
              stack since it was last initialized. "
           ::= { usrPptpStatsEntry 14 }
       usrPptpStatsOutOfOrderDataTunnelRecvPackets     OBJECT-TYPE
           SYNTAX          Counter32
           MAX-ACCESS      read-only
           STATUS          current
           DESCRIPTION
              " This object returns a count of the number of data
              packets that were received out of order by this PPTP
              stack since it was last initialized. "
           ::= { usrPptpStatsEntry 15 }
         usrPptpStatsFlowDiscardedDataTunnelRecvPackets    OBJECT-TYPE
           SYNTAX          Counter32
           MAX-ACCESS      read-only
           STATUS          current
           DESCRIPTION
              " This object returns a count of the number of data
              packets that were received and discarded due to flow
              control by this PPTP stack since it was last initialized."
           ::= { usrPptpStatsEntry 16 }
         usrPptpStatsOutOfOrderDiscardedDataTunnelRecvPackets   OBJECT-TYPE
           SYNTAX          Counter32
           MAX-ACCESS      read-only
           STATUS          current
           DESCRIPTION
              " This object returns a count of the number of data
              packets that were received and discarded due to
              ordering by this PPTP stack since it was last initialized."
           ::= { usrPptpStatsEntry 17 }
        usrPptpStatsDataTunnelSendPackets    OBJECT-TYPE
           SYNTAX          Counter32
           MAX-ACCESS      read-only
           STATUS          current
           DESCRIPTION
              " This object returns a count of the number of data
              packets that were transmitted on the data Tunnel by
              this PPTP stack since it was last initialized."
           ::= { usrPptpStatsEntry 18 }
      usrPptpStatsDataTunnelWithdataSendPackets    OBJECT-TYPE
           SYNTAX          Counter32
           MAX-ACCESS      read-only
           STATUS          current
           DESCRIPTION
              " This object returns a count of the number of data
              packets that were transmitted with data by this PPTP
              stack since it was last initialized. "
           ::= { usrPptpStatsEntry 19 }
       usrPptpStatsDataTunnelWithoutdataSendPackets    OBJECT-TYPE
           SYNTAX          Counter32
           MAX-ACCESS      read-only
           STATUS          current
           DESCRIPTION
              " This object returns a count of the number of Zero
              length data packets that were transmitted by this PPTP
              stack since it was last initialized."
           ::= { usrPptpStatsEntry 20 }
       usrPptpStatsDataTunnelFlowControlTimeouts      OBJECT-TYPE
           SYNTAX          Counter32
           MAX-ACCESS      read-only
           STATUS          current
           DESCRIPTION
              " This object returns a count of the number of data
              Tunnel flow control time-outs seen by this PPTP stack
              since it was last initialized.  "
           ::= { usrPptpStatsEntry 21 }
         usrPptpStatsLocalDataTunnelFlowControlEnables    OBJECT-TYPE
