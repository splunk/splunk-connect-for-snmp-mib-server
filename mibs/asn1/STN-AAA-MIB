    stnNamedAddressPoolEntry  OBJECT-TYPE
        SYNTAX                    StnNamedAddressPoolEntry
        MAX-ACCESS                not-accessible
        STATUS                    current
        DESCRIPTION               "A named address pool entry. An entry in 
                                  this table corresponds to a single named
                                  address pool"
        INDEX           { stnNamedAddressPoolIndex }
        ::= { stnNamedAddressPoolTable 1 }
                
        StnNamedAddressPoolEntry ::=
            SEQUENCE { 
                    stnNamedAddressPoolIndex              Integer32,
                    stnNamedAddressPoolName               DisplayString
        }

    stnNamedAddressPoolIndex     OBJECT-TYPE
        SYNTAX                   Integer32
        MAX-ACCESS               read-only
        STATUS                   current
        DESCRIPTION              "The configuration instance of this 
                                 Named Address Pool entry."
        ::= { stnNamedAddressPoolEntry 1 }

    stnNamedAddressPoolName  OBJECT-TYPE
        SYNTAX                   DisplayString (SIZE(0..32))
        MAX-ACCESS               read-only
        STATUS                   current
        DESCRIPTION              "The name of this named address pool table 
                                 entry"
        ::= { stnNamedAddressPoolEntry 2 }

    --
    -- STN Named Address Pool Address Range Table
    --
    stnNamedAddressPoolRangeTable  OBJECT-TYPE
        SYNTAX                    SEQUENCE OF StnNamedAddressPoolRangeEntry
        MAX-ACCESS                not-accessible
        STATUS                    current
        DESCRIPTION               "The Named Address Pool Address Range Table."
        ::= { stnAAAConfig 17 }

    stnNamedAddressPoolRangeEntry  OBJECT-TYPE
        SYNTAX                    StnNamedAddressPoolRangeEntry
        MAX-ACCESS                not-accessible
        STATUS                    current
        DESCRIPTION               "A named address pool range entry. An entry
                                  in this table corresponds to a single
                                  address range"
        INDEX           { stnNamedAddressPoolIndex,
                          stnNamedAddressPoolRangeIndex }
        ::= { stnNamedAddressPoolRangeTable 1 }
                
        StnNamedAddressPoolRangeEntry ::=
            SEQUENCE { 
                    stnNamedAddressPoolRangeIndex      Integer32,
                    stnNamedAddressPoolStartIpAddress  IpAddress,
                    stnNamedAddressPoolEndIpAddress    IpAddress
        }

    stnNamedAddressPoolRangeIndex  OBJECT-TYPE
        SYNTAX                   Integer32
        MAX-ACCESS               read-only
        STATUS                   current
        DESCRIPTION              "The index into the array of address ranges"
        ::= { stnNamedAddressPoolRangeEntry 1 }

    stnNamedAddressPoolStartIpAddress  OBJECT-TYPE
        SYNTAX                   IpAddress 
        MAX-ACCESS               read-only
        STATUS                   current
        DESCRIPTION              "The starting IP address of the address range"
        ::= { stnNamedAddressPoolRangeEntry 2 }

    stnNamedAddressPoolEndIpAddress  OBJECT-TYPE
        SYNTAX                           IpAddress 
        MAX-ACCESS                       read-only
        STATUS                           current
        DESCRIPTION                      "The ending IP address of the address range"
        ::= { stnNamedAddressPoolRangeEntry 3 }


    --
    -- STN Session notification support objects.
    --
    stnNotificationRadiusAuthServerIndex OBJECT-TYPE
        SYNTAX          Integer32                              
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION
              "The priority of this Radius authentication server - 
               highest priority is 1 and higher priority servers
               will be contacted first - the priorities must be contiguous"
        ::= { stnAAATrapVars 1 }

    stnNotificationRadiusAccServerIndex  OBJECT-TYPE
        SYNTAX          Integer32                              
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION
              "The priority of this accounting server - higher priority 
               servers will attempt to be contacted before lower priority
               servers - highest priority is 1 - priority indexes must
               be contiguous"
        ::= { stnAAATrapVars 2 }


    --
    -- STN Notifications 
    --
    stnMaxUserSessionLimitExceeded NOTIFICATION-TYPE
        OBJECTS         { stnEngineIndex, stnEngineSlot, stnEngineCpu }
        STATUS          current
        DESCRIPTION     "A stnMaxUserSessionLimitExceeded trap signifies that 
                        the agent entity has detected that the maximum 
                        number of sessions has been exceeded on one of its 
                        engines.
                        The generation of this trap can be controlled by the
                        AAATraps configuration object."
        ::= { stnNotification 20 }

    stnRadiusAuthenMismatch NOTIFICATION-TYPE
        OBJECTS         { stnNotificationRadiusAuthServerIndex, 
                          stnRadiusAuthServerName,
                          stnRadiusAuthServerConfigInstance, 
                          stnRouterIndex }
        STATUS          current
        DESCRIPTION     "A stnRadiusAuthenMismatch trap signifies that 
                        the agent entity has detected that the a 
                        authentication failure (security breach or 
                        retransmission parameter) has been detected on one 
                        of its routers.
                        The generation of this trap can be controlled by the
                        AAATraps configuration object."
        ::= { stnNotification 21 }

    stnPrimaryBillingServerFailure NOTIFICATION-TYPE
        OBJECTS         { stnNotificationRadiusAccServerIndex, 
                          stnRadiusAccServerName,
                          stnRadiusAccServerConfigInstance, 
                          stnRouterIndex }
        STATUS          current
        DESCRIPTION     "A stnPrimaryBillingServerFailure trap signifies that 
                        the agent entity has detected that a problem has
                        occurred with the primary billing server and is 
                        switching to a secondary billing server.
                        The generation of this trap can be controlled by the
                        AAATraps configuration object."
        ::= { stnNotification 22 }

    stnNoAddressesAvailable NOTIFICATION-TYPE
        OBJECTS         { stnRouterIndex }
        STATUS          current
        DESCRIPTION     "A stnNoAddressesAvailable trap signifies that 
                        the agent entity has detected that no addresses
                        are available on the indicated router.
                        The generation of this trap can be controlled by the
                        AAATraps configuration object."
        ::= { stnNotification 23 }

END
