    --
    --      The Default PPP User Group
    --
    --      This group contains objects that can be used to
    --      configure the operational characteristics of 
    --      the PPP default user - the PPP default user is used
    --      when PPP is not authenticating and there is no user name
    --      defined in the no authentication NoAuthenPppUserGroup below.

    stnDefaultUserIpAddress  OBJECT-TYPE
        SYNTAX          IpAddress 
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION     "The IP address for the default PPP user session."
        ::= { stnDefaultPppUserGroup 1 }

    stnDefaultUserIpNetMask  OBJECT-TYPE
        SYNTAX          IpAddress 
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION
            "The IP sub-network mask that the PPP default user will use."
        ::= { stnDefaultPppUserGroup 2 }
        
    stnDefaultUserIpRouting  OBJECT-TYPE
        SYNTAX          INTEGER {
                            none(1),
                            send(2),
                            listen(3),
                            both(4)
                        }
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION     "The routing method for the PPP default user"
        DEFVAL          { none }
        ::= { stnDefaultPppUserGroup 3 }

    stnDefaultUserMTU  OBJECT-TYPE
        SYNTAX          INTEGER (64..65532)
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION
            "The maximum transmission unit used by the framed protocol
            that the PPP default user is configured for."
        DEFVAL          { 1500 }
        ::= { stnDefaultPppUserGroup 4 }

    stnDefaultUserIpCompression  OBJECT-TYPE
        SYNTAX          INTEGER {
                            none(1),
                            vj(2)       -- Van Jacobson
                        }
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION     "The compression type for the PPP default user"
        DEFVAL          { none }
        ::= { stnDefaultPppUserGroup 5 }

    stnDefaultUserTimeOut  OBJECT-TYPE
        SYNTAX          Integer32
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION
            "The total amount of time the default PPP user should be allowed 
            to stay connected. A value of 0 indicates no timeout"
        DEFVAL          { 0 }
        ::= { stnDefaultPppUserGroup 6 }

    stnDefaultUserIdleTime  OBJECT-TYPE
        SYNTAX          Integer32
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION
            "The total amount of time the PPP default user should be allowed 
            to stay connected when idle. A value of 0 indicates no timeout" 
        DEFVAL          { 0 }
        ::= { stnDefaultPppUserGroup 7 }

    stnDefaultUserTunnelType  OBJECT-TYPE
        SYNTAX          INTEGER {
                            none(1),
                            pptp(2),
                            l2tp(4)
                        }
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION
            "The type of tunnel that should be used for compulsory tunneling 
            for the PPP default user - if none is indicated then PPP 
            termination with no tunneling occurs"
        DEFVAL          { none }
        ::= { stnDefaultPppUserGroup 8 }

    stnDefaultUserTunnelServerEndPoint  OBJECT-TYPE
        SYNTAX          DisplayString (SIZE(0..32))
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION
            "The name of the tunnel server endpoint for the PPP default user 
