        MANDATORY-GROUPS {
            tmnxMsdpGlobalGroup,
            tmnxMsdpPeerGroup,
            tmnxMsdpPeerGroupGroup,
            tmnxMsdpSourceGroup,
            tmnxMsdpSrcActiveGroup,
            tmnxMsdpNotificationGroup
        }
    ::= { tmnxMsdpCompliances 1 }

