                 INTEGER,
             ncmCircuitInfoBackPlaneBusID
                 INTEGER,
             ncmCircuitInfoAssoCircName
                 DisplayString,
             ncmCircuitInfoAction
                 INTEGER,
	     ncmAddEditActDeactCircuitStatus
	   	 INTEGER,
	     ncmDeleteCircuitStatus
	   	 INTEGER,
	     ncmGetDetailCircuitStatus
	   	 INTEGER
 
                }
 
 
