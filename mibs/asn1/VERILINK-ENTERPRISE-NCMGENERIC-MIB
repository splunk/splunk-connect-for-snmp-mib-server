                 INTEGER,
             ncmCircuitInfoSrcPortSite
                 INTEGER,
             ncmCircuitInfoSrcCardEquipID
                 INTEGER,
             ncmCircuitInfoSrcPortID
                 INTEGER,
             ncmCircuitInfoSrcTimeslotmap
                 INTEGER,
             ncmCircuitInfoDstPortSite
                 INTEGER,
             ncmCircuitInfoDstCardEquipID
                 INTEGER,
             ncmCircuitInfoDstPortID
                 INTEGER,
