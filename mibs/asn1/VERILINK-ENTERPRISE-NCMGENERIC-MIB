	VERILINK-ENTERPRISE-NCMGENERIC-MIB

	-- General mibs to manage Quad T1, ACE E1/T1 and NCM-ISDN products


	-- Created:  Latha Ganapathy on 15 Feb 1996.
	-- set up MIB-wide defaults
	-- all functions can use %n for object name and
	-- %t for object type, %p for parent name,
	-- %d for name of default-bearing node and %% for %

	  DEFINITIONS ::= BEGIN

     IMPORTS
        Gauge
            FROM RFC1155-SMI
        DisplayString
             FROM RFC1213-MIB
        OBJECT-TYPE
             FROM RFC-1212
        ncm-generic
             FROM VERILINK-ENTERPRISE-NCMALARM-MIB;


     --  This MIB module uses the extended OBJECT-TYPE macro as
     --  defined in RFC 1212.
     --  This MIB module uses the TRAP-TYPE macro as
     --  defined in RFC 1215.
     --  enterprises{iso(1).org(3).dod(6).intrnet(1).pvt(4).entrprs(1).
     --                                             		vlnk(321).as2000(1).x }

 	 -- verilink                    OBJECT IDENTIFIER ::= { enterprises 321 }
 	 -- as2000                      OBJECT IDENTIFIER ::= { verilink 1 }
 	 -- ncm-generic                 OBJECT IDENTIFIER ::= { as2000 3012 }                            	
	   
 -- 	MIB Groups


     -- The mibs consists of following groups:
     --    ncmNodeCurrTable
     --    ncmNodeInfoTable
     --    ncmShelfInfo
     --    ncmCardInfo
     --    ncmShelfSyncncmSource
     --    ncmCardSyncncmSource
     --    ncmIDProm
     --    ncmE1PortConfig
     --    ncmE1PortStatus
     --    ncmE1Current
     --    ncmE1Interval
     --    ncmE1Total
     --    ncmE1PrevTotal
     --    ncmE1PerformanceSnapShot
     --    ncmE1PortLatchedStatus
     --    ncmE1ThesholdStatus
     --    ncmT1Config
     --    ncmT1PortStatus
     --    ncmT1Current
     --    ncmT1Interval
     --    ncmT1Total
     --    ncmT1PrevTotal
     --    ncmT1PreformanceSnapShot
     --    ncmAdvancedT1PortConfig
     --    ncmT1PortLatchedStatus
     --    ncmT1ThresholdStatus
     --    ncmDataPortConfig
     --    ncmDataPortStatus
     --    ncmAlarmConfig
     --    ncmAlarmCurrentMsg
     --    ncmAlarmMessage
     --    ncmEnhancedAlarmConfig
     --	   ncmLoopback
     --    ncmTestPattern
     --    ncmCircuitInfo
     --    ncmPortTimeslotAllocGrp
     --    ncmDialBackupGrp
     --    ncmClearCircuitGrp
     --    ncmGetMsgNum
     --	   ncmCircuitListccb
     --    ncmEquipmentID
     --    ncmE1AlarmThresholdConfig
     --    ncmE1AlarmThresholdStatus
     --    ncmAlarmThresholdConfig
     --    ncmAlarmThresholdStatus
     --    ncmShelfSyncSourceSeg

--      ncm Node Current Group
         
        ncmNodeCurrTable OBJECT-TYPE
             SYNTAX   SEQUENCE OF NcmNodeCurrEntry
             ACCESS   not-accessible
             STATUS   mandatory
	     --DEFAULT test-function    node-test
             --DEFAULT next-function    node-next

 
             ::= { ncm-generic  1001 }

                                
        ncmNodeCurrEntry OBJECT-TYPE
             SYNTAX   NcmNodeCurrEntry
             ACCESS   not-accessible
             STATUS   mandatory
             DESCRIPTION
                "An entry in the ncmNode Current table
                  for selecting the active node in operation."
             INDEX   { ncmNodeCurrIndex }
            ::= { ncmNodeCurrTable 1 }
      
      
        NcmNodeCurrEntry ::=
             SEQUENCE {
             ncmNodeCurrIndex
                 INTEGER,
             ncmNodeCurrAddress
                DisplayString,
             ncmActiveNodeStatus
                 INTEGER
                 
             }           

               
        ncmNodeCurrIndex OBJECT-TYPE
             SYNTAX  INTEGER (1..'7fffffff'h)
             ACCESS  read-only
             STATUS  mandatory
             DESCRIPTION
                "This variable indicates the node id
                 value inside the array containing all called nodes."
               ::= { ncmNodeCurrEntry 1 }
                
                                       
        ncmNodeCurrAddress  OBJECT-TYPE
             SYNTAX  DisplayString
             ACCESS  read-only
             STATUS  mandatory
             DESCRIPTION
             "This variable indicates the ncmNode Current Address."
               ::= { ncmNodeCurrEntry 2 }
 
 
        ncmActiveNodeStatus OBJECT-TYPE
             SYNTAX  INTEGER {
 
                     inactive(1),
                     active(2)
             }
             ACCESS  read-write
             STATUS  mandatory
             DESCRIPTION
                "This variable indicates what is the current node of the
                 network that is being managed; when this object is set,
                 another node table will be activated and treat as active."
               ::= { ncmNodeCurrEntry 3 }


--	ncmNode Group
 	 
        ncmNodeInfoTable OBJECT-TYPE
             SYNTAX   SEQUENCE OF NcmNodeInfoEntry
             ACCESS   not-accessible
             STATUS   mandatory
	     --DEFAULT test-function    node-test
             --DEFAULT next-function    node-next


             ::= { ncm-generic  1002 }
 
 
 
        ncmNodeInfoEntry OBJECT-TYPE
             SYNTAX   NcmNodeInfoEntry
             ACCESS   not-accessible
             STATUS   mandatory
             DESCRIPTION
                "An entry in the ncmNode Info table."
             INDEX   { ncmNodeIndex }
            ::= { ncmNodeInfoTable 1 }
 
 
        NcmNodeInfoEntry ::=
             SEQUENCE {
             ncmNodeIndex
                 INTEGER,
	     ncmNodeAddress  
	        DisplayString, 
	     ncmTime       
	        DisplayString,
	     ncmDate       
	        DisplayString,
             ncmNodeShelf
                 INTEGER,
             ncmNodeID
                 INTEGER,
	     ncmControllerEqID    
                 INTEGER
                }

 
             ncmNodeIndex OBJECT-TYPE
             SYNTAX  INTEGER (1..'7fffffff'h)
             ACCESS  read-only
             STATUS  mandatory
             DESCRIPTION
                "This variable indicates the node id
	         value of the node."  
               ::= { ncmNodeInfoEntry 1 }


	     ncmNodeAddress  OBJECT-TYPE
	     SYNTAX  DisplayString 
	    -- ACCESS  read-write
	     ACCESS  read-only
	     STATUS  mandatory
	     DESCRIPTION
	     "This variable indicates the ncmNode Address."
               ::= { ncmNodeInfoEntry 2 }


	     ncmTime       OBJECT-TYPE
	      SYNTAX  DisplayString
	      ACCESS  read-write
	      STATUS  mandatory
	      DESCRIPTION
	      "The time (in the hh:mm:sec) format for the card."
               ::= { ncmNodeInfoEntry 3 }


	     ncmDate           OBJECT-TYPE
	     SYNTAX  DisplayString (SIZE (0..255))
	     ACCESS  read-write
	     STATUS  mandatory
	     DESCRIPTION
	     "The date (in mm:dd:yy) format for the card."
               ::= { ncmNodeInfoEntry 4 }

		
	     ncmNodeShelf    OBJECT-TYPE
	     	SYNTAX  INTEGER
	     ACCESS  read-only
	     STATUS  mandatory
	     DESCRIPTION
	     "The variable indicates the bit mapping of shelf number
 	     associated with the node the bit 1 if equipped and 0 if not
                 equipped. i.e. ncmShelf 0 = bit 0"
               ::= { ncmNodeInfoEntry 5 }


	   ncmNodeID    OBJECT-TYPE
	     SYNTAX  INTEGER
	     ACCESS  read-only
	     STATUS  mandatory
	     DESCRIPTION
	     "The variable indicates the node id that uniquely identifies
              a node."
               ::= { ncmNodeInfoEntry 6 }


	   ncmControllerEqID    OBJECT-TYPE
             SYNTAX  INTEGER
             ACCESS  read-only
             STATUS  mandatory
             DESCRIPTION
             "The variable indicates the Equipment Id of the Controller."
               ::= { ncmNodeInfoEntry 7 }




	--      ncm Shelf Info group
 
 
        ncmShelfInfoTable OBJECT-TYPE
             SYNTAX   SEQUENCE OF NcmShelfInfoEntry
             ACCESS  	not-accessible
             STATUS   	mandatory
	     --DEFAULT 	test-function    	shelf-test
             --DEFAULT 	next-function    	shelf-next

             ::= { ncm-generic  1003 }
 
 
 
        ncmShelfInfoEntry OBJECT-TYPE
             SYNTAX   NcmShelfInfoEntry
             ACCESS   not-accessible
             STATUS   mandatory
             DESCRIPTION
                "An entry in the ncmShelf Info table."
            -- INDEX   { ncmShelfIndex }
             INDEX   { ncmShelfNIDIndex, ncmShelfIndex }
            ::= { ncmShelfInfoTable 1 }
 
 
        NcmShelfInfoEntry ::=
             SEQUENCE {
             ncmShelfNIDIndex
                 INTEGER,
             ncmShelfIndex
                 INTEGER,
             ncmShelfNumber
                INTEGER,
             ncmShelfHardware
                 INTEGER,
             ncmNumberofCards
                 INTEGER,
             ncmListCardsInShelf
                DisplayString
                }
 
        ncmShelfNIDIndex OBJECT-TYPE
             SYNTAX  INTEGER (1..'7fffffff'h)
             ACCESS  read-only
             STATUS  mandatory
             DESCRIPTION
                "This variable indicates the node id
	         value of the node."  
               ::= { ncmShelfInfoEntry 1 }


 
        ncmShelfIndex OBJECT-TYPE
             SYNTAX  INTEGER (1..'7fffffff'h)
             ACCESS  read-only
             STATUS  mandatory
             DESCRIPTION
                "This object is the identifier of an Interface
                on a managed device.  If there is an ifEntry
                that is directly associated with this and
                only this DS1 interface, it should have the
                same value as ifIndex.  Otherwise, the value
                exceeds ifNumber, and is a unique identifier
                following this rule: inside interfaces (e.g.,
                equipment side) with even numbers and outside
                interfaces (e.g., network side) with odd
                numbers."
            ::= { ncmShelfInfoEntry 2 }
 



	 ncmShelfNumber  OBJECT-TYPE
             SYNTAX  INTEGER
             ACCESS  read-only
             STATUS  mandatory
             DESCRIPTION
                "This variable indicates the number of the shelf."
             ::= { ncmShelfInfoEntry 3 }
 
 
 
         
        ncmShelfHardware  OBJECT-TYPE
             SYNTAX  INTEGER {
                         uninstalled(1),
                         duallineshelf(2),
                         multilineshelf(3),
                         nb-2200lineshelf(4), 
                         nb-2200-4ilineshelf(10),
                         nb-2200qlineshelf(52), 
                         nb-2200-4iqlineshelf(54)
                     }
             ACCESS  read-only
             STATUS  mandatory
             DESCRIPTION
                "This variable indicates the shelf hardware type."
             ::= { ncmShelfInfoEntry 4 }
 
 
 
        ncmNumberofCards  OBJECT-TYPE
             SYNTAX  INTEGER
             ACCESS  read-only
             STATUS  mandatory
             DESCRIPTION
             "This variable indicates the total number of cards in
              the shelf."
             ::= { ncmShelfInfoEntry 5 }
 
 
 
        ncmListCardsInShelf  OBJECT-TYPE
             SYNTAX  DisplayString
             ACCESS  read-only
             STATUS  mandatory
             DESCRIPTION
                 "This variable indicates the cards present in the shelf.
                  For Example 1,3,5 if the cards 1,3 and 5 are present."
             ::= { ncmShelfInfoEntry 6 }
 



	
 	--      ncm Card Info Group
 
 
 
        ncmCardInfoTable OBJECT-TYPE
             SYNTAX   SEQUENCE OF NcmCardInfoEntry
             ACCESS   not-accessible
             STATUS   mandatory
	     --DEFAULT test-function    	card-test
             --DEFAULT next-function    	card-next


             ::= { ncm-generic  1004 }
 
 
 
        ncmCardInfoEntry OBJECT-TYPE
             SYNTAX   NcmCardInfoEntry
             ACCESS   not-accessible
             STATUS   mandatory
             DESCRIPTION
                "An entry in the ncmCard Info table."
          --  INDEX   { ncmCardIndex }
             INDEX   { ncmCardNIDIndex, ncmCardIndex }
            ::= { ncmCardInfoTable 1 }
 
 
        NcmCardInfoEntry ::=
             SEQUENCE {
             ncmCardNIDIndex
                 INTEGER,
             ncmCardIndex
                 INTEGER,
             ncmFirmwareType
                INTEGER,
             ncmCimType
                INTEGER,
             ncmClearCardInfo 
                INTEGER
                }
 
        ncmCardNIDIndex OBJECT-TYPE
             SYNTAX  INTEGER (1..'7fffffff'h)
             ACCESS  read-only
             STATUS  mandatory
             DESCRIPTION
                "This variable indicates the node id
	         value of the node."  
               ::= { ncmCardInfoEntry 1 }


        ncmCardIndex OBJECT-TYPE
             SYNTAX  INTEGER (1..'7fffffff'h)
             ACCESS  read-only
             STATUS  mandatory
             DESCRIPTION
                "This object is the identifier of an Interface
                on a managed device.  If there is an ifEntry
                that is directly associated with this and
                only this DS1 interface, it should have the
                same value as ifIndex.  Otherwise, the value
                exceeds ifNumber, and is a unique identifier
                following this rule: inside interfaces (e.g.,
                equipment side) with even numbers and outside
                interfaces (e.g., network side) with odd
                numbers."
            ::= { ncmCardInfoEntry 2 }
 

	 ncmFirmwareType OBJECT-TYPE
             SYNTAX  INTEGER {

			 card-sigma-sam(48),
                         card-sigma-sam-1(64),
                         card-sigma-csu(49),
                         card-sigma-dsu(50),
                         card-sigma-ddsu(51),
                         card-sigma-tu(52),
                         card-sigma-smds(53),
                         card-sigma-idc-sam(54),
                         card-sigma-idc(55),
                         card-ct1-sam(56),
                         card-ct1-csu(57),
                         card-ct1-dsu(58),
                         card-ct1-tu(60),
                         card-sigma-dsu-dds(61),
                         card-sigma-dsu-dsu(62),
                         card-sigma-aps(63),
                         card-sigma-vcu(77),
                         e1-ace-firmware(80),
                         t1-ace-firmware(81),
                         quad-T1-ace-firmware(82),
                         quad-E1-ace-firmware(83),
                         imux-tabs-firmware(85),
                         ace-dual-t1-firmware(86),
                         ace-dual-e1-firmware(87),
                         ds3-cdsu-firmware(88),
                         ncm-t1-firmware(96),
			 card-sigma-diu-2144(97),
			 card-sigma-dsu-2131(98),
                         ace-fw-isdn-T1(112),
                         ace-fw-isdn-E1(113),
                         quad-fw-isdn-T1(114),
                         quad-fw-isdn-E1(115),
			 dual-port-ds3(116),
			 m13-2113-firmware(117),
			 imux-2164-firmware(119)
                     }
             ACCESS  read-only
             STATUS  mandatory
             DESCRIPTION
             "This variable indicates the firmware type for cards."
             ::= { ncmCardInfoEntry 3 }
 
 
        ncmCimType OBJECT-TYPE
             SYNTAX  INTEGER {

			 cim-2001-e1(16),
                         cim-2002-e1(17),
                         cim-2003-e1(18),
                         cim-2004-e1(19),
                         cim-2005-e1(20),
                         cim-2006-e1(21),
                         cim-nim-2000(22),
                         cim-2101-t1(32),
                         cim-2102-t1(33),
                         cim-2164(34),
                         cim-2168(35),
                         cim-2162(36),
                         cim-2061(56),
                         cim-2065(37),
                         cim-hssi(48),
                         cim-v35(49),
                         cim-eia530A(50),
                         cim-eia530(51),
                         cim-rs449(52),
                         cim-hsm-2113(62),
                         cim-vcu-4W(64),
                         cim-vcu-2W(65),
                         cim-dim-2435(82),
                         cim-dim-2530(83),
                         cim-dim-2035(88),
                         cim-dim-2449(89),
                         cim-dim-2232(90),
                         cim-uninstall(255)
                     }
             ACCESS  read-only
             STATUS  mandatory
             DESCRIPTION
                "This variable indicates the cim type for the cards."
             ::= { ncmCardInfoEntry 4 }


      	ncmClearCardInfo OBJECT-TYPE
	    SYNTAX INTEGER {
			no(1),
			yes(2)                     
		 }
 
	    ACCESS  read-write
	    STATUS  mandatory
	    DESCRIPTION
	    "This variable indicates the option to clear the ncmCard
               Configuration in EEProm." 
	    ::= { ncmCardInfoEntry  5 }




	--      ncm Shelf Sync ncm Source Table
 

         ncmShelfSyncSourceTable OBJECT-TYPE
             SYNTAX  SEQUENCE OF NcmShelfSyncSourceEntry
             ACCESS  not-accessible
             STATUS  mandatory
	     --DEFAULT test-function    shelf-test
             --DEFAULT next-function    acesync-next

 
            ::= { ncm-generic  1005 }


        ncmShelfSyncSourceEntry OBJECT-TYPE
             SYNTAX NcmShelfSyncSourceEntry
             ACCESS  not-accessible
             STATUS  mandatory
             DESCRIPTION
                "An entry in the ncm Shelf Sync Source Time table."
            -- INDEX   { ncmShelfSyncSourceLineIndex }
            INDEX   { ncmShelfSyncNIDIndex, ncmShelfSyncSourceIndex }
            ::= { ncmShelfSyncSourceTable 1 }


        NcmShelfSyncSourceEntry ::=
         SEQUENCE {
           ncmShelfSyncNIDIndex
                 INTEGER,
           ncmShelfSyncSourceIndex
                 INTEGER,
           ncmSourceOneShelfNum
                INTEGER,
            ncmSourceOneCardNum
                INTEGER,
            ncmSourceOneClockRef
                INTEGER,
            ncmAutoRestore1
                INTEGER,
            ncmSourceTwoShelfNum
                INTEGER,
            ncmSourceTwoCardNum
                INTEGER,
            ncmSourceTwoClockRef
                INTEGER,
            ncmAutoRestore2
                INTEGER,
            ncmSourceThreeShelfNum
                INTEGER,
            ncmSourceThreeCardNum
                INTEGER,
            ncmSourceThreeClockRef
                INTEGER,
            ncmAutoRestore3
                INTEGER,
            ncmShelfCurrentSyncSource
                INTEGER

                }

        ncmShelfSyncNIDIndex OBJECT-TYPE
             SYNTAX  INTEGER (1..'7fffffff'h)
             ACCESS  read-only
             STATUS  mandatory
             DESCRIPTION
                "This variable indicates the node id
	         value of the node."  
            ::= { ncmShelfSyncSourceEntry 1 }


         ncmShelfSyncSourceIndex OBJECT-TYPE
             SYNTAX  INTEGER (1..'7fffffff'h)
             ACCESS  read-only
             STATUS  mandatory
	      DESCRIPTION
                "This object is the identifier of an Interface
                on a managed device.  If there is an ifEntry
                that is directly associated with this and
                only this DS1 interface, it should have the
                same value as ifIndex.  Otherwise, the value
                exceeds ifNumber, and is a unique identifier
                following this rule: inside interfaces (e.g.,
                equipment side) with even numbers and outside
                interfaces (e.g., network side) with odd
                numbers."
            ::= { ncmShelfSyncSourceEntry 2 }
 
 
         
        ncmSourceOneShelfNum OBJECT-TYPE
            SYNTAX INTEGER
            ACCESS read-write
            STATUS mandatory
            DESCRIPTION
                "This variable indicates the source one shelf number."
            ::= { ncmShelfSyncSourceEntry 3 }
 
 
         
         ncmSourceOneCardNum OBJECT-TYPE
            SYNTAX INTEGER
            ACCESS  read-write
            STATUS  mandatory
            DESCRIPTION
                "This variable indicates the source one card number."
            ::= {ncmShelfSyncSourceEntry 4 }
 
 
 
        ncmSourceOneClockRef OBJECT-TYPE
            SYNTAX INTEGER {
                        e1-t1port1(5),
                        e1-t1port2(6),
                        external-timing(7),
                        internal-timing(8),
                        ncmdata-port1(9),
                        ncmdata-port2(10),
                        quad-t1-port3(11),
                        quad-t1-port4(12)
                           }
            ACCESS  read-write
            STATUS  mandatory
         DESCRIPTION
            "This variable indicates the source one clock reference."
            ::= {ncmShelfSyncSourceEntry 5 }
 
 
         ncmAutoRestore1 OBJECT-TYPE
            SYNTAX INTEGER {
                        disable(1),
			enable(2)
                    }
            ACCESS  read-write
            STATUS  mandatory
            DESCRIPTION
            "This variable indicates the source one auto restore."
            ::= {ncmShelfSyncSourceEntry 6 }
 
 
 
         ncmSourceTwoShelfNum OBJECT-TYPE
            SYNTAX INTEGER
            ACCESS  read-write
            STATUS  mandatory
            DESCRIPTION
            "This variable indicates the source two shelf number."
            ::= { ncmShelfSyncSourceEntry 7 }
 
 
         
        ncmSourceTwoCardNum OBJECT-TYPE
            SYNTAX INTEGER
            ACCESS  read-write
            STATUS  mandatory
            DESCRIPTION
            "This variable indicates the source two card number."
            ::= { ncmShelfSyncSourceEntry 8 }
 
         
        ncmSourceTwoClockRef OBJECT-TYPE
            SYNTAX INTEGER {
                        e1-t1port1(5),
                        e1-t1port2(6),
                        external-timing(7),
                        internal-timing(8),
                        ncmdata-port1(9),
                        ncmdata-port2(10),
                        quad-t1-port3(11),
                        quad-t1-port4(12)
                           }
            ACCESS  read-write
            STATUS  mandatory
            DESCRIPTION
            "This variable indicates the source two clock reference."
            ::= { ncmShelfSyncSourceEntry 9 }
 
 
        ncmAutoRestore2 OBJECT-TYPE
            SYNTAX INTEGER {
                        disable(1),
                        enable(2)
                    }
            ACCESS  read-write
            STATUS  mandatory
            DESCRIPTION
                "This variable indicates the source two auto restore."
            ::= { ncmShelfSyncSourceEntry 10 }


	ncmSourceThreeShelfNum OBJECT-TYPE
            SYNTAX INTEGER
            ACCESS  read-write
            STATUS  mandatory
            DESCRIPTION
                "This variable indicates the source three shelf number."
            ::= { ncmShelfSyncSourceEntry 11 }
 
 
        ncmSourceThreeCardNum OBJECT-TYPE
            SYNTAX INTEGER
            ACCESS  read-write
            STATUS  mandatory
            DESCRIPTION
                "This variable indicates the source three card number."
            ::= { ncmShelfSyncSourceEntry 12 }
 
 
        ncmSourceThreeClockRef OBJECT-TYPE
            SYNTAX INTEGER {
                        e1-t1port1(5),
                        e1-t1port2(6),
                        external-timing(7),
                        internal-timing(8),
                        ncmdata-port1(9),
                        ncmdata-port2(10),
                        quad-t1-port3(11),
                        quad-t1-port4(12)
                           }
            ACCESS  read-write
            STATUS  mandatory
            DESCRIPTION
            "This variable indicates the source three clock reference."
            ::= { ncmShelfSyncSourceEntry 13 }
 
 
         ncmAutoRestore3 OBJECT-TYPE
            SYNTAX INTEGER {
                        disable(1),
                        enable(2)
                    }
            ACCESS  read-write
            STATUS  mandatory
            DESCRIPTION
                "This variable indicates the source three auto restore."
            ::= { ncmShelfSyncSourceEntry 14 }
 
 
         ncmShelfCurrentSyncSource OBJECT-TYPE
            SYNTAX INTEGER {
                        source1(1),
                        source2(2),
                        source3(3)
                    }
		ACCESS  read-write
            	STATUS  mandatory
            	DESCRIPTION
                "This variable indicates the current sync source."
            ::= { ncmShelfSyncSourceEntry 15 }



--	ncm Card Sync ncmSource Table


	 ncmCardSyncSourceTable OBJECT-TYPE
	     SYNTAX  SEQUENCE OF NcmCardSyncSourceEntry
	     ACCESS  not-accessible
	     STATUS  mandatory
	     --DEFAULT test-function    card-test
             --DEFAULT next-function  cardsync-next
             --DEFAULT next-function    acesync-next


	    ::= { ncm-generic  1006 }


	ncmCardSyncSourceEntry OBJECT-TYPE
	     SYNTAX NcmCardSyncSourceEntry
	     ACCESS  not-accessible
	     STATUS  mandatory
	     DESCRIPTION
		"An entry in the ncmCard Sync ncmSource Time table."
	    -- INDEX   { ncmCardSyncncmSourceLineIndex }
	     INDEX   { ncmCardSyncNIDIndex, ncmCardSyncSourceIndex }
	    ::= { ncmCardSyncSourceTable 1 }



	NcmCardSyncSourceEntry ::=
	 SEQUENCE {
            ncmCardSyncNIDIndex 
		 INTEGER,
	    ncmCardSyncSourceIndex
		 INTEGER,
	    ncmCardReceiveClockFromShelf
		INTEGER,
	    ncmCardSourceOneShelfNum
		INTEGER,
	    ncmCardSourceOneCardNum
		INTEGER,
	    ncmCardSourceOneClockRef
		INTEGER,
	    ncmCardAutoRestore1
		INTEGER,
	    ncmCardSourceTwoShelfNum
		INTEGER,
	    ncmCardSourceTwoCardNum
		INTEGER,
	    ncmCardSourceTwoClockRef
		INTEGER,
	    ncmCardAutoRestore2
		INTEGER,
	    ncmCardSourceThreeShelfNum
		INTEGER,
	    ncmCardSourceThreeCardNum
		INTEGER,
	    ncmCardSourceThreeClockRef
		INTEGER,
	    ncmCardAutoRestore3
		INTEGER,
	    ncmCardCurrentSyncSource
		INTEGER
	}


        ncmCardSyncNIDIndex OBJECT-TYPE
             SYNTAX  INTEGER (1..'7fffffff'h)
             ACCESS  read-only
             STATUS  mandatory
             DESCRIPTION
                "This variable indicates the node id
	         value of the node."  
	    ::= { ncmCardSyncSourceEntry 1 }


	 ncmCardSyncSourceIndex OBJECT-TYPE
	     SYNTAX  INTEGER (1..'7fffffff'h)
	     ACCESS  read-only
	     STATUS  mandatory
	     DESCRIPTION
		"This object is the identifier of an Interface
		on a managed device.  If there is an ifEntry
		that is directly associated with this and
		only this DS1 interface, it should have the
		same value as ifIndex.  Otherwise, the value
		exceeds ifNumber, and is a unique identifier
		following this rule: inside interfaces (e.g.,
		equipment side) with even numbers and outside
		interfaces (e.g., network side) with odd
		numbers."
	    ::= { ncmCardSyncSourceEntry 2 }


       	ncmCardReceiveClockFromShelf OBJECT-TYPE
	    SYNTAX INTEGER {
			no(1),
			yes(2)
			
		    }
	    ACCESS  read-write
	    STATUS  mandatory
	    DESCRIPTION 
	    "This variable indicates receive sync clock from the shelf for
                 the card ."
	    ::= {ncmCardSyncSourceEntry 3 }

	
      	 ncmCardSourceOneShelfNum OBJECT-TYPE
	    SYNTAX INTEGER 
	    ACCESS  read-write
	    STATUS  mandatory
	    DESCRIPTION
	    "This variable indicates the source one shelf number."
	    ::= { ncmCardSyncSourceEntry 4 }


	    
      	 ncmCardSourceOneCardNum OBJECT-TYPE
	    SYNTAX INTEGER 
	    ACCESS  read-write
	    STATUS  mandatory
	    DESCRIPTION
	    "This variable indicates the source one card number."
	    ::= {ncmCardSyncSourceEntry 5 }




	ncmCardSourceOneClockRef OBJECT-TYPE
	    SYNTAX INTEGER {
