        ncmE1PortTestPattern  OBJECT-TYPE
             SYNTAX  INTEGER {
                         none(1),
                         three-24(2),
                         qRSS(3),
                         two-n-20-1(4),
                         one-in-8(5),
                         two-n-15-1(6),
                         all-zeros(7),
                         fifty-octets(8),
                         all-ones(9)
                     }
             ACCESS  read-only
             STATUS  mandatory
             DESCRIPTION
             "This variable indicates the test patern in progress:
              1 = none
              2 = 3/24
              3 = QRSS
              4 = 2n20-1
              5 = 1 in 8
              6 = 2N15 - 1
              7 = All Zeros
              8 = 55 Octets
              9 = all ones  "
 
             ::= { ncmE1PortStatusEntry 23 }

 
	--     ncm E1 Performance Monitor Current Table
 
     -- The current table contains various statistics of error being
     -- collected for the current 15 minute interval.
 
       ncmE1CurrentTable OBJECT-TYPE
             SYNTAX  SEQUENCE OF NcmE1CurrentEntry
             ACCESS  not-accessible
             STATUS  mandatory
	     --DEFAULT test-function    acee1-test
             --DEFAULT next-function    acee1-next

            ::= { ncm-generic 1010 }
 
 
       ncmE1CurrentEntry OBJECT-TYPE
             SYNTAX  NcmE1CurrentEntry
             ACCESS  not-accessible
             STATUS  mandatory
             DESCRIPTION
                "An entry in the ncm E1 Current table."
            INDEX   { ncmE1CurrentNIDIndex, ncmE1CurrentIndex, ncmE1CurrentEndType }
            ::= { ncmE1CurrentTable 1 }
 
 
       NcmE1CurrentEntry ::=
           SEQUENCE {
           ncmE1CurrentNIDIndex
                 INTEGER,
           ncmE1CurrentIndex
                 INTEGER,
           ncmE1CurrentEndType
                 INTEGER,
           ncmE1CRC4Status
                INTEGER,
           ncmE1Timestamp
                INTEGER,
           ncmE1Timestamp1
                INTEGER,
           ncmE1CurrentIntervalSec
                INTEGER,
           ncmE1CurrentASs
                 Gauge,
           ncmE1CurrentUASs
                 Gauge,
           ncmE1CurrentESs
                 Gauge,
           ncmE1CurrentSESs
                 Gauge,
           ncmE1CurrentFAE
                 Gauge,
           ncmE1CurrentBBE
                 Gauge,
	   ncmE1CurrentSEFSs
                 Gauge,
           ncmE1CurrentAISSs
                 Gauge,
           ncmE1CurrentLOSSs
                 Gauge,
           ncmE1CurrentLOFSs
                 Gauge,
           ncmE1CurrentOOFSs
                 Gauge,
           ncmE1CurrentCVPath
                 Gauge,
           ncmE1CurrentCVLine
                 Gauge,
           ncmE1CurrentESsLine
                 Gauge,
           ncmE1CurrentSESsLine
                 Gauge,
           ncmE1CurrentFECPath
                 Gauge,
           ncmE1CurrentFECLine
                 Gauge
 
           }


	 ncmE1CurrentNIDIndex OBJECT-TYPE
             SYNTAX  INTEGER (1..'7fffffff'h)
             ACCESS  read-only
             STATUS  mandatory
             DESCRIPTION
                "This object identifies the node id of the node."
            ::= { ncmE1CurrentEntry 1 }
 
 
        ncmE1CurrentIndex OBJECT-TYPE
             SYNTAX  INTEGER (1..'7fffffff'h)
             ACCESS  read-only
             STATUS  mandatory
             DESCRIPTION
                "The index value which uniquely identifies  the
                interface  to which this entry is applicable.
                The interface identified by a  particular
                value  of  this  index is the same interf as
                identified by the same value as a LineIndex
                object instance."
            ::= { ncmE1CurrentEntry 2 }
 
 
        ncmE1CurrentEndType OBJECT-TYPE
            SYNTAX INTEGER {
                       near-End(1),
                       far-End(2)
                        }
           ACCESS read-only
           STATUS mandatory
           DESCRIPTION
           "The variable specifies the End Type."
 
            ::= { ncmE1CurrentEntry 3 }


	ncmE1CRC4Status OBJECT-TYPE
            SYNTAX INTEGER {
                       disabled(1),
                       enabled(2)
                        }
           ACCESS read-only
           STATUS mandatory
           DESCRIPTION
           "The variable specifies the CRC4 Status."
 
            ::= { ncmE1CurrentEntry 4 }
 
 
        ncmE1Timestamp OBJECT-TYPE
           SYNTAX INTEGER
           ACCESS read-only
           STATUS mandatory
           DESCRIPTION
           "The variable specifies the timestamp in seconds."
 
           ::= { ncmE1CurrentEntry 5 }
 
         ncmE1Timestamp1 OBJECT-TYPE
           SYNTAX INTEGER
           ACCESS read-only
           STATUS mandatory
           DESCRIPTION
           "The variable specifies the timestamp in milliseconds."
 
           ::= { ncmE1CurrentEntry 6 }
 
 
         ncmE1CurrentIntervalSec OBJECT-TYPE
           SYNTAX INTEGER
           ACCESS read-only
           STATUS mandatory
           DESCRIPTION
           "The variable specifies the current interval in seconds."
 
           ::= { ncmE1CurrentEntry 7 }
 
         
         ncmE1CurrentASs OBJECT-TYPE
             SYNTAX  Gauge
             ACCESS  read-only
             STATUS  mandatory
             DESCRIPTION
                "The number of Availabe Seconds, encountered by
                a DS1 interface in the current 15 minute interval."
            ::= { ncmE1CurrentEntry 8 }


	 ncmE1CurrentUASs OBJECT-TYPE
             SYNTAX  Gauge
             ACCESS  read-only
             STATUS  mandatory
             DESCRIPTION
                "The number of Out of Unavailable Seconds encountered
                by a DS1 interface in the current 15 minute interval."
            ::= { ncmE1CurrentEntry 9 }
 
 
        ncmE1CurrentESs OBJECT-TYPE
             SYNTAX  Gauge
             ACCESS  read-only
             STATUS  mandatory
             DESCRIPTION
                "The number of Errored Seconds encountered
                by a DS1 interface in the current 15 minute interval."
            ::= { ncmE1CurrentEntry 10 }
 
 
         ncmE1CurrentSESs OBJECT-TYPE
             SYNTAX  Gauge
             ACCESS  read-only
             STATUS  mandatory
             DESCRIPTION
                "The number of Severely Errored Seconds encountered
