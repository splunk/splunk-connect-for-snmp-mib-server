--    Dial-Backup Group

        ncmDialBkUpInfoTable OBJECT-TYPE
             SYNTAX   SEQUENCE OF NcmDialBkUpInfoEntry
             ACCESS   not-accessible
             STATUS   mandatory
	     --DEFAULT test-function    ccb-test
             --DEFAULT next-function    ccb-next


             ::= { ncm-generic 1038 }


        ncmDialBkUpInfoEntry OBJECT-TYPE
             SYNTAX   NcmDialBkUpInfoEntry
             ACCESS   not-accessible
             STATUS   mandatory
             DESCRIPTION
                "An entry in the retrieval of dial-backup parameter table for ncmCircuit Build."
             INDEX   { ncmDialBkUpInfoNIDIndex }
            ::= { ncmDialBkUpInfoTable 1 }


        NcmDialBkUpInfoEntry ::=
             SEQUENCE {
             ncmDialBkUpInfoNIDIndex
                 INTEGER,
             ncmDialBkUpReqCircName
                 DisplayString,
             ncmPrimaryCircName
                 DisplayString,
             ncmBackupCircName
                 DisplayString,
             ncmCallReferenceNumber
                 INTEGER,
             ncmCirSrcPortErrsetupMask
                 INTEGER,
             ncmCirDstPortErrsetupMask
                 INTEGER,
             ncmCirErrClearanceMask
                 INTEGER,
             ncmDialBkUpTimeout
                 INTEGER,
	     ncmDialBkUpAction
		 INTEGER,
	     ncmDialBkUpSetupStatus
		 INTEGER

                }


        ncmDialBkUpInfoNIDIndex OBJECT-TYPE
             SYNTAX  INTEGER (1..'7fffffff'h)
             ACCESS  read-only
             STATUS  mandatory
             DESCRIPTION
                "This variable indicates the node id
                 value of the node."
               ::= { ncmDialBkUpInfoEntry 1 }


        ncmDialBkUpReqCircName OBJECT-TYPE
            SYNTAX DisplayString (SIZE (0..12))
            ACCESS  read-write
            STATUS  mandatory
            DESCRIPTION
            "This variable indicates the circuit name for request dial-backup
		parameter and retrieve from the circuitbuilder."
            ::= { ncmDialBkUpInfoEntry 2 }


        ncmPrimaryCircName  OBJECT-TYPE
            SYNTAX DisplayString (SIZE (0..12))       
            ACCESS  read-write
            STATUS  mandatory  
            DESCRIPTION
            "This variable indicates the primary circuit name for request 
		and retrieve from the circuitbuilder as a parameter of dial-backup."
            ::= { ncmDialBkUpInfoEntry 3 }
 

        ncmBackupCircName  OBJECT-TYPE
            SYNTAX DisplayString (SIZE (0..12))       
            ACCESS  read-write
            STATUS  mandatory  
            DESCRIPTION
            "This variable indicates the backup circuit name for request and 
		retrieve from the circuitbuilder as a parameter of dial-backup."
            ::= { ncmDialBkUpInfoEntry 4 }
 
 
        ncmCallReferenceNumber  OBJECT-TYPE
             SYNTAX  INTEGER
             ACCESS  read-write
             STATUS  mandatory
             DESCRIPTION
             "The variable indicates the call reference number of the dial-backup."
          ::= { ncmDialBkUpInfoEntry 5 }


        ncmCirSrcPortErrsetupMask  OBJECT-TYPE
             SYNTAX  INTEGER {
        		ccb-DBKP-ALM-AIS-CTS(1),
        		ccb-DBKP-ALM-YEL-DSR(2),
        		ccb-DBKP-ALM-LOS-DCD(4),
        		ccb-DBKP-ALM-LOF(8),
        		ccb-DBKP-ALM-UAS(16),
        		ccb-DBKP-ALM-SES(32)

		}
             ACCESS  read-write
             STATUS  mandatory
             DESCRIPTION
             "The variable indicates the bitmap to setup the mask for circuit 
		source port error of the dial-backup; ncmAlarm Mask ALM-AIS, ALM-YEL
		and ALM-LOS are defined for t1 port; whilst ALM-CTS, ALM-DSR and
		ALM-DCD are defined for data port."
          ::= { ncmDialBkUpInfoEntry 6 }
 

        ncmCirDstPortErrsetupMask  OBJECT-TYPE
             SYNTAX  INTEGER {
        		ccb-DBKP-ALM-AIS-CTS(1),
        		ccb-DBKP-ALM-YEL-DSR(2),
        		ccb-DBKP-ALM-LOS-DCD(4),
                        ccb-DBKP-ALM-LOF(8),
                        ccb-DBKP-ALM-UAS(16),
                        ccb-DBKP-ALM-SES(32)
 
                }
             ACCESS  read-write
             STATUS  mandatory
             DESCRIPTION
             "The variable indicates the bitmap to setup the mask for circuit
                destination port error of the dial-backup; ncmAlarm Mask ALM-AIS, 
		ALM-YEL and ALM-LOS are defined for t1 port; whilst ALM-CTS, 
		ALM-DSR and ALM-DCD are defined for data port."
          ::= { ncmDialBkUpInfoEntry 7 }


        ncmCirErrClearanceMask  OBJECT-TYPE
             SYNTAX  INTEGER {
        		ccb-DBKP-ALM-AIS-CTS(1),
        		ccb-DBKP-ALM-YEL-DSR(2),
        		ccb-DBKP-ALM-LOS-DCD(4),
                        ccb-DBKP-ALM-LOF(8),
                        ccb-DBKP-ALM-UAS(16),
                        ccb-DBKP-ALM-SES(32)
 
                }
             ACCESS  read-write
             STATUS  mandatory
             DESCRIPTION
             "The variable indicates the bitmap to setup the mask for circuit
                error clearance of the dial-backup; ncmAlarm Mask ALM-AIS, 
                ALM-YEL and ALM-LOS are defined for t1 port; whilst ALM-CTS, 
                ALM-DSR and ALM-DCD are defined for data port."
          ::= { ncmDialBkUpInfoEntry 8 }


        ncmDialBkUpTimeout  OBJECT-TYPE
             SYNTAX  INTEGER
             ACCESS  read-write
             STATUS  mandatory
             DESCRIPTION
             "The variable indicates the dial-backup timeout."
          ::= { ncmDialBkUpInfoEntry 9 }


        ncmDialBkUpAction  OBJECT-TYPE
             SYNTAX  INTEGER {
                set(1)
                }
             ACCESS  read-write
             STATUS  mandatory
             DESCRIPTION
                "The object send the corresponding acp command to
                   execute the dial-backup set action."
            ::= { ncmDialBkUpInfoEntry 10 }


        ncmDialBkUpSetupStatus  OBJECT-TYPE
             SYNTAX  INTEGER {
                ccb-BUILD-SUCCESS(1),
                ccb-BUILD-NO-PORT-BW(2),
