	--   ncm T1 Port Configuration Table

 
       ncmT1ConfigTable OBJECT-TYPE
             SYNTAX  SEQUENCE OF NcmT1ConfigEntry
             ACCESS  not-accessible
             STATUS  mandatory
	     --DEFAULT test-function    ace-test
             --DEFAULT next-function    ace-next


            ::= { ncm-generic 1017 }


       ncmT1ConfigEntry OBJECT-TYPE
             SYNTAX  NcmT1ConfigEntry
             ACCESS  not-accessible
             STATUS  mandatory
             DESCRIPTION
                "An entry in the  ncm T1 Configuration table."
            INDEX   { ncmT1ConfigNIDIndex, ncmT1LineIndex }
            ::= { ncmT1ConfigTable 1 }


       NcmT1ConfigEntry ::=
         SEQUENCE {

	   ncmT1ConfigNIDIndex
		INTEGER,
           ncmT1LineIndex
                 INTEGER,
           ncmT1CRC6
                 INTEGER,
           ncmT1FramingFormat
                 INTEGER,
           ncmT1LineCoding
                 INTEGER,
	   ncmT1FramingType
		 INTEGER,
           ncmT1FDL
                 INTEGER,
           ncmT1InbandISDNEnableDisable
                 INTEGER,
           ncmT1EnableNetLoopback
                 INTEGER,
           ncmT1EnableNetOnesDensity
                INTEGER,
           ncmT1DensityPattern
                 INTEGER,
           ncmT1EnableRepeaterLpbkTimeout
                 INTEGER,
           ncmT1RepeaterLoopbackTimeout
                 INTEGER,
           ncmT1UserAlarmDeclareTime
                 INTEGER,
           ncmT1State
                 INTEGER,
           ncmT1NetworkLBOEQLIZ 
                 INTEGER,
           ncmT1TimeSlot
                DisplayString,
           ncmT1TimeSlotSelect
			INTEGER
		}

	ncmT1ConfigNIDIndex OBJECT-TYPE
             SYNTAX  INTEGER (1..'7fffffff'h)
             ACCESS  read-only
             STATUS  mandatory
             DESCRIPTION
                "This object is the identifier of the node id index."
            ::= { ncmT1ConfigEntry 1 }
 

	
 
        ncmT1LineIndex OBJECT-TYPE
             SYNTAX  INTEGER (1..'7fffffff'h)
             ACCESS  read-only
             STATUS  mandatory
             DESCRIPTION
                "This object is the identifier of an Interface
                on a managed device.  If there is an ifEntry
                that is directly associated with this and
                only this DS1 interface, it should have the
                same value as ifIndex.  Otherwise, the value
                exceeds ifNumber, and is a unique identifier
                following this rule: inside interfaces (e.g.,
                equipment side) with even numbers and outside
                interfaces (e.g., network side) with odd
                numbers."
            ::= { ncmT1ConfigEntry 2 }
 
 
         
         ncmT1CRC6  OBJECT-TYPE
             SYNTAX  INTEGER {
                         disabled(1),
                         enabled(2)
                     }
             ACCESS  read-write
             STATUS  mandatory
             DESCRIPTION
             "This variable indicates Whether CRC6 is enabled or not"
             ::= { ncmT1ConfigEntry 3 }
        
 
        ncmT1FramingFormat OBJECT-TYPE
             SYNTAX  INTEGER {
                         sF(1),
                         eSF(2)
                          }
             ACCESS  read-write
             STATUS  mandatory
             DESCRIPTION
                "This variable indicates  the  variety  of
                Line  implementing  this  circuit.  The type of
                circuit affects the number of bits  per  second
                that  the circuit can reasonably carry, as well
                as the interpretation of the  usage  and  error
                statistics.  The values, in sequence, describe:
 
                TITLE:          SPECIFICATION:
                ESF             Extended SuperFrame DS1
                SF              Super Frame"
 
         ::= { ncmT1ConfigEntry 4 }
	
	 ncmT1LineCoding OBJECT-TYPE
             SYNTAX  INTEGER {
        
                         aMI (1),
                         b8ZS (2)
                     }
             ACCESS  read-write
             STATUS  mandatory
             DESCRIPTION
                "This variable describes the  variety  of  Zero
                Code  Suppression  used  on  the link, which in
                turn affects a number of its characteristics.
 
                B8ZS refers to the use of a specified  pat-
                tern  of  normal  bits  and  bipolar violations
                which are used to replace a  sequence  of  eight
                zero bits.
 
                AMI refers to a mode wherein no  zero  code
                suppression  is  present  and the line encoding
                does not solve the problem directly.   In  this
                application, the higher layer must provide data
                which meets or exceeds the  pulse  density  re-
                quirements, such as inverting HDLC data."
 
            ::= { ncmT1ConfigEntry 5 }

 
          ncmT1FramingType OBJECT-TYPE
            SYNTAX INTEGER {
                          cAS-BR(1),
                          cAS-CC(2),
                          cCS(3)
                          }   

            ACCESS  read-write
            STATUS  mandatory
            DESCRIPTION
            "This variable indicates the framing type in this
		 T1 port, i.e., CAS-BR, CAS-CC, CCS"
            ::= { ncmT1ConfigEntry 6 }

        
         ncmT1FDL OBJECT-TYPE
             SYNTAX  INTEGER {
             disabled(1),
             enabled(2)
                     }
             ACCESS  read-write
             STATUS  mandatory
             DESCRIPTION
          "The variable specifies the Disable/Enable of the fdl State."
            ::= { ncmT1ConfigEntry 7 }
 
 
 
         ncmT1InbandISDNEnableDisable OBJECT-TYPE
             SYNTAX  INTEGER {
             	disablebothinbandisdn(1),
             	enableinband(2),
             	enableisdn(3)
                             }
             ACCESS  read-write
             STATUS  mandatory
             DESCRIPTION
             "This variable indicates the Inband, ISDN enable and disable options."
            ::= { ncmT1ConfigEntry 8 }
 
 
