--     ncm E1 Port Config Group


      ncmE1PortConfigTable OBJECT-TYPE
        SYNTAX  SEQUENCE OF NcmE1PortConfigEntry
        ACCESS  not-accessible
        STATUS  mandatory
	--DEFAULT test-function    acee1-test
        --DEFAULT next-function    acee1-next

         ::= { ncm-generic  1008  }


      ncmE1PortConfigEntry OBJECT-TYPE
        SYNTAX  NcmE1PortConfigEntry
        ACCESS  not-accessible
        STATUS  mandatory
        DESCRIPTION
        "An entry in the  E1 Port Configuration table."
         INDEX   { ncmE1ConfigNIDIndex, ncmE1PortLineIndex }
         ::= { ncmE1PortConfigTable 1 }


      NcmE1PortConfigEntry ::=
         SEQUENCE {
           ncmE1ConfigNIDIndex
                INTEGER,
           ncmE1PortLineIndex
                 INTEGER,
           ncmE1PortCRC4
                INTEGER,
           ncmE1PortFaseAlarm
                INTEGER,
           ncmE1PortCarrierFailureAlarm
                INTEGER,
           ncmE1PortInbandISDNEnableDisable
                INTEGER,
           ncmE1PortRepeaterLoopbackTimeoutEnable
                INTEGER,
           ncmE1PortFraming
                INTEGER,
           ncmE1PortRepeaterLoopbackTimeout
                INTEGER,
           ncmE1PortCarrierFailureAlarmDeclareTime
                INTEGER,
           ncmE1PortServiceState
                INTEGER,
           ncmE1PortIdlePattern
                INTEGER,
           ncmE1TimeSlot
                DisplayString,
           ncmE1TimeSlotSelect
		INTEGER 
 
                }
         
        
        ncmE1ConfigNIDIndex OBJECT-TYPE
             SYNTAX  INTEGER (1..'7fffffff'h)
             ACCESS  read-only
             STATUS  mandatory
             DESCRIPTION
                "This object is the identifier of the node id index."
            ::= { ncmE1PortConfigEntry 1 }
 
             
      ncmE1PortLineIndex OBJECT-TYPE
          SYNTAX  INTEGER (1..'7fffffff'h)
          ACCESS  read-only
          STATUS  mandatory
          DESCRIPTION
         "The index value which uniquely identifies the
          interface  to which this entry is applicable."
 
          ::= { ncmE1PortConfigEntry  2 }
 
 
      ncmE1PortCRC4 OBJECT-TYPE
            SYNTAX INTEGER {
                        disable(1),
                        enable(2)
                           }
 
            ACCESS  read-write
            STATUS  mandatory
            DESCRIPTION
            "This variable indicates whether the CRC4 for the port is
                 Enabled or Disabled."
            ::= { ncmE1PortConfigEntry 3 }
 
         
        ncmE1PortCarrierFailureAlarm OBJECT-TYPE
            SYNTAX INTEGER {
                        disable(1),
                        enable(2)
                           }
           ACCESS  read-write
            STATUS  mandatory
            DESCRIPTION
            "This variable indicates whether the Carrier Failure ncmAlarm
                is Enabled or Disabled."
            ::= { ncmE1PortConfigEntry 4 }
 

	 ncmE1PortFaseAlarm OBJECT-TYPE
            SYNTAX INTEGER {
                        minor(1),
                        major(2)
                           }
 
            ACCESS  read-write
            STATUS  mandatory
            DESCRIPTION
            "This variable indicates whether the Fase ncmAlarm is Major or
                 Minor Category."
            ::= { ncmE1PortConfigEntry 5 }
 
 
         ncmE1PortInbandISDNEnableDisable OBJECT-TYPE
            SYNTAX INTEGER {
                disablebothinbandisdn(1),
                enableinband(2),
                enableisdn(3)
                             }
             ACCESS  read-write
             STATUS  mandatory
             DESCRIPTION
             "This variable indicates the Inband, ISDN enable and disable options."
            ::= { ncmE1PortConfigEntry 6 }
 
 
        ncmE1PortRepeaterLoopbackTimeoutEnable OBJECT-TYPE
            SYNTAX INTEGER {
                        disable(1),
                        enable(2)
                           }
 
            ACCESS  read-write
            STATUS  mandatory
            DESCRIPTION
            "This variable indicates the option to enable the repeater
                loopback timeout."
            ::= { ncmE1PortConfigEntry 7 }
 
 
         ncmE1PortFraming OBJECT-TYPE
            SYNTAX INTEGER {
                        cAS(1),
                        cCS(2),
                        thirty-one-Ch(3),
                        g703(4)
                           }
 
            ACCESS  read-write
            STATUS  mandatory
            DESCRIPTION
            "This variable indicates the framing for E1 Port."
            ::= { ncmE1PortConfigEntry 8 }


	 ncmE1PortRepeaterLoopbackTimeout OBJECT-TYPE
            SYNTAX INTEGER
            ACCESS  read-write
            STATUS  mandatory
            DESCRIPTION
            "This variable indicates the repeater loopback timeout in
                seconds for the card."
            ::= { ncmE1PortConfigEntry 9  }
 
 
         ncmE1PortCarrierFailureAlarmDeclareTime OBJECT-TYPE
            SYNTAX INTEGER
            ACCESS  read-write
            STATUS  mandatory
            DESCRIPTION
            "This variable indicates the carrier failure alarm declare
                 time in seconds for the card."
            ::= { ncmE1PortConfigEntry 10  }
 
 
         
         ncmE1PortServiceState OBJECT-TYPE
            SYNTAX INTEGER {
                        out-of-service(1),
                        in-service(2)
                           }
 
            ACCESS  read-write
            STATUS  mandatory
            DESCRIPTION
                "This variable indicates service state of the port."
            ::= { ncmE1PortConfigEntry 11 }
 
 
 
        ncmE1PortIdlePattern OBJECT-TYPE
            SYNTAX INTEGER
            ACCESS  read-write
            STATUS  mandatory
            DESCRIPTION
            "This variable indicates the idle pattern for the card."
            ::= { ncmE1PortConfigEntry 12  }
 
 
 
        ncmE1TimeSlot OBJECT-TYPE
          --  SYNTAX DisplayString
            SYNTAX  DisplayString (SIZE (0..255))
            ACCESS  read-only
            STATUS  mandatory
            DESCRIPTION
            "This variable indicates the Time slot used for the given port
                directly from the hardware cross point switch for example slot         
                1-10, if slots 1 to 10 is used or 1,3,4 if slots 1,3 and 4 are
                used."
            ::= { ncmE1PortConfigEntry 13  }
 

        ncmE1TimeSlotSelect OBJECT-TYPE  
            SYNTAX INTEGER
            ACCESS  read-write
            STATUS  mandatory
            DESCRIPTION
            "This variable indicates the number which is choosed for the inband/isdn timeslot."
            ::= { ncmE1PortConfigEntry 14  }


