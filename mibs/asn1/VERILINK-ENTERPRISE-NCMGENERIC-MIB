--               T1 24 Hour Performance Snap Shot
       
       
         ncmT1PreformanceSnapShotTable OBJECT-TYPE
             SYNTAX  SEQUENCE OF NcmT1PreformanceSnapShotEntry
             ACCESS  not-accessible
             STATUS  mandatory
	     --DEFAULT test-function    ace-test
             --DEFAULT next-function    ace-next

 
            ::= { ncm-generic 1023  }
 
 
         ncmT1PreformanceSnapShotEntry OBJECT-TYPE
             SYNTAX  NcmT1PreformanceSnapShotEntry
             ACCESS  not-accessible
             STATUS  mandatory
             DESCRIPTION
             "An entry in the  T1 Performance Snap Shot Entry
              table."
            INDEX   { ncmT1SnapShotNIDIndex, ncmT1SnapShotIndex, ncmT1SnapShotEndType }
            ::= { ncmT1PreformanceSnapShotTable 1 }
 
 
        NcmT1PreformanceSnapShotEntry ::=
         SEQUENCE {
	
	   ncmT1SnapShotNIDIndex
		INTEGER,
           ncmT1SnapShotIndex
                 INTEGER,
	   ncmT1SnapShotEndType
                 INTEGER,
           ncmT1SnapShot
                INTEGER,
           ncmT1TimeStampSec
                INTEGER,
           ncmT1TimeStampMilliSec
                 INTEGER,
           ncmT1CurrIntervalSec
                INTEGER,
           ncmT1ResetPerfReg
                 INTEGER
 
                }


	
	ncmT1SnapShotNIDIndex OBJECT-TYPE
             SYNTAX  INTEGER (1..'7fffffff'h)
             ACCESS  read-only
             STATUS  mandatory
             DESCRIPTION
                "The object specifies the node id index of the node."
 
 
            ::= { ncmT1PreformanceSnapShotEntry 1 }

       
 
          ncmT1SnapShotIndex OBJECT-TYPE
             SYNTAX  INTEGER (1..'7fffffff'h)
             ACCESS  read-only
             STATUS  mandatory
             DESCRIPTION
                "The index value which uniquely identifies  the
                DS1  interface  to which this entry is applica-
                ble.  The interface identified by a  particular
                value  of  this  index is the same interface as
                identified by the same value as a LineIndex
                object instance."
 
 
            ::= { ncmT1PreformanceSnapShotEntry 2 }


        ncmT1SnapShotEndType OBJECT-TYPE
            SYNTAX INTEGER {

                       near-End(1),
                       far-End(2)

                        }
           ACCESS read-only
           STATUS mandatory
           DESCRIPTION
           "The variable specifies the End Type   ."

            ::= { ncmT1PreformanceSnapShotEntry 3 }

 
	ncmT1SnapShot OBJECT-TYPE
            SYNTAX INTEGER {
 
                                no(1),
                                yes(2)
                       }
           ACCESS read-write
           STATUS mandatory
           DESCRIPTION
           "The variable specifies the option to intiate snap shot
               command."
       
            ::= { ncmT1PreformanceSnapShotEntry 4 }
 
 
 
        ncmT1TimeStampSec OBJECT-TYPE
            SYNTAX INTEGER
           ACCESS read-only
           STATUS mandatory
           DESCRIPTION
           "The variable specifies the Time Stamp in Seconds ."
 
           ::= { ncmT1PreformanceSnapShotEntry 5 }
 
