                INTEGER,
           ncmE1PortLOFA
                INTEGER,
           ncmE1PortAIS
                INTEGER,
           ncmE1PortLOS
                INTEGER,
           ncmE1PortBPVThresholdExceeded
                INTEGER,
           ncmE1BlockErrorCounter
                Gauge,
	   ncmE1CodeViolationMode
                INTEGER,
           ncmE1CurrentHDB3ORFEBEErrorCounts
                Gauge,
           ncmE1CurrentFrameErrorCounts
                Gauge,
           ncmE1CurrentCRC4ErrorCounts
                Gauge,
	   ncmE1ResetPerfCount
		INTEGER,
           ncmE1PortSendRAI
                INTEGER,
           ncmE1PortSendAIS
                INTEGER,
           ncmE1PortRFA
                INTEGER,
           ncmE1PortBERThresholdExceeded
                INTEGER,
           ncmE1PortLLB
                INTEGER,
           ncmE1PortPLB
                INTEGER,
           ncmE1PortLOC
                INTEGER,
           ncmE1PortTestPattern
                INTEGER
                  
                }
 
