                INTEGER,
           ncmT1TotalASs
                 Gauge,
           ncmT1TotalUASs
                 Gauge,
           ncmT1TotalESs
                 Gauge,
           ncmT1TotalBESs
                 Gauge,
           ncmT1TotalSESs
                 Gauge,
           ncmT1TotalSEFs
                Gauge,
           ncmT1TotalLOSs
                Gauge,
           ncmT1TotalAISs
                Gauge,
           ncmT1TotalLOFs
                Gauge,
