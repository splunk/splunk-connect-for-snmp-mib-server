             ACCESS   not-accessible
             STATUS   mandatory
		 --DEFAULT test-function  almcfg-test
             --DEFAULT next-function    	almcfg-next



 
 
             ::= { ncm-generic  1046 }
 
 
        ncmAlarmThresholdStatusEntry OBJECT-TYPE
             SYNTAX   NcmAlarmThresholdStatusEntry
             ACCESS   not-accessible
             STATUS   mandatory
             DESCRIPTION
                "An entry in the  ncmAlarm Threshold Status table."
             INDEX   { ncmAlarmThresholdStatusNIDIndex, ncmAlarmThresholdStatusIndex }
            ::= { ncmAlarmThresholdStatusTable 1 }
 
 
        NcmAlarmThresholdStatusEntry ::=
             SEQUENCE {
             ncmAlarmThresholdStatusNIDIndex
                 INTEGER,
             ncmAlarmThresholdStatusIndex
                 INTEGER,
             ncmLOFExceeded
                INTEGER,
             ncmLOSExceeded
