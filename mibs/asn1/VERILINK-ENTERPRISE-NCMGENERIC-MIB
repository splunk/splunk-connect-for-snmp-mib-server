	--      ncm Shelf Sync Source Segment Table

 
         ncmShelfSyncSourceSegTable OBJECT-TYPE
             SYNTAX  SEQUENCE OF NcmShelfSyncSourceSegEntry
             ACCESS  not-accessible
             STATUS  mandatory
	     --DEFAULT test-function    acesyncseg-test
             --DEFAULT next-function    acesyncseg-next
 
            ::= { ncm-generic  1050 }


        ncmShelfSyncSourceSegEntry OBJECT-TYPE
             SYNTAX NcmShelfSyncSourceSegEntry
             ACCESS  not-accessible
             STATUS  mandatory
             DESCRIPTION
                "An entry in the ncm Shelf Sync Source Segment Time table."
            INDEX   { ncmShelfSyncSegNIDIndex, ncmShelfSyncSourceSegIndex, ncmShelfSyncSourceSegNum }
            ::= { ncmShelfSyncSourceSegTable 1 }


        NcmShelfSyncSourceSegEntry ::=
         SEQUENCE {
           ncmShelfSyncSegNIDIndex
                INTEGER,
           ncmShelfSyncSourceSegIndex
                INTEGER,
	   ncmShelfSyncSourceSegNum
                INTEGER,
           ncmShelfTypeSeg
                INTEGER,
	   ncmRefBusSeg
                INTEGER,
           ncmSourceSegOneShelfNum
                INTEGER,
           ncmSourceSegOneCardNum
                INTEGER,
           ncmSourceSegOneClockRef
                INTEGER,
           ncmSourceSegAutoRestore1
                INTEGER,
           ncmSourceSegTwoShelfNum
                INTEGER,
           ncmSourceSegTwoCardNum
                INTEGER,
           ncmSourceSegTwoClockRef
                INTEGER,
           ncmSourceSegAutoRestore2
                INTEGER,
           ncmSourceSegThreeShelfNum
                INTEGER,
           ncmSourceSegThreeCardNum
                INTEGER,
           ncmSourceSegThreeClockRef
                INTEGER,
           ncmSourceSegAutoRestore3
                INTEGER,
           ncmShelfCurrentSyncSourceSeg
                INTEGER

                }

        ncmShelfSyncSegNIDIndex OBJECT-TYPE
             SYNTAX  INTEGER (1..'7fffffff'h)
             ACCESS  read-only
             STATUS  mandatory
             DESCRIPTION
                "This variable indicates the node id
	         value of the node."  
            ::= { ncmShelfSyncSourceSegEntry 1 }


         ncmShelfSyncSourceSegIndex OBJECT-TYPE
             SYNTAX  INTEGER (1..'7fffffff'h)
             ACCESS  read-only
             STATUS  mandatory
	      DESCRIPTION
                "This object is the identifier of an Interface
                on a managed device.  If there is an ifEntry
                that is directly associated with this and
                only this DS1 interface, it should have the
                same value as ifIndex.  Otherwise, the value
                exceeds ifNumber, and is a unique identifier
                following this rule: inside interfaces (e.g.,
                equipment side) with even numbers and outside
                interfaces (e.g., network side) with odd
                numbers."
            ::= { ncmShelfSyncSourceSegEntry 2 }
 
 
	   ncmShelfSyncSourceSegNum OBJECT-TYPE
             SYNTAX  INTEGER (1..'7fffffff'h)
             ACCESS  read-only
             STATUS  mandatory
             DESCRIPTION
                "This variable indicates the segment index."
            ::= { ncmShelfSyncSourceSegEntry 3 }

	     
           ncmShelfTypeSeg OBJECT-TYPE
             SYNTAX  INTEGER {
                         uninstalled(1),
                         dualLineShelf(2),
                         multiLineShelf(3),
                         nb-2200LineShelf(4),
                         nb-2200-4ILineShelf(10),
                         nb-2200QLineShelf(52),
                         nb-2200-4IQLineShelf(54)
                     }
             ACCESS  read-only
             STATUS  mandatory
             DESCRIPTION
                "This variable indicates the shelf hardware type."
            ::= { ncmShelfSyncSourceSegEntry 4 }


	   ncmRefBusSeg OBJECT-TYPE
             SYNTAX  INTEGER {
                         c(1),
                         a(2)
                     }
             ACCESS  read-write
             STATUS  mandatory
             DESCRIPTION
                "This variable indicates the reference bus of the segment."
            ::= { ncmShelfSyncSourceSegEntry 5 }

         
