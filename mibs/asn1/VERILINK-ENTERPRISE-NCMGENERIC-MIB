             STATUS   mandatory
	         --DEFAULT test-function    enhalm-test
             --DEFAULT next-function    enhalm-next
             ::= { ncm-generic  1033 }

         ncmEnhanAlmCfgEntry OBJECT-TYPE
             SYNTAX    NcmEnhanAlmCfgEntry
             ACCESS   not-accessible
             STATUS   mandatory
             DESCRIPTION
                "An entry in the Enhanced ncm Alarm Configure table."
             INDEX   {  ncmEnhanAlmCfgNIDIndex,  ncmEnhanAlmCfgIndex }
            ::= {  ncmEnhanAlmCfgTable 1 }
 
         NcmEnhanAlmCfgEntry ::=
             SEQUENCE {
              ncmEnhanAlmCfgNIDIndex
                 INTEGER,
              ncmEnhanAlmCfgIndex
                 INTEGER,
              ncmEnhanAlmCfgCRCThreshold
                 INTEGER,
              ncmEnhanAlmCfgSESThreshold
                 INTEGER,
              ncmEnhanAlmCfgUASThreshold
                 INTEGER,
              ncmEnhanAlmCfgBERThreshold
                 INTEGER,
              ncmEnhanAlmCfgBPVThreshold
                 INTEGER,
              ncmEnhanAlmCfgLineRestoral
                 INTEGER,
              ncmEnhanAlmCfgTODHour
                 INTEGER,
              ncmEnhanAlmCfgTODMinute
                 INTEGER,
              ncmEnhanAlmCfgLinRestEvalPer
                 INTEGER
             }

          ncmEnhanAlmCfgNIDIndex OBJECT-TYPE
          SYNTAX  INTEGER (1..'7fffffff'h)
          ACCESS  read-only
          STATUS  mandatory
          DESCRIPTION
         "The object specifies the node id of the node."
          ::= {  ncmEnhanAlmCfgEntry 1}


          ncmEnhanAlmCfgIndex OBJECT-TYPE
          SYNTAX  INTEGER (1..'7fffffff'h)
          ACCESS  read-only
          STATUS  mandatory
          DESCRIPTION
         "The index value which uniquely identifies  the
          interface to which this entry is applicable."
          ::= {  ncmEnhanAlmCfgEntry 2 }
 
          ncmEnhanAlmCfgCRCThreshold OBJECT-TYPE
          SYNTAX  INTEGER
          ACCESS  read-write
          STATUS mandatory
          DESCRIPTION
          "The variable specifies the consecutive CRC error threshold
	       of the port; 0 is to disable this error threshold and 1~31 is
	       to enable also specify a particular value for this threshold."
          ::= {  ncmEnhanAlmCfgEntry 3 }

          ncmEnhanAlmCfgSESThreshold OBJECT-TYPE
          SYNTAX  INTEGER
          ACCESS  read-write
          STATUS mandatory
          DESCRIPTION
          "The variable specifies the severly errored seconds threshold
           of the port; 0 is to disable this error threshold and 1~100 is
           to enable also specify a particular value for this threshold."
          ::= {  ncmEnhanAlmCfgEntry 4 }

          ncmEnhanAlmCfgUASThreshold OBJECT-TYPE
          SYNTAX  INTEGER
          ACCESS  read-write
          STATUS mandatory
          DESCRIPTION
          "The variable specifies the unavailable seconds threshold
           of the port; 0 is to disable this error threshold and 1~100 is
           to enable also specify a particular value for this threshold."
          ::= {  ncmEnhanAlmCfgEntry 5 }                                      
 
          ncmEnhanAlmCfgBERThreshold OBJECT-TYPE
          SYNTAX INTEGER {

                       disable(1),
                       d10E-4(2),
                       d10E-5(3),
                       d10E-6(4),
                       d10E-7(5),
                       d10E-8(6),
                       d10E-9(7)
               } 
          ACCESS  read-write
          STATUS mandatory
          DESCRIPTION
          "The variable specifies the bit error rate threshold of the 
               port; the value = 10 Exponential to the power minus n."
          ::= {  ncmEnhanAlmCfgEntry 6 }
 

          ncmEnhanAlmCfgBPVThreshold OBJECT-TYPE
          SYNTAX  INTEGER (1..'7fffffff'h)
          ACCESS  read-write
          STATUS  mandatory
          DESCRIPTION
         "The object specifies the BPV threshold the port."
          ::= {  ncmEnhanAlmCfgEntry 7 }

          ncmEnhanAlmCfgLineRestoral OBJECT-TYPE
          SYNTAX INTEGER {
                       disable(1),
                       automatic(2),
                       manual(3),
                       time-of-day(4)
               } 
          ACCESS  read-write
          STATUS mandatory
          DESCRIPTION
          "The variable specifies the Line Restoral mode of the port."
          ::= {  ncmEnhanAlmCfgEntry 8 }
 

          ncmEnhanAlmCfgTODHour OBJECT-TYPE
          SYNTAX  INTEGER 
          ACCESS  read-write
          STATUS  mandatory
