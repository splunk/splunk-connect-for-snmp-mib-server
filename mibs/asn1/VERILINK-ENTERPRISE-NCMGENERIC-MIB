                INTEGER,
              ncmCVL24HRExceeded
                INTEGER,
              ncmESL15MINExceeded
                INTEGER,
              ncmESL24HRExceeded
                INTEGER,
              ncmSESL15MINExceeded
                INTEGER,
              ncmSESL24HRExceeded
                INTEGER,
              ncmUASP15MINExceeded
                INTEGER,
              ncmUASP24HRExceeded
                INTEGER,
	      ncmLOFThresholdCount
		INTEGER,
	      ncmLOFTimeIntervals
		INTEGER,
	      ncmLOSThresholdCount
		INTEGER,
	      ncmLOSTimeIntervals
		INTEGER,
	      ncmRAIThresholdCount
		INTEGER,
	      ncmRAITimeIntervals
		INTEGER,
	      ncmAISThresholdCount
		INTEGER,
	      ncmAISTimeIntervals
		INTEGER,
	      ncmBPVThresholdCount
		INTEGER,
	      ncmBPVTimeIntervals
		INTEGER,
	     ncmES15MINThresholdCount
