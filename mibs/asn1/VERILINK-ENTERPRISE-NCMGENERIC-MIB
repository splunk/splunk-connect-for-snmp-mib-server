--	ncm E1 Performance Monitor Interval Table

     -- The ncm Interval Table contains various statistics
     -- collected by each DS1 Interface over the previous 24 hours of
     -- operation.  The past 24 hours are broken into 96 completed
     -- 15 minute intervals.

     
       	ncmE1IntervalTable OBJECT-TYPE
	     SYNTAX  SEQUENCE OF NcmE1IntervalEntry
	     ACCESS  not-accessible
	     STATUS  mandatory
	     --DEFAULT test-function   ace-e1-interval-test 
             --DEFAULT next-function    acee1interval-next

	    ::= { ncm-generic 1011 }


       	ncmE1IntervalEntry OBJECT-TYPE
	     SYNTAX  NcmE1IntervalEntry
	     ACCESS  not-accessible
	     STATUS  mandatory
	     DESCRIPTION
		"An entry in the  E1 Interval table."
	    INDEX   { ncmE1IntervalNIDIndex, ncmE1IntervalIndex, ncmE1IntervalEndType, ncmE1IntervalNumber }
	    ::= { ncmE1IntervalTable 1 }


     	 NcmE1IntervalEntry ::=
	 SEQUENCE {
           ncmE1IntervalNIDIndex
                  INTEGER,
	   ncmE1IntervalIndex
		 INTEGER,
	   ncmE1IntervalEndType
		 INTEGER,
	   ncmE1IntervalNumber
		 INTEGER,
	   ncmE1IntervalASs
		 Gauge,
	   ncmE1IntervalUASs
		 Gauge,
	   ncmE1IntervalESs
		 Gauge,
	   ncmE1IntervalSESs
		 Gauge,
	   ncmE1IntervalFAE
		 Gauge,
	   ncmE1IntervalBBE
		 Gauge,
	   ncmE1IntervalSEFSs
                 Gauge,
           ncmE1IntervalAISSs
                 Gauge,
           ncmE1IntervalLOSSs
                 Gauge,
           ncmE1IntervalLOFSs
                 Gauge,
           ncmE1IntervalOOFSs
                 Gauge,
           ncmE1IntervalCVPath
                 Gauge,
           ncmE1IntervalCVLine
                 Gauge,
           ncmE1IntervalESsLine
                 Gauge,
           ncmE1IntervalSESsLine
                 Gauge,
           ncmE1IntervalFECPath
                 Gauge,
           ncmE1IntervalFECLine
                 Gauge
			     
	 }

        ncmE1IntervalNIDIndex OBJECT-TYPE
             SYNTAX  INTEGER (1..'7fffffff'h)
             ACCESS  read-only
             STATUS  mandatory
             DESCRIPTION
                "The object specifies the node id index."
            ::= { ncmE1IntervalEntry 1 }
 

	 ncmE1IntervalIndex OBJECT-TYPE
	     SYNTAX  INTEGER (1..'7fffffff'h)
	     ACCESS  read-only
	     STATUS  mandatory
	     DESCRIPTION
		"The index value which uniquely identifies the
		DS1 interface to which this entry is applicable.
		The interface identified by a particular
		value of this index is the same interface as
		identified by the same value as a LineIndex
		object instance."
	    ::= { ncmE1IntervalEntry 2 }


          ncmE1IntervalEndType OBJECT-TYPE
            SYNTAX INTEGER {
                       near-End(1),
                       far-End(2)
                        }
           ACCESS read-only
           STATUS mandatory
           DESCRIPTION
           "The variable specifies the End Type."
 
          ::= { ncmE1IntervalEntry 3 }
 

        ncmE1IntervalNumber OBJECT-TYPE
             SYNTAX  INTEGER (1..96)
             ACCESS  read-only
             STATUS  mandatory
             DESCRIPTION
                "A number between 1 and 96, where 1 is the most
                recently completed 15 minute interval and 96 is
                the least recently completed 15 minutes interval
                (assuming that all 96 intervals are valid)."
            ::= { ncmE1IntervalEntry 4 }
 

    	  ncmE1IntervalASs OBJECT-TYPE
	     SYNTAX  Gauge
	     ACCESS  read-only
	     STATUS  mandatory
	     DESCRIPTION
		"The number of Available Seconds encountered by a
		DS1 interface in one of the previous 96, individual
		15 minute, intervals."
	    ::= { ncmE1IntervalEntry 5 }



     	 ncmE1IntervalUASs OBJECT-TYPE
	     SYNTAX  Gauge
	     ACCESS  read-only
	     STATUS  mandatory
	     DESCRIPTION
		"The number of Unavailable Seconds encountered
		by a DS1 interface in one of the previous
		96, individual 15 minute, intervals."
	    ::= { ncmE1IntervalEntry 6 }


     	 ncmE1IntervalESs OBJECT-TYPE
	     SYNTAX  Gauge
	     ACCESS  read-only
	     STATUS  mandatory
	     DESCRIPTION
		"The number of Error Seconds encountered
		by a DS1 interface in one of the previous
		96, individual 15 minute, intervals."
	    ::= { ncmE1IntervalEntry 7 }


     	 ncmE1IntervalSESs OBJECT-TYPE
	     SYNTAX  Gauge
	     ACCESS  read-only
	     STATUS  mandatory
	     DESCRIPTION
		"The number of Severe Error Seconds encountered
		by a DS1 interface in one of the previous
		96, individual 15 minute, intervals."
	    ::= { ncmE1IntervalEntry 8 }



    	
	  ncmE1IntervalFAE OBJECT-TYPE
	     SYNTAX  Gauge
	     ACCESS  read-only
	     STATUS  mandatory
	     DESCRIPTION
		"The number of Frame Alignment Errors encoun-
		tered by a DS1 interface in one of the previous
		96, individual 15 minute, intervals."
	    ::= { ncmE1IntervalEntry 9 }


		
    	  ncmE1IntervalBBE OBJECT-TYPE
	     SYNTAX  Gauge
	     ACCESS  read-only
	     STATUS  mandatory
	     DESCRIPTION
		"The number of Background Block Errors encoun-
		tered by a DS1 interface in one of the previous
		96, individual 15 minute, intervals."
	    ::= { ncmE1IntervalEntry 10 }


	 ncmE1IntervalSEFSs OBJECT-TYPE
             SYNTAX  Gauge
             ACCESS  read-only
             STATUS  mandatory
             DESCRIPTION
                "The number of Severly Error Framing seconds  encountered
                by a DS1 interface in one of the previous
                96, individual 15 minute, intervals."
            ::= { ncmE1IntervalEntry 11 }


        ncmE1IntervalAISSs OBJECT-TYPE
             SYNTAX  Gauge
             ACCESS  read-only
             STATUS  mandatory
             DESCRIPTION
                "The number of ncmAlarm Indication Signal seconds  encountered
                by a DS1 interface in one of the previous
                96, individual 15 minute, intervals."
            ::= { ncmE1IntervalEntry 12 }

        ncmE1IntervalLOSSs OBJECT-TYPE
             SYNTAX  Gauge
             ACCESS  read-only
             STATUS  mandatory
             DESCRIPTION
                "The number of Loss of Signal seconds  encountered
                by a DS1 interface in one of the previous
                96, individual 15 minute, intervals."
            ::= { ncmE1IntervalEntry 13 }
               
 
        ncmE1IntervalLOFSs OBJECT-TYPE
             SYNTAX  Gauge
             ACCESS  read-only
             STATUS  mandatory
             DESCRIPTION
                "The number of Loss of Signal seconds  encountered
                by a DS1 interface in one of the previous
                96, individual 15 minute, intervals."
            ::= { ncmE1IntervalEntry 14 }
               
 
        ncmE1IntervalOOFSs OBJECT-TYPE
             SYNTAX  Gauge
             ACCESS  read-only
             STATUS  mandatory
             DESCRIPTION
                "The number of Out Of Frame seconds  encountered
                by a DS1 interface in one of the previous
                96, individual 15 minute, intervals."
            ::= { ncmE1IntervalEntry 15 }
               
        ncmE1IntervalCVPath OBJECT-TYPE
             SYNTAX  Gauge
             ACCESS  read-only
             STATUS  mandatory
             DESCRIPTION
                "The number of Code Violation Path encountered
                by a DS1 interface in one of the previous
                96, individual 15 minute, intervals.
		This object only has significance for QUAD cards."
            ::= { ncmE1IntervalEntry 16 }


	 ncmE1IntervalCVLine OBJECT-TYPE
             SYNTAX  Gauge
             ACCESS  read-only
             STATUS  mandatory
             DESCRIPTION
                "The number of Code Violation Line encountered
                by a DS1 interface in one of the previous
                96, individual 15 minute, intervals.
		This object only has significance for QUAD cards."
            ::= { ncmE1IntervalEntry 17 }
               
 
        ncmE1IntervalESsLine OBJECT-TYPE
             SYNTAX  Gauge
             ACCESS  read-only
             STATUS  mandatory
             DESCRIPTION
                "The number of Errored Seconds Line encountered
                by a DS1 interface in one of the previous
                96, individual 15 minute, intervals."
            ::= { ncmE1IntervalEntry 18 }
               
        ncmE1IntervalSESsLine OBJECT-TYPE
             SYNTAX  Gauge
             ACCESS  read-only
             STATUS  mandatory
             DESCRIPTION
                "The number of Severely Errored Seconds Line encountered
                by a DS1 interface in one of the previous
                96, individual 15 minute, intervals."
            ::= { ncmE1IntervalEntry 19 }
               
 
 
        ncmE1IntervalFECPath OBJECT-TYPE
             SYNTAX  Gauge
             ACCESS  read-only
             STATUS  mandatory
             DESCRIPTION
                "The number of Failure Event Count Path encountered
                by a DS1 interface in one of the previous
                96, individual 15 minute, intervals."
            ::= { ncmE1IntervalEntry 20 }
               
 
 
        ncmE1IntervalFECLine OBJECT-TYPE
             SYNTAX  Gauge
             ACCESS  read-only
             STATUS  mandatory
             DESCRIPTION
                "The number of Failure Event Count Line encountered
                by a DS1 interface in one of the previous
                96, individual 15 minute, intervals."
            ::= { ncmE1IntervalEntry 21 }


  

	--    ncm  E1 Total Group 24 hour Interval
 
         ncmE1TotalTable OBJECT-TYPE
             SYNTAX  SEQUENCE OF NcmE1TotalEntry
             ACCESS  not-accessible
             STATUS  mandatory
	     --DEFAULT test-function    acee1-test
             --DEFAULT next-function    acee1-next

            ::= { ncm-generic 1012 }
 
 
         ncmE1TotalEntry OBJECT-TYPE
             SYNTAX  NcmE1TotalEntry
             ACCESS  not-accessible
             STATUS  mandatory
             DESCRIPTION
                "An entry in the  Total Entry table."
            INDEX   { ncmE1TotalNIDIndex, ncmE1TotalIndex, ncmE1TotalEndType }
            ::= { ncmE1TotalTable 1 }
 
 
 
 
         NcmE1TotalEntry ::=
         SEQUENCE {
           ncmE1TotalNIDIndex
                INTEGER,
           ncmE1TotalIndex
                 INTEGER,
	   ncmE1TotalEndType
		 INTEGER,
	   ncmE1NumberOfValidInterval
		INTEGER,
           ncmE1TotalASs
                 Gauge,
           ncmE1TotalUASs
                 Gauge,
           ncmE1TotalESs
                 Gauge,
           ncmE1TotalSESs
                 Gauge,
           ncmE1TotalFAE
                 Gauge,
           ncmE1TotalBBE
                 Gauge,
	   ncmE1TotalSEFSs
                 Gauge,
           ncmE1TotalAISSs
                 Gauge,
           ncmE1TotalLOSSs
                 Gauge,
           ncmE1TotalLOFSs
                 Gauge,
           ncmE1TotalOOFSs
                 Gauge,
           ncmE1TotalCVPath
                 Gauge,
           ncmE1TotalCVLine
                 Gauge,
           ncmE1TotalESsLine
                 Gauge,
           ncmE1TotalSESsLine
                 Gauge,
           ncmE1TotalFECPath
                 Gauge,
           ncmE1TotalFECLine
                 Gauge

                         
             }
 
        ncmE1TotalNIDIndex OBJECT-TYPE
             SYNTAX  INTEGER (1..'7fffffff'h)
             ACCESS  read-only
             STATUS  mandatory
             DESCRIPTION
                "The object specifies the node id index of the node."
            ::= { ncmE1TotalEntry 1 }
 


