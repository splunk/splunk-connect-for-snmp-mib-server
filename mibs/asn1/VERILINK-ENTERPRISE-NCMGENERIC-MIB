--	ncm ID Prom Group


	ncmIDPromTable OBJECT-TYPE
	     SYNTAX  SEQUENCE OF NcmIDPromEntry
	     ACCESS  not-accessible
	     STATUS  mandatory
	     --DEFAULT test-function    card-test
             --DEFAULT next-function    cardsync-next
 


	    ::= { ncm-generic  1007 }


	ncmIDPromEntry OBJECT-TYPE
	     SYNTAX NcmIDPromEntry
	     ACCESS  not-accessible
	     STATUS  mandatory
	     DESCRIPTION
		"An entry in the ncm ID Prom Table."
	    -- INDEX   { ncmIDPromIndex }
	    INDEX   { ncmIDPromNIDIndex, ncmIDPromIndex }
	    ::= { ncmIDPromTable 1 }


	NcmIDPromEntry ::=
	 SEQUENCE {
	     ncmIDPromNIDIndex
		 INTEGER,
	     ncmIDPromIndex
		 INTEGER,
	     ncmCardType
		INTEGER,
	     ncmCardRevision
		DisplayString,
	     ncmCardDate
		DisplayString,
	     ncmCardSerialNumber
		DisplayString, 
             ncmManufacturePartNumber
		DisplayString, 
	     ncmVendorCageCode
		DisplayString, 
	     ncmIDCimType
		INTEGER,
	     ncmCimRevision
		DisplayString,
	     ncmCimDate
		DisplayString, 
	     ncmCimSerialNumber
		DisplayString,
	     ncmCimManufacturePartNumber
		DisplayString, 
	     ncmCimVendorCageCode
		DisplayString  

		}


        ncmIDPromNIDIndex OBJECT-TYPE
             SYNTAX  INTEGER (1..'7fffffff'h)
             ACCESS  read-only
             STATUS  mandatory
             DESCRIPTION
                "This variable indicates the node id
	         value of the node."  
	    ::= { ncmIDPromEntry 1 }


	ncmIDPromIndex OBJECT-TYPE
	     SYNTAX  INTEGER (1..'7fffffff'h)
	     ACCESS  read-only
	     STATUS  mandatory
	     DESCRIPTION
		"This object is the identifier of an Interface
		on a managed device.  If there is an ifEntry
		that is directly associated with this and
		only this DS1 interface, it should have the
		same value as ifIndex.  Otherwise, the value
		exceeds ifNumber,  and is a unique identifier
		following this rule: inside interfaces (e.g.,
		equipment side) with even numbers and outside
		interfaces (e.g., network side) with odd
		numbers."
	    ::= { ncmIDPromEntry 2 }


	ncmCardType OBJECT-TYPE
	    SYNTAX INTEGER {
			ace-card(1),
			quad-card-for-imux(2),
			dual-csu(3)
			   }
	    ACCESS  read-only
	    STATUS  mandatory
	    DESCRIPTION
	    "This variable indicates the ncmCard type of the card." 
	    ::= { ncmIDPromEntry  3 }




	ncmCardRevision OBJECT-TYPE
	    SYNTAX DisplayString (SIZE (0..255)) 
	    ACCESS  read-only
	    STATUS  mandatory
	    DESCRIPTION
	    "This variable indicates the revision number for the card." 
	    ::= { ncmIDPromEntry  4 }

	

	ncmCardDate OBJECT-TYPE
	    SYNTAX DisplayString (SIZE (0..255)) 
	    ACCESS  read-only
	    STATUS  mandatory
	    DESCRIPTION
	    "This variable indicates the date (mm-dd-yy)
                for the card." 
	    ::= { ncmIDPromEntry  5 }


	ncmCardSerialNumber OBJECT-TYPE
	    SYNTAX  DisplayString (SIZE (0..255))
	    ACCESS  read-only
	    STATUS  mandatory
	    DESCRIPTION
	    "This variable indicates the Serial Number for the card." 
	    ::= { ncmIDPromEntry  6 }


	ncmManufacturePartNumber OBJECT-TYPE
	    SYNTAX  DisplayString (SIZE (0..255))
	    ACCESS  read-only
	    STATUS  mandatory
	    DESCRIPTION
	    "This variable indicates the Manufacture Part Number for the
                card." 
	    ::= { ncmIDPromEntry  7 }

	
	ncmVendorCageCode OBJECT-TYPE
	    SYNTAX  DisplayString (SIZE (0..255))
	    ACCESS  read-only
	    STATUS  mandatory
	    DESCRIPTION
	    "This variable indicates the Vendor Cage Code for the card." 
	    ::= { ncmIDPromEntry  8 }


	ncmIDCimType OBJECT-TYPE
	    SYNTAX INTEGER {
				cIM29004(16),
                                cIM29005(17),
                                cIM29006(18),
                                cIM29007(19),
                                cIM29008(20),
                                cIM29009(21),
                                cIM29010(32),
                                cIM29011(33),
                                cIM-2164(34),
                                cIM-2168(35),
                                cIM29003(36),
                                cIM29002(56),
                                cIM29001(37),
                                nIM2000(22),
                                cIM-UNINSTALLED(255)
	
			  } 
	    ACCESS  read-only
	    STATUS  mandatory
	    DESCRIPTION
		"This variable indicates the cim Type for the card." 
