             SYNTAX  INTEGER
             ACCESS  read-write
             STATUS  mandatory
             DESCRIPTION
             "The variable specifies the timeout value in seconds for
                 repeater loopback."
            ::= { ncmT1ConfigEntry 13 }
 
 
 
        ncmT1UserAlarmDeclareTime OBJECT-TYPE
             SYNTAX  INTEGER
             ACCESS  read-write
             STATUS  mandatory
             DESCRIPTION
            "The variable specifies the User ncmAlarm declaration Time in
                 seconds."
            ::= { ncmT1ConfigEntry 14 }
 
         ncmT1State  OBJECT-TYPE
             SYNTAX  INTEGER {
                        out-of-service(1),
                        in-service(2)
                              }
             ACCESS  read-write
             STATUS  mandatory
             DESCRIPTION
          "The variable specifies whether port state is in service or
               not."
            ::= { ncmT1ConfigEntry 15 }
 
         
          ncmT1NetworkLBOEQLIZ OBJECT-TYPE
            SYNTAX INTEGER {
                        bldLn-0db-FT-0-133(1),
                        bldLnNeg7d5db-FT-133-266(2),
                        bldLnNeg15db-FT-266-399(3),
                        bldLnNeg22d5-FT-399-533(4),
                	fT-533-655(5)
 
                        }
            ACCESS  read-write
            STATUS  mandatory
            DESCRIPTION
            "This variable indicates either
		(1) for long haul -- the db rate at which the line or port
                    using, i.e., is 0 , -7.5db, -15db or -22.5db.[pvt];
		or (2) for short haul -- the equalization at which the line 
		    or port using, i.e., is 0~133, 133~266, 266~399, 399~533
		    533~655 ft., which is only applicable in port 3 & 4 of 
		    dual csu card."
            ::= { ncmT1ConfigEntry 16 }


	   ncmT1TimeSlot  OBJECT-TYPE
             SYNTAX  DisplayString
             ACCESS  read-only
             STATUS  mandatory
             DESCRIPTION
             "This variable represents the timeslots used for the given
                  port directly from the hardware crosspoint switch for
                  example 1-10 if slot 1 to 10 are selected and 1,3,5 if 1,3
                  and 5 are selected."
 
             ::= { ncmT1ConfigEntry 17 }
 
        ncmT1TimeSlotSelect OBJECT-TYPE
            SYNTAX INTEGER
            ACCESS  read-write
            STATUS  mandatory
            DESCRIPTION
