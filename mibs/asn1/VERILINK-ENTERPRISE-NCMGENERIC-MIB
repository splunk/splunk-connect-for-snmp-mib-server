--      ncm Advanced T1 Port Configuration Table

         ncmAdvancedT1ConfigTable OBJECT-TYPE
        SYNTAX  SEQUENCE OF NcmAdvancedT1ConfigEntry
        ACCESS  not-accessible
        STATUS  mandatory
	--DEFAULT test-function    ace-test
        --DEFAULT next-function    ace-next


         ::= { ncm-generic 1024  }


         ncmAdvancedT1ConfigEntry OBJECT-TYPE
        SYNTAX  NcmAdvancedT1ConfigEntry
        ACCESS  not-accessible
        STATUS  mandatory
        DESCRIPTION
        "An entry in the Advanced T1 Configuration table  ."
         INDEX   { ncmAdvancedT1NIDIndex, ncmAdvancedT1LineIndex }
         ::= { ncmAdvancedT1ConfigTable 1 }


         NcmAdvancedT1ConfigEntry ::=
         SEQUENCE {
	
	   ncmAdvancedT1NIDIndex
		 INTEGER,
           ncmAdvancedT1LineIndex
                 INTEGER,
           ncmadvfdlMode
                 INTEGER,
           ncmadvfdlStandard
                INTEGER,
           ncmadvfdlPerformanceReport
                INTEGER,
           ncmadvfdlLBEnable
                INTEGER,
           ncmadvfdlLLBT1BOPMsg
                INTEGER,
           ncmadvfdlPLBT1BOPMsg
                INTEGER,
           ncmadvfdlIdlePattern
                INTEGER,
           ncmadvfdlMonitoringCsuType
                INTEGER
                }


	 ncmAdvancedT1NIDIndex OBJECT-TYPE
          SYNTAX  INTEGER (1..'7fffffff'h)
          ACCESS  read-only
          STATUS  mandatory
          DESCRIPTION
         "The object specifies the node id of the node."
           
          ::= { ncmAdvancedT1ConfigEntry 1 }



	 ncmAdvancedT1LineIndex OBJECT-TYPE
          SYNTAX  INTEGER (1..'7fffffff'h)
          ACCESS  read-only
          STATUS  mandatory
          DESCRIPTION
         "The index value which uniquely identifies  the
          interface to which this entry is applicable."
           
          ::= { ncmAdvancedT1ConfigEntry 2 }
 
 
         ncmadvfdlMode OBJECT-TYPE
          SYNTAX INTEGER {
           
                       terminated(1),
                       pass(2)
 
               } 
          ACCESS read-write
          STATUS mandatory
          DESCRIPTION
          "The variable specifies the fdl mode which is bit 0 of  fdl
           Parameter1."
 
          ::= { ncmAdvancedT1ConfigEntry 3 }
 
 
         ncmadvfdlStandard OBJECT-TYPE
          SYNTAX INTEGER {
           
                       type-54016(1),
                       type-T1-403(2),
                       type-both(3)
 
               } 
          ACCESS read-write
          STATUS mandatory
          DESCRIPTION
          "The variable specifies the fdl standard which is bits 1
          and bit 2 of fdl Parameter1."
            
          ::= { ncmAdvancedT1ConfigEntry 4 }
 


	ncmadvfdlPerformanceReport OBJECT-TYPE
          SYNTAX INTEGER {
           
                       disable(1),
                       user(2),
                       telco(3)
        
               } 
          ACCESS read-write
          STATUS mandatory
          DESCRIPTION
          "The variable specifies the fdl standard which is bits 3
          and bit 4 of fdl Parameter1."
          ::= { ncmAdvancedT1ConfigEntry 5 }
 
 
 
         ncmadvfdlLBEnable OBJECT-TYPE
          SYNTAX INTEGER {
           
                       disable(1),
                       enable(2)
 
               } 
          ACCESS read-write
          STATUS mandatory
          DESCRIPTION
          "The variable specifies the Disable/Enable of the fdl Loopback
           which is bit 0 of the fdl-loop"
           ::= { ncmAdvancedT1ConfigEntry 6 }
 
 
	 ncmadvfdlLLBT1BOPMsg OBJECT-TYPE
          SYNTAX INTEGER {
           
                       disable(1),
                       enable(2)
 
               } 
          ACCESS read-write
          STATUS mandatory
