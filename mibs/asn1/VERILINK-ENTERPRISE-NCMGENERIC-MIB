                INTEGER,
             ncmESExceeded
                INTEGER,
	     ncmSESExceeded
                INTEGER,
	     ncmLOFThresholdCountst
		INTEGER,
	     ncmLOFTimeIntervalst
		INTEGER,
	      ncmLOSThresholdCountst
		INTEGER,
	      ncmLOSTimeIntervalst
		INTEGER,
	      ncmRAIThresholdCountst
		INTEGER,
	      ncmRAITimeIntervalst
		INTEGER,
	      ncmAISThresholdCountst
		INTEGER,
	      ncmAISTimeIntervalst
		INTEGER,
	     ncmBPVThresholdCountst
		INTEGER,
	     ncmBPVTimeIntervalst
		INTEGER,
	    ncmES15MINThresholdCountst
		INTEGER,
	    ncmES24HRThresholdCountst
		INTEGER,
	     ncmSES15MINThresholdCountst
		INTEGER,
	     ncmSES24HRThresholdCountst
		INTEGER,
	     ncmResetAlmThresholdCount
		INTEGER

	}

	     

