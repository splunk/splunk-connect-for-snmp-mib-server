            ::= { ncmDataPortConfigTable 1 }


        NcmDataPortConfigEntry ::=
         SEQUENCE {
           ncmDataPortConfigNIDIndex
                INTEGER,
           ncmDataPortConfigIndex
                 INTEGER,
           ncmDataPortConfigMode
                INTEGER,
           ncmDataPortConfigLineIndicate
                INTEGER,
