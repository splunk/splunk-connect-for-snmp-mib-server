--      The ncm  E1 Previous Total Group 24 hour Interval
 
        ncmE1PrevTotalTable OBJECT-TYPE
             SYNTAX  SEQUENCE OF NcmE1PrevTotalEntry
             ACCESS  not-accessible
             STATUS  mandatory
	     --DEFAULT test-function    acee1-test
             --DEFAULT next-function    acee1-next

            ::= { ncm-generic 1013 }
 
 
         ncmE1PrevTotalEntry OBJECT-TYPE
             SYNTAX  NcmE1PrevTotalEntry
             ACCESS  not-accessible
             STATUS  mandatory
             DESCRIPTION
                "An entry in the  Previous day total  table."
            INDEX   { ncmE1PrevTotalNIDIndex, ncmE1PrevTotalIndex, ncmE1PrevTotalEndType }
            ::= { ncmE1PrevTotalTable 1 }
 
 
        NcmE1PrevTotalEntry ::=
