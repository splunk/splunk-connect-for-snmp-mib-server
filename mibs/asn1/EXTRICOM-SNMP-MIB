-- =====================================================================
--
-- channel table
--

channel OBJECT-TYPE
SYNTAX SEQUENCE OF ChannelEntry
MAX-ACCESS not-accessible
STATUS current
DESCRIPTION
"A table that contains information about every channel"
::= { mibObjects 3 }

channelEntry OBJECT-TYPE
SYNTAX ChannelEntry
MAX-ACCESS not-accessible
STATUS current
DESCRIPTION
"A list of the channel parameters. This is indexed
by the index of the channel."
INDEX { channelIndex }
::= { channel 1 }

ChannelEntry ::=
SEQUENCE {
  channelIndex 		                        Integer32,
  mode                                      Integer32,
  channelNum      	                        Integer32,
  retriesNum                               Integer32, 
  macAddress                               DisplayString,   
  bssidMask                                DisplayString,
  disabledChannel	                        Integer32,     
  basicRates                               DisplayString,
  optionalRates                            DisplayString,
  reuseIndex                               Integer32,     
  adaptationRates                          DisplayString,
  rateAdaptationDll                       Integer32,     
  useShortPreamble                        Integer32,     
  forceShortPreamble                      Integer32,  
  rts                                       Integer32,
  extBasicRates                           DisplayString,
  extOptionalRates                        DisplayString,
  vsRadioIndex                            Integer32,
  vsApBitmap                              DisplayString,
  ht40m                                    Integer32,
  htSecondaryChannelOffset               Integer32,
  htNOnly                                 Integer32,
  htSgi                                    Integer32,
  htTxMcs                                 Integer32,
  useWmmCh                                Integer32,
  wmmAckPolicyCh                         Integer32,
  loadBalancing                            Integer32,
  loadBalancingAllowedDiff               Integer32,
  loadBalancingRetiresBeforeFreepass    Integer32,
  noHwTkip                                Integer32,
  noHwRc4                                 Integer32,
  dscp                                      Integer32,
  wmmToDscp                               Integer32,
  htProtection                             Integer32,
  olbcRssi                                 Integer32,
  allowTripleChain                        Integer32,
  legacyChainMask                         Integer32,
  singleStreamChainMask                  Integer32,
  dualStreamChainMask                    Integer32,
  ignoreRatesNum                          Integer32,
  addBRates                               Integer32,  
  channelRowStatus                        RowStatus
}


channelIndex OBJECT-TYPE
SYNTAX Integer32 (1..6)
MAX-ACCESS not-accessible
STATUS current
DESCRIPTION
"The unique identifier of the channel."
::= { channelEntry 1 }


mode OBJECT-TYPE
SYNTAX Integer32 (0..10)
MAX-ACCESS read-write
STATUS current
DESCRIPTION
