EXTRICOM-SNMP-MIB DEFINITIONS ::= BEGIN


IMPORTS
    MODULE-IDENTITY,Integer32,Counter32,enterprises,IpAddress,NOTIFICATION-TYPE,OBJECT-TYPE
    	FROM SNMPv2-SMI
    NOTIFICATION-GROUP,OBJECT-GROUP
    	FROM SNMPv2-CONF
    DisplayString,RowStatus,TruthValue,MacAddress
    	FROM SNMPv2-TC;

extricomSnmpMIB MODULE-IDENTITY

LAST-UPDATED "201005170000Z"
ORGANIZATION "www.extricom.com"
CONTACT-INFO    
"info@extricom.com"
DESCRIPTION
"Octopus Wireless switch MIB"
::= { octopus 1 }



-- mib            OBJECT IDENTIFIER ::= { mgmt 1  }
