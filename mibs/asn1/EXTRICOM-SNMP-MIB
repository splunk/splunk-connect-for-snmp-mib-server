-- 24hr Switch Statistics

pm24hrswitchStat  OBJECT IDENTIFIER  ::= { performance24hr 5 } 

pm24hrswitchStatTable OBJECT-TYPE
SYNTAX SEQUENCE OF Pm24hrSwitchStatEntry
MAX-ACCESS not-accessible
STATUS current
DESCRIPTION
"A table that contains Switch global statistics"
::= { pm24hrswitchStat 1 }

pm24hrSwitchStatEntry OBJECT-TYPE
SYNTAX Pm24hrSwitchStatEntry
MAX-ACCESS not-accessible
STATUS current
DESCRIPTION
"N/A"
INDEX { pm24hrSwtimeIx }
::= { pm24hrswitchStatTable 1 }

-- INDEX { pm24hrswitchStatIndex, pm24hrSwtimeIx }


Pm24hrSwitchStatEntry ::=
SEQUENCE {
  pm24hrswitchStatIndex               Integer32,
  pm24hrSwtimeIx	               	Integer32,
  pm24hrRxPacket			Integer32,
  pm24hrTxPacket 			Integer32,
  pm24hrRxOctet 			Integer32,
  pm24hrTxOctet 			Integer32,
  pm24hrTxFailedOctet 			Integer32,
  pm24hrTxFailedPacket 			Integer32,
  pm24hrTxRetansFail 			Integer32,
  pm24hrTxRetansFailOctet 		Integer32,
  pm24hrRxMcastOctet 			Integer32,
  pm24hrRxMcastPacket 			Integer32,
  pm24hrTxMcastPacket 			Integer32,
  pm24hrTxMcastOctet 			Integer32,
  pm24hrTxDropedCount 			Integer32,
  pm24hrClientsCount 			Integer32,
  pm24hrRtsSucces 			Integer32,
  pm24hrRtsFailed 			Integer32,
  pm24hrRxLanPacket			Integer32,
  pm24hrTxLanPacket			Integer32,
  pm24hrRxLanOctet			Integer32,
  pm24hrTxLanOctet			Integer32,
  pm24hrRxLanMcastPacket		Integer32,
  pm24hrTxLanError			Integer32,
  pm24hrTxLanDropped 			Integer32,
  pm24hrRxLanDropped 			Integer32,
  pm24hrTxLanCollission 		Integer32,
  pm24hrRxLanFrame 			Integer32,
  pm24hrTxLanCarrier 			Integer32,
  pm24hrSwTimeStamp		        Integer32
}
