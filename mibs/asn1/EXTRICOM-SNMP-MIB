essid OBJECT-TYPE
SYNTAX SEQUENCE OF EssidEntry
MAX-ACCESS not-accessible
STATUS current
DESCRIPTION
"A table that contains information about every essid"
::= { mibObjects 4 }


essidEntry OBJECT-TYPE
SYNTAX EssidEntry
MAX-ACCESS not-accessible
STATUS current
DESCRIPTION
"A list of the essid parameters. This is indexed
by the index of the essid."
INDEX { essidIndex }
::= { essid 1 }

EssidEntry ::=
SEQUENCE {
  essidIndex                Integer32,
  name                      DisplayString,
  channelIndex             Integer32,
  bssid                     DisplayString,
  vlanIndex                Integer32,
  useTimPeriod            Integer32,
  disassocTime             Integer32,
  allowAnyEssid           Integer32,
  bcastEssid               Integer32,
  allowStoreAndForward   Integer32,
  allowInterEssForward   Integer32,
  arpCaching               Integer32,
  enableMulticastDup      Integer32,
  enableDynamicBssid      Integer32,
  usePwrMnt               Integer32,
  usPsQMaxSize          Integer32,
  voice                     Integer32,
  useAccessList           Integer32,
  authServer1              Integer32,
  authServer2              Integer32,
  authServer3              Integer32,
  authServer4              Integer32,
  useSecurity              Integer32,
  cipherWep                Integer32,
  cipherWep104             Integer32,
  cipherTkip               Integer32,
  cipherCcmp               Integer32,
  useCipher                Integer32,
  useCrypt                 Integer32,
  useAuth                  Integer32,
  groupCipher              Integer32,
  useWpa                   Integer32,
  auth1x                   Integer32,
  authForce1x              Integer32,
  authWpa                  Integer32,
  authPsk                  Integer32,
  defaultBcastKeyid       Integer32,
  wpa2Only                 Integer32,
  useWpa2                  Integer32,
  aesOnly                  Integer32,
  groupKeyTtl             Integer32,  
  essidEnable80211d       Integer32,
  requiredBeaconsPercentage Integer32,
  aeroscoutMuSupport      Integer32,
  useWmm                   Integer32,
  wmmAckPolicy            Integer32,
  newEssid                 Integer32,
  inBandManagement        Integer32,
  captivePortalEnable     Integer32,
  macAuthServer           Integer32,
  useGre                   Integer32,
  pinpointDhcpClient      Integer32,
  eapolLowestRateEnable  Integer32,
  secMode                  Integer32,
  allowGroupRekey         Integer32,
  handleEapolStart        Integer32,
  handleEapolDelay        Integer32,
  handleEapolPending      Integer32,
  allowEapolData          Integer32,
  allowCountermeasures     Integer32,  
  essRowStatus            RowStatus
}

