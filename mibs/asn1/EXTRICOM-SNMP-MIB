trapMibTrapDescription OBJECT-TYPE
	SYNTAX INTEGER {  
	  notify(1),
	  minor(2),
      major(3)
	} 
	MAX-ACCESS read-only
	STATUS current
	DESCRIPTION 	 "Trap description."
	::= { mibObjects 1 }       
	
trapMibTrapSeverity OBJECT-TYPE
	SYNTAX INTEGER {  
	  info(1),
	  error(2),
