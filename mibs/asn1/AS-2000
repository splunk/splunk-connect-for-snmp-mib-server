--=============================================================================
-- IPX objects
--=============================================================================

	psIpxStatus OBJECT-TYPE
	    SYNTAX INTEGER 
	  --      {
	--	        ip(1),
          --      ipx(2),
          --      both(3)
          --  }
	    ACCESS read-write
	    STATUS mandatory
        DESCRIPTION
            "Which protocol is supported, i.e, IP or IPX or both IP and IPX."
        ::= { orinocoIPX 1 }

	psIpxDefaultRouterNodeNum OBJECT-TYPE
	    SYNTAX PhysAddress
        ACCESS read-write
	    STATUS mandatory
        DESCRIPTION
            "If the source IPX network number is not equal to the destination 
            IPX Network number in the packet from wireless side to the wired 
            side, then replace the destination Ether MAC addr with the value of 
            this object (Default IPX router node number)."
        ::= { orinocoIPX 2 }

	psIpxWiredToWlessBridged OBJECT-TYPE
	    SYNTAX Counter
        ACCESS read-only
	    STATUS mandatory
        DESCRIPTION
            "Number of packets bridged from wired side to wireless side."
        ::= { orinocoIPX 3 }

	psIpxWlessToWiredBridged OBJECT-TYPE
	    SYNTAX Counter
        ACCESS read-only
	    STATUS mandatory
        DESCRIPTION
            "Number of packets bridged from wiredless side to wired side."
        ::= { orinocoIPX 4 }

	psIpxEthernetEncapsulationFormat OBJECT-TYPE
	    SYNTAX INTEGER 
	  --      {
	  --          ethII802dot3encapsulation(1),
          --      eth802dot3withSnapHdr(2),
          --      eth802dot3woSnapHdr(3)
          --  }
        ACCESS read-write
	    STATUS mandatory
        DESCRIPTION
            "Encapsulation type to be used for the IPX packets sent in wired 
            side"
        ::= { orinocoIPX 5 }

psASClientTable OBJECT-TYPE
        SYNTAX SEQUENCE OF PsASClientEntry
        ACCESS not-accessible
        STATUS mandatory
        DESCRIPTION
           "The (conceptual) table listing the ASClients (IPX) connected to the 
           access server 2000 device."
        ::= { orinocoIPX 6 }

    psASClientEntry OBJECT-TYPE
        SYNTAX PsASClientEntry
        ACCESS not-accessible
        STATUS mandatory
        DESCRIPTION
           "An entry (conceptual row) representing a ASClient (IPX) connnected to
           the Access Server 2000 device."
        INDEX {psPPPSessionId}
        ::= { psASClientTable 1 }

    PsASClientEntry ::=
        SEQUENCE 
            {
                psPPPSessionId       INTEGER,
                psIpxClientNetworkNumber    OCTET STRING,
                psIpxClientMAC          PhysAddress,
                psIpxClientStatus			INTEGER,
                psASClientSessionUpTime		TimeTicks,
                psASClientPacketsIn			Counter,
                psASClientPacketsOut		Counter
            }

    psPPPSessionId OBJECT-TYPE
        SYNTAX INTEGER 
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
            "The PPP Session ID assigned by the AS-2000 to this 
            ASClient."
        ::= { psASClientEntry 1 }

    psIpxClientNetworkNumber OBJECT-TYPE
        SYNTAX OCTET STRING
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
            "The IPX Node Number assigned by the Novell Server to 
            this IPX client."
        ::= { psASClientEntry 2 }

    psIpxClientMAC OBJECT-TYPE
        SYNTAX PhysAddress
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
            "The Wireless MAC of this IPX Client."
        ::= { psASClientEntry 3 }

    psIpxClientStatus  OBJECT-TYPE
        SYNTAX INTEGER
            {

                 up (1),
                disable(2),
                delete(3)
            }
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION
           "Flag to indicate whether the IPX Client is Active or 
            Inactive.  IPX Client status can also be 
            disabled/enabled/deleted."
        ::= { psASClientEntry 4 }

    psASClientSessionUpTime OBJECT-TYPE
        SYNTAX TimeTicks
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
            "This object represents the session up time for each AS Client
            or PPP virttual interface."
        ::= { psASClientEntry 5 }

    psASClientPacketsIn OBJECT-TYPE
        SYNTAX Counter
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
            "This object represents the number of packets (unicast and 
            non-unicast)recieved for each AS Client or PPP virttual 
            interface."
        ::= { psASClientEntry 6 }

    psASClientPacketsOut OBJECT-TYPE
        SYNTAX Counter
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
            "This object represents the number of packets (unicast and 
            non-unicast)sent for each AS Client or PPP virttual 
            interface."
        ::= { psASClientEntry 7 } 
        
