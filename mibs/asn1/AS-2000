--#condInclude "rfc1155.smi"
--#condInclude "rfc1212.smi"
--#condInclude "rfc1213.inc"
--#condInclude "rfc1215.smi"

--=============================================================================
-- Access Server 2000 MIBs Begin
--=============================================================================

AS-2000

DEFINITIONS ::= BEGIN

IMPORTS

    enterprises, IpAddress, Counter, TimeTicks
        FROM RFC1155-SMI

        OBJECT-TYPE
            FROM RFC-1212

        TRAP-TYPE
	        FROM RFC-1215;

        DisplayString ::=
            OCTET STRING
            -- This data type is used to model textual information taken
            -- from the NVT ASCII character set.  By convention, objects
            -- with this syntax are declared as having
            --
            --      SIZE (0..255)

        PhysAddress ::=
            OCTET STRING
            -- This data type is used to model media addresses.  For many
            -- types of media, this will be in a binary representation.
            -- For example, an ethernet address would be represented as
            -- a string of 6 octets.

--=============================================================================
-- Defining the MIB OID tree for AS-2000
--=============================================================================
 
    lucent    OBJECT IDENTIFIER ::= { enterprises 1751 }
    mibs      OBJECT IDENTIFIER ::= { lucent 2 }
    orinoco   OBJECT IDENTIFIER ::= { mibs 4 }
    as2000    OBJECT IDENTIFIER ::= { orinoco 3 }

--=============================================================================
-- Defining the Product Identification for AS-2000 (to be used for sysObjectID)
--=============================================================================

    products        OBJECT IDENTIFIER ::= { lucent 1 }
    orinocoProducts OBJECT IDENTIFIER ::= { products 4 }
    as2000Product   OBJECT IDENTIFIER ::= { orinocoProducts 4 }

--=============================================================================
-- The various groups in this agent
--=============================================================================

    orinocoInterface    OBJECT IDENTIFIER ::= { as2000 3 }
    orinocoSNMPSetup    OBJECT IDENTIFIER ::= { as2000 4 }
    orinocoPPPSetup     OBJECT IDENTIFIER ::= { as2000 5 }
    orinocoAgent        OBJECT IDENTIFIER ::= { as2000 6 } 
    radius              OBJECT IDENTIFIER ::= { as2000 7 }
    orinocoShimECPSetup OBJECT IDENTIFIER ::= { as2000 8 }
    orinocoDiagnostics  OBJECT IDENTIFIER ::= { as2000 10 }
    orinocoIAPP         OBJECT IDENTIFIER ::= { as2000 11 }
    orinocoIPX          OBJECT IDENTIFIER ::= { as2000 12 }
    orinocoLinkTest     OBJECT IDENTIFIER ::= { as2000 13 }
    accessServerTraps   OBJECT IDENTIFIER ::= { as2000 15 }

--=============================================================================
-- The sub-groups of orinocoInterface
--=============================================================================

    orinocoPHY          OBJECT IDENTIFIER ::= { orinocoInterface 2 }
    orinocoDriver       OBJECT IDENTIFIER ::= { orinocoInterface 4 }

--=============================================================================
-- The orinocoPHY sub-group of orinocoInterface
--=============================================================================

    pliSystemName OBJECT-TYPE
	    SYNTAX DisplayString
        ACCESS read-write
	    STATUS mandatory
	    DESCRIPTION				
		    "The sysName of the Access Server. This may be used for 
		    diagnostic purposes. This name will be the same as the
		    sysName of the system group of MIB II"
        ::= { orinocoPHY 1 }

    psWlanIfTable OBJECT-TYPE
	    SYNTAX SEQUENCE OF PsWlanIfEntry
	    ACCESS not-accessible
	    STATUS mandatory
	    DESCRIPTION
		    "The table contains the information about the
		    wavelan interfaces present in the Access Server."
        ::= { orinocoPHY 2 }

    psWlanIfEntry OBJECT-TYPE
	    SYNTAX PsWlanIfEntry
	    ACCESS not-accessible
	    STATUS mandatory
	    DESCRIPTION
	        "The entry in the wavelan interfac table. Described
	        next."
        INDEX { pliWlanIfIndex }
        ::= { psWlanIfTable 1 }

	PsWlanIfEntry ::=
	    SEQUENCE
	        {
	            pliWlanIfIndex                       INTEGER,
	            pliWlanIfNetworkName                 DisplayString,
	            pliWlanIfMACAddress	                 PhysAddress,
	            pliWlanIfMediumReservation           INTEGER,
	            pliWlanIfTransmitRate	             INTEGER,
	            pliWlanIfOperatingFrequency          INTEGER,
	            pliWlanIfAPDensity	                 INTEGER,
	            pliWlanIfClosedSystem 	             INTEGER,
	            pliWlanIfAllowedTransmitRates        INTEGER,
	            pliWlanIfRegulatoryDomainList        OCTET STRING,
	            pliWlanIfAllowedOperatingFrequencies INTEGER,
	            pliWlanIfCapabilityOptions           INTEGER,
	            pliWlanIfProfileCode                 INTEGER
	        }

    pliWlanIfIndex OBJECT-TYPE
        SYNTAX  INTEGER
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION
            "A unique value for each interface. The value for
			each interface must remain constant at
            least from one re-initialization of the entity's
            network management system to the next re-
            initialization."
        ::= { psWlanIfEntry 1 }
	  
	pliWlanIfNetworkName OBJECT-TYPE
	    SYNTAX DisplayString
	    ACCESS read-write
	    STATUS mandatory
	    DESCRIPTION
		    "The network name of this wavelan interface"
	    ::= { psWlanIfEntry 2 }

	pliWlanIfMACAddress	OBJECT-TYPE
	    SYNTAX PhysAddress
	    ACCESS read-only
	    STATUS mandatory
	    DESCRIPTION
		    "The MAC address of the wavelan card present in this interface."
	    ::= { psWlanIfEntry 3 }

	pliWlanIfMediumReservation 	OBJECT-TYPE
	    SYNTAX INTEGER(0..2347)
	    ACCESS read-write
	    STATUS mandatory
	    DESCRIPTION
		    "The medium reservation value. The range is 0 - 2347.
	         Default is 2347 - means disabled.
		     Medium reservation specifies the number of octets in
		     a message above which a RTS/CTS handshake is performed."
        ::= { psWlanIfEntry 4 }

	pliWlanIfTransmitRate	OBJECT-TYPE
	    SYNTAX INTEGER
		--    {
		--        one-fixed(1),
		 --       two-fixed(2),
		--        two-autofallback(3),
		--        fixed-medium(4),
		--        fixed-high(5),
		--        auto-fallback-standard(6),
		--        auto-fallback-medium(7)
          --  }
        ACCESS read-write
	    STATUS mandatory
        DESCRIPTION
		    "The transmit rate of this interface."
        ::= { psWlanIfEntry 5 }
        
	pliWlanIfOperatingFrequency OBJECT-TYPE
	    SYNTAX INTEGER(1..14)
        ACCESS read-write
        STATUS mandatory
	    DESCRIPTION
	        "The RF channel that the Access Server is working on."
        ::= { psWlanIfEntry 6 }

	pliWlanIfAPDensity	OBJECT-TYPE
	    SYNTAX INTEGER
	   --      {
	    --	     low(1),
	    --	     medium(2),
	    --	     high(3)
	    --     }
        ACCESS read-write
	    STATUS mandatory
        DESCRIPTION
		    "The AP density in the region."
	    ::= { psWlanIfEntry 7 }

	pliWlanIfClosedSystem 	OBJECT-TYPE
	    SYNTAX INTEGER
	     --   {
	     --       closed(1),
	     --       open(2)
	     --  }
	    ACCESS read-write
	    STATUS mandatory
	    DESCRIPTION
		    "This flag identifies whether the Access Server will accept 
		    associate requests in this interface, from AS-Clients having a 
		    network name of 'ANY'. The Access Server should always be a closed 
		    station and such requests should always be rejected. This flag is 
		    provided for diagnostic purposes."
        ::= { psWlanIfEntry 8 }

	pliWlanIfAllowedTransmitRates OBJECT-TYPE
	    SYNTAX INTEGER
        ACCESS read-only
        STATUS mandatory
	    DESCRIPTION
	        "This variables reflects the transmit rates that the 
		    wavelan card supports"
        ::= { psWlanIfEntry 9 }

	pliWlanIfRegulatoryDomainList OBJECT-TYPE
	    SYNTAX OCTET STRING
        ACCESS read-only
        STATUS mandatory
	    DESCRIPTION
	        "This variables reflects the regulatory domain the  wavelan card 
		    belongs to."
        ::= { psWlanIfEntry 10 }

	pliWlanIfAllowedOperatingFrequencies OBJECT-TYPE
	    SYNTAX INTEGER
        ACCESS read-only
        STATUS mandatory
	    DESCRIPTION
	        "This variables reflects the RF channels that the
		    wavelan card supports."
        ::= { psWlanIfEntry 11 }

	pliWlanIfCapabilityOptions OBJECT-TYPE
	    SYNTAX INTEGER
        ACCESS read-only
        STATUS mandatory
	    DESCRIPTION
	        "The capabilities of the card. Indicates the support of 
		    features like WEP."
        ::= { psWlanIfEntry 12 }

	pliWlanIfProfileCode OBJECT-TYPE
	    SYNTAX INTEGER
        ACCESS read-only
        STATUS mandatory
	    DESCRIPTION
	        "The varible conprises of a vendor indication (Lucent or
		    an OEM customer) and a capability indication (eg. bronze
		    or gold)."
        ::= { psWlanIfEntry 13 }

--=============================================================================
-- The orinocoDriver sub-group of orinocoInterface
--=============================================================================

	pliDriverName	OBJECT-TYPE
	    SYNTAX DisplayString
	    ACCESS read-only
	    STATUS mandatory
	    DESCRIPTION
	        "The driver name."
        ::= { orinocoDriver 1 }

	pliDriverVersion	OBJECT-TYPE
	    SYNTAX DisplayString
	    ACCESS read-only
	    STATUS mandatory
	    DESCRIPTION
	        "The driver's version number. This is a text string
	        including the major and minor release number, and, 
	        optionally, the patch level:  'mm.nn.pp', where
                    mm = major release number
                    nn = point release number
                    pp = optional patch number."
	    ::= { orinocoDriver 2 }

--=============================================================================
-- The orinocoSNMPSetup specific management objects
--=============================================================================

	psSNMPReadPassword	OBJECT-TYPE
	    SYNTAX DisplayString
	    ACCESS write-only
	    STATUS mandatory
	    DESCRIPTION
	        "The password for reading object values from the agent."
	    ::= { orinocoSNMPSetup 1 }

	psSNMPReadWritePassword	 OBJECT-TYPE
	    SYNTAX DisplayString
	    ACCESS write-only
	    STATUS mandatory
	    DESCRIPTION
	        "The password for reading and modifying object values 
	        from the agent."
        ::= { orinocoSNMPSetup 2 }

	psSNMPTrapHostIPAddress	  OBJECT-TYPE
	    SYNTAX IpAddress
	    ACCESS read-write
	    STATUS mandatory
	    DESCRIPTION
	        "The IP address of the host to which traps should be sent."
	    ::= { orinocoSNMPSetup 3 }

	psSNMPTrapHostPassword	OBJECT-TYPE
	    SYNTAX DisplayString
	    ACCESS read-write
	    STATUS mandatory
	    DESCRIPTION
	        "The password sent with the trap to enable the management
	        station to ignore unsolicited traps."
	    ::= { orinocoSNMPSetup 4 }

	psSNMPManagerCount	OBJECT-TYPE
	    SYNTAX INTEGER
	    ACCESS read-only
	    STATUS mandatory
	    DESCRIPTION
	        "The number of management stations that have been 
	        authorised to manage this Access Server."
	    ::= { orinocoSNMPSetup 5 }

	psSNMPAccessTable OBJECT-TYPE
	    SYNTAX SEQUENCE OF PsSNMPAccessTableEntry
	    ACCESS not-accessible
	    STATUS mandatory
	    DESCRIPTION
		    "The table containing the information about the
		    managers authorised to manage this Access Server."
        ::= { orinocoSNMPSetup 6 }
	
	psSNMPAccessTableEntry OBJECT-TYPE
	    SYNTAX PsSNMPAccessTableEntry
	    ACCESS not-accessible
	    STATUS mandatory
	    DESCRIPTION
	        "The entry in the SNMP access table. Described
	        next."
        INDEX { psSNMPManagerIndex }
        ::= { psSNMPAccessTable 1 }

	PsSNMPAccessTableEntry ::=
	    SEQUENCE
	        {
	            managerIndex   INTEGER,
	            managerIPAddress IpAddress,
	            managerSubnetMask  IpAddress,
	            managerStatus  INTEGER
	        }
	  
	managerIndex	OBJECT-TYPE
	    SYNTAX INTEGER
	    ACCESS read-only
	    STATUS mandatory
	    DESCRIPTION
	        "Management station identification number.
	        Is the index for the table."
	    ::= { psSNMPAccessTableEntry 1 }

	managerIPAddress	OBJECT-TYPE
	    SYNTAX IpAddress
	    ACCESS read-write
	    STATUS mandatory
	    DESCRIPTION
	        "IP address of the management station."
	    ::= { psSNMPAccessTableEntry 2 }

	managerSubnetMask	OBJECT-TYPE
	    SYNTAX IpAddress
	    ACCESS read-write
	    STATUS mandatory
	    DESCRIPTION
		    "The IP subnet  mask. Used to grant access to a complete subnet."
        ::= { psSNMPAccessTableEntry 3 }

	managerStatus	OBJECT-TYPE
	    SYNTAX INTEGER
		    {
		        enable(1),
		        delete(2)
		    }
        ACCESS read-write
	    STATUS mandatory
	    DESCRIPTION
	        "Whether the manager is enabled or disabled.
			  Used to delete the unwanted entries."
        ::= { psSNMPAccessTableEntry 4 }

	psSNMPInBadManagers OBJECT-TYPE
	    SYNTAX Counter
	    ACCESS read-only
	    STATUS mandatory
	    DESCRIPTION
	        "The no of attempts to manage this Access Server
			  by unauthorised managers."
	    ::= { orinocoSNMPSetup 7 }

	psTestSNMPReadWritePassword	 OBJECT-TYPE
	    SYNTAX DisplayString
	    ACCESS write-only
	    STATUS mandatory
	    DESCRIPTION
	        "The password for reading and modifying object values 
	        from the agent can be verified by setting this."
        ::= { orinocoSNMPSetup 8 }

--=============================================================================
-- The orinocoPPPSetup specific management objects
--=============================================================================

	psPPPIPAddressAssignmentType  OBJECT-TYPE 
	    SYNTAX INTEGER
	      --  {
		--        apm(1),
		--        dhcp-relay(2),
		--        radius(3)
		--    }
        ACCESS read-write
	    STATUS mandatory
	    DESCRIPTION
            "This identifies whether the IP addresses assigned 
            to the AS-Clients are obtained through APM (address
            pool manager), through DHCP or through the RADIUS server."
        ::= { orinocoPPPSetup 1 }

	psPPPNoOfMACIPMappingEntries  OBJECT-TYPE
	    SYNTAX INTEGER
	    ACCESS read-only
	    STATUS mandatory
	    DESCRIPTION
	        "The number of static mapping entries."
	    ::= { orinocoPPPSetup 2 }

	psPPPMACIPMappingTable  OBJECT-TYPE
	    SYNTAX SEQUENCE OF PsPPPMACIPMappingTableEntry
	    ACCESS not-accessible
	    STATUS mandatory
	    DESCRIPTION
		    "This table contains the static mapping of		
		    MAC addresses to IP addresses."	
		::= { orinocoPPPSetup 3 }
        	
    psPPPMACIPMappingTableEntry  OBJECT-TYPE
	    SYNTAX PsPPPMACIPMappingTableEntry
	    ACCESS not-accessible
	    STATUS mandatory
        DESCRIPTION
		    "The entry of the asPPPMACIPMappingTable Described below."
        INDEX { psPPPMACIPTableIndex }
        ::= { psPPPMACIPMappingTable 1 }

	PsPPPMACIPMappingTableEntry  ::=
	    SEQUENCE
	        {
	            mACIPIndex       INTEGER,
	            mACAddress  PhysAddress,
	            iPAddress   IpAddress,
	            comment     DisplayString,
	            entryStatus INTEGER
	        }

	mACIPIndex OBJECT-TYPE
	    SYNTAX INTEGER
	    ACCESS read-only
	    STATUS mandatory
	    DESCRIPTION
	        " This is the index for the mappping table."
	    ::= { psPPPMACIPMappingTableEntry 1 }

	mACAddress OBJECT-TYPE
	    SYNTAX PhysAddress
	    ACCESS read-write
	    STATUS mandatory
	    DESCRIPTION
	        "This is the MAC address."
	    ::= { psPPPMACIPMappingTableEntry 2 }

	iPAddress  OBJECT-TYPE
	    SYNTAX IpAddress
	    ACCESS read-write
	    STATUS mandatory
	    DESCRIPTION
	        "The IP address corresponding to the MAC address."
        ::= { psPPPMACIPMappingTableEntry 3 }

	comment   OBJECT-TYPE
	    SYNTAX  DisplayString
	    ACCESS read-write
	    STATUS mandatory
	    DESCRIPTION
	        "The comment for this entry, maybe the PC's owner."
        ::= { psPPPMACIPMappingTableEntry 4 }

	entryStatus	OBJECT-TYPE
	    SYNTAX INTEGER
		    {
		        enable(1),
		     --   disable(2)
		     delete(2)
		    }
        ACCESS read-write
	    STATUS mandatory
	    DESCRIPTION
	        "Whether the entry is enabled or disabled. Used to delete 
            the unwanted entries."
        ::= { psPPPMACIPMappingTableEntry 5 }

	psPPPKeepAliveInterval  OBJECT-TYPE
	    SYNTAX INTEGER
	    ACCESS read-write
	    STATUS mandatory
	    DESCRIPTION
	        "The keep alive interval to be notified to the PPP 
	        layer."
	    ::= { orinocoPPPSetup 4 }

	psPPPNoOfKeepAliveTimeouts  OBJECT-TYPE
	    SYNTAX INTEGER
	    ACCESS read-write
	    STATUS mandatory
	    DESCRIPTION
	        "The maximum number of timeouts before the session 
	        is torn down."
        ::= { orinocoPPPSetup 5 }

	psPPPPrimaryDNSIPAddress  OBJECT-TYPE
	    SYNTAX IpAddress
	    ACCESS read-write
	    STATUS mandatory
	    DESCRIPTION
	        "The IP address of the primary DNS server in the network."
        ::= { orinocoPPPSetup 6 }

	psPPPSecondaryDNSIPAddress  OBJECT-TYPE
	    SYNTAX IpAddress
	    ACCESS read-write
	    STATUS mandatory
	    DESCRIPTION
	        "The IP address of the secondary DNS server in the network."
        ::= { orinocoPPPSetup 7 }

	psPPPMaxNoOfUsers  OBJECT-TYPE
