	        "Number of IAPP End-PPP session PDUs received."
        ::= { orinocoIAPP 11 }

	psIappEndSessAckOut OBJECT-TYPE
	    SYNTAX Counter
	    ACCESS read-only
	    STATUS mandatory
        DESCRIPTION
	        "Number of IAPP End-PPP session acknowledgement PDUs sent."
        ::= { orinocoIAPP 12 }

	psIappEndSessAckIn OBJECT-TYPE
	    SYNTAX Counter
	    ACCESS read-only
	    STATUS mandatory
        DESCRIPTION
	        "Number of IAPP End-PPP session acknowledgement PDUs received."
        ::= { orinocoIAPP 13 }

	psIappPduDrops OBJECT-TYPE
	    SYNTAX Counter
	    ACCESS read-only
	    STATUS mandatory
        DESCRIPTION
	        "Number of IAPP PDUs dropped."
        ::= { orinocoIAPP 14 }

	psIappHandOvrReqReSent OBJECT-TYPE
	    SYNTAX Counter
	    ACCESS read-only
	    STATUS mandatory
        DESCRIPTION
	        "Number of HANDOVER requests re-transmitted.."
        ::= { orinocoIAPP 15 }
       
	psIappAnnInterval OBJECT-TYPE
        SYNTAX INTEGER
          --  {
          --      eighty(80),
	--			oneHundredTwenty(120),
	--			oneHundredSixty(160),
	--			twoHundred(200)
	--		}
	    ACCESS read-write
	    STATUS mandatory
	    DESCRIPTION
			"This parameter represents interval in seconds for performing an 
			IAPP announce operation by the device.  The units for this 
			parameter is seconds and default value should be 120 seconds."
        ::= { orinocoIAPP 16 }
       
	psIappAnnRespTime OBJECT-TYPE
	    SYNTAX INTEGER
	    ACCESS read-only
	    STATUS mandatory
	    DESCRIPTION
			"This parameter indicates the amount of time in seconds the 
			device waits to send an IAPP announce response after an announce 
			request message is sent.  This parameter is set to a default value
			of 2 seconds and is not configurable."
        ::= { orinocoIAPP 17 }
       
	psIappHandOvrTimeOut OBJECT-TYPE
	    SYNTAX INTEGER
		--	{
		--		fiveHundredTwelve(512),
		--		sixHundredFourteen(614),
		--		sevenHundredSeventeen(717),
		--		eightHundredNineteen(819)
		--	}
	    ACCESS read-write
	    STATUS mandatory
	    DESCRIPTION
			"This parameter represents the time in milliseconds the device 
			waits before it resends a handover response message.  The default 
			value for this parameter should be 512 milliseconds."
        ::= { orinocoIAPP 18 }
       
	psIappMaxHandOvrRetries OBJECT-TYPE
	    SYNTAX INTEGER(1..4)
	    ACCESS read-write
	    STATUS mandatory	
	    DESCRIPTION
			"This parameter indicates the maximum amount of retransmission sent
			by the device for a handover request message.  The default for this
			parameter should be 2."
        ::= { orinocoIAPP 19 }

	psIappRoamingClients OBJECT-TYPE
	    SYNTAX Counter
	    ACCESS read-only
	    STATUS mandatory
        DESCRIPTION
	        "Number of roaming clients currently attached to the 
	        Access Server."
        ::= { orinocoIAPP 20 }

