--=============================================================================
-- Access Server Trap Variables
--=============================================================================

    accessServerTrapVariables OBJECT IDENTIFIER 
        ::= { accessServerTraps 1 }

	accessServerGenericInformationMessage OBJECT-TYPE
	    SYNTAX DisplayString
	    ACCESS not-accessible
	    STATUS mandatory
	    DESCRIPTION
		    "Any generic string information can be sent as this message."
        ::= { accessServerTrapVariables 1 }

	accessServerMacAddress OBJECT-TYPE
	    SYNTAX PhysAddress
	    ACCESS not-accessible
	    STATUS mandatory
	    DESCRIPTION
	        "The MAC address that is sent with the traps to
		    identify which Access Server has the problem "
        ::= { accessServerTrapVariables 2 }

	accessServerFailedTFTPServerAddress   OBJECT-TYPE
	    SYNTAX IpAddress
	    ACCESS not-accessible
	    STATUS mandatory
	    DESCRIPTION
	 	    "The ip address configured as the TFTP server 
		    for which the operation upload/download failed."
        ::= { accessServerTrapVariables 3 }

    accessServerFailedTFTPFilename   OBJECT-TYPE
	    SYNTAX DisplayString
	    ACCESS not-accessible
	    STATUS mandatory
	    DESCRIPTION
	 	    "The filename configured to be retrieved/sent from/to 
		    the TFTP server for which the operation upload/download
		    failed."
        ::= { accessServerTrapVariables 4 }

    accessServerFailedTFTPOperation   OBJECT-TYPE
	    SYNTAX INTEGER
		--    {
		--       upload(1),
		--       download(2)
		--    }
	    ACCESS not-accessible
	    STATUS mandatory
	    DESCRIPTION 
		    "This variable used in conjunction with the different TFTP traps (
		    TFTPInitiated, TFTPFailed, TFTPCompleted)"
        ::= { accessServerTrapVariables 5 }

    accessServerSuspendedTaskName   OBJECT-TYPE
	    SYNTAX DisplayString
        ACCESS not-accessible
	    STATUS mandatory
	    DESCRIPTION 
		    "The task that was suspended, can be several tasks executed within
		    the Access Server."
        ::= { accessServerTrapVariables 6 }

    accessServerWirelessCard   OBJECT-TYPE
	    SYNTAX INTEGER
	        {
	            pc-cardA(1),
	            pc-cardB(2)
	        }
        ACCESS not-accessible
	    STATUS mandatory
	    DESCRIPTION 
		    "This trap variable is used to determine on which Wireless Card,
		    PC Card A or PC Card B, a wireless TRAP has occured on."
		::= { accessServerTrapVariables 7 }
        
--=============================================================================
-- Access Server Flash Related Traps
--=============================================================================

	accessServerFlashRelatedTraps OBJECT IDENTIFIER 
	    ::= { accessServerTraps 2 }

	accessServerFlashEmpty TRAP-TYPE
	    ENTERPRISE accessServerFlashRelatedTraps
	    DESCRIPTION
	        "This trap is sent when the FLASH is empty."
	    ::= 1

	accessServerFlashCorrupted TRAP-TYPE
	    ENTERPRISE accessServerFlashRelatedTraps
	    DESCRIPTION
	        "This trap is sent when the data in the FLASH is corrupted."
	    ::= 2

--=============================================================================
-- Access Server Configuration Related Traps
--=============================================================================

	accessServerConfigurationRelatedTraps OBJECT IDENTIFIER
	    ::= { accessServerTraps 3 }

	accessServerInvalidKey TRAP-TYPE
	    ENTERPRISE accessServerConfigurationRelatedTraps
	    DESCRIPTION
	        "This trap is sent when an invalid key is detected at the
		    accessServer."
	    ::= 1

	accessServerAPMNotConfigured TRAP-TYPE
	    ENTERPRISE accessServerConfigurationRelatedTraps
	    DESCRIPTION
	        "This trap is sent when the APM information is not 
		    configured in the Access Server."
	    ::= 2

	accessServerWLCIncompatibleFirmware TRAP-TYPE
	    ENTERPRISE accessServerConfigurationRelatedTraps
	    DESCRIPTION
            "This trap is sent when an ORiNOCO PC Card plugged
		    in the Access Server is not compatible with the firmware
		    versions supported."
	    ::= 3

	accessServerWLCFailure TRAP-TYPE
        ENTERPRISE accessServerConfigurationRelatedTraps
        DESCRIPTION
            "This trap is sent when an ORiNOCO PC Card is not present or
            there is a failure in the ORiNOCO PC card."
        ::= 4

	accessServerWLCRemoval TRAP-TYPE
        ENTERPRISE accessServerConfigurationRelatedTraps
        DESCRIPTION
            "This trap is generated when the ORiNOCO PC Card is removed
            from the Access Server."
        ::= 5

	accessServerRadiusNotConfigured TRAP-TYPE
        ENTERPRISE accessServerConfigurationRelatedTraps
        DESCRIPTION
            "This trap is sent when the radius information is not configured
	        in the Access Server."
        ::= 6

	accessServerDNSIPNotConfigured TRAP-TYPE
        ENTERPRISE accessServerConfigurationRelatedTraps
        DESCRIPTION
            "This trap is sent when the DNS IP information is not 
		    configured in the Access Server."
        ::= 7

	accessServerNBNSIPNotConfigured TRAP-TYPE
        ENTERPRISE accessServerConfigurationRelatedTraps
        DESCRIPTION
            "This trap is sent when the NBNS IP information is not 
		    configured in the Access Server."
        ::= 8

     accessServerRadiusAuthenticationNotConfigured TRAP-TYPE
        ENTERPRISE accessServerConfigurationRelatedTraps
        DESCRIPTION
            "This trap is sent when the radius authentication information
            is not configured in the Access Server."
        ::= 9 
        
     accessServerRadiusAccountingNotConfigured TRAP-TYPE
        ENTERPRISE accessServerConfigurationRelatedTraps
        DESCRIPTION
            "This trap is sent when the radius accounting information
            is not configured in the Access Server."
        ::= 10

     accessServerDuplicateIPAddressEncountered TRAP-TYPE
        ENTERPRISE accessServerConfigurationRelatedTraps
        DESCRIPTION
            "This trap is sent when the IP Address of the Access Server
            is also used by someother device in the network."
        ::= 11

    accessServerWLCVoltageDiscrepancy TRAP-TYPE
        ENTERPRISE accessServerConfigurationRelatedTraps
        DESCRIPTION
            "This trap is sent when non 5volt card or 3.3 v card is 
            inserted in the Access Server."
        ::= 12
     
    accessServerWLCIncompatibleVendor TRAP-TYPE
        ENTERPRISE accessServerConfigurationRelatedTraps
        DESCRIPTION
            "This trap is generated when an incompatible wireless vendor
            card is inserted or present in the Access Server."
        ::= 13
        
    accessServerWLCFirmwareDownloadFailure TRAP-TYPE
        ENTERPRISE accessServerConfigurationRelatedTraps
        DESCRIPTION
            "This trap is generated when a failure occurs during the firmware
            download process."
        ::= 14
        
--=============================================================================
-- Access Server Diagnostic Traps
--=============================================================================

	accessServerDiagnosticTraps OBJECT IDENTIFIER
	    ::= { accessServerTraps 4 }

	accessServerTFTPInitiated TRAP-TYPE
	    ENTERPRISE accessServerDiagnosticTraps
	    DESCRIPTION
	        "This trap is sent when a TFTP operation happens
	        at the Access Server."
	    ::= 1

	accessServerTFTPCompleted TRAP-TYPE
	    ENTERPRISE accessServerDiagnosticTraps
	    DESCRIPTION
	        "This trap is sent when a TFTP operation failure happens
	        at the Access Server."
	    ::= 2

    accessServerTFTPFailure TRAP-TYPE
	    ENTERPRISE accessServerDiagnosticTraps
	    DESCRIPTION
	        "This trap is sent when a TFTP operation failure happens
	        at the Access Server."
	    ::= 3

	accessServerAdminStateMadeUp TRAP-TYPE
	    ENTERPRISE accessServerDiagnosticTraps
	    DESCRIPTION
	        "This trap is sent when the Access Server Admin state is made
		    'up'"
	    ::= 4

	accessServerAdminStateMadeDown TRAP-TYPE
	    ENTERPRISE accessServerDiagnosticTraps
	    DESCRIPTION
	        "This trap is sent when the Access Server Admin state is made
		    'down'"
	    ::= 5

	accessServerRebootingNow TRAP-TYPE
	    ENTERPRISE accessServerDiagnosticTraps
	    DESCRIPTION
	        "This trap is sent when the Access Server is about to reboot."
	    ::= 6

	accessServerTaskSuspended TRAP-TYPE
	    ENTERPRISE accessServerDiagnosticTraps
	    DESCRIPTION
	        "This trap is sent when a task has suspended in the Access Server."
	    ::= 7

	accessServerBootPFailed TRAP-TYPE
	    ENTERPRISE accessServerDiagnosticTraps
	    DESCRIPTION
	        "This trap is sent when the BootP protocol in the bootloader has
	        failed."
	    ::= 8

	accessServerDHCPFailed TRAP-TYPE
	    ENTERPRISE accessServerDiagnosticTraps
	    DESCRIPTION
	        "This trap is sent when the DHCP in the application software has
	        failed."
	    ::= 9

--=============================================================================
-- Access Server Image Related Traps
--=============================================================================

	accessServerImageTraps OBJECT IDENTIFIER
	    ::= { accessServerTraps 5 }

	accessServerZeroSizeImage TRAP-TYPE
	    ENTERPRISE accessServerImageTraps
	    DESCRIPTION
	        "This trap is sent when an image of size zero (0) is downloaded
	        to the access server."
	    ::= 1

	accessServerNonVxWorksImage TRAP-TYPE
	    ENTERPRISE accessServerImageTraps
	    DESCRIPTION
	        "This trap is sent when a non-VxWorks image is downloaded to the
	        access server."
	    ::= 2

	accessServerImageTooLarge TRAP-TYPE
	    ENTERPRISE accessServerImageTraps
	    DESCRIPTION
	        "This trap is sent when the image downloaded to the access server
	        exceeds the size limitation of 5 MB."
	    ::= 3
	    	    	        
END

--=============================================================================
-- Access Server 2000 MIBs End
--=============================================================================
