      ACCESS  not-accessible
      STATUS  mandatory
      DESCRIPTION
              ""
      ::= { lanBlazer70003ComMapping 1 }

   lanBlazer70003ComMappingEntry OBJECT-TYPE
      SYNTAX  LanBlazer70003ComMappingEntry
      ACCESS  not-accessible
      STATUS  mandatory
      DESCRIPTION
              ""
      INDEX   { lanBlazer70003ComMappingTableIndex }
      ::= { lanBlazer70003ComMappingTable 1 }

   LanBlazer70003ComMappingEntry ::=
      SEQUENCE {
         lanBlazer70003ComMappingTableIndex
            INTEGER,
         lanBlazer70003ComMappingTableName
            DisplayString,
         lanBlazer70003ComMappingTableStatus
            INTEGER
      }

   lanBlazer70003ComMappingTableIndex OBJECT-TYPE
      SYNTAX  INTEGER
      ACCESS  read-only
      STATUS  mandatory
      DESCRIPTION
              ""
      ::= { lanBlazer70003ComMappingEntry 1 }

   lanBlazer70003ComMappingTableName OBJECT-TYPE
      SYNTAX  DisplayString (SIZE (1..31))
      ACCESS  read-write
      STATUS  mandatory
      DESCRIPTION
              "A user-readable name associated with this table."
      ::= { lanBlazer70003ComMappingEntry 2 }

   lanBlazer70003ComMappingTableStatus OBJECT-TYPE
      SYNTAX  INTEGER {
                 createRequest(1),
                 destroyRequest(2),
                 active(3),
                 entryExistsError(4),
                 otherError(5)
              }
      ACCESS  read-write
      STATUS  mandatory
      DESCRIPTION
              "The status of an entry to be created. When adding an entry all
              fields will be set, and then the status is set to
              createRequest(1), indicating that the entry is to be created.
              If the creation is successful, then the status will be set to
              active(3).  Otherwise if the creation was not successful then
              one of the following error codes will be set and the entry will
              not be created:
                 entryExistsError(4) - An entry already exists.
                 otherError(5) - An error other than the others defined."
      ::= { lanBlazer70003ComMappingEntry 3 }


-- 3Com VLAN Mappings

   lanBlazer7000Vlan3ComMapping OBJECT IDENTIFIER ::= { lanBlazer7000VlanMappings 2 }

   lanBlazer7000Vlan3ComMappingTable OBJECT-TYPE
      SYNTAX  SEQUENCE OF LanBlazer7000Vlan3ComMappingEntry
      ACCESS  not-accessible
      STATUS  mandatory
      DESCRIPTION
              ""
      ::= { lanBlazer7000Vlan3ComMapping 1 }

