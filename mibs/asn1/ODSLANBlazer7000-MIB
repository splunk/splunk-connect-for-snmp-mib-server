         lanBlazer7000Vlan3ComMappingIndex
            INTEGER,
         lanBlazer7000Vlan3ComMappingVlanID
            INTEGER,
         lanBlazer7000Vlan3ComMappingStatus
            INTEGER
      }

   lanBlazer7000Vlan3ComMappingIndex OBJECT-TYPE
      SYNTAX  INTEGER (1..16)
      ACCESS  read-only
      STATUS  mandatory
      DESCRIPTION
              "The external tag of this 3Com VLAN."
      ::= { lanBlazer7000Vlan3ComMappingEntry 1 }

   lanBlazer7000Vlan3ComMappingVlanID OBJECT-TYPE
      SYNTAX  INTEGER
      ACCESS  read-write
      STATUS  mandatory
      DESCRIPTION
              "The VLAN ID of the VLAN that this 3Com tag is associated with."
      ::= { lanBlazer7000Vlan3ComMappingEntry 2 }

   lanBlazer7000Vlan3ComMappingStatus OBJECT-TYPE
      SYNTAX  INTEGER {
                 createRequest(1),
                 destroyRequest(2),
                 active(3),
                 otherError(4)
              }
      ACCESS  read-write
      STATUS  mandatory
      DESCRIPTION
              "The status of an entry to be created. When adding an entry all
              fields will be set, and then the status is set to
              createRequest(1), indicating that the entry is to be created.
              If the creation is successful, then the status will be set to
              active(3).  Otherwise if the creation was not successful then
              one of the following error codes will be set and the entry will
              not be created:
                 otherError(4) - An error other than the others defined."
      ::= { lanBlazer7000Vlan3ComMappingEntry 3 }



-- VIRTUAL SWITCH PORTS

   lanBlazer7000VirtualPorts OBJECT IDENTIFIER ::= { lanBlazer7000VlanMgt 4 }

   lanBlazer7000VirtualSwitchPortTable OBJECT-TYPE
      SYNTAX  SEQUENCE OF LanBlazer7000VirtualSwitchPortEntry
      ACCESS  not-accessible
      STATUS  mandatory
      DESCRIPTION
              ""
      ::= { lanBlazer7000VirtualPorts 1 }

   lanBlazer7000VirtualSwitchPortEntry OBJECT-TYPE
      SYNTAX  LanBlazer7000VirtualSwitchPortEntry
      ACCESS  not-accessible
      STATUS  mandatory
      DESCRIPTION
              "An instance of a virtual switch port indicates that this
              switch port is a member of this VLAN."
      INDEX   { lanBlazer7000VirtualSwitchPortIndex }
      ::= { lanBlazer7000VirtualSwitchPortTable 1 }

   LanBlazer7000VirtualSwitchPortEntry ::=
      SEQUENCE {
         lanBlazer7000VirtualSwitchPortIndex
            ResourceId,
         lanBlazer7000VirtualSwitchPortFormat
            INTEGER,
         lanBlazer7000VirtualSwitchPortBridgePort
            ResourceId,
         lanBlazer7000VirtualSwitchPortBindingType
            INTEGER,
         lanBlazer7000VirtualSwitchPortStatus
            INTEGER
      }

   lanBlazer7000VirtualSwitchPortIndex OBJECT-TYPE
      SYNTAX  ResourceId
      ACCESS  read-only
      STATUS  mandatory
      DESCRIPTION
              "The Resource ID of the virtual switch port bound to the VLAN."
      ::= { lanBlazer7000VirtualSwitchPortEntry 1 }

   lanBlazer7000VirtualSwitchPortFormat OBJECT-TYPE
      SYNTAX  INTEGER {
                 clear(1),          -- don't tag for this VLAN, port
                 trunkingFormat(2)  -- use switch port format - may be clear
              }
      ACCESS  read-write
      STATUS  mandatory
      DESCRIPTION
              "Typically, a VLAN capable switch port has one of three modes:
              access, trunk, or hybrid.  In access mode, the port sends
              frames in clear format (untagged). In trunk mode, all outbound
              frames are translated into the switch port's configured
              trunking format.

              In hybrid mode, it is possible for a port to send trunked
              frames for some VLANs and clear frames for others.  In this
              case, the switch port is configured to trunk, and the virtual
              switch port(s) for those VLANs that require clear formatted
              frames are configured to be override the switch port setting.
              This is done by setting this object to clear(1).

              By default, the value of this object is trunkingFormat(2)
              which means to use the trunking format configured for this
              switch port. (which may be clear)."
      ::= { lanBlazer7000VirtualSwitchPortEntry 2 }

   lanBlazer7000VirtualSwitchPortBridgePort OBJECT-TYPE
      SYNTAX  ResourceId
      ACCESS  read-write
      STATUS  mandatory
      DESCRIPTION
              "The bridge port resource bound to this virtual port."
      ::= { lanBlazer7000VirtualSwitchPortEntry 3 }

   lanBlazer7000VirtualSwitchPortBindingType OBJECT-TYPE
      SYNTAX  INTEGER {
                 static(1),         -- port binding was manually created
                 persistent(2),     -- port binding was automatically created
                 dynamic(3)         -- port binding was automatically created
              }
      ACCESS  read-only
      STATUS  mandatory
      DESCRIPTION
              "The method by which this switch port was bound to the VLAN.
              If the value is static(1), the binding was manually created
              by the administrator.  If the value is persistent(2), the
              binding was created by the switch because the VLAN is the
              port-based VLAN for the switch port, or the switch port
              VLAN Binding Method is persistent.  These bindings may not
              be removed.  If the value is dynamic(3), the binding was
              created by the switch as a result of receiving a tagged
              frame on the switch port with a VLAN ID corresponding to
              the VLAN."
      ::= { lanBlazer7000VirtualSwitchPortEntry 4 }

   lanBlazer7000VirtualSwitchPortStatus OBJECT-TYPE
      SYNTAX  INTEGER {
                 createRequest(1),
                 destroyRequest(2),
                 active(3),
                 otherError(4),
                 entryExistsError(5),
                 entryNoExistError(6)
              }
      ACCESS  read-write
      STATUS  mandatory
      DESCRIPTION
              "The status of an entry to be created or deleted.
              When adding an entry all fields will be set, and then the status
              is set to createRequest(1) (indicating that the entry is to be
              created).  When deleting an entry the status is set to
              destroyRequest(2) (indicating that the entry is to be destroyed).
              If the creation is successful, then the status will be set to
              active(3).  Otherwise if the creation was not successful then
              one of the following error codes will be set and the entry will
              not be created:
                 otherError(4) - An error other than the others defined.
                 entryExistsError(5) - On creation, an entry already exists.
                                       On deletion, the entry may not be
                                       removed.
                 entryNoExistError(6) - The VLAN specified by ID
                                        does not exist."
      ::= { lanBlazer7000VirtualSwitchPortEntry 5 }



-- EVENT MANAGEMENT

   lanBlazer7000Events OBJECT IDENTIFIER ::= { odsLANBlazer7000Mib 10 }

-- EVENT TABLE

   lanBlazer7000EventMgt OBJECT IDENTIFIER ::= { lanBlazer7000Events 1 }

-- Event Table

   lanBlazer7000EventTable OBJECT-TYPE
      SYNTAX  SEQUENCE OF  LanBlazer7000EventEntry
      ACCESS  not-accessible
      STATUS  mandatory
      DESCRIPTION
              "Table of events currently supported."
      ::= { lanBlazer7000EventMgt 1 }

   lanBlazer7000EventEntry OBJECT-TYPE
      SYNTAX  LanBlazer7000EventEntry
      ACCESS  not-accessible
      STATUS  mandatory
      DESCRIPTION
              "Attributes associated with the event."
      INDEX   { lanBlazer7000EventIndex }
      ::= { lanBlazer7000EventTable 1 }

   LanBlazer7000EventEntry ::=
      SEQUENCE {
         lanBlazer7000EventIndex
            INTEGER,
         lanBlazer7000EventMode
            INTEGER,
         lanBlazer7000EventLogAction
            INTEGER,
         lanBlazer7000EventTrapAction
            INTEGER,
         lanBlazer7000EventConsoleAction
            INTEGER
      }

   lanBlazer7000EventIndex OBJECT-TYPE
      SYNTAX  INTEGER
      ACCESS  read-only
      STATUS  mandatory
      DESCRIPTION
              ""
      ::= { lanBlazer7000EventEntry 1 }

   lanBlazer7000EventMode OBJECT-TYPE
      SYNTAX  INTEGER {
                 enable(1),
                 disable(2)
              }
      ACCESS  read-write
      STATUS  mandatory
      DESCRIPTION
              "Disabling an event prevents this event from taking any actions when
              triggered.
              When set to enable to the console, the event will print the event information
              to the console serial port.  The user can select whether to view log
              messages, trap messages or any event at the console."
      ::= { lanBlazer7000EventEntry 2 }

--
-- lanBlazer7000EventCategory has been removed
--

   lanBlazer7000EventLogAction OBJECT-TYPE
      SYNTAX  INTEGER {
                 enable(1),
                 disable(2)
              }
      ACCESS  read-write
      STATUS  mandatory
      DESCRIPTION
              "When enabled, this action will cause an event log entry to be created."
      ::= { lanBlazer7000EventEntry 3 }

   lanBlazer7000EventTrapAction OBJECT-TYPE
      SYNTAX  INTEGER {
                 enable(1),
                 disable(2)
              }
      ACCESS  read-write
      STATUS  mandatory
      DESCRIPTION
              "When enabled, this event will cause an SNMP trap to be generated."
      ::= { lanBlazer7000EventEntry 4 }

   lanBlazer7000EventConsoleAction OBJECT-TYPE
      SYNTAX  INTEGER {
                 enable(1),
                 disable(2)
              }
      ACCESS  read-write
      STATUS  mandatory
      DESCRIPTION
              "When enabled, this event will cause a message to printed to the console serial port."
      ::= { lanBlazer7000EventEntry 5 }



-- EVENT LOG

   lanBlazer7000EventLogMgt OBJECT IDENTIFIER ::= { lanBlazer7000Events 2 }

