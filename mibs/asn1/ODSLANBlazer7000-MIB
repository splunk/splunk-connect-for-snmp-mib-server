              Changing this value causes the existing log to be
              truncated and rebuilt."
      ::= { lanBlazer7000EventLogMgt 1 }

   lanBlazer7000LogLastEntry OBJECT-TYPE
      SYNTAX  INTEGER (1..65535)
      ACCESS  read-only
      STATUS  mandatory
      DESCRIPTION
              "The log index of the last entry entered in the log."
      ::= { lanBlazer7000EventLogMgt 2 }


   lanBlazer7000LogWraps OBJECT-TYPE
      SYNTAX  Counter
      ACCESS  read-only
      STATUS  mandatory
      DESCRIPTION
              "The number of times that the last entry has wrapped from 65K back to 1."
      ::= { lanBlazer7000EventLogMgt 3 }


-- EVENT LOG TABLE

   lanBlazer7000EventLog OBJECT IDENTIFIER ::= { lanBlazer7000Events 3 }

   lanBlazer7000EventLogTable OBJECT-TYPE
      SYNTAX  SEQUENCE OF LanBlazer7000EventLogEntry
      ACCESS  not-accessible
      STATUS  mandatory
      DESCRIPTION
              "The log table for the events in the event table that are enabled for the
               Log Action."
      ::= { lanBlazer7000EventLog 1 }

   lanBlazer7000EventLogEntry OBJECT-TYPE
      SYNTAX  LanBlazer7000EventLogEntry
      ACCESS  not-accessible
      STATUS  mandatory
      DESCRIPTION
              "An entry in the log indicates information associated with a particular
              event."
      INDEX   { lanBlazer7000EventLogIndex }
      ::= { lanBlazer7000EventLogTable 1 }


   LanBlazer7000EventLogEntry ::=
      SEQUENCE {
         lanBlazer7000EventLogEventIndex
            INTEGER,
         lanBlazer7000EventLogIndex
            INTEGER,
         lanBlazer7000EventLogTime
            TimeTicks,
         lanBlazer7000EventLogDescr
            DisplayString,
         lanBlazer7000EventLogType
            EventCategory,
         lanBlazer7000EventLogSeverity
            INTEGER,
         lanBlazer7000EventLogDTM
            DisplayString,
         lanBlazer7000EventLogResType
            ResourceType,
         lanBlazer7000EventLogResID
            ResourceId,
         lanBlazer7000EventLogResLeaf
            INTEGER,
         lanBlazer7000EventLogValueType
            EventValueType,
         lanBlazer7000EventLogValue
            OCTET STRING,
         lanBlazer7000EventLogEpochTime
            INTEGER,
         lanBlazer7000EventLogID
            INTEGER
      }


   lanBlazer7000EventLogEventIndex OBJECT-TYPE
      SYNTAX  INTEGER (1..65535)
      ACCESS  read-only
      STATUS  mandatory
      DESCRIPTION
              "The index that uniquely identifies the event that caused this log entry."
      ::= { lanBlazer7000EventLogEntry 1 }

   lanBlazer7000EventLogIndex OBJECT-TYPE
      SYNTAX  INTEGER (1..65535)
      ACCESS  read-only
      STATUS  mandatory
      DESCRIPTION
              "An index that uniquely identifies this log entry."
      ::= { lanBlazer7000EventLogEntry 2 }

   lanBlazer7000EventLogTime OBJECT-TYPE
      SYNTAX  TimeTicks
      ACCESS  read-only
      STATUS  mandatory
      DESCRIPTION
              "The value of sysUpTime when this event was triggered."
      ::= { lanBlazer7000EventLogEntry 3 }

   lanBlazer7000EventLogDescr OBJECT-TYPE
      SYNTAX  DisplayString (SIZE(0..255))
      ACCESS  read-only
      STATUS  mandatory
      DESCRIPTION
              "The event log description."
      ::= { lanBlazer7000EventLogEntry 4 }

   lanBlazer7000EventLogType OBJECT-TYPE
      SYNTAX  EventCategory
      ACCESS  read-only
      STATUS  mandatory
      DESCRIPTION
              "The type of event that caused this log entry."
      ::= { lanBlazer7000EventLogEntry 5 }

   lanBlazer7000EventLogSeverity OBJECT-TYPE
      SYNTAX  INTEGER (0..100)
      ACCESS  read-only
      STATUS  mandatory
      DESCRIPTION
              "The severity associated with this event.  It is recommended that the
              severity be interpreted in the following manner:

               0-19: Normal
              20-39: Informational
              40-59: Warning
              60-79: Alarm
              80-99: Severe Error
                100: Failure."
      ::= { lanBlazer7000EventLogEntry 6 }

   lanBlazer7000EventLogDTM OBJECT-TYPE
      SYNTAX  DisplayString (SIZE (18))
      ACCESS  read-only
      STATUS  mandatory
      DESCRIPTION
              "The date and time when this log entry was made.  The format is yy-Mon-
              dd hh:mm:ss, time is in 24 hour time."
      ::= { lanBlazer7000EventLogEntry 7 }

   lanBlazer7000EventLogResType OBJECT-TYPE
      SYNTAX  ResourceType
      ACCESS  read-only
      STATUS  mandatory
      DESCRIPTION
              "The type of object (if provided) that triggered this event.  If not provided,
              the value is equal to 'Invalid Resource'."
      ::= { lanBlazer7000EventLogEntry 8 }

   lanBlazer7000EventLogResID OBJECT-TYPE
      SYNTAX  ResourceId
      ACCESS  read-only
      STATUS  mandatory
      DESCRIPTION
              "The instance of this resource (if provided - see lanBlazer7000EventLogResType)
              that triggered this event."
      ::= { lanBlazer7000EventLogEntry 9 }

   lanBlazer7000EventLogResLeaf OBJECT-TYPE
      SYNTAX  INTEGER
      ACCESS  read-only
      STATUS  mandatory
      DESCRIPTION
              "A number corresponding to the attribute associated with this resource and
              this event entry.  It corresponds exactly to the leaf MIB number of the MIB
              that manages this resource.  For example, if a port's mode changed, the
              configuration event log entry would indicate the value of  5 which is the
              leaf index of the lanBlazer7000PortMode within the lanBlazer7000PortTable MIB table."
      ::= { lanBlazer7000EventLogEntry 10 }


   lanBlazer7000EventLogValueType OBJECT-TYPE
      SYNTAX  EventValueType
      ACCESS  read-only
      STATUS  mandatory
      DESCRIPTION
              "The data type associated with the log event value.  This object indicates
              how to interpret the data stored in the event log value:

              - none(1) indicates no value returned.
              - integer32(2) -  a 4 byte unsigned integer.
              - integer64(3) - an 8 byte unsigned integer.
              - displayString(4) - a null terminated (or up to 8 characters) string.
              - ipv4NetworkAddress(5) - a 4 byte IP version 4 network address.
              - ieee802MACAddress(6) - a 6 byte MAC Address.
              - timeticks(7) - sysUpTime type value (4 bytes)"
      ::= { lanBlazer7000EventLogEntry 11 }


   lanBlazer7000EventLogValue OBJECT-TYPE
      SYNTAX  OCTET STRING (SIZE(0..8))
      ACCESS  read-only
      STATUS  mandatory
      DESCRIPTION
              "The value associated with the event encoded in an octet string.  Refer to
              lanBlazer7000EventLogValueType for how to interpret this value.
              The value encoded in this string is in Big Endian order."
      ::= { lanBlazer7000EventLogEntry 12 }

   lanBlazer7000EventLogEpochTime OBJECT-TYPE
      SYNTAX  INTEGER
      ACCESS  read-only
      STATUS  mandatory
      DESCRIPTION
              "The number of time ticks since the epoch when this
              event was logged. The interpretation of this value is
              as follows:

              struct DateTimeOvly {
               UNS32 year:6;
               UNS32 month:4;
               UNS32 day:5;
               UNS32 hour:5;
               UNS32 minute:6;
               UNS32 second:6;
               };

              The epoch is January 1, 1997, at 00:00:00.  A value of
              0 refers to this date and time.  "
      ::= { lanBlazer7000EventLogEntry 13 }

      lanBlazer7000EventLogID OBJECT-TYPE
         SYNTAX  INTEGER (1..65535)
         ACCESS  read-only
         STATUS  mandatory
         DESCRIPTION
              "A unique index that identifies the occurence of this
              event.  This ID can be correlated between traps, logs
              and the like."
      ::= { lanBlazer7000EventLogEntry 14 }

-- SHUTDOWN EVENT LOG

   lanBlazer7000ShutdownLogMgt OBJECT IDENTIFIER ::= { lanBlazer7000Events 4 }

-- The most recent event log entries are stored in non-volatile memory.  In the event
-- of a system restart, it may be useful to look at the last few events that were
-- logged before the system last went down.  The Shutdown Log Table stores those
-- last events.
--
-- SHUTDOWN EVENT LOG OBJECTS

   lanBlazer7000ShutdownLogTableMaxSize OBJECT-TYPE
      SYNTAX  INTEGER (0..128)
      ACCESS  read-write
      STATUS  mandatory
      DESCRIPTION
              "The maximum number of entries in the non-volatile log table.  Changing
              the value of this object changes the maximum number of entries to be
              stored in Shutdown."
      ::= { lanBlazer7000ShutdownLogMgt 1 }

   lanBlazer7000ShutdownLogLastEntry OBJECT-TYPE
      SYNTAX  INTEGER (1..65535)
      ACCESS  read-only
      STATUS  mandatory
      DESCRIPTION
              "The ID of the last entry made to the shutdown log."
      ::= { lanBlazer7000ShutdownLogMgt 2 }

   lanBlazer7000ShutdownLogAcknowledged OBJECT-TYPE
      SYNTAX  INTEGER {
                 not-acknowledged(1),
                 acknowledged(2)
              }
      ACCESS  read-only
      STATUS  mandatory
      DESCRIPTION
              "This object is to set to acknowledged(2) the first time the Shutdown Log
              Table is accessed indicating that the Shutdown log has been read (at least
              once) since the system restarted."
      ::= { lanBlazer7000ShutdownLogMgt 3 }



-- SHUTDOWN LOG TABLE
--
-- The Shutdown Log Table is a copy of the log file stored in non-volatile memory.  It is copied
-- to dynamic memory (RAM) when the system is initialized and the non-volatile version
-- is re-initialized.

   lanBlazer7000EventShutdownLog OBJECT IDENTIFIER ::= { lanBlazer7000Events 5 }

   lanBlazer7000EventShutdownLogTable OBJECT-TYPE
      SYNTAX  SEQUENCE OF LanBlazer7000EventShutdownLogEntry
      ACCESS  not-accessible
      STATUS  mandatory
      DESCRIPTION
              "A table of the last events logged before the system restarted."
      ::= { lanBlazer7000EventShutdownLog 1 }

   lanBlazer7000EventShutdownLogEntry OBJECT-TYPE
      SYNTAX  LanBlazer7000EventShutdownLogEntry
      ACCESS  not-accessible
      STATUS  mandatory
      DESCRIPTION
              "A log entry stored in non-volatile memory."
      INDEX   { lanBlazer7000EventShutdownLogIndex }
      ::= { lanBlazer7000EventShutdownLogTable 1 }

   LanBlazer7000EventShutdownLogEntry ::=
      SEQUENCE {
         lanBlazer7000EventShutdownLogEventIndex
            INTEGER,
         lanBlazer7000EventShutdownLogIndex
            INTEGER,
         lanBlazer7000EventShutdownLogTime
            TimeTicks,
         lanBlazer7000EventShutdownLogDescr
            DisplayString,
         lanBlazer7000EventShutdownLogType
            EventCategory,
         lanBlazer7000EventShutdownLogSeverity
            INTEGER,
         lanBlazer7000EventShutdownLogDTM
            DisplayString,
         lanBlazer7000EventShutdownLogResType
            ResourceType,
         lanBlazer7000EventShutdownLogResID
            ResourceId,
         lanBlazer7000EventShutdownLogResLeaf
            INTEGER,
         lanBlazer7000EventShutdownLogValueType
            EventValueType,
         lanBlazer7000EventShutdownLogValue
            OCTET STRING,
         lanBlazer7000EventShutdownLogEpochTime
            INTEGER,
         lanBlazer7000EventShutdownLogID
            INTEGER

      }


   lanBlazer7000EventShutdownLogEventIndex OBJECT-TYPE
      SYNTAX  INTEGER (1..65535)
      ACCESS  read-only
      STATUS  mandatory
      DESCRIPTION
              "The index that uniquely identifies the event that caused this ShutdownLog
              entry."
      ::= { lanBlazer7000EventShutdownLogEntry 1 }

   lanBlazer7000EventShutdownLogIndex OBJECT-TYPE
      SYNTAX  INTEGER (1..65535)
      ACCESS  read-only
      STATUS  mandatory
      DESCRIPTION
              "An index that uniquely identifies this ShutdownLog entry."
      ::= { lanBlazer7000EventShutdownLogEntry 2 }

   lanBlazer7000EventShutdownLogTime OBJECT-TYPE
      SYNTAX  TimeTicks
      ACCESS  read-only
      STATUS  mandatory
      DESCRIPTION
              "The value of sysUpTime when this event was triggered.  Note, 
              the value corresponds to the sysUpTime when the system was last 
              running (i.e. before it was shutdown.)"
      ::= { lanBlazer7000EventShutdownLogEntry 3 }

   lanBlazer7000EventShutdownLogDescr OBJECT-TYPE
      SYNTAX  DisplayString (SIZE(0..255))
      ACCESS  read-only
      STATUS  mandatory
      DESCRIPTION
              "The event ShutdownLog description."
      ::= { lanBlazer7000EventShutdownLogEntry 4 }

   lanBlazer7000EventShutdownLogType OBJECT-TYPE
      SYNTAX  EventCategory
      ACCESS  read-only
      STATUS  mandatory
      DESCRIPTION
              "The type of event that caused this ShutdownLog entry."
      ::= { lanBlazer7000EventShutdownLogEntry 5 }

   lanBlazer7000EventShutdownLogSeverity OBJECT-TYPE
      SYNTAX  INTEGER (0..100)
      ACCESS  read-only
      STATUS  mandatory
      DESCRIPTION
              "The severity associated with this event.  It is recommended that the
              severity be interpreted in the following manner:

               0-19: Normal
              20-39: Informational
              40-59: Warning
              60-79: Alarm
              80-99: Severe Error
                100: Failure."
      ::= { lanBlazer7000EventShutdownLogEntry 6 }

   lanBlazer7000EventShutdownLogDTM OBJECT-TYPE
      SYNTAX  DisplayString (SIZE (18))
      ACCESS  read-only
      STATUS  mandatory
      DESCRIPTION
              "The date and time when this ShutdownLog entry was made.  The format
              is yy-Mon-dd hh:mm:ss, time is in 24 hour time."
      ::= { lanBlazer7000EventShutdownLogEntry 7 }

   lanBlazer7000EventShutdownLogResType OBJECT-TYPE
      SYNTAX  ResourceType
      ACCESS  read-only
      STATUS  mandatory
      DESCRIPTION
              "The type of object (if provided) that triggered this event.  If not provided,
              the value is invalid."
      ::= { lanBlazer7000EventShutdownLogEntry 8 }

   lanBlazer7000EventShutdownLogResID OBJECT-TYPE

      SYNTAX  ResourceId
      ACCESS  read-only
      STATUS  mandatory
      DESCRIPTION
              "The instance of this resource (if provided) that triggered this event."
      ::= { lanBlazer7000EventShutdownLogEntry 9 }

   lanBlazer7000EventShutdownLogResLeaf OBJECT-TYPE

      SYNTAX  INTEGER
      ACCESS  read-only
      STATUS  mandatory
      DESCRIPTION
              "To be provided."
      ::= { lanBlazer7000EventShutdownLogEntry 10 }

   lanBlazer7000EventShutdownLogValueType OBJECT-TYPE
      SYNTAX  EventValueType
      ACCESS  read-only
      STATUS  mandatory
      DESCRIPTION
              "The data type associated with the ShutdownLog event value.  This object
              indicates how to interpret the data stored in the event ShutdownLog
              value."
      ::= { lanBlazer7000EventShutdownLogEntry 11 }


   lanBlazer7000EventShutdownLogValue OBJECT-TYPE
      SYNTAX  OCTET STRING (SIZE(0..8))
      ACCESS  read-only
      STATUS  mandatory
      DESCRIPTION
              "The value associated with the event encoded in an octet string. "
      ::= { lanBlazer7000EventShutdownLogEntry 12 }

   lanBlazer7000EventShutdownLogEpochTime OBJECT-TYPE
      SYNTAX  INTEGER
      ACCESS  read-only
      STATUS  mandatory
      DESCRIPTION
              "The number of time ticks since the epoch when this
              event was logged. The interpretation of this value is
              as follows:

              struct DateTimeOvly {
               UNS32 year:6;
               UNS32 month:4;
               UNS32 day:5;
               UNS32 hour:5;
               UNS32 minute:6;
               UNS32 second:6;
               };

              The epoch is January 1, 1997, at 00:00:00.  A value of
              0 refers to this date and time.  "
      ::= { lanBlazer7000EventShutdownLogEntry 13 }

    lanBlazer7000EventShutdownLogID OBJECT-TYPE
         SYNTAX  INTEGER (1..65535)
         ACCESS  read-only
         STATUS  mandatory
         DESCRIPTION
              "A unique index that identifies the occurence of this
              event.  This ID can be correlated between traps, logs
              and the like."
      ::= { lanBlazer7000EventShutdownLogEntry 14 }

