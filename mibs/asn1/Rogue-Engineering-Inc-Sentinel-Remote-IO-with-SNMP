Rogue-Engineering-Inc-Sentinel-Remote-IO-with-SNMP
-- ******************************************************
--Rogue Engineering Inc., SNMP  MIB
--Prefix for private enterprises:  1.3.6.1.4.1  
--Rogue Engineering Inc. private enterprize number:  28459
--
--This MIB is for the Sentinel Remote I/O with SNMP product
--Version supported: Firmware 228-02
--Last Updated: June 4, 2008
--
--The following objects are implemented in the MIB-II System Group
--
--1.3.6.1.2.1.1.1   sysDescr
--1.3.6.1.2.1.1.2   sysObjectID
--1.3.6.1.2.1.1.3   sysUpTime
--1.3.6.1.2.1.1.4   sysContact
--1.3.6.1.2.1.1.5   sysName
--1.3.6.1.2.1.1.6   sysLocation
--1.3.6.1.2.1.1.7   sysServices
--
--
--
-- ************************************************************

DEFINITIONS ::= BEGIN

IMPORTS  
    enterprises
        FROM RFC1155-SMI
    MODULE-IDENTITY, OBJECT-TYPE, NOTIFICATION-TYPE, Integer32 
        FROM SNMPv2-SMI
    MODULE-COMPLIANCE, OBJECT-GROUP
        FROM SNMPv2-CONF
    SnmpAdminString
        FROM SNMP-FRAMEWORK-MIB;

rogueSentinelv2 MODULE-IDENTITY
    LAST-UPDATED "200806041700Z"
    ORGANIZATION "Rogue Engineering Inc."
    CONTACT-INFO
        "    Tim Kirk
             Rogue Engineering Inc.
             3860 S. Jason St.
             Englewood, CO 80110
             
             Tel: 303-734-0706
             Fax: 303-734-0517
             Internet: http://www.rogue-engr.com
             Email:    tkirk@rogue-engr.com"
    DESCRIPTION 
        "This MIB is for the Sentinel Remote I/O with SNMP"
    REVISION "200806041700Z"
    DESCRIPTION
        "Supports Firmware 228-02"
    ::= { rogueRIOS 2 }                                         -- 28459.228.2


-- Rogue Engineering Inc. MIBs
rogueEngr      OBJECT IDENTIFIER ::= { enterprises 28459 }     -- 1.3.6.1.4.1.28459

-- Items under the rogueEngr tree are product-specific
-- The RIOS product is ID 228
rogueRIOS   OBJECT IDENTIFIER ::= { rogueEngr 228 }            -- 28459.228


-- Major object categories for Remote I/O with SNMP product
regMap          OBJECT IDENTIFIER ::= { rogueSentinelv2 1 }     -- 28459.228.2.1
trapMap         OBJECT IDENTIFIER ::= { rogueSentinelv2 2 }     -- 28459.228.2.2
history         OBJECT IDENTIFIER ::= { rogueSentinelv2 3 }     -- 28459.228.2.3


-- ******************************************************
--
-- READ/WRITE REGISTER DEFINITIONS (corresponds to the Modbus Register map)
-- 
-- ******************************************************

regFirmwareVersion OBJECT-TYPE
    SYNTAX  Integer32
    MAX-ACCESS  read-only
    STATUS  current
    DESCRIPTION
        "Firmware Version"
    ::= { regMap 3 }


