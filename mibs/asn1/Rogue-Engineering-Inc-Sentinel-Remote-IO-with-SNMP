    SYNTAX  Integer32
    MAX-ACCESS  read-write
    STATUS  current
    DESCRIPTION
        "Decrement Log Register by 7 - If this is written non-zero, it will
        move the log pointer back 7 points."
    ::= { regMap 2113 }


    
-- ******************************************************
--
-- TRAP DEFINITIONS (not actual readable values)
-- 
-- ******************************************************
trapContents        OBJECT IDENTIFIER ::= { trapMap 1 }

valueTrapItem   OBJECT-TYPE
    SYNTAX  ValueTrapItem
    MAX-ACCESS  accessible-for-notify
    STATUS      current
    DESCRIPTION
        "Trap Item structure."
    ::= { trapMap 2 }

ValueTrapItem   ::= SEQUENCE {
    valueControllerID       SnmpAdminString,
    valueTrapDesc           SnmpAdminString,
    valueCurrentValue       Integer32
    }


valueControllerID   OBJECT-TYPE
    SYNTAX  SnmpAdminString
    MAX-ACCESS  accessible-for-notify
    STATUS      current
    DESCRIPTION
        "Unit Name or ID sending the trap."
    ::= { trapContents 1 }


valueTrapDesc   OBJECT-TYPE
    SYNTAX  SnmpAdminString
    MAX-ACCESS  accessible-for-notify
    STATUS      current
    DESCRIPTION
        "Short description of trap item."
    ::= { trapContents 2 }


valueCurrentValue    OBJECT-TYPE
    SYNTAX  Integer32
    MAX-ACCESS  accessible-for-notify
    STATUS      current
    DESCRIPTION
        "Current value of the item."
    ::= { trapContents 3 }

-- ******************************************************
--
-- SNMP NOTIFICATIONS
-- 
-- ******************************************************

trapInputLevel      NOTIFICATION-TYPE
    OBJECTS {
        valueControllerID,
        valueTrapDesc,
        valueCurrentValue
    }
    STATUS      current
    DESCRIPTION
        "This trap is sent when an input with an associated value
        goes above or below a setpoint level if notifications are enabled 
        for the object."
    ::= { trapMap 1 }

trapSequenceInputLevel    NOTIFICATION-TYPE
    OBJECTS {
        valueTrapItem
    }
    STATUS      current
    DESCRIPTION
        "This trap is sent when an input with an associated value
        goes above or below a setpoint level if notifications are enabled 
        for the object. Same as trapInputLevel, but sent as a sequence."
    ::= { trapMap 2 }

END