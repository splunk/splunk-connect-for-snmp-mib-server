			SYNTAX Unsigned32 (1..1000000 | 9999999)
			UNITS "Kbps"
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"look at table."
			::= { sheGBondPortConfControl 9 }

		
		sheGBondPortConfControlThreshLowUpRate OBJECT-TYPE
			SYNTAX Unsigned32 (1..1000000)
			UNITS "Kbps"
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"look at table."
			::= { sheGBondPortConfControl 10 }

		
		sheGBondPortConfControlThreshLowDownRate OBJECT-TYPE
			SYNTAX Unsigned32 (1..1000000)
			UNITS "Kbps"
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"look at table."
			::= { sheGBondPortConfControl 11 }

		
		sheGBondPortConfControlLowRateCrossingEnable OBJECT-TYPE
			SYNTAX TruthValue
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"look at table."
			::= { sheGBondPortConfControl 12 }

		
		sheGBondPortCapabilityTable OBJECT-TYPE
			SYNTAX SEQUENCE OF SheGBondPortCapabilityEntry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"Table for Capabilities of G.Bond Ports. Entries in this table
				MUST be maintained in a persistent manner"
			::= { sheGBondPort 5 }

		
		sheGBondPortCapabilityEntry OBJECT-TYPE
			SYNTAX SheGBondPortCapabilityEntry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"An entry in the G.Bond Port Capability table.
				Each entry represents an G.Bond port indexed by the ifIndex.
				Note that a G.Bond GBS port runs on top of a single
				or multiple BCE port(s), which are also indexed by ifIndex and channelnumber."
			INDEX { ifIndex }
			::= { sheGBondPortCapabilityTable 1 }

		
		SheGBondPortCapabilityEntry ::=
			SEQUENCE { 
				sheGBondPeerBond
					TruthValueOrUnknown,
				sheGBondCapacity
					Unsigned32,
				sheGBondPeerCapacity
					Unsigned32
			 }

		sheGBondPeerBond OBJECT-TYPE
			SYNTAX TruthValueOrUnknown
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"Bonding Capability of the G.Bond port (GBS) link partner.
				This object has a value of true(1) when the remote GBS
				supports the same bonding scheme as the local port.
				A value of false(2) is returned when the remote GBS does not
				support the same bonding scheme.
				Ports whose peers cannot be reached because of the link
				state, SHALL return a value if unknown(0).
				
				This object maps to the WT-157 attribute aGroupPeerBndScheme."
			REFERENCE
				"[WT-157] 5.5.1.3"
			::= { sheGBondPortCapabilityEntry 1 }

		
		sheGBondCapacity OBJECT-TYPE
			SYNTAX Unsigned32 (1..32)
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"Number of BCEs that can be aggregated by the local GBS.
				The number of BCEs currently assigned to a particular G.Bond
				port (sheGBondNumBCEs) is never greater than sheGBondCapacity.
				
				This object maps to the WT-157 attribute aGroupCapacity."
			REFERENCE
				"[WT-157] 5.5.1.8"
			::= { sheGBondPortCapabilityEntry 2 }

		
		sheGBondPeerCapacity OBJECT-TYPE
			SYNTAX Unsigned32 (0 | 1..32)
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"Number of BCEs that can be aggregated by the peer GBS port.
				Value of 0 is returned when peer Bonding Capacity is unknown
				(peer cannot be reached).
				
				This object maps to the WT-157 attribute aGroupRemoteCapacity."
			REFERENCE
				"[WT-157] 5.5.1.9"
			::= { sheGBondPortCapabilityEntry 3 }

		
		sheGBondPortStatusTable OBJECT-TYPE
			SYNTAX SEQUENCE OF SheGBondPortStatusEntry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"This table provides overall status information of G.Bond
				ports, complementing the generic status information from the
				ifTable of IF-MIB. Additional status information about
				connected BCEs is available from the relevant line MIBs
				
				This table contains live data from the equibcent. As such,
				it is NOT persistent."
			::= { sheGBondPort 6 }

		
		sheGBondPortStatusEntry OBJECT-TYPE
			SYNTAX SheGBondPortStatusEntry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"An entry in the G.Bond Port Status table.
				Each entry represents an G.Bond port indexed by the ifIndex.
				Note that an G.Bond GBS port runs on top of a single
				or multiple BCE port(s), which are also indexed by ifIndex and channelnumber."
			INDEX { ifIndex }
			::= { sheGBondPortStatusTable 1 }

		
		SheGBondPortStatusEntry ::=
			SEQUENCE { 
				sheGBondUpDataRate
					Gauge32,
				sheGBondDownDataRate
					Gauge32,
				sheGBondFltStatus
					SheGBondFltStatusType,
				sheGBondPortSide
					SheGBondPortSideType,
				sheGBondNumBCEs
					Unsigned32
			 }

		sheGBondUpDataRate OBJECT-TYPE
			SYNTAX Gauge32
			UNITS "bps"
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"A current G.Bond port operational Data Rate in the upstream
				direction, in bps.
				This object represents an estimation of the sum of individual
				BCE upstream data rates, modified to compensate for
				fragmentation and encapsulation overhead (e.g. for an Ethernet
				service, the target data rate of 10Mbps SHALL allow lossless
				transmission of full-duplex 10Mbps Ethernet frame stream with
				minimal inter-frame gap).
				
				Note that for symmetrical interfaces sheGBondUpDataRate ==
				sheGBondDownDataRate == ifSpeed."
			::= { sheGBondPortStatusEntry 1 }

		
		sheGBondDownDataRate OBJECT-TYPE
			SYNTAX Gauge32
			UNITS "bps"
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"A current G.Bond port operational Data Rate in the downstream
				direction, in bps.
				This object represents an estimation of the sum of individual
				BCE downstream data rates, modified to compensate for
				fragmentation and encapsulation overhead (e.g. for an Ethernet
				service, the target data rate of 10Mbps SHALL allow lossless
				transmission of full-duplex 10Mbps Ethernet frame stream with
				minimal inter-frame gap).
				
				Note that for symmetrical interfaces sheGBondUpDataRate ==
				sheGBondDownDataRate == ifSpeed."
			::= { sheGBondPortStatusEntry 2 }

		
		sheGBondFltStatus OBJECT-TYPE
			SYNTAX SheGBondFltStatusType
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"G.Bond (GBS) port Fault Status. This is a bitmap of possible
				conditions. The various bit positions are:
				  noPeer              - peer PHY cannot be reached (e.g.
				                        no BCEs attached, all BCEs are Down
				                        etc.).
				  peerPowerLoss       - peer PHY has indicated impending unit
				                        failure due to loss of local power
				                        ('Dying Gasp').
				  bceSubTypeMismatch  - local BCEs in the aggregation group
				                        are not of the same sub-type, e.g.
				                        some BCEs in the local device are -O
				                        while others are -R subtype.
				  lowRate             - sheGBondUpRate/sheGBondDownRate of the port
				                        has reached or dropped below
				                        sheGBondThreshLowUpRate/
				                        sheGBondThreshLowUpRate.
				
				This object is intended to supplement ifOperStatus object
				in IF-MIB."
			REFERENCE
				"IF-MIB, ifOperStatus; MAU-MIB, ifMauMediaAvailable"
			::= { sheGBondPortStatusEntry 3 }

		
		sheGBondPortSide OBJECT-TYPE
			SYNTAX SheGBondPortSideType
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"G.Bond port mode of operation (subtype).
				The value of 'subscriber' indicates the port is designated as
				'-R' subtype (all BCEs assigned to this port are of subtype
				'-R').
				The value of the 'office' indicates that the port is
				designated as '-O' subtype (all BCEs assigned to this port are
				of subtype '-O').
				The value of 'unknown' indicates that the port has no assigned
				BCEs yet or that the assigned BCEs are not of the same side
				(subTypeBCEMismatch).
				
				This object maps to the WT-157 attribute aGroupEnd."
			REFERENCE
				"[WT-157] 5.5.1.6"
			::= { sheGBondPortStatusEntry 4 }

		
		sheGBondNumBCEs OBJECT-TYPE
			SYNTAX Unsigned32 (0..32)
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"Number of BCEs that is currently aggregated by the local GBS
				(assigned to the G.Bond port using ifStackTable).
				This number is never greater than sheGBondCapacity.
				
				This object SHALL be automatically incremented or decremented
				when a BCE is added or deleted to/from the G.Bond port using
				sheGbondPortStackTable.
				
				This object maps to the WT-157 attribute aGroupNumChannels"
			REFERENCE
				"[WT-157] 5.5.1.9"
			::= { sheGBondPortStatusEntry 5 }

		
--     Port Notifications Group
		sheGBondPortNotifications OBJECT IDENTIFIER ::= { sheGBondPort 7 }

		
--     ifIndex is not needed here since we are under specific GBS
		sheGBondLowUpRateCrossing NOTIFICATION-TYPE
			OBJECTS { sheGBondUpDataRate, sheGBondThreshLowUpRate }
			STATUS current
			DESCRIPTION 
				"This notification indicates that the G.Bond port' upstream
				data rate has reached/dropped below or exceeded the low
				upstream rate threshold, specified by sheGBondThreshLowUpRate.
				
				This notification MAY be send for the -O subtype ports
				while the port is up, on the crossing event in both
				directions: from normal (rate is above the threshold) to low
				(rate equals the threshold or below it) and from low to
				normal. This notification is not applicable to the -R
				subtypes.
				
				It is RECOMMENDED that a small debouncing period of 2.5 sec,
				between the detection of the condition and notification,
				is implemented to prevent simultaneous LinkUp/LinkDown and
				sheGBondLowUpRateCrossing notifications to be sent.
				
				The adaptive nature of the G.Bond technology allows the port
				to adapt itself to the changes in the copper environment,
				e.g. an impulse noise, alien crosstalk or a micro-interruption
				may temporarily drop one or more BCEs in the aggregation
				group, causing a rate degradation of the aggregated G.Bond
				link. The dropped BCEs would then try to re-initialize,
				possibly at a lower rate than before, adjusting the rate to
				provide required target SNR margin.
				
				Generation of this notification is controlled by the
				sheGBondLowRateCrossingEnable object."
			::= { sheGBondPortNotifications 1 }

		
--     ifIndex is not needed here since we are under specific GBS
		sheGBondLowDownRateCrossing NOTIFICATION-TYPE
			OBJECTS { sheGBondDownDataRate, sheGBondThreshLowDownRate }
			STATUS current
			DESCRIPTION 
				"This notification indicates that the G.Bond port' downstream
				data rate has reached/dropped below or exceeded the low
				downstream rate threshold, specified by
				sheGBondThreshLowDownRate.
				
				This notification MAY be send for the -O subtype ports
				while the port is up, on the crossing event in both
				directions: from normal (rate is above the threshold) to low
				(rate equals the threshold or below it) and from low to
				normal. This notification is not applicable to the -R
				subtypes.
				
				It is RECOMMENDED that a small debouncing period of 2.5 sec,
				between the detection of the condition and notification,
				is implemented to prevent simultaneous LinkUp/LinkDown and
				sheGBondLowDownRateCrossing notifications to be sent.
				
				The adaptive nature of the G.Bond technology allows the port
				to adapt itself to the changes in the copper environment,
				e.g. an impulse noise, alien crosstalk or a micro-interruption
				may temporarily drop one or more BCEs in the aggregation
				group, causing a rate degradation of the aggregated G.Bond
				link. The dropped BCEs would then try to re-initialize,
				possibly at a lower rate than before, adjusting the rate to
				provide required target SNR margin.
				
				Generation of this notification is controlled by the
				sheGBondLowRateCrossingEnable object."
			::= { sheGBondPortNotifications 2 }

		
--  Notifications for GBond port configurations.
		sheGBondPortConfNotifications OBJECT IDENTIFIER ::= { sheGBondPortNotifications 3 }

		
		sheGbondPortCreated NOTIFICATION-TYPE
			OBJECTS { sheGBondPortStackControlIndex, sheGBondPortStackControlTimeStamp, sheGBondPortStackControlAssignedIfList, sheGBondPortStackControlBondingType, sheGBondPortStackControlACIRequestID
				 }
			STATUS current
			DESCRIPTION 
				"Event for successful portstack creation."
			::= { sheGBondPortConfNotifications 1 }

		
		sheGbondPortStackModified NOTIFICATION-TYPE
			OBJECTS { sheGBondPortStackControlIndex, sheGBondPortStackControlTimeStamp, sheGBondPortStackControlAssignedIfList }
			STATUS current
			DESCRIPTION 
				"Event for successful portstack modification."
			::= { sheGBondPortConfNotifications 2 }

		
		sheGbondPortDeleted NOTIFICATION-TYPE
			OBJECTS { sheGBondPortStackControlIndex, sheGBondPortStackControlTimeStamp }
			STATUS current
			DESCRIPTION 
				"Event for successful portstack delete."
			::= { sheGBondPortConfNotifications 3 }

		
		sheGBondPortConfigurationModified NOTIFICATION-TYPE
			OBJECTS { sheGBondPortConfControlIndex, sheGBondPortConfControlTimeStamp, sheGBondPortConfControlDiscoveryCode, sheGBondPortConfControlTargetUpDataRate, sheGBondPortConfControlTargetDownDataRate
				 }
			STATUS current
			DESCRIPTION 
				"Event for successful port conf modification."
			::= { sheGBondPortConfNotifications 4 }

		
		sheGBondPortThreshConfigurationModified NOTIFICATION-TYPE
			OBJECTS { sheGBondPortConfControlIndex, sheGBondPortConfControlTimeStamp, sheGBondPortConfControlThreshLowUpRate, sheGBondPortConfControlThreshLowDownRate, sheGBondPortConfControlLowRateCrossingEnable
				 }
			STATUS current
			DESCRIPTION 
				"Event for successful port threshold modification."
			::= { sheGBondPortConfNotifications 5 }

		
		sheGbondPortReqResult NOTIFICATION-TYPE
			OBJECTS { sheGBondPortStackControlIndex, sheGBondPortStackControlRequest, sheGBondPortStackControlTimeStamp, sheGBondPortStackControlReqResult }
			STATUS current
			DESCRIPTION 
				"Result notification for long running commands."
			::= { sheGBondPortConfNotifications 6 }

		
		sheGBondPortActivationFailed NOTIFICATION-TYPE
			OBJECTS { sheAlarmTimeAndDate, sheAlarmReportingSource, sheAlarmReportingSourceType, sheAlarmSeverity }
			STATUS current
			DESCRIPTION 
				"Portalarm if the IU reject the gbond portactivation request."
			::= { sheGBondPortNotifications 4 }

		
