--*****************************************************************************
--                   Statistics Collection
--*****************************************************************************

sc553StoreUserTotals OBJECT-TYPE
	SYNTAX INTEGER {
		   normal(1),  -- read-only
		   store(2)    -- write-only
	       }
	ACCESS  read-write
	STATUS  mandatory
	DESCRIPTION
		 "A set of this object will be broadcasted to every
		 sc553 in the shelf.  Upon receiving the broadcast,
		 the sc553 will write the contents of sc553TotalUserStat
		 to memory for collection at a later time.
		 A get of this object will always return a 1."
	::= { sc553Performance 10 }


sc553StoreUserIntervals OBJECT-TYPE
	SYNTAX INTEGER {
		   normal(1),  -- read-only
		   store(2)    -- write-only
	       }
	ACCESS  read-write
	STATUS  mandatory
	DESCRIPTION
		 "A set of this object will be broadcasted to every
		 sc553 in the shelf.  Upon receiving the broadcast,
		 the sc553 will write the contents of sc553UserIntervalStats
		 for each interval to memory for collection at a later time.
		 A get of this object will always return a 1."
	::= { sc553Performance 11 }


sc553ShelfUserTotals OBJECT-TYPE
	 SYNTAX OCTET STRING (SIZE (0..465))
	 ACCESS read-only
	 STATUS mandatory
	 DESCRIPTION
	 "Collects the User Totals that were stored by the set 
	  of sc553StoreUserTotals for all sc553s in the shelf.
          The data for each sc553 is contained in 15 octet groupings
          where each grouping is encoded in the following way:

	  Octet 1:     Control Octet containing the slot (1-32) of the sc553 
	               and an invalid/valid data flag. The data in the following
		       14 octets is invalid if the MSB is set.
          Octet 2,3:   Errored Seconds Total
	  Octet 4,5:   Bursty Errored Seconds Total
	  Octet 6,7:   Severely Errored Seconds Total
	  Octet 8,9:   Unavailable Seconds Total
	  Octet 10,11: CRC Errors Total
	  Octet 12,13: Loss of Frame Total
	  Octet 14,15: Controlled Slip Seconds Total

	  If there are no sc553s in the shelf then a null string is returned."
	
	::= { sc553Performance 12 }
 

sc553ShelfUserIntvlsTable OBJECT-TYPE
    SYNTAX  SEQUENCE OF Sc553ShelfUserIntervalsEntry
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
          "The sc553ShelfUserIntvlsTable contains the user statistics
	   for all sc553s in the shelf."
        ::= { sc553Performance 13 }

sc553ShelfUserIntervalsEntry 	OBJECT-TYPE
    SYNTAX  Sc553ShelfUserIntervalsEntry
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
        "An entry in the  Shelf User Intervals table."
    INDEX   { sc553ShelfUserIntvlsIndex }
    ::= { sc553ShelfUserIntvlsTable 1 }

Sc553ShelfUserIntervalsEntry ::=
    SEQUENCE {
        sc553ShelfUserIntvlsIndex		INTEGER,
        sc553ShelfUserIntervals  		OCTET STRING
}

sc553ShelfUserIntvlsIndex OBJECT-TYPE
    SYNTAX  INTEGER (1..96)
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
        "The index value which uniquely determines the interval."
    ::= { sc553ShelfUserIntervalsEntry 1 }

sc553ShelfUserIntervals OBJECT-TYPE
	 SYNTAX OCTET STRING (SIZE (0..465))
	 ACCESS read-only
	 STATUS mandatory
	 DESCRIPTION
	 "Collects the User Statistics that were stored by the set of 
	  sc553StoreUserIntervals for the interval given by
          sc553ShelfUserIntervalsIndex for all sc553s in the shelf.
          The data for each sc553 is contained in 15 octet groupings
          where each grouping is encoded in the following way:

	  Octet 1:     Control Octet containing the slot (1-32) of the sc553 
	               and an invalid/valid data flag. The data in the following
		       14 octets is invalid if the MSB is set.
          Octet 2,3:   Errored Seconds 
	  Octet 4,5:   Bursty Errored Seconds
	  Octet 6,7:   Severely Errored Seconds
	  Octet 8,9:   Unavailable Seconds
	  Octet 10,11: CRC Errors
	  Octet 12,13: Loss of Frame
	  Octet 14,15: Controlled Slip Seconds

	  If there are no sc553s in the shelf then a null string is returned."
	
	::= { sc553ShelfUserIntervalsEntry 2 }
 

END 
