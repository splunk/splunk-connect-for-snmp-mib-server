-- units of conformance
mrvEfmOamGroup  OBJECT-GROUP
OBJECTS {
    mrvEfmPhysicalIfIndex,
    mrvEfmOamIfIndex,
    mrvEfmOamPeerIfIndex,
    mrvEfmOamMode,
    mrvEfmOamCfg,
    mrvEfmOamPduCfg,
    mrvEfmOamFlagsField,
