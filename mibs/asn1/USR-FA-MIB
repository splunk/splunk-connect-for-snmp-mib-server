		--CONFIGURABLE
	::= { usrFASystemConfig 23 }
        usrFAStatsResetTime OBJECT-TYPE
                SYNTAX DateAndTime
                MAX-ACCESS  read-write
                STATUS  current
                DESCRIPTION
                        "Reading:
                                Time the FA MIP statistics counters were last
                                reset. The FA MIP statistics are valid from
                                usrFAStatsResetTime until present.
                                Resetting counters causes a discontinuity in
                                the monotonically increasing value of counters.
                                Management stations that sample these counters
                                need also to sample this object so that they
                                can know that a discontinuity occurred with
                                respect to the previous sample. Its value is
                                described in SNMPv2-TC (RFC1903).
                        Writing:
                            Writing of DateAndTime causes update of usrFAStatsResetTime
                                to the current time
                                and the FA MIP counters to be reset to 0:
                                usrFARegReqStats
                                usrFAInitRegReqStats
                                usrFAReRegReqStats
                                usrFADeRegReqStats
                                usrFARegReqDeniedByPDSNStats
                                usrFARegReqDeniedByHAStats
                                usrFAMIPAuthStats
                                usrFAMIPCallReleasedStats
                        This object replaces usrFAResetStats "
        ::= { usrFASystemConfig 24 }
        usrFAMPAStatsResetTime OBJECT-TYPE
                SYNTAX DateAndTime
                MAX-ACCESS  read-write
                STATUS  current
                DESCRIPTION
                        "Reading:
                                Time the FA MPA statistics counters were last
                                reset. The FA MPA statistics are valid from
                                usrFAMPAStatsResetTime until present.
                                Resetting counters causes a discontinuity in
                                the monotonically increasing value of counters.
                                Management stations that sample these counters
                                need also to sample this object so that they
                                can know that a discontinuity occurred with
                                respect to the previous sample.  Its value is
                                described in SNMPv2-TC (RFC1903).
                        Writing:
                            Writing of DateAndTime causes update of usrFAMPAStatsResetTime
                                to the current time
                                and the FA MPA counters to be reset to 0:
                                usrFAMPARegStat
                        This object replaces usrFAResetStats "
        ::= { usrFASystemConfig 25 }
-- Foreign Agent Registration Group
	usrFAVisitorTable OBJECT-TYPE
		SYNTAX SEQUENCE OF UsrFAVisitorEntry
		MAX-ACCESS not-accessible
		STATUS     current
		DESCRIPTION
			"A table containing the foreign agent's visitor
			 list.  The foreign agent updates this table in
			 response to registration events from mobile nodes."
	::= { usrFARegistration 1 }
	usrFAVisitorEntry OBJECT-TYPE
		SYNTAX UsrFAVisitorEntry
		MAX-ACCESS not-accessible
		STATUS     current
		DESCRIPTION
			"An entry in the Foreign Agent Visitors Table."
		INDEX { usrFAVisitorHomeAddress }
	::= { usrFAVisitorTable 1 }
	UsrFAVisitorEntry ::= SEQUENCE {
		usrFAVisitorHomeAddress       IpAddress,
		usrFAVisitorHomeAgentAddress  IpAddress,
		usrFAVisitorTimeGranted       Integer32,
		usrFAVisitorTimeRemaining     Integer32,
		usrFAVisitorRegFlags          Integer32,
		usrFAVisitorRegIDLow          UsrDisplayIntAsHex,
		usrFAVisitorRegIDHigh         UsrDisplayIntAsHex,
		usrFAVisitorRegIsAccepted     TruthValue
	}
	usrFAVisitorHomeAddress OBJECT-TYPE
		SYNTAX IpAddress
		MAX-ACCESS read-only
		STATUS     current
		DESCRIPTION
			"Home IP address of visiting mobile node."
	::= { usrFAVisitorEntry 1 }
	usrFAVisitorHomeAgentAddress OBJECT-TYPE
		SYNTAX IpAddress
		MAX-ACCESS read-only
		STATUS     current
		DESCRIPTION
			"Home agent IP address for this visiting mobile
			 node."
	::= { usrFAVisitorEntry 2 }
	usrFAVisitorTimeGranted OBJECT-TYPE
		SYNTAX Integer32
		MAX-ACCESS read-only
		STATUS     current
		DESCRIPTION
			"The lifetime in seconds granted to the mobile
			 node for this registration.  Only valid if
			 usrFAVisitorRegIsAccepted is true(1)."
	::= { usrFAVisitorEntry 3 }
	usrFAVisitorTimeRemaining OBJECT-TYPE
		SYNTAX Integer32
		MAX-ACCESS read-only
		STATUS     current
		DESCRIPTION
			"The number of seconds remaining until the
			 registration is expired.  It has the same initial
			 value as usrFAVisitorTimeGranted and is counted
			 down by this foreign agent."
	::= { usrFAVisitorEntry 4 }
	usrFAVisitorRegFlags OBJECT-TYPE
		SYNTAX Integer32 (0..255)
		MAX-ACCESS read-only
		STATUS     current
		DESCRIPTION
			"Registration flags as sent by the mobile node in the
			 Registration Request message."
	::= { usrFAVisitorEntry 5 }
	usrFAVisitorRegIDLow OBJECT-TYPE
		SYNTAX     UsrDisplayIntAsHex
		MAX-ACCESS read-only
		STATUS     current
		DESCRIPTION
			"Low 32 bits of Identification used in the mobile
			 node's Registration Request message."
	::= { usrFAVisitorEntry 6 }
	usrFAVisitorRegIDHigh OBJECT-TYPE
		SYNTAX     UsrDisplayIntAsHex
		MAX-ACCESS read-only
		STATUS     current
		DESCRIPTION
			"High 32 bits of Identification used in the mobile
			 node's Registration Request message."
	::= { usrFAVisitorEntry 7 }
	usrFAVisitorRegIsAccepted OBJECT-TYPE
		SYNTAX TruthValue
		MAX-ACCESS read-only
		STATUS     current
		DESCRIPTION
			"Whether the registration has been accepted or not.
			 If it is false(2), this registration is still
			 pending for reply."
	::= { usrFAVisitorEntry 8 }
