		SYNTAX IpAddress 
		MAX-ACCESS read-write
		STATUS     current
		DESCRIPTION
			"The IP address of PDSN/FA."
		--CONFIGURABLE
	::= { usrFaSaEntry 2 }
	usrFaSaSpi OBJECT-TYPE
		SYNTAX UsrSPI
		MAX-ACCESS read-write
		STATUS     current
		DESCRIPTION
			"The SPI is the 4-byte opaque index within
			 the FA-HA Security Association which
			 selects the specific security parameters
			 to be used to authenticate the peer."
		--CONFIGURABLE
	::= { usrFaSaEntry 3 }
	usrFaSaKey OBJECT-TYPE
		SYNTAX     UsrSecretDisplay
		MAX-ACCESS read-write
		STATUS     current
