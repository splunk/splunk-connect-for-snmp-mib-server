	usrFAResetStats OBJECT-TYPE
		SYNTAX Integer32 (1..1)
		MAX-ACCESS read-write
		STATUS     obsolete
		DESCRIPTION
			"Resets FA Statistics to zero.
                         This object is replaced by usrFAStatsResetTime."
	::= { usrFASystemConfig 21 }
	usrFaSaTable OBJECT-TYPE
		SYNTAX SEQUENCE OF UsrFaSaEntry
		MAX-ACCESS not-accessible
		STATUS     current
		DESCRIPTION
			"A table for configuring FA security
			 associations between PDSN/FA and the
			 Home Agent."
	::= { usrFASystemConfig 22 }
	usrFaSaEntry OBJECT-TYPE
		SYNTAX UsrFaSaEntry
		MAX-ACCESS not-accessible
		STATUS     current
		DESCRIPTION
			"An entry in the FA-HA Security Association
			 Table."
		INDEX { usrFaSaIndexNum }
	::= { usrFaSaTable 1 }
	UsrFaSaEntry ::= SEQUENCE {
		usrFaSaIndexNum    Integer32,
		usrFaSaIpAddress   IpAddress,
		usrFaSaSpi         UsrSPI,
		usrFaSaKey         UsrSecretDisplay,
		usrFaSaStatus      RowStatus
	}
	usrFaSaIndexNum OBJECT-TYPE
		SYNTAX Integer32 (1..100)
		MAX-ACCESS read-only
		STATUS     current
		DESCRIPTION
			"PDSN can support up to 100 FA-HA
			 security associations."
	::= { usrFaSaEntry 1 }
