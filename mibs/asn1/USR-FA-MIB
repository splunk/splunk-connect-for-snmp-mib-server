-- Foreign Agent System Configuration Group
	usrFAEncapsulationSupported OBJECT-TYPE
		SYNTAX Integer32 (0..7)
		MAX-ACCESS read-write
		STATUS     current
		DESCRIPTION
			"Encapsulation methods supported by this foreign
			 agent entity.  The foreign agent may support
			 multiple encapsulation methods or none of them.
			 This is a bit-mapped value with the following
			 representation:
			   7   6   5   4   3   2   1   0
			 +---+---+---+---+---+---+---+---+
			 | 0 | 0 | 0 | 0 | 0 | M | G | I |
			 +---+---+---+---+---+---+---+---+
			 where,
			 I - IP Encapsulation within IP
			 G - Generic Routing Encapsulation as per RFC1701
			 M - Minimal Encapsulation within IP."
		--CONFIGURABLE
	::= { usrFASystemConfig 1 }
	usrFAAdvRetransmit OBJECT-TYPE
		SYNTAX Integer32 (1..10)
		MAX-ACCESS read-write
		STATUS     current
		DESCRIPTION
			"Number of times to retransmit Agent Advertisement
			 if a Mobile Node does not respond with a valid
			 Registration Request."
		--CONFIGURABLE
	::= { usrFASystemConfig 2 }
	usrFAAdvInterval OBJECT-TYPE
		SYNTAX Integer32 (1..300)
		MAX-ACCESS read-write
		STATUS     current
		DESCRIPTION
			"The time (in seconds) between successive
			 transmissions of Agent Advertisements sent from
			 this Foreign Agent."
		--CONFIGURABLE
	::= { usrFASystemConfig 3 }
	usrFAMaxAdvLifetime OBJECT-TYPE
		SYNTAX Integer32 (1..65535)
		MAX-ACCESS read-write
		STATUS     current
		DESCRIPTION
			"The time (in seconds) to be placed in the Lifetime
			 field of the RFC 1256-portion of the Agent
			 Advertisement sent from this Foreign Agent."
		--CONFIGURABLE
	::= { usrFASystemConfig 4 }
        usrFAVisitorListSize OBJECT-TYPE
		SYNTAX Integer32 (0..8000)
		MAX-ACCESS read-write
		STATUS     current
		DESCRIPTION
                        "The maximum number of visitors that can be accepted
                         by this Foreign Agent. If the Performance Feature
                         is not enabled then the maximum size allowed is
                         only 4000."
		--CONFIGURABLE
	::= { usrFASystemConfig 5 }
	usrFAChallengeWindow OBJECT-TYPE
		SYNTAX Integer32 (1..5)
		MAX-ACCESS read-write
		STATUS     current
		DESCRIPTION
			"The size of the Challenge window, i.e. how
			 many of the past challenges plus current
			 are valid."
		--CONFIGURABLE
	::= { usrFASystemConfig 6 }
	usrFAMnAAAExtForward OBJECT-TYPE
		SYNTAX TruthValue
		MAX-ACCESS read-write
		STATUS     current
		DESCRIPTION
