		STATUS current
		DESCRIPTION
			"An entry in the FA Service Option Stats Table."
		INDEX { usrFAServiceOptionStatsTableIndex }
	::= { usrFAServiceOptionStatsTable 1 }
	UsrFAServiceOptionStatsEntry ::= SEQUENCE {
		usrFAServiceOptionStatsTableIndex     INTEGER,
		usrFAServiceOptionRegReqRcvd          Counter32,
		usrFAServiceOptionInitRegReqRcvd      Counter32,
		usrFAServiceOptionReRegReqRcvd        Counter32,
		usrFAServiceOptionDeRegReqRcvd        Counter32
	}
	usrFAServiceOptionStatsTableIndex OBJECT-TYPE
		SYNTAX INTEGER (1..65535)
		MAX-ACCESS read-only
		STATUS current
		DESCRIPTION
			"The value of Service Option received over All interface by PDSN."
	::= { usrFAServiceOptionStatsEntry 1 }
	usrFAServiceOptionRegReqRcvd OBJECT-TYPE
		SYNTAX Counter32
		MAX-ACCESS read-only
		STATUS current
		DESCRIPTION
			"Total number of valid MIP Registration Requests
			 received for the Service Option."
	::= { usrFAServiceOptionStatsEntry 2 }
	usrFAServiceOptionInitRegReqRcvd OBJECT-TYPE
		SYNTAX Counter32
		MAX-ACCESS read-only
		STATUS current
		DESCRIPTION
			"Total number of valid Initial MIP Registration Requests
			 received for the Service Option."
	::= { usrFAServiceOptionStatsEntry 3 }
	usrFAServiceOptionReRegReqRcvd OBJECT-TYPE
		SYNTAX Counter32
		MAX-ACCESS read-only
		STATUS current
		DESCRIPTION
			"Total number of valid Repeat MIP Registration Requests
			 received for the Service Option."
	::= { usrFAServiceOptionStatsEntry 4 }
	usrFAServiceOptionDeRegReqRcvd OBJECT-TYPE
		SYNTAX Counter32
		MAX-ACCESS read-only
		STATUS current
		DESCRIPTION
			"Total number of valid MIP Registration Requests
			 received with a Lifetime of zero (requests
			 to deregister) for this service option."
	::= { usrFAServiceOptionStatsEntry 5 }
-- Foreign Agent MPA Registration Statistics Group
	usrFAMPACreated OBJECT-TYPE
