     ::= { usrFAMIPDataStats 1 }
-- Foreign Agent MIP DMU Statistics (usrFAMIPDMUStats 15)
     usrFAMIPDMUUpdateKeyAR OBJECT-TYPE
          SYNTAX Counter32
          MAX-ACCESS read-only
          STATUS     current
          DESCRIPTION
               "Total number of DMU Update Key Access Rejects received from
               AAA. Update Key state is where the AAA requires that the Mobile
               generate a new set of keys and update the AAA."
     ::= { usrFAMIPDMUStats 1 }
     usrFAMIPDMUKeysUpdatedAR OBJECT-TYPE
          SYNTAX Counter32
          MAX-ACCESS read-only
          STATUS     current
          DESCRIPTION
               "Total number of DMU Keys Updated Access Rejects received from
               AAA. Keys Updated state is when the AAA has received a new set
               of keys from the Mobile and the user profile is updated."
     ::= { usrFAMIPDMUStats 2 }
-- Foreign Agent visitor tables which support Duplicate IP entries
	usrFAVisitor2Table OBJECT-TYPE
		SYNTAX SEQUENCE OF UsrFAVisitor2Entry
		MAX-ACCESS not-accessible
		STATUS     current
		DESCRIPTION
			"A table containing the foreign agent's visitor
			 list. This MIB supports duplicate entries in the foreign agent
       visitor table." 
	::= { usrFARegistration 23 }
	UsrFAVisitor2Entry ::= SEQUENCE {
		usrFAVisitor2ConnId            Integer32,
		usrFAVisitor2SessionId         Integer32,
		usrFAVisitor2HomeAddress       IpAddress,
		usrFAVisitor2HomeAgentAddress  IpAddress,
		usrFAVisitor2TimeGranted       Integer32,
		usrFAVisitor2TimeRemaining     Integer32,
		usrFAVisitor2RegFlags          Integer32,
		usrFAVisitor2RegIDLow          UsrDisplayIntAsHex,
		usrFAVisitor2RegIDHigh         UsrDisplayIntAsHex,
		usrFAVisitor2RegIsAccepted     TruthValue
	}
	usrFAVisitor2Entry OBJECT-TYPE
		SYNTAX UsrFAVisitor2Entry
		MAX-ACCESS not-accessible
		STATUS     current
		DESCRIPTION
			"An entry in the Foreign Agent Visitors Table which supports duplicate IP entries."
		INDEX { usrFAVisitor2ConnId, usrFAVisitor2SessionId }
	::= { usrFAVisitor2Table 1 }
	usrFAVisitor2ConnId OBJECT-TYPE
		SYNTAX Integer32 (0..2147483647)
		MAX-ACCESS not-accessible
		STATUS     current
		DESCRIPTION
			"The connection Id associated with the PPP connection
			 that the user is using to register."
	::= { usrFAVisitor2Entry 9 }
	usrFAVisitor2SessionId OBJECT-TYPE
		SYNTAX Integer32 (0..2147483647)
		MAX-ACCESS not-accessible
		STATUS     current
		DESCRIPTION
			"The session Id associated with the visitor session."
	::= { usrFAVisitor2Entry 10 }
	usrFAVisitor2HomeAddress OBJECT-TYPE
		SYNTAX IpAddress
		MAX-ACCESS read-only
		STATUS     current
		DESCRIPTION
			"Home IP address of visiting mobile node."
	::= { usrFAVisitor2Entry 1 }
	usrFAVisitor2HomeAgentAddress OBJECT-TYPE
		SYNTAX IpAddress
		MAX-ACCESS read-only
		STATUS     current
		DESCRIPTION
			"Home agent IP address for this visiting mobile
			 node."
	::= { usrFAVisitor2Entry 2 }
	usrFAVisitor2TimeGranted OBJECT-TYPE
		SYNTAX Integer32
		MAX-ACCESS read-only
		STATUS     current
		DESCRIPTION
			"The lifetime in seconds granted to the mobile
			 node for this registration.  Only valid if
			 usrFAVisitor2RegIsAccepted is true(1)."
	::= { usrFAVisitor2Entry 3 }
	usrFAVisitor2TimeRemaining OBJECT-TYPE
		SYNTAX Integer32
		MAX-ACCESS read-only
		STATUS     current
		DESCRIPTION
			"The number of seconds remaining until the
			 registration is expired.  It has the same initial
			 value as usrFAVisitor2TimeGranted and is counted
			 down by this foreign agent."
	::= { usrFAVisitor2Entry 4 }
	usrFAVisitor2RegFlags OBJECT-TYPE
		SYNTAX Integer32 (0..255)
		MAX-ACCESS read-only
		STATUS     current
		DESCRIPTION
			"Registration flags as sent by the mobile node in the
			 Registration Request message."
	::= { usrFAVisitor2Entry 5 }
	usrFAVisitor2RegIDLow OBJECT-TYPE
		SYNTAX     UsrDisplayIntAsHex
		MAX-ACCESS read-only
		STATUS     current
		DESCRIPTION
			"Low 32 bits of Identification used in the mobile
			 node's Registration Request message."
	::= { usrFAVisitor2Entry 6 }
	usrFAVisitor2RegIDHigh OBJECT-TYPE
		SYNTAX     UsrDisplayIntAsHex
		MAX-ACCESS read-only
		STATUS     current
		DESCRIPTION
			"High 32 bits of Identification used in the mobile
			 node's Registration Request message."
	::= { usrFAVisitor2Entry 7 }
	usrFAVisitor2RegIsAccepted OBJECT-TYPE
		SYNTAX TruthValue
		MAX-ACCESS read-only
		STATUS     current
		DESCRIPTION
			"Whether the registration has been accepted or not.
			 If it is false(2), this registration is still
			 pending for reply."
	::= { usrFAVisitor2Entry 8 }
END
