              ACCESS  read-write
              STATUS  mandatory
              DESCRIPTION
                      "The number of Auto Recoveries performed by this unit."
              ::=  { superStackHubTableEntry 35 }

          superStackHubCodeVersion      OBJECT-TYPE
              SYNTAX  OCTET STRING (SIZE (2..2))  
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      "The firmware version number of this unit."
              ::=  { superStackHubTableEntry 36 }

          superStackHubAutoRecovState         OBJECT-TYPE
              SYNTAX  INTEGER {
                              in-recovery(1),
                              normal(2),
                              halted(3)
                  }
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      "The current auto recovery state of this unit."
              ::=  { superStackHubTableEntry 37 }

          superStackHubRawCommandData      OBJECT-TYPE
              SYNTAX  OCTET STRING (SIZE (1..255))  
              ACCESS  read-write
              STATUS  mandatory
              DESCRIPTION
                      "This is used to send raw 485 serial data to
                       this unit."
              ::=  { superStackHubTableEntry 38 }

          superStackHubAutoRecovMode     OBJECT-TYPE
              SYNTAX  INTEGER {
                          selfEnabling(1),
                          non-selfEnabling(2)
                   }
              ACCESS  read-write
              STATUS  mandatory
              DESCRIPTION
                      "This parameter describes the auto recovery mode."
              ::=  { superStackHubTableEntry 39 }

          superStackHubRouterPortPhDet  OBJECT-TYPE
              SYNTAX  INTEGER {
                                  noPhantom(1),
                                  phantDetected(2)
                           }
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      "Phantom current status for the router port on rear. A value of 1 
                       corresponds to no phantom current detected. A value of 2 
                       corresponds to phantom presence." 
              ::=  { superStackHubTableEntry 40 }

          superStackHubRouterPortMask    OBJECT-TYPE
              SYNTAX  INTEGER {
                                  isolated(1),
                                  enabled(2)
                           }
              ACCESS  read-write
              STATUS  mandatory
              DESCRIPTION
                      "Administrative status for the router port on rear. By setting 
                      this parameter, a remote manager can control access to 
                      the lobe port." 
              ::=  { superStackHubTableEntry 41 }

          superStackHubRouterPortNsrtStatus    OBJECT-TYPE
              SYNTAX  INTEGER {
                                  bypassed(1),
                                  inserted(2)
                           }
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      "Actual insertion status for the router port on rear. A 1 value 
                       means that the corresponding station is not connected to 
                       the ring. A 2 value means that the corresponding station 
                       is connected to the ring." 
              ::=  { superStackHubTableEntry 42 }

          superStackHubRouterPortMacAddr       OBJECT-TYPE
              SYNTAX  OCTET STRING (SIZE (6..6))
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      "The MAC layer address of the station which is attached 
                       to the router port on rear."
              ::=  { superStackHubTableEntry 43 }

          superStackHubRouterPortBcnRemStatus  OBJECT-TYPE
              SYNTAX  INTEGER {
                          ok(1),
                          autoIsolated(2),
                          unknown(3)
                   }
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      "This parameter describes the result of any automatic 
                       beacon removal operations performed by the agent on the
                       router port. Automatic beacon removal operations consist of 
                       agent-initiated isolation of a lobe port in order to 
                       remove the cause of a persistent beaconing condition." 
              ::=  { superStackHubTableEntry 44 }

          superStackHubRouterPortBcnRemCause  OBJECT-TYPE
              SYNTAX  INTEGER {
                          no-removal(1),
                          phantom-error(2),
                          no-carrier(3),
                          bad-rate(4),
                          unknown(5)
                   }
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      "This parameter describes the cause of any automatic 
                       beacon removal operations performed by the agent on this
                       port." 
              ::=  { superStackHubTableEntry 45 }

          --- 

          superStackHubLobeNumber OBJECT-TYPE
              SYNTAX  INTEGER
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      "The number of items in the hub lobe table."
              ::= { superStackHubs 3 }


          -- Connections table for superStack hubs

          superStackHubLobeTable OBJECT-TYPE
              SYNTAX  SEQUENCE OF SuperStackHubLobeEntry
              ACCESS  not-accessible
              STATUS  mandatory
              DESCRIPTION     
                      "A table which contains the control and status 
                       information for the lobes."
              ::= { superStackHubs 4 }

          superStackHubLobeEntry OBJECT-TYPE
              SYNTAX  SuperStackHubLobeEntry
              ACCESS  not-accessible
              STATUS  mandatory
              DESCRIPTION
                      "A collection of control and status parameters kept for
                       each lobe."
              INDEX   { superStackHubIndex, superStackHubLobePort  }
              ::= { superStackHubLobeTable 1 }

          SuperStackHubLobeEntry ::=
                  SEQUENCE  {
                          superStackHubIndex             INTEGER,
                          superStackHubLobePort          INTEGER,
                          superStackHubLobeConfig        INTEGER,
                          superStackHubLobePhDet         INTEGER,
                          superStackHubLobeMask          INTEGER,
                          superStackHubLobeNsrtStatus    INTEGER,
                          superStackHubLobeMacAddr       OCTET STRING,
                          superStackHubLobeBcnRemStatus  INTEGER,
                          superStackHubLobeBcnRemCause   INTEGER
                   }

          superStackHubIndex OBJECT-TYPE
              SYNTAX  INTEGER
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      "The index into the superStackHubTable of the mau
                       this lobe is on."
              ::=  { superStackHubLobeEntry 1 }

          superStackHubLobePort OBJECT-TYPE
              SYNTAX  INTEGER
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      "The port of the mau this lobe is on."
              ::=  { superStackHubLobeEntry 2 }

           superStackHubLobeConfig OBJECT-TYPE
              SYNTAX  INTEGER {
                              user-normal(1),
                              ring-out-unused(2),
                              ring-in-unused(3),
                              unknown(4)
              }

              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      "The hardware configuration of the port."
              ::=  { superStackHubLobeEntry 3 }

          superStackHubLobePhDet  OBJECT-TYPE
              SYNTAX  INTEGER {
                              noPhantom(1),
                              phantDetected(2)
                           }
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      "Phantom current status for this lobe port. A value of 1 
                       corresponds to no phantom current detected. A value of 2 
                       corresponds to phantom presence." 
              ::=  { superStackHubLobeEntry 4 }

          superStackHubLobeMask    OBJECT-TYPE
              SYNTAX  INTEGER {
                                  isolated(1),
                                  enabled(2)
                           }
              ACCESS  read-write
              STATUS  mandatory
              DESCRIPTION
                      "Administrative status for this lobe port. By setting 
                      this parameter, a remote manager can control access to 
                      the lobe port." 
              ::=  { superStackHubLobeEntry 5 }

          superStackHubLobeNsrtStatus    OBJECT-TYPE
              SYNTAX  INTEGER {
                                  bypassed(1),
                                  inserted(2)
                           }
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      "Actual insertion status for this lobe port. A 1 value 
                       means that the corresponding station is not connected to 
                       the ring. A 2 value means that the corresponding station 
                       is connected to the ring." 
              ::=  { superStackHubLobeEntry 6 }

          superStackHubLobeMacAddr       OBJECT-TYPE
              SYNTAX  OCTET STRING (SIZE (6..6))
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      "The MAC layer address of the station which is attached 
                       to the corresponding lobe port."
              ::=  { superStackHubLobeEntry 7 }

          superStackHubLobeBcnRemStatus  OBJECT-TYPE
              SYNTAX  INTEGER {
                          ok(1),
                          autoIsolated(2),
                          unknown(3)
                   }
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      "This parameter describes the result of any automatic 
                       beacon removal operations performed by the agent on this
                       port. Automatic beacon removal operations consist of 
                       agent-initiated isolation of a lobe port in order to 
                       remove the cause of a persistent beaconing condition." 
              ::=  { superStackHubLobeEntry 8 }

          superStackHubLobeBcnRemCause  OBJECT-TYPE
              SYNTAX  INTEGER {
                          no-removal(1),
                          phantom-error(2),
                          no-carrier(3),
                          bad-rate(4),
                          unknown(5)
                   }
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      "This parameter describes the cause of any automatic 
                       beacon removal operations performed by the agent on this
                       port." 
              ::=  { superStackHubLobeEntry 9 }

