              SYNTAX    OCTET STRING (SIZE (2..2))
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      "The Version of FLASH release."
              ::= {  versions 3  }


          -- Configuration section 

          configEPROMSize OBJECT-TYPE
              SYNTAX  INTEGER
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      "The Quantity of EPROM installed (number of bytes)."
              ::= {  configuration 1  }

          configFLASHSize OBJECT-TYPE
              SYNTAX  INTEGER
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      "The Quantity of FLASH installed (number of bytes)."
              ::= {  configuration 2  }

          configFLASHCount OBJECT-TYPE
              SYNTAX  INTEGER
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      "The Quantity of FLASH installed (number of ICs)."
              ::= {  configuration 3  }

          configDRAMSize OBJECT-TYPE
              SYNTAX  INTEGER
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      "The Quantity of DRAM installed (number of bytes)."
              ::= {  configuration 4  }

          configDRAMCount OBJECT-TYPE
              SYNTAX  INTEGER
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      "The Quantity of DRAM installed (number of SIMMs)."
              ::= {  configuration 5  }

          configRS232Number OBJECT-TYPE
              SYNTAX  INTEGER
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      "The Number of RS-232 ports in the RS-232 port table."
              ::= { configuration 6  }  

          configRS232PortTable OBJECT-TYPE
              SYNTAX  SEQUENCE OF ConfigRS232PortEntry
              ACCESS  not-accessible
              STATUS  mandatory
              DESCRIPTION 
                      "A list of port entries. The number of entries is 
                       given by the value of configRS232Number."
              ::= { configuration 7 }

          configRS232PortEntry OBJECT-TYPE
              SYNTAX  ConfigRS232PortEntry
              ACCESS  not-accessible
              STATUS  mandatory
              DESCRIPTION
                      "Status and parameter values for a port."
              INDEX { configRS232PortIndex }
              ::= { configRS232PortTable 1 }

          ConfigRS232PortEntry ::=
              SEQUENCE {
                  configRS232PortIndex            INTEGER,
                  configRS232PortName             DisplayString,
                  configRS232BaudRate             INTEGER,
                  configRS232ModemStatus          INTEGER,
                  configRS232ConnectionStatus     INTEGER,
                  configRS232ModemConfigString    DisplayString,
                  configRS232OutputData           OCTET STRING (SIZE (0..42)),
                  configRS232InputData            OCTET STRING (SIZE (0..100)),
                  configRS232HardwareFlowControl  INTEGER
              }

          configRS232PortIndex OBJECT-TYPE
              SYNTAX  INTEGER
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      "A unique value for each port."
              ::= { configRS232PortEntry 1 }

          configRS232PortName OBJECT-TYPE
              SYNTAX  DisplayString
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      "A text description of the port, i.e. COM1."
              ::= { configRS232PortEntry 2 }

          configRS232BaudRate OBJECT-TYPE
              SYNTAX  INTEGER {
                      b1200(1),
                      b2400(2),
                      b4800(3),
                      b9600(4),
                      b19200(5),
                      b38400(6)
              }
              ACCESS  read-write
              STATUS  mandatory
              DESCRIPTION
                      "The baudrate at which the port is operating."
              ::= { configRS232PortEntry 3 }

          configRS232ModemStatus OBJECT-TYPE
              SYNTAX  INTEGER {
                      modemDetected(1),
                      autobauding(2),
                      noModemDetected(3)
              }
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      "The current status of the modem port connection."
              ::= { configRS232PortEntry 4 }

          configRS232ConnectionStatus OBJECT-TYPE
              SYNTAX  INTEGER {
                      connected(1),
                      connectionWait(2),
                      unknown(3)
              }
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      "The current status of the modem data connection."
              ::= { configRS232PortEntry 5 }

          configRS232ModemConfigString OBJECT-TYPE
              SYNTAX  DisplayString (SIZE (0..40))
              ACCESS  read-write
              STATUS  mandatory
              DESCRIPTION
                      "A user-configurable ASCII string used to initialize
                       a modem on this port. If this string is set to Z, the 
                       Star-Tek Agent sends ATZ<CR> to the modem during 
                       configuration."
              ::= { configRS232PortEntry 6 }

          configRS232OutputData OBJECT-TYPE
              SYNTAX  OCTET STRING (SIZE (0..42))
              ACCESS  read-write
              STATUS  mandatory
              DESCRIPTION
                      "Raw data to be sent out this port. Data can only 
                       be sent if the ConnectionStatus is connectionWait."
              ::= { configRS232PortEntry 7 }

          configRS232InputData OBJECT-TYPE
              SYNTAX  OCTET STRING (SIZE (0..100))
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      "The response, if any, to the output data."
              ::= { configRS232PortEntry 8 }

          configRS232HardwareFlowControl OBJECT-TYPE
              SYNTAX  INTEGER {
                      disabled(1),
                      enabled(2)
              }
              ACCESS  read-write
              STATUS  mandatory
              DESCRIPTION
                      "Controls the use of RTS-CTS hardware flow control during
                       modem connections."
              ::= { configRS232PortEntry 9 }

          configMonitorMode OBJECT-TYPE
              SYNTAX  INTEGER {
                      monitor(1),
                      slip(2)
              }
              ACCESS  read-write
              STATUS  mandatory
              DESCRIPTION
                      "The mode of operation of the monitor port. The monitor
                       port is COM2 (if present, COM1 if not)."
              ::= { configuration 8 }

          configMonitorInput OBJECT-TYPE
              SYNTAX  INTEGER {
                      enabled(1),
                      disabled(2)
              }
              ACCESS  read-write
              STATUS  mandatory
              DESCRIPTION
                      "Controls whether the monitor function will accept
                       input."
              ::= { configuration 9 }

          configMonitorDiags OBJECT-TYPE
              SYNTAX  INTEGER {
                      enabled(1),
                      disabled(2)
              }
              ACCESS  read-write
              STATUS  mandatory
              DESCRIPTION
                      "Controls whether the monitor function will produce
                       diagnostic output."
              ::= { configuration 10 }

          configParseControl OBJECT-TYPE
              SYNTAX  INTEGER {
                      parseAll(1),
                      parseMost(2),
                      parseLess(3),
                      parseNone(4)
              }
              ACCESS  read-write
              STATUS  mandatory
              DESCRIPTION
                      "Controls the degree to which frames are copied and
                       parsed. Under high network loads this control can be 
                       used to prevent agent overload."
              ::= { configuration 11 }

          configBootSource OBJECT-TYPE
              SYNTAX  INTEGER {
                      eprom(1),
                      flash(2)
              }
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      "Describes where the agent is executing code from."
              ::= { configuration 12 }



