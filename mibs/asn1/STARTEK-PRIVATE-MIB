          -- Stackable group
          --
          -- This group supports the LinkBuilder TRi series of
          -- stackable hubs.
          --

          stackableHubs      OBJECT IDENTIFIER ::= { stackable 1 }


          stackableHubNumber OBJECT-TYPE
              SYNTAX  INTEGER
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      "The number of items in hub table"
              ::= { stackableHubs 1 }

          stackableHubTable OBJECT-TYPE
              SYNTAX  SEQUENCE OF StackablehubTableEntry
              ACCESS  not-accessible
              STATUS  mandatory
              DESCRIPTION     
                      "A table which maintains a list of all of the intelligent
                       stackable series hubs on the ring being managed by the 
                       addressed agent. This table is indexed by the RS485 OOB 
                       address of the stackable series intelligent hubs."
              ::= { stackableHubs 2 }

          stackableHubTableEntry        OBJECT-TYPE
              SYNTAX  StackablehubTableEntry
              ACCESS  not-accessible
              STATUS  mandatory
              DESCRIPTION
                      "An entry in the stackableHubTable."
              INDEX   { stackableHubTableIndex }
              ::= { stackableHubTable 1 }

          StackablehubTableEntry ::=
              SEQUENCE  {
                      stackableHubTableIndex           INTEGER,
                      stackableHubModel                INTEGER,
                      stackableHub485Address           OCTET STRING,
                      stackableHubRiFTolState          INTEGER,
                      stackableHubRiMask               INTEGER,
                      stackableHubRiPhDet              INTEGER,
                      stackableHubRiNsrtStatus         INTEGER,
                      stackableHubRiBcnRemStatus       INTEGER,
                      stackableHubRiNeighborType       INTEGER,
                      stackableHubRiNeighborAddr       OCTET STRING,
                      stackableHubRiTimeStamp          TimeTicks,
                      stackableHubRoFTolState          INTEGER,
                      stackableHubRoMask               INTEGER,
                      stackableHubRoPhDet              INTEGER,
                      stackableHubRoNsrtStatus         INTEGER,
                      stackableHubRoBcnRemStatus       INTEGER,
                      stackableHubRoNeighborType       INTEGER,
                      stackableHubRoNeighborAddr       OCTET STRING,
                      stackableHubRoTimeStamp          TimeTicks,
                      stackableHubDataRateState        INTEGER,
                      stackableHubDataRateTimeStamp    TimeTicks,
                      stackableHubMgmtStatus           INTEGER,
                      stackableHubProxyAgentID         OCTET STRING,
                      stackableHubAttachedAgentType    INTEGER,
                      stackableHubAttachedAgentState   INTEGER,
                      stackableHubTemperatureStatus    INTEGER,
                      stackableHubAutoRecovControl     INTEGER,
                      stackableHubAutoRecovRetries     INTEGER,
                      stackableHubCodeVersion          OCTET STRING,
                      stackableHubAutoRecovState       INTEGER,
                      stackableHubRawCommandData       OCTET STRING,
                      stackableHubAutoRecovMode        INTEGER
                      }

          stackableHubTableIndex        OBJECT-TYPE
              SYNTAX  INTEGER
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      "The index for this hub entry"
              ::= { stackableHubTableEntry 1 }

          stackableHubModel     OBJECT-TYPE
              SYNTAX  INTEGER {
                      standard-issue(1)     -- Star-Tek stackable unit  
                      }
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION     
                      "The model number of the stackable series hub being
                       managed."
              ::= { stackableHubTableEntry 2 }

          stackableHub485Address        OBJECT-TYPE
              SYNTAX  OCTET STRING (SIZE (3..3))
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      "The 6 hex digit address used on the RS485 serial OOB 
                       management channel."
              ::=     { stackableHubTableEntry 3 }

          stackableHubRiFTolState       OBJECT-TYPE
              SYNTAX  INTEGER {
                          off(1),
                          on(2)
                  }
              ACCESS  read-write
              STATUS  mandatory
              DESCRIPTION
                     "This parameter controls the Ring In (RI) fault tolerance 
                      behavior. Off(1) means that phantom current is not used
                      on the corresponding RI interface as a condition for main
                      ring link insertion. On (2) means tha phantom current is
                      used on the corresponding RI interface as a condition for
                      main ring link insertion. The preferred setting for this
                      parameter is on (2)."
              ::= { stackableHubTableEntry 4 }

          stackableHubRiMask    OBJECT-TYPE
              SYNTAX  INTEGER {
                          isolate(1),
                          enable(2)
                  }
              ACCESS  read-write
              STATUS  mandatory
              DESCRIPTION
                      "Administrative status for this Ring In port. By
                       setting this parameter, a remote manager can control
                       the partitioning of this port."
              ::= { stackableHubTableEntry 5 }

          stackableHubRiPhDet   OBJECT-TYPE
              SYNTAX  INTEGER {
                          noPhantom(1),
                          phantDetected(2)
                  }
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      "This parameter describes the phantom detect status for
                       this Ring In port."
              ::= { stackableHubTableEntry 6 }

          stackableHubRiNsrtStatus  OBJECT-TYPE
              SYNTAX  INTEGER {
                          bypassed(1),
                          inserted(2)
                  }
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      "Insertion status for this Ring In port."
              ::= { stackableHubTableEntry 7 }

          stackableHubRiBcnRemStatus    OBJECT-TYPE
              SYNTAX  INTEGER {
                          ok(1),
                          autoIsolated(2)
                  }
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      "This parameter describes the result of any automatic 
                       beacon removal operations performed by the agent on this
                       port. Automatic beacon removal operations consist of 
                       agent-initiated isolation of a lobe port in order to 
                       remove the cause of a persistent beaconing condition." 
              ::= { stackableHubTableEntry 8 }

          stackableHubRiNeighborType    OBJECT-TYPE
              SYNTAX  INTEGER {
                          series828(1),
                          fSeries(2),
                          tokenRingMAC(3),
                          none(4),
                          stackable(5)
                  }
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      "This parameter indicates the type of nearest neighbor
                       on the Ring In port."
              ::= { stackableHubTableEntry 9 }

          stackableHubRiNeighborAddr    OBJECT-TYPE
              SYNTAX  OCTET STRING (SIZE (2..6))
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      "The address of the nearest neighbor on the Ring In port."
              ::= { stackableHubTableEntry 10 }

          stackableHubRiTimeStamp    OBJECT-TYPE
              SYNTAX  TimeTicks
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      "The time the status of Ring In (was) last changed."
              ::= { stackableHubTableEntry 11 }

          stackableHubRoFTolState       OBJECT-TYPE
              SYNTAX  INTEGER {
                          off(1),
                          on(2)
                   }
              ACCESS  read-write
              STATUS  mandatory
              DESCRIPTION
                      "This parameter controls the Ring Out(RO) fault tolerance 
                       behavior. Off(1) means that phantom current is not used
                       on the corresponding RO interface as a condition for main
                       ring link insertion. On (2) means tha phantom current is
                       used on the corresponding RO interface as a condition for
                       main ring link insertion. The preferred setting for this
                       parameter is on (2)."
              ::= { stackableHubTableEntry 12 }

          stackableHubRoMask    OBJECT-TYPE
              SYNTAX  INTEGER {
                          isolate(1),
                          enable(2)
                   }
              ACCESS  read-write
              STATUS  mandatory
              DESCRIPTION
                      "Administrative status for this Ring Out port. By
                       setting this parameter, a remote manager can control
                       the partitioning of this port."
              ::= { stackableHubTableEntry 13 }

          stackableHubRoPhDet   OBJECT-TYPE
              SYNTAX  INTEGER {
                          wireFault(1),
                          phantOk(2)      --corresponds to 
                                          --forward ok
                   }
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      "This parameter describes the phantom detect status for
                       this Ring Out port."
              ::= { stackableHubTableEntry 14 }

          stackableHubRoNsrtStatus      OBJECT-TYPE
              SYNTAX  INTEGER {
                          bypassed(1),
                          inserted(2)
                   }
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      "Insertion status for this Ring Out port."
              ::= { stackableHubTableEntry 15 }

          stackableHubRoBcnRemStatus    OBJECT-TYPE
              SYNTAX  INTEGER {
                          ok(1),
                          autoIsolated(2)
                   }
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      "This parameter describes the result of any automatic 
                       beacon removal operations performed by the agent on this
                       port. Automatic beacon removal operations consist of 
                       agent-initiated isolation of a lobe port in order to 
                       remove the cause of a persistent beaconing condition." 
              ::= { stackableHubTableEntry 16 }

          stackableHubRoNeighborType    OBJECT-TYPE
              SYNTAX  INTEGER {
                          series828(1),
                          fSeries(2),
                          tokenRingMAC(3),
                          none(4),
                          stackable(5)
                  }
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      "This parameter indicates the type of nearest neighbor
                       on the Ring Out port."
              ::= { stackableHubTableEntry 17 }

          stackableHubRoNeighborAddr    OBJECT-TYPE
              SYNTAX  OCTET STRING (SIZE (2..6))
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      "The address of the nearest neighbor on the Ring Out
                       port."
              ::= { stackableHubTableEntry 18 }

          stackableHubRoTimeStamp    OBJECT-TYPE
              SYNTAX  TimeTicks
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      "The time the status of Ring Out (was) last changed."
              ::= { stackableHubTableEntry 19 }

          stackableHubDataRateState     OBJECT-TYPE
              SYNTAX  INTEGER {
                          s4Mbps(1),
                          s16Mbps(2),
                          autoConfig(3)
                   }
              ACCESS  read-write
              STATUS  mandatory
              DESCRIPTION
                      "This parameter indicates the current data rate of
                       this unit."
              ::= { stackableHubTableEntry 20 }

          stackableHubDataRateTimeStamp    OBJECT-TYPE
              SYNTAX  TimeTicks
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      "The time the data rate status (was) last changed."
              ::= { stackableHubTableEntry 21 }

          stackableHubMgmtStatus     OBJECT-TYPE
              SYNTAX  INTEGER {
                          operational(1),
                          down(2)
                   }
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      "The operational status of this unit."
              ::= { stackableHubTableEntry 22 }

          stackableHubProxyAgentID      OBJECT-TYPE
              SYNTAX  OCTET STRING (SIZE (6..6))  -- the MAC address of the
                                                  -- TR interface in the agent
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      "The physical layer MAC Address of the agent."
              ::= { stackableHubTableEntry 23 }

          stackableHubAttachedAgentType      OBJECT-TYPE
              SYNTAX  INTEGER {
                              tokenring(1),
                              tokenring-ethernet(2),
                              reserved(3),
                              none(4)
                  }
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      "The type of the attached agent."
              ::=  { stackableHubTableEntry 24 }

          stackableHubAttachedAgentState     OBJECT-TYPE
              SYNTAX  INTEGER {
                              enabled(1),
                              disabled(2),
                              not-applicable(3)
                  }
              ACCESS  read-write
              STATUS  mandatory
              DESCRIPTION
                      "The state of the attached agent."
              ::=  { stackableHubTableEntry 25 }

          stackableHubTemperatureStatus      OBJECT-TYPE
              SYNTAX  INTEGER {
                              ok(1),
                              bad(2)
                  }
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      "The temperature status of this unit."
              ::=  { stackableHubTableEntry 26 }

          stackableHubAutoRecovControl       OBJECT-TYPE
              SYNTAX  INTEGER {
                              enabled(1),
                              disabled(2),
                              reset(3), 
                              unknown(4)
                  }
              ACCESS  read-write
              STATUS  mandatory
              DESCRIPTION
                      "This parameter controls the auto recovery operation
                       of this unit."
              ::=  { stackableHubTableEntry 27 }

          stackableHubAutoRecovRetries       OBJECT-TYPE
              SYNTAX  INTEGER (1..255)
              ACCESS  read-write
              STATUS  mandatory
              DESCRIPTION
                      "The number of Auto Recoveries performed by this unit."
              ::=  { stackableHubTableEntry 28 }

          stackableHubCodeVersion      OBJECT-TYPE
              SYNTAX  OCTET STRING (SIZE (2..2))  
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      "The firmware version number of this unit."
              ::= { stackableHubTableEntry 29 }

          stackableHubAutoRecovState         OBJECT-TYPE
              SYNTAX  INTEGER {
                              in-recovery(1),
                              normal(2),
                              halted(3)
                  }
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      "The auto recovery state of this unit."
              ::=  { stackableHubTableEntry 30 }

          stackableHubRawCommandData      OBJECT-TYPE
              SYNTAX  OCTET STRING (SIZE (1..255))  
              ACCESS  read-write
              STATUS  mandatory
              DESCRIPTION
                      "This is used to send raw 485 serial data to
                       this unit."
              ::= { stackableHubTableEntry 31 }

          stackableHubAutoRecovMode     OBJECT-TYPE
              SYNTAX  INTEGER {
                          selfEnabling(1),
                          non-selfEnabling(2)
                   }
              ACCESS  read-write
              STATUS  mandatory
              DESCRIPTION
                      "This parameter describes the auto recovery mode."
              ::= { stackableHubTableEntry 32 }


          --- 

          stackableHubLobeNumber OBJECT-TYPE
              SYNTAX  INTEGER
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      "The number of items in the hub lobe table."
              ::= { stackableHubs 3 }


          -- Connections table for stackable hubs

          stackableHubLobeTable OBJECT-TYPE
              SYNTAX  SEQUENCE OF StackablehubLobeEntry
              ACCESS  not-accessible
              STATUS  mandatory
              DESCRIPTION     
                      "A table which contains the control and status 
                       information for the lobes."
              ::= { stackableHubs 4 }

          stackableHubLobeEntry OBJECT-TYPE
              SYNTAX  StackablehubLobeEntry
              ACCESS  not-accessible
              STATUS  mandatory
              DESCRIPTION
                      "A collection of control and status parameters kept for
                       each lobe."
              INDEX   { stackableHubIndex, stackableHubLobePort  }
              ::= { stackableHubLobeTable 1 }

          StackablehubLobeEntry ::=
                  SEQUENCE  {
                          stackableHubIndex             INTEGER,
                          stackableHubLobePort          INTEGER,
                          stackableHubLobePhDet         INTEGER,
                          stackableHubLobeMask          INTEGER,
                          stackableHubLobeNsrtStatus    INTEGER,
                          stackableHubLobeMacAddr       OCTET STRING,
                          stackableHubLobeBcnRemStatus  INTEGER,
                          stackableHubLobeBcnRemCause   INTEGER,
                          stackableHubLobeTimeStamp     TimeTicks
                   }

          stackableHubIndex OBJECT-TYPE
              SYNTAX  INTEGER
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      "The index into the stackableHubTable of the mau
                       this lobe is on."
              ::=  { stackableHubLobeEntry 1 }

          stackableHubLobePort OBJECT-TYPE
              SYNTAX  INTEGER
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      "The port of the mau this lobe is on."
              ::=  { stackableHubLobeEntry 2 }

          stackableHubLobePhDet  OBJECT-TYPE
              SYNTAX  INTEGER {
                                  noPhantom(1),
                                  phantDetected(2)
                           }
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      "Phantom current status for this lobe port. A value of 1 
                       corresponds to no phantom current detected. A value of 2 
                       corresponds to phantom presence." 
              ::=  { stackableHubLobeEntry 3 }

          stackableHubLobeMask    OBJECT-TYPE
              SYNTAX  INTEGER {
                                  isolated(1),
                                  enabled(2)
                           }
              ACCESS  read-write
              STATUS  mandatory
              DESCRIPTION
                      "Administrative status for this lobe port. By setting 
                      this parameter, a remote manager can control access to 
                      the lobe port." 
              ::=  { stackableHubLobeEntry 4 }

          stackableHubLobeNsrtStatus    OBJECT-TYPE
              SYNTAX  INTEGER {
                                  bypassed(1),
                                  inserted(2)
                           }
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      "Actual insertion status for this lobe port. A 1 value 
                       means that the corresponding station is not connected to 
                       the ring. A 2 value means that the corresponding station 
                       is connected to the ring." 
              ::=  { stackableHubLobeEntry 5 }

          stackableHubLobeMacAddr       OBJECT-TYPE
              SYNTAX  OCTET STRING (SIZE (6..6))
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      "The MAC layer address of the station which is attached 
                       to the corresponding lobe port."
              ::=  { stackableHubLobeEntry 6 }

          stackableHubLobeBcnRemStatus  OBJECT-TYPE
              SYNTAX  INTEGER {
                          ok(1),
                          autoIsolated(2),
                          unknown(3)
                   }
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      "This parameter describes the result of any automatic 
                       beacon removal operations performed by the agent on this
                       port. Automatic beacon removal operations consist of 
                       agent-initiated isolation of a lobe port in order to 
                       remove the cause of a persistent beaconing condition." 
              ::=  { stackableHubLobeEntry 7 }

          stackableHubLobeBcnRemCause  OBJECT-TYPE
              SYNTAX  INTEGER {
                          no-removal(1),
                          phantom-error(2),
                          no-carrier(3),
                          bad-rate(4),
                          unknown(5)
                   }
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      "This parameter describes the cause of any automatic 
                       beacon removal operations performed by the agent on this
                       port." 
              ::=  { stackableHubLobeEntry 8 }

          stackableHubLobeTimeStamp      OBJECT-TYPE
              SYNTAX  TimeTicks
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      "The time the status of this lobe (was) last changed."
              ::=  { stackableHubLobeEntry 9 }

--  END



          -- SuperStack group
          --
          -- This group supports the SuperStack series of 
          -- stackable hubs.
          --

          superStackHubs      OBJECT IDENTIFIER ::= { stackable 2 }


          superStackHubNumber OBJECT-TYPE
              SYNTAX  INTEGER
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      "The number of items in hub table"
              ::= { superStackHubs 1 }

          superStackHubTable OBJECT-TYPE
              SYNTAX  SEQUENCE OF SuperStackHubTableEntry
              ACCESS  not-accessible
              STATUS  mandatory
              DESCRIPTION     
                      "A table which maintains a list of all of the intelligent
                       superStack series hubs on the ring being managed by the 
                       addressed agent. This table is indexed by the RS485 OOB 
                       address of the superStack series intelligent hubs."
              ::= { superStackHubs 2 }

          superStackHubTableEntry        OBJECT-TYPE
              SYNTAX  SuperStackHubTableEntry
              ACCESS  not-accessible
              STATUS  mandatory
              DESCRIPTION
                      "An entry in the superStackHubTable."
              INDEX   { superStackHubTableIndex }
              ::= { superStackHubTable 1 }

          SuperStackHubTableEntry ::=
              SEQUENCE  {
                      superStackHubTableIndex              INTEGER,                -- 1
                      superStackHubModel                   INTEGER,                -- 2
                      superStackHub485Address              OCTET STRING,           -- 3
                      superStackHubStackPosition           INTEGER,                -- 4
                      superStackHubRiConfiguration         INTEGER,                -- 5
                      superStackHubRiFTolState             INTEGER,                -- 6
                      superStackHubRiMask                  INTEGER,                -- 7
                      superStackHubRiPhDet                 INTEGER,                -- 8
                      superStackHubRiNsrtStatus            INTEGER,                -- 9
                      superStackHubRiBcnRemStatus          INTEGER,                -- 10
                      superStackHubRoConfiguration         INTEGER,                -- 11
                      superStackHubRoFTolState             INTEGER,                -- 12
                      superStackHubRoMask                  INTEGER,                -- 13
                      superStackHubRoPhDet                 INTEGER,                -- 14
                      superStackHubRoNsrtStatus            INTEGER,                -- 15
                      superStackHubRoBcnRemStatus          INTEGER,                -- 16
                      superStackHubCascadeUpMask           INTEGER,                -- 17
                      superStackHubCascadeUpPhDet          INTEGER,                -- 18
                      superStackHubCascadeUpNsrtStatus     INTEGER,                -- 19
                      superStackHubCascadeUpBcnRemStatus   INTEGER,                -- 20
                      superStackHubCascadeDownMask         INTEGER,                -- 21
                      superStackHubCascadeDownPhDet        INTEGER,                -- 22
                      superStackHubCascadeDownNsrtStatus   INTEGER,                -- 23
                      superStackHubCascadeDownBcnRemStatus INTEGER,                -- 24
                      superStackHubDataRateState           INTEGER,                -- 25
                      superStackHubDataDeviceTimeStamp     TimeTicks,              -- 26
                      superStackHubMgmtStatus              INTEGER,                -- 27
                      superStackHubProxyAgentID            OCTET STRING,           -- 28
                      superStackHubAttachedAgentType       INTEGER,                -- 29
                      superStackHubAttachedAgentState      INTEGER,                -- 30
                      superStackHubAttachedAgentControl    INTEGER,                -- 31
                      superStackHubTemperatureStatus       INTEGER,                -- 32
                      superStackHubPowerStatus             INTEGER,                -- 33
                      superStackHubAutoRecovControl        INTEGER,                -- 34
                      superStackHubAutoRecovRetries        INTEGER,                -- 35
                      superStackHubCodeVersion             OCTET STRING,           -- 36
                      superStackHubAutoRecovState          INTEGER,                -- 37
                      superStackHubRawCommandData          OCTET STRING,           -- 38
                      superStackHubAutoRecovMode           INTEGER,                -- 39
                      superStackHubRouterPortPhDet         INTEGER,                -- 40
                      superStackHubRouterPortMask          INTEGER,                -- 41
                      superStackHubRouterPortNsrtStatus    INTEGER,                -- 42
                      superStackHubRouterPortMacAddr       OCTET STRING,           -- 43
                      superStackHubRouterPortBcnRemStatus  INTEGER,                -- 44
                      superStackHubRouterPortBcnRemCause   INTEGER                 -- 45
                      }

          superStackHubTableIndex        OBJECT-TYPE
              SYNTAX  INTEGER
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      "The index for this hub entry"
              ::= { superStackHubTableEntry 1 }

          superStackHubModel     OBJECT-TYPE
              SYNTAX  INTEGER {
                      active-12-port-base-mau(1),         -- 3Com superStack base unit  
                      active-24-port-expansion-mau(2)     -- 3Com superStack expansion unit  
                      }
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION     
                      "The model number of the superStack series hub being
                       managed."
              ::= { superStackHubTableEntry 2 }

          superStackHub485Address        OBJECT-TYPE
              SYNTAX  OCTET STRING (SIZE (3..3))
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      "The 6 hex digit address used on the RS485 serial OOB 
                       management channel."
              ::=     { superStackHubTableEntry 3 }

          superStackHubStackPosition     OBJECT-TYPE
              SYNTAX  INTEGER 
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION     
                      "The integer position in a stack of units.  The top position
                       is 1."
              ::= { superStackHubTableEntry 4 }

          superStackRiConfiguration     OBJECT-TYPE
              SYNTAX  INTEGER {
                      no-ring-in-configured(1),
                      copper-ring-in-configured(2),
                      fiber-ring-in-configured(3),
                      copper-ring-in-disabled(4),
                      fiber-ring-in-disabled(5)
                      }
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION     
                      "This parameter describes the physical configured
                       state of Ring In.  Ring In is disabled if there is
                       another Ring In within the cascaded stack of hubs."
              ::= { superStackHubTableEntry 5 }

          superStackHubRiFTolState       OBJECT-TYPE
              SYNTAX  INTEGER {
                          off(1),
                          on(2)
                  }
              ACCESS  read-write
              STATUS  mandatory
              DESCRIPTION
                     "This parameter controls the Ring In (RI) fault tolerance 
                      behavior. Off(1) means that phantom current is not used
                      on the corresponding RI interface as a condition for main
                      ring link insertion. On (2) means tha phantom current is
                      used on the corresponding RI interface as a condition for
                      main ring link insertion. The preferred setting for this
                      parameter is on (2)."
              ::= { superStackHubTableEntry 6 }

          superStackHubRiMask    OBJECT-TYPE
              SYNTAX  INTEGER {
                          isolate(1),
                          enable(2)
                  }
              ACCESS  read-write
              STATUS  mandatory
              DESCRIPTION
                      "Administrative status for this Ring In port. By
                       setting this parameter, a remote manager can control
                       the partitioning of this port."
              ::= { superStackHubTableEntry 7 }

          superStackHubRiPhDet   OBJECT-TYPE
              SYNTAX  INTEGER {
                          noPhantom(1),
                          phantDetected(2)
                  }
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      "This parameter describes the phantom detect status for
                       this Ring In port."
              ::= { superStackHubTableEntry 8 }

          superStackHubRiNsrtStatus  OBJECT-TYPE
              SYNTAX  INTEGER {
                          bypassed(1),
                          inserted(2)
                  }
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      "Insertion status for this Ring In port."
              ::= { superStackHubTableEntry 9 }

          superStackHubRiBcnRemStatus    OBJECT-TYPE
              SYNTAX  INTEGER {
                          ok(1),
                          autoIsolated(2)
                  }
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      "This parameter describes the result of any automatic 
                       beacon removal operations performed by the agent on this
                       port. Automatic beacon removal operations consist of 
                       agent-initiated isolation of a lobe port in order to 
                       remove the cause of a persistent beaconing condition." 
              ::= { superStackHubTableEntry 10 }

          superStackRoConfiguration     OBJECT-TYPE
              SYNTAX  INTEGER {
                      no-ring-out-configured(1),
                      copper-ring-out-configured(2),
                      fiber-ring-out-configured(3),
                      copper-ring-out-disabled(4),
                      fiber-ring-out-disabled(5)
                      }
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION     
                      "This parameter describes the physical configured
                       state of Ring Out.  Ring Out is disabled if there is
                       another Ring Out within the cascaded stack of hubs."
              ::= { superStackHubTableEntry 11 }

          superStackHubRoFTolState       OBJECT-TYPE
              SYNTAX  INTEGER {
                          off(1),
                          on(2)
                   }
              ACCESS  read-write
              STATUS  mandatory
              DESCRIPTION
                      "This parameter controls the Ring Out(RO) fault tolerance 
                       behavior. Off(1) means that phantom current is not used
                       on the corresponding RO interface as a condition for main
                       ring link insertion. On (2) means tha phantom current is
                       used on the corresponding RO interface as a condition for
                       main ring link insertion. The preferred setting for this
                       parameter is on (2)."
              ::= { superStackHubTableEntry 12 }

          superStackHubRoMask    OBJECT-TYPE
              SYNTAX  INTEGER {
                          isolate(1),
