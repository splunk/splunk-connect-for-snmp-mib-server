          --         fTRLobe group

          fTRLobeNumber OBJECT-TYPE
              SYNTAX  INTEGER
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                  "The number of items in the lobe table"
              ::= { fTRLobe 1 }

          fTRLobeTable OBJECT-TYPE
              SYNTAX  SEQUENCE OF FTRLobeTableEntry
              ACCESS  not-accessible
              STATUS  mandatory
              DESCRIPTION     
                      "A table which maintains a list of all of the intelligent
                       Focus series hubs on the ring being managed by the 
                       addressed agent. This table is INDEXed by the RS485 OOB 
                       address of the Focus series intelligent hubs."
              ::= { fTRLobe 2 }

          fTRLobeTableEntry        OBJECT-TYPE
              SYNTAX  FTRLobeTableEntry
              ACCESS  not-accessible
              STATUS  mandatory
              DESCRIPTION
                      "An entry in the fTRLobeTable"
              INDEX   { fTRLobeTableIndex  }
              ::= { fTRLobeTable 1 }

          FTRLobeTableEntry ::=
                  SEQUENCE  {
                          fTRLobeTableIndex        INTEGER,
                          fTRLobeModel             INTEGER,
                          fTRLobe485Address        OCTET STRING,
                          fTRLobeSlotIndex         INTEGER,
                          fTRLobeChassisIndex      INTEGER,
                          fTRLobeDataRateState     INTEGER,
                          fTRLobeDataRateTimeStamp TimeTicks,
                          fTRLobeBkPlnNsrtState    INTEGER,
                          fTRLobeBkPlnTimeStamp    TimeTicks,
                          fTRLobeMgmtStatus        INTEGER,
                          fTRLobeProxyAgentID      OCTET STRING,
                          fTRLobeTemperatureStatus INTEGER,
                          fTRLobeAutoRecovControl  INTEGER,
                          fTRLobeAutoRecovMode     INTEGER,
                          fTRLobeAutoRecovRetries  INTEGER,
                          fTRLobeCodeVersion       OCTET STRING,
                          fTRLobeAutoRecovState    INTEGER,
                          fTRLobeRawCommandData    OCTET STRING,
                          fTRLobePowerStatus       INTEGER,
                          fTRLobeCageType          INTEGER
                       }

          fTRLobeTableIndex        OBJECT-TYPE
              SYNTAX  INTEGER
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      "The index for this lobe entry"
              ::= {  fTRLobeTableEntry 1 }

          fTRLobeModel     OBJECT-TYPE
