                              reserved4(8),
                              reserved5(9)
                  }
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      "The type of the attached agent."
              ::=  { superStackHubTableEntry 29 }

          superStackHubAttachedAgentState     OBJECT-TYPE
              SYNTAX  INTEGER {
                              not-applicable(1),
                              normal(2),
                              duplicate-detected(3),
