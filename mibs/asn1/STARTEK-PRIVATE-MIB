          -- Connections table for focus hubs

          fTRLobeConnectNumber OBJECT-TYPE
              SYNTAX  INTEGER
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      "The number of items in connection table"
              ::= { fTRLobe 3 }

          fTRLobeConnectTable OBJECT-TYPE
              SYNTAX  SEQUENCE OF FTRLobeConnectEntry
              ACCESS  not-accessible
              STATUS  mandatory
              DESCRIPTION     
                      "A table which contains the control and status 
                       information for the lobes."
              ::= { fTRLobe 4 }

          fTRLobeConnectEntry OBJECT-TYPE
              SYNTAX  FTRLobeConnectEntry
              ACCESS  not-accessible
              STATUS  mandatory
              DESCRIPTION
                      "A collection of control and status parameters kept for 
                       each lobe."
              INDEX   { fTRLobeIndex, fTRLobeConnectPort      }
              ::= { fTRLobeConnectTable 1 }

          FTRLobeConnectEntry ::=
                  SEQUENCE  {
                          fTRLobeIndex                INTEGER,
                          fTRLobeConnectPort          INTEGER,
                          fTRLobeConnectPhDet         INTEGER,
                          fTRLobeConnectMask          INTEGER,
                          fTRLobeConnectNsrtStatus    INTEGER,
                          fTRLobeConnectMacAddr       OCTET STRING,
                          fTRLobeConnectBcnRemStatus  INTEGER,
                          fTRLobeConnectBcnRemCause   INTEGER,
                          fTRLobeConnectTimeStamp     TimeTicks
                   }

          fTRLobeIndex OBJECT-TYPE
              SYNTAX  INTEGER
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      "The index into the fTRLobeTable of the mau this lobe is
                       on."
              ::=  { fTRLobeConnectEntry 1 }

          fTRLobeConnectPort OBJECT-TYPE
              SYNTAX  INTEGER
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      "The port of the mau this lobe is on."
              ::=  { fTRLobeConnectEntry 2 }

          fTRLobeConnectPhDet  OBJECT-TYPE
              SYNTAX  INTEGER {
                                  noPhantom(1),
                                  phantDetected(2)
                           }
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      "Phantom current status for this lobe port. A value of 1 
                       corresponds to no phantom current detected. A value of 2 
                       corresponds to phantom presence." 
              ::=  { fTRLobeConnectEntry 3 }

          fTRLobeConnectMask    OBJECT-TYPE
              SYNTAX  INTEGER {
                                  isolated(1),
                                  enabled(2)
                           }
              ACCESS  read-write
              STATUS  mandatory
              DESCRIPTION
                      "Administrative status for this lobe port. By setting 
                       this parameter, a remote manager can control access to 
                       the lobe port." 
              ::=  { fTRLobeConnectEntry 4 }

          fTRLobeConnectNsrtStatus    OBJECT-TYPE
              SYNTAX  INTEGER {
                                  bypassed(1),
                                  inserted(2)
                           }
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      "Actual insertion status for this lobe port. A 1 value 
                       means that the corresponding station is not connected to
                       the ring. A 2 value means that the corresponding station
                       is connected to the ring." 
              ::=  { fTRLobeConnectEntry 5 }

          fTRLobeConnectMacAddr       OBJECT-TYPE
              SYNTAX  OCTET STRING (SIZE (6..6))
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      "The MAC layer address of the station which is attached 
                       to the corresponding lobe port."
              ::=  { fTRLobeConnectEntry 6 }

          fTRLobeConnectBcnRemStatus  OBJECT-TYPE
              SYNTAX  INTEGER {
                          ok(1),
                          autoIsolated(2),
                          unknown(3)
                   }
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      "This parameter describes the result of any automatic 
                       beacon removal operations performed by the agent on this 
                       port. Automatic beacon removal operations consist of 
                       agent-initiated isolation of a lobe port in order to 
                       remove the cause of a persistent beaconing condition." 
              ::=  { fTRLobeConnectEntry 7 }

          fTRLobeConnectBcnRemCause  OBJECT-TYPE
              SYNTAX  INTEGER {
                          no-removal(1),
                          phantom-error(2),
                          no-carrier(3),
                          bad-rate(4),
                          unknown(5)
                   }
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      "This parameter describes the cause of any automatic 
                      beacon removal" 
              ::=  { fTRLobeConnectEntry 8 }

          fTRLobeConnectTimeStamp      OBJECT-TYPE
              SYNTAX  TimeTicks
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      "The time the status of this lobe (was) last changed."
              ::=  { fTRLobeConnectEntry 9 }


          -- fEthernet group
          --
          -- This section is NOT supported by the Star-Tek
          -- LinkBuilder TRi Model 1.
          --

          fEthernetNumber OBJECT-TYPE
              SYNTAX  INTEGER
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      "The number of items in concentrator table"
              ::= { fEthernet 1 }

          fEthernetTable OBJECT-TYPE
              SYNTAX  SEQUENCE OF FEthernetTableEntry
              ACCESS  not-accessible
              STATUS  mandatory
              DESCRIPTION     
                      "A table which maintains a list of all of the Focus
                       series ethernet concentrators in the repeater being 
                       managed by the addressed agent. This table is INDEXed 
                       by the repeater group id of the Focus series
                       ethernet concentrators."
              ::= { fEthernet 2 }

          fEthernetTableEntry        OBJECT-TYPE
              SYNTAX  FEthernetTableEntry
              ACCESS  not-accessible
              STATUS  mandatory
              DESCRIPTION
                      "An entry in the fEthernetTable"
              INDEX   { fEthernetTableIndex  }
              ::= { fEthernetTable 1 }

          FEthernetTableEntry ::=
                  SEQUENCE  {
                          fEthernetTableIndex        INTEGER,
                          fEthernetModel             INTEGER,
                          fEthernet485Address        OCTET STRING,
                          fEthernetSlotIndex         INTEGER,
                          fEthernetChassisIndex      INTEGER,
                          fEthernetBkPlnNsrtState    INTEGER,
                          fEthernetBkPlnTimeStamp    TimeTicks,
                          fEthernetMgmtStatus        INTEGER,
                          fEthernetProxyAgentID      OCTET STRING,
                          fEthernetTemperatureStatus INTEGER,
                          fEthernetCodeVersion       OCTET STRING,
                          fEthernetRawCommandData    OCTET STRING,
                          fEthernetPowerStatus       INTEGER,
                          fEthernetCageType          INTEGER
                       }

          fEthernetTableIndex        OBJECT-TYPE
              SYNTAX  INTEGER
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      "The index for this hub entry."
              ::= {  fEthernetTableEntry 1 }

          fEthernetModel     OBJECT-TYPE
              SYNTAX  INTEGER {
                                  f10160(1),
                                  f10161(2)
                           }
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION     
                      "The model number of the Focus series ethernet 
                       concentrator being managed."
              ::= {  fEthernetTableEntry 2 }

          fEthernet485Address        OBJECT-TYPE
              SYNTAX  OCTET STRING (SIZE (3..3))
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      "The 6 hex digit address used on the RS485 serial OOB 
                       management channel."
              ::=  {  fEthernetTableEntry 3 }

          fEthernetSlotIndex       OBJECT-TYPE
              SYNTAX  INTEGER
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      "The slot id."
              ::= { fEthernetTableEntry 4 }

          fEthernetChassisIndex       OBJECT-TYPE
              SYNTAX  INTEGER
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      "The index of the chassis the card is in."
              ::= { fEthernetTableEntry 5 }

          fEthernetBkPlnNsrtState     OBJECT-TYPE
              SYNTAX  INTEGER {
                          inserted(1),
                          bypassed(2)
                   }
              ACCESS  read-write
              STATUS  mandatory
              DESCRIPTION
                      "Backplane insertion status for this unit." 
              ::= { fEthernetTableEntry 6 }

          fEthernetBkPlnTimeStamp    OBJECT-TYPE
              SYNTAX  TimeTicks
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                     "The time the backplane insert status (was) last changed."
              ::= { fEthernetTableEntry 7 }

          fEthernetMgmtStatus     OBJECT-TYPE
              SYNTAX  INTEGER {
                          operational(1),
                          down(2)
                   }
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      "The operational status of this unit."
              ::= { fEthernetTableEntry 8 }

          fEthernetProxyAgentID      OBJECT-TYPE
              SYNTAX  OCTET STRING (SIZE (6..6))  -- the MAC address of the
                                                  -- interface in the agent
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      "The physical layer MAC Address of the agent."
              ::= { fEthernetTableEntry 9 }

          fEthernetTemperatureStatus     OBJECT-TYPE
              SYNTAX  INTEGER {
                          ok(1),
                          bad(2)
                   }
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      "The temperature status of this unit."
              ::= { fEthernetTableEntry 10 }

          fEthernetCodeVersion      OBJECT-TYPE
              SYNTAX  OCTET STRING (SIZE (2..2))  
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      "The firmware version number of this unit."
              ::= { fEthernetTableEntry 11 }

          fEthernetRawCommandData      OBJECT-TYPE
              SYNTAX  OCTET STRING (SIZE (1..255))  
              ACCESS  read-write
              STATUS  mandatory
              DESCRIPTION
                      "This is used to send raw 485 serial data to
                       this unit."
              ::= { fEthernetTableEntry 12 }

          fEthernetPowerStatus        OBJECT-TYPE
              SYNTAX  INTEGER {
                                  singleSupOK(1),                 
                                  dualSupOK(2),            
                                  dualSup-BSupBAD(3),
                                  dualSup-ASupBAD(4),
                                  illegal(5)
                  }
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      "The status of the power supply as seen by this unit."
              ::=  { fEthernetTableEntry 13 }

          fEthernetCageType OBJECT-TYPE
              SYNTAX  INTEGER {
                                  twoslot-10100(1),  
                                  fiveslot-10101(2),
                                  fiveslotredundant-10102(3),
                                  reserved4(4),    
                                  reserved5(5),             
                                  reserved6(6),
                                  reserved7(7),             
                                  reserved8(8)
                           }
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      "The model number of the fseries chassis the card is is."
              ::= {  fEthernetTableEntry 14 }

--
-- This is the beginning of the Star-Tek enterprise specific MIB.
-- This is implemented by all Star-Tek products which support
-- management via SNMP.
-- 
--         STARTEK-MIB DEFINTIONS ::= BEGIN
--
--           IMPORTS
--             enterprises            FROM RFC1155-SMI;


          rmonAgent     OBJECT IDENTIFIER ::= { fSeries 7 }

