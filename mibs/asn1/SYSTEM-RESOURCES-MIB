    SYNTAX          Integer32 (0..86400)
    UNITS           "Seconds"
    MAX-ACCESS      read-write
    STATUS          deprecated
    DESCRIPTION     "Internal type for Blue Coat Director use only."
    ::= { cpu 5 }

