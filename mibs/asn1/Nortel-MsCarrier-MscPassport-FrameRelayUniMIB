   mscFrUniDlciSpRowStatus
      RowStatus,
   mscFrUniDlciSpComponentName
      DisplayString,
   mscFrUniDlciSpStorageType
      StorageType,
   mscFrUniDlciSpIndex
      NonReplicated }

