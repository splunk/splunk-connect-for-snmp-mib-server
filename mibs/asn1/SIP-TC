        ::= { sipMIB 1 } 

   -- 
   -- Textual Conventions 
   -- 
   SipServerActions ::= TEXTUAL-CONVENTION 
           STATUS current 
           DESCRIPTION 
                "Lists the possible actions of a SIP network server. 
                 A specific action may be requested by a user agent 
                 as a Contact parameter in a REGISTER." 
           SYNTAX INTEGER { 
                          redirect(1), 
                          proxy(2) 
           } 

END
