                loSegmentAisle 	, loSegmentFrame ,	loSegmentRack , 
        		loSegmentSection , loSegmentCol , loSegmentStartingRow }        

        ::= { loIeSegTable 1 } 

        LoIeSegEntry ::= 
            SEQUENCE {  
            	loIeSegSize				INTEGER ,
                loIeSegReserve 			INTEGER ,
                loIeSegReservedBy		INTEGER ,
                loIeSegStartingAddress	INTEGER            	
            }  
