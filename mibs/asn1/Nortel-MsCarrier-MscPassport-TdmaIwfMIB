   mscTdmaBcV42BisRowStatus
      RowStatus,
   mscTdmaBcV42BisComponentName
      DisplayString,
   mscTdmaBcV42BisStorageType
      StorageType,
   mscTdmaBcV42BisIndex
      NonReplicated }

