   mscTdmaCsRlp1RowStatus
      RowStatus,
   mscTdmaCsRlp1ComponentName
      DisplayString,
   mscTdmaCsRlp1StorageType
      StorageType,
   mscTdmaCsRlp1Index
      INTEGER }

