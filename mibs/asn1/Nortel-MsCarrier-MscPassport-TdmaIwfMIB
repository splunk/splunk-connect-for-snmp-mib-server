   INDEX {
      mscTdmaBcCsIndex,
      mscTdmaBcBcIndex,
      mscTdmaBcFramerIndex }
::= { mscTdmaBcFramerStatsTable 1 }

MscTdmaBcFramerStatsEntry ::= SEQUENCE {
