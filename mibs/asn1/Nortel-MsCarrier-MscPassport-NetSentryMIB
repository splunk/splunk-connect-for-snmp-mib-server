   SYNTAX     MscVrNsFileRowStatusEntry
   ACCESS     not-accessible
   STATUS     mandatory
   DESCRIPTION
     "A single entry in the table represents a single 
      mscVrNsFile component."
   INDEX {
      mscVrIndex,
      mscVrNsIndex,
      mscVrNsFileIndex }
::= { mscVrNsFileRowStatusTable 1 }

MscVrNsFileRowStatusEntry ::= SEQUENCE {
   mscVrNsFileRowStatus
      RowStatus,
   mscVrNsFileComponentName
      DisplayString,
   mscVrNsFileStorageType
      StorageType,
   mscVrNsFileIndex
      AsciiStringIndex }

