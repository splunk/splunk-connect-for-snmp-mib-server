    STATUS      current
    DESCRIPTION 
        "Module Compliance"

    MODULE      -- this module

        MANDATORY-GROUPS        
                { dfl2560SystemObjectGroup, 
                  dfl2560IPsecObjectGroup, 
                  dfl2560StateCountersGroup, 
