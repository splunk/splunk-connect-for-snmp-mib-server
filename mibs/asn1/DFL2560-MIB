    ::= { dfl2560reg 3 }


dfl2560System OBJECT IDENTIFIER
    ::= { dfl2560OSStats 1 }

dfl2560SysCpuLoad OBJECT-TYPE
    SYNTAX      Gauge32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION 
        "The system cpu load."
    ::= { dfl2560System 1 }

