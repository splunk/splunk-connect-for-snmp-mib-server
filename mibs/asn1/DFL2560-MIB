    dfl2560PipeIndex            Integer32,
    dfl2560PipeName             DisplayString,
    dfl2560PipeMinPrec          Integer32,
    dfl2560PipeMaxPrec          Integer32,
    dfl2560PipeDefPrec          Integer32,
    dfl2560PipeNumPrec          Integer32,
    dfl2560PipeNumUsers         Gauge32,
    dfl2560PipeCurrentBps       Gauge32,
    dfl2560PipeCurrentPps       Gauge32,
    dfl2560PipeDelayedPackets   Counter32,
    dfl2560PipeDropedPackets    Counter32
}

