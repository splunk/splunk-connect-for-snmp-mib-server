cxMcVoxRegenOpe        OBJECT IDENTIFIER ::= { cxMcVox 28 }

cxMcVoxRegenOpeExt   OBJECT-TYPE
   SYNTAX INTEGER {  
          disabled (1),
          enabled (2)
        }
   ACCESS  read-write
   STATUS  mandatory
   DESCRIPTION
"The RegenOpeExt is used to specify if the local extension tones are
to be regenerated."
   DEFVAL { disabled }
   --#STARTING-VERSION 3
   ::= { cxMcVoxRegenOpe 1 }

cxMcVoxRegenOpeGid      OBJECT-TYPE
   SYNTAX INTEGER {  
          disabled (1),
          enabled (2)
        }
   ACCESS  read-write
   STATUS  mandatory
   DESCRIPTION
"The RegenOpeGid is used to specify if the group ids tones are to be
regenerated."
   DEFVAL { disabled }
   --#STARTING-VERSION 3
   ::= { cxMcVoxRegenOpe 2 }

cxMcVoxRegenOpeNbDigits OBJECT-TYPE
   SYNTAX INTEGER (1..8)
   ACCESS  read-write
   STATUS  mandatory
   DESCRIPTION
"The number of digits to be regenerated."
   DEFVAL { 3 }
   --#STARTING-VERSION 3
   ::= { cxMcVoxRegenOpe 3 }

cxMcVoxRegenOpeExtBitMask OBJECT-TYPE
   SYNTAX INTEGER (0..255)
   ACCESS  read-write
   STATUS  mandatory
   DESCRIPTION
"This object is a bitmap that selects up to 
cxMcVoxRegenOpeNbDigits, which can be set up to 
8 digits, corresponding to a bit that is set. The default
value corresponds to 7 (or 00000111 in binary).

Range of Values: 0 to 255

Default Value: 7"
   DEFVAL { 7 }
   --#STARTING-VERSION 11
   ::= { cxMcVoxRegenOpe 4 }

-- ======================================================================

--  cxMcVox OPERATIONAL CODE TRANSLATION GROUP
-- 


cxMcVoxTranslOpe      OBJECT IDENTIFIER ::= {cxMcVox 29 }

cxMcVoxTranslOpeCntryCodeEnable      OBJECT-TYPE
   SYNTAX INTEGER {  
          disabled (1),
          enabled (2)
        }
   ACCESS  read-write
   STATUS  mandatory
   DESCRIPTION
"This specifies whether to translate the local country code."
   DEFVAL { disabled }
   --#STARTING-VERSION 3
   ::= { cxMcVoxTranslOpe 1 }


cxMcVoxTranslOpeCntryCode OBJECT-TYPE
   SYNTAX  DisplayString (SIZE (1..4))
   ACCESS  read-write
   STATUS  mandatory
   DESCRIPTION

"Displays the number of the operational local country code which will serve 
in the verification of the phone number to be dialed depending on where 
the remote site is installed.

Range of Values: 1 to 4 digits from 0 to 9

Default Value: none."
	DEFVAL { "1" }
   --#STARTING-VERSION 3
   ::= { cxMcVoxTranslOpe 2 }


cxMcVoxTranslOpeCntryCodeLng    OBJECT-TYPE
   SYNTAX  INTEGER (1..4)
   ACCESS  read-only
   STATUS  mandatory
   DESCRIPTION

"Displays the length (in digits) of the operational local country code.

Range of Values: 1 to 4.

Default Value: none"
	DEFVAL { 1 }
   --#STARTING-VERSION 3
   ::= { cxMcVoxTranslOpe 3 }


cxMcVoxTranslOpeCCPrefix OBJECT-TYPE
   SYNTAX  DisplayString (SIZE (1..4))
   ACCESS  read-write
   STATUS  mandatory
   DESCRIPTION

"Displays the number of the operational local country code which will be 
inserted in front of the Country Code if the latter does not correspond to 
the local country code parameter.

Range of Values: 1 to 4 digits from 0 to 9

Default Value: none"
	DEFVAL { "2" }
   --#STARTING-VERSION 3
   ::= { cxMcVoxTranslOpe 4 }


cxMcVoxTranslOpeCCPrefixLng    OBJECT-TYPE
   SYNTAX  INTEGER (1..4)
   ACCESS  read-only
   STATUS  mandatory
   DESCRIPTION

"Displays the length (in digits) of the operational non local country code.

Range of Values: 1 to 4.

Default Value: none"
	DEFVAL { 1 }
   --#STARTING-VERSION 3
   ::= { cxMcVoxTranslOpe 5 }


cxMcVoxTranslOpeAreaCodeEnable      OBJECT-TYPE
   SYNTAX INTEGER {  
          disabled (1),
          enabled (2)
