--  cxMcVox ADMINISTRATIVE FXO DRIVER GROUP


cxMcVoxFxoAdmTable OBJECT-TYPE
   SYNTAX  SEQUENCE OF CxMcVoxFxoAdmEntry
   ACCESS  not-accessible
   STATUS  mandatory
   DESCRIPTION
      "cx Voice Station Administrative FXO Table."
   ::= { cxMcVoxDriverAdm 3 }

cxMcVoxFxoAdmEntry OBJECT-TYPE
   SYNTAX  CxMcVoxFxoAdmEntry
   ACCESS  not-accessible
   STATUS  mandatory
   DESCRIPTION
      "cx Voice Station Administrative FXO Entry "
   INDEX { cxMcVoxFxoAdmCardUsed, cxMcVoxFxoAdmPortUsed }
   ::= { cxMcVoxFxoAdmTable 1 }

CxMcVoxFxoAdmEntry ::=
   SEQUENCE {
      cxMcVoxFxoAdmCardUsed           INTEGER,        
      cxMcVoxFxoAdmPortUsed           INTEGER,
      cxMcVoxFxoAdmPortStatus         INTEGER,
      cxMcVoxFxoAdmVocoder            INTEGER,
      cxMcVoxFxoAdmFaxBw              INTEGER,
      cxMcVoxFxoAdmAutoCnx            INTEGER,
      cxMcVoxFxoAdmPathId             DisplayString,
      cxMcVoxFxoAdmTxGain             INTEGER,
      cxMcVoxFxoAdmRxGain             INTEGER,
      cxMcVoxFxoAdmEchoCancel         INTEGER,
      cxMcVoxFxoAdmSignaling          INTEGER,
      cxMcVoxFxoAdmCnctType           INTEGER,  
      cxMcVoxFxoAdmRingType           INTEGER,
      cxMcVoxFxoAdmImpedance          INTEGER,
      cxMcVoxFxoAdmDialType           INTEGER,
      cxMcVoxFxoAdmDidSignalType      INTEGER,
      cxMcVoxFxoAdmRmtExt             DisplayString,
      cxMcVoxFxoAdmRmtId              DisplayString,
      cxMcVoxFxoAdmTranspMode         INTEGER,
      cxMcVoxFxoAdmFaxEnable          INTEGER,
      cxMcVoxFxoAdmBroadcast          INTEGER
    }

-- USER MODE OBJECTS:

      cxMcVoxFxoAdmCardUsed         OBJECT-TYPE
     SYNTAX INTEGER (1..8)
     ACCESS  read-only
     STATUS  mandatory
     DESCRIPTION
"Displays the slot number of the voice I/O card.

You cannot modify this object. It is a display that the software provides.

Range of Values

CX900: 1 to 6

CX1000: 1 to 8

Default: none"
     ::= { cxMcVoxFxoAdmEntry 1 }

      cxMcVoxFxoAdmPortUsed         OBJECT-TYPE
     SYNTAX INTEGER (1..15)
     ACCESS  read-only
     STATUS  mandatory
     DESCRIPTION
"Displays the number of the voice I/O port.

You cannot modify this object. It is a display that the software provides.

Options:

CX900: 1 is the only acceptable value. Each I/O card only has 1 port.

CX1000: up to 15 for the DI600

Default: none"
     ::= { cxMcVoxFxoAdmEntry 2 }

      cxMcVoxFxoAdmPortStatus       OBJECT-TYPE
     SYNTAX   INTEGER { 
          disable (1),
          enable (2) 
           }
     ACCESS  read-write
     STATUS  mandatory
     DESCRIPTION
"Determines the functional status of the administrative port.

Options:

disable (1): port is not functional

enable (2): port is up and active

Default Value: disable (1)

Configuration Changed: administrative"
     DEFVAL { disable }
     ::= { cxMcVoxFxoAdmEntry 3 }

      cxMcVoxFxoAdmVocoder OBJECT-TYPE
     SYNTAX INTEGER { bps5800 (1),
              bps8000 (2),
              kbps40 (3),
              kbps32 (4),
              kbps24 (5),
              kbps16 (6),
              opt7 (7),
              opt8 (8)
            }
     ACCESS  read-write
     STATUS  mandatory
     DESCRIPTION 
"Determines the transmission speed (in bits per second) of the compressed 
voice stream on the administrative FXO port. Defining the speed also sets 
the ACELP voice compression ratio.

Options:

bps5800 (1): compresses at a higher rate and uses slightly less bandwidth.

bps8000 (2): compresses at approximately an 8:1 ratio (lower rate) with a 
higher quality voice sound at the receiving end.

opt3 (3): ADPCM 40.

opt4 (4): ADPCM 32.

opt5 (5): ADPCM 24.

opt6 (6): ADPCM 16.

opt7 (7): Memotec use only.

opt8 (8): Memotec use only.

Default Value: bps8000 (2)

Configuration Changed: administrative"
     DEFVAL { bps8000 }
     ::= { cxMcVoxFxoAdmEntry 4 }

      cxMcVoxFxoAdmFaxBw      OBJECT-TYPE
     SYNTAX   INTEGER { 
          bps2400 (1),
          bps4800 (2),
          bps7200 (3),
          bps9600 (4)
           }
     ACCESS  read-write
     STATUS  mandatory
     DESCRIPTION
"Determines the speed (in bits per second) of the FAX channel on the 
administrative FXO port. This value also determines the bandwidth required 
to pass FAX traffic through the voice software. This value can be set to 
match the speed of your FAX machine or it can be lowered to reduce 
bandwidth allocation.

Options:

bps2400 (1)

bps4800 (2)

bps7200 (3)

bps9600 (4)

Default Value: bps7200 (3)

Configuration Changed: administrative"
     DEFVAL { bps7200 }
     ::= { cxMcVoxFxoAdmEntry 5 }

-- obsoleted (has been redefined)

      cxMcVoxFxoAdmAutoCnx OBJECT-TYPE
     SYNTAX   INTEGER { 
          no (1),
          yes (2) 
           }
     ACCESS  read-write
     STATUS  obsolete
     DESCRIPTION
"obsolete"
     DEFVAL { no }
      ::= { cxMcVoxFxoAdmEntry 6 }

      cxMcVoxFxoAdmPathId        OBJECT-TYPE
     SYNTAX  DisplayString (SIZE (1..4))
     ACCESS  read-write
     STATUS  deprecated
     DESCRIPTION
"Path IDs no longer exist with the new Voice Routing (VR) 
scheme.

The Path ID is a numeric dialing code the user enters to gain access to 
the remote telephone or fax machine.

Note: The number of digits in the Path ID must match the value of the 
object cxMcVoxPathAdmPathLng of the cxMcVox Administrative Path Group.

Range of Values: 1 to 4 numeric digits

Default Value: none

Configuration Changed: administrative"
	 --#ENDING-VERSION 1
	 DEFVAL { "000" }
     ::= { cxMcVoxFxoAdmEntry 7 }

      cxMcVoxFxoAdmTxGain     OBJECT-TYPE
     SYNTAX INTEGER (820..1180)
     ACCESS  read-write
     STATUS  mandatory
     DESCRIPTION
"Determines the value defining the sound intensity (loudness) of voice 
transmission on the administrative FXO port. The transmission gain can be 
modified in the administrative configuration or be fine-tuned dynamically.

Note: Transmission gains should be modified during a connection (i.e. 
dynamically) as changes in the value affect the local listener.

Range of Values: 820 to 1180 (-18.0dB to 18.0dB)

Default Value: 960

Configuration Changed: administrative"
     DEFVAL { 960 }
     ::= { cxMcVoxFxoAdmEntry 8 }

      cxMcVoxFxoAdmRxGain     OBJECT-TYPE
     SYNTAX INTEGER (820..1180)
     ACCESS  read-write
     STATUS  mandatory
     DESCRIPTION
"Determines the value defining the sound intensity (loudness) of voice 
reception on the administrative FXO port. The reception gain can be 
modified in the administrative configuration or be fine-tuned dynamically.

Note: Reception gains should be modified during a connection (i.e. 
dynamically) as changes in the value affect the remote listener.

Range of Values: 820 to 1180 (-18.0dB to 18.0dB)

Default Value: 960

Configuration Changed: administrative"
     DEFVAL { 960 }
     ::= { cxMcVoxFxoAdmEntry 9 }

      cxMcVoxFxoAdmEchoCancel OBJECT-TYPE
     SYNTAX   INTEGER { 
          disable (1),
          enable (2) 
           }
     ACCESS  read-write
     STATUS  mandatory
     DESCRIPTION
"Determines the state of echo cancellation on the administrative FXO port. 
When set to 'enable' at both the local and remote ends, echo cancellation 
stops echoes from the remote that are heard through the receiver at the 
local end.

Options:

disable (1)

enable (2)

Default Value: enable (2)

Configuration Changed: administrative"
     DEFVAL { enable }
     ::= { cxMcVoxFxoAdmEntry 10 }

      cxMcVoxFxoAdmSignaling  OBJECT-TYPE
     SYNTAX   INTEGER { 
          ground-start (1),
          loop-start   (2),
          did          (3)
           }
     ACCESS  read-write
     STATUS  mandatory
     DESCRIPTION
"Determines the type of signalling the administrative FXO port uses to 
seize the line.

Options:

ground-start (1)

loop-start (2)

did (3)

Default Value: loop-start (2)

Configuration Changed: administrative"
     DEFVAL { loop-start }
     ::= { cxMcVoxFxoAdmEntry  11 }

