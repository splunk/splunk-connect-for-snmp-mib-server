-- the port loading table
cwspLoadTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF CwspLoadEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "A table specifying the load information for each
        interface."
    ::= { cwspLoad 1 }
 
cwspLoadEntry OBJECT-TYPE
    SYNTAX      CwspLoadEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION 
        "An entry contains the load information."
    INDEX { ifIndex }
    ::= { cwspLoadTable 1 }
 
CwspLoadEntry ::= SEQUENCE {
    cwspLoadBwTotal     Integer32,
    cwspLoadMaxBwCbr    Integer32,
    cwspLoadMaxBwRtVbr  Integer32,
    cwspLoadMaxBwNrtVbr Integer32,
    cwspLoadMaxBwAbr    Integer32,
    cwspLoadMaxBwUbr    Integer32,
    cwspLoadBwAvail     Integer32,
    cwspLoadAvlBwCbr    Integer32,
    cwspLoadAvlBwRtVbr  Integer32,
    cwspLoadAvlBwNrtVbr Integer32,
    cwspLoadAvlBwAbr    Integer32,
    cwspLoadAvlBwUbr    Integer32,
    cwspLoadVcAvail     Integer32,
    cwspLoadAvlVcCbr    Integer32,
    cwspLoadAvlVcRtVbr  Integer32,
    cwspLoadAvlVcNrtVbr Integer32,
    cwspLoadAvlVcAbr    Integer32,
    cwspLoadAvlVcUbr    Integer32,
    cwspLoadCtdCbr      Integer32,
    cwspLoadCtdRtVbr    Integer32,
    cwspLoadCtdNrtVbr   Integer32,
    cwspLoadCtdAbr      Integer32,
    cwspLoadCtdUbr      Integer32,
    cwspLoadCdvCbr      Integer32,
    cwspLoadCdvRtVbr    Integer32,
    cwspLoadCdvNrtVbr   Integer32,
    cwspLoadCdvAbr      Integer32,
    cwspLoadCdvUbr      Integer32,
    cwspLoadClr0Cbr     Integer32,
    cwspLoadClr0RtVbr   Integer32,
    cwspLoadClr0NrtVbr  Integer32,
    cwspLoadClr0Abr     Integer32,
    cwspLoadClr0Ubr     Integer32,
    cwspLoadClr01Cbr    Integer32,
    cwspLoadClr01RtVbr  Integer32,
    cwspLoadClr01NrtVbr Integer32,
    cwspLoadClr01Abr    Integer32,
    cwspLoadClr01Ubr    Integer32,
    cwspLoadMinGurCrCbr Integer32,
    cwspLoadMinGurCrRtVbr  Integer32,
    cwspLoadMinGurCrNrtVbr Integer32,
    cwspLoadMinGurCrAbr Integer32,
    cwspLoadMinGurCrUbr Integer32
    }

cwspLoadBwTotal OBJECT-TYPE
    SYNTAX      Integer32 (0..2147483647)
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "The total bandwidth of the interface."
    ::= { cwspLoadEntry 1 }
 
cwspLoadMaxBwCbr OBJECT-TYPE
    SYNTAX      Integer32 (0..2147483647)
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
