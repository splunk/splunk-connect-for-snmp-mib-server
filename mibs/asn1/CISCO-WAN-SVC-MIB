            cwspPrefixGrp,
            cwspLoadGrp,
            cwspAddressGrp,
            cwspConnTraceGrp,
            cwspOperationGrp
        }
 
        GROUP   cwspSigConfigGrp
        DESCRIPTION
            "The signaling config group is optional."
 
        GROUP   cwspSscopConfigGrp
        DESCRIPTION
            "The sscop config group is optional."
 
        GROUP   cwspSscopStatsGrp
        DESCRIPTION
            "The sscop stat group is optional."
 
        GROUP   cwspRegAddressGrp
        DESCRIPTION
            "The registered atm address group is optional."
 
        GROUP   cwspRoutingGrp
        DESCRIPTION
            "The PNNI routing group is optional."
        
        GROUP   cwspSpvcGrp
        DESCRIPTION
            "The Spvc group is optional."

        OBJECT cwspSpvcNodePrefix
        MIN-ACCESS  read-only
        DESCRIPTION
            "Write Access is not permitted now until future
            release."

        OBJECT cwspMaxP2pCalls
        MIN-ACCESS  read-only
        DESCRIPTION
            "Write Access is not permitted now until future
            release."
    ::= { ciscoWANSvcMIBCompliances 1 }

