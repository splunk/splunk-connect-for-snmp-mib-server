    --
    -- STN ATM Port Signaling Service Specific Convergence Sublayer (SSCS)
    -- Table
    --
    stnAtmPortSigSSCSTable OBJECT-TYPE
        SYNTAX        SEQUENCE OF StnAtmPortSigSSCSEntry
        MAX-ACCESS    not-accessible
        STATUS        current
        DESCRIPTION   "A list of ATM Port Signaling SSCS entries."
        ::= { stnAtmPortSigRetries 2 }
 
    stnAtmPortSigSSCSEntry OBJECT-TYPE
        SYNTAX          StnAtmPortSigSSCSEntry
        MAX-ACCESS      not-accessible
        STATUS          current
        DESCRIPTION     "Entry contains information about a particular 
                        Atm Port Signaling SSCS entry."
        INDEX           { stnAtmPortSigSSCSIfIndex }
        ::= { stnAtmPortSigSSCSTable 1 }
 
    StnAtmPortSigSSCSEntry ::= 
        SEQUENCE {
                stnAtmPortSigSSCSIfIndex            InterfaceIndex,
                stnAtmPortSigSSCSPollDuration       Integer32,
                stnAtmPortSigSSCSKeepAliveDuration  Integer32,
                stnAtmPortSigSSCSIdleDuration       Integer32,
                stnAtmPortSigSSCSNoRspDuration      Integer32,
                stnAtmPortSigSSCSCcDuration         Integer32,
                stnAtmPortSigSSCSMaxRcvWindowSize   Integer32,
                stnAtmPortSigSSCSMaxCcRetries       Integer32,
                stnAtmPortSigSSCSMaxSdBetwPolls     Integer32,
                stnAtmPortSigSSCSMaxStatListElms    Integer32,
                stnAtmPortSigSSCSPollAfterRetrans   TruthValue,
                stnAtmPortSigSSCSRepeatUstat        TruthValue,
                stnAtmPortSigSSCSUstatRspToPoll     TruthValue
        }

     stnAtmPortSigSSCSIfIndex OBJECT-TYPE
        SYNTAX          InterfaceIndex
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION     "The interface index represents a connection point on
                         the switch fabric from a module or line card. It is
                         calculated based on the slot and port of the module."
        ::= { stnAtmPortSigSSCSEntry 1 }

    stnAtmPortSigSSCSPollDuration OBJECT-TYPE     
        SYNTAX          Integer32
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION     "The duration of the SSCS Poll timer in milliseconds."
        DEFVAL          { 750 }
        ::= { stnAtmPortSigSSCSEntry 2 }

    stnAtmPortSigSSCSKeepAliveDuration OBJECT-TYPE     
        SYNTAX          Integer32
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION     "The duration of the SSCS keep-alive timer in
                         milliseconds."
        DEFVAL          { 2000 }
        ::= { stnAtmPortSigSSCSEntry 3 }

    stnAtmPortSigSSCSIdleDuration OBJECT-TYPE     
        SYNTAX          Integer32
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION     "The duration of the SSCS idle timer in milliseconds."
        DEFVAL          { 15000 }
        ::= { stnAtmPortSigSSCSEntry 4 }

    stnAtmPortSigSSCSNoRspDuration OBJECT-TYPE     
        SYNTAX          Integer32
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION     "The duration of the SSCS no-response timer in
                         milliseconds."
        DEFVAL          { 7000 }
        ::= { stnAtmPortSigSSCSEntry 5 }

    stnAtmPortSigSSCSCcDuration OBJECT-TYPE     
        SYNTAX          Integer32
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION     "The duration of the SSCS CC timer in milliseconds."
        DEFVAL          { 1000 }
        ::= { stnAtmPortSigSSCSEntry 6 }

    stnAtmPortSigSSCSMaxRcvWindowSize OBJECT-TYPE     
        SYNTAX          Integer32
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION     "The maximum size on a receive window."
        DEFVAL          { 20 }
        ::= { stnAtmPortSigSSCSEntry 7 }

    stnAtmPortSigSSCSMaxCcRetries OBJECT-TYPE     
        SYNTAX          Integer32
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION     "The maximum number of expiries and subsequent
                         connection control PDU retransmissions acceptable
                         before closing the connection."
        DEFVAL          { 4 }
        ::= { stnAtmPortSigSSCSEntry 8 }

    stnAtmPortSigSSCSMaxSdBetwPolls OBJECT-TYPE     
        SYNTAX          Integer32
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION     "The maximum number of STAT list elements in a STAT
                         PDU."
        DEFVAL          { 25 }
        ::= { stnAtmPortSigSSCSEntry 9 }

    stnAtmPortSigSSCSMaxStatListElms OBJECT-TYPE     
        SYNTAX          Integer32
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION     "The maximum number of STAT list elements in a STAT
                         PDU."
        DEFVAL          { 67 }
        ::= { stnAtmPortSigSSCSEntry 10 }

    stnAtmPortSigSSCSPollAfterRetrans OBJECT-TYPE     
        SYNTAX          TruthValue
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION     "Whether to send a POLL after retransmitting PDUs."
        DEFVAL          { false }
        ::= { stnAtmPortSigSSCSEntry 11 }

    stnAtmPortSigSSCSRepeatUstat OBJECT-TYPE     
        SYNTAX          TruthValue
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION     "QSAAL only: if true, send two USTATs each time a
                         USTAT is sent."
        DEFVAL          { false }
        ::= { stnAtmPortSigSSCSEntry 12 }

    stnAtmPortSigSSCSUstatRspToPoll OBJECT-TYPE     
        SYNTAX          TruthValue
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION     "QSAAL only: whether USTAT PDUs are issued in response
                         to POLLs."
        DEFVAL          { false }
        ::= { stnAtmPortSigSSCSEntry 13 }


    --
    -- STN ATM Port Traffic Table
    --
    stnAtmPortTrafficTable OBJECT-TYPE
        SYNTAX        SEQUENCE OF StnAtmPortTrafficEntry
        MAX-ACCESS    not-accessible
        STATUS        current
        DESCRIPTION   "A list of ATM port traffic entries."
        ::= { stnAtmPortTraffic 1 }
 
    stnAtmPortTrafficEntry OBJECT-TYPE
        SYNTAX          StnAtmPortTrafficEntry
        MAX-ACCESS      not-accessible
        STATUS          current
        DESCRIPTION     "Entry contains information about a particular 
                        ATM port traffic entry."
        INDEX           { stnAtmPortTrafIndex }
        ::= { stnAtmPortTrafficTable 1 }
 
    StnAtmPortTrafficEntry ::= 
        SEQUENCE {
                stnAtmPortTrafIndex                 InterfaceIndex,
                stnAtmPortTrafTxCells               Counter32,
                stnAtmPortTrafRxCells               Counter32,
                stnAtmPortTrafTxParityErrors        Counter32,
                stnAtmPortTrafRxHdrChkSumErrors     Counter32,
                stnAtmPortTrafPhyParityErrors       Counter32,
                stnAtmPortTrafCrc10Errors           Counter32
        }

     stnAtmPortTrafIndex OBJECT-TYPE 
        SYNTAX          InterfaceIndex
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION     "The interface index represents a connection point on
                         the switch fabric from a module or line card. It is
                         calculated based on the slot and port of the module."
        ::= { stnAtmPortTrafficEntry 1 }

    stnAtmPortTrafTxCells OBJECT-TYPE
        SYNTAX          Counter32
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION     "The number of ATM cells transmitted."
        ::= { stnAtmPortTrafficEntry 2 }

    stnAtmPortTrafRxCells OBJECT-TYPE
        SYNTAX          Counter32
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION     "The number of ATM cells received."
        ::= { stnAtmPortTrafficEntry 3 }

    stnAtmPortTrafTxParityErrors OBJECT-TYPE
        SYNTAX          Counter32
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION     "The number of ATM transmit parity errors."
        ::= { stnAtmPortTrafficEntry 4 }

    stnAtmPortTrafRxHdrChkSumErrors OBJECT-TYPE
        SYNTAX          Counter32
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION     "The number of received ATM header checksum errors."
        ::= { stnAtmPortTrafficEntry 5 }

    stnAtmPortTrafPhyParityErrors OBJECT-TYPE
        SYNTAX          Counter32
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION     "The number of ATM physical parity errors."
        ::= { stnAtmPortTrafficEntry 6 }

    stnAtmPortTrafCrc10Errors OBJECT-TYPE
        SYNTAX          Counter32
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION     "The number of ATM CRC-10 errors."
        ::= { stnAtmPortTrafficEntry 7 }


    --
    -- STN ATM Address Table
    --
    stnAtmAddressTable OBJECT-TYPE
        SYNTAX        SEQUENCE OF StnAtmAddressEntry
        MAX-ACCESS    not-accessible
        STATUS        current
        DESCRIPTION   "A list of ATM address entries."
