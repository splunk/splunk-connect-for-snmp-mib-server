    --
    -- STN ATM Port Attribute Table
    --

    stnAtmPortAttrTable OBJECT-TYPE
        SYNTAX        SEQUENCE OF StnAtmPortAttrEntry
        MAX-ACCESS    not-accessible
        STATUS        current
        DESCRIPTION   "A list of ATM port attribute entries."
        ::= { stnAtmPortAttrs 1 }
 
    stnAtmPortAttrEntry OBJECT-TYPE
        SYNTAX          StnAtmPortAttrEntry
        MAX-ACCESS      not-accessible
        STATUS          current
        DESCRIPTION     "Entry contains information about a particular 
                        ATM port attribute entry."
        INDEX           { stnAtmPortIfIndex }
        ::= { stnAtmPortAttrTable 1 }
 
    StnAtmPortAttrEntry ::= 
        SEQUENCE {
                stnAtmPortIfIndex               InterfaceIndex,
                stnAtmPortIfType                Integer32,
                stnAtmPortIfState               INTEGER,
                stnAtmPortEnabled               TruthValue,
                stnAtmPortMinVccVpi             INTEGER,
                stnAtmPortMaxVccVpi             INTEGER,
                stnAtmPortMinVccVci             INTEGER,
                stnAtmPortMaxVccVci             INTEGER,
                stnAtmPortMinVpcVpi             INTEGER,
                stnAtmPortMaxVpcVpi             INTEGER,
                stnAtmPortMaxActivePaths        INTEGER,
                stnAtmPortMaxActiveChannels     INTEGER,
                stnAtmPortPVCAddress            NSAPAddress,
                stnAtmPortSVCAddress            NSAPAddress,
                stnAtmPortSigVpi                Integer32,
                stnAtmPortSigVci                Integer32,
                stnAtmPortSigEnabled            TruthValue,
                stnAtmPortIlmiVpi               Integer32,
                stnAtmPortIlmiVci               Integer32,
                stnAtmPortIlmiEnabled           TruthValue,
                stnAtmPortOamManagementEnabled  TruthValue,
                stnAtmPortOamXmtFrequency       Integer32,
                stnAtmPortOamRetryUpCount       Integer32,
                stnAtmPortOamRetryDownCount     Integer32,
                stnAtmPortOamRetryFrequency     Integer32,
                stnAtmPortOperStatus            AtmPortOperStatus
        }

    stnAtmPortIfIndex OBJECT-TYPE
        SYNTAX          InterfaceIndex
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION     "The interface index represents a connection point on
                         the switch fabric from a module or line card. It is
                         calculated based on the slot and port of the module."
        ::= { stnAtmPortAttrEntry 1 }

    stnAtmPortIfType OBJECT-TYPE     
        SYNTAX          Integer32
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION     "The interface type of the connection point to the
                         switch fabric. Values are:
                                sonet(1),
                                enet(2),
                                epif(3),
                                rim-enet(4)."
        ::= { stnAtmPortAttrEntry 2 }

    stnAtmPortIfState OBJECT-TYPE     
        SYNTAX          INTEGER (0..8)
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION     "The interface state of the connection point to
                         the switch fabric. Values are:
                                not-present(0),
                                detected(1),
                                configured(2),
                                up-non-redundant(3),
                                up-redundant(4),
                                down-non-redundant(5),
                                down-redundant(6),
                                failed(7),
                                initialized(8)"
        ::= { stnAtmPortAttrEntry 3 }

    stnAtmPortEnabled OBJECT-TYPE
        SYNTAX          TruthValue
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION     "Whether the ATM port is enabled."
        DEFVAL          { true }
        ::= { stnAtmPortAttrEntry 4 }

    stnAtmPortMinVccVpi OBJECT-TYPE     
        SYNTAX          INTEGER (0..255)
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION     "Minimum VPI used for Virtual Channel Circuits."
        DEFVAL          { 0 }
        ::= { stnAtmPortAttrEntry 5 }

    stnAtmPortMaxVccVpi OBJECT-TYPE     
        SYNTAX          INTEGER (0..255)
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION     "Maximum VPI used for Virtual Channel Circuits."
        DEFVAL          { 0 }
        ::= { stnAtmPortAttrEntry 6 }

    stnAtmPortMinVccVci OBJECT-TYPE     
        SYNTAX          INTEGER (32..65554)
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION     "Minimum VCI used for Virtual Channel Circuits.
                        To avoid clashes with the reserved ATM VCI values 
                        (such as 5 for signaling and any other reserved values
                        including Connection Manager VCs), the recommended 
                        value is 32. "
        DEFVAL          { 32 }
        ::= { stnAtmPortAttrEntry 7 }

    stnAtmPortMaxVccVci OBJECT-TYPE     
        SYNTAX          INTEGER (32..65554)
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION     "Maximum VCI used for Virtual Channel Circuits."
        DEFVAL          { 4095 }
        ::= { stnAtmPortAttrEntry 8 }

    stnAtmPortMinVpcVpi OBJECT-TYPE     
        SYNTAX          INTEGER (1..255)
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION     "Minimum VPI used for Virtual Path Circuits. To avoid 
                        clashes with the reserved ATM VCCs values (such as 5 
                        for signaling and any other reserved values including 
                        Connection Manager VCs), the recommended value is 1."
        DEFVAL          { 1 }
        ::= { stnAtmPortAttrEntry 9 }

    stnAtmPortMaxVpcVpi OBJECT-TYPE     
