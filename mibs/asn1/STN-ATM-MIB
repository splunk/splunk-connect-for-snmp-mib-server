    stnAtmAddrIfIndex OBJECT-TYPE
        SYNTAX          InterfaceIndex
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION     "The interface index represents a connection point on
                         the switch fabric from a module or line card. It is
                         calculated based on the slot and port of the module."
        ::= { stnAtmAddressEntry 1 }

    stnAtmAddrAddress OBJECT-TYPE
        SYNTAX          NSAPAddress
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION     "The value of the ATM NSAP address."
        ::= { stnAtmAddressEntry 2 }

    stnAtmAddrAddressLen OBJECT-TYPE
        SYNTAX          Integer32
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION     "The number of relevent bits in the ATM address."
        ::= { stnAtmAddressEntry 3 }


    --
    -- STN ATM Threshold Table
    --
    stnAtmThresholdTable OBJECT-TYPE
        SYNTAX        SEQUENCE OF StnAtmThresholdEntry
        MAX-ACCESS    not-accessible
        STATUS        current
        DESCRIPTION   "A list of ATM threshold entries."
        ::= { stnAtmThresholds 1 }
 
    stnAtmThresholdEntry OBJECT-TYPE
        SYNTAX          StnAtmThresholdEntry
        MAX-ACCESS      not-accessible
        STATUS          current
        DESCRIPTION     "Entry contains information about a particular 
                        ATM threshold entry."
        INDEX           { stnAtmTholdTgrpId, stnAtmTholdRegionId }
        ::= { stnAtmThresholdTable 1 }
 
    StnAtmThresholdEntry ::= 
        SEQUENCE {
                stnAtmTholdTgrpId               INTEGER,
                stnAtmTholdRegionId             INTEGER,
                stnAtmTholdCellLowerLimit       INTEGER,
                stnAtmTholdCellUpperLimit       INTEGER,
                stnAtmTholdCellQLimitMarking    INTEGER, 
                stnAtmTholdQLimitDiscard        INTEGER,
                stnAtmTholdQueueLimit           INTEGER
        }

    stnAtmTholdTgrpId OBJECT-TYPE
        SYNTAX          INTEGER (0..15)
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION     "Specifies the threshold group indentifier."
        ::= { stnAtmThresholdEntry 1 }

    stnAtmTholdRegionId OBJECT-TYPE
        SYNTAX          INTEGER (0..7)
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION     "Region identifier within a threshold group."
        ::= { stnAtmThresholdEntry 2 }

    stnAtmTholdCellLowerLimit OBJECT-TYPE
        SYNTAX          INTEGER (0..65535)
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION     "Lower Cell number limit for region."
        DEFVAL          { 0 }
        ::= { stnAtmThresholdEntry 3 }

    stnAtmTholdCellUpperLimit OBJECT-TYPE
        SYNTAX          INTEGER (0..65535)
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION     "Upper Cell number limit for region."
        DEFVAL          { 1023 }
        ::= { stnAtmThresholdEntry 4 }

    stnAtmTholdCellQLimitMarking OBJECT-TYPE    
        SYNTAX          INTEGER (0..65535)
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION     "Queue limit for any flow above which EFCI marking 
                        occurs."
        DEFVAL          { 1023 }
        ::= { stnAtmThresholdEntry 5 }

    stnAtmTholdQLimitDiscard OBJECT-TYPE
        SYNTAX          INTEGER (0..65535)
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION     "Queue limit for any flow above which Discard 
                        processing occurs."
        DEFVAL          { 1023 }
        ::= { stnAtmThresholdEntry 6 }

    stnAtmTholdQueueLimit OBJECT-TYPE
        SYNTAX          INTEGER (0..65535)
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION     "Queue limit for any flow above which cells are 
                        automatically discarded."
        DEFVAL          { 1023 }
        ::= { stnAtmThresholdEntry 7 }


    --
    -- STN ATM Accounting Table
    --

    stnAtmAcctEnabled OBJECT-TYPE     
        SYNTAX          TruthValue 
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION     "Specifies whether account logging is turned on
                        or off."
        DEFVAL          { false }
        ::= { stnAtmAcct 1 }

    stnAtmAcctMaxSize OBJECT-TYPE     
        SYNTAX          Integer32
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION     "Specifies the maximum accounting log buffer size.
                        in Kbytes. The accounting log file will be closed 
                        when it reaches the maximum size and the name 
                        appended with a numeric differentiation."
        DEFVAL          { 1024 }
        ::= { stnAtmAcct 2 }

    stnAtmAcctMaxFiles OBJECT-TYPE     
        SYNTAX          Integer32
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION     "Specifies the maximum number of history files to 
                        be stored. Once the maximum number of files is 
                        reached, the oldest file is deleted."
        DEFVAL          { 10 }
        ::= { stnAtmAcct 3 }

    stnAtmAcctFilePath OBJECT-TYPE     
        SYNTAX          OCTET STRING (SIZE(0..128))
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION     "Specifies the accounting log output path and file
                        name on remote device."
        ::= { stnAtmAcct 4 }

    stnAtmAcctFtpServer1 OBJECT-TYPE    
        SYNTAX          IpAddress
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION     "The IP address of the first remote FTP server."
        ::= { stnAtmAcct 5 }

    stnAtmAcctFtpUserName1 OBJECT-TYPE
        SYNTAX          DisplayString (SIZE(0..20))
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION     "The first FTP server username."
        ::= { stnAtmAcct 6 }

    stnAtmAcctFtpPassword1 OBJECT-TYPE
        SYNTAX          OCTET STRING (SIZE(0..255))
        MAX-ACCESS      not-accessible
        STATUS          current
        DESCRIPTION     "The first FTP server password."
        ::= { stnAtmAcct 7 }

    stnAtmAcctFtpServer2 OBJECT-TYPE    
        SYNTAX          IpAddress
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION     "The IP address of the second remote FTP server." 
        ::= { stnAtmAcct 8 }

    stnAtmAcctFtpUserName2 OBJECT-TYPE
        SYNTAX          DisplayString (SIZE(0..20))
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION     "The second FTP server username."
        ::= { stnAtmAcct 9 }

    stnAtmAcctFtpPassword2 OBJECT-TYPE
        SYNTAX          OCTET STRING (SIZE(0..255))
        MAX-ACCESS      not-accessible
        STATUS          current
        DESCRIPTION     "The second FTP server password."
        ::= { stnAtmAcct 10 }

    stnAtmAcctFtpServer3 OBJECT-TYPE    
        SYNTAX          IpAddress
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION     "The IP address of the third remote FTP server."
        ::= { stnAtmAcct 11 }

    stnAtmAcctFtpUserName3 OBJECT-TYPE
        SYNTAX          DisplayString (SIZE(0..20))
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION     "The third FTP server username."
        ::= { stnAtmAcct 12 }

    stnAtmAcctFtpPassword3 OBJECT-TYPE
        SYNTAX          OCTET STRING (SIZE(0..255))
        MAX-ACCESS      not-accessible
        STATUS          current
        DESCRIPTION     "The third FTP server password."
        ::= { stnAtmAcct 13 }
   
    stnAtmAcctCurrentFtpServer OBJECT-TYPE    
        SYNTAX          IpAddress
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION     "The IP address of the current FTP server."
        ::= { stnAtmAcct 14 }

    stnAtmAcctPreviousFtpServer OBJECT-TYPE    
        SYNTAX          IpAddress
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION     "The IP address of the previous FTP server."
        ::= { stnAtmAcct 15 }


    --
    -- STN Traps & Notifications 
    --
    stnCdrServerFailure NOTIFICATION-TYPE
        OBJECTS         { stnAtmAcctCurrentFtpServer, 
                          stnAtmAcctPreviousFtpServer }
        STATUS          current
        DESCRIPTION     "A stnCdrServerFailure trap signifies that the agent 
                        entity has detected that the accounting server has
                        failed over to an auxilary server.
                        The generation of this trap can be controlled by the
                        stnNotificationEnableCdrServerFailure object in this MIB."
        ::= { stnNotification 15 }

    stnCdrLogFailure NOTIFICATION-TYPE
        OBJECTS         { stnAtmAcctFtpServer1, stnAtmAcctFtpServer2,
                          stnAtmAcctFtpServer3 }
        STATUS          current
        DESCRIPTION     "A stnCdrLogFailure trap signifies that the agent 
                        entity has detected that all accounting servers
                        are inaccessible. Accounting data could be lost.
                        The generation of this trap can be controlled by the
                        stnNotificationEnableCdrLogFailure object in this MIB."
        ::= { stnNotification 16 }


    stnConfigAuditPvcFailure NOTIFICATION-TYPE
        OBJECTS         { stnVcLinkIfIndex, stnVcLinkVpi,
                          stnVcLinkVci,     stnRouterIndex }
        STATUS          current
        DESCRIPTION     "A stnConfigAuditPvcFailure trap signifies that the 
                        agent entity has detected an inconsistency between
                        an active and configured PVC after a failover.
                        The generation of this trap can be controlled by the
                        CfgAuditPvcTraps configuration object."
        ::= { stnNotification 31 }

    stnConfigAuditSpvcFailure NOTIFICATION-TYPE
        OBJECTS         { stnVcLinkIfIndex, stnVcLinkVpi,
                          stnVcLinkVci,     stnRouterIndex }
        STATUS          current
        DESCRIPTION     "A stnConfigAuditSpvcFailure trap signifies that the 
                        agent entity has detected an inconsistency between
                        an active and configured SPVC after a failover.
                        The generation of this trap can be controlled by the
                        CfgAuditSpvcTraps configuration object."
        ::= { stnNotification 32 }

END

