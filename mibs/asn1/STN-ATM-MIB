        ::= { stnAtmAddressTable 1 }
 
    StnAtmAddressEntry ::= 
        SEQUENCE {
                stnAtmAddrIfIndex       InterfaceIndex,
                stnAtmAddrAddress       NSAPAddress,
                stnAtmAddrAddressLen    Integer32
