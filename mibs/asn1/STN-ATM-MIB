    --
    -- STN ATM Port ILMI Table
    --
    stnAtmPortIlmiTable OBJECT-TYPE
        SYNTAX        SEQUENCE OF StnAtmPortIlmiEntry
        MAX-ACCESS    not-accessible
        STATUS        current
        DESCRIPTION   "A list of ATM ILMI Port entries."
        ::= { stnAtmPortIlmi 1 }
 
    stnAtmPortIlmiEntry OBJECT-TYPE
        SYNTAX          StnAtmPortIlmiEntry
        MAX-ACCESS      not-accessible
        STATUS          current
        DESCRIPTION     "Entry contains information about a particular 
                        ATM Port ILMI entry."
        INDEX           { stnAtmPortIlmiIfIndex }
        ::= { stnAtmPortIlmiTable 1 }
 
    StnAtmPortIlmiEntry ::= 
        SEQUENCE {
                stnAtmPortIlmiIfIndex               InterfaceIndex,
                stnAtmPortIlmiVersion               INTEGER,
                stnAtmPortIlmiNetworkOrientation    INTEGER,
                stnAtmPortIlmiAutoNegotiate         TruthValue,
                stnAtmPortIlmiPollInterval          Integer32,
                stnAtmPortIlmiCheckConnectionInt    Integer32,
                stnAtmPortIlmiInactivityFactor      Integer32,
                stnAtmPortIlmiLocalConn             INTEGER,
                stnAtmPortIlmiAdminStatus           BITS,
                stnAtmPortIlmiEnterprise            OBJECT IDENTIFIER,
                stnAtmPortIlmiLocalOid              OBJECT IDENTIFIER,
                stnAtmPortIlmiNetPrefix             OCTET STRING,
                stnAtmPortIlmiPrefixLen             Integer32
        }

    stnAtmPortIlmiIfIndex OBJECT-TYPE
        SYNTAX          InterfaceIndex
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION     "The interface index represents a connection point on
                         the switch fabric from a module or line card. It is
                         calculated based on the slot and port of the module."
        ::= { stnAtmPortIlmiEntry 1 }

    stnAtmPortIlmiVersion OBJECT-TYPE     
        SYNTAX          INTEGER {
                                uni-3-dot-0(1),
                                uni-3-dot-1(2),
                                uni-4-dot-0(3),
                                iisp-3-dot-1(4)
                        }
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION     "Specifies the signaling stack protocol type to be 
                        used on this port. Valid options are: 
                                uni-3-dot-0
                                uni-3-dot-1
                                uni-4-dot-0 
                                iisp-3-dot-1"
        DEFVAL          { uni-3-dot-1 }
        ::= { stnAtmPortIlmiEntry 2 }

    stnAtmPortIlmiNetworkOrientation OBJECT-TYPE     
        SYNTAX          INTEGER {
                                user(1),
                                network(2)
                        }
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION     "Specifies the ATM device type. Valid options are: 
                                user,
                                network"
        DEFVAL          { network }       
        ::= { stnAtmPortIlmiEntry 3 }

    stnAtmPortIlmiAutoNegotiate OBJECT-TYPE     
        SYNTAX          TruthValue
        MAX-ACCESS      read-only 
        STATUS          current
        DESCRIPTION     "Indicates ILMI auto negotiation mode"
        DEFVAL          { false }          
        ::= { stnAtmPortIlmiEntry 4 }

    stnAtmPortIlmiPollInterval OBJECT-TYPE     
        SYNTAX          Integer32
        MAX-ACCESS      read-only 
        STATUS          current
        DESCRIPTION     "ILMI connection poll interval (S) in seconds. When 
                        DC-ILMI is transmitting SNMP Get messages during the 
                        attempt to establish connectivity with the remote ILMI
                        component, this parameter determines the time in 
                        seconds between transmissions. The default value is
                        from the ATM Forum ILMI Specification Version 4.0:
                        ILMI Connectivity Procedures (section 8.3.1)."
        DEFVAL          { 1 }      
        ::= { stnAtmPortIlmiEntry 5 }

    stnAtmPortIlmiCheckConnectionInt OBJECT-TYPE     
        SYNTAX          Integer32
        MAX-ACCESS      read-only 
        STATUS          current
        DESCRIPTION     "ILMI check connection poll interval (T) in seconds. 
                        If connectivity checking is enabled (as spec by the 
                        value connect in the Admin_Status parameter below),
                        this parameter specifies the time in seconds between 
                        successive transmissions of MIB Get msgs for the 
                        purpose of detecting loss of ILMI connectivity. If
                        connectivity checking is not enabled, this parameter
                        must be set to zero. The default value is from the ATM
                        Forum ILMI Specification Version 4.0: ILMI 
                        Connectivity Procedures (section 8.3.1)."
        DEFVAL          { 5 }      
        ::= { stnAtmPortIlmiEntry 6 }

    stnAtmPortIlmiInactivityFactor OBJECT-TYPE     
        SYNTAX          Integer32
        MAX-ACCESS      read-only 
        STATUS          current
        DESCRIPTION     "ILMI inactivity factor (K). The number of 
                        consecutive polls for which no response is received 
                        from the remote ILMI component before ILMI 
                        connectivity is assumed to be lost. The dflt value is 
                        from the ATM Forum ILMI Specification Version 4.0: 
                        ILMI Connectivity Procedures (section 8.3.1)."
        DEFVAL          { 4 }      
        ::= { stnAtmPortIlmiEntry 7 }

    stnAtmPortIlmiLocalConn OBJECT-TYPE     
        SYNTAX          INTEGER {
                                other(1),
                                public(2),  
                                private(3) 
                        }
        MAX-ACCESS      read-only 
        STATUS          current
        DESCRIPTION     "Specifies whether the local end of the ILMI 
                        connection is advertised as a 'public' or 'private' 
                        ILMI device. Values are:
                                other,
                                public,  
                                private"
        DEFVAL          { private }    
        ::= { stnAtmPortIlmiEntry 8 }

    stnAtmPortIlmiAdminStatus OBJECT-TYPE     
        SYNTAX          BITS {
                                std-ilmi(0),
                                addr-reg(1),  
                                connect(2) 
                        }
        MAX-ACCESS      read-only 
        STATUS          current
        DESCRIPTION     "Specifies which functions of DC-ILMI are enabled. The
                        value std-ilmi must be specified; this value may be 
                        combined (using a bitwise OR) with one or more of the 
                        following values:  
                        std-ilmi - Standard ILMI functions are enabled.
                        addr-reg - Address registration is enabled.       
                        connect - Connectivity chcking is enabled (DC-ILMI 
                            periodically sends a MIB Get request for the 
                            remote system's System ID, to ensure that it can 
                            still send and rcvd data on the ILMI connection).

                        Default: (std-ilmi | addr-reg | connect)"
        ::= { stnAtmPortIlmiEntry 9 }

    stnAtmPortIlmiEnterprise OBJECT-TYPE    
        SYNTAX          OBJECT IDENTIFIER
        MAX-ACCESS      read-only 
        STATUS          current
        DESCRIPTION     "Enterprise Object ID that DC-ILMI will use in 
                        building SNMP traps (or other generic MIB traps). This
                        parameter can be used to identify the originator of
                        the trap.
                        
                        The Object ID is specified as an array of unsigned
                        long vals, as follows: 
                                                                     
                        The first entry in the array is the length of the
                        array (the num of unsigned long values it contains,
                        including the len value).  
                                                                     
                        The second entry in the array is an identifier
                        specifying the Family type of the Object ID. This reps
                        a prefix Object ID; the MIB Sub-Agent is responsible
                        for matching this id with the dotted-decimal notation
                        for this prefix.                    
                                                                     
                        The remainder of the array gives the index part of the
                        Object ID (the dotted-decimal notation from the end of
                        the Family), with 1 unsigned long for each decimal
                        number in this notation."
        ::= { stnAtmPortIlmiEntry 10 }

    stnAtmPortIlmiLocalOid OBJECT-TYPE     
        SYNTAX          OBJECT IDENTIFIER
        MAX-ACCESS      read-only 
        STATUS          current
        DESCRIPTION     "Object ID identifying the local machine - the first
                         4 octets indicate the length of the object id.  For
                         example, the null object id (0.0), which should be
                         used if no object id is specified,  is encoded as
                         3, 0, 0."
        ::= { stnAtmPortIlmiEntry 11 }

    stnAtmPortIlmiNetPrefix OBJECT-TYPE     
        SYNTAX          OCTET STRING (SIZE(0..13))
        MAX-ACCESS      read-only 
        STATUS          current
        DESCRIPTION     "Default network prefix. If the prefix length is 
                        non-zero, ILMI creates an entry in the ILMI NetPrefix 
                        Table for this prefix; otherwise the Net_Prefix 
                        parameter is not used."
        ::= { stnAtmPortIlmiEntry 12 }

    stnAtmPortIlmiPrefixLen OBJECT-TYPE     
        SYNTAX          Integer32
        MAX-ACCESS      read-only 
        STATUS          current
        DESCRIPTION     "The length of the default network prefix"
        ::= { stnAtmPortIlmiEntry 13 }


    --
    -- STN ATM Port Signaling Table
    --
    stnAtmPortSigTable OBJECT-TYPE
        SYNTAX        SEQUENCE OF StnAtmPortSigEntry
        MAX-ACCESS    not-accessible
        STATUS        current
        DESCRIPTION   "A list of ATM port signal entries."
        ::= { stnAtmPortSigs 1 }
 
    stnAtmPortSigEntry OBJECT-TYPE
        SYNTAX          StnAtmPortSigEntry
        MAX-ACCESS      not-accessible
        STATUS          current
        DESCRIPTION     "Entry contains information about a particular 
                        ATM port signal entry."
        INDEX           { stnAtmPortSigIfIndex }
        ::= { stnAtmPortSigTable 1 }
 
    StnAtmPortSigEntry ::= 
        SEQUENCE {
                stnAtmPortSigIfIndex                    InterfaceIndex,
                stnAtmPortSigVersion                    INTEGER,
                stnAtmPortSigNetworkOrientation         INTEGER,
                stnAtmPortSigAssignVpiVci               TruthValue,
                stnAtmPortSigVpSigType                  INTEGER,
                stnAtmPortSigParseMode                  INTEGER,
                stnAtmPortSigPrefCarrierPresent         TruthValue,
                stnAtmPortSigPrefCarrier                TruthValue
        }

    stnAtmPortSigIfIndex OBJECT-TYPE
        SYNTAX          InterfaceIndex
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION     "The interface index represents a connection point on
                         the switch fabric from a module or line card. It is
                         calculated based on the slot and port of the module."
        ::= { stnAtmPortSigEntry 1 }

    stnAtmPortSigVersion OBJECT-TYPE
        SYNTAX          INTEGER {
                            uni-3-dot-0(1),
                            uni-3-dot-1(2),
                            iisp-3-dot-1(3),
                            pnni-1-dot-0(4)
                        }
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION     "Specifies the signaling stack protocol type to be 
                        used on this port. Valid options are:
                            UNI 3.0, 
                            UNI 3.1,
                            IISP 3.1,
                            PNNI 1.0"
        ::= { stnAtmPortSigEntry 2 }

    stnAtmPortSigNetworkOrientation OBJECT-TYPE
        SYNTAX          INTEGER {
                            user(1),
                            network(2)
                        }
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION     "Indicates whether the ATM device is of the user or
                        network type. Valid values are:
                            user,
                            network"
        DEFVAL          { network }
        ::= { stnAtmPortSigEntry 3 }

    stnAtmPortSigAssignVpiVci OBJECT-TYPE
        SYNTAX          TruthValue
        MAX-ACCESS      not-accessible
        STATUS          current
        DESCRIPTION     "Specifies whether VPI/VCI assignment is the 
                        responsibility of the local signaling stack or of its
                        partner stack. This parameter is used only for UNI 
                        4.0. Values are: 
                            true  - The local signaling stack is responsible
                                    for assigning VPIs/VCIs. 
                            false - The parent-signaling stack is responsible
                                    for assigning VPIs/VCIs."
        DEFVAL          { true }
        ::= { stnAtmPortSigEntry 4 }

    stnAtmPortSigVpSigType OBJECT-TYPE
        SYNTAX          INTEGER {
                            associated-signaling(1),
                            explicit-vpci(2)
                        }
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION     "Specifies whether this signaling stack performs VP 
                        associated  signaling: Valid options are: 
                            associated-signaling 
                            explicit-vpci"
        DEFVAL          { explicit-vpci }
        ::= { stnAtmPortSigEntry 5 }

    stnAtmPortSigParseMode OBJECT-TYPE
        SYNTAX          INTEGER {
                            normal(1),
                            strict(2)
                        }
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION     "Specifies whether the message parser component of
                        Sig should perform validity checking on the contents 
                        of received IEs. This element resides in the CCS MIB
                        to allow software config of this behavior for 
                        compliance testing. Values are:   

                        normal - The parser checks the contents 
                                of the IEs the Sig uses and ensures that they
                                are valid. For other IEs, it checks only the
                                IE type and length and does not parse the 
                                contents.      
                        strict - The parser checks the contents of
                                addit IEs as well as those used by Signaling
                                and also checks for incorrect repetition of 
                                these add it IEs. Errors are handled in the
                                same way as for the IEs used by sig (by 
                                sending a status message in UNI 3.1 or by 
                                following the IE instruction indicator in 
                                UNI 4.0)."
        DEFVAL          { normal }
        ::= { stnAtmPortSigEntry 6 }

    stnAtmPortSigPrefCarrierPresent OBJECT-TYPE
        SYNTAX          TruthValue
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION     "Specifies whether a default prefrd carrier
                        (transit network) is defined. Valid options are: 
                    
                            true -  the Default_Network_Id below defines 
                                    which carr is the preferred carrier. 
                            false - no default preferred carrier is defined."
        DEFVAL          { false }
        ::= { stnAtmPortSigEntry 7 }

    stnAtmPortSigPrefCarrier OBJECT-TYPE
        SYNTAX          TruthValue
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION     "Specifies the default preferred carrier. If 
                        stnAtmPortIlmiPrefCarrierPresent is set to false, 
                        this element is not used."
        ::= { stnAtmPortSigEntry 8 }


    --
    -- STN ATM Port Signaling Timer Duration Table
    --
    --
    -- Signaling Timer Durations values milliseconds. For more info 
    -- on the usage of these timers, refer to the appropriate UNI/NNI 
    -- specification. 
    -- 
    stnAtmPortSigDurationTable OBJECT-TYPE
        SYNTAX        SEQUENCE OF StnAtmPortSigDurationEntry
        MAX-ACCESS    not-accessible
        STATUS        current
        DESCRIPTION   "A list of ATM Port Signaling Duration entries."
        ::= { stnAtmPortSigDurations 1 }
 
