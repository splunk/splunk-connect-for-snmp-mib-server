        SEQUENCE {
                stnAtmPortSigDurIfIndex         InterfaceIndex,
                stnAtmPortSigT301Duration       Integer32,
                stnAtmPortSigT303Duration       Integer32,
                stnAtmPortSigT306Duration       Integer32,
                stnAtmPortSigT308Duration       Integer32,
                stnAtmPortSigT309Duration       Integer32,
                stnAtmPortSigT310Duration       Integer32,
                stnAtmPortSigT313Duration       Integer32,
                stnAtmPortSigT316Duration       Integer32,
                stnAtmPortSigT317Duration       Integer32,
                stnAtmPortSigT322Duration       Integer32,
                stnAtmPortSigT331Duration       Integer32,
                stnAtmPortSigT333Duration       Integer32,
                stnAtmPortSigT397Duration       Integer32,
                stnAtmPortSigT398Duration       Integer32,
                stnAtmPortSigT399Duration       Integer32,
                stnAtmPortSigSaalRetryDuration  Integer32
        }

