-- MIB Object definitions for Spring Tide Network ATM 


STN-ATM-MIB DEFINITIONS ::= BEGIN

    IMPORTS
        MODULE-IDENTITY,
        OBJECT-TYPE,
        NOTIFICATION-TYPE, 
        Integer32,
        Counter32,
        IpAddress
                FROM SNMPv2-SMI

        TEXTUAL-CONVENTION,
        DisplayString,
        TruthValue
                FROM SNMPv2-TC

        MODULE-COMPLIANCE 
                FROM SNMPv2-CONF

        
        InterfaceIndex
                FROM IF-MIB

        NSAPAddress
                FROM SPRING-TIDE-NETWORKS-TC

        stnSystems,
        stnNotification
                FROM SPRING-TIDE-NETWORKS-SMI

        stnRouterIndex
                FROM STN-ROUTER-MIB;


    stnAtm          MODULE-IDENTITY
        LAST-UPDATED    "0002160000Z"
        ORGANIZATION    "Spring Tide Networks, Inc."
        CONTACT-INFO
                "
                Spring Tide Networks, Inc.
                Customer Service
                Postal: 3 Clock Tower Place
                        Maynard, MA 01754
                Tel:    1 888-786-4357
                Email:  stncs@springtidenet.com
                "
        DESCRIPTION
                "Initial version of this MIB module."
        ::= { stnSystems 6 }


    --
    -- Top Level STN ATM MIB Objects
    --
    stnAtmObjects           OBJECT IDENTIFIER ::= { stnAtm 1 }
    stnAtmMibConformance    OBJECT IDENTIFIER ::= { stnAtm 2 }

    stnAtmLinks             OBJECT IDENTIFIER ::= { stnAtmObjects 1 }
    stnAtmCrossConns        OBJECT IDENTIFIER ::= { stnAtmObjects 2 }
    stnAtmTrafficDescrs     OBJECT IDENTIFIER ::= { stnAtmObjects 3 }
    stnAtmPorts             OBJECT IDENTIFIER ::= { stnAtmObjects 4 }
    stnAtmTraces            OBJECT IDENTIFIER ::= { stnAtmObjects 5 }
    stnAtmQos               OBJECT IDENTIFIER ::= { stnAtmObjects 6 }
    stnAtmAddresses         OBJECT IDENTIFIER ::= { stnAtmObjects 7 }
    stnAtmThresholds        OBJECT IDENTIFIER ::= { stnAtmObjects 8 }
    stnAtmAcct              OBJECT IDENTIFIER ::= { stnAtmObjects 9 }

    -- Mid-Level STN ATM MIB Objects
    stnVcLinks              OBJECT IDENTIFIER ::= { stnAtmLinks 1 }

    stnVcCrossConns         OBJECT IDENTIFIER ::= { stnAtmCrossConns 1 }
    stnVpCrossConns         OBJECT IDENTIFIER ::= { stnAtmCrossConns 2 }

    stnAtmPortAttrs         OBJECT IDENTIFIER ::= { stnAtmPorts 1 }
    stnAtmPortIlmi          OBJECT IDENTIFIER ::= { stnAtmPorts 2 }
    stnAtmPortSignaling     OBJECT IDENTIFIER ::= { stnAtmPorts 3 }
    stnAtmPortTraffic       OBJECT IDENTIFIER ::= { stnAtmPorts 4 }

    stnTraceIps             OBJECT IDENTIFIER ::= { stnAtmTraces 1 }
    stnTracePd              OBJECT IDENTIFIER ::= { stnAtmTraces 2 }

    stnQosCLR               OBJECT IDENTIFIER ::= { stnAtmQos 1 }
    stnQosCDV               OBJECT IDENTIFIER ::= { stnAtmQos 2 }
    stnQosCTD               OBJECT IDENTIFIER ::= { stnAtmQos 3 }


    -- Low-Level STN ATM MIB Objects
    stnAtmPortSigs          OBJECT IDENTIFIER ::= { stnAtmPortSignaling 1 }
    stnAtmPortSigDurations  OBJECT IDENTIFIER ::= { stnAtmPortSignaling 2 }
    stnAtmPortSigRetries    OBJECT IDENTIFIER ::= { stnAtmPortSignaling 3 }


    -- Additional STN ATM MIB Textual Conventions
    VcLinkOperStatus ::= TEXTUAL-CONVENTION
        STATUS      current
        DESCRIPTION "Operational status of a VC Link."
        SYNTAX      INTEGER {
                                active(1),            --
                                not-in-service(2),    -- 
                                not-ready(3),         -- 
                                create-and-go(4),     -- 
                                create-and-wait(5),   -- 
                                destroy(6)            -- 
                            }

    VcCrossConnOperStatus ::= TEXTUAL-CONVENTION
        STATUS      current
        DESCRIPTION "Operational status of a VC cross connection."
        SYNTAX      INTEGER {   active(1),            --
                                not-in-service(2),    -- 
                                not-ready(3),         -- 
                                create-and-go(4),     -- 
                                create-and-wait(5),   -- 
                                destroy(6)            -- 
                            }

    TrafficDescrOperStatus ::= TEXTUAL-CONVENTION
        STATUS      current
        DESCRIPTION "Operational status of a traffic descriptor."
        SYNTAX      INTEGER {   active(1),            --
                                not-in-service(2),    -- 
                                not-ready(3),         -- 
                                create-and-go(4),     -- 
                                create-and-wait(5),   -- 
                                destroy(6)            -- 
                            }

    AtmPortOperStatus ::= TEXTUAL-CONVENTION
        STATUS      current
        DESCRIPTION "Operational status of an ATM port."
        SYNTAX      INTEGER {   active(1),          --
                                inactive(2)         -- 
                            }


    --
    -- STN ATM VC Link Table
    --

    stnVcLinkTable OBJECT-TYPE
        SYNTAX        SEQUENCE OF StnVcLinkEntry
        MAX-ACCESS    not-accessible
        STATUS        current
        DESCRIPTION   "A list of VC Link entries."
        ::= { stnVcLinks 1 }
 
    stnVcLinkEntry OBJECT-TYPE
        SYNTAX          StnVcLinkEntry
        MAX-ACCESS      not-accessible
        STATUS          current
        DESCRIPTION     "Entry contains information about a particular 
                        VC link."  
        INDEX           { stnVcLinkIfIndex, stnVcLinkVpi, stnVcLinkVci }
        ::= { stnVcLinkTable 1 }
 
    StnVcLinkEntry ::= 
        SEQUENCE {
                stnVcLinkIfIndex                InterfaceIndex,
                stnVcLinkVpi                    Integer32,
                stnVcLinkVci                    Integer32,
                stnVcLinkRcvTrafDescr           Integer32,
                stnVcLinkXmtTrafDescr           Integer32,
                stnVcLinkAalObjectsValid        Integer32,
                stnVcLinkAalType                INTEGER,
                stnVcLinkAal5XmtSDUSize         Integer32,
                stnVcLinkAal5RcvSDUSize         Integer32,
                stnVcLinkAal5EncapsType         Integer32,
                stnVcLinkCastType               INTEGER,
                stnVcLinkConnKind               INTEGER,
                stnVcLinkAccEnabled             TruthValue,
                stnVcLinkOAMMgmtEnabled         TruthValue,
                stnVcLinkOAMXmtFrequency        Integer32,
                stnVcLinkOAMRetryUpCount        Integer32,
                stnVcLinkOAMRetryDownCount      Integer32,
                stnVcLinkOAMRetryFrequency      Integer32,
                stnVcLinkPppId                  Integer32,     
                stnVcLinkOperStatus             VcLinkOperStatus
        }

    stnVcLinkIfIndex OBJECT-TYPE
        SYNTAX          InterfaceIndex
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION     "The interface index represents a connection point on
                         the switch fabric from a module or line card. It is
                         calculated based on the slot and port of the module."
        ::= { stnVcLinkEntry 1 }

    stnVcLinkVpi OBJECT-TYPE     
        SYNTAX          Integer32 
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION     "The VPI value of the VPL. Note that the VPI=0 is 
                        not used for a VPL not associated with a VCL.  
                        The maximum VPI value cannot exceed the value 
                        allowable by the atmInterfaceMaxVpiBits."
        ::= { stnVcLinkEntry 2 }

    stnVcLinkVci OBJECT-TYPE     
        SYNTAX          Integer32 
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION     "The VCI value of the VCL. The maximum VCI value 
                        cannot exceed the value allowable by the 
                        atmInterfaceMaxVciBits."
        ::= { stnVcLinkEntry 3 }

    stnVcLinkRcvTrafDescr OBJECT-TYPE     
        SYNTAX          Integer32 
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION     "Receive traffic descriptor identifier for this VPL."
        ::= { stnVcLinkEntry 4 }

    stnVcLinkXmtTrafDescr OBJECT-TYPE     
        SYNTAX          Integer32 
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION     "Transmit traffic descriptor identifier for this VPL."
        ::= { stnVcLinkEntry 5 }

    stnVcLinkAalObjectsValid OBJECT-TYPE
        SYNTAX          Integer32
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION     "Indicates whether the following AAL objects are valid
                         (whether the local VCL end-point is also the VCC
                         end-point). Transmit traffic descriptor identifier for
                        this VPL."
        ::= { stnVcLinkEntry 6 }

    stnVcLinkAalType OBJECT-TYPE     
        SYNTAX          INTEGER {
                                aal1(1),
                                aal3-4(2),
                                aal5(3),
                                other(4)
                        }
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION     "An instance of this object only exists when the 
                        local VCL end-point is also the VCC end-point, 
                        and AAL is in use. The type of AAL used on this VCC. 
                        The AAL type includes AAL1, AAL3/4, and AAL5. 
                        The other(4) may be used as a user-defined AAL type. 
                        The unknown type indicates that the AAL type cannot 
                        be determined."
        ::= { stnVcLinkEntry 7 }

    stnVcLinkAal5XmtSDUSize OBJECT-TYPE     
        SYNTAX          Integer32 
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION     "An instance of this object only exists when the 
                        local VCL end-point is also the VCC end-point, and 
                        AAL5 is in use. The maximum AAL5 CPCS SDUsize in 
                        octets that is supported on the transmit direction 
                        of this VCC."
        DEFVAL          { 4096 }
        ::= { stnVcLinkEntry 8 }

    stnVcLinkAal5RcvSDUSize OBJECT-TYPE     
        SYNTAX          Integer32 
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION     "An instance of this object only exists when the 
                        local VCL end-point is also the VCC end-point, and 
                        AAL5 is in use.  The maximum AAL5 CPCS SDU size in 
                        octets that is supported on the receive direction 
                        of this VCC."
        DEFVAL          { 4096 }
        ::= { stnVcLinkEntry 9 }

    stnVcLinkAal5EncapsType OBJECT-TYPE     
        SYNTAX          Integer32 
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION     "An instance of this object only exists when the 
                        local VCL end-point is also the VCC end-point, and 
                        AAL5 is in use.  The type of data encapsulation used 
                        over the AAL5 SSCS layer.  The definitions reference 
                        RFC 1483 Multiprotocol Encapsulation over ATM AAL5 
                        and to the ATM Forum LAN Emulation specification."
        ::= { stnVcLinkEntry 10 }

    stnVcLinkCastType OBJECT-TYPE     
        SYNTAX          INTEGER {
                                p2mpRoot(1),
                                p2mpLeaf(2)
                        }
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION     "The type of topology of a connection 
                        (point- to-point, point-to-multipoint).
                        In the case of point-to-multipoint, the orientation 
                        of this VPL in the connection.  On a host, p2mpRoot 
                        indicates that the host is the root of the p2mp 
                        connection, p2mpLeaf indicates that the host is a 
                        leaf of the p2mp connection.  On a switch, p2mpRoot 
                        indicates that cells received by the switching fabric 
                        from the interface are from the root of the p2mp 
                        connection, p2mpLeaf indicates that cells transmitted 
                        to the interface from the switching fabric are to the 
                        leaf of the p2mp connection."
        ::= { stnVcLinkEntry 11 }

    stnVcLinkConnKind OBJECT-TYPE    
        SYNTAX          INTEGER {
                                pvc(1),
                                svc-incomming(2),
                                svc-outgoing(3),
                                spvc-initiator(4),
                                spvc-target(5)
                        }
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION     "The use of call control.  An spvcInitiator is always
                        cross-connected to an svcOutgoing, and an spvcTarget
                        is always cross-connected to an svcIncoming."
        ::= { stnVcLinkEntry 12 }

    stnVcLinkAccEnabled OBJECT-TYPE     
        SYNTAX          TruthValue
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION     "ATM accounting is enabled for this VC Link."
        DEFVAL          { false }
        ::= { stnVcLinkEntry 13 }

    stnVcLinkOAMMgmtEnabled OBJECT-TYPE     
        SYNTAX          TruthValue
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION     "OAM management is enabled."
        DEFVAL          { false }
        ::= { stnVcLinkEntry 14 }

    stnVcLinkOAMXmtFrequency OBJECT-TYPE     
        SYNTAX          Integer32
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION     "Number of seconds between transmitting OAM 
                        loopback cells."
        DEFVAL          { 5 }
        ::= { stnVcLinkEntry 15 }

    stnVcLinkOAMRetryUpCount OBJECT-TYPE     
        SYNTAX          Integer32
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION     "Number of consecutive end-to-end F5 OAM loopback 
                        cell responses that must be received in order to 
                        change a PVC state to up. Does not apply to SVCs."
        DEFVAL          { 3 }
        ::= { stnVcLinkEntry 16 }

    stnVcLinkOAMRetryDownCount OBJECT-TYPE     
        SYNTAX          Integer32
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION     "Number of consecutive end-to-end F5 OAM loopback 
                        cell responses that are not received in order to 
                        change a PVC state to down or tear down an SVC."
        DEFVAL          { 5 }
        ::= { stnVcLinkEntry 17 }

    stnVcLinkOAMRetryFrequency OBJECT-TYPE     
        SYNTAX          Integer32
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION     "The frequency (in seconds) that end-to-end F5 
                        loopback cells should be transmitted when a change 
                        in UP/DOWN state is being verified. For example, if 
                        a PVC is up and a loopback cell response is not 
                        received after the frequency (in seconds, then 
                        loopback cells are sent at the retry-frequency to 
                        verify whether or not the PVC is down."
        DEFVAL          { 5 }
        ::= { stnVcLinkEntry 18 }

    stnVcLinkPppId OBJECT-TYPE     
        SYNTAX          Integer32
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION     "PPP ID"
        ::= { stnVcLinkEntry 19 }

    stnVcLinkOperStatus OBJECT-TYPE     
        SYNTAX          VcLinkOperStatus
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION     "Operational status of the VC Link."
        ::= { stnVcLinkEntry 20 }


    --
    -- STN ATM VC Cross Connection Table
    --

    stnVcCrossConnTable OBJECT-TYPE
        SYNTAX        SEQUENCE OF StnVcCrossConnEntry
        MAX-ACCESS    not-accessible
        STATUS        current
        DESCRIPTION   "A list of VC cross connection entries."
        ::= { stnVcCrossConns 1 }
 
    stnVcCrossConnEntry OBJECT-TYPE
        SYNTAX          StnVcCrossConnEntry
        MAX-ACCESS      not-accessible
        STATUS          current
        DESCRIPTION     "Entry contains information about a particular 
                        VC cross connection."
        INDEX           { stnVcCrossConnLowIfIndex,
                          stnVcCrossConnLowVpi,
                          stnVcCrossConnLowVci,
                          stnVcCrossConnHighIfIndex,
                          stnVcCrossConnHighVpi,
                          stnVcCrossConnHighVci }
        ::= { stnVcCrossConnTable 1 }
 
    StnVcCrossConnEntry ::= 
        SEQUENCE {
                stnVcCrossConnLowIfIndex        InterfaceIndex,
                stnVcCrossConnLowVpi            Integer32,
                stnVcCrossConnLowVci            Integer32,
                stnVcCrossConnHighIfIndex       InterfaceIndex,
                stnVcCrossConnHighVpi           Integer32,
                stnVcCrossConnHighVci           Integer32,
                stnVcCrossConnLowAutoAssign     TruthValue,
                stnVcCrossConnHighAutoAssign    TruthValue,
                stnVcCrossConnOperStatus        VcCrossConnOperStatus
        }

    stnVcCrossConnLowIfIndex OBJECT-TYPE     
        SYNTAX          InterfaceIndex
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION     "The value of this object is equal to MIB II's 
                        ifIndex value of the ATM interface port for this 
                        VP cross-connect. The term low implies that this ATM 
                        interface has the numerically lower ifIndex."
        ::= { stnVcCrossConnEntry 1 }

    stnVcCrossConnLowVpi OBJECT-TYPE     
        SYNTAX          Integer32
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION     "The value of this object is equal to the VPI value 
                        at the ATM interface associated with the VP 
                        cross-connect that is identified by LowIfIndex. 
                        The VPI value cannot exceed the number supported by 
                        the atmInterfaceMaxVpiBits at the low ATM interface 
                        port."
        ::= { stnVcCrossConnEntry 2 }

    stnVcCrossConnLowVci OBJECT-TYPE     
        SYNTAX          Integer32
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION     "The value of this object is equal to the VCI value 
                        at the ATM interface associated with the VC 
                        cross-connect that is identified by LowIfIndex. 
                        The VCI value cannot exceed the number supported by 
                        the atmInterfaceMaxVciBits at the low ATM interface 
                        port."
        ::= { stnVcCrossConnEntry 3 }

    stnVcCrossConnHighIfIndex OBJECT-TYPE     
        SYNTAX          InterfaceIndex
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION     "The value of this object is equal to MIB II's 
                        ifIndex value of the ATM interface port for this VP 
                        cross-connect. The term low implies that this ATM 
                        interface has the numerically lower ifIndex."
        ::= { stnVcCrossConnEntry 4 }

    stnVcCrossConnHighVpi OBJECT-TYPE     
        SYNTAX          Integer32
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION     "The value of this object is equal to the VPI value 
                        at the ATM interface associated with the VP 
                        cross-connect that is identified by LowIfIndex. 
                        The VPI value cannot exceed the number supported by 
                        the atmInterfaceMaxVpiBits at the low ATM interface 
                        port."
        ::= { stnVcCrossConnEntry 5 }

    stnVcCrossConnHighVci OBJECT-TYPE     
        SYNTAX          Integer32
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION     "The value of this object is equal to the VCI value 
                        at the ATM interface associated with the VC 
                        cross-connect that is identified by LowIfIndex. 
                        The VCI value cannot exceed the number supported by 
                        the atmInterfaceMaxVciBits at the low ATM interface 
                        port. "
        ::= { stnVcCrossConnEntry 6 }

    stnVcCrossConnLowAutoAssign OBJECT-TYPE     
        SYNTAX          TruthValue
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION     "Controls low vpi/vci/ifIndex auto assignement to an
                        ECF. If set to true MSM will attempt to dynamically
                        auto assign the connection to an available ECF."
        DEFVAL          { false }
        ::= { stnVcCrossConnEntry 7 }

    stnVcCrossConnHighAutoAssign OBJECT-TYPE     
        SYNTAX          TruthValue
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION     "Controls high vpi/vci/ifIndex auto assignement to an
                        ECF. If set to true MSM will attempt to dynamically
                        auto assign the connection to an available ECF."
        DEFVAL          { false }
        ::= { stnVcCrossConnEntry 8 }

    stnVcCrossConnOperStatus OBJECT-TYPE     
        SYNTAX          VcCrossConnOperStatus
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION     "Operational status of the VC cross connection."
        ::= { stnVcCrossConnEntry 9 }


    --
    -- STN ATM Traffic Descriptor Table
    --

    stnTrafficDescrTable OBJECT-TYPE
        SYNTAX        SEQUENCE OF StnTrafficDescrEntry
        MAX-ACCESS    not-accessible
        STATUS        current
        DESCRIPTION   "A list of ATM traffic descriptor entries."
        ::= { stnAtmTrafficDescrs 1 }
 
    stnTrafficDescrEntry OBJECT-TYPE
        SYNTAX          StnTrafficDescrEntry
        MAX-ACCESS      not-accessible
        STATUS          current
        DESCRIPTION     "Entry contains information about a particular 
                        ATM traffic descriptor."
        INDEX           { stnTrafDescrIndex }
        ::= { stnTrafficDescrTable 1 }
 
    StnTrafficDescrEntry ::= 
        SEQUENCE {
                stnTrafDescrIndex            Integer32,
                stnTrafDescrName             OCTET STRING,
                stnTrafDescrType             INTEGER,
                stnTrafDescrParam1           Integer32,
                stnTrafDescrParam2           Integer32,
                stnTrafDescrParam3           Integer32,
                stnTrafDescrParam4           Integer32,
                stnTrafDescrParam5           Integer32,
                stnTrafDescrQosClass         INTEGER,
                stnTrafDescrServiceCategory  INTEGER,
                stnTrafDescrFrameDiscard     TruthValue,
                stnTrafDescrOperStatus       TrafficDescrOperStatus,
                stnTrafDescrCommittedRate    Integer32,
                stnTrafDescrMaximumRate      Integer32
        }

    stnTrafDescrIndex OBJECT-TYPE     
        SYNTAX          Integer32
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION     "The value of this object identifies the row
                         in the stnTrafficDescrTable."
        ::= { stnTrafficDescrEntry 1 }

    stnTrafDescrName OBJECT-TYPE     
        SYNTAX          OCTET STRING (SIZE(0..32))
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION     "Text name assigned to this traffic descriptor."
        ::= { stnTrafficDescrEntry 2 }

    stnTrafDescrType OBJECT-TYPE     
        SYNTAX          INTEGER {
                                no-td(1),               --
                                no-clp-no-scr(2),       --
                                clp-no-tag-no-scr(3),   --
                                clp-tagging-no-scr(4),  --
                                no-clp-scr(5),          --
                                clp-no-tag-scr(6),      --
                                clp-tagging-scr(7),     --
                                clp-no-tag-mcr(8),      --
                                clp-trans-no-scr(9),    --
                                clp-trans-scr(10),      --
                                no-clp-tag-no-scr(11)   --
                        }
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION     "The value of this object identifies the type of ATM 
                        traffic descriptor. The type may indicate no traffic 
                        descriptor or traffic descriptor with one or more 
                        parameters.  These parameters are specified as a 
                        parameter vector, in the corresponding instances of 
                        the objects: 
                                    atmTrafDescrParam1, 
                                    atmTrafDescrParam2, 
                                    atmTrafDescrParam3, 
                                    atmTrafDescrParam4, 
                                    atmTrafDescrParam5 

                        Type values are as follows: 
                                    no-td
                                    no-clp-no-scr  
                                    clp-no-ag-no-scr
                                    clp-agging-no-scr
                                    no-clp-scr         
                                    clp-no-tag-scr  
                                    clp-tagging-scr  
                                    clp-no-tag-mcr    
                                    clp-trans-no-scr
                                    clp-trans-scr    
                                    no-clp-tag-no-scr" 
        DEFVAL          { no-td }
        ::= { stnTrafficDescrEntry 3 }

    stnTrafDescrParam1 OBJECT-TYPE     
        SYNTAX          Integer32
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION     "The first parameter of the ATM traffic descriptor 
                        used according to the value of atmTrafDescrType."
        DEFVAL          { 0 }
        ::= { stnTrafficDescrEntry 4 }

    stnTrafDescrParam2 OBJECT-TYPE     
        SYNTAX          Integer32
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION     "The second parameter of the ATM traffic descriptor 
                        used according to the value of atmTrafDescrType."
        DEFVAL          { 0 }
        ::= { stnTrafficDescrEntry 5 }

    stnTrafDescrParam3 OBJECT-TYPE     
        SYNTAX          Integer32
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION     "The third parameter of the ATM traffic descriptor 
                        used according to the value of atmTrafDescrType."
        DEFVAL          { 0 }
        ::= { stnTrafficDescrEntry 6 }

    stnTrafDescrParam4 OBJECT-TYPE     
        SYNTAX          Integer32
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION     "The fourth parameter of the ATM traffic descriptor 
                        used according to the value of atmTrafDescrType."
        DEFVAL          { 0 }
        ::= { stnTrafficDescrEntry 7 }

    stnTrafDescrParam5 OBJECT-TYPE     
        SYNTAX          Integer32
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION     "The fifth parameter of the ATM traffic descriptor 
                        used according to the value of atmTrafDescrType."
        DEFVAL          { 0 }
        ::= { stnTrafficDescrEntry 8 }

    stnTrafDescrQosClass OBJECT-TYPE     
        SYNTAX          INTEGER {
                                best-effort(0),
                                service-class-a(1),
                                service-class-b(2),
                                service-class-c(3),
                                service-class-d(4)
                        }
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION     "The value of this object identifies the QoS Class.  
                        Four Service classes have been specified in the ATM 
                        Forum UNI Specification: 

                        Service Class A: Constant bit rate video and Circuit emulation.
                        Service Class B: Variable bit rate video/audio.
                        Service Class C: Connection-oriented data.
                        Service Class D: Connectionless data.

                        Four QoS classes numbered 1, 2, 3, and 4 have been 
                        specified with the aim to support service classes 
                        A, B, C, and D respectively.  An unspecified QoS 
                        Class numbered '0' is used for best effort traffic."
        DEFVAL          { best-effort }
        ::= { stnTrafficDescrEntry 9 }

    stnTrafDescrServiceCategory OBJECT-TYPE
        SYNTAX          INTEGER {
                                other(1),   --
                                cbr(2),     --
                                rt-vbr(3),  --
                                nrt-vbr(4), --
                                abr(5),     --
                                ubr(6)      --  "
                        }
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION     "The ATM service category.
                        Service categories are as follows:
                                other,
                                cbr,
                                rt-vbr,
                                nrt-vbr,
                                abr,
                                ubr"
        DEFVAL          { ubr }
        ::= { stnTrafficDescrEntry 10 }
 
    stnTrafDescrFrameDiscard OBJECT-TYPE     
        SYNTAX          TruthValue 
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION     "Whether the network should discard frames rather
                        than cells."
        DEFVAL          { false }
        ::= { stnTrafficDescrEntry 11 }

    stnTrafDescrOperStatus OBJECT-TYPE     
        SYNTAX          TrafficDescrOperStatus
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION     "Operational status of the traffic descriptor."
        ::= { stnTrafficDescrEntry 12 }

    stnTrafDescrCommittedRate OBJECT-TYPE     
        SYNTAX          Integer32
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION     "The traffic rate in bits per second that the network 
                         is committed to transmit for VCs using this transmit
                         traffic descriptor."
        DEFVAL          { 0 }
        ::= { stnTrafficDescrEntry 13 }

    stnTrafDescrMaximumRate OBJECT-TYPE     
        SYNTAX          Integer32
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION     "The maximum traffic rate in bits per second that the 
                         network will transmit for VCs using this transmit
                         traffic descriptor, if it differs from the committed 
                         rate."
        DEFVAL          { 0 }
        ::= { stnTrafficDescrEntry 14 }

