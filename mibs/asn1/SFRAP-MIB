--
--      DTE Status 
--
--         The current status of the TO-DTE and TO-DCE ports.
--

sfrapStatusDteTable   OBJECT-TYPE
         SYNTAX   SEQUENCE OF SfrapStatusDteEntry
         ACCESS   not-accessible
         STATUS   mandatory
         DESCRIPTION
            "The table of DTE port statuses."
         ::= { sfrapStatus 7 }

sfrapStatusDteEntry   OBJECT-TYPE
         SYNTAX   SfrapStatusDteEntry
         ACCESS   not-accessible
         STATUS   mandatory
         DESCRIPTION
            "An status entry for a DTE port."
         INDEX { sfrapStatusDteIndex }
            ::= { sfrapStatusDteTable 1 }

SfrapStatusDteEntry ::=
         SEQUENCE {
             sfrapStatusDteIndex       INTEGER,
             sfrapStatusDteMode        INTEGER,
             sfrapStatusDteRts         INTEGER,
             sfrapStatusDteDtr         INTEGER,
             sfrapStatusDteDcd         INTEGER,
             sfrapStatusDteDsr         INTEGER,
             sfrapStatusDteCts         INTEGER,
             sfrapStatusDteSendtiming  INTEGER,
             sfrapStatusDteRxtiming    INTEGER,
             sfrapStatusDteTermtiming  INTEGER,
             sfrapStatusDteCtrl        INTEGER,
             sfrapStatusDteInd         INTEGER
         }

sfrapStatusDteIndex         OBJECT-TYPE  
     SYNTAX       INTEGER
     {
         to-dte   (1),
         to-dce   (2)
     }
     ACCESS       read-only
     STATUS       mandatory
     DESCRIPTION
     "Selects which DTE port."
     ::= { sfrapStatusDteEntry 99 }                                 

sfrapStatusDteMode   OBJECT-TYPE
     SYNTAX  INTEGER
     {
        no-connections (1),
        active         (2),
        test           (3)
     }
     ACCESS  read-only
     STATUS  mandatory
     DESCRIPTION
      "Status of the port. no-connections (1) indicates
      the port has no connections. Active (2) indicates 
      connections and data mode. Test (3) indicates
      connections and diagnostic state for that port."
     ::= { sfrapStatusDteEntry 1 }

sfrapStatusDteRts   OBJECT-TYPE
     SYNTAX  INTEGER
     {
        on  (1),
        off (2),
        na  (3)
     }
