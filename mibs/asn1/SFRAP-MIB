   --  Routing Short Term Statistics for each DLCI.

   sfrapPerfRoutingPerDlciTable OBJECT-TYPE
         SYNTAX   SEQUENCE OF SfrapPerfRoutingPerDlciEntry
         ACCESS   not-accessible
         STATUS   mandatory
         DESCRIPTION
            "The Short Term Statistics on the Routing
            protocol for each DLCI."
         ::= { sfrapPerfNetworkShortTerm 9 }

         sfrapPerfRoutingPerDlciEntry OBJECT-TYPE
             SYNTAX   SfrapPerfRoutingPerDlciEntry
             ACCESS   not-accessible
             STATUS   mandatory
             DESCRIPTION
                "The Routing Short Term Statistics for a particular DLCI."
            INDEX { sfrapPerfRoutingPerDlciInterval,
                    sfrapPerfRoutingPerDlciValue }
            ::= { sfrapPerfRoutingPerDlciTable 1 }

         SfrapPerfRoutingPerDlciEntry ::=
             SEQUENCE {
                 sfrapPerfRoutingPerDlciInterval  INTEGER,
                 sfrapPerfRoutingPerDlciValue     INTEGER,
                 sfrapPerfRoutingPerDlciRxOspf    Counter,
                 sfrapPerfRoutingPerDlciTxOspf    Counter,
                 sfrapPerfRoutingPerDlciRxRip     Counter,
                 sfrapPerfRoutingPerDlciTxRip     Counter,
                 sfrapPerfRoutingPerDlciRxNetbios Counter,
                 sfrapPerfRoutingPerDlciTxNetbios Counter
               }

      sfrapPerfRoutingPerDlciInterval    OBJECT-TYPE
             SYNTAX      INTEGER
             {
                  current-interval  (1),
                  previous-interval (2),
                  cumulative-counts (3)
             } 
             ACCESS      read-only
             STATUS      mandatory
             DESCRIPTION
                "The Short Term statistics model maintains three 
                 intervals: current, previous, and cumulative totals. 
                 Interval duration is defined by the Short Term Timer
                 (CfgFrPerfTimersSTInterval).
                 (1) Currently active short term interval
                 (2) Previously completed short term interval
                 (3) Cumulative total since last cleared."
            ::= { sfrapPerfRoutingPerDlciEntry 1 }

      sfrapPerfRoutingPerDlciValue    OBJECT-TYPE
             SYNTAX      INTEGER 
             ACCESS      read-only
             STATUS      mandatory
             DESCRIPTION
                "The DLCI value with which the Statistics are associated."
            ::= { sfrapPerfRoutingPerDlciEntry 2 }


      sfrapPerfRoutingPerDlciRxOspf    OBJECT-TYPE
             SYNTAX      Counter
             ACCESS      read-only
             STATUS      mandatory
