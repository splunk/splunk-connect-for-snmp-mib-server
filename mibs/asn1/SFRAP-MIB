              SYNTAX  Counter
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      "The count of all octets transmitted.
                       Same as ifOutOctets in mib-2."
              ::= { sfrapPerfMgmtIpIFStatsTable 3 }

          sfrapPerfMgmtIpIFOperStatus OBJECT-TYPE
              SYNTAX  INTEGER 
              {
                  up(1),
                  down(2),
                  testing(3)
              }
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      "The current operational state.
                       Same as ifOperStatus in mib-2."
              ::= { sfrapPerfMgmtIpIFStatsTable 4 }

