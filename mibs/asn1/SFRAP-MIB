   --  ICMP Short Term Statistics for each DLCI.

   sfrapPerfIcmpPerDlciTable OBJECT-TYPE
         SYNTAX   SEQUENCE OF SfrapPerfIcmpPerDlciEntry
         ACCESS   not-accessible
         STATUS   mandatory
         DESCRIPTION
            "Short Term Statistics on the ICMP
             protocol for each DLCI."
         ::= { sfrapPerfNetworkShortTerm 5 }

         sfrapPerfIcmpPerDlciEntry OBJECT-TYPE
             SYNTAX   SfrapPerfIcmpPerDlciEntry
             ACCESS   not-accessible
             STATUS   mandatory
             DESCRIPTION
                "The ICMP Short Term Statistics for a particular DLCI."
            INDEX { sfrapPerfIcmpPerDlciInterval, sfrapPerfIcmpPerDlciValue }
            ::= { sfrapPerfIcmpPerDlciTable 1 }

         SfrapPerfIcmpPerDlciEntry ::=
             SEQUENCE {
                 sfrapPerfIcmpPerDlciInterval        INTEGER,
                 sfrapPerfIcmpPerDlciValue           INTEGER,
                 sfrapPerfIcmpPerDlciRxTotal         Counter,
                 sfrapPerfIcmpPerDlciTxTotal         Counter,
                 sfrapPerfIcmpPerDlciRxEchoRep       Counter,
                 sfrapPerfIcmpPerDlciTxEchoRep       Counter,
                 sfrapPerfIcmpPerDlciRxDestUnr       Counter,
                 sfrapPerfIcmpPerDlciTxDestUnr       Counter,
                 sfrapPerfIcmpPerDlciRxSrcQuench     Counter,
                 sfrapPerfIcmpPerDlciTxSrcQuench     Counter,
                 sfrapPerfIcmpPerDlciRxRedirect      Counter,
                 sfrapPerfIcmpPerDlciTxRedirect      Counter,
                 sfrapPerfIcmpPerDlciRxEchoReq       Counter,
                 sfrapPerfIcmpPerDlciTxEchoReq       Counter,
                 sfrapPerfIcmpPerDlciRxTimeExcd      Counter,
                 sfrapPerfIcmpPerDlciTxTimeExcd      Counter,
                 sfrapPerfIcmpPerDlciRxParamProb     Counter,
                 sfrapPerfIcmpPerDlciTxParamProb     Counter,
                 sfrapPerfIcmpPerDlciRxTimestpReq    Counter,
                 sfrapPerfIcmpPerDlciTxTimestpReq    Counter,
                 sfrapPerfIcmpPerDlciRxTimestpRep    Counter,
                 sfrapPerfIcmpPerDlciTxTimestpRep    Counter,
                 sfrapPerfIcmpPerDlciRxAddrMaskReq   Counter,
                 sfrapPerfIcmpPerDlciTxAddrMaskReq   Counter,
                 sfrapPerfIcmpPerDlciRxAddrMaskRep   Counter,
                 sfrapPerfIcmpPerDlciTxAddrMaskRep   Counter,
                 sfrapPerfIcmpPerDlciRxPktTooBig     Counter,
                 sfrapPerfIcmpPerDlciTxPktTooBig     Counter,
                 sfrapPerfIcmpPerDlciRxGmQuery       Counter,
                 sfrapPerfIcmpPerDlciTxGmQuery       Counter,
                 sfrapPerfIcmpPerDlciRxGmReport      Counter,
                 sfrapPerfIcmpPerDlciTxGmReport      Counter,
                 sfrapPerfIcmpPerDlciRxGmReduct      Counter,
                 sfrapPerfIcmpPerDlciTxGmReduct      Counter
            }

      sfrapPerfIcmpPerDlciInterval    OBJECT-TYPE
             SYNTAX      INTEGER
             {
                  current-interval  (1),
                  previous-interval (2),
                  cumulative-counts (3)
             } 
             ACCESS      read-only
             STATUS      mandatory
             DESCRIPTION
                "The Short Term statistics model maintains three 
                 intervals: current, previous, and cumulative totals. 
                 Interval duration is defined by the Short Term Timer
                 (CfgFrPerfTimersSTInterval).
                 (1) Currently active short term interval
                 (2) Previously completed short term interval
                 (3) Cumulative total since last cleared."
            ::= { sfrapPerfIcmpPerDlciEntry 1 }

      sfrapPerfIcmpPerDlciValue    OBJECT-TYPE
             SYNTAX      INTEGER 
            ACCESS      read-only
             STATUS      mandatory
             DESCRIPTION
