   --  Application-level Short Term Statistics for each DLCI.

   sfrapPerfApplicationPerDlciTable OBJECT-TYPE
         SYNTAX   SEQUENCE OF SfrapPerfApplicationPerDlciEntry
         ACCESS   not-accessible
         STATUS   mandatory
         DESCRIPTION
            "The Short Term Statistics on the Application
            protocol for each DLCI."
         ::= { sfrapPerfNetworkShortTerm 7 }

         sfrapPerfApplicationPerDlciEntry OBJECT-TYPE
             SYNTAX   SfrapPerfApplicationPerDlciEntry
             ACCESS   not-accessible
             STATUS   mandatory
             DESCRIPTION
                "The Application Short Term Statistics for a particular DLCI."
            INDEX { sfrapPerfApplicationPerDlciInterval, 
                    sfrapPerfApplicationPerDlciValue }
            ::= { sfrapPerfApplicationPerDlciTable 1 }

         SfrapPerfApplicationPerDlciEntry ::=
             SEQUENCE {
                 sfrapPerfApplicationPerDlciInterval   INTEGER,
                 sfrapPerfApplicationPerDlciValue      INTEGER,
                 sfrapPerfApplicationPerDlciRxSnmp     Counter,
                 sfrapPerfApplicationPerDlciTxSnmp     Counter,
                 sfrapPerfApplicationPerDlciRxSnmpTrap Counter,
                 sfrapPerfApplicationPerDlciTxSnmpTrap Counter,
                 sfrapPerfApplicationPerDlciRxHttp     Counter,
                 sfrapPerfApplicationPerDlciTxHttp     Counter,
                 sfrapPerfApplicationPerDlciRxTelnet   Counter,
                 sfrapPerfApplicationPerDlciTxTelnet   Counter,
                 sfrapPerfApplicationPerDlciRxSmtp     Counter,
                 sfrapPerfApplicationPerDlciTxSmtp     Counter,
                 sfrapPerfApplicationPerDlciRxFtp      Counter,
                 sfrapPerfApplicationPerDlciTxFtp      Counter,
                 sfrapPerfApplicationPerDlciRxTftp     Counter,
                 sfrapPerfApplicationPerDlciTxTftp     Counter,
                 sfrapPerfApplicationPerDlciRxCustom1  Counter,
                 sfrapPerfApplicationPerDlciTxCustom1  Counter,
                 sfrapPerfApplicationPerDlciRxCustom2  Counter,
                 sfrapPerfApplicationPerDlciTxCustom2  Counter,
                 sfrapPerfApplicationPerDlciRxCustom3  Counter,
                 sfrapPerfApplicationPerDlciTxCustom3  Counter,
                 sfrapPerfApplicationPerDlciRxCustom4  Counter,
                 sfrapPerfApplicationPerDlciTxCustom4  Counter
               }

      sfrapPerfApplicationPerDlciInterval    OBJECT-TYPE
             SYNTAX      INTEGER
             {
                  current-interval  (1),
                  previous-interval (2),
                  cumulative-counts (3)
             } 
             ACCESS      read-only
             STATUS      mandatory
             DESCRIPTION
                "The Short Term statistics model maintains three 
                 intervals: current, previous, and cumulative totals. 
                 Interval duration is defined by the Short Term Timer
                 (CfgFrPerfTimersSTInterval).
                 (1) Currently active short term interval
                 (2) Previously completed short term interval
                 (3) Cumulative total since last cleared."
            ::= { sfrapPerfApplicationPerDlciEntry 1 }

      sfrapPerfApplicationPerDlciValue    OBJECT-TYPE
             SYNTAX      INTEGER 
             ACCESS      read-only
             STATUS      mandatory
             DESCRIPTION
                "The DLCI value with which the Statistics are associated."
            ::= { sfrapPerfApplicationPerDlciEntry 2 }

      sfrapPerfApplicationPerDlciRxSnmp    OBJECT-TYPE
             SYNTAX      Counter
             ACCESS      read-only
             STATUS      mandatory
             DESCRIPTION
                "The number of received SNMP bytes
                 that have been counted on this DLCI."
            ::= { sfrapPerfApplicationPerDlciEntry 3 }

      sfrapPerfApplicationPerDlciTxSnmp    OBJECT-TYPE
             SYNTAX      Counter
