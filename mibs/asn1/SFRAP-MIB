   --  Application-level Short Term Statistics totals.

   sfrapPerfApplicationTotalTable OBJECT-TYPE
         SYNTAX   SEQUENCE OF SfrapPerfApplicationTotalEntry
         ACCESS   not-accessible
         STATUS   mandatory
         DESCRIPTION
            "The Short Term Statistics on the Application
            protocol across all DLCIs."
         ::= { sfrapPerfNetworkShortTerm 8 }

         sfrapPerfApplicationTotalEntry OBJECT-TYPE
             SYNTAX   SfrapPerfApplicationTotalEntry
             ACCESS   not-accessible
             STATUS   mandatory
             DESCRIPTION
                "The Application Short Term Statistics for a particular DLCI."
            INDEX { sfrapPerfApplicationTotalInterval }
            ::= { sfrapPerfApplicationTotalTable 1 }

         SfrapPerfApplicationTotalEntry ::=
             SEQUENCE {
                 sfrapPerfApplicationTotalInterval   INTEGER,
                 sfrapPerfApplicationTotalRxSnmp     Counter,
                 sfrapPerfApplicationTotalTxSnmp     Counter,
                 sfrapPerfApplicationTotalRxSnmpTrap Counter,
                 sfrapPerfApplicationTotalTxSnmpTrap Counter,
                 sfrapPerfApplicationTotalRxHttp     Counter,
                 sfrapPerfApplicationTotalTxHttp     Counter,
                 sfrapPerfApplicationTotalRxTelnet   Counter,
                 sfrapPerfApplicationTotalTxTelnet   Counter,
                 sfrapPerfApplicationTotalRxSmtp     Counter,
                 sfrapPerfApplicationTotalTxSmtp     Counter,
                 sfrapPerfApplicationTotalRxFtp      Counter,
                 sfrapPerfApplicationTotalTxFtp      Counter,
                 sfrapPerfApplicationTotalRxTftp     Counter,
                 sfrapPerfApplicationTotalTxTftp     Counter,
                 sfrapPerfApplicationTotalRxCustom1  Counter,
                 sfrapPerfApplicationTotalTxCustom1  Counter,
                 sfrapPerfApplicationTotalRxCustom2  Counter,
                 sfrapPerfApplicationTotalTxCustom2  Counter,
                 sfrapPerfApplicationTotalRxCustom3  Counter,
                 sfrapPerfApplicationTotalTxCustom3  Counter,
                 sfrapPerfApplicationTotalRxCustom4  Counter,
                 sfrapPerfApplicationTotalTxCustom4  Counter
               }

      sfrapPerfApplicationTotalInterval    OBJECT-TYPE
             SYNTAX      INTEGER
             {
                  current-interval  (1),
                  previous-interval (2),
                  cumulative-counts (3)
             } 
             ACCESS      read-only
             STATUS      mandatory
             DESCRIPTION
                "The Short Term statistics model maintains three 
                 intervals: current, previous, and cumulative totals. 
                 Interval duration is defined by the Short Term Timer
                 (CfgFrPerfTimersSTInterval).
                 (1) Currently active short term interval
                 (2) Previously completed short term interval
                 (3) Cumulative total since last cleared."
            ::= { sfrapPerfApplicationTotalEntry 1 }

      sfrapPerfApplicationTotalRxSnmp    OBJECT-TYPE
             SYNTAX      Counter
             ACCESS      read-only
             STATUS      mandatory
             DESCRIPTION
                "The number of received SNMP bytes
                 that have been counted across all DLCIs."
            ::= { sfrapPerfApplicationTotalEntry 3 }

      sfrapPerfApplicationTotalTxSnmp    OBJECT-TYPE
             SYNTAX      Counter
             ACCESS      read-only
             STATUS      mandatory
             DESCRIPTION
