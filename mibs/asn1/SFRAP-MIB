   --  IPX Short Term Statistics for each DLCI.

   sfrapPerfIpxPerDlciTable OBJECT-TYPE
         SYNTAX   SEQUENCE OF SfrapPerfIpxPerDlciEntry
         ACCESS   not-accessible
         STATUS   mandatory
         DESCRIPTION
            "Short Term Statistics on the IPX
             protocol for each DLCI."
         ::= { sfrapPerfNetworkShortTerm 11 }

         sfrapPerfIpxPerDlciEntry OBJECT-TYPE
             SYNTAX   SfrapPerfIpxPerDlciEntry
             ACCESS   not-accessible
             STATUS   mandatory
             DESCRIPTION
                "The IPX Short Term Statistics for a particular DLCI."
            INDEX { sfrapPerfIpxPerDlciInterval, sfrapPerfIpxPerDlciValue }
