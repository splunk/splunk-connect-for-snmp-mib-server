              ::= { sfrapPerfMgmtIpTCPStatsTable 6 }


--
--       The sfrap Performance Thruput Table
--              This table contains Thruput statistics for the TO-DTE port
--              and the TO-DCE port.  This table does NOT contain any PVC 
--              DLCI thruput statistics.
--

sfrapPerfThruput   OBJECT IDENTIFIER ::= { sfrapPerformance 3 }

--
--       The DLCI Statistics Table
--              This table contains the DLCI Statistics (layer 2).

   Index ::= INTEGER

sfrapPerfThruputPerIntfTable  OBJECT-TYPE
     SYNTAX  SEQUENCE OF  SfrapPerfThruputPerIntfEntry
     ACCESS  not-accessible
     STATUS  mandatory
     DESCRIPTION
        "The throughput per interface table. These values are accumulated
         across all DLCIs."
     ::= { sfrapPerfThruput 1 }

sfrapPerfThruputPerIntfEntry  OBJECT-TYPE
     SYNTAX  SfrapPerfThruputPerIntfEntry
     ACCESS  not-accessible
     STATUS  mandatory
     DESCRIPTION
        "An entry in the throughput per interface table."
     INDEX   { sfrapPerfThruputPerIntfIndex }
     ::= { sfrapPerfThruputPerIntfTable 1 }

SfrapPerfThruputPerIntfEntry ::=
     SEQUENCE {
        sfrapPerfThruputPerIntfIndex       INTEGER, 
        sfrapPerfThruputPerIntfRxByteCnt   Counter,
        sfrapPerfThruputPerIntfTxByteCnt   Counter,
        sfrapPerfThruputPerIntfRxFrameCnt  Counter,
        sfrapPerfThruputPerIntfTxFrameCnt  Counter,
        sfrapPerfThruputPerIntfRxCrcErrCnt Counter,
        sfrapPerfThruputPerIntfRxAbortCnt  Counter,
        sfrapPerfThruputPerIntfRxBpvCnt    Counter
     }

   sfrapPerfThruputPerIntfIndex    OBJECT-TYPE
          SYNTAX      INTEGER
          {
             to-dte   (1),
             to-dce   (2)
          }
          ACCESS      read-only
          STATUS      mandatory
          DESCRIPTION
             "Interface for which the statistics apply.
              (1) TO-DTE interface
              (2) TO-DCE interface"
          ::= { sfrapPerfThruputPerIntfEntry 1 }

   sfrapPerfThruputPerIntfRxByteCnt    OBJECT-TYPE
          SYNTAX      Counter 
          ACCESS      read-only
          STATUS      mandatory
          DESCRIPTION
             "The number of framed bytes that have been received
              on this interface."
          ::= { sfrapPerfThruputPerIntfEntry 2 }

   sfrapPerfThruputPerIntfTxByteCnt    OBJECT-TYPE
             SYNTAX      Counter
             ACCESS      read-only
             STATUS      mandatory
             DESCRIPTION
                "The number of framed bytes that have been transmitted
                 on this interface."
             ::= { sfrapPerfThruputPerIntfEntry 3 }

   sfrapPerfThruputPerIntfRxFrameCnt    OBJECT-TYPE
             SYNTAX      Counter 
             ACCESS      read-only
             STATUS      mandatory
             DESCRIPTION
                "The number of frames that have been received
                 on this interface."
             ::= { sfrapPerfThruputPerIntfEntry 4 }

   sfrapPerfThruputPerIntfTxFrameCnt    OBJECT-TYPE
             SYNTAX      Counter 
             ACCESS      read-only
             STATUS      mandatory
             DESCRIPTION
                "The number of frames that have been transmitted
                 on this interface."
             ::= { sfrapPerfThruputPerIntfEntry 5 }

   sfrapPerfThruputPerIntfRxCrcErrCnt  OBJECT-TYPE
             SYNTAX      Counter 
             ACCESS      read-only
             STATUS      mandatory
             DESCRIPTION
                "The number of frames with CRC errors received
                 on this interface."
            ::= { sfrapPerfThruputPerIntfEntry 6 }

   sfrapPerfThruputPerIntfRxAbortCnt   OBJECT-TYPE
             SYNTAX      Counter 
             ACCESS      read-only
             STATUS      mandatory
             DESCRIPTION
                "The number of aborted frames received
                 on this interface."
            ::= { sfrapPerfThruputPerIntfEntry 7 }

   sfrapPerfThruputPerIntfRxBpvCnt   OBJECT-TYPE
             SYNTAX      Counter 
             ACCESS      read-only
             STATUS      mandatory
             DESCRIPTION
                "The number of BPV errors received
                 on this interface."
            ::= { sfrapPerfThruputPerIntfEntry 8 }


   sfrapPerfThruputPerDlciTable OBJECT-TYPE
         SYNTAX   SEQUENCE OF SfrapPerfThruputPerDlciEntry
         ACCESS   not-accessible
         STATUS   mandatory
         DESCRIPTION
            "Layer 2 statistics on a per-DLCI basis. Transmit direction
             is from TO-DTE to WAN and receive direction is from the WAN
             towards the TO-DTE."
         ::= { sfrapPerfThruput 2 }

         sfrapPerfThruputPerDlciEntry OBJECT-TYPE
             SYNTAX   SfrapPerfThruputPerDlciEntry
             ACCESS   not-accessible
             STATUS   mandatory
             DESCRIPTION
                "The Statistics for a particular Data Link
                 Connection Management Interface (DLCI)."
            INDEX { sfrapPerfThruputPerDlciIndex,
                    sfrapPerfThruputPerDlciValue }
            ::= { sfrapPerfThruputPerDlciTable 1 }

         SfrapPerfThruputPerDlciEntry ::=
             SEQUENCE {
                 sfrapPerfThruputPerDlciIndex               Index,
                 sfrapPerfThruputPerDlciValue               INTEGER,
                 sfrapPerfThruputPerDlciCreateTime          INTEGER,
                 sfrapPerfThruputPerDlciChangeTime          INTEGER,
                 sfrapPerfThruputPerDlciRxByte              Counter,
                 sfrapPerfThruputPerDlciTxByte              Counter,
                 sfrapPerfThruputPerDlciRxFrame             Counter,
                 sfrapPerfThruputPerDlciTxFrame             Counter,
                 sfrapPerfThruputPerDlciRxFecn              Counter,
                 sfrapPerfThruputPerDlciRxBecn              Counter,
                 sfrapPerfThruputPerDlciRxDe                Counter,
                 sfrapPerfThruputPerDlciTxDe                Counter,
                 sfrapPerfThruputPerDlciRxThruput           INTEGER,
                 sfrapPerfThruputPerDlciTxThruput           INTEGER,
                 sfrapPerfThruputPerDlciCIR                 INTEGER,
                 sfrapPerfThruputPerDlciUptime              INTEGER,
                 sfrapPerfThruputPerDlciDowntime            INTEGER,
                 sfrapPerfThruputPerDlciCirType             INTEGER,
                 sfrapPerfThruputPerDlciPvcState            INTEGER,
                 sfrapPerfThruputPerDlciOutageCount         Counter,
                 sfrapPerfThruputPerDlciAvailability        INTEGER,
                 sfrapPerfThruputPerDlciMTBSO               INTEGER,
                 sfrapPerfThruputPerDlciMTTSR               INTEGER,
                 sfrapPerfThruputPerDlciEncapType           INTEGER,
                 sfrapPerfThruputPerDlciRxUtilizationStatus INTEGER,
                 sfrapPerfThruputPerDlciTxUtilizationStatus INTEGER,
                 sfrapPerfThruputPerDlciEIR                 INTEGER
             }

   sfrapPerfThruputPerDlciIndex OBJECT-TYPE
             SYNTAX   Index
             ACCESS   read-only
             STATUS   mandatory
             DESCRIPTION
                "This value must be in the range 1-3.
                 Other than that, this value is ignored as
                 all three will return the same result."
            ::= { sfrapPerfThruputPerDlciEntry 1 }

   sfrapPerfThruputPerDlciValue    OBJECT-TYPE
             SYNTAX      INTEGER 
             ACCESS      read-only
             STATUS      mandatory
             DESCRIPTION
                "The DLCI value in which the Statistics are associated."
            ::= { sfrapPerfThruputPerDlciEntry 2 }

   sfrapPerfThruputPerDlciCreateTime    OBJECT-TYPE
             SYNTAX      INTEGER 
             ACCESS      read-only
             STATUS      mandatory
             DESCRIPTION
                "The amount of time elapsed since this DLCI was first
                 detected through traffic sensing or in an LMI message 
                 (in seconds)."
            ::= { sfrapPerfThruputPerDlciEntry 3 }

   sfrapPerfThruputPerDlciChangeTime    OBJECT-TYPE
             SYNTAX      INTEGER 
             ACCESS      read-only
             STATUS      mandatory
             DESCRIPTION
                "The amount of elapsed time since this DLCI last
                 changed state from active to inactive or vice versa
                 (in seconds)."
            ::= { sfrapPerfThruputPerDlciEntry 4 }

   sfrapPerfThruputPerDlciRxByte    OBJECT-TYPE
             SYNTAX      Counter 
             ACCESS      read-only
             STATUS      mandatory
             DESCRIPTION
                "The number of bytes that have been received from
                 the WAN towards the DTE on this DLCI. This count
                 will include any frames that are terminated by the 
                 unit and do not pass through to the opposite interface 
                 (management and networking data)."
            ::= { sfrapPerfThruputPerDlciEntry 5 }

   sfrapPerfThruputPerDlciTxByte    OBJECT-TYPE
             SYNTAX      Counter
             ACCESS      read-only
             STATUS      mandatory
             DESCRIPTION
                "The number of bytes that have been transmitted from the
                 TO-DTE towards the WAN on this DLCI. This count
                 will include any frames that are terminated by the 
                 unit and do not pass through to the opposite interface 
                 (management and networking data)."
       ::= { sfrapPerfThruputPerDlciEntry 6 }

   sfrapPerfThruputPerDlciRxFrame    OBJECT-TYPE
             SYNTAX      Counter
             ACCESS      read-only
             STATUS      mandatory
             DESCRIPTION
                "The number of frames that have been received from
                 the WAN towards to the DTE on this DLCI. This count
                 will include any frames that are terminated by the 
                 unit and do not pass through to the opposite interface 
                 (management and networking data)."
            ::= { sfrapPerfThruputPerDlciEntry 7 }

   sfrapPerfThruputPerDlciTxFrame    OBJECT-TYPE
             SYNTAX      Counter
             ACCESS      read-only
             STATUS      mandatory
             DESCRIPTION
                "The number of frames that have been transmitted from the
                 TO-DTE towards the WAN on this DLCI. This count
                 will include any frames that are terminated by the 
                 unit and do not pass through to the opposite interface 
                 (management and networking data)."
            ::= { sfrapPerfThruputPerDlciEntry 8 }

   sfrapPerfThruputPerDlciRxFecn    OBJECT-TYPE
             SYNTAX      Counter
             ACCESS      read-only
             STATUS      mandatory
             DESCRIPTION
                "The number of frames received from the WAN towards the TO-DTE 
                 that have had the Forward Explict Congestion Notification 
                 (FECN) bit set on this DLCI. This count will include any 
                 frames that are terminated by the unit and do not pass 
                 through to the opposite interface (management and networking 
                 data)."
            ::= { sfrapPerfThruputPerDlciEntry 9 }

   sfrapPerfThruputPerDlciRxBecn    OBJECT-TYPE
             SYNTAX      Counter
             ACCESS      read-only
             STATUS      mandatory
             DESCRIPTION
                "The number of frames received from the WAN towards the TO-DTE 
                 that have had the Backward Explict Congestion Notification 
                 (BECN) bit set on this DLCI. This count will include any 
                 frames that are terminated by the unit and do not pass 
                 through to the opposite interface (management and networking 
                 data)."
            ::= { sfrapPerfThruputPerDlciEntry 10 }

   sfrapPerfThruputPerDlciRxDe    OBJECT-TYPE
             SYNTAX      Counter
             ACCESS      read-only
             STATUS      mandatory
             DESCRIPTION
                "The number of frames received from the WAN towards the TO-DTE 
                 that have had the Discard Eligibility (DE) bit set on 
                 this DLCI. This count will include any frames that are 
                 terminated by the unit and do not pass through to the 
                 opposite interface (management and networking data)."
            ::= { sfrapPerfThruputPerDlciEntry 11 }

   sfrapPerfThruputPerDlciTxDe    OBJECT-TYPE
             SYNTAX      Counter
             ACCESS      read-only
             STATUS      mandatory
             DESCRIPTION
                "The number of frames transmit towards the WAN from the TO-DTE 
                 that have had the Discard Eligibility (DE) bit set on 
                 this DLCI. This count will include any frames that are 
                 terminated by the unit and do not pass through to the 
                 opposite interface (management and networking data)."
            ::= { sfrapPerfThruputPerDlciEntry 12 }

   sfrapPerfThruputPerDlciRxThruput    OBJECT-TYPE
             SYNTAX      INTEGER
             ACCESS      read-only
             STATUS      mandatory
             DESCRIPTION
                "The number of bits/sec received from the WAN on this 
                 DLCI. This count will include any frames that are 
                 terminated by the unit and do not pass through to the 
                 opposite interface (management and networking data)."
            ::= { sfrapPerfThruputPerDlciEntry 13 }

   sfrapPerfThruputPerDlciTxThruput    OBJECT-TYPE
             SYNTAX      INTEGER
             ACCESS      read-only
             STATUS      mandatory
             DESCRIPTION
                "The number of bits/sec transmit to the WAN on this 
                 DLCI. This count will include any frames that are 
                 terminated by the unit and do not pass through to 
                 the opposite interface (management and networking data)."
            ::= { sfrapPerfThruputPerDlciEntry 14 }

   sfrapPerfThruputPerDlciCIR    OBJECT-TYPE
             SYNTAX      INTEGER
             ACCESS      read-only
             STATUS      mandatory
             DESCRIPTION
                "The Committed Information Rate (CIR) for this DLCI.
                 This can come form one of three sources: From the LMI
                 Full Status Response, configured by the user, or 
                 the DTE line rate (default)."
            ::= { sfrapPerfThruputPerDlciEntry 15 }

   sfrapPerfThruputPerDlciCirType    OBJECT-TYPE
             SYNTAX      INTEGER 
             {
               cir-acquired-from-lmi  (1),
               cir-configured-by-user (2),
               cir-is-dte-datarate    (3)
             }
             ACCESS      read-only
             STATUS      mandatory
             DESCRIPTION
               "The source of the CIR value for this DLCI. 
                (1) CIR acquired from LMI message. Will override user
                    configured CIR. This feature is not supported by all
                    Frame Relay DCE (switches).
                (2) CIR configured by user.
                (3) CIR is DTE Line Rate. Default if CIR is not set
                    by one of the other methods."
            ::= { sfrapPerfThruputPerDlciEntry 18 }

   sfrapPerfThruputPerDlciUptime    OBJECT-TYPE
             SYNTAX      INTEGER 
             ACCESS      read-only
             STATUS      mandatory
             DESCRIPTION
                "The total amount of time that the DLCI has been up as
                 reported by the LMI Full Status Response (in seconds)."
            ::= { sfrapPerfThruputPerDlciEntry 16 }

   sfrapPerfThruputPerDlciDowntime    OBJECT-TYPE
             SYNTAX      INTEGER 
             ACCESS      read-only
             STATUS      mandatory
             DESCRIPTION
                "The total amount of time that the DLCI has been declared
                 down (in seconds). A DLCI is Down if it's explicitly 
                 declared Inactive through LMI or if it's missing from the 
                 LMI Full Status message or if there is no Full Status 
                 message at all."
            ::= { sfrapPerfThruputPerDlciEntry 17 }

   sfrapPerfThruputPerDlciPvcState   OBJECT-TYPE
             SYNTAX       INTEGER
             {
               pvc-active        (1),
               pvc-inactive      (2),
               pvc-unprovisioned (3),
               pvc-not-in-lmi    (4),
               pvc-lmi-timeout   (5),
               pvc-undetermined  (6)
             }
             ACCESS      read-only
             STATUS      mandatory
             DESCRIPTION
                "The current state of the DLCI:
                 (1) DLCI marked active in last full status LMI
                 (2) DLCI in last full status LMI but not marked active
                 (3) DLCI has never been seen in a full status LMI
                 (4) DLCI was seen at least once in a full status LMI
                     but was not in the last full status LMI
                 (5) the full status LMI has timed out; all previously
                     active or inactive DLCIs are changed to this state
                 (6) DLCI was detected in the traffic stream and a full
                     status LMI has not been received so the state cannot
                     be determined yet. "
            ::= { sfrapPerfThruputPerDlciEntry 19 }
         
   sfrapPerfThruputPerDlciOutageCount   OBJECT-TYPE
              SYNTAX      Counter
              ACCESS      read-only
              STATUS      mandatory
              DESCRIPTION
                 "The number of times the smperPerfThruputPerDlciPvcState
                  transitions from pvc-active or pvc-undetermined to one
                  of the other (inactive) states. "
             ::= { sfrapPerfThruputPerDlciEntry 20 }

   sfrapPerfThruputPerDlciAvailability    OBJECT-TYPE
             SYNTAX      INTEGER
             ACCESS      read-only
             STATUS      mandatory
             DESCRIPTION
                "The measure of the percentage time the DLCI is
                 available: UpTime/CreateTime or zero if CreateTime
                 = 0. (in 1/1000 per cent; i.e. availability = 1000 
                 converts to 1%). "
            ::= { sfrapPerfThruputPerDlciEntry 21 }

   sfrapPerfThruputPerDlciMTBSO    OBJECT-TYPE
             SYNTAX      INTEGER
             ACCESS      read-only
             STATUS      mandatory
             DESCRIPTION
                "Mean Time Between Service Outages: UpTime/OutageCount
                 or zero if OutageCount = 0 (in seconds). "
            ::= { sfrapPerfThruputPerDlciEntry 22 }

   sfrapPerfThruputPerDlciMTTSR    OBJECT-TYPE
             SYNTAX      INTEGER
             ACCESS      read-only
             STATUS      mandatory
             DESCRIPTION
                "Mean Time to ServiceRestoral: DownTime/OutageCount
                 or zero if OutageCount = 0 (in seconds). "
            ::= { sfrapPerfThruputPerDlciEntry 23 }

   sfrapPerfThruputPerDlciEncapType   OBJECT-TYPE
             SYNTAX      INTEGER
             {
                encap-na      (1),
                encap-1490    (2),
                encap-cisco   (3),
                encap-annex-g (4),
                encap-other   (5)
             }
             ACCESS      read-only
             STATUS      mandatory
             DESCRIPTION
                "The encapsulation protocol seen in the last frame
                 analyzed on this DLCI:
                 (1) DLCI is the LMI DLCI or no frames have been
                     analyzed
                 (2) The encapsulation is per rfc1490
                 (3) The encapsulation is per Cisco proprietary
                 (4) The encapsulation is per Annex-G (X.25 over
                     frame relay)
                 (5) The encapsulation is not one of the above. "
            ::= { sfrapPerfThruputPerDlciEntry 24 }

   sfrapPerfThruputPerDlciRxUtilizationStatus OBJECT-TYPE
             SYNTAX      INTEGER
             {
                clear                  (1),
                over-threshold         (2),
                alarm                  (3),
                alarm-under-threshold  (4)
             }
             ACCESS      read-only
             STATUS      mandatory
             DESCRIPTION
                "The status of the per-DLCI utilization
                 alarm in the receive direction.
                 (1) there is no alarm condition; utilization
                     is under the configured CIR percentage
                     threshold; if traps are enabled and the 
                     alarm had been previously triggered, a
                     utilization alarm clear trap will be sent.
                 (2) the utilization has been over the configured 
                     CIR percentage threshold for less than the
                     configured duration.
                 (3) the utilization has been over the configured 
                     CIR percentage threshold for more than the
                     configured duration; if traps are enabled
                     a utilization exceeded trap will be sent.
                 (4) the utilization has been under the configured 
                     CIR percentage threshold for less than the
                     configured duration. "
            ::= { sfrapPerfThruputPerDlciEntry 25 }
             
   sfrapPerfThruputPerDlciTxUtilizationStatus OBJECT-TYPE
             SYNTAX      INTEGER
             {
                clear                  (1),
                over-threshold         (2),
                alarm                  (3),
                alarm-under-threshold  (4)
             }
             ACCESS      read-only
             STATUS      mandatory
             DESCRIPTION
                "The status of the per-DLCI utilization
                 alarm in the transmit direction.
                 (1) there is no alarm condition; utilization
                     is under the configured CIR percentage
                     threshold; if traps are enabled and the 
                     alarm had been previously triggered, a
                     utilization alarm clear trap will be sent.
                 (2) the utilization has been over the configured 
                     CIR percentage threshold for less than the
                     configured duration.
                 (3) the utilization has been over the configured 
                     CIR percentage threshold for more than the
                     configured duration; if traps are enabled
                     a utilization exceeded trap will be sent.
                 (4) the utilization has been under the configured 
                     CIR percentage threshold for less than the
                     configured duration. "
             ::= { sfrapPerfThruputPerDlciEntry 26 }
             
   sfrapPerfThruputPerDlciEIR    OBJECT-TYPE
             SYNTAX      INTEGER
             ACCESS      read-only
             STATUS      mandatory
             DESCRIPTION
                "The Excess Information Rate.  This is defined to
                 be the maximum rate traffic is (supposed to be)
                 allowed to burst to."
            ::= { sfrapPerfThruputPerDlciEntry 27 }
             


sfrapPerfThruputCommands OBJECT IDENTIFIER ::= { sfrapPerfThruput 3 }

   sfrapPerfThruputCmdClearToDteStats    OBJECT-TYPE
          SYNTAX      INTEGER
          {
              clear-statistics (1)
          }
          ACCESS      write-only
          STATUS      mandatory
          DESCRIPTION
             "Allows the user to zero out all the interface statistics in
              the TO-DTE portion of the ThruputPerIntf statistics table.
              (1) Clear TO-DTE interface statistics command."
          ::= { sfrapPerfThruputCommands 1 }
          
   sfrapPerfThruputCmdClearToDceStats    OBJECT-TYPE
          SYNTAX      INTEGER
          {
              clear-statistics (1)
          }
          ACCESS      write-only
          STATUS      mandatory
          DESCRIPTION
             "Allows the user to zero out all the interface statistics in
              the WAN portion of the ThruputPerIntf statistics table.
              (1) Clear WAN interface statistics command."
          ::= { sfrapPerfThruputCommands 2 }
          
   sfrapPerfThruputCmdClearAllIntfStats    OBJECT-TYPE
          SYNTAX      INTEGER
          {
              clear-statistics (1)
          }
          ACCESS      write-only
          STATUS      mandatory
          DESCRIPTION
             "Allows the user to zero out all the statistics in
              the ThruputPerIntf statistics table."
          ::= { sfrapPerfThruputCommands 3 }

   sfrapPerfThruputCmdClearDlciStats    OBJECT-TYPE
          SYNTAX      INTEGER
          {
              clear-statistics (1)
          }
          ACCESS      write-only
          STATUS      mandatory
          DESCRIPTION
             "Allows the user to zero out all the per-DLCI statistics
              in the ThruputPerDlci statistics table and the the short 
              term statistics tables.
              (1) Clear layer 2 per-DLCI statistics command."
          ::= { sfrapPerfThruputCommands 4 }

   sfrapPerfThruputCmdClearAllStats    OBJECT-TYPE
          SYNTAX      INTEGER
          {
              clear-statistics (1)
          }
          ACCESS      write-only
          STATUS      mandatory
          DESCRIPTION
             "Allows the user to zero out all the statistics
              in the ThruputPerIntf and ThruputPerDlci statistics
              tables and in the short term statistics tables.
              (1) Clear all interface and layer 2 per-DLCI statistics."
          ::= { sfrapPerfThruputCommands 5 }

   sfrapPerfThruputCmdRemoveStsDlci    OBJECT-TYPE
          SYNTAX      INTEGER
          ACCESS      write-only
          STATUS      mandatory
          DESCRIPTION
             "Allows the user to remove a Dlci from the short term 
              statistics tables."
          ::= { sfrapPerfThruputCommands 6 }

   sfrapPerfThruputCmdReplaceDlciTable    OBJECT-TYPE
          SYNTAX      SEQUENCE OF SfrapPerfThruputCmdReplaceDlciEntry
          ACCESS      not-accessible
          STATUS      mandatory
          DESCRIPTION
             "Allows the user to replace one DLCI in the short-term
              statistics table with another one."
          ::= { sfrapPerfThruputCommands 7 }

       sfrapPerfThruputCmdReplaceDlciEntry   OBJECT-TYPE
          SYNTAX      SfrapPerfThruputCmdReplaceDlciEntry
          ACCESS      not-accessible
          STATUS      mandatory
          DESCRIPTION
             "Allows the user to replace one DLCI in the short-term
              statistics table with another one."
          INDEX { sfrapPerfThruputCmdReplaceDlciValue }
          ::= { sfrapPerfThruputCmdReplaceDlciTable 1 }

       SfrapPerfThruputCmdReplaceDlciEntry ::=
          SEQUENCE {
             sfrapPerfThruputCmdReplaceDlciValue     INTEGER,
             sfrapPerfThruputCmdReplaceDlciNewValue  INTEGER
          }
            
          sfrapPerfThruputCmdReplaceDlciValue  OBJECT-TYPE
             SYNTAX      INTEGER
             ACCESS      read-only
             STATUS      mandatory
             DESCRIPTION
                "A Dlci that is in the short-term stats table.  Index by
                 this Dlci value to identify the statistics entry to be
                 replaced."
             ::= { sfrapPerfThruputCmdReplaceDlciEntry 1 }

          sfrapPerfThruputCmdReplaceDlciNewValue  OBJECT-TYPE
             SYNTAX      INTEGER
             ACCESS      write-only
             STATUS      mandatory
             DESCRIPTION
                "A Dlci that is to replace another in the short-term stats 
                 table.  Index by this Dlci value to identify the statistics 
                 entry to do the replacing."
             ::= { sfrapPerfThruputCmdReplaceDlciEntry 2 }

   sfrapPerfThruputCmdAvailabilityStsDlciReset    OBJECT-TYPE
          SYNTAX      INTEGER
          ACCESS      write-only
          STATUS      mandatory
          DESCRIPTION
             "Allows the user to reset the availability
              statistics of an individual Dlci within
              the short-term stats table."
          ::= { sfrapPerfThruputCommands 8 }

   sfrapPerfThruputCmdAvailabilityStsDlciResetAll    OBJECT-TYPE
          SYNTAX      INTEGER
          ACCESS      write-only
          STATUS      mandatory
          DESCRIPTION
             "Allows the user to reset the availability
              statistics of all Dlci's within the short-term
              stats table."
          ::= { sfrapPerfThruputCommands 9 }

   sfrapPerfThruputCmdCountsStsDlciReset    OBJECT-TYPE
          SYNTAX      INTEGER
          ACCESS      write-only
          STATUS      mandatory
          DESCRIPTION
             "Allows the user to reset the count 
              statistics of an individual Dlci within
              the short-term stats table."
          ::= { sfrapPerfThruputCommands 10 }

   sfrapPerfThruputCmdCountsStsDlciResetAll    OBJECT-TYPE
          SYNTAX      INTEGER
          ACCESS      write-only
          STATUS      mandatory
          DESCRIPTION
             "Allows the user to reset the count
              statistics of all Dlci's within the short-term
              stats table."
          ::= { sfrapPerfThruputCommands 11 }

   sfrapPerfThruputCmdAllStsDlciReset    OBJECT-TYPE
          SYNTAX      INTEGER
          ACCESS      write-only
          STATUS      mandatory
          DESCRIPTION
             "Allows the user to reset both the count 
              and availability statistics of an individual Dlci within
              the short-term stats table."
          ::= { sfrapPerfThruputCommands 12 }

   sfrapPerfThruputCmdAllStsDlciResetAll    OBJECT-TYPE
          SYNTAX      INTEGER
          ACCESS      write-only
          STATUS      mandatory
          DESCRIPTION
             "Allows the user to reset both the count and the
              availability statistics of all Dlci's within the short-term
              stats table."
          ::= { sfrapPerfThruputCommands 13 }



sfrapPerfNetworkShortTerm OBJECT IDENTIFIER ::= { sfrapPerformance 4 }

   --  Network Layer Short Term Statistics for each DLCI.

   sfrapPerfNetwProtoPerDlciTable OBJECT-TYPE
         SYNTAX   SEQUENCE OF SfrapPerfNetwProtoPerDlciEntry
         ACCESS   not-accessible
         STATUS   mandatory
         DESCRIPTION
            "The Short Term Statistics on the Network Layer
             protocol for each DLCI. These are protocol-based
             per-DLCI statistics. The Short Term model maintains
             three intervals: current, previous, and cumulative
             totals. Interval duration is defined by the Short Term
             Timer (CfgFrPerfTimersSTInterval)."
         ::= { sfrapPerfNetworkShortTerm 1 }

         sfrapPerfNetwProtoPerDlciEntry OBJECT-TYPE
             SYNTAX   SfrapPerfNetwProtoPerDlciEntry
             ACCESS   not-accessible
             STATUS   mandatory
             DESCRIPTION
                "The Network Layer Short Term Statistics
                 for a particular DLCI. This table organizes
                 statistics by high-layer network protocol."
            INDEX { sfrapPerfNetwProtoPerDlciInterval,
                    sfrapPerfNetwProtoPerDlciValue }
            ::= { sfrapPerfNetwProtoPerDlciTable 1 }

         SfrapPerfNetwProtoPerDlciEntry ::=
             SEQUENCE {
                 sfrapPerfNetwProtoPerDlciInterval  INTEGER,
                 sfrapPerfNetwProtoPerDlciValue     INTEGER,
                 sfrapPerfNetwProtoPerDlciRxTotal   Counter,
                 sfrapPerfNetwProtoPerDlciTxTotal   Counter,
                 sfrapPerfNetwProtoPerDlciRxIp      Counter,
                 sfrapPerfNetwProtoPerDlciTxIp      Counter,
                 sfrapPerfNetwProtoPerDlciRxIpx     Counter,
                 sfrapPerfNetwProtoPerDlciTxIpx     Counter,
                 sfrapPerfNetwProtoPerDlciRxSna     Counter,
                 sfrapPerfNetwProtoPerDlciTxSna     Counter,
                 sfrapPerfNetwProtoPerDlciRxArp     Counter,
                 sfrapPerfNetwProtoPerDlciTxArp     Counter,
                 sfrapPerfNetwProtoPerDlciRxCisco   Counter,
                 sfrapPerfNetwProtoPerDlciTxCisco   Counter,
                 sfrapPerfNetwProtoPerDlciRxOther   Counter,
                 sfrapPerfNetwProtoPerDlciTxOther   Counter,
                 sfrapPerfNetwProtoPerDlciRxVnip    Counter,
                 sfrapPerfNetwProtoPerDlciTxVnip    Counter,
                 sfrapPerfNetwProtoPerDlciRxAnnexG  Counter,
                 sfrapPerfNetwProtoPerDlciTxAnnexG  Counter
               }

      sfrapPerfNetwProtoPerDlciInterval    OBJECT-TYPE
             SYNTAX      INTEGER
             {
                  current-interval  (1),
                  previous-interval (2),
                  cumulative-counts (3)
             } 
             ACCESS      read-only
             STATUS      mandatory
             DESCRIPTION
                "The Short Term statistics model maintains three 
                 intervals: current, previous, and cumulative totals. 
                 Interval duration is defined by the Short Term Timer
                 (CfgFrPerfTimersSTInterval).
                 (1) Currently active short term interval
                 (2) Previously completed short term interval
                 (3) Cumulative total since last cleared."
            ::= { sfrapPerfNetwProtoPerDlciEntry 1 }

      sfrapPerfNetwProtoPerDlciValue    OBJECT-TYPE
             SYNTAX      INTEGER 
             ACCESS      read-only
             STATUS      mandatory
             DESCRIPTION
                "The DLCI value with which the Statistics are associated."
            ::= { sfrapPerfNetwProtoPerDlciEntry 2 }

      sfrapPerfNetwProtoPerDlciRxTotal    OBJECT-TYPE
             SYNTAX      Counter 
             ACCESS      read-only
             STATUS      mandatory
             DESCRIPTION
                "The total number of received Network Layer bytes
                 that have been counted on this DLCI."
            ::= { sfrapPerfNetwProtoPerDlciEntry 3 }

      sfrapPerfNetwProtoPerDlciTxTotal    OBJECT-TYPE
             SYNTAX      Counter
             ACCESS      read-only
             STATUS      mandatory
             DESCRIPTION
                "The total number of transmitted Network Layer bytes
                 that have been counted on this DLCI."
            ::= { sfrapPerfNetwProtoPerDlciEntry 4 }

      sfrapPerfNetwProtoPerDlciRxIp    OBJECT-TYPE
             SYNTAX      Counter
             ACCESS      read-only
             STATUS      mandatory
             DESCRIPTION
                "The number of received IP bytes
                 that have been counted on this DLCI."
            ::= { sfrapPerfNetwProtoPerDlciEntry 5 }

      sfrapPerfNetwProtoPerDlciTxIp    OBJECT-TYPE
             SYNTAX      Counter
             ACCESS      read-only
             STATUS      mandatory
             DESCRIPTION
                "The number of transmitted IP bytes
                 that have been counted on this DLCI."
            ::= { sfrapPerfNetwProtoPerDlciEntry 6 }

      sfrapPerfNetwProtoPerDlciRxIpx    OBJECT-TYPE
             SYNTAX      Counter
             ACCESS      read-only
             STATUS      mandatory
             DESCRIPTION
                "The number of received IPX bytes
                 that have been counted on this DLCI."
            ::= { sfrapPerfNetwProtoPerDlciEntry 7 }

      sfrapPerfNetwProtoPerDlciTxIpx    OBJECT-TYPE
             SYNTAX      Counter
             ACCESS      read-only
             STATUS      mandatory
             DESCRIPTION
                "The number of transmitted IPX bytes
                 that have been counted on this DLCI."
            ::= { sfrapPerfNetwProtoPerDlciEntry 8 }

      sfrapPerfNetwProtoPerDlciRxSna    OBJECT-TYPE
             SYNTAX      Counter
             ACCESS      read-only
             STATUS      mandatory
             DESCRIPTION
                "The number of received SNA bytes
                 that have been counted on this DLCI."
            ::= { sfrapPerfNetwProtoPerDlciEntry 9 }

      sfrapPerfNetwProtoPerDlciTxSna    OBJECT-TYPE
             SYNTAX      Counter
             ACCESS      read-only
             STATUS      mandatory
             DESCRIPTION
                "The number of transmitted SNA bytes
                 that have been counted on this DLCI."
            ::= { sfrapPerfNetwProtoPerDlciEntry 10 }

      sfrapPerfNetwProtoPerDlciRxArp    OBJECT-TYPE
             SYNTAX      Counter
             ACCESS      read-only
             STATUS      mandatory
             DESCRIPTION
                "The number of received ARP bytes
                 that have been counted on this DLCI."
            ::= { sfrapPerfNetwProtoPerDlciEntry 11 }

      sfrapPerfNetwProtoPerDlciTxArp    OBJECT-TYPE
             SYNTAX      Counter
             ACCESS      read-only
             STATUS      mandatory
             DESCRIPTION
                "The number of transmitted ARP bytes
                 that have been counted on this DLCI."
            ::= { sfrapPerfNetwProtoPerDlciEntry 12 }

      sfrapPerfNetwProtoPerDlciRxCisco    OBJECT-TYPE
             SYNTAX      Counter
             ACCESS      read-only
             STATUS      mandatory
             DESCRIPTION
                "The number of received Cisco protocol bytes
                 that have been counted on this DLCI."
            ::= { sfrapPerfNetwProtoPerDlciEntry 13 }

      sfrapPerfNetwProtoPerDlciTxCisco    OBJECT-TYPE
             SYNTAX      Counter
             ACCESS      read-only
             STATUS      mandatory
             DESCRIPTION
                "The number of transmitted Cisco protocol bytes
                 that have been counted on this DLCI."
            ::= { sfrapPerfNetwProtoPerDlciEntry 14 }

      sfrapPerfNetwProtoPerDlciRxOther    OBJECT-TYPE
             SYNTAX      Counter
             ACCESS      read-only
             STATUS      mandatory
             DESCRIPTION
                "The number of received bytes on this DLCI
                 from protocols that are not counted elsewhere
                 in this table."
            ::= { sfrapPerfNetwProtoPerDlciEntry 15 }

      sfrapPerfNetwProtoPerDlciTxOther    OBJECT-TYPE
             SYNTAX      Counter
             ACCESS      read-only
             STATUS      mandatory
             DESCRIPTION
                "The number of transmitted bytes on this DLCI
                 from protocols that are not counted elsewhere
                 in this table."
            ::= { sfrapPerfNetwProtoPerDlciEntry 16 }

      sfrapPerfNetwProtoPerDlciRxVnip    OBJECT-TYPE
             SYNTAX      Counter 
             ACCESS      read-only
             STATUS      mandatory
             DESCRIPTION
                "The number of received VNIP protocol bytes
                 that have been counted on this DLCI."
            ::= { sfrapPerfNetwProtoPerDlciEntry 17 }

      sfrapPerfNetwProtoPerDlciTxVnip    OBJECT-TYPE
             SYNTAX      Counter 
             ACCESS      read-only
             STATUS      mandatory
             DESCRIPTION
                "The number of transmitted VNIP protocol bytes
                 that have been counted on this DLCI."
            ::= { sfrapPerfNetwProtoPerDlciEntry 18 }

      sfrapPerfNetwProtoPerDlciRxAnnexG  OBJECT-TYPE
             SYNTAX      Counter 
             ACCESS      read-only
             STATUS      mandatory
             DESCRIPTION
                "The number of received Annex G protocol 
                 bytes that have been counted on this DLCI."
            ::= { sfrapPerfNetwProtoPerDlciEntry 19 }

      sfrapPerfNetwProtoPerDlciTxAnnexG  OBJECT-TYPE
             SYNTAX      Counter 
             ACCESS      read-only
             STATUS      mandatory
             DESCRIPTION
                "The number of transmitted Annex G protocol 
                 bytes that have been counted on this DLCI."
            ::= { sfrapPerfNetwProtoPerDlciEntry 20 }



   --  Network Layer Short Term Statistics totals.

   sfrapPerfNetwProtoTotalTable OBJECT-TYPE
         SYNTAX   SEQUENCE OF SfrapPerfNetwProtoTotalEntry
         ACCESS   not-accessible
         STATUS   mandatory
         DESCRIPTION
            "The Short Term Statistics on Network Layer
            protocols summed across all DLCIs."
         ::= { sfrapPerfNetworkShortTerm 2 }

         sfrapPerfNetwProtoTotalEntry OBJECT-TYPE
             SYNTAX   SfrapPerfNetwProtoTotalEntry
             ACCESS   not-accessible
             STATUS   mandatory
             DESCRIPTION
                "The Network Layer Short Term Statistics
                 for a particular DLCI."
            INDEX { sfrapPerfNetwProtoTotalInterval }
            ::= { sfrapPerfNetwProtoTotalTable 1 }

         SfrapPerfNetwProtoTotalEntry ::=
             SEQUENCE {
                 sfrapPerfNetwProtoTotalInterval  INTEGER,
                 sfrapPerfNetwProtoTotalRxTotal   Counter,
                 sfrapPerfNetwProtoTotalTxTotal   Counter,
                 sfrapPerfNetwProtoTotalRxIp      Counter,
                 sfrapPerfNetwProtoTotalTxIp      Counter,
                 sfrapPerfNetwProtoTotalRxIpx     Counter,
                 sfrapPerfNetwProtoTotalTxIpx     Counter,
                 sfrapPerfNetwProtoTotalRxSna     Counter,
                 sfrapPerfNetwProtoTotalTxSna     Counter,
                 sfrapPerfNetwProtoTotalRxArp     Counter,
                 sfrapPerfNetwProtoTotalTxArp     Counter,
                 sfrapPerfNetwProtoTotalRxCisco   Counter,
                 sfrapPerfNetwProtoTotalTxCisco   Counter,
                 sfrapPerfNetwProtoTotalRxOther   Counter,
                 sfrapPerfNetwProtoTotalTxOther   Counter,
                 sfrapPerfNetwProtoTotalRxVnip    Counter,
                 sfrapPerfNetwProtoTotalTxVnip    Counter,
                 sfrapPerfNetwProtoTotalRxAnnexG  Counter,
                 sfrapPerfNetwProtoTotalTxAnnexG  Counter
               }

      sfrapPerfNetwProtoTotalInterval    OBJECT-TYPE
             SYNTAX      INTEGER
             {
                  current-interval  (1),
                  previous-interval (2),
                  cumulative-counts (3)
             } 
             ACCESS      read-only
             STATUS      mandatory
             DESCRIPTION
                "The Short Term statistics model maintains three 
                 intervals: current, previous, and cumulative totals. 
                 Interval duration is defined by the Short Term Timer
                 (CfgFrPerfTimersSTInterval).
                 (1) Currently active short term interval
                 (2) Previously completed short term interval
                 (3) Cumulative total since last cleared."
            ::= { sfrapPerfNetwProtoTotalEntry 1 }

