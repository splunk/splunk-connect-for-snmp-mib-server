                 sfrapPerfIpxPerDlciRxSpx      Counter,
                 sfrapPerfIpxPerDlciTxSpx      Counter,
                 sfrapPerfIpxPerDlciRxNcp      Counter,
                 sfrapPerfIpxPerDlciTxNcp      Counter,
                 sfrapPerfIpxPerDlciRxSap      Counter,
                 sfrapPerfIpxPerDlciTxSap      Counter,
                 sfrapPerfIpxPerDlciRxRip      Counter,
                 sfrapPerfIpxPerDlciTxRip      Counter,
                 sfrapPerfIpxPerDlciRxNetbios  Counter,
                 sfrapPerfIpxPerDlciTxNetbios  Counter,
                 sfrapPerfIpxPerDlciRxOther    Counter,
                 sfrapPerfIpxPerDlciTxOther    Counter
             }

