sfrapPerfCirPercentUtilization OBJECT IDENTIFIER ::= { sfrapPerformance 6 }

--
--  CIR Percentage Statistics for each DLCI.
--

 sfrapPerfCirPercentUtilizationTable OBJECT-TYPE
         SYNTAX   SEQUENCE OF SfrapPerfCirPercentUtilizationEntry
         ACCESS   not-accessible
         STATUS   mandatory
         DESCRIPTION
            "Short Term Statistics on the percentage of CIR used
             on each DLCI.  Each short term statistics interval,
             the count of bytes transmitted and received is used
             to calculate the percentage of CIR used.  The byte
             count is then added to the appropriate bucket for
             the CIR percentage range."
         ::= { sfrapPerfCirPercentUtilization 1 }

 sfrapPerfCirPercentUtilizationEntry OBJECT-TYPE
             SYNTAX   SfrapPerfCirPercentUtilizationEntry
             ACCESS   not-accessible
             STATUS   mandatory
             DESCRIPTION
                "The CIR Percentage Statistics for a particular DLCI.
                 These calculations are done at the completion of each 
                 Short Term interval."
             INDEX { sfrapPerfCirPercentUtilizationInterval,
                     sfrapPerfCirPercentUtilizationDlciValue }
             ::= { sfrapPerfCirPercentUtilizationTable 1 }

         SfrapPerfCirPercentUtilizationEntry ::=
             SEQUENCE {
                 sfrapPerfCirPercentUtilizationInterval    INTEGER,
                 sfrapPerfCirPercentUtilizationDlciValue   INTEGER,
                 sfrapPerfCirRxPercentUtilizationRange1    INTEGER,
                 sfrapPerfCirRxPercentUtilizationRange2    INTEGER,
                 sfrapPerfCirRxPercentUtilizationRange3    INTEGER,
                 sfrapPerfCirRxPercentUtilizationRange4    INTEGER,
                 sfrapPerfCirRxPercentUtilizationRange5    INTEGER,
                 sfrapPerfCirRxPercentUtilizationRange6    INTEGER,
                 sfrapPerfCirRxPercentUtilizationRange7    INTEGER,
                 sfrapPerfCirRxPercentUtilizationRange8    INTEGER,
                 sfrapPerfCirTxPercentUtilizationRange1    INTEGER,
                 sfrapPerfCirTxPercentUtilizationRange2    INTEGER,
                 sfrapPerfCirTxPercentUtilizationRange3    INTEGER,
                 sfrapPerfCirTxPercentUtilizationRange4    INTEGER,
                 sfrapPerfCirTxPercentUtilizationRange5    INTEGER,
                 sfrapPerfCirTxPercentUtilizationRange6    INTEGER,
                 sfrapPerfCirTxPercentUtilizationRange7    INTEGER,
                 sfrapPerfCirTxPercentUtilizationRange8    INTEGER
            }

 sfrapPerfCirPercentUtilizationInterval OBJECT-TYPE
             SYNTAX      INTEGER
             {
                  current-interval  (1),
                  previous-interval (2),
                  cumulative-counts (3)
             } 
             ACCESS      read-only
             STATUS      mandatory
             DESCRIPTION
                "The Short Term statistics model maintains three 
                 intervals: current, previous, and cumulative totals. 
                 Interval duration is defined by the Short Term Timer
                 (CfgFrPerfTimersSTInterval).
                 (1) Currently active short term interval
                 (2) Previously completed short term interval
                 (3) Cumulative total since last cleared."
            ::= { sfrapPerfCirPercentUtilizationEntry 1 }

 sfrapPerfCirPercentUtilizationDlciValue OBJECT-TYPE
             SYNTAX      INTEGER 
             ACCESS      read-only
             STATUS      mandatory
             DESCRIPTION
                "The DLCI value with which the Statistics are associated."
            ::= { sfrapPerfCirPercentUtilizationEntry 2 }

 sfrapPerfCirRxPercentUtilizationRange1 OBJECT-TYPE
             SYNTAX      INTEGER 
             ACCESS      read-only
             STATUS      mandatory
             DESCRIPTION
                "The number of short-term intervals during
                 which rx data throughput on this DLCI compared
                 to CIR was in range 1 (0% of CIR)."
            ::= { sfrapPerfCirPercentUtilizationEntry 21 }

 sfrapPerfCirRxPercentUtilizationRange2 OBJECT-TYPE
             SYNTAX      INTEGER 
             ACCESS      read-only
             STATUS      mandatory
             DESCRIPTION
                "The number of short-term intervals during
                 which rx data throughput on this DLCI compared
                 to CIR was in range 2 (1-10% of CIR)."
            ::= { sfrapPerfCirPercentUtilizationEntry 22 }

 sfrapPerfCirRxPercentUtilizationRange3 OBJECT-TYPE
             SYNTAX      INTEGER 
             ACCESS      read-only
             STATUS      mandatory
             DESCRIPTION
                "The number of short-term intervals during
                 which rx data throughput on this DLCI compared
                 to CIR was in range 3 (11-20% of CIR)."
            ::= { sfrapPerfCirPercentUtilizationEntry 23 }

 sfrapPerfCirRxPercentUtilizationRange4 OBJECT-TYPE
             SYNTAX      INTEGER 
             ACCESS      read-only
             STATUS      mandatory
             DESCRIPTION
                "The number of short-term intervals during
                 which rx data throughput on this DLCI compared
                 to CIR was in range 4 (21-50% of CIR)."
            ::= { sfrapPerfCirPercentUtilizationEntry 24 }

 sfrapPerfCirRxPercentUtilizationRange5 OBJECT-TYPE
             SYNTAX      INTEGER 
             ACCESS      read-only
             STATUS      mandatory
             DESCRIPTION
                "The number of short-term intervals during
                 which rx data throughput on this DLCI compared
                 to CIR was in range 5 (51-80% of CIR)."
            ::= { sfrapPerfCirPercentUtilizationEntry 25 }

 sfrapPerfCirRxPercentUtilizationRange6 OBJECT-TYPE
             SYNTAX      INTEGER 
             ACCESS      read-only
             STATUS      mandatory
             DESCRIPTION
                "The number of short-term intervals during
                 which rx data throughput on this DLCI compared
                 to CIR was in range 6 (81-100% of CIR)."
            ::= { sfrapPerfCirPercentUtilizationEntry 26 }

 sfrapPerfCirRxPercentUtilizationRange7 OBJECT-TYPE
             SYNTAX      INTEGER 
             ACCESS      read-only
             STATUS      mandatory
             DESCRIPTION
                "The number of short-term intervals during
                 which rx data throughput on this DLCI compared
                 to CIR was in range 7 (101-150% of CIR)."
            ::= { sfrapPerfCirPercentUtilizationEntry 27 }

 sfrapPerfCirRxPercentUtilizationRange8 OBJECT-TYPE
             SYNTAX      INTEGER 
             ACCESS      read-only
             STATUS      mandatory
             DESCRIPTION
                "The number of short-term intervals during
                 which rx data throughput on this DLCI compared
