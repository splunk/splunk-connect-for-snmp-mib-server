   --  ICMP Short Term Statistics totals.

   sfrapPerfIcmpTotalTable OBJECT-TYPE
         SYNTAX   SEQUENCE OF SfrapPerfIcmpTotalEntry
         ACCESS   not-accessible
         STATUS   mandatory
         DESCRIPTION
            "Short Term Statistics on the ICMP
             protocol across all DLCIs."
         ::= { sfrapPerfNetworkShortTerm 6 }

         sfrapPerfIcmpTotalEntry OBJECT-TYPE
             SYNTAX   SfrapPerfIcmpTotalEntry
             ACCESS   not-accessible
             STATUS   mandatory
             DESCRIPTION
                "The ICMP Short Term Statistics for a particular DLCI."
            INDEX { sfrapPerfIcmpTotalInterval }
            ::= { sfrapPerfIcmpTotalTable 1 }

         SfrapPerfIcmpTotalEntry ::=
             SEQUENCE {
                 sfrapPerfIcmpTotalInterval        INTEGER,
                 sfrapPerfIcmpTotalRxTotal         Counter,
                 sfrapPerfIcmpTotalTxTotal         Counter,
                 sfrapPerfIcmpTotalRxEchoRep       Counter,
                 sfrapPerfIcmpTotalTxEchoRep       Counter,
                 sfrapPerfIcmpTotalRxDestUnr       Counter,
                 sfrapPerfIcmpTotalTxDestUnr       Counter,
                 sfrapPerfIcmpTotalRxSrcQuench     Counter,
                 sfrapPerfIcmpTotalTxSrcQuench     Counter,
                 sfrapPerfIcmpTotalRxRedirect      Counter,
                 sfrapPerfIcmpTotalTxRedirect      Counter,
                 sfrapPerfIcmpTotalRxEchoReq       Counter,
                 sfrapPerfIcmpTotalTxEchoReq       Counter,
                 sfrapPerfIcmpTotalRxTimeExcd      Counter,
                 sfrapPerfIcmpTotalTxTimeExcd      Counter,
                 sfrapPerfIcmpTotalRxParamProb     Counter,
                 sfrapPerfIcmpTotalTxParamProb     Counter,
                 sfrapPerfIcmpTotalRxTimestpReq    Counter,
                 sfrapPerfIcmpTotalTxTimestpReq    Counter,
                 sfrapPerfIcmpTotalRxTimestpRep    Counter,
                 sfrapPerfIcmpTotalTxTimestpRep    Counter,
                 sfrapPerfIcmpTotalRxAddrMaskReq   Counter,
                 sfrapPerfIcmpTotalTxAddrMaskReq   Counter,
                 sfrapPerfIcmpTotalRxAddrMaskRep   Counter,
                 sfrapPerfIcmpTotalTxAddrMaskRep   Counter,
                 sfrapPerfIcmpTotalRxPktTooBig     Counter,
                 sfrapPerfIcmpTotalTxPktTooBig     Counter,
                 sfrapPerfIcmpTotalRxGmQuery       Counter,
                 sfrapPerfIcmpTotalTxGmQuery       Counter,
                 sfrapPerfIcmpTotalRxGmReport      Counter,
                 sfrapPerfIcmpTotalTxGmReport      Counter,
                 sfrapPerfIcmpTotalRxGmReduct      Counter,
                 sfrapPerfIcmpTotalTxGmReduct      Counter
            }

      sfrapPerfIcmpTotalInterval    OBJECT-TYPE
             SYNTAX      INTEGER
             {
                  current-interval  (1),
                  previous-interval (2),
                  cumulative-counts (3)
             } 
             ACCESS      read-only
             STATUS      mandatory
             DESCRIPTION
                "The Short Term statistics model maintains three 
                 intervals: current, previous, and cumulative totals. 
                 Interval duration is defined by the Short Term Timer
                 (CfgFrPerfTimersSTInterval).
                 (1) Currently active short term interval
                 (2) Previously completed short term interval
                 (3) Cumulative total since last cleared."
            ::= { sfrapPerfIcmpTotalEntry 1 }

      sfrapPerfIcmpTotalRxTotal    OBJECT-TYPE
             SYNTAX      Counter
             ACCESS      read-only
             STATUS      mandatory
             DESCRIPTION
                "The total number of ICMP bytes that have been
                 counted across all DLCIs."
            ::= { sfrapPerfIcmpTotalEntry 3 }

      sfrapPerfIcmpTotalTxTotal    OBJECT-TYPE
             SYNTAX      Counter
             ACCESS      read-only
               STATUS      mandatory
             DESCRIPTION
                "The total number of ICMP bytes that have been
                 counted across all DLCIs."
            ::= { sfrapPerfIcmpTotalEntry 4 }

      sfrapPerfIcmpTotalRxEchoRep    OBJECT-TYPE
             SYNTAX      Counter
             ACCESS      read-only
             STATUS      mandatory
             DESCRIPTION
                "The number of bytes in ICMP ECHO repies
                 that have been counted across all DLCIs."
            ::= { sfrapPerfIcmpTotalEntry 5 }

      sfrapPerfIcmpTotalTxEchoRep    OBJECT-TYPE
             SYNTAX      Counter
             ACCESS      read-only
               STATUS      mandatory
             DESCRIPTION
                "The number of bytes in ICMP ECHO repies
                 that have been counted across all DLCIs."
            ::= { sfrapPerfIcmpTotalEntry 6 }

