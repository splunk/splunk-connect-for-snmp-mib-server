             SYNTAX      Counter  
             ACCESS      read-only
             STATUS      mandatory
             DESCRIPTION
                "The number of received bytes on this DLCI
                 from LMI protocols that are not counted
                 elsewhere (other than Total) in this table."
            ::= { sfrapPerfLmiPerDlciEntry 13 }

      sfrapPerfLmiPerDlciTxOtherByteCnt    OBJECT-TYPE
             SYNTAX      Counter  
             ACCESS      read-only
             STATUS      mandatory
             DESCRIPTION
                "The number of transmitted bytes on this DLCI
                 from LMI protocols that are not counted
                 elsewhere (other than Total) in this table."
            ::= { sfrapPerfLmiPerDlciEntry 14 }


   --  LMI Statistics for totals.

   sfrapPerfLmiTotalTable OBJECT-TYPE
         SYNTAX   SEQUENCE OF SfrapPerfLmiTotalEntry
         ACCESS   not-accessible
         STATUS   mandatory
         DESCRIPTION
            "Short Term Statistics on LMI protocol across all DLCIs."
         ::= { sfrapPerfNetworkShortTerm 18 }

         sfrapPerfLmiTotalEntry OBJECT-TYPE
             SYNTAX   SfrapPerfLmiTotalEntry
             ACCESS   not-accessible
             STATUS   mandatory
             DESCRIPTION
                "The LMI Short Term Statistics across all DLCIs."
             INDEX { sfrapPerfLmiTotalInterval }
             ::= { sfrapPerfLmiTotalTable 1 }

         SfrapPerfLmiTotalEntry ::=
             SEQUENCE {
                 sfrapPerfLmiTotalInterval            INTEGER,
                 sfrapPerfLmiTotalDlciValue           INTEGER,
                 sfrapPerfLmiTotalRxTotalByteCnt      Counter,
                 sfrapPerfLmiTotalTxTotalByteCnt      Counter,
                 sfrapPerfLmiTotalRxLivoEnqByteCnt    Counter,
                 sfrapPerfLmiTotalTxLivoEnqByteCnt    Counter,
                 sfrapPerfLmiTotalRxLivoStatByteCnt   Counter,
                 sfrapPerfLmiTotalTxLivoStatByteCnt   Counter,
                 sfrapPerfLmiTotalRxFullEnqByteCnt    Counter,
                 sfrapPerfLmiTotalTxFullEnqByteCnt    Counter,
                 sfrapPerfLmiTotalRxFullStatByteCnt   Counter,
                 sfrapPerfLmiTotalTxFullStatByteCnt   Counter,
                 sfrapPerfLmiTotalRxOtherByteCnt      Counter,
                 sfrapPerfLmiTotalTxOtherByteCnt      Counter
            }

