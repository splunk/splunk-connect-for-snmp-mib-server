sfrapStatus  OBJECT IDENTIFIER ::= { sfrap 4 }

--
--      LED Status
--
--         The current status of the SFRAP front-panel LED indicators.
--
sfrapStatusLedTable   OBJECT IDENTIFIER ::= { sfrapStatus 4 }

sfrapStatusLedOpStatusLED   OBJECT-TYPE
     SYNTAX  INTEGER
     {
        greenLED-high-speed-mode          (1),
        blinkgreenLED-low-speed-mode      (2),
        yellowLED-test-mode               (3),
        redLED-alarm-mode                 (4),
        blinkredLED-cut-through-mode      (5)
     }
     ACCESS  read-only
     STATUS  mandatory
     DESCRIPTION
      "Status of the Operation Mode LED."
     ::= { sfrapStatusLedTable 1 }

sfrapStatusLedToDTETxLED   OBJECT-TYPE
     SYNTAX  INTEGER
     {
        offLED-inactive                   (1),
        greenLED-tx-data-receiving        (2)
     }
     ACCESS  read-only
     STATUS  mandatory
     DESCRIPTION
      "Status of the TO-DTE TxD LED."
     ::= { sfrapStatusLedTable 2 }
     
sfrapStatusLedToDTERxLED   OBJECT-TYPE
     SYNTAX  INTEGER
     {
        offLED-inactive                   (1),
        greenLED-rx-data-receiving        (2)
     }
     ACCESS  read-only
     STATUS  mandatory
     DESCRIPTION
      "Status of the TO-DTE RxD LED."
     ::= { sfrapStatusLedTable 3 }

sfrapStatusLedRtsLED   OBJECT-TYPE
     SYNTAX  INTEGER
     {
        greenLED-rts-active               (1),
        offLED-rts-inactive               (2)
     }
     ACCESS  read-only
     STATUS  mandatory
     DESCRIPTION
      "Status of the RTS Signal LED."
     ::= { sfrapStatusLedTable 4 }

sfrapStatusLedCtsLED   OBJECT-TYPE
     SYNTAX  INTEGER
     {
        greenLED-cts-active               (1),
        offLED-cts-inactive               (2)
     }
     ACCESS  read-only
     STATUS  mandatory
     DESCRIPTION
      "Status of the CTS Signal LED."
     ::= { sfrapStatusLedTable 5 }
        
sfrapStatusLedDsrLED   OBJECT-TYPE
     SYNTAX  INTEGER
     {
        greenLED-dsr-active               (1),
        offLED-dsr-inactive               (2)
     }
     ACCESS  read-only
     STATUS  mandatory
     DESCRIPTION
      "Status of the DSR Signal LED."
     ::= { sfrapStatusLedTable 7 }
        
sfrapStatusLedDcdLED   OBJECT-TYPE
     SYNTAX  INTEGER
     {
        greenLED-dcd-active               (1),
        offLED-dcd-inactive               (2)
     }
     ACCESS  read-only
     STATUS  mandatory
     DESCRIPTION
      "Status of the DCD Signal LED."
     ::= { sfrapStatusLedTable 8 }

sfrapStatusLedToDCETxLED   OBJECT-TYPE
     SYNTAX  INTEGER
     {
        offLED-inactive                   (1),
        greenLED-tx-data-receiving        (2)
     }
     ACCESS  read-only
     STATUS  mandatory
     DESCRIPTION
      "Status of the TO-DCE TxD LED."
     ::= { sfrapStatusLedTable 9 }
     
sfrapStatusLedToDCERxLED   OBJECT-TYPE
     SYNTAX  INTEGER
     {
        offLED-inactive                   (1),
        greenLED-rx-data-receiving        (2)
     }
     ACCESS  read-only
     STATUS  mandatory
     DESCRIPTION
      "Status of the TO-DCE RxD LED."
     ::= { sfrapStatusLedTable 10 }
  
sfrapStatusLedLmiErrLED   OBJECT-TYPE
     SYNTAX  INTEGER
     {
        offLED-lmi-active-state           (1),
        yellowLED-lmi-error-state         (2),
        blinkyellowLED-lmi-source-state   (3)
     }
     ACCESS  read-only
     STATUS  mandatory
     DESCRIPTION
      "Status of the LMI Error LED."
     ::= { sfrapStatusLedTable 15 }
        
sfrapStatusLedFrmActLED   OBJECT-TYPE
     SYNTAX  INTEGER
     {
        greenLED-frm-active               (1),
        offLED-frm-inactive               (2)
     }
     ACCESS  read-only
     STATUS  mandatory
     DESCRIPTION
      "Status of the Frame Activity LED."
     ::= { sfrapStatusLedTable 16 }
     
sfrapStatusLedCtrlLED   OBJECT-TYPE
     SYNTAX  INTEGER
     {
        yellowLED-ctrl-active            (1),
        offLED-ctrl-inactive             (2)
     }
     ACCESS  read-only
     STATUS  mandatory
     DESCRIPTION
      "Status of the X.21 Control Signal LED.
       Only applicable when in X.21."
     ::= { sfrapStatusLedTable 17 }

sfrapStatusLedIndLED   OBJECT-TYPE
     SYNTAX  INTEGER
     {
        yellowLED-ind-active              (1),
        offLED-ind-inactive               (2)
     }
     ACCESS  read-only
     STATUS  mandatory
     DESCRIPTION
      "Status of the X.21 Indication Signal LED.
       Only applicable when in X.21."
     ::= { sfrapStatusLedTable 18 }
          
sfrapStatusAllLEDs   OBJECT-TYPE
     SYNTAX  DisplayString (SIZE (0..10))
     ACCESS  read-only
     STATUS  mandatory
     DESCRIPTION
        "Status of all ten SFRAP LEDs, encoded in a string.
             'F'    off
             '5'    green
             '0'    yellow
             'A'    red
             '7'    blinking green and off
             '3'    blinking yellow and off
             'B'    blinking red and off
             '4'    blinking green and yellow
             '6'    blinking green and red
             '8'    blinking yellow and red

         Positionally, the 13 letters are SFRAP Op Mode,
         TO-DCE Rx, TO-DCE Tx, RTS, CTS, DCD, DSR,
         LMI Error, Frame Activity, Ctrl, Ind, TO-DTE Rx, TO-DTE Tx."
     ::= { sfrapStatusLedTable 14 }
     

