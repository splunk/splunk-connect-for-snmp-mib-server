  XYLOGICS-ANNEX-MIB DEFINITIONS ::= BEGIN                                      
                                                                                
   IMPORTS                                                                      
                 enterprises, NetworkAddress, IpAddress,                        
                                 Counter, Gauge, TimeTicks                      
                         FROM RFC1155-SMI                                       
                 OBJECT-TYPE                                                    
                         FROM RFC-1212;                                         
                                                                                
   xylogics	OBJECT IDENTIFIER ::= { enterprises 15 }                            
                                                                                
   prod		OBJECT IDENTIFIER ::= { xylogics 1 }                                   
   prodannex	OBJECT-TYPE                                                        
 		SYNTAX OCTET STRING                                                          
 		ACCESS read-only                                                             
 		STATUS mandatory                                                             
 		::= { prod 1 }                                                               
                                                                                
   annex		OBJECT IDENTIFIER ::= { xylogics 2 }                                  
                                                                                
   hw		OBJECT IDENTIFIER ::= { annex 1 }                                        
   sw		OBJECT IDENTIFIER ::= { annex 2 }                                        
   ports		OBJECT IDENTIFIER ::= { annex 3 }                                     
                                                                                
 -- the hw group                                                                
                                                                                
   hwType	OBJECT-TYPE                                                           
 		SYNTAX INTEGER {                                                             
 			err(1),                                                                     
 			annexI(11),                                                                 
 			annexII(16),                                                                
 			annexX25(23),                                                               
 			annex3(42),
 
                        microannex(52)
                                                                  
 		}                                                                            
 		ACCESS read-only                                                             
 		STATUS mandatory                                                             
 		::= { hw 1 }                                                                 
                                                                                
   hwRev		OBJECT-TYPE                                                           
 		SYNTAX OCTET STRING                                                          
 		ACCESS read-only                                                             
 		STATUS mandatory                                                             
 		::= { hw 2 }                                                                 
                                                                                
   romRev	OBJECT-TYPE                                                           
   		-- Note: old Annex-Is cannot return this value                             
 		SYNTAX OCTET STRING                                                          
 		ACCESS read-only                                                             
 		STATUS mandatory                                                             
 		::= { hw 3 }                                                                 
                                                                                
   serialNumber	OBJECT-TYPE                                                     
 		SYNTAX INTEGER                                                               
 		ACCESS read-only                                                             
 		STATUS mandatory                                                             
 		::= { hw 4 }                                                                 
                                                                                
   memorySize	OBJECT-TYPE                                                       
 		SYNTAX INTEGER                                                               
 		ACCESS read-only                                                             
 		STATUS mandatory                                                             
 		::= { hw 5 }                                                                 
                                                                                
