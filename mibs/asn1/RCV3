    ::= { rc3Snmp 6 }

-- =======================
-- Client Configuration Group
-- =======================

rc3ClientAuthentication OBJECT-TYPE
	SYNTAX INTEGER {
		enableRadius(1),
		disableAuthentication(2),
        enableLocal (3)
	}
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
        "Enable or Disable client Authentication. There are two Authentication methods.
        Either RADIUS or LOCAL. Client Authentication is not avaliable on the 
        personal Ravlin."
	::= { rc3ClientCfg 1 }

rc3ActiveRadiusServer OBJECT-TYPE
	SYNTAX IpAddress
	ACCESS read-only
	STATUS mandatory
	DESCRIPTION
		"Specify the current active RADIUS server by its IP Address. If this
		RADIUS server is not responding, then the agent will direct RADIUS
		requests to the next preferred RADIUS server in the list, and
		accordingly change the value of this object to indicate the active server."
	::= { rc3ClientCfg 2 }

rc3RadiusPriority OBJECT-TYPE
	SYNTAX OCTET STRING (SIZE(1..3))
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
		"Specify the priority of the Radius servers in the table.  Each octet
		will specify one of the entries in order of preference."
	::= { rc3ClientCfg 3 }

-- =======================
-- Radius Server Table
-- =======================

rc3RadiusAuthServerTable OBJECT-TYPE
	SYNTAX SEQUENCE OF Rc3RadiusAuthServerEntry
	ACCESS not-accessible
	STATUS mandatory
	DESCRIPTION
		"Table, to setup Primary and/or Secondary Authentication Server.
		This is a fixed sized table. RADIUS packets will be routed to
		their destination using the Route Table."
	::= { rc3ClientCfg 4 }

rc3RadiusAuthServerEntry OBJECT-TYPE
	SYNTAX Rc3RadiusAuthServerEntry
	ACCESS not-accessible
	STATUS mandatory
	DESCRIPTION
		"RADIUS Auth Server entry containing all the attributes
		required to configure Auth Server. There will be a
        maximum of three entries in the table. If 
        rc3RadiusAuthServerIP is 0.0.0.0, then that row is not used."
	INDEX  { rc3RadiusAuthServerEntryIndx }
	::= { rc3RadiusAuthServerTable 1 }

Rc3RadiusAuthServerEntry ::= SEQUENCE {
    rc3RadiusAuthServerEntryIndx         INTEGER,
	rc3RadiusAuthServerIP				IpAddress,
	rc3RadiusAuthServerPort				INTEGER,
    rc3RadiusAttributeOffset             INTEGER,
	rc3RadiusAuthServerSharedSecret		OCTET STRING,
	rc3RadiusAuthServerRetries			INTEGER,
    rc3RadiusAuthServerSharedSecretFips PbeShaEncryptedObject
	}

rc3RadiusAuthServerEntryIndx OBJECT-TYPE
	SYNTAX INTEGER (1..3)
	ACCESS not-accessible
--	ACCESS read-only
	STATUS mandatory
	DESCRIPTION
		"A unique identifier for the entry in this table.  Valid values
		are 1-3."	
	::= { rc3RadiusAuthServerEntry 1 }

rc3RadiusAuthServerIP OBJECT-TYPE
	SYNTAX IpAddress
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
		"The IP Address (Network Address), of the RADIUS Authentication
		server."	
	::= { rc3RadiusAuthServerEntry 2 }

rc3RadiusAuthServerPort OBJECT-TYPE
	SYNTAX INTEGER
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
		"Specifies the UDP port number used by the RADIUS service on the
		Authentication Server."
	::= { rc3RadiusAuthServerEntry 3 }

rc3RadiusAttributeOffset OBJECT-TYPE
    SYNTAX INTEGER
    ACCESS read-write
    STATUS mandatory
    DESCRIPTION
		"If zero, then use vendor specific attributes. Otherwise this object
		specifies the first offset to use for the RedCreek attribute values."
    ::= { rc3RadiusAuthServerEntry 4 }

rc3RadiusAuthServerSharedSecret OBJECT-TYPE
	SYNTAX OCTET STRING
	ACCESS read-write
	STATUS deprecated
	DESCRIPTION
		"This is the shared secret between the Ravlin and Radius Server.
		For SNMP set operations the string is encrypted. The key for
		encrypt/Decrypt is generated by hashing the password and a 
		selector value. This object will return NULL for get/getnext."
	::= { rc3RadiusAuthServerEntry 5 }

rc3RadiusAuthServerRetries OBJECT-TYPE
	SYNTAX INTEGER
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
		"This limits the number of retries with the Authentication Server.
		Default is 3. Maximum is 5. Min is 1"
	::= { rc3RadiusAuthServerEntry 6 }

rc3RadiusAuthServerSharedSecretFips OBJECT-TYPE
	SYNTAX PbeShaEncryptedObject
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
		"Available for release 3.30 and up. Object used to set shared secret
        key between the Ravlin and Radius Server. Refer to the textual convention of 
        PbeShaEncryptedObject, for the process used to set this object.
        If the preshared key exists, then on read a dummy string is 
        returned. shared secret key length is (0..32)"
	::= { rc3RadiusAuthServerEntry 7 }

