    " "
	::= { rc3Cert 6 }

-- Group under Policy Database.
-- =======================
-- ISAKMP proposal Table
-- =======================

rc3IsakmpProposalTable OBJECT-TYPE
	SYNTAX SEQUENCE OF Rc3IsakmpProposalEntry
	ACCESS not-accessible
	STATUS mandatory
	DESCRIPTION
		"This table contains a list of ISAKMP phase one proposals."
	::= { rc3Proposals 1 }

rc3IsakmpProposalEntry OBJECT-TYPE
	SYNTAX Rc3IsakmpProposalEntry
	ACCESS not-accessible
	STATUS mandatory
	DESCRIPTION
		"An entry in the ISAKMP phase one proposal table."
	INDEX { rc3IsakmpProposalIndx }
	::= { rc3IsakmpProposalTable 1 }

Rc3IsakmpProposalEntry ::= SEQUENCE {
	rc3IsakmpProposalIndx            INTEGER,
	rc3IsakmpProposalEncryption      INTEGER,
	rc3IsakmpProposalHash            INTEGER,
	rc3IsakmpProposalAuthMode        INTEGER,
	rc3IsakmpProposalDhGroup         INTEGER
	}

rc3IsakmpProposalIndx OBJECT-TYPE
	SYNTAX INTEGER
	ACCESS not-accessible
--	ACCESS read-only
	STATUS mandatory
	DESCRIPTION
		"This value is used as a unique identifier for entries in this table."
	::= { rc3IsakmpProposalEntry 1 }

rc3IsakmpProposalEncryption OBJECT-TYPE
	SYNTAX INTEGER{
        des-cbc(1),
		triple-des-cbc(5),
		des-cbc-40(249)
	}
	ACCESS read-only
	STATUS mandatory
	DESCRIPTION
		"The encryption type.  Currently only des-cbc-56 and 3des-cbc
		and des-cbc-40 are supported."
	::= { rc3IsakmpProposalEntry 2 }

rc3IsakmpProposalHash OBJECT-TYPE
	SYNTAX INTEGER{
		md5(1),
		sha(2)
	}
	ACCESS read-only
	STATUS mandatory
	DESCRIPTION
		"The hash type.  Currently md5 and sha are supported."
	::= { rc3IsakmpProposalEntry 3 }

rc3IsakmpProposalAuthMode OBJECT-TYPE
	SYNTAX INTEGER{
		pre-sharedkey(1),
		dss-signature(2),
		rsa-signature(3),
		rsa-encryption(4)
	}
	ACCESS read-only
	STATUS mandatory
	DESCRIPTION
		"later"
	::= { rc3IsakmpProposalEntry 4 }

rc3IsakmpProposalDhGroup OBJECT-TYPE
	SYNTAX INTEGER{
		group1(1),
		group(2)
	}
	ACCESS read-only
	STATUS mandatory
	DESCRIPTION
		"later"
	::= { rc3IsakmpProposalEntry 5 }

-- =======================
-- ESP proposal Table
-- =======================

rc3EspProposalTable OBJECT-TYPE
	SYNTAX SEQUENCE OF Rc3EspProposalEntry 
	ACCESS not-accessible
	STATUS mandatory
	DESCRIPTION
		"This table contains a list of ESP proposals."
	::= { rc3Proposals 2 }

rc3EspProposalEntry OBJECT-TYPE
	SYNTAX Rc3EspProposalEntry
	ACCESS not-accessible
	STATUS mandatory
	DESCRIPTION
		"An entry in the ESP proposal table."
	INDEX { rc3EspProposalIndx }
	::= { rc3EspProposalTable 1 }

Rc3EspProposalEntry ::= SEQUENCE {
	rc3EspProposalIndx                INTEGER,
    rc3EspProposalCipherAlgo          INTEGER,
	rc3EspProposalEncapsulation       INTEGER,
    rc3EspProposalAuth                INTEGER,
    rc3EspProposalGroup               INTEGER
    }

rc3EspProposalIndx OBJECT-TYPE
	SYNTAX INTEGER
	ACCESS not-accessible
--	ACCESS read-only
	STATUS mandatory
	DESCRIPTION
		"This value is used as a unique identifier for entries in this table."
   ::= { rc3EspProposalEntry 1 }

rc3EspProposalCipherAlgo OBJECT-TYPE
    SYNTAX INTEGER{
		esp-null(0),
        esp-des(2),
		esp-3des(3),
        esp-40des(249)
		}
	ACCESS read-only
	STATUS mandatory
	DESCRIPTION
	"later"
	::= { rc3EspProposalEntry 2 }
	
rc3EspProposalEncapsulation OBJECT-TYPE
	SYNTAX INTEGER{
		tunnel(1),
		transport(2)
	}
	ACCESS read-only
	STATUS mandatory
	DESCRIPTION
		"later"
	::= { rc3EspProposalEntry 3 }

rc3EspProposalAuth  OBJECT-TYPE
	SYNTAX INTEGER{
        no-auth(0),
        hmac-md5(1),
        hmac-sha-1(2),
        des-mac(3)
    }
	ACCESS read-only
	STATUS mandatory
	DESCRIPTION
		"later"
	::= { rc3EspProposalEntry 4 }	

rc3EspProposalGroup  OBJECT-TYPE
	SYNTAX INTEGER{
		group1(1),
		group(2)
	}
	ACCESS read-only
	STATUS deprecated
	DESCRIPTION
		"later"
	::= { rc3EspProposalEntry 5 }	

