-- Imports

IMPORTS
		enterprises,
		IpAddress,
		Counter
		    FROM RFC1155-SMI

		DisplayString FROM RFC1213-MIB
		OBJECT-TYPE
		    FROM RFC-1212

		ifIndex
		    FROM RFC1213-MIB;

-- Top Branches under RedCreek

redCreek		OBJECT IDENTIFIER ::= { enterprises 1958 }

products		OBJECT IDENTIFIER ::= { redCreek 1 }
registration	OBJECT IDENTIFIER ::= { redCreek 2 }

-- rcRavlin is common to both Ravlin 10, Ravlin 4, PCI
rcRavlin    	OBJECT IDENTIFIER ::= { products 1 }

-- Groups defined under the RedCreek Ravlin Product
rcAdmin			OBJECT IDENTIFIER ::= { rcRavlin 1 }
rcSecure		OBJECT IDENTIFIER ::= { rcRavlin 2 }
rcBoot			OBJECT IDENTIFIER ::= { rcRavlin 3 }
rcStatus		OBJECT IDENTIFIER ::= { rcRavlin 4 }
rcTrap			OBJECT IDENTIFIER ::= { rcRavlin 5 }
rcDebug			OBJECT IDENTIFIER ::= { rcRavlin 6 }
rcEsp           OBJECT IDENTIFIER ::= { rcRavlin 7 }
rcMgmt          OBJECT IDENTIFIER ::= { rcRavlin 8 }
rcPolicy        OBJECT IDENTIFIER ::= { rcRavlin 9 }

-- Branches under registration, will be all the different products. 
-- (OID used for sysObjectId)

rcRavlin10  	OBJECT IDENTIFIER ::= { registration 1 }
rcRavlin4   	OBJECT IDENTIFIER ::= { registration 2 }
rcPCI           OBJECT IDENTIFIER ::= { registration 3 }

-- New OID for for products that support 3.0 MIBS.

rcRavlin10Ver3  OBJECT IDENTIFIER ::= { registration 4 }
rcRavlin4Ver3   OBJECT IDENTIFIER ::= { registration 5 }
rcPCIVer3       OBJECT IDENTIFIER ::= { registration 6 }

-- Textual Conventions

RowStatus ::= INTEGER
{
     -- the following two values are states:
     -- these values may be read or written
     active(1),
     notInService(2),
     -- the following value is a state:
     -- this value may be read, but not written
     notReady(3),
     -- the following three values are
     -- actions: these values may be written,
     --   but are never read
     createAndGo(4),
     createAndWait(5),
     destroy(6)
}

PbeShaEncryptedObject ::= OCTET STRING
--{
--	STATUS	current
--	DESCRIPTION
--		"An object of this type is formed by taking the original data, and
--         encrypting the object using password based encryption (PKCS5) where
--        the algorithm is 'PBEwithSHA1andDES'.  The password required by the
--        PbeShaEncryptedObject textual convention is that stored internally
--        as rc3PasswordSha. The length of object of this type is specified 
--        in the description clause of the specific object.

--        Unless otherwise specified, an object of this type returns an 
--        OCTET STRING of SIZE(0) upon read regardless of the actual internal
--        value."
--}

-- Group under Management.

rc3System           OBJECT IDENTIFIER ::= { rcMgmt 1 }
rc3Network          OBJECT IDENTIFIER ::= { rcMgmt 2 }
rc3Control          OBJECT IDENTIFIER ::= { rcMgmt 3 }
rc3Stat             OBJECT IDENTIFIER ::= { rcMgmt 4 }
rc3Snmp             OBJECT IDENTIFIER ::= { rcMgmt 5 }
rc3ClientCfg        OBJECT IDENTIFIER ::= { rcMgmt 6 }
rc3Download         OBJECT IDENTIFIER ::= { rcMgmt 7 }
rc3Misc             OBJECT IDENTIFIER ::= { rcMgmt 8 }
rc3Cert             OBJECT IDENTIFIER ::= { rcMgmt 9 }

-- Group under Policy Database.

rc3Proposals        OBJECT IDENTIFIER ::= { rcPolicy 1 }
rc3Pde              OBJECT IDENTIFIER ::= { rcPolicy 2 }
rc3PdMisc           OBJECT IDENTIFIER ::= { rcPolicy 3 }

-- Begin Group under Management.
rc3BootRomVer OBJECT-TYPE
    SYNTAX DisplayString (SIZE(0..12))
	ACCESS read-only
	STATUS mandatory
	DESCRIPTION
		"Boot Rom Software version, in the form 'major.minor[letters]'.
		If the version is unknown or not avaliable then the value
		should be a zero length string."
	::= { rc3System 1 }

rc3FirmwareVer OBJECT-TYPE
    SYNTAX DisplayString (SIZE(0..12))
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
		"Software version, in the form 'major.minor[letters]'. If
		the version is unknown or not avaliable then the value
		should be a zero length string."
    ::= { rc3System 2 }

rc3FirmwareID OBJECT-TYPE
	SYNTAX DisplayString (SIZE(0..24))
	ACCESS read-only
	STATUS mandatory
	DESCRIPTION
		"A Textual description to identify the firmware on the Ravlin."
	::= { rc3System 3 }

rc3HardwareVer OBJECT-TYPE
    SYNTAX DisplayString (SIZE(0..12))
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
		"Hardware version, in the form 'major.minor[letters]'. If
		the version is unknown or not avaliable then the value
		should be a zero length string."
    ::= { rc3System 4 }

rc3DistinguishedName	OBJECT-TYPE
	SYNTAX DisplayString
	ACCESS read-only
	STATUS mandatory
	DESCRIPTION
		"Factory Assigned Unique Security ID for the Ravlin Unit.
		It consists of three parts of the form 'mmmCK-xxx-sssss'.
		mmm - is the model number.
		CK - is a single digit checksum
		xxx,sssss - is a sequence number."
	::= { rc3System 5 }

rc3HostName	OBJECT-TYPE
	SYNTAX DisplayString (SIZE(0..15))
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
		"Host name assigned to the Ravlin unit. If host name exists,
		then it is used as the DHCP host name, else rcDistinguishedName
		is used. Hence the name must be unique in a single DHCP scope.
		Max Size for Host name is 15 characters."
	::= { rc3System 6 }

-- -------------
-- NETWORK GROUP
-- -------------

-- =======================
-- Interface Table
-- =======================

rc3InterfaceTable OBJECT-TYPE
	SYNTAX SEQUENCE OF Rc3InterfaceEntry
	ACCESS not-accessible
	STATUS mandatory
	DESCRIPTION
		"Interface table, for the Ravlin unit. This is a fixed sized table. The
		size of the table is determined by the value of ifNumber. Entries cannot
		be added or deleted."
	::= { rc3Network 1 }

rc3InterfaceEntry OBJECT-TYPE
	SYNTAX Rc3InterfaceEntry
	ACCESS not-accessible
	STATUS mandatory
	DESCRIPTION
		"An interface entry containing objects at the
		subnetwork layer and below for a particular
		interface."
	INDEX { ifIndex }
	::= { rc3InterfaceTable 1 }

Rc3InterfaceEntry ::= SEQUENCE {
--	rc3InterfaceIfIndex	INTEGER,
	rc3InterfaceIp		IpAddress,
	rc3InterfaceMask		IpAddress,
	rc3InterfaceMac		OCTET STRING
    }

-- rc3InterfaceIfIndex OBJECT-TYPE
--  SYNTAX INTEGER
--  ACCESS not-accessible
--	ACCESS read-only
--  STATUS mandatory
--	DESCRIPTION
--		"A unique value for each interface.  Its value 
--		ranges between 1 and the value of ifNumber. 
--		The interface identified by a particular value
--		of this index is the same interface as identified
--		by the same value of ifIndex."
--	::= { rc3InterfaceEntry 1 }

rc3InterfaceIp OBJECT-TYPE
	SYNTAX IpAddress
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
		"The interface's address at the network layer
		in the protocol stack."
	::= { rc3InterfaceEntry 2 }

rc3InterfaceMask OBJECT-TYPE
	SYNTAX IpAddress
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
		"The subnet mask associated with the IP address of
		this entry.  The value of the mask is an IP
		address with all the network bits set to 1 and all
		the hosts bits set to 0."
	::= { rc3InterfaceEntry 3 }

rc3InterfaceMac	OBJECT-TYPE
	SYNTAX OCTET STRING
	ACCESS read-only
	STATUS mandatory
	DESCRIPTION
		"The interface's address at the protocol layer
		immediately `below' the network layer in the
		protocol stack.  For interfaces which do not have
		such an address (e.g., a serial line), this object
		should contain an octet string of zero length."
	::= { rc3InterfaceEntry 4 }

-- =======================
-- Routing Table
-- =======================

rc3IpRouteTable OBJECT-TYPE
	SYNTAX SEQUENCE OF Rc3IpRouteEntry
	ACCESS not-accessible
	STATUS mandatory
	DESCRIPTION
		"The routing table for this Ravlin."
	::= { rc3Network 2 }

rc3IpRouteEntry OBJECT-TYPE
	SYNTAX Rc3IpRouteEntry
	ACCESS not-accessible
	STATUS mandatory
	DESCRIPTION
		"A route to a particular destination."
	INDEX { rc3IpRouteDest, rc3IpRouteMask }
	::= { rc3IpRouteTable 1 }

Rc3IpRouteEntry ::= SEQUENCE {
	rc3IpRouteDest           IpAddress,
	rc3IpRouteMask           IpAddress,
    rc3IpRouteIfIndex        INTEGER,
    rc3IpRouteNextHop        IpAddress,
    rc3IpRouteMetric         INTEGER,
    rc3IpRouteProto          INTEGER,
    rc3IpRouteAge            INTEGER,
	rc3IpRouteRowStatus   	RowStatus
	}

rc3IpRouteDest OBJECT-TYPE
    SYNTAX IpAddress
	ACCESS not-accessible
--	ACCESS read-only
    STATUS mandatory
    DESCRIPTION
		"The destination IP address of this route.  An entry with
		a value of 0.0.0.0 is considered a default route.  This
		value should be logical-ANDed with the rc3IpRouteMask
		prior to being added to the table."
    ::= { rc3IpRouteEntry 1 }

rc3IpRouteMask OBJECT-TYPE
    SYNTAX IpAddress
	ACCESS not-accessible
--	ACCESS read-only
    STATUS mandatory
    DESCRIPTION
		"Indicates the mask that will be logical-ANDed with the
		destination address before being compared to the value in
		the rc3IpRouteDest field."
    ::= { rc3IpRouteEntry 2 }

rc3IpRouteIfIndex OBJECT-TYPE
    SYNTAX INTEGER
    ACCESS read-write
    STATUS mandatory
    DESCRIPTION
		"The index value which uniquely identifies the local
		interface through which the next hop of this route
		should be reached."
    ::= { rc3IpRouteEntry 3 }

rc3IpRouteNextHop OBJECT-TYPE
    SYNTAX IpAddress
    ACCESS read-write
    STATUS mandatory
    DESCRIPTION
		"The IP address of the next hop of this route."
    ::= { rc3IpRouteEntry 4 }

rc3IpRouteMetric OBJECT-TYPE
    SYNTAX INTEGER
    ACCESS read-write
    STATUS mandatory
    DESCRIPTION
		"The primary routing metric for this route."
    ::= { rc3IpRouteEntry 5 }

rc3IpRouteProto OBJECT-TYPE
    SYNTAX INTEGER {
		other(1),		-- none of the following

						-- non-protocol information,
						-- e.g., manually configured
		local(2),		-- entries

						-- set via a network
		netmgmt(3),		-- management protocol

						-- obtained via ICMP,
		icmp(4),		-- e.g., Redirect

						-- the remaining values are
						-- all gateway routing
						-- protocols
		egp(5),
		ggp(6),
		hello(7),
		rip(8),
		is-is(9),
		es-is(10),
		ciscoIgrp(11),
		bbnSpfIgp(12),
		ospf(13),
		bgp(14)
	}
	ACCESS read-write
    STATUS mandatory
    DESCRIPTION
		"The routing mechanism via which this route was learned."
    ::= { rc3IpRouteEntry 6 }

rc3IpRouteAge OBJECT-TYPE
    SYNTAX INTEGER
    ACCESS read-write
    STATUS mandatory
    DESCRIPTION
		"The number of seconds since this route was last updated
		or otherwise determined to be correct."
    ::= { rc3IpRouteEntry 7 }

rc3IpRouteRowStatus OBJECT-TYPE
    SYNTAX RowStatus
    ACCESS read-write
    STATUS mandatory
    DESCRIPTION
		"This object is used to create/delete rows from the IP
		Route Table. See the definition for RowStatus for a range
		of valid values used. On read status indicates ACTIVE."
    ::= { rc3IpRouteEntry 8 }

rc3PacketHandlingOptions OBJECT-TYPE
    SYNTAX INTEGER
    ACCESS read-write
    STATUS mandatory
    DESCRIPTION
		"This is Bit Masked Object. Zero or more bits can be set or cleared.
		Bit 0 - Forward non-IP Traffic in Bridge Mode
		Bit 1 - Forward Local IP Traffic in Bridge Mode
		Bit 2 - Forward MAC Broadcast in Bridge Mode
		Bit 3 - Forward IP Broadcast in Router Mode
		Bit 4 - Send ICMP Re-Direct Messages in Router Mode
		Bit 5 - Forward Multicast
        Bit 6 - ARP proxy on the Remote Port
        Bit 7 - Ignore Don't frag bit. Default is disable. In default mode, if
              - the Don't frag bit is set, and packets needs to be fragmented 
              - then Ravlin will send ICMP back to the sender and drop the packet.
        Bit 8 - NAT packets matching bypass selector list.
		
        Bit 9 - SNMP access on Remote port in clear. Default is enabled. If 
                disabled then the ONLY way to access the device from the Remote 
                side is to manage it via a tunnel.
                Note - 0 Allow SNMP on remote in the clear.
                     - 1 Allow SNMP on remote thru. the tunnel.
                -- Enable/Disable is reversed for bit 9 to avoid hassles in backward
                -- compatibility
        Bit 10 - Enable/Disable PPPoE on Remote Port - Default is disable.
                Note - 1 Enable
                     - 0 Disable
 
        Bit 11 - Enable/Disable PPPoE connect on Demand - Default is disable.
                Note - 1 Enable
                     - 0 Disable

        Bit 29 - Enable/Disable listening to unsolicited ARP on local port.
                Default is disable.
                Note - 1 Enable
                     - 0 Disable

        Bit 30 - Enable/Disable cascading packets from one gateway to another.
                Default is disable.
                Note - 1 Enable
                     - 0 Disable

		The flags specifying behavior for bridge mode are ignored
		when the device is in router mode, and the flags specifying
		behavior for router mode are ignored when the device is in
		bridge mode."
    ::= { rc3Network 3 }

