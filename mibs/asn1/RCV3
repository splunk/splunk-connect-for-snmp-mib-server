-- Policy Database Entry

-- =======================
-- Peer Info Table
-- =======================

rc3PdePeerInfoTable OBJECT-TYPE
	SYNTAX SEQUENCE OF Rc3PdePeerInfoEntry
	ACCESS not-accessible
	STATUS mandatory
	DESCRIPTION
		"This table contains info. about the remote unit.
		Remote unit type determines the attributes that
		are required to be configured. Entries in this
		table can be added or deleted."
	::= { rc3Pde 1 }

rc3PdePeerInfoEntry OBJECT-TYPE
	SYNTAX Rc3PdePeerInfoEntry
	ACCESS not-accessible
	STATUS mandatory
	DESCRIPTION
		"A Remote unit info entry containing objects to
		identify remote unit type, DN, Key Mgmt method used,
		IPSEC protocol to be negotiated, and Next Hop if there
		is a need to OVERRIDE the routing table."
	INDEX { rc3PdePeerIndx }
	::= { rc3PdePeerInfoTable 1 }

Rc3PdePeerInfoEntry ::= SEQUENCE {
	rc3PdePeerIndx                       INTEGER,
	rc3PdePeerType                       INTEGER,
    rc3PdePeerAddr                       IpAddress,
	rc3PdePeerDN                         OCTET STRING,
	rc3PdePeerIssuerDN                   OCTET STRING,
	rc3PdePeerLocalCertIndx              INTEGER,
    rc3PdePeerKeyMgmtType                INTEGER,
    rc3PdePeerKeyMgmtIndx                INTEGER,
    rc3PdePeerIpsecProtocolIndx          INTEGER,
    rc3PdePeerIfIndex                    INTEGER,
    rc3PdePeerNextHop                    IpAddress,
    rc3PdePeerContinue                   INTEGER,
    rc3PdePeerIsakmpLifeType             INTEGER,
    rc3PdePeerIsakmpLifeTimeSeconds      INTEGER,
    rc3PdePeerIsakmpLifeTimeKiloBytes    INTEGER,
    rc3PdePeerIpsecLifeType              INTEGER,
    rc3PdePeerIpsecLifeTimeSeconds       INTEGER,
    rc3PdePeerIpsecLifeTimeKiloBytes     INTEGER,
	rc3PdePeerRowStatus                  RowStatus,
    rc3PdeFilterProtocol                 INTEGER,
    rc3PdeLocalPort                      INTEGER,
    rc3PdeRemotePort                     INTEGER,
    rc3PdeName                           DisplayString,
    rc3PdeULA                            INTEGER,
    rc3PdePeerLocalInterfaceIpAddr       IpAddress
	}
		
rc3PdePeerIndx OBJECT-TYPE
	SYNTAX INTEGER
	ACCESS not-accessible
--	ACCESS read-only
	STATUS mandatory
	DESCRIPTION
		"A unique numeric ID value for each Pde. This value
		is used to index into the table."
	::= { rc3PdePeerInfoEntry 1 }

rc3PdePeerType OBJECT-TYPE
	SYNTAX INTEGER {
		host(1),
		gateway(2),
		bypassboth(3),
		bypassoutbound(4)
	}
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
		"The semantics for the different remote unit type is
			host - need to specify DN, KeyMgmt, and IpsecProtocol (Addr optional)
			gateway - need to specify DN, KeyMgmt, and IpsecProtocol (Addr optional)
			bypassboth - need to specify nothing else in this table
			bypassoutbound - need to specify nothing else in this table"
	::= { rc3PdePeerInfoEntry 2 }

rc3PdePeerAddr OBJECT-TYPE
	SYNTAX IpAddress
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
		"IP Address of the remote Ravlin unit. This is used for hosts and
		gateways that have static IP addresses.  A value of 0.0.0.0 indicates
		that the peer is has a dynamic IP address."
	::= { rc3PdePeerInfoEntry 3 }

rc3PdePeerDN OBJECT-TYPE
	SYNTAX OCTET STRING
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
		"The Distinguished Name of the peer.  Can be a Security ID (SID),
		DER encoded DN,	or a DN filter."
	::= { rc3PdePeerInfoEntry 4 }

rc3PdePeerIssuerDN OBJECT-TYPE
	SYNTAX OCTET STRING
	ACCESS read-write
	STATUS deprecated
	DESCRIPTION
		"The Distinguished Name of the issuer that signed the peer's certificate.
		If the SID is being used, then the issuer DN should be null."
	::= { rc3PdePeerInfoEntry 5 }

rc3PdePeerLocalCertIndx OBJECT-TYPE
	SYNTAX INTEGER
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
		"This index corresponds to an entry in the local certificate table, which
		will determine which local certificate will be used to identify this device
		to the peer device."
	::= { rc3PdePeerInfoEntry 6 }

rc3PdePeerKeyMgmtType OBJECT-TYPE
	SYNTAX INTEGER {
		manual(1),
		isakmp(2)
    }
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
		"manual - use manual method to derive session keys.
		isakmp - use IETF protocol ISAKMP to derive session keys."
	::= { rc3PdePeerInfoEntry 7 }

rc3PdePeerKeyMgmtIndx OBJECT-TYPE
	SYNTAX INTEGER
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
		"This index corresponds to an entry in either the rc3PdeManualKeyMgmtTable,
		or an entry in the rc3PdeIsakmpKeyMgmtTable depending on the value of
		rc3PdePeerKeyMgmtType."
	::= { rc3PdePeerInfoEntry 8 }

rc3PdePeerIpsecProtocolIndx OBJECT-TYPE
	SYNTAX INTEGER
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
		"This index corresponds to an entry in the rc3PdeIpsecProtocolTable, use
		a value of zero for eip."
	::= { rc3PdePeerInfoEntry 9 }

rc3PdePeerIfIndex OBJECT-TYPE
    SYNTAX INTEGER {
		none(1),
--		local(2),
		remote(3)
    }
    ACCESS read-write
    STATUS mandatory
    DESCRIPTION
		"This indicates the interface to use in order to deliver
		the packet to the Remote unit.  If none is specified, then
		the routing table will be used to determine the next hop."
    ::= { rc3PdePeerInfoEntry 10 }

rc3PdePeerNextHop OBJECT-TYPE
    SYNTAX IpAddress
    ACCESS read-write
    STATUS mandatory
    DESCRIPTION
		"If non-zero, then this represents the next hop IP Address
		used by this Pde. If this value is zero, then use the next hop
		from the Routing table."
    ::= { rc3PdePeerInfoEntry 11 }

rc3PdePeerContinue OBJECT-TYPE
    SYNTAX INTEGER {
        enable(1),
        disable(2)
    }
    ACCESS read-write
    STATUS deprecated
    DESCRIPTION
		"Allow the packet to continue the Pde selection process when
		rc3StatConnStatus is failed or dropped."
    ::= { rc3PdePeerInfoEntry 12 }

rc3PdePeerIsakmpLifeType OBJECT-TYPE
	SYNTAX INTEGER{
		seconds(1)
--		kilobytes(2),
--		both(3)
	}
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
		"The type of key lifetime for ISAKMP phase one."
	::= { rc3PdePeerInfoEntry 13 }

rc3PdePeerIsakmpLifeTimeSeconds OBJECT-TYPE
	SYNTAX INTEGER
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
		"The lifetime in seconds for ISAKMP phase one."
	::= { rc3PdePeerInfoEntry 14 }

rc3PdePeerIsakmpLifeTimeKiloBytes OBJECT-TYPE
	SYNTAX INTEGER
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
		"The lifetime in kilobytes for ISAKMP phase one."
	::= { rc3PdePeerInfoEntry 15 }

rc3PdePeerIpsecLifeType OBJECT-TYPE
	SYNTAX INTEGER{
		seconds(1)
--		kilobytes(2),   - Not supported
--		both(3)         - Not Supported
	}
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
		"The type of key lifetime for IPsec."
	::= { rc3PdePeerInfoEntry 16 }

rc3PdePeerIpsecLifeTimeSeconds OBJECT-TYPE
	SYNTAX INTEGER
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
		"The lifetime in seconds for IPsec."
	::= { rc3PdePeerInfoEntry 17 }

rc3PdePeerIpsecLifeTimeKiloBytes OBJECT-TYPE
	SYNTAX INTEGER
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
		"The lifetime in kilobytes for IPsec."
	::= { rc3PdePeerInfoEntry 18 }

rc3PdePeerRowStatus OBJECT-TYPE
	SYNTAX RowStatus
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
		"This object is used to create/delete rows from the Policy Database
		Entry Peer Info Table. See the definition for RowStatus for a range
		of valid values used. On read status indicates ACTIVE."
	::= { rc3PdePeerInfoEntry 19 }

rc3PdeFilterProtocol OBJECT-TYPE
	SYNTAX INTEGER
	ACCESS read-write
	STATUS deprecated
	DESCRIPTION
		"Value specifying an associated IP protocol ID (e.g UDP/TCP).
        A value of zero means that the protocol ID field should be 
        ignored."
	::= { rc3PdePeerInfoEntry 20 }

rc3PdeLocalPort OBJECT-TYPE
	SYNTAX INTEGER
	ACCESS read-write
	STATUS deprecated
	DESCRIPTION
		"Value specifying an associated port. A value of zero means
        that the port field should be ignored."
	::= { rc3PdePeerInfoEntry 21 }

rc3PdeRemotePort OBJECT-TYPE
	SYNTAX INTEGER
	ACCESS read-write
	STATUS deprecated
	DESCRIPTION
		"Value specifying an associated port. A value of zero means
        that the port field should be ignored."
	::= { rc3PdePeerInfoEntry 22 }

rc3PdeName OBJECT-TYPE
    SYNTAX DisplayString (SIZE(1..15))
    ACCESS read-write
    STATUS mandatory
    DESCRIPTION
    "Name used to uniquely identify a PDE. Ravlin firmware will 
    assign a unique Default pdeName value for each PDE inserted,
    and can be changed."
	::= { rc3PdePeerInfoEntry 23 }

rc3PdeULA OBJECT-TYPE
	SYNTAX INTEGER{
        enable(1),
        disable(2)
	}
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
		"If enabled for a gateway PDE ONLY, then each user needs to be
         authenticated after SA is established. This bit has no meaning
         for any other type of PDE."
	::= { rc3PdePeerInfoEntry 24 }

rc3PdePeerLocalInterfaceIpAddr OBJECT-TYPE
	SYNTAX IpAddress
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
		"Specifies remote ravlin's local IP Address. A value of 0 means
        that this PDE does not participate in polling. "
	::= { rc3PdePeerInfoEntry 25 }

-- =======================
-- Local Selector table.
-- =======================

rc3PdeLocalNetworkTable OBJECT-TYPE
	SYNTAX SEQUENCE OF Rc3PdeLocalNetworkEntry
	ACCESS not-accessible
	STATUS mandatory
	DESCRIPTION
		"This table provides the selector list containing 1..n
		entry pair(s) of network number and mask for the local
		interface. Entries in this table can be added or deleted."
	::= { rc3Pde 2 }

rc3PdeLocalNetworkEntry OBJECT-TYPE
	SYNTAX Rc3PdeLocalNetworkEntry
	ACCESS not-accessible
	STATUS mandatory
	DESCRIPTION
		"An entry in the Local Network Selector Table. Rows are created
		by an SNMP SET request seeting the value of rc3PdeLocalNetworkRowStatus
		to 'createAndGo' or 'createAndWait'. Rows are deleted by an SNMP SET
		request setting the value of rc3PdeLocalNetworkRowStatus to 'destroy'."
	INDEX { rc3PdePeerIndx, rc3PdeLocalNetworkNumber, rc3PdeLocalNetworkMask }
	::= { rc3PdeLocalNetworkTable 1 }

Rc3PdeLocalNetworkEntry ::= SEQUENCE {
	rc3PdeLocalNetworkNumber		IpAddress,
	rc3PdeLocalNetworkMask		IpAddress,
	rc3PdeLocalNetworkRowStatus	RowStatus
	}
		
rc3PdeLocalNetworkNumber OBJECT-TYPE
	SYNTAX IpAddress
	ACCESS not-accessible
--	ACCESS read-only
	STATUS mandatory
	DESCRIPTION
		"The local network address that will be used to match packets for
		this entry in the Policy Database."
	::= { rc3PdeLocalNetworkEntry 1 }

rc3PdeLocalNetworkMask OBJECT-TYPE
	SYNTAX IpAddress
	ACCESS not-accessible
--	ACCESS read-only
	STATUS mandatory
	DESCRIPTION
		"The local network mask that will be used to match packets for
		this entry in the Policy Database."
	::= { rc3PdeLocalNetworkEntry 2 }

rc3PdeLocalNetworkRowStatus OBJECT-TYPE
	SYNTAX RowStatus
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
		"This object is used to create/delete rows from the Policy Database
		Entry Local Network Table. See the definition for RowStatus for a range
		of valid values used. On read status indicates ACTIVE."
	::= { rc3PdeLocalNetworkEntry 3 }

