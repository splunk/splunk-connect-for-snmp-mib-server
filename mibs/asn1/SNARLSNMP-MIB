			DESCRIPTION
				"The total transfer volume for this task mod (2 to power of 32)."
			::= { siteEntry 88 }
--
-- definitions
--

		groupStatus OBJECT-TYPE
			SYNTAX Integer32
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"The group status in variable bound to a trap."
			::= { snarl 100 }

		proxyStatus OBJECT-TYPE
			SYNTAX Integer32
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"The proxy status in variable bound to a trap."
			::= { snarl 101 }

		siteStatus OBJECT-TYPE
			SYNTAX Integer32
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"The site status in variable bound to a trap."
			::= { snarl 102 }

		subTaskID OBJECT-TYPE
			SYNTAX INTEGER
			{
			 general(1),
			 performance(2),
			 httpCode(3),
			 matchRegex(4),
			 matchSize(5),
			 urlSizeMatch(6),
			 urlContentMatch(7),
			 numericalMatch(8),
			 dateAgeMatch(9),
			 suspension(10),
			 warmStart(11),
			 coldStart(12),
                         heartBeat(13),
                         syncRequest(14)
			}
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"The subTaskID in variable bound to a trap."
			::= { snarl 103 }

		eventTime OBJECT-TYPE
			SYNTAX Integer32
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"The unix time in variable bound to a trap."
			::= { snarl 104 }
		

	    siteTrapMessage OBJECT-TYPE
			SYNTAX DisplayString
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"The status message of the trap."
			::= { snarl 105 }
		

--
-- Trap definitions
--
	
		snarlsnmpInfo TRAP-TYPE
			ENTERPRISE snarlsnmp
			VARIABLES { siteIndex, subTaskID, siteTrapMessage, groupStatus, siteName, siteID, siteProxy, eventTime, siteStatus, proxyStatus }
			DESCRIPTION
				"An snarlsnmpInfo trap signifies that the sending
				protocol entity has undergone minor run changes
				that usually require no special attention. The
				information given in this trap are mainly for
				logging purposes"
			::=  1
			
		snarlsnmpNormal TRAP-TYPE
			ENTERPRISE snarlsnmp
			VARIABLES { siteIndex, subTaskID, siteTrapMessage, groupStatus, siteName, siteID, siteProxy, eventTime, siteStatus, proxyStatus }
			DESCRIPTION
				"An snarlsnmpNormal trap signifies that the sending
				protocol entity is working properly"
			::=  2
			
		snarlsnmpWarning TRAP-TYPE
			ENTERPRISE snarlsnmp
			VARIABLES { siteIndex, subTaskID, siteTrapMessage, groupStatus, siteName, siteID, siteProxy, eventTime, siteStatus, proxyStatus }
			DESCRIPTION
				"An snarlsnmpWarning trap signifies that the sending
				protocol entity is impaired and requires attention"
			::=  3
			
		snarlsnmpCritical TRAP-TYPE
			ENTERPRISE snarlsnmp
			VARIABLES { siteIndex, subTaskID, siteTrapMessage, groupStatus, siteName, siteID, siteProxy, eventTime, siteStatus, proxyStatus }
			DESCRIPTION
				"An snarlsnmpCritical trap signifies that the sending
				protocol entity is having major problems and need
				immediate attention"
			::=  4
			
		snarlsnmpSuspended TRAP-TYPE
			ENTERPRISE snarlsnmp
			VARIABLES { siteIndex, subTaskID, siteTrapMessage, groupStatus, siteName, siteID, siteProxy, eventTime, siteStatus, proxyStatus }
			DESCRIPTION
				"An snarlsnmpSuspended signfies that the sending task is about to be suspended"
			::=  5
			
		snarlsnmpResumed TRAP-TYPE
			ENTERPRISE snarlsnmp
			VARIABLES { siteIndex, subTaskID, siteTrapMessage, groupStatus, siteName, siteID, siteProxy, eventTime, siteStatus, proxyStatus }
			DESCRIPTION
				"An snarlsnmpResumed trap signfies that the sending task is about to resume from suspension"
			::=  6
			
		snarlsnmpGenericInfo TRAP-TYPE
			ENTERPRISE snarlsnmp
			VARIABLES { siteIndex, subTaskID, siteTrapMessage, groupStatus, siteName, siteID, siteProxy, eventTime, siteStatus, proxyStatus }
			DESCRIPTION
				"An snarlsnmpGenericInfo trap signifies that the sending
				protocol entity sends logging information"
			::=  7
			
		snarlsnmpStateUnknown TRAP-TYPE
			ENTERPRISE snarlsnmp
			VARIABLES { siteIndex, subTaskID, siteTrapMessage, groupStatus, siteName, siteID, siteProxy, eventTime, siteStatus, proxyStatus }
			DESCRIPTION
				"An snarlsnmpStateUnknown trap signifies that the sending
				protocol entity is undefined state"
			::=  8	
	END

