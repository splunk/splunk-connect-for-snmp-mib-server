switchActiveAlarmTable		OBJECT-TYPE
			SYNTAX   SEQUENCE OF SwitchActiveAlarmEntry
			ACCESS	not-accessible
			STATUS	mandatory
			DESCRIPTION 
			""
			::= { alarmGrp	 2 }

switchActiveAlarmEntry		OBJECT-TYPE
			SYNTAX	SwitchActiveAlarmEntry
			ACCESS	not-accessible
			STATUS	mandatory
			DESCRIPTION 
			""
			INDEX { switchAlarmMsgId, switchAlarmInstNo }
			::= {	switchActiveAlarmTable   1 }

SwitchActiveAlarmEntry		::= SEQUENCE {
			switchAlarmMsgId		INTEGER,
			switchAlarmInstNo		INTEGER,
			switchAlarmTime		DisplayString,
			switchAlarmType		EventType,
			switchAlarmMainEvent		AlarmEvent,
			switchAlarmSubEvent		AlarmSubEvent,
			switchAlarmId		INTEGER,
			switchAlarmSeverity		AlarmSeverity,
			switchAlarmDesc		DisplayString,
			switchAlarmAck		ActiveAlarmAckStatus,
			switchAlarmRepeatTime		INTEGER
			}

switchAlarmMsgId		OBJECT-TYPE
			SYNTAX	INTEGER(1..65535)
			ACCESS	read-only
			STATUS	mandatory
			DESCRIPTION 
	"Alarm Message Id. Can be any integer from 1-65535. "
			::= {	switchActiveAlarmEntry   1 }

