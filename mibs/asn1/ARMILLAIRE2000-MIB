 -- *************************************************     
 -- Alarm Group
 -- *************************************************     

alarmTraps		OBJECT IDENTIFIER   ::=	{ alarmGrp	 1 }

 -- *************************************************     
 -- Alarm Traps Group
 -- *************************************************     

genswitchSnmpAgentReadyNotify		TRAP-TYPE
	ENTERPRISE	alarmTraps
	VARIABLES   { trapSeverity, trapOpc, trapSwitchName }
	DESCRIPTION 
	"Info : SNMP Agent is up "
	::=  1

genConsoleLoginNotify		TRAP-TYPE
	ENTERPRISE	alarmTraps
	VARIABLES   { trapSeverity, trapOpc, trapSwitchName, trapUserName }
	DESCRIPTION 
	"Info : A user has successfully logged in from console "
	::=  2

genConsoleLogoutNotify		TRAP-TYPE
	ENTERPRISE	alarmTraps
	VARIABLES   { trapSeverity, trapOpc, trapSwitchName, trapUserName }
	DESCRIPTION 
	"Info : A user has successfully logged out from console "
	::=  3

genProcessBufOverflowNotify		TRAP-TYPE
	ENTERPRISE	alarmTraps
	VARIABLES   { trapSeverity, trapOpc, trapSwitchName, trapProcName, trapProcInstNo, trapBucketSize }
	DESCRIPTION 
	"Info : Process ran out of buffer "
	::=  4

genProcessBufOverflowRecoverNotify		TRAP-TYPE
	ENTERPRISE	alarmTraps
	VARIABLES   { trapSeverity, trapOpc, trapSwitchName, trapProcName, trapProcInstNo, trapBucketSize }
	DESCRIPTION 
	"Info : Process Recovered from Buffer Overflow "
