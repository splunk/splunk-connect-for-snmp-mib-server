ss7LinkCfgTable		OBJECT-TYPE
			SYNTAX   SEQUENCE OF Ss7LinkCfgEntry
			ACCESS	not-accessible
			STATUS	mandatory
			DESCRIPTION 
	"SS7 Link Table. Information about the SS7 Link(s) in MSSC are 
	provided in this table. The index of this table is ss7LinkName (String) "
			::= { ss7Sig	 1 }

ss7LinkCfgEntry		OBJECT-TYPE
			SYNTAX	Ss7LinkCfgEntry
			ACCESS	not-accessible
			STATUS	mandatory
			DESCRIPTION 
	"The Current Entry Containes all the objects for this Table. 
	This object is not-accessible, but the user can access all 
	the objects in this table at the next level. "
			INDEX { ss7LinkName }
			::= {	ss7LinkCfgTable   1 }

Ss7LinkCfgEntry		::= SEQUENCE {
			ss7LinkName		NameString,
			ss7LinkId		INTEGER,
			ss7LinkTrunkId		INTEGER,
			ss7LinkChannel		INTEGER,
			ss7LinkSlc		INTEGER,
			ss7LinkSpeed		LinkType,
			ss7LinkMode		Standard,
			ss7LinkRowStatus		RowStatus,
			ss7LinkOpStatus		LinkOpStatus
			}

ss7LinkName		OBJECT-TYPE
			SYNTAX	NameString
			ACCESS	read-write
			STATUS	mandatory
			DESCRIPTION 
	"Link name. Can be any combination of alphanumeric characters. 
	Spaces are not allowed and cannot exceed 15 characters. "
			::= {	ss7LinkCfgEntry   1 }

ss7LinkId		OBJECT-TYPE
			SYNTAX	INTEGER(1..65535)
			ACCESS	read-write
			STATUS	mandatory
			DESCRIPTION 
	"User assigned Link id. Can be any integer 1-65535. "
			::= {	ss7LinkCfgEntry   2 }

