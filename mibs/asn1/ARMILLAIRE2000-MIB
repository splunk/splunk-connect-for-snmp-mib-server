measTrnkGrpTrafficEntry		OBJECT-TYPE
			SYNTAX	MeasTrnkGrpTrafficEntry
			ACCESS	not-accessible
			STATUS	mandatory
			DESCRIPTION 
	"The Current Entry Containes all the objects for this Table. 
	This object is not-accessible, but the user can access all 
	the objects in this table at the next level. "
			INDEX { measTrnkGrpName }
			::= {	measTrnkGrpTrafficTable   1 }

MeasTrnkGrpTrafficEntry		::= SEQUENCE {
			measTrnkGrpName		DisplayString,
			measTrnkGrpDataSuspect		INTEGER,
			measTrnkGrpincomingUsage		INTEGER,
			measTrnkGrpoutgoingUsage		INTEGER,
			measTrnkGrpISCicsChnls		INTEGER,
			measTrnkGrpOOSCicsChnls		INTEGER,
			measTrnkGrptotalUsage		INTEGER,
			measTrnkGrpmaintainanceUsage		INTEGER
			}

measTrnkGrpName		OBJECT-TYPE
			SYNTAX	DisplayString
			ACCESS	read-only
			STATUS	mandatory
			DESCRIPTION 
	"Trunk Group Name. Can be any combination of alphanumeric characters. 
	Spaces are not allowed and cannot exceed 15 characters. "
			::= {	measTrnkGrpTrafficEntry   1 }

measTrnkGrpDataSuspect		OBJECT-TYPE
			SYNTAX	INTEGER
			ACCESS	read-only
			STATUS	mandatory
			DESCRIPTION 
	"Trunk Group Data Suspect. Boolean value indicating if the measurement 
	data set is suspect. Suspect data is that which may or may not be accurate 
	and cannot be verified accurate due to some condition that occurred during 
	the collection interval. "
			::= {	measTrnkGrpTrafficEntry   2 }

