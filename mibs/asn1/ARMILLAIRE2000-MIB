 -- *************************************************     
 -- ISDN Trunk Config Table
 -- *************************************************     

isdnTrunkCfgTable		OBJECT-TYPE
			SYNTAX   SEQUENCE OF IsdnTrunkCfgEntry
			ACCESS	not-accessible
			STATUS	mandatory
			DESCRIPTION 
	"ISDN Trunk Table. Information about the ISDN Trunk(s) in MSSC are 
	provided in this table. The index of this table is isdnTrunkId (Integer) "
			::= { isdn	 2 }

isdnTrunkCfgEntry		OBJECT-TYPE
			SYNTAX	IsdnTrunkCfgEntry
			ACCESS	not-accessible
			STATUS	mandatory
			DESCRIPTION 
	"The Current Entry Containes all the objects for this Table. 
	This object is not-accessible, but the user can access all 
	the objects in this table at the next level. "
			INDEX { isdnTrunkId }
			::= {	isdnTrunkCfgTable   1 }

IsdnTrunkCfgEntry		::= SEQUENCE {
			isdnTrunkId		INTEGER,
			isdnTrunkName		NameString,
			isdnTrunkGrpId		INTEGER,
			isdnDchnlId		INTEGER,
			isdnTrunkType		IsdnType,
			isdnXconnectId		INTEGER,
			isdnSlotId		INTEGER,
			isdnPortId		INTEGER,
			isdnDirection		Direction,
			isdnPriority		IsdnPriority,
			isdnDurthreshold		INTEGER,
			isdnCntthreshold		INTEGER,
			isdnPhyIntfType		IfType,
			isdnDchnlTimeSlot		INTEGER,
			isdnE1MultiFrame		E1MultiFrame,
			isdnE1CRC4		E1CRC4,
			isdnE1TransClk		TransClkSrc,
			isdnDS1LineType		DS1LineType,
			isdnDS1LineCoding		DS1LineCoding,
			isdnDS1TransClkSrc		TransClkSrc,
			isdnDS1LBO		DS1LBO,
			isdnDS3LineType		DS3LineType,
			isdnDS3TransClkSrc		TransClkSrc,
			isdnDS3LBO		DS3LBO,
			isdnDS3DS1LineType		DS1LineType,
			isdnDS3DS1TransClkSrc		TransClkSrc,
			isdnTrunkRowStatus		RowStatus,
			isdnTrunkOpStatus		IsdnTrnkOpStatus
			}

isdnTrunkId		OBJECT-TYPE
			SYNTAX	INTEGER(1..65535)
			ACCESS	read-write
			STATUS	mandatory
			DESCRIPTION 
	"Trunk id. Can be any integer 1-65535. "
			::= {	isdnTrunkCfgEntry   1 }

