 -- *************************************************     

isdnMgmntGroup		OBJECT IDENTIFIER   ::=	{ isdn	 4 }

 -- *************************************************     
 -- Isdn Trunk management table
 -- *************************************************     

isdnTrnkMgmntTable		OBJECT-TYPE
			SYNTAX   SEQUENCE OF IsdnTrnkMgmntEntry
			ACCESS	not-accessible
			STATUS	mandatory
			DESCRIPTION 
	"ISDN Trunk Management Table. The Management Commands for ISDN Trunk(s) 
	in MSSC are provided in this table. This table has the index isdnTrnkMgmntId (Integer). "
			::= { isdnMgmntGroup	 1 }

isdnTrnkMgmntEntry		OBJECT-TYPE
			SYNTAX	IsdnTrnkMgmntEntry
			ACCESS	not-accessible
			STATUS	mandatory
			DESCRIPTION 
	"The Current Entry Containes all the objects for this Table. 
	This object is not-accessible, but the user can access all 
	the objects in this table at the next level. "
			INDEX { isdnTrnkMgmntId }
			::= {	isdnTrnkMgmntTable   1 }

IsdnTrnkMgmntEntry		::= SEQUENCE {
			isdnTrnkMgmntId		INTEGER,
			isdnTrnkMgmntCmd		IsdnTrnkMgtOperation
			}

isdnTrnkMgmntId		OBJECT-TYPE
			SYNTAX	INTEGER
			ACCESS	read-only
			STATUS	mandatory
			DESCRIPTION 
	"Trunk id. "
			::= {	isdnTrnkMgmntEntry   1 }

isdnTrnkMgmntCmd		OBJECT-TYPE
			SYNTAX	IsdnTrnkMgtOperation
			ACCESS	read-write
			STATUS	mandatory
			DESCRIPTION 
	"Management Command for Enabling/Disabling the specified 
	trunk group. This command will make all the channels in this 
	trunk available 
	The possible options are : 
	1) Enable 
	2) Disable [default] "
			::= {	isdnTrnkMgmntEntry   2 }

