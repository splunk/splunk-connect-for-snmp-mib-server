 -- *************************************************     
 -- MFD Managment table
 -- *************************************************     

mfdMgmntTable		OBJECT-TYPE
			SYNTAX   SEQUENCE OF MfdMgmntEntry
			ACCESS	not-accessible
			STATUS	mandatory
			DESCRIPTION 
	"Modem Fault Management Table. The Management Commands for Modem(s) in MSSC 
	are provided in this table. This table has the index isdnTnkMgmntId (Integer). "
			::= { isdnMgmntGroup	 3 }

mfdMgmntEntry		OBJECT-TYPE
			SYNTAX	MfdMgmntEntry
			ACCESS	not-accessible
			STATUS	mandatory
			DESCRIPTION 
	"The Current Entry Containes all the objects for this Table. 
	This object is not-accessible, but the user can access all 
	the objects in this table at the next level. "
			INDEX { isdnTnkMgmntId }
			::= {	mfdMgmntTable   1 }

MfdMgmntEntry		::= SEQUENCE {
			isdnTnkMgmntId		INTEGER,
			isdnDurThreshold		INTEGER,
			isdnCntThreshold		INTEGER,
			mfdMgmntCmd		EnableOperation
			}

isdnTnkMgmntId		OBJECT-TYPE
			SYNTAX	INTEGER
			ACCESS	read-only
			STATUS	mandatory
			DESCRIPTION 
	"Isdn Trunk id. "
			::= {	mfdMgmntEntry   1 }

isdnDurThreshold		OBJECT-TYPE
			SYNTAX	INTEGER(1..65535)
			ACCESS	read-write
			STATUS	mandatory
			DESCRIPTION 
	"Bad Call Duration Threshold. 
	A value 0 means disable. 
	If mfd is enabled the normal value for this field would be 3 "
			::= {	mfdMgmntEntry   2 }

isdnCntThreshold		OBJECT-TYPE
			SYNTAX	INTEGER(1..65535)
			ACCESS	read-write
			STATUS	mandatory
			DESCRIPTION 
	"Bad Call Count Threshold 
	A value 0 means disable. 
	If mfd is enabled the normal value for this field would be 3 "
			::= {	mfdMgmntEntry   3 }

mfdMgmntCmd		OBJECT-TYPE
			SYNTAX	EnableOperation
			ACCESS	read-write
			STATUS	mandatory
			DESCRIPTION 
	"Management Command for Enabling/Disabling 'MODEM FAULT DETECTION' feature in MSSC. 
	The possible options are : 
	1) Enable 
	2) Disable [default] "
			::= {	mfdMgmntEntry   4 }

 -- *************************************************     
 -- Isdn Physical Interface Configuration Table
 -- *************************************************     

 -- *************************************************     
 -- Route Config Table
 -- *************************************************     

routeTable		OBJECT-TYPE
			SYNTAX   SEQUENCE OF RouteEntry
			ACCESS	not-accessible
			STATUS	mandatory
			DESCRIPTION 
	"Route Table. Information about the Route(s) in MSSC are 
	provided in this table. The index of this table is routeId (Integer) "
			::= { rt	 1 }

routeEntry		OBJECT-TYPE
			SYNTAX	RouteEntry
			ACCESS	not-accessible
			STATUS	mandatory
			DESCRIPTION 
	"The Current Entry Containes all the objects for this Table. 
	This object is not-accessible, but the user can access all 
	the objects in this table at the next level. "
			INDEX { routeId }
			::= {	routeTable   1 }

RouteEntry		::= SEQUENCE {
			routeId		INTEGER,
			routeAddrType		AddrType,
			routeAdrId		AddrIdentifier,
			routeAddr		AddrString,
			routeNumPlan		NumPlan,
			routeNumOfDigit		INTEGER,
			routeRouteName		NameString,
			routeRouteType		RouteType,
			routeRowStatus		RowStatus,
			routeOpStatus		OpStatus
			}

routeId		OBJECT-TYPE
			SYNTAX	INTEGER(1..65535)
			ACCESS	read-write
			STATUS	mandatory
			DESCRIPTION 
	"Route Group id. Can be any integer 1-65535. "
			::= {	routeEntry   1 }

routeAddrType		OBJECT-TYPE
			SYNTAX	AddrType
			ACCESS	read-write
			STATUS	mandatory
			DESCRIPTION 
	"Call Control routing type 
	The possible option are: 
	1) Call party number 
	2) Transit Network Selection  "
			::= {	routeEntry   2 }

routeAdrId		OBJECT-TYPE
			SYNTAX	AddrIdentifier
			ACCESS	read-write
			STATUS	mandatory
			DESCRIPTION 
	"Call Control route request nature of address. 
	The possible options are: 
	1) National number 
	2) International number 
	3) Unknown  "
			::= {	routeEntry   3 }

routeAddr		OBJECT-TYPE
			SYNTAX	AddrString
			ACCESS	read-write
			STATUS	mandatory
			DESCRIPTION 
	"Call Control routing number "
			::= {	routeEntry   4 }

routeNumPlan		OBJECT-TYPE
			SYNTAX	NumPlan
			ACCESS	read-write
			STATUS	mandatory
			DESCRIPTION 
	"Call control route request numbering plan. 
	The possible options are: 
	1) Number not present(nonumber) 
	2) ISDN numbering Plan(isdnnumplan) 
	3) Telephony numbering plan(telephonynumplan) 
	4) Private Numbering Plan(privatenumplan) "
			::= {	routeEntry   5 }

routeNumOfDigit		OBJECT-TYPE
			SYNTAX	INTEGER(1..65535)
			ACCESS	read-write
			STATUS	mandatory
			DESCRIPTION 
	"Number of Digit in the route. "
			::= {	routeEntry   6 }

routeRouteName		OBJECT-TYPE
			SYNTAX	NameString
			ACCESS	read-write
			STATUS	mandatory
			DESCRIPTION 
	"Route Name. Can be any combination of alphanumeric characters. 
	Spaces are not allowed.  "
			::= {	routeEntry   7 }

routeRouteType		OBJECT-TYPE
			SYNTAX	RouteType
			ACCESS	read-write
			STATUS	mandatory
			DESCRIPTION 
	"Route type: SS7/ISDN "
			::= {	routeEntry   8 }

routeRowStatus		OBJECT-TYPE
			SYNTAX	RowStatus
			ACCESS	read-write
			STATUS	mandatory
			DESCRIPTION 
	"Current Status (configurational Status) of this ROUTE. 
	To Configure this Route, the only option is 'CREATEANDWAIT' 
	To Delete this Route, the only option is 'DESTROY' 
	For the 'GET' Operation, the possible options are 
	1) active 
	2) notinservice 
	3) notr"
			::= {	routeEntry   9 }

routeOpStatus		OBJECT-TYPE
			SYNTAX	OpStatus
			ACCESS	read-only
			STATUS	mandatory
			DESCRIPTION 
	"Current Status (Operational Status) of this Route. 
	'SET' Operation is NOT APPLICABLE. 
	For the 'GET' Operation, the possible options are 
	1) active 
	2) down  "
			::= {	routeEntry   10 }

 -- *************************************************     
 -- CC Route ISDN Configuration Table
