 -- *************************************************     
 -- SS7 circuit config table
 -- *************************************************     

ss7CktCfgTable		OBJECT-TYPE
			SYNTAX   SEQUENCE OF Ss7CktCfgEntry
			ACCESS	not-accessible
			STATUS	mandatory
			DESCRIPTION 
	"SS7 Circuit Table. The Information about SS7 Circuit(s) 
	in MSSC are provided in this table. 
	This table has two indices: ss7CktTrunkId and ss7CktChnlId (both Integer). "
			::= { ss7Br	 2 }

ss7CktCfgEntry		OBJECT-TYPE
			SYNTAX	Ss7CktCfgEntry
			ACCESS	not-accessible
			STATUS	mandatory
			DESCRIPTION 
	"The Current Entry Containes all the objects for this Table. 
	This object is not-accessible, but the user can access all 
	the objects in this table at the next level. "
			INDEX { ss7CktTrunkId, ss7CktChnlId }
			::= {	ss7CktCfgTable   1 }

Ss7CktCfgEntry		::= SEQUENCE {
			ss7CktTrunkId		INTEGER,
			ss7CktChnlId		INTEGER,
			ss7CktId		INTEGER,
			ss7CktDpc		INTEGER,
			ss7CktCic		INTEGER,
			ss7CktDir		Direction,
			ss7CktPriority		INTEGER,
			ss7CktT3		INTEGER,
			ss7CktT12		INTEGER,
			ss7CktT13		INTEGER,
			ss7CktT14		INTEGER,
			ss7CktT15		INTEGER,
			ss7CktT16		INTEGER,
			ss7CktT17		INTEGER,
			ss7CktTVal		INTEGER,
			ss7CktRowStatus		CktRowStatus,
			ss7CktOpStatus		CktOpStatus
			}

ss7CktTrunkId		OBJECT-TYPE
			SYNTAX	INTEGER(1..65535)
			ACCESS	read-write
			STATUS	mandatory
			DESCRIPTION 
	"User Assigned Trunk id.  "
			::= {	ss7CktCfgEntry   1 }

ss7CktChnlId		OBJECT-TYPE
			SYNTAX	INTEGER(1..65535)
			ACCESS	read-write
			STATUS	mandatory
			DESCRIPTION 
	"User Assigned Channel id "
			::= {	ss7CktCfgEntry   2 }

ss7CktId		OBJECT-TYPE
			SYNTAX	INTEGER(1..4294967295)
			ACCESS	read-only
			STATUS	mandatory
			DESCRIPTION 
	"Circuit id. The Id assingned to each of the circuits for a particular Trunk.  "
			::= {	ss7CktCfgEntry   3 }

ss7CktDpc		OBJECT-TYPE
			SYNTAX	INTEGER(1..16777215)
			ACCESS	read-write
			STATUS	mandatory
			DESCRIPTION 
	"Destination Point Code (The Integer Value). 
	The Point Code should be converted into its Integer 
	equivalent. For Example: 
	0-1-1 is converted as 257 (for ANSI) 
	0-1-2 is converted as 10 (for ITU) "
			::= {	ss7CktCfgEntry   4 }

ss7CktCic		OBJECT-TYPE
			SYNTAX	INTEGER(1..16383)
			ACCESS	read-write
			STATUS	mandatory
			DESCRIPTION 
	"Circuit identifier code. Can be any integer from 
	1) 1-4095 for ITU 
