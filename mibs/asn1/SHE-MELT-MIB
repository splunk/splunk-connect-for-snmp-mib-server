-- -----------------------------------------------------------------
--       Single End Loop Test Notifications for ADSL
-- -----------------------------------------------------------------
-- 
		sheMeltNotifications OBJECT IDENTIFIER ::= { sheMelt 10 }

		
		sheMeltTestRequestParametersModified NOTIFICATION-TYPE
			OBJECTS { sheMeltTestRequestControlReqTimeStamp, sheMeltTestParameterFreq1 }
			STATUS current
			DESCRIPTION 
				"The MELT testparameters are modified."
			::= { sheMeltNotifications 1 }

		
		sheMeltTestRequestStateModified NOTIFICATION-TYPE
			OBJECTS { sheMeltTestRequestControlReqTimeStamp, sheMeltTestRequestIfState, sheMeltTestRequestIuState }
			STATUS current
			DESCRIPTION 
				"The MELT state of an interface was modified by command or by NE."
			::= { sheMeltNotifications 2 }

		
		sheMeltTestRequestIfTestToneIndexModified NOTIFICATION-TYPE
			OBJECTS { sheMeltTestRequestControlReqTimeStamp, sheMeltTestRequestTestToneIndex }
			STATUS current
			DESCRIPTION 
				"The used test tone set of an interface (assignment  interface - test tone index) was modified."
			::= { sheMeltNotifications 3 }

		
		sheMeltTestToneEntryCreated NOTIFICATION-TYPE
			OBJECTS { sheMeltTestToneControlIndex, sheMeltTestToneControlReqTimeStamp, sheMeltTestToneControlFreq1, sheMeltTestToneControlFreq2, sheMeltTestToneControlLevel
				 }
			STATUS current
			DESCRIPTION 
				"Configuration trap for new created MELT testTone set."
			::= { sheMeltNotifications 4 }

		
		sheMeltTestToneEntryModified NOTIFICATION-TYPE
			OBJECTS { sheMeltTestToneControlIndex, sheMeltTestToneControlReqTimeStamp, sheMeltTestToneControlFreq1, sheMeltTestToneControlFreq2, sheMeltTestToneControlLevel
				 }
			STATUS current
			DESCRIPTION 
				"Configuration trap for modified MELT testTone set."
			::= { sheMeltNotifications 5 }

		
		sheMeltTestToneEntryDeleted NOTIFICATION-TYPE
			OBJECTS { sheMeltTestToneControlIndex, sheMeltTestToneControlReqTimeStamp }
			STATUS current
			DESCRIPTION 
				"Configuration trap for delete a MELT testTone set."
			::= { sheMeltNotifications 6 }

		
		sheMeltConformance OBJECT IDENTIFIER ::= { sheMelt 100 }

		
		sheMeltCompliances OBJECT IDENTIFIER ::= { sheMeltConformance 1 }

		
		sheMeltMibCompliance MODULE-COMPLIANCE
			STATUS current
			DESCRIPTION 
				"Description."
			MODULE -- this module
				MANDATORY-GROUPS { sheMeltGroup, sheMeltControlGroup, sheMeltNotificationGroup }
			::= { sheMeltCompliances 1 }

		
		sheMeltGroups OBJECT IDENTIFIER ::= { sheMeltConformance 2 }

		
