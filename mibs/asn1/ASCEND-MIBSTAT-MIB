	    e3AtmLineStatus-PhysicalAddress-Shelf
		INTEGER,
	    e3AtmLineStatus-PhysicalAddress-Slot
		INTEGER,
	    e3AtmLineStatus-PhysicalAddress-ItemNumber
		INTEGER,
	    e3AtmLineStatus-LineState
		INTEGER,
	    e3AtmLineStatus-SparePhysicalAddress-Shelf
		INTEGER,
	    e3AtmLineStatus-SparePhysicalAddress-Slot
		INTEGER,
	    e3AtmLineStatus-SparePhysicalAddress-ItemNumber
		INTEGER,
	    e3AtmLineStatus-SparingState
		INTEGER,
	    e3AtmLineStatus-SparingChangeReason
		INTEGER,
	    e3AtmLineStatus-SparingChangeTime
		INTEGER,
	    e3AtmLineStatus-SparingChangeCounter
		INTEGER,
	    e3AtmLineStatus-VpiVciRange
		INTEGER,
	    e3AtmLineStatus-VcSwitchingVpi
		DisplayString,
