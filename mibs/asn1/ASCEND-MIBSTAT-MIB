    mibe3AtmLineStatusTable	OBJECT-TYPE
	SYNTAX		SEQUENCE OF Mibe3AtmLineStatusEntry
	ACCESS		not-accessible
	STATUS		mandatory
	DESCRIPTION	"A list of mibe3AtmLineStatus profile entries."
	::= { mibe3AtmLineStatus 1 }

    mibe3AtmLineStatusEntry	OBJECT-TYPE
	SYNTAX		Mibe3AtmLineStatusEntry
	ACCESS		not-accessible
	STATUS		mandatory
	DESCRIPTION	"A mibe3AtmLineStatus entry containing objects that
			 maps to the parameters of mibe3AtmLineStatus profile."
	INDEX		{ e3AtmLineStatus-Shelf-o, e3AtmLineStatus-Slot-o, e3AtmLineStatus-Item-o }
	::= { mibe3AtmLineStatusTable 1 }

