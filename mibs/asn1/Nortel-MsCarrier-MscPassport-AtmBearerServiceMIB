   mscAtmIfVptVccMnrpRowStatus
      RowStatus,
   mscAtmIfVptVccMnrpComponentName
      DisplayString,
   mscAtmIfVptVccMnrpStorageType
      StorageType,
   mscAtmIfVptVccMnrpIndex
      NonReplicated }

