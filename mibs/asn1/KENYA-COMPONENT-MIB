KENYA-COMPONENT-MIB DEFINITIONS ::= BEGIN

IMPORTS
	OBJECT-TYPE FROM RFC1155-SMI
	vlbComponents
		FROM VOLUBILL-ROOT-MIB
	Integer32, IpAddress FROM SNMPv2-SMI;


bayServer OBJECT IDENTIFIER ::= { vlbComponents 1 }


isLms OBJECT-TYPE
	SYNTAX INTEGER
	ACCESS read-only
	STATUS mandatory
	::= { bayServer 1 }

isProxy OBJECT-TYPE
	SYNTAX INTEGER
	ACCESS read-only
	STATUS mandatory
	::= { bayServer 2 }

isBilling OBJECT-TYPE
	SYNTAX INTEGER
	ACCESS read-only
	STATUS mandatory
	::= { bayServer 3 }




lmsUnit  OBJECT IDENTIFIER ::= { bayServer 4 }

lmsUnitGlobalStatus OBJECT-TYPE
	SYNTAX INTEGER
	{
		unmanaged(0),
		green(1),
		yellow(2),
		orange(3),
		red(4)
	}
	ACCESS read-only
	STATUS mandatory
	::= { lmsUnit 1 }

lmsUnitActivation  OBJECT-TYPE
	SYNTAX INTEGER
	{
		start(1),
		stop(0),
		setonly(2)
	}
	ACCESS read-write
	STATUS mandatory
	::= { lmsUnit 2 }


