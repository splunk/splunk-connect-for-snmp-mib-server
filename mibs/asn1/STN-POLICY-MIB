-- MIB Object definitions for Spring Tide Network Policy Manager


STN-POLICY-MIB DEFINITIONS ::= BEGIN

    IMPORTS
	TEXTUAL-CONVENTION FROM SNMPv2-TC
        NOTIFICATION-TYPE, MODULE-IDENTITY,
        OBJECT-TYPE,
        IpAddress,
        Integer32,
        Unsigned32, 
        Counter32,
        Counter64,
        Gauge32
                FROM SNMPv2-SMI

        RowStatus,
        TimeStamp,
        TruthValue,
        DisplayString
                FROM SNMPv2-TC

        MODULE-COMPLIANCE 
                FROM SNMPv2-CONF

        InterfaceIndex
                FROM IF-MIB

        stnSystems
                FROM SPRING-TIDE-NETWORKS-SMI

        stnRouterIndex
                FROM STN-ROUTER-MIB;


    stnPm MODULE-IDENTITY
        LAST-UPDATED    "0012060000Z"
        ORGANIZATION    "Spring Tide Networks, Inc."
        CONTACT-INFO
                "
                Spring Tide Networks, Inc.
                Customer Service
                Postal: 3 Clock Tower Place
                        Maynard, MA 01754
                Tel:    1 888-786-4357
                Email:  stncs@springtidenet.com
                "
        DESCRIPTION     "Added stnPmClassTable"
        REVISION        "0005120000Z"
        DESCRIPTION
                "This MIB module describes managed objects of
                Spring Tide Networks policy module ."
        ::= { stnSystems 11 }


    --
    -- Top Level STN PM MIB Objects
    --
    stnPmObjects                OBJECT IDENTIFIER ::= { stnPm 1 }
    stnPmMibConformance         OBJECT IDENTIFIER ::= { stnPm 2 }

    stnPmService                OBJECT IDENTIFIER ::= { stnPmObjects 1 }
    stnPmPolicy                 OBJECT IDENTIFIER ::= { stnPmObjects 2 }
    stnPmPreference             OBJECT IDENTIFIER ::= { stnPmObjects 3 }
    stnPmIPsec                  OBJECT IDENTIFIER ::= { stnPmObjects 4 }
    stnPmEncaps                 OBJECT IDENTIFIER ::= { stnPmObjects 5 }
    stnPmManualSa               OBJECT IDENTIFIER ::= { stnPmObjects 6 }
    stnPmMarker                 OBJECT IDENTIFIER ::= { stnPmObjects 7 }
    stnPmProfile                OBJECT IDENTIFIER ::= { stnPmObjects 8 }
    stnPmQueue                  OBJECT IDENTIFIER ::= { stnPmObjects 9 }

    stnPmTraps                  OBJECT IDENTIFIER ::= { stnPmObjects 10 }
    stnPmFirewall               OBJECT IDENTIFIER ::= { stnPmObjects 11 }
    stnPmFirewallAction         OBJECT IDENTIFIER ::= { stnPmObjects 12 }
    stnPmServiceList            OBJECT IDENTIFIER ::= { stnPmObjects 13 }
    stnPmSLService              OBJECT IDENTIFIER ::= { stnPmObjects 14 }
    stnPmClass                  OBJECT IDENTIFIER ::= { stnPmObjects 15 }
    stnPMProxyTunnel            OBJECT IDENTIFIER ::= { stnPmObjects 16 }

    stnPmFirewallTrap           OBJECT IDENTIFIER ::= { stnPmTraps 1 }
    stnPmQosTrap                OBJECT IDENTIFIER ::= { stnPmTraps 2 }

    --
    -- Top Level STN PM Firewall Trap MIB Objects
    --
    stnPmFirewallTrapVars               OBJECT IDENTIFIER ::= { stnPmFirewallTrap 1 }
    stnPmFirewallNotificationPrefix     OBJECT IDENTIFIER ::= { stnPmFirewallTrap 2 }
    stnPmFirewallNotification           OBJECT IDENTIFIER ::= { stnPmFirewallNotificationPrefix 0 }

    --
    -- Top Level STN PM QoS Trap MIB Objects
    --
    stnPmQosTrapVars               OBJECT IDENTIFIER ::= { stnPmQosTrap 1 }
    stnPmQosNotificationPrefix     OBJECT IDENTIFIER ::= { stnPmQosTrap 2 }
    stnPmQosNotification           OBJECT IDENTIFIER ::= { stnPmQosNotificationPrefix 0 }

    -- Additional STN PM MIB Textual Conventions
    StnPmPolicyMatchType ::= TEXTUAL-CONVENTION
        STATUS      current
        DESCRIPTION "Types of match criteria for policy"
        SYNTAX         INTEGER {
                           any(1),
                           single(2),
                           range(3),
                           dynamic(4),
                           subnet(5)
                       }

    StnPmSelectorType ::= TEXTUAL-CONVENTION
        STATUS         current
        DESCRIPTION    "Selector type to use for connection"
        SYNTAX         INTEGER {
                            policy(1),
                            packet(2)
                        }

    StnPmAuthAlg ::= TEXTUAL-CONVENTION
        STATUS          current
        DESCRIPTION     "Types of authentication algorithms"
        SYNTAX          INTEGER {
                            none(1),
                            hmac-md5(2),
                            hmac-sha(3)
                        }

    StnPmEncrAlg ::= TEXTUAL-CONVENTION
        STATUS          current
        DESCRIPTION     "Types of encryption algorithms"
        SYNTAX          INTEGER {
                            none(1),
                            des(2),
                            des3(3)
                        }

    StnPmDirection ::= TEXTUAL-CONVENTION
        STATUS          current
        DESCRIPTION     "Types of directions"
        SYNTAX          INTEGER {
                            inbound(1),
                            outbound(2)
                        }

    StnPmBitRate ::= TEXTUAL-CONVENTION
        STATUS          current
        DESCRIPTION     "A data rate in bits/second."
        SYNTAX          Integer32 (0..'7FFFFFFF'h)

    --
    -- STN PM service table
    --

    stnPmServiceTable OBJECT-TYPE
        SYNTAX        SEQUENCE OF StnPmServiceEntry
        MAX-ACCESS    not-accessible
        STATUS        current
        DESCRIPTION   "A list of PM service entries."
        ::= { stnPmService 1 }
 
    stnPmServiceEntry OBJECT-TYPE
        SYNTAX          StnPmServiceEntry
        MAX-ACCESS      not-accessible
        STATUS          current
        DESCRIPTION     "Entry contains information about a particular 
                        policy manager service."  
        INDEX           { stnPmServiceIndex }
        ::= { stnPmServiceTable 1 }
 
    StnPmServiceEntry ::= 
        SEQUENCE {
                 stnPmServiceIndex              Integer32,
                 stnPmServiceName               DisplayString,
                 stnPmServiceIdleTimeout        Integer32,
                 stnPmServiceNumPolicies        Integer32,
                 stnPmServiceEnabled            TruthValue,
                 stnPmServiceType               INTEGER,
                 stnPmServiceFirewallID         Integer32
        }       

    stnPmServiceIndex OBJECT-TYPE
        SYNTAX          Integer32
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION     "Index into service table"
        ::= { stnPmServiceEntry 1 }

    stnPmServiceName OBJECT-TYPE
        SYNTAX          DisplayString (SIZE(0..65))
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION     "Name of service"
        ::= { stnPmServiceEntry 2 }

    stnPmServiceIdleTimeout OBJECT-TYPE
        SYNTAX          Integer32
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION     "Idle timeout for service"
        ::= { stnPmServiceEntry 3 }

    stnPmServiceNumPolicies OBJECT-TYPE
        SYNTAX          Integer32
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION     "Number of policies in this service"
        ::= { stnPmServiceEntry 4 }

    stnPmServiceEnabled OBJECT-TYPE
        SYNTAX          TruthValue
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION     "Flag to indicate if service is enabled"
        ::= { stnPmServiceEntry 5 }

    stnPmServiceType OBJECT-TYPE
        SYNTAX          INTEGER {
                            vpn(1),
                            firewall(2),
                            ip-fileter(3),
                            forwarding(4),
                            qos(5),
                            mpls(6)
                        }
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION     "Type of service"
        ::= { stnPmServiceEntry 7 }

    stnPmServiceFirewallID OBJECT-TYPE
        SYNTAX          Integer32
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION     "Instance of service's firewall record"
        ::= { stnPmServiceEntry 8 }


    --
    -- STN PM Policy Table
    --

    stnPmPolicyTable OBJECT-TYPE
        SYNTAX        SEQUENCE OF StnPmPolicyEntry
        MAX-ACCESS    not-accessible
        STATUS        current
        DESCRIPTION   "A list of policy entries."
        ::= { stnPmPolicy 1 }
 
    stnPmPolicyEntry OBJECT-TYPE
        SYNTAX          StnPmPolicyEntry
        MAX-ACCESS      not-accessible
        STATUS          current
        DESCRIPTION     "Entry contains information about a particular 
                        policy."
        INDEX           { stnPmPolicyServiceIndex, 
                          stnPmPolicyIndex }
        ::= { stnPmPolicyTable 1 }
 
    StnPmPolicyEntry ::= 
        SEQUENCE {
                stnPmPolicyServiceIndex             Integer32,
                stnPmPolicyIndex                    Integer32,
                stnPmPolicyName                     DisplayString,
                stnPmPolicySrcAddrMatchType         StnPmPolicyMatchType,
                stnPmPolicySrcAddr                  IpAddress,
                stnPmPolicySrcMask                  IpAddress,
                stnPmPolicySrcEndAddr               IpAddress,
                stnPmPolicyDestAddrMatchType        StnPmPolicyMatchType,
                stnPmPolicyDestAddr                 IpAddress,
                stnPmPolicyDestMask                 IpAddress,
                stnPmPolicyDestEndAddr              IpAddress,
                stnPmPolicySrcPortMatchType         StnPmPolicyMatchType,
                stnPmPolicySrcPort                  Integer32,
                stnPmPolicySrcEndPort               Integer32,
                stnPmPolicyDestPortMatchType        StnPmPolicyMatchType,
                stnPmPolicyDestPort                 Integer32,
                stnPmPolicyDestEndPort              Integer32,
                stnPmPolicyProtocolMatchType        StnPmPolicyMatchType,
                stnPmPolicyProtocol                 INTEGER,
                stnPmPolicyAction                   INTEGER,
                stnPmPolicyActionID                 Integer32,
                stnPmPolicyDirection                StnPmDirection,
                stnPmPolicyCreateMirror             TruthValue,
                stnPmPolicySrcAddrSelector          StnPmSelectorType,
                stnPmPolicyDestAddrSelector         StnPmSelectorType,
                stnPmPolicyEnabled                  TruthValue,
                stnPmPolicyMatches                  Integer32,
                stnPmPolicyMisses                   Integer32,
                stnPmPolicyMarkedOctets             Counter64,
                stnPmPolicyMarkedPkts               Counter64,
                stnPmPolicyIcmpTypes                Unsigned32,
                stnPmPolicyTosByteMatchType         StnPmPolicyMatchType,
                stnPmPolicyTosByteValue             INTEGER,
                stnPmPolicyTosByteMask              INTEGER,
                stnPmPolicyMarkerID                 Integer32,
                stnPmPolicyTxclassID                Integer32
        }


    stnPmPolicyServiceIndex OBJECT-TYPE
        SYNTAX          Integer32
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION     "Service index of policy"
        ::= { stnPmPolicyEntry 1 }

    stnPmPolicyIndex OBJECT-TYPE
        SYNTAX          Integer32
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION     "Policy index of policy"
        ::= { stnPmPolicyEntry 2 }

    stnPmPolicyName OBJECT-TYPE
        SYNTAX          DisplayString (SIZE(0..65))
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION     "Policy name"
        ::= { stnPmPolicyEntry 3 }

    stnPmPolicySrcAddrMatchType OBJECT-TYPE
        SYNTAX          StnPmPolicyMatchType
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION     "Type of match for source address"
        ::= { stnPmPolicyEntry 4 }

    stnPmPolicySrcAddr OBJECT-TYPE
        SYNTAX          IpAddress
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION     "Source address to match"
        ::= { stnPmPolicyEntry 5 }

    stnPmPolicySrcMask OBJECT-TYPE
        SYNTAX          IpAddress
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION     "Source IP netmask to match"
        ::= { stnPmPolicyEntry 6 }

    stnPmPolicySrcEndAddr OBJECT-TYPE
        SYNTAX          IpAddress
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION     "End of source address range to match"
        ::= { stnPmPolicyEntry 7 }

    stnPmPolicyDestAddrMatchType OBJECT-TYPE
        SYNTAX          StnPmPolicyMatchType
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION     "Type of match for source address"
        ::= { stnPmPolicyEntry 8 }

    stnPmPolicyDestAddr OBJECT-TYPE
        SYNTAX          IpAddress
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION     "Source address to match"
        ::= { stnPmPolicyEntry 9 }

    stnPmPolicyDestMask OBJECT-TYPE
        SYNTAX          IpAddress
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION     "Source IP netmask to match"
        ::= { stnPmPolicyEntry 10 }

    stnPmPolicyDestEndAddr OBJECT-TYPE
        SYNTAX          IpAddress
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION     "End of source address range to match"
        ::= { stnPmPolicyEntry 11 }

    stnPmPolicySrcPortMatchType OBJECT-TYPE
        SYNTAX          StnPmPolicyMatchType
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION     "Type of match for source port"
        ::= { stnPmPolicyEntry 12 }

    stnPmPolicySrcPort OBJECT-TYPE
        SYNTAX          Integer32
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION     "Source port to match"
        ::= { stnPmPolicyEntry 13 }

    stnPmPolicySrcEndPort OBJECT-TYPE
        SYNTAX          Integer32
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION     "End of source port range to match"
        ::= { stnPmPolicyEntry 14 }

    stnPmPolicyDestPortMatchType OBJECT-TYPE
        SYNTAX          StnPmPolicyMatchType
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION     "Type of match for destination port"
        ::= { stnPmPolicyEntry 15 }

    stnPmPolicyDestPort OBJECT-TYPE
        SYNTAX          Integer32
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION     "Destination port to match"
        ::= { stnPmPolicyEntry 16 }

    stnPmPolicyDestEndPort OBJECT-TYPE
        SYNTAX          Integer32
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION     "End of destination port range to match"
        ::= { stnPmPolicyEntry 17 }

    stnPmPolicyProtocolMatchType OBJECT-TYPE
        SYNTAX          StnPmPolicyMatchType
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION     "Type of match for protocol"
        ::= { stnPmPolicyEntry 18 }

    stnPmPolicyProtocol OBJECT-TYPE
        SYNTAX          INTEGER {
                            icmp(1),
                            tcp(6),
                            udp(17),
                            esp(50),
                            ah(51),
                            ipcomp(108)
                        }
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION     "Protocol to match"
        ::= { stnPmPolicyEntry 19 }

    stnPmPolicyAction OBJECT-TYPE
        SYNTAX          INTEGER {
                            deny(1),
                            permit(2),
                            secure(3),
                            encaps(4),
                            reject(5),
                            fec(6)
                        }
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION     "Type of action to take upon policy match"
        ::= { stnPmPolicyEntry 20 }

    stnPmPolicyActionID OBJECT-TYPE
        SYNTAX          Integer32
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION     "Configuration ID of action record"
        ::= { stnPmPolicyEntry 21 }

    stnPmPolicyDirection OBJECT-TYPE
        SYNTAX          StnPmDirection
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION     "Direction of policy"
        ::= { stnPmPolicyEntry 22 }

    stnPmPolicyCreateMirror OBJECT-TYPE     
        SYNTAX          TruthValue
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION     "Mirror policy flag"
        ::= { stnPmPolicyEntry 23 }

    stnPmPolicySrcAddrSelector OBJECT-TYPE
        SYNTAX          StnPmSelectorType
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION     "Source selector to choose for policy matching"
        ::= { stnPmPolicyEntry 24 }

    stnPmPolicyDestAddrSelector OBJECT-TYPE
        SYNTAX          StnPmSelectorType
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION     "Destination selector to choose for policy matching"
        ::= { stnPmPolicyEntry 25 }

    stnPmPolicyEnabled OBJECT-TYPE
        SYNTAX          TruthValue
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION     "Flag to indicate if policy is enabled"
        ::= { stnPmPolicyEntry 26 }

    stnPmPolicyMatches OBJECT-TYPE
        SYNTAX          Integer32
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION     "Number of times policy search matched this policy"
        ::= { stnPmPolicyEntry 27 }

    stnPmPolicyMisses OBJECT-TYPE
        SYNTAX          Integer32
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION     "Number of times policy search did not match this 
                         policy"
        ::= { stnPmPolicyEntry 28 }

    stnPmPolicyMarkedOctets OBJECT-TYPE
        SYNTAX          Counter64
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION     "Number of octets that have been marked for this 
                         policy."
        ::= { stnPmPolicyEntry 29 }

    stnPmPolicyMarkedPkts OBJECT-TYPE
        SYNTAX          Counter64
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION     "Number of packets that have been marked for this 
                         policy."
        ::= { stnPmPolicyEntry 30 }

    stnPmPolicyIcmpTypes OBJECT-TYPE
        SYNTAX          Unsigned32
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION     "ICMP Types matching the policy entry expressed as 
                         bit-map."
        ::= { stnPmPolicyEntry 31 }

    stnPmPolicyTosByteMatchType OBJECT-TYPE
        SYNTAX          StnPmPolicyMatchType
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION     "Type of match for TOS Byte"
        ::= { stnPmPolicyEntry 32 }

    stnPmPolicyTosByteValue OBJECT-TYPE
        SYNTAX          INTEGER
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION     "Value containing bits to set in TOS Byte"
        ::= { stnPmPolicyEntry 33 }

    stnPmPolicyTosByteMask OBJECT-TYPE
        SYNTAX          INTEGER
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION     "Mask indicating bits to match in TOS byte"
        ::= { stnPmPolicyEntry 34 }

    stnPmPolicyMarkerID OBJECT-TYPE
        SYNTAX          Integer32
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION     "Configuration ID of marker to apply"
        ::= { stnPmPolicyEntry 35 }

    stnPmPolicyTxclassID OBJECT-TYPE
        SYNTAX          Integer32
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION     "Configuration ID of transmit class to assign packet to"
        ::= { stnPmPolicyEntry 36 }



    --
    -- STN PM Preference table
    --

    stnPmPreferenceTable OBJECT-TYPE
        SYNTAX        SEQUENCE OF StnPmPreferenceEntry
        MAX-ACCESS    not-accessible
        STATUS        current
        DESCRIPTION   "Entry contains information about a particular
                      preference entry."
        ::= { stnPmPreference 1 }
 
    stnPmPreferenceEntry OBJECT-TYPE
        SYNTAX          StnPmPreferenceEntry
        MAX-ACCESS      not-accessible
        STATUS          current
        DESCRIPTION     "Entry contains information about a particular
                        preference."
        INDEX           { stnPmPreferenceIndex }
        ::= { stnPmPreferenceTable 1 }
 
    StnPmPreferenceEntry ::= 
        SEQUENCE {
                stnPmPreferenceIndex       Integer32,
                stnPmPreferenceNum         Integer32,
                stnPmPreferenceAhAlg       StnPmAuthAlg,
                stnPmPreferenceEspAuthAlg  StnPmAuthAlg,
                stnPmPreferenceEspEncrAlg  StnPmEncrAlg,
                stnPmPreferenceLifeTime    Integer32,
                stnPmPreferenceLifeBytes   Integer32,
                stnPmPreferenceIPsecID     Integer32,
                stnPmPreferencePFSGroup    INTEGER,
                stnPmPreferenceDescription DisplayString
        }

    stnPmPreferenceIndex OBJECT-TYPE
        SYNTAX          Integer32
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION     "Preference index"
        ::= { stnPmPreferenceEntry 1 }

    stnPmPreferenceNum OBJECT-TYPE
        SYNTAX          Integer32
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION     "Preference number"
        ::= { stnPmPreferenceEntry 2 }

    stnPmPreferenceAhAlg OBJECT-TYPE
        SYNTAX          StnPmAuthAlg
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION     "AH authentication algorithm"
        ::= { stnPmPreferenceEntry 3 }

    stnPmPreferenceEspAuthAlg OBJECT-TYPE
        SYNTAX          StnPmAuthAlg
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION     "ESP authentication algorithm"
        ::= { stnPmPreferenceEntry 4 }

    stnPmPreferenceEspEncrAlg OBJECT-TYPE
        SYNTAX          StnPmEncrAlg
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION     "ESP encryption algorithm"
        ::= { stnPmPreferenceEntry 5 }

    stnPmPreferenceLifeTime OBJECT-TYPE
        SYNTAX          Integer32
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION     "Life time in minutes"
        ::= { stnPmPreferenceEntry 6 }

    stnPmPreferenceLifeBytes OBJECT-TYPE
        SYNTAX          Integer32
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION     "Life value in kilobytes"
        ::= { stnPmPreferenceEntry 7 }

    stnPmPreferenceIPsecID OBJECT-TYPE
        SYNTAX          Integer32
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION     "ID of corresponding IPSEC record"
        ::= { stnPmPreferenceEntry 8 }

    stnPmPreferencePFSGroup OBJECT-TYPE
        SYNTAX          INTEGER {
                            group1(1),
                            group2(2),
                            group5(5)
                        }
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION     "ESP encryption algorithm"
        ::= { stnPmPreferenceEntry 9 }

    stnPmPreferenceDescription OBJECT-TYPE
        SYNTAX          DisplayString (SIZE(0..65))
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION     "Description of preference"
        ::= { stnPmPreferenceEntry 10 }


    --
    -- STN PM IPsec table
    --

    stnPmIPsecTable OBJECT-TYPE
        SYNTAX          SEQUENCE OF StnPmIPsecEntry
        MAX-ACCESS      not-accessible
        STATUS          current
        DESCRIPTION     "Entry contains information about a particular
                        IPsec entry."
        ::= { stnPmIPsec 1 }
 
    stnPmIPsecEntry OBJECT-TYPE
        SYNTAX          StnPmIPsecEntry
        MAX-ACCESS      not-accessible
        STATUS          current
        DESCRIPTION     "Entry contains information about a particular
                        IPsec entry."
        INDEX           { stnPmIPsecIndex }
        ::= { stnPmIPsecTable 1 }
 
    StnPmIPsecEntry ::= 
        SEQUENCE {
                stnPmIPsecIndex                     Integer32,
                stnPmIPsecPeerIpAddr                IpAddress,
                stnPmIPsecLocalIpAddr               IpAddress,
                stnPmIPsecMode                      INTEGER, 
                stnPmIPsecKeyNegType                INTEGER,
                stnPmIPsecName                      DisplayString,
                stnPmIPsecNumManualSAs              Integer32,
                stnPmIPsecNumPreferences            Integer32,
                stnPmIPsecReplayDetectionEnabled    TruthValue
        }

    stnPmIPsecIndex OBJECT-TYPE
        SYNTAX          Integer32
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION     "IPsec index"
        ::= { stnPmIPsecEntry 1 }

    stnPmIPsecPeerIpAddr OBJECT-TYPE
        SYNTAX          IpAddress
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION     "IP address of peer for tunnel endpoint"
        ::= { stnPmIPsecEntry 2 }

    stnPmIPsecLocalIpAddr OBJECT-TYPE
        SYNTAX          IpAddress
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION     "Local IP address for tunnel endpoint"
        ::= { stnPmIPsecEntry 3 }

    stnPmIPsecMode OBJECT-TYPE
        SYNTAX          INTEGER {
                            tunnel(1),
                            transport(2)
                        }
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION     "Type of IPsec connection to create"
        ::= { stnPmIPsecEntry 4 }

    stnPmIPsecKeyNegType OBJECT-TYPE
        SYNTAX          INTEGER {
                            ike(1),
                            manual(2)
                        }
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION     "Key negotiation method for IPsec Sa"
        ::= { stnPmIPsecEntry 5 }

    stnPmIPsecName OBJECT-TYPE
        SYNTAX          DisplayString (SIZE(0..65))
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION     "Description of IPsec entry"
        ::= { stnPmIPsecEntry 6 }

    stnPmIPsecNumManualSAs OBJECT-TYPE
        SYNTAX          Integer32
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION     "Number of manual SAs assigned to this IPsec record"
        ::= { stnPmIPsecEntry 7 }

    stnPmIPsecNumPreferences OBJECT-TYPE
        SYNTAX          Integer32
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION     "Number of preferences assigned to this IPsec record"
        ::= { stnPmIPsecEntry 8 }

    stnPmIPsecReplayDetectionEnabled OBJECT-TYPE
        SYNTAX          TruthValue
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION     "Enable Replay detection."
        ::= { stnPmIPsecEntry 9 }


    --
    -- STN PM Encaps table
    --

    stnPmEncapsTable OBJECT-TYPE
        SYNTAX        SEQUENCE OF StnPmEncapsEntry
        MAX-ACCESS    not-accessible
        STATUS        current
        DESCRIPTION   "Entry contains information about a particular
                      Encaps entry."
        ::= { stnPmEncaps 1 }
 
    stnPmEncapsEntry OBJECT-TYPE
        SYNTAX          StnPmEncapsEntry
        MAX-ACCESS      not-accessible
        STATUS          current
        DESCRIPTION     "Entry contains information about a particular
                        Encaps entry."
        INDEX           { stnPmEncapsIndex }
        ::= { stnPmEncapsTable 1 }
 
    StnPmEncapsEntry ::= 
        SEQUENCE {
                stnPmEncapsIndex       Integer32,
                stnPmEncapsPeerIpAddr  IpAddress,
                stnPmEncapsLocalIpAddr IpAddress,
                stnPmEncapsType        INTEGER, 
                stnPmEncapsGREKey      Integer32,
                stnPmEncapsName        DisplayString
        }

