        SYNTAX          IpAddress
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION     "IP address of peer for tunnel endpoint"
        ::= { stnPmEncapsEntry 2 }

    stnPmEncapsLocalIpAddr OBJECT-TYPE
        SYNTAX          IpAddress
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION     "Local IP address for tunnel endpoint"
        ::= { stnPmEncapsEntry 3 }

    stnPmEncapsType OBJECT-TYPE
        SYNTAX          INTEGER {
                            ip-ip(1),
                            ip-gre(2)
                        }
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION     "Type of Encaps connection to create"
