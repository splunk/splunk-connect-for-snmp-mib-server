                                --#STATE   OPERATIONAL
        	                ::= 12

tapeAlertTrap13                 TRAP-TYPE       
                                ENTERPRISE      tapeAlert
                                VARIABLES       {
                                                hbaNumber,
                                                hbaChannel,
                                                driveScsiID
                                                }
        			DESCRIPTION
					"The operation has failed because the tape in the 
					drive has snapped:
					1. Discard the old tape.
					2. Restart the operation with a different tape."
                                --#TYPE "HP TapeAlert #13"
                                --#SUMMARY "Hba%d:Channel%d:ScsiID%d Tape Device Has Detected Snapped Tape"
                                --#ARGUMENTS {0,1,2}
                                --#SEVERITY CRITICAL
                                --#TIMEINDEX 9
				--#HELP "ta_traps.hlp"
				--#HELPTAG 13
                                --#STATE   DEGRADED
        	                ::= 13

