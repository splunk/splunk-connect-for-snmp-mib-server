                                --#STATE   DEGRADED
        	                ::= 16

tapeAlertTrap17                 TRAP-TYPE
                                ENTERPRISE      tapeAlert
                                VARIABLES       {
                                                hbaNumber,
                                                hbaChannel,
                                                driveScsiID
                                                }
        			DESCRIPTION
					"You have loaded a cartridge of a type that is read-only in this drive.
                                         The cartridge will appear as write-protected"
                                --#TYPE "HP TapeAlert #17"
                                --#SUMMARY "Hba%d:Channel%d:ScsiID%d Tape Device Has Detected Read Only Media"
                                --#ARGUMENTS {0,1,2}
                                --#SEVERITY MINOR
                                --#TIMEINDEX 9
				--#HELP "ta_traps.hlp"
				--#HELPTAG 17
                                --#STATE   OPERATIONAL
        	                ::= 17

tapeAlertTrap18                 TRAP-TYPE
                                ENTERPRISE      tapeAlert
                                VARIABLES       {
                                                hbaNumber,
                                                hbaChannel,
                                                driveScsiID
                                                }
        			DESCRIPTION
					"The directory on the tape cartridge has been corrupted.
