                                ENTERPRISE      tapeAlert
                                VARIABLES       {
                                                hbaNumber,
                                                hbaChannel,
                                                driveScsiID
                                                }
        			DESCRIPTION
					"The operation has failed because the tape in the 
					drive has snapped:
					1. Do not attempt to extract the tape cartridge.
                                        2. Call the tape drive supplier helpline."
                                --#TYPE "HP TapeAlert #14"
                                --#SUMMARY "Hba%d:Channel%d:ScsiID%d Tape Device Has Detected Snapped Tape"
                                --#ARGUMENTS {0,1,2}
                                --#SEVERITY CRITICAL
                                --#TIMEINDEX 9
				--#HELP "ta_traps.hlp"
				--#HELPTAG 14
                                --#STATE   DEGRADED
        	                ::= 14

tapeAlertTrap15                 TRAP-TYPE       
                                ENTERPRISE      tapeAlert
                                VARIABLES       {
                                                hbaNumber,
                                                hbaChannel,
                                                driveScsiID,
                                                currentMedia
                                                }
        			DESCRIPTION
					"The memory in the tape cartridge has failed, which reduces performance. 
                                         Do not use the cartridge for further backup operations."
                                --#TYPE "HP TapeAlert #15"
                                --#SUMMARY "Hba%d:Channel%d:ScsiID%d Tape Device Has Memory Chip in Cartridge Failure [%s]"
                                --#ARGUMENTS {0,1,2,3}
                                --#SEVERITY MINOR
                                --#TIMEINDEX 9
				--#HELP "ta_traps.hlp"
				--#HELPTAG 15
                                --#STATE   DEGRADED
        	                ::= 15

