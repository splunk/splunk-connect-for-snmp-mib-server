                                ENTERPRISE      tapeAlert
                                VARIABLES       {
                                                hbaNumber,
                                                hbaChannel,
                                                driveScsiID
                                                }
        			DESCRIPTION
					"You have tried to load a cartridge of a type 
					which is not supported by this drive."
                                --#TYPE "HP TapeAlert #12"
                                --#SUMMARY "Hba%d:Channel%d:ScsiID%d Media Loaded In Tape Device Is Unsupported Type"
                                --#ARGUMENTS {0,1,2}
                                --#SEVERITY INFORMATIONAL
                                --#TIMEINDEX 9
				--#HELP "ta_traps.hlp"
