        ::= {hdslSpanCurr24hPerformanceEntry 3}

    hdslCurr24hESLoopBNet  OBJECT-TYPE
        SYNTAX    INTEGER 
        ACCESS    read-only
        STATUS    mandatory
        DESCRIPTION
            "HDSL 24-hour ES count on Loop B's Network side of the span."
        ::= {hdslSpanCurr24hPerformanceEntry 4}

    hdslCurr24hUASLoopANet OBJECT-TYPE
        SYNTAX    INTEGER 
        ACCESS    read-only
        STATUS    mandatory
