    ----------------------------------------------------
    -- HDSL Alarms Table for All spans
    ----------------------------------------------------
    hdslSpanAlarmTable  OBJECT-TYPE
        SYNTAX    SEQUENCE OF HdslSpanAlarmEntry
        ACCESS    not-accessible
        STATUS    mandatory
        DESCRIPTION
            "This table provides the HDSL alarms indication on both loops
             of the HDSL spans.
            "
        ::= {pgainHDSL 5}

    hdslSpanAlarmEntry  OBJECT-TYPE
        SYNTAX    HdslSpanAlarmEntry
        ACCESS    not-accessible
        STATUS    mandatory
        DESCRIPTION
            "Entries of the HDSL span alarm table "
        INDEX
            {hdslSpanAlarmSlotId, hdslSpanAlarmSpanId}
        ::= {hdslSpanAlarmTable 1}

    HdslSpanAlarmEntry ::= SEQUENCE {
        hdslSpanAlarmSlotId
            INTEGER,
        hdslSpanAlarmSpanId
            INTEGER,
        hdslMarginAlarmLoopANet
            Byte,
        hdslMarginAlarmLoopBNet
            Byte,
        hdslESAlarmLoopANet
            Byte,
        hdslESAlarmLoopBNet
            Byte,
        hdslUASAlarmLoopANet
            Byte,
        hdslUASAlarmLoopBNet
            Byte,
        hdslMarginAlarmLoopACust
            Byte,  
        hdslMarginAlarmLoopBCust  
            Byte,  
        hdslESAlarmLoopACust 
            Byte, 
        hdslESAlarmLoopBCust
            Byte,
        hdslUASAlarmLoopACust 
            Byte, 
        hdslUASAlarmLoopBCust
            Byte,
        hdslLOSWAlarmLoopA
            Byte,
        hdslLOSWAlarmLoopB
            Byte,
        hdslPFOAlarmLoopA
            Byte,
        hdslPFOAlarmLoopB
            Byte,
        hdslPFSAlarmLoopA
            Byte,
        hdslPFSAlarmLoopB
            Byte,
        hdslBERAlarmLoopA
            Byte,
        hdslBERAlarmLoopB
            Byte

    }

    hdslSpanAlarmSlotId  OBJECT-TYPE
        SYNTAX    INTEGER
        ACCESS    read-only
        STATUS    mandatory
        DESCRIPTION
            "Identifies the HDSL line occupied in the chassis.
             The slots are numbered 1 through 16 from left to right."
        ::= {hdslSpanAlarmEntry 1}

    hdslSpanAlarmSpanId  OBJECT-TYPE
        SYNTAX    INTEGER
        ACCESS    read-only
        STATUS    mandatory
        DESCRIPTION
            "Identifies the span of the HDSL line we are monitoring.
             Depending on the number of regenerator(s) (REGs)
             present in an HDSL loop, a span will be numbered as follows:
                 0 REG   - only 1 span.
                 1 REG   - span between LTU and REG is identified as 1.
                           span between REG and NTU is identified as 2.
                 2 REGs  - span between LTU and REG #1 is identified as 1.
                           span between REG #1 and REG #2 is identified as 2.
                           span between REG #2 and NTU is identified as 3.
             "
        ::= {hdslSpanAlarmEntry 2}

    hdslMarginAlarmLoopANet OBJECT-TYPE
        SYNTAX    Byte
        ACCESS    read-only
        STATUS    mandatory
        DESCRIPTION
            "Margin Threshold Exceeded on loop A's Network side of the span."
        ::= {hdslSpanAlarmEntry 3}

    hdslMarginAlarmLoopBNet OBJECT-TYPE
        SYNTAX    Byte
        ACCESS    read-only
        STATUS    mandatory
        DESCRIPTION
            "Margin Threshold Exceeded on loop B's Network side of the span."
        ::= {hdslSpanAlarmEntry 4}

    hdslESAlarmLoopANet OBJECT-TYPE
        SYNTAX    Byte
        ACCESS    read-only
        STATUS    mandatory
        DESCRIPTION
            "Errored Seconds threshold exceeded on loop A's Network side 
             of the span.
            "
        ::= {hdslSpanAlarmEntry 5}

    hdslESAlarmLoopBNet OBJECT-TYPE
