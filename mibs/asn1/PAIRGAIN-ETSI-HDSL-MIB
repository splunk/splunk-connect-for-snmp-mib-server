    ----------------------------------------------------
    -- HDSL Statistics for All spans
    ----------------------------------------------------
    hdslSpanStatisticsTable  OBJECT-TYPE
        SYNTAX    SEQUENCE OF HdslSpanStatisticsEntry
        ACCESS    not-accessible
        STATUS    mandatory
        DESCRIPTION
            "This table lists statistics for margin, pulse attenuation and 
             state of the loop for all spans of the HDSL loop.
            "
        ::= {pgainHDSL 4}

    hdslSpanStatisticsEntry  OBJECT-TYPE
        SYNTAX    HdslSpanStatisticsEntry
        ACCESS    not-accessible
        STATUS    mandatory
        DESCRIPTION
            "Entries of the HDSL span statistics table "
        INDEX
            {hdslStatisticsSlotId, hdslStatisticsSpanId}
        ::= {hdslSpanStatisticsTable 1}

    HdslSpanStatisticsEntry ::= SEQUENCE {
        hdslStatisticsSlotId
            INTEGER,
        hdslStatisticsSpanId
            INTEGER,
        hdslLoopPairState
            Byte,
        hdslLoopATipRingReversal
            Byte,
        hdslLoopBTipRingReversal
            Byte,
        hdslLoopAUpState
            Byte,
        hdslLoopBUpState
            Byte,
        hdslMarginLoopANet
            Byte,
        hdslMarginHighLoopANet
            Byte,
        hdslMarginLowLoopANet
            Byte,
        hdslMarginLoopBNet
            Byte,
        hdslMarginHighLoopBNet
            Byte,
        hdslMarginLowLoopBNet
            Byte,
        hdslPulseAttenuationLoopANet
            Byte,
        hdslPulseAttenuationLoopBNet
            Byte,
        hdslMarginLoopACust
            Byte,
        hdslMarginHighLoopACust
            Byte,
        hdslMarginLowLoopACust
            Byte,
        hdslMarginLoopBCust
            Byte,
        hdslMarginHighLoopBCust
            Byte,
        hdslMarginLowLoopBCust
            Byte,
        hdslPulseAttenuationLoopACust
            Byte,
        hdslPulseAttenuationLoopBCust
            Byte
        }

    hdslStatisticsSlotId  OBJECT-TYPE
        SYNTAX    INTEGER
        ACCESS    read-only
        STATUS    mandatory
        DESCRIPTION
            "Identifies the HDSL line occupied in the chassis.
             The slots are numbered 1 through 16 from left to right."
        ::= {hdslSpanStatisticsEntry 1}

    hdslStatisticsSpanId  OBJECT-TYPE
        SYNTAX    INTEGER
        ACCESS    read-only
        STATUS    mandatory
        DESCRIPTION
            "Identifies the span of the HDSL line we are monitoring.
             Depending on the number of regenerator(s) (REGs)
             present in an HDSL loop, a span will be numbered as follows:
                 0 REG   - only 1 span.
                 1 REG   - span between LTU and REG is identified as 1.
                           span between REG and NTU is identified as 2.
                 2 REGs  - span between LTU and REG #1 is identified as 1.
                           span between REG #1 and REG #2 is identified as 2.
                           span between REG #2 and NTU is identified as 3.
             "
        ::= {hdslSpanStatisticsEntry 2}

    hdslLoopPairState OBJECT-TYPE
        SYNTAX    Byte
        ACCESS    read-only
        STATUS    mandatory
        DESCRIPTION
            "Current state on the HDSL loop of span:
                 0 - loop pair is normal 
                 1 - loop pair is exchanged.
            "
        ::= {hdslSpanStatisticsEntry 3}

    hdslLoopATipRingReversal OBJECT-TYPE
        SYNTAX    Byte
        ACCESS    read-only 
        STATUS    mandatory 
        DESCRIPTION  
            "Current TIP/RING state on the HDSL loop A of span: 
                 0 - TIP/RING on loop A is not reversed. 
                 1 - TIP/RING on loop A is reversed.   
            " 
        ::= {hdslSpanStatisticsEntry 4}

    hdslLoopBTipRingReversal OBJECT-TYPE
        SYNTAX    Byte
        ACCESS    read-only 
        STATUS    mandatory 
        DESCRIPTION  
            "Current TIP/RING state on the HDSL loop B of span: 
                 0 - TIP/RING on loop B is not reversed. 
                 1 - TIP/RING on loop B is reversed.   
            " 
        ::= {hdslSpanStatisticsEntry 5}

    hdslLoopAUpState  OBJECT-TYPE
        SYNTAX    Byte
        ACCESS    read-only
        STATUS    mandatory
        DESCRIPTION
            "Current loop state on loop A of the span:
                 0 - loop is up.
                 1 - loop is acquiring/converging.
                 2 - loop is signaling.
            "
        ::= {hdslSpanStatisticsEntry 6}

    hdslLoopBUpState  OBJECT-TYPE
        SYNTAX    Byte
        ACCESS    read-only
        STATUS    mandatory
        DESCRIPTION
            "Current loop state on loop B of the span:
                 0 - loop is up.
                 1 - loop is acquiring/converging.
                 2 - loop is signaling.
            "
        ::= {hdslSpanStatisticsEntry 7}

    hdslMarginLoopANet  OBJECT-TYPE
        SYNTAX    Byte
        ACCESS    read-only
        STATUS    mandatory
        DESCRIPTION
            "Current Margin on Loop A's Network side of the span."
        ::= {hdslSpanStatisticsEntry 8}

    hdslMarginHighLoopANet  OBJECT-TYPE
        SYNTAX    Byte
        ACCESS    read-only
        STATUS    mandatory
        DESCRIPTION
            "Highest Margin on Loop A's Network side of the span."
