                INTEGER,
        hdsl24hSESLoopANet
                INTEGER,
        hdsl24hSESLoopBNet
                INTEGER,
        hdsl24hESLoopACust
                INTEGER,
        hdsl24hESLoopBCust
                INTEGER,
        hdsl24hUASLoopACust
                INTEGER,
        hdsl24hUASLoopBCust
                INTEGER,
        hdsl24hSESLoopACust
                INTEGER,
        hdsl24hSESLoopBCust
                INTEGER
        }

