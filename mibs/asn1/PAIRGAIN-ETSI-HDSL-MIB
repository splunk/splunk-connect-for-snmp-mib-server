    ----------------------------------------------------
    -- HDSL Current 24-Hour Performance Counts for All spans
    ----------------------------------------------------
    hdslSpanCurr24hPerformanceTable  OBJECT-TYPE
        SYNTAX    SEQUENCE OF HdslSpanCurr24hPerformanceEntry
        ACCESS    not-accessible
        STATUS    mandatory
        DESCRIPTION
            "This table provides the current 24-hour performance history 
             monitoring for all HDSL spans Errored Seconds (ES), Severely 
             Errored Seconds (SES) and Unavailable Seconds (UAS)."
        ::= {pgainHDSL 3}

    hdslSpanCurr24hPerformanceEntry  OBJECT-TYPE
        SYNTAX    HdslSpanCurr24hPerformanceEntry
        ACCESS    not-accessible
        STATUS    mandatory
        DESCRIPTION
            "Entries of the HDSL span Current 24-hour PM counts table "
        INDEX
            {hdslSpanCurr24hSlotId, hdslSpanCurr24hSpanId}
        ::= {hdslSpanCurr24hPerformanceTable 1}

    HdslSpanCurr24hPerformanceEntry ::= SEQUENCE {
        hdslSpanCurr24hSlotId
                INTEGER,
        hdslSpanCurr24hSpanId
                INTEGER,
        hdslCurr24hESLoopANet
                INTEGER,
        hdslCurr24hESLoopBNet
                INTEGER,
        hdslCurr24hUASLoopANet
                INTEGER,
        hdslCurr24hUASLoopBNet
                INTEGER,
        hdslCurr24hSESLoopANet
                INTEGER,
        hdslCurr24hSESLoopBNet
                INTEGER,
        hdslCurr24hESLoopACust
                INTEGER,
        hdslCurr24hESLoopBCust
                INTEGER,
        hdslCurr24hUASLoopACust
                INTEGER,
        hdslCurr24hUASLoopBCust
                INTEGER,
        hdslCurr24hSESLoopACust
                INTEGER,
        hdslCurr24hSESLoopBCust
                INTEGER
        }

    hdslSpanCurr24hSlotId OBJECT-TYPE
        SYNTAX    INTEGER
        ACCESS    read-only
        STATUS    mandatory
        DESCRIPTION
            "Identifies the HDSL line occupied in the chassis.
             The slots are numbered 1 through 16 from left to right."
        ::= {hdslSpanCurr24hPerformanceEntry 1}

    hdslSpanCurr24hSpanId  OBJECT-TYPE
        SYNTAX    INTEGER
        ACCESS    read-only
        STATUS    mandatory
        DESCRIPTION
            "Identifies the span of the HDSL line we are monitoring.
             Depending on the number of regenerator(s) (REGs)
             present in an HDSL loop, a span will be numbered as follows:
                 0 REG   - only 1 span.
                 1 REG   - span between LTU and REG is identified as 1.
                           span between REG and NTU is identified as 2.
                 2 REGs  - span between LTU and REG #1 is identified as 1.
                           span between REG #1 and REG #2 is identified as 2.
                           span between REG #2 and NTU is identified as 3.
             "
        ::= {hdslSpanCurr24hPerformanceEntry 2}

    hdslCurr24hESLoopANet  OBJECT-TYPE
        SYNTAX    INTEGER 
