     -- The ACP Ethernet Port Group

     acpEnetPortTable OBJECT-TYPE 
          SYNTAX  SEQUENCE OF AcpEnetPortEntry
          ACCESS  not-accessible
          STATUS  mandatory
          ::= { acpEnetPort 1 }


     acpEnetPortEntry OBJECT-TYPE 
          SYNTAX  AcpEnetPortEntry
          ACCESS  not-accessible
          STATUS  mandatory
	  INDEX { acpEnetPortIgnoreHeartbeat }
          ::= { acpEnetPortTable 1 }


     AcpEnetPortEntry ::= SEQUENCE {
          acpEnetPortIgnoreHeartbeat
               INTEGER,
          acpEnetPortRxFewDescr
               Counter,
          acpEnetPortRxNoDescr
               Counter,
          acpEnetPortRxOflo
               Counter,
          acpEnetPortTxDevMem
               Counter,
          acpEnetPortTxEnp
               Counter,
          acpEnetPortTxDevInvRead
               Counter,
          acpEnetPortTx16Coll
               Counter,
          acpEnetPortTxMissHeart
               Counter,
          acpEnetPortTxDfr
               Counter,
          acpEnetPortTxOneColl
               Counter,
          acpEnetPortTxMulColl
               Counter,
          acpEnetPortTxLate
               Counter,
          acpEnetPortTxTdr
               Counter,
          acpEnetPortLanUtil
               Counter,
          acpEnetPortRxBrRat
               Counter,
          acpEnetPortTxBrRat
               Counter
     }


     acpEnetPortIgnoreHeartbeat OBJECT-TYPE 
          SYNTAX  INTEGER {
                      no(1),
                      yes(2)
                  }
          ACCESS  read-only
          STATUS  mandatory
		--% GROUP acpEnetPortEntry
          ::= { acpEnetPortEntry 1 }


