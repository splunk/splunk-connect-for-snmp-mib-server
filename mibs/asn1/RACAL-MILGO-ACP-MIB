     RACAL-MILGO-ACP-MIB

DEFINITIONS ::= BEGIN
IMPORTS
	OBJECT-TYPE, NetworkAddress, IpAddress,
	Counter, Gauge, TimeTicks
		FROM RFC1155-SMI;


internet        OBJECT IDENTIFIER ::= { iso org(3) dod(6) 1 }
experimental    OBJECT IDENTIFIER ::= { internet 3 }
private         OBJECT IDENTIFIER ::= { internet 4 }

enterprises     OBJECT IDENTIFIER ::= { private 1 }


     mib-racal-milgo        OBJECT IDENTIFIER ::= { enterprises 124 }

     rmAcp                  OBJECT IDENTIFIER ::= { mib-racal-milgo 2 }

     acpSys                 OBJECT IDENTIFIER ::= { rmAcp 1 }
     acpComPort             OBJECT IDENTIFIER ::= { rmAcp 2 }
     acpEnetPort            OBJECT IDENTIFIER ::= { rmAcp 3 }
     acpWanPort             OBJECT IDENTIFIER ::= { rmAcp 4 }
     acpMesh                OBJECT IDENTIFIER ::= { rmAcp 5 }
     acpTb                  OBJECT IDENTIFIER ::= { rmAcp 6 }



     -- The ACP System Group

     acpSysNodeId OBJECT-TYPE 
          SYNTAX  OCTET STRING
          ACCESS  read-only
          STATUS  mandatory
		--% GROUP acpSys
          ::= { acpSys 1 }


     acpSysAlarm OBJECT-TYPE 
          SYNTAX  OCTET STRING
          ACCESS  read-only
          STATUS  mandatory
		--% GROUP acpSys
          ::= { acpSys 2 }


     acpSysModTable OBJECT-TYPE 
          SYNTAX  SEQUENCE OF AcpSysModEntry
          ACCESS  not-accessible
          STATUS  mandatory
          ::= { acpSys 3 }


     acpSysModEntry OBJECT-TYPE 
          SYNTAX  AcpSysModEntry
          ACCESS  not-accessible
          STATUS  mandatory
	  INDEX { acpSysModType }
          ::= { acpSysModTable 1 }


     AcpSysModEntry ::= SEQUENCE {
          acpSysModType
               INTEGER,
          acpSysModStatus
               INTEGER,
          acpSysModSwVersion
               OCTET STRING,
          acpSysModSwDate
               OCTET STRING,
          acpSysModSwComment
               OCTET STRING,
          acpSysModFrontIdProm
               OCTET STRING,
          acpSysModRearIdProm
               OCTET STRING
     }



     acpSysModType OBJECT-TYPE 
          SYNTAX  INTEGER {
                      none(1),
                      iopLan(2),
                      iopWan(3),
                      iopHyb(4),
                      iop(5), 
                      me(6),    
                      nam(7)  
                  }
          ACCESS  read-only
          STATUS  mandatory
		--% GROUP acpSysModEntry
          ::= { acpSysModEntry 1 }

     acpSysModStatus OBJECT-TYPE 
          SYNTAX  INTEGER {
                      reset(1),
                      noHardware(2),
                      diag(3),
                      failed(4),
                      loading(5),
                      loaded(6),
                      config(7),
                      onLine(8),
                      disabled(9)
                  }
          ACCESS  read-only
          STATUS  mandatory
		--% GROUP acpSysModEntry
          ::= { acpSysModEntry 2 }

     acpSysModSwVersion OBJECT-TYPE 
          SYNTAX  OCTET STRING
          ACCESS  read-only
          STATUS  mandatory
		--% GROUP acpSysModEntry
          ::= { acpSysModEntry 3 }

     acpSysModSwDate OBJECT-TYPE 
          SYNTAX  OCTET STRING
          ACCESS  read-only
          STATUS  mandatory
		--% GROUP acpSysModEntry
          ::= { acpSysModEntry 4 }

     acpSysModSwComment OBJECT-TYPE 
          SYNTAX  OCTET STRING
          ACCESS  read-only
          STATUS  mandatory
		--% GROUP acpSysModEntry
          ::= { acpSysModEntry 5 }

     acpSysModFrontIdProm OBJECT-TYPE 
          SYNTAX  OCTET STRING
          ACCESS  read-only
          STATUS  mandatory
		--% GROUP acpSysModEntry
          ::= { acpSysModEntry 6 }

     acpSysModRearIdProm OBJECT-TYPE 
          SYNTAX  OCTET STRING
          ACCESS  read-only
          STATUS  mandatory
		--% GROUP acpSysModEntry
          ::= { acpSysModEntry 7 }


     -- The ACP Common Port Group


     acpComPortTable OBJECT-TYPE 
          SYNTAX  SEQUENCE OF AcpComPortEntry
          ACCESS  not-accessible
          STATUS  mandatory
          ::= { acpComPort 1 }


     acpComPortEntry OBJECT-TYPE 
          SYNTAX  AcpComPortEntry
          ACCESS  not-accessible
          STATUS  mandatory
	  DESCRIPTION " "
	  INDEX	{ ifIndex }
          ::= { acpComPortTable 1 }


      AcpComPortEntry ::= SEQUENCE {
          acpComQtimer
               INTEGER,
          acpComCongTimer
               INTEGER,
          acpComErrMax
               INTEGER,
          acpComPortRxPktsAcp
               Counter,
          acpComPortRxPktsKnown
               Counter,
          acpComPortRxPktsUnk
               Counter,
          acpComPortRxOutcomeFilIop
               Counter,
          acpComPortTxPktsAcp
               Counter,
          acpComPortTxPktsKnown
               Counter,
          acpComPortTxPktsUnk
               Counter,
          acpComPortRxOutcomeFwd
               Counter,
          acpComPortRxOutcomeFilLcl
               Counter,
          acpComPortRxOutcomeFilNf
               Counter,
          acpComPortRxOutcomeFilSrc
               Counter,
          acpComPortRxOutcomeFilDest
               Counter,
          acpComPortRxFilterChars
               Counter,
          acpComPortRxSizeErr
               Counter,
          acpComPortRxCrcErr
               Counter,
          acpComPortRxFrmErr
               Counter,
          acpComPortNzRxUtil
               Counter,
          acpComPortNzTxUtil
               Counter
     }



     acpComPortQtimer OBJECT-TYPE 
          SYNTAX  INTEGER
          ACCESS  read-only
          STATUS  mandatory
		--% GROUP acpComPortEntry
          ::= { acpComPortEntry 1 }


     acpComPortCongTimer OBJECT-TYPE 
