          ACCESS  read-only
          STATUS  mandatory
		--% GROUP acpTb
          ::= { acpTb 4 }


     acpTbStatAddrTabSize OBJECT-TYPE 
          SYNTAX  Gauge
          ACCESS  read-only
          STATUS  mandatory
		--% GROUP acpTb
          ::= { acpTb 5 }


     acpTbStatAddrTabSizePeak OBJECT-TYPE 
          SYNTAX  Gauge
          ACCESS  read-only
          STATUS  mandatory
		--% GROUP acpTb
          ::= { acpTb 6 }


     acpTbPortTable OBJECT-TYPE 
          SYNTAX  SEQUENCE OF AcpTbPortEntry
          ACCESS  not-accessible
          STATUS  mandatory
          ::= { acpTb 7 }


     acpTbPortEntry OBJECT-TYPE 
          SYNTAX  AcpTbPortEntry
          ACCESS  not-accessible
          STATUS  mandatory
	  INDEX { acpTbPortStn }
          ::= { acpTbPortTable 1  }


     AcpTbPortEntry ::= SEQUENCE {
          acpTbPortStn
               INTEGER,
          acpTbPortTimeForwarding
               INTEGER,
          acpTbPortTimeNotForwarding
               INTEGER
     }



     acpTbPortStn OBJECT-TYPE 
          SYNTAX  INTEGER
          ACCESS  read-only
          STATUS  mandatory
		--% GROUP acpTbPortEntry
          ::= { acpTbPortEntry 1 }


     acpTbPortTimeForwarding OBJECT-TYPE 
          SYNTAX  INTEGER
          ACCESS  read-only
          STATUS  mandatory
		--% GROUP acpTbPortEntry
          ::= { acpTbPortEntry 2 }


     acpTbPortTimeNotForwarding OBJECT-TYPE 
          SYNTAX  INTEGER
          ACCESS  read-only
          STATUS  mandatory
		--% GROUP acpTbPortEntry
          ::= { acpTbPortEntry 3 }



     END

