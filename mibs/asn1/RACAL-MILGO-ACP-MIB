     -- The ACP WAN Port Group


     acpWanPortTable OBJECT-TYPE 
          SYNTAX  SEQUENCE OF AcpWanPortEntry
          ACCESS  not-accessible
          STATUS  mandatory
          ::= { acpWanPort 1 }


     acpWanPortEntry OBJECT-TYPE 
          SYNTAX  AcpWanPortEntry
          ACCESS  not-accessible
          STATUS  mandatory
	  INDEX { acpWanPortInvert }
          ::= { acpWanPortTable 1 }


      AcpWanPortEntry ::= SEQUENCE {
          acpWanPortInvert
               INTEGER,
          acpWanPortRxQOflo
               Counter,
          acpWanPortRxUtil
               Counter,
          acpWanPortTxUtil
               Counter,
          acpWanPortClock
               INTEGER
     }


     acpWanPortInvert OBJECT-TYPE 
          SYNTAX  INTEGER {
                      no(1),
                      yes(2)
                  }
          ACCESS  read-only
          STATUS  mandatory
		--% GROUP acpWanPortEntry
          ::= { acpWanPortEntry 1 }

     acpWanPortRxQOflo OBJECT-TYPE 
          SYNTAX  Counter
          ACCESS  read-only
          STATUS  mandatory
		--% GROUP acpWanPortEntry
          ::= { acpWanPortEntry 2 }


     acpWanPortRxUtil OBJECT-TYPE 
          SYNTAX  Counter
          ACCESS  read-only
          STATUS  mandatory
		--% GROUP acpWanPortEntry
          ::= { acpWanPortEntry 3 }


     acpWanPortTxUtil OBJECT-TYPE 
          SYNTAX  Counter
          ACCESS  read-only
          STATUS  mandatory
		--% GROUP acpWanPortEntry
          ::= { acpWanPortEntry 4 }

     acpWanPortClock OBJECT-TYPE 
          SYNTAX  INTEGER
          ACCESS  read-only
          STATUS  mandatory
		--% GROUP acpWanPortEntry
          ::= { acpWanPortEntry 5 }

     -- The ACP Enhanced Mesh Bridge Group

     acpMeshProtocol OBJECT-TYPE 
          SYNTAX INTEGER {
                     spanningTree(1),
                     enhancedMesh(2)
                 }
          ACCESS  read-only
          STATUS  mandatory
		--% GROUP acpMesh
          ::= { acpMesh 1 }


     acpMeshBridgeCost OBJECT-TYPE 
          SYNTAX  INTEGER
          ACCESS  read-only
          STATUS  mandatory
		--% GROUP acpMesh
          ::= { acpMesh 2 }


     acpMeshCostTabChanges OBJECT-TYPE 
          SYNTAX  Counter
          ACCESS  read-only
          STATUS  mandatory
		--% GROUP acpMesh
          ::= { acpMesh 3 }


     acpMeshLastCostTabChange OBJECT-TYPE 
          SYNTAX  Counter
          ACCESS  read-only
          STATUS  mandatory
		--% GROUP acpMesh
          ::= { acpMesh 4 }


     acpMeshSubRootAddr OBJECT-TYPE 
          SYNTAX  OCTET STRING
          ACCESS  read-only
          STATUS  mandatory
		--% GROUP acpMesh
          ::= { acpMesh 5 }


     acpMeshCostInfoLost OBJECT-TYPE 
          SYNTAX  Counter
          ACCESS  read-only
          STATUS  mandatory
		--% GROUP acpMesh
          ::= { acpMesh 6 }


     acpMeshNodeAddrTabSize OBJECT-TYPE 
          SYNTAX  Gauge
          ACCESS  read-only
          STATUS  mandatory
		--% GROUP acpMesh
          ::= { acpMesh 7 }


     acpMeshNodeAddrTabSizePeak OBJECT-TYPE 
          SYNTAX  Counter
          ACCESS  read-only
          STATUS  mandatory
		--% GROUP acpMesh
          ::= { acpMesh 8 }


     acpMeshPortTable OBJECT-TYPE 
          SYNTAX  SEQUENCE OF AcpMeshPortEntry
          ACCESS  not-accessible
          STATUS  mandatory
          ::= { acpMesh 9 }


     acpMeshPortEntry OBJECT-TYPE 
          SYNTAX  AcpMeshPortEntry
          ACCESS  not-accessible
          STATUS  mandatory
	  INDEX { acpMeshPortRemoteNodeIfIdx }
          ::= { acpMeshPortTable 1 }


      AcpMeshPortEntry ::= SEQUENCE {
          acpMeshPortDisabledReason
               INTEGER,
          acpMeshPortBlockingSubstate
               INTEGER,
          acpMeshPortLinkCost
               INTEGER,
          acpMeshPortRemoteNodeAddr
               OCTET STRING,
          acpMeshPortSubnetAddr
               OCTET STRING,
          acpMeshPortRemoteNodeIfIdx
               INTEGER
     }


     acpMeshPortDisabledReason OBJECT-TYPE 
          SYNTAX INTEGER {
                     broken(1),
                     looped(2),
                     stopped(3),
                     notCommunicating(4)
                 }
          ACCESS  read-only
          STATUS  mandatory
		--% GROUP acpMeshPortEntry
          ::= { acpMeshPortEntry 1 }


     acpMeshPortBlockingSubstate OBJECT-TYPE 
          SYNTAX  INTEGER {
                      blocking(1),
                      forwarding(2)
                  }
          ACCESS  read-only
          STATUS  mandatory
		--% GROUP acpMeshPortEntry
          ::= { acpMeshPortEntry 2 }


     acpMeshPortLinkCost OBJECT-TYPE 
          SYNTAX  INTEGER
          ACCESS  read-only
          STATUS  mandatory
		--% GROUP acpMeshPortEntry
          ::= { acpMeshPortEntry 3 }

     acpMeshPortRemoteNodeAddr OBJECT-TYPE 
          SYNTAX  OCTET STRING
          ACCESS  read-only
          STATUS  mandatory
		--% GROUP acpMeshPortEntry
          ::= { acpMeshPortEntry 4 }


     acpMeshPortSubnetAddr OBJECT-TYPE 
          SYNTAX  OCTET STRING
          ACCESS  read-only
          STATUS  mandatory
		--% GROUP acpMeshPortEntry
          ::= { acpMeshPortEntry 5 }


     acpMeshPortRemoteNodeIfIdx OBJECT-TYPE 
          SYNTAX  INTEGER
          ACCESS  read-only
          STATUS  mandatory
		--% GROUP acpMeshPortEntry
          ::= { acpMeshPortEntry 6 }


     -- The ACP Transparent Bridge Group

     acpTbAddrTabFree OBJECT-TYPE 
