          DESCRIPTION
               "There is a TapeAlert warning."
          --#TYPE      "User advised of event"
          --#SUMMARY   "%s"
          --#ARGUMENTS {0}
          --#SEVERITY  MINOR
          --#TIMEINDEX 1        
          --#STATE     OPERATIONAL
