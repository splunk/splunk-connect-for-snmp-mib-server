--                                 mpcMpsMacAddressGroup,
--                                 mpcIngressCacheTotalPacketGroup,
--                                 mpcIngressCacheGroup,
--                                mpcEgressCacheTotalPacketGroup,
--                                 mpcEgressCacheGroup,
--                                mpcMpsMultipleMacAddressGroup
--                            }
