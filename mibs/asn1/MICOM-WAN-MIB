	SYNTAX	NvmWanDTEPortTunnelPvcEntry
	ACCESS	not-accessible
	STATUS	deprecated
	DESCRIPTION
	  "NAME = ;
	   DESC = WAN DTE port tunneling PVC DLCI Entry;
	   HELP = ;
	   CAPABILITIES = ;"
	INDEX	{nvmWanDTEPortTunnelPvcIfIndex}
	::= {nvmWanDTEPortTunnelPvcTable 1}

