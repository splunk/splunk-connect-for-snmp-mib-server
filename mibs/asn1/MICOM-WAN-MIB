	SYNTAX	SEQUENCE OF NvmWanDTEPortTunnelPvcEntry
	ACCESS	not-accessible
	STATUS	deprecated
	DESCRIPTION
	  "NAME = ;
	   DESC = Tunneling PVC DLCI Configuration table.\
                  (configuration);
	   HELP = ;
	   CAPABILITIES = NET_CFG, VPN_CFG;"
	::= {wan-configuration 4}

