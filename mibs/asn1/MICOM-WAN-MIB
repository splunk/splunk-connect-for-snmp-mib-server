	ACCESS	read-only
	STATUS	obsolete
	DESCRIPTION
	  "NAME = ;
	   DESC = Traffic class emission priority \
		  1 voice, 2 NMS and NAS, 3 signalling \
		  4 LAN data;
	   HELP = ;
	   CAPABILITIES = ;"
