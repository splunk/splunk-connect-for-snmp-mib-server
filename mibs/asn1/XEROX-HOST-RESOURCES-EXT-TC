        --  'xcmHrDeviceFax'
        faxNone(10400),

        --  'xcmHrDeviceMailbox'
        mailboxNone(10500),

        --  'xcmHrDeviceMarker'
        markerNone(11100),

        --  'xcmHrDeviceFinisherBFM'
        finisherBFMNone(11200),

        --  'xcmHrDeviceFinisherMFF'


        finisherMFFNone(11300),

        --  'xcmHrDeviceFinisherXIM'
        finisherXIMNone(11400),

