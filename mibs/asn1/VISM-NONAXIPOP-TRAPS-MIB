trapRudpSessionActive    TRAP-TYPE
    ENTERPRISE    basis
    VARIABLES     {
        lastSequenceNumber,
        shelfNodeName,
        shelfNum,
        moduleSlotNumber,
        moduleTrapAlarmSeverity,
        functionModuleType,
        genericTimeStamp,
        vismRudpSessionNum,
        vismRudpSessionState
    }
    DESCRIPTION
        "When a 'primary-is' session in a group receives ACTIVE message 
         from the MGC this trap will be sent out.
         The severity should be 'don't care', no alarm.
        "
    ::= 50772

trapSessionGrpOos    TRAP-TYPE
    ENTERPRISE    basis
    VARIABLES     {
        lastSequenceNumber,
        shelfNodeName,
        shelfNum,
        moduleSlotNumber,
        moduleTrapAlarmSeverity,
        functionModuleType,
        genericTimeStamp,
        vismSessionGrpNum,
        vismSessionGrpState
    }
    DESCRIPTION
        "When all the sessions in the group have changed to 'oos' state,
         the state of this group is changed to 'oos' and this trap 
         will be sent out.
         The severity should be 'minor', no alarm.
        "
    ::= 50773

