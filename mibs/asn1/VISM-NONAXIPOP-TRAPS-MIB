--    LAST-UPDATED "200112261500Z"
--    DESCRIPTION
--        "TRAPS specific to VISM card which have been defined
--         as extensions to SMI v2 MIBs MGMIB, SRCP-MIB. The traps in
--         this file do not go into AXIPOP MIB and hence are kept in a 
--         separate file."
--
--    REVISION    "200112261500Z"
--    DESCRIPTION
--        "Added traps for Announcement Files:
--         cwAnnFileAddStarted   = 50674,
--         cwAnnFileLoadComplete = 50675,
--         cwAnnFileLoadFailed   = 50676,           
--         cwAnnFileDeleted      = 50677."
--       
--    REVISION    "200109281500Z"
--    DESCRIPTION "
--                Add vismRtpLcn to trap:50650"
--
--    REVISION    "200109101500Z"
--    DESCRIPTION "
--                Each state change trap has been split into multiple traps each 
--                representing a specific state.
--                For example: mgAdministrativeStateChange in MGMIB has been 
--                changed to  mgAdministrativeStateLocked, 
--                mgAdministrativeStateUnlocked, mgAdministrativeStateShuttingDown.
--                "
-- 
--    REVISION    "200108281500Z"
--    DESCRIPTION "
--                Deleted traps for mgcRedundancyGrpAssociated,
--                mgcRedundancyGrpUnassociated, mgcRedundancyGrpAssociatedCommLoss,
--                and added traps for mgcRedundancyGrpCommOk and 
--                mgcRedundancyGrpCommLoss."
--
--    REVISION    "200108281400Z"
--    DESCRIPTION "
--                Changed trap number of mgDomainNameAdded as the trap used the
--                same number as trapNetworkCacConfigState. "
--
--    REVISION    "200107011400Z"
--    DESCRIPTION "
--                Added VoIP Trunking traps - vismRtpConnAdded, vismRtpConnDeleted
--                vismRtpConnStateChg, vismLapdTrStateIs, vismLapdTrStateOos,
--                vismLapdTrunkAdded, vismLapdTrunkDeleted "
--
--    REVISION    "200105251400Z"
--    DESCRIPTION "
--                Obsoleted the following traps - mgcAssociated, mgcUnassociated,
--                mgcAssociatedCommLoss, mgcProtocolAdded, mgcProtocolDeleted and
--                added new traps - mgcRedundancyGrpAdded mgcRedundancyGrpDeleted,
--                mgcRedundancyGrpAssociated, mgcRedundancyGrpUnassociated,
--                mgcRedundancyGrpAssociatedCommLoss,
--                mgcRedundancyGrpProtocolAdded and
--                mgcRedundancyGrpProtocolDeleted."
--
--    REVISION    "200005111400Z"
--    DESCRIPTION "
--                Move vismRudpSession and vismLapd related traps from 
--                vismTraps.my to this file."
--
--    REVISION    "199908101400Z"
--    DESCRIPTION "
--                Replaced all "not-accessible" varbinds with
--                vismTrapIntIndex1 & vismTrapIntIndex2"
--
--    REVISION    "200107191500Z"
--    DESCRIPTION "
--                Added traps when an entry is implicitly added or removed
--                in mgcRedundancyGrpParamTable or srcpPeerGrpParamTable"
--
--    REVISION    "200202111500Z"
--    DESCRIPTION "RSVP Compilation problem fix."
--                

VISM-NONAXIPOP-TRAPS-MIB DEFINITIONS ::= BEGIN

IMPORTS
        TRAP-TYPE                       FROM RFC-1215
        lastSequenceNumber              FROM RTM-MIB
        shelfNodeName,
        shelfNum                       FROM BASIS-SHELF-MIB

        moduleSlotNumber,
        moduleTrapAlarmSeverity,
        functionModuleType             FROM BASIS-GENERIC-MIB

        genericTimeStamp               FROM GENERICOBJECT-MIB
        mgAdministrativeState,
        mgcName,
        mgcAssociationState,
        mgcResolutionCommState,
        mgEndpointLineNumber,
        mgEndpointChannelMap,
        mgEndpointState,
        mgDomainName,
        mgDnsResolutionType,
        mgcResolutionName,
        mgcResolutionIpAddress         FROM CISCO-WAN-MG-MIB
        mgcRedundancyGrpActState       FROM CISCO-WAN-MGC-REDUN-MIB    
        srcpPeerGrpTimeSinceHeartbeat,
        srcpPeerTimeSinceHeartbeat         FROM CISCO-WAN-SRCP-MIB
        vismLapdIndex,
        vismLapdDlcIndex,
        vismLapdDlcSapi,
        vismLapdDlcTei,
        vismLapdDlcLinkState            FROM VISM-LAPD-MIB
        vismSessionSetNum,
        vismSessionSetState,
        vismSessionGrpNum,
        vismRudpSessionNum,
        vismRudpSessionState,
        vismSessionGrpState            FROM VISM-SESSION-MIB
        vismTrapIntIndex1,
        vismTrapIntIndex2              FROM CISCO-WAN-AXIPOP-MIB
        persistentXgcpEventName        FROM CISCO-WAN-PERSISTENT-XGCP-EVENTS-MIB
        tonePlanRegionName,
        tonePlanVersionNumber,
        tonePlanFileName               FROM CISCO-WAN-VISM-TONE-PLAN-MIB
        vismLapdTrunkState             FROM CISCO-WAN-LAPD-TRUNK-MIB
        vismRtpConnAlarmState,        
        vismRtpLcn,
        vismRtpFailReason              FROM CISCO-WAN-RTP-CONN-MIB
        cwAnnFileName,
        cwAnnFileCodec,
        cwAnnFileStatus                FROM CISCO-WAN-ANNOUNCEMENT-MIB
        basis                          FROM BASIS-MIB
        intSrvFlowStatus               FROM INT-SERV-MIB
        rsvpSessionDestAddr,
        rsvpResvFwdStatus,
        rsvpResvStatus,
        rsvpSenderStatus              FROM RSVP-MIB;





mgAdministrativeStateLocked     TRAP-TYPE
        ENTERPRISE      basis
        VARIABLES       { 
                        lastSequenceNumber,
                        shelfNodeName,
                        shelfNum,
                        moduleSlotNumber,
                        moduleTrapAlarmSeverity,
			functionModuleType,
			genericTimeStamp,
                        mgAdministrativeState
                        }
 
        DESCRIPTION
            "Triggered when mgAdministrativeState transitions to locked.
             The transition may be from unlocked to locked or from 
             shuttingdown to locked.
	    "
        ::= 50719

mgAdministrativeStateUnlocked     TRAP-TYPE
        ENTERPRISE      basis
        VARIABLES       { 
                        lastSequenceNumber,
                        shelfNodeName,
                        shelfNum,
                        moduleSlotNumber,
                        moduleTrapAlarmSeverity,
			functionModuleType,
			genericTimeStamp,
                        mgAdministrativeState
                        }
 
        DESCRIPTION
            "Triggered when mgAdministrativeState transitions from locked
             to unlocked.
	    "
        ::= 50720


mgAdministrativeStateShuttingDown     TRAP-TYPE
