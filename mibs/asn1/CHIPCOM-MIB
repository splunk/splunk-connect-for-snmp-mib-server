	SYNTAX IpAddress
	ACCESS read-only
	STATUS mandatory
	DESCRIPTION
		"The IP Address associated with this port."
	::= { ol51nnBEEPortEntry 3 }

ol51nnBEEPortDipNetwork OBJECT-TYPE
	SYNTAX INTEGER {
		isolated(2),
		front-panel(5),
		ethernet-1(6),
		ethernet-2(7),
		ethernet-3(8)
		}
	ACCESS read-only
	STATUS mandatory
	DESCRIPTION
		"The network for this port as specified by this port's
		dip switches."
	::= { ol51nnBEEPortEntry 4 }

