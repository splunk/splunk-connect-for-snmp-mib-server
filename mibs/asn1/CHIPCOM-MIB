    SYNTAX Counter
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
	"The total number of non-MAC frames received that were between
	512 and 1023 octets in length inclusive."
    ::= { olTRTrafEntry 14 }

olTRTrafFrames1024to2047Octets OBJECT-TYPE
    SYNTAX Counter
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
	"The total number of non-MAC frames received that were between
	1024 and 2047 octets in length inclusive."
    ::= { olTRTrafEntry 15 }

olTRTrafFrames2048to4095Octets OBJECT-TYPE
    SYNTAX Counter
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
	"The total number of non-MAC frames received that were between
	2048 and 4095 octets in length inclusive."
    ::= { olTRTrafEntry 16 }

olTRTrafFrames4096to8191Octets OBJECT-TYPE
    SYNTAX Counter
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
