	SYNTAX INTEGER (1..16)
	ACCESS read-only
	STATUS mandatory
	DESCRIPTION
                "Up stream slot (receve from)"
	::= { ol53nnMMGTModEntry 10 }

ol53nnMMGTModDownStreamOperPath OBJECT-TYPE
	SYNTAX INTEGER {
                isolated(2),
                fddi-path-1(24),
                fddi-path-2(25),
                fddi-path-3(26),
                fddi-path-4(27),
                fddi-path-5(28),
