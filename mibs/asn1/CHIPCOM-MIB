			INTEGER,
		ol51nnMFPPortBuddySlot
			INTEGER,
		ol51nnMFPPortBuddyPort
			INTEGER,
		ol51nnMFPPortDipAdminState
			INTEGER,
		ol51nnMFPPortDipNetwork
			INTEGER,
		ol51nnMFPPortLLW
			INTEGER,
		ol51nnMFPPortDipLLW
			INTEGER,
		ol51nnMFPPortHipwr
			INTEGER,
		ol51nnMFPPortDipHipwr
			INTEGER
		}

