	SYNTAX  SEQUENCE OF FsALCEntry
    	ACCESS  not-accessible
     	STATUS  mandatory
     	DESCRIPTION  ""
     	::= { fsALCStats 1 }

   	FsALCEntry ::= SEQUENCE {
      	fsALCPortIndex          INTEGER,
		fsALCResToPoll          INTEGER,
		fsALCNumPolls           Counter,
		fsALCResTimeOut         Counter,
		fsALCFrameRec           Counter,
		fsALCFrameTrans         Counter,
		fsALCCCCErrors          Counter,
		fsALCStreamDetect       INTEGER,
		fsALCAttemPollWhileRTS  Counter,
		fsALCLargeFrameRec      Counter
  		}

     	fsALCEntry OBJECT-TYPE
       	SYNTAX  FsALCEntry
            ACCESS  not-accessible
            STATUS  mandatory
            DESCRIPTION ""
            INDEX { fsALCPortIndex }
            ::= { fsALCTable 1 }

	fsALCPortIndex OBJECT-TYPE
		SYNTAX  INTEGER {
			portN		(1),
			port1       (2),
			port2       (3),
