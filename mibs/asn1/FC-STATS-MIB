		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"The SDLC Port for this switching entry."
		::= { fsSnaSdlcFrStatsEntry 6 }
 
	fsSnaSdlcFrStatsSdlcStnAddr OBJECT-TYPE
		SYNTAX  DisplayString (SIZE(2))
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"The SDLC Station Address for this switching entry."
		::= { fsSnaSdlcFrStatsEntry 7 }

	fsSnaSdlcFrStatsFrPortNum OBJECT-TYPE
		SYNTAX  INTEGER
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"The Frame Relay Port for this switching entry."
		::= { fsSnaSdlcFrStatsEntry 8 }

	fsSnaSdlcFrStatsFrCircuit OBJECT-TYPE
		SYNTAX  INTEGER
		ACCESS  read-only
