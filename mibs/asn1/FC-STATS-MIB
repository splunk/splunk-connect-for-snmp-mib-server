IMPORTS
	fsStats
		FROM FC-FASTC-MIB
	Counter, IpAddress
		FROM RFC1155-SMI
	OBJECT-TYPE
		FROM RFC-1212
	DisplayString FROM SNMPv2-TC;

fsFrameRelayStats             OBJECT IDENTIFIER ::= { fsStats 1 }
fsAsyncEncapsulationStats     OBJECT IDENTIFIER ::= { fsStats 2 }
fsBitSyncEncapsulationStats   OBJECT IDENTIFIER ::= { fsStats 3 }
fsSlipStats                   OBJECT IDENTIFIER ::= { fsStats 6 }
fsRipRoutingStats			OBJECT IDENTIFIER ::= { fsStats 8 }
fsAsyncBurosEmulationStats    OBJECT IDENTIFIER ::= { fsStats 9 }
fsSyncBurosEmulationStats     OBJECT IDENTIFIER ::= { fsStats 10 }
fsSdlcRoutingStats            OBJECT IDENTIFIER ::= { fsStats 11 }
fsByteSyncEncapsulationStats  OBJECT IDENTIFIER ::= { fsStats 12 }
fsTelnetTerminalStats         OBJECT IDENTIFIER ::= { fsStats 13 }
fsIpxStats                    OBJECT IDENTIFIER ::= { fsStats 15 }
fsALCStats                    OBJECT IDENTIFIER ::= { fsStats 17 }
fsTokenStats                  OBJECT IDENTIFIER ::= { fsStats 18 }    
fsEtherStats			OBJECT IDENTIFIER ::= { fsStats 20 }               
fsIPStats                     OBJECT IDENTIFIER ::= { fsStats 30 }
fsICMPStats                   OBJECT IDENTIFIER ::= { fsStats 31 }
fsUDPStats                    OBJECT IDENTIFIER ::= { fsStats 32 }
fsTCPStats                    OBJECT IDENTIFIER ::= { fsStats 33 }
fsSNMPStats                   OBJECT IDENTIFIER ::= { fsStats 34 }
fsNetLoaderStats              OBJECT IDENTIFIER ::= { fsStats 35 }
fsPriorityStats               OBJECT IDENTIFIER ::= { fsStats 38 }
fsSdlc2Stats              	OBJECT IDENTIFIER ::= { fsStats 39 }
fsLlcStats                	OBJECT IDENTIFIER ::= { fsStats 40 }
fsSnaSwStats               	OBJECT IDENTIFIER ::= { fsStats 41 }
fsT1DsuStats			OBJECT IDENTIFIER ::= { fsStats 43 }
fsE1DsuStats			OBJECT IDENTIFIER ::= { fsStats 44 }
fsX25NewStats                 OBJECT IDENTIFIER ::= { fsStats 46 }
fsXPadStats                   OBJECT IDENTIFIER ::= { fsStats 47 }
fsISDNStats				OBJECT IDENTIFIER ::= { fsStats 51 }
fsIpFilteringStats		OBJECT IDENTIFIER ::= { fsStats 52 }
fsNewPPPStats                 OBJECT IDENTIFIER ::= { fsStats 53 }
fsMLPStats                    OBJECT IDENTIFIER ::= { fsStats 54 }
fsTrigStats                   OBJECT IDENTIFIER ::= { fsStats 55 }
fsVoiceStats                  OBJECT IDENTIFIER ::= { fsStats 56 }

-- ***************************************************************************
-- Frame Relay Stats

fsFrameRelayTable OBJECT-TYPE
	SYNTAX  SEQUENCE OF FsFrameRelayEntry
	ACCESS  not-accessible
	STATUS  mandatory
	DESCRIPTION  ""
	::= { fsFrameRelayStats 1 }

	FsFrameRelayEntry ::= SEQUENCE {
		fsFrameRelayPortIndex               INTEGER,
		fsFrameRelayMngtProtocol	 	DisplayString,
		fsFrameRelayMngtStatus          	INTEGER,
		fsFrameRelayTotalFramesRx       	Counter,
		fsFrameRelayTotalFramesTx       	Counter,
		fsFrameRelayMngtFramesRx        	Counter,
		fsFrameRelayMngtFramesRxError   	Counter,
		fsFrameRelayMngtFramesTx        	Counter,
		fsFrameRelayRxNonRoutableIP     	Counter,
		fsFrameRelayRxNonRoutableIPX    	Counter,
		fsFrameRelayRxNonRoutableEncaps 	Counter,
		fsFrameRelayRxUnknownFrameType  	Counter,
		fsFrameRelayRxInactiveDLCIs     	Counter,
		fsFrameRelayTxInactiveDLCIs     	Counter,
		fsFrameRelayLastInvalidDLCI     	INTEGER
		}

	fsFrameRelayEntry OBJECT-TYPE
		SYNTAX  FsFrameRelayEntry
		ACCESS  not-accessible
		STATUS  mandatory
		DESCRIPTION ""
		INDEX { fsFrameRelayPortIndex }
		::= { fsFrameRelayTable 1 }

	fsFrameRelayPortIndex OBJECT-TYPE
		SYNTAX  INTEGER {
			portN		(1),
			port1       (2),
			port2       (3),
			port3       (4),
			port4       (5),
			port10      (10),
			port11	(11),
			port12      (12),
			port13      (13),
			port14      (14),
			port15      (15),
			port16	(16),
			port17      (17),
			port18      (18),
			port19      (19),
                  port20      (20),
                  port21      (21),
                  port22      (22),
                  port23      (23),
                  port24      (24),
                  port25      (25),
                  port26      (26),
                  port27      (27),
                  port28      (28),
                  port29      (29)
			}
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION ""
		::= { fsFrameRelayEntry 1 }

	fsFrameRelayMngtProtocol OBJECT-TYPE
		SYNTAX  DisplayString (SIZE (0..25))
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION "Management protocol selected for Frame Relay."
		::= { fsFrameRelayEntry 2 }

	fsFrameRelayMngtStatus OBJECT-TYPE
		SYNTAX  INTEGER {
			none                    (0),
			receivingPolls       	(1),
			notReceivingPolls    	(2),
			respondingToPolls    	(3),
			notRespondingToPolls 	(4)
			}
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION "Status of the Frame Relay port. In its default 
			       state as Frame Relay User, the FRAD initates 
				 polling to the switch, and the switch responds. 
				 Receiving Polls means that the FRAD is receiving 
				 a response from the switch.  If the FRAD has been 
				 configured as a switch Frame Relay Network, it 
				 should be responding To Polls from a user device."
		::= { fsFrameRelayEntry 3 }

	fsFrameRelayTotalFramesRx OBJECT-TYPE
		SYNTAX  Counter
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION "Total Number of incoming DATA frames on 
				 the Frame Relay port."
		::= { fsFrameRelayEntry 4 }

	fsFrameRelayTotalFramesTx OBJECT-TYPE
		SYNTAX  Counter
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION "Total Number of outgoing DATA frames on the 
			       Frame Relay port. This count excludes management 
				 frames."
		::= { fsFrameRelayEntry 5 }

	fsFrameRelayMngtFramesRx OBJECT-TYPE
		SYNTAX  Counter
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION "Total number of Frame Relay management frames 
				 received."
		::= { fsFrameRelayEntry 6 }

	fsFrameRelayMngtFramesRxError OBJECT-TYPE
		SYNTAX  Counter
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION "Total number of Frame Relay management frames 
				 received in error."
		::= { fsFrameRelayEntry 7 }

	fsFrameRelayMngtFramesTx OBJECT-TYPE
		SYNTAX  Counter
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION "Total number of Frame Relay management frames sent."
		::= { fsFrameRelayEntry 8 }

	fsFrameRelayRxNonRoutableIP OBJECT-TYPE
		SYNTAX  Counter
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION "IP frames received and discarded because their 
				 route could not be determined."
		::= { fsFrameRelayEntry 9 }

	fsFrameRelayRxNonRoutableIPX OBJECT-TYPE
		SYNTAX  Counter
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION "IPX frames received and discarded because their 
				 route could not be determined."
		::= { fsFrameRelayEntry 10 }

	fsFrameRelayRxNonRoutableEncaps OBJECT-TYPE
		SYNTAX  Counter
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION "Frames received on an Encapsulation DLCI and 
				 discarded because the destination port is not 
				 configured for that protocol."
		::= { fsFrameRelayEntry 11 }

	fsFrameRelayRxUnknownFrameType OBJECT-TYPE
		SYNTAX  Counter
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION "Frames received and discarded because the 
				 type of protocol was unknown."
		::= { fsFrameRelayEntry 12 }

	fsFrameRelayRxInactiveDLCIs OBJECT-TYPE
		SYNTAX  Counter
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION "Frames received and discarded because the 
				 DLCIs were not configured in the FRAD."
		::= { fsFrameRelayEntry 13 }

	fsFrameRelayTxInactiveDLCIs OBJECT-TYPE
		SYNTAX  Counter
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION "Number of transmit frames discarded because 
				 a device attached to the FRAD attempted to send 
				 data using a DLCI that was not assigned by the 
				 carrier to the circuit, or it is inactive."
		::= { fsFrameRelayEntry 14 }

	fsFrameRelayLastInvalidDLCI OBJECT-TYPE
		SYNTAX  INTEGER (16..1007)
		ACCESS  read-write
		STATUS  mandatory
		DESCRIPTION "Last received DLCI that is not configured on this FRAD."
		::= { fsFrameRelayEntry 15 }


-- ***************************************************************************
-- Async Encapsulation Stats

fsAsyncEncapsTable OBJECT-TYPE
	SYNTAX  SEQUENCE OF FsAsyncEncapsEntry
	ACCESS  not-accessible
	STATUS  mandatory
	DESCRIPTION  ""
	::= { fsAsyncEncapsulationStats 1 }

	FsAsyncEncapsEntry ::= SEQUENCE {
		fsAsyncEncapsPortIndex        INTEGER,
		fsAsyncEncapsRxBytes          Counter,
		fsAsyncEncapsTxBytes          Counter,
		fsAsyncEncapsRxBreak          Counter,
		fsAsyncEncapsRxParityErr      Counter,
		fsAsyncEncapsRxOverrun        Counter,
		fsAsyncEncapsRxFramingErr     Counter
		}

	fsAsyncEncapsEntry OBJECT-TYPE
		SYNTAX  FsAsyncEncapsEntry
		ACCESS  not-accessible
		STATUS  mandatory
		DESCRIPTION ""
		INDEX { fsAsyncEncapsPortIndex }
		::= { fsAsyncEncapsTable 1 }

	fsAsyncEncapsPortIndex OBJECT-TYPE
		SYNTAX  INTEGER {
			portN		(1),
			port1       (2),
			port2       (3),
			port3       (4),
			port4       (5),
			port10      (10),
			port11	(11),
			port12      (12),
			port13      (13),
			port14      (14),
			port15      (15),
			port16	(16),
			port17      (17),
			port18      (18),
			port19      (19),
                  port20      (20),
                  port21      (21),
                  port22      (22),
                  port23      (23),
                  port24      (24),
                  port25      (25),
                  port26      (26),
                  port27      (27),
                  port28      (28),
                  port29      (29)
			}
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION ""
		::= { fsAsyncEncapsEntry 1 }

	fsAsyncEncapsRxBytes OBJECT-TYPE
		SYNTAX  Counter
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION "Number of incoming bytes."
		::= { fsAsyncEncapsEntry 2 }

	fsAsyncEncapsTxBytes OBJECT-TYPE
		SYNTAX  Counter
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION "Number of outgoing bytes"
		::= { fsAsyncEncapsEntry 3 }

	fsAsyncEncapsRxBreak OBJECT-TYPE
		SYNTAX  Counter
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION "Number of break characters received."
		::= { fsAsyncEncapsEntry 4 }

	fsAsyncEncapsRxParityErr OBJECT-TYPE
		SYNTAX  Counter
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION "Received characters with parity errors."
		::= { fsAsyncEncapsEntry 5 }

	fsAsyncEncapsRxOverrun OBJECT-TYPE
		SYNTAX  Counter
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION "Count of overrun errors on the receive port."
		::= { fsAsyncEncapsEntry 6 }

	fsAsyncEncapsRxFramingErr OBJECT-TYPE
		SYNTAX  Counter
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION "Number of characters received with framing errors."
		::= { fsAsyncEncapsEntry 7 }


-- ***************************************************************************
-- Bit Synchronous Encapsulation Stats

fsBitSyncEncapsTable OBJECT-TYPE
	SYNTAX  SEQUENCE OF FsBitSyncEncapsEntry
	ACCESS  not-accessible
	STATUS  mandatory
	DESCRIPTION  ""
	::= { fsBitSyncEncapsulationStats 1 }

	FsBitSyncEncapsEntry ::= SEQUENCE {
		fsBitSyncEncapsPortIndex      INTEGER,
		fsBitSyncEncapsRxFrames       Counter,
		fsBitSyncEncapsTxFrames       Counter,
		fsBitSyncEncapsRxTooBig       Counter,
		fsBitSyncEncapsRxCrcError     Counter,
		fsBitSyncEncapsRxOverrun      Counter,
		fsBitSyncEncapsRxNonAligned   Counter,
		fsBitSyncEncapsRxAbort        Counter,
		fsBitSyncEncapsTxUnderrun     Counter
		}

	fsBitSyncEncapsEntry OBJECT-TYPE
		SYNTAX  FsBitSyncEncapsEntry
		ACCESS  not-accessible
		STATUS  mandatory
		DESCRIPTION ""
		INDEX { fsBitSyncEncapsPortIndex }
		::= { fsBitSyncEncapsTable 1 }

	fsBitSyncEncapsPortIndex OBJECT-TYPE
		SYNTAX  INTEGER {
			portN		(1),
			port1       (2),
			port2       (3),
			port3       (4),
			port4       (5),
			port10      (10),
			port11	(11),
			port12      (12),
			port13      (13),
			port14      (14),
			port15      (15),
			port16	(16),
			port17      (17),
			port18      (18),
			port19      (19),
                  port20      (20),
                  port21      (21),
                  port22      (22),
                  port23      (23),
                  port24      (24),
                  port25      (25),
                  port26      (26),
                  port27      (27),
                  port28      (28),
                  port29      (29)
			}
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION ""
		::= { fsBitSyncEncapsEntry 1 }

	fsBitSyncEncapsRxFrames OBJECT-TYPE
		SYNTAX  Counter
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION "Number of incoming frames."
		::= { fsBitSyncEncapsEntry 2 }

	fsBitSyncEncapsTxFrames OBJECT-TYPE
		SYNTAX  Counter
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION "Number of outgoing frames."
		::= { fsBitSyncEncapsEntry 3 }

	fsBitSyncEncapsRxTooBig OBJECT-TYPE
		SYNTAX  Counter
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION "Frame length greater than maximum allowed (4K bytes)."
		::= { fsBitSyncEncapsEntry 4 }

	fsBitSyncEncapsRxCrcError OBJECT-TYPE
		SYNTAX  Counter
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION "Receiver Cyclic Redundancy Check errors."
		::= { fsBitSyncEncapsEntry 5 }

	fsBitSyncEncapsRxOverrun OBJECT-TYPE
		SYNTAX  Counter
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION "Count of overrun errors occurred on the receive port."
		::= { fsBitSyncEncapsEntry 6 }

	fsBitSyncEncapsRxNonAligned OBJECT-TYPE
		SYNTAX  Counter
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION "NonOctet Aligned frames received."
		::= { fsBitSyncEncapsEntry 7 }

	fsBitSyncEncapsRxAbort OBJECT-TYPE
		SYNTAX  Counter
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION "Count of Receiver Abort messages received."
		::= { fsBitSyncEncapsEntry 8 }

	fsBitSyncEncapsTxUnderrun OBJECT-TYPE
		SYNTAX  Counter
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION "Number of transmit underrun errors."
		::= { fsBitSyncEncapsEntry 9 }

-- ***************************************************************************
-- SLIP Stats

fsSlipTable OBJECT-TYPE
	SYNTAX  SEQUENCE OF FsSlipEntry
	ACCESS  not-accessible
	STATUS  mandatory
	DESCRIPTION  ""
	::= { fsSlipStats 1 }

	FsSlipEntry ::= SEQUENCE {
		fsSlipPortIndex           INTEGER,
		fsSlipRxFrames            Counter,
		fsSlipTxFrames            Counter,
		fsSlipRxNonRoutableFrames Counter,
		fsSlipRxBreak             Counter,
		fsSlipRxParityErr         Counter,
		fsSlipRxOverrun           Counter,
		fsSlipRxFramingErr        Counter
		}

	fsSlipEntry OBJECT-TYPE
		SYNTAX  FsSlipEntry
		ACCESS  not-accessible
		STATUS  mandatory
		DESCRIPTION ""
		INDEX { fsSlipPortIndex }
		::= { fsSlipTable 1 }

	fsSlipPortIndex OBJECT-TYPE
		SYNTAX  INTEGER {
			portN		(1),
			port1       (2),
			port2       (3),
			port3       (4),
			port4       (5),
			port10      (10),
			port11	(11),
			port12      (12),
			port13      (13),
			port14      (14),
			port15      (15),
			port16	(16),
			port17      (17),
			port18      (18),
			port19      (19),
                  port20      (20),
                  port21      (21),
                  port22      (22),
                  port23      (23),
                  port24      (24),
                  port25      (25),
                  port26      (26),
                  port27      (27),
                  port28      (28),
                  port29      (29)
			}
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION "SLIP port number."
		::= { fsSlipEntry 1 }

	fsSlipRxFrames OBJECT-TYPE
		SYNTAX  Counter
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION "Total number of frames received."
		::= { fsSlipEntry 2 }

	fsSlipTxFrames OBJECT-TYPE
		SYNTAX  Counter
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION "Total number of frames sent."
		::= { fsSlipEntry 3 }

	fsSlipRxNonRoutableFrames OBJECT-TYPE
		SYNTAX  Counter
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION "This is the total number of frames discarded 
				 because their routing could not be determined."
		::= { fsSlipEntry 4 }

	fsSlipRxBreak OBJECT-TYPE
		SYNTAX  Counter
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION "Number of Break characters received."
		::= { fsSlipEntry 5 }

	fsSlipRxParityErr OBJECT-TYPE
		SYNTAX  Counter
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION "Total number of parity errors received."
		::= { fsSlipEntry 6 }

	fsSlipRxOverrun OBJECT-TYPE
		SYNTAX  Counter
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION "Total number of receiver overruns."
		::= { fsSlipEntry 7 }

	fsSlipRxFramingErr OBJECT-TYPE
		SYNTAX  Counter
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION "Total number of framing errors."
		::= { fsSlipEntry 8 }

-- ***************************************************************************
-- Rip Routing Stats

fsRipRoutingTable OBJECT-TYPE
	SYNTAX  SEQUENCE OF FsRipRoutingEntry
	ACCESS  not-accessible
	STATUS  mandatory
	DESCRIPTION  ""
	::= { fsRipRoutingStats 1 }

	FsRipRoutingEntry ::= SEQUENCE {
		fsRipRoutingTableIndex        INTEGER,
		fsRipRoutingNetwork           IpAddress,
	  	fsRipRoutingMask              IpAddress,
		fsRipRoutingGateway           IpAddress,
		fsRipRoutingHops              INTEGER,
    		fsRipRoutingTimer             INTEGER,
		fsRipRoutingPort              DisplayString,
		fsRipRoutingDlci              INTEGER
		}

	fsRipRoutingEntry OBJECT-TYPE
		SYNTAX  FsRipRoutingEntry
		ACCESS  not-accessible
		STATUS  mandatory
		DESCRIPTION ""
		INDEX { fsRipRoutingTableIndex }
		::= { fsRipRoutingTable 1 }

	fsRipRoutingTableIndex OBJECT-TYPE
		SYNTAX  INTEGER 
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION ""
		::= { fsRipRoutingEntry 1 }

	fsRipRoutingNetwork OBJECT-TYPE
		SYNTAX  IpAddress 
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION "Destination Network address."
		::= { fsRipRoutingEntry 2 }

	fsRipRoutingMask OBJECT-TYPE
		SYNTAX  IpAddress 
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION "Subnet mask for destination network."
		::= { fsRipRoutingEntry 3 }

	fsRipRoutingGateway OBJECT-TYPE
		SYNTAX  IpAddress 
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION "IP Address of next hop gateway (all 0s indicates 
				 a network directly attached to the FRAD)."
		::= { fsRipRoutingEntry 4 }

	fsRipRoutingHops OBJECT-TYPE
		SYNTAX  INTEGER 
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION "Number of links from the FRAD to the network."
		::= { fsRipRoutingEntry 5 }

	fsRipRoutingTimer OBJECT-TYPE
		SYNTAX  INTEGER 
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION "Maximum number of hops."
		::= { fsRipRoutingEntry 6 }

	fsRipRoutingPort OBJECT-TYPE
		SYNTAX  DisplayString (SIZE (1..8))
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION "Local Port on the FRAD being used to access network."
		::= { fsRipRoutingEntry 7 }

	fsRipRoutingDlci OBJECT-TYPE
		SYNTAX  INTEGER (16..1007)
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION "For Frame Relay port, the DLCI to get to the network."
		::= { fsRipRoutingEntry 8 }


-- ***************************************************************************
-- Async Buros Emulation Stats

fsAsyncBurosTable OBJECT-TYPE
	SYNTAX  SEQUENCE OF FsAsyncBurosEntry
	ACCESS  not-accessible
	STATUS  mandatory
	DESCRIPTION  ""
	::= { fsAsyncBurosEmulationStats 1 }

	FsAsyncBurosEntry ::= SEQUENCE {
		fsAsyncBurosPortIndex           INTEGER,
		fsAsyncBurosRxNoSession         Counter,
		fsAsyncBurosTxTimeouts          Counter,
		fsAsyncBurosRxBreak             Counter,
		fsAsyncBurosRxParityErr         Counter,
		fsAsyncBurosRxOverrun           Counter,
		fsAsyncBurosRxFramingErr        Counter
		}

	fsAsyncBurosEntry OBJECT-TYPE
		SYNTAX  FsAsyncBurosEntry
		ACCESS  not-accessible
		STATUS  mandatory
		DESCRIPTION ""
		INDEX { fsAsyncBurosPortIndex }
		::= { fsAsyncBurosTable 1 }

	fsAsyncBurosPortIndex OBJECT-TYPE
		SYNTAX  INTEGER {
			portN		(1),
			port1       (2),
			port2       (3),
			port3       (4),
			port4       (5),
			port10      (10),
			port11	(11),
			port12      (12),
			port13      (13),
			port14      (14),
			port15      (15),
			port16	(16),
			port17      (17),
			port18      (18),
			port19      (19),
                  port20      (20),
                  port21      (21),
                  port22      (22),
                  port23      (23),
                  port24      (24),
                  port25      (25),
                  port26      (26),
                  port27      (27),
                  port28      (28),
                  port29      (29)
			}
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION ""
		::= { fsAsyncBurosEntry 1 }

	fsAsyncBurosRxNoSession OBJECT-TYPE
		SYNTAX  Counter
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION "Received frames discarded due to lack of connection."
		::= { fsAsyncBurosEntry 2 }

	fsAsyncBurosTxTimeouts OBJECT-TYPE
		SYNTAX  Counter
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION "Transmitter timeouts issued because device is not responding."
		::= { fsAsyncBurosEntry 3 }

	fsAsyncBurosRxBreak OBJECT-TYPE
		SYNTAX  Counter
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION "Number of break characters received."
		::= { fsAsyncBurosEntry 4 }

	fsAsyncBurosRxParityErr OBJECT-TYPE
		SYNTAX  Counter
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION "Received characters with parity errors."
		::= { fsAsyncBurosEntry 5 }

	fsAsyncBurosRxOverrun OBJECT-TYPE
		SYNTAX  Counter
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION "Count of overrun errors on the receive port."
		::= { fsAsyncBurosEntry 6 }

	fsAsyncBurosRxFramingErr OBJECT-TYPE
		SYNTAX  Counter
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION "Number of characters received with framing errors."
		::= { fsAsyncBurosEntry 7 }

