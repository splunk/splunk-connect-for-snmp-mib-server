-- Channel Echo Stats

fsChanEchoStats OBJECT IDENTIFIER ::= { fsVoiceStats 9 }

fsChanEchoStatsTable OBJECT-TYPE
	SYNTAX  SEQUENCE OF FsChanEchoStatsEntry
	ACCESS  not-accessible
	STATUS  mandatory
	DESCRIPTION  ""
	::= { fsChanEchoStats 1 }

	FsChanEchoStatsEntry ::= SEQUENCE {
		fsChanEchoIndex		INTEGER,
            fsEchoCNVState          INTEGER,
            fsEchoPxLevel           INTEGER,
            fsEchoPyLevel           INTEGER,
            fsEchoPeLevel           INTEGER,
            fsEchoPostUpdateCnt     Counter,
            fsEchoAttempUpdateCnt   Counter,
            fsEchoDivergeCnt        Counter,
            fsEchoERLEBypassCnt     Counter,
            fsEchoXTONECnt          Counter,
            fsEchoForeSwitchCnt     Counter,
            fsEchoBackSwitchCnt     Counter,
            fsEchoXIDLECnt          Counter,
            fsEchoOtherBypassCnt    Counter
		}

	fsChanEchoStatsEntry OBJECT-TYPE
		SYNTAX  FsChanEchoStatsEntry
		ACCESS  not-accessible
		STATUS  mandatory
		DESCRIPTION ""
		INDEX { fsChanEchoIndex }
		::= { fsChanEchoStatsTable 1 }

	fsChanEchoIndex OBJECT-TYPE
		SYNTAX  INTEGER 
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION ""
		::= { fsChanEchoStatsEntry 1 }

	fsEchoCNVState OBJECT-TYPE
		SYNTAX  INTEGER 
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION ""
		::= { fsChanEchoStatsEntry 2 }

	fsEchoPxLevel OBJECT-TYPE
		SYNTAX  INTEGER 
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION ""
		::= { fsChanEchoStatsEntry 3 }

	fsEchoPyLevel OBJECT-TYPE
		SYNTAX  INTEGER 
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION ""
		::= { fsChanEchoStatsEntry 4 }

	fsEchoPeLevel OBJECT-TYPE
		SYNTAX  INTEGER 
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION ""
		::= { fsChanEchoStatsEntry 5 }

	fsEchoPostUpdateCnt OBJECT-TYPE
		SYNTAX  Counter 
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION ""
		::= { fsChanEchoStatsEntry 6 }

	fsEchoAttempUpdateCnt OBJECT-TYPE
		SYNTAX  Counter 
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION ""
		::= { fsChanEchoStatsEntry 7 }

	fsEchoDivergeCnt OBJECT-TYPE
		SYNTAX  Counter 
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION ""
		::= { fsChanEchoStatsEntry 8 }

	fsEchoERLEBypassCnt OBJECT-TYPE
		SYNTAX  Counter 
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION ""
		::= { fsChanEchoStatsEntry 9 }

	fsEchoXTONECnt OBJECT-TYPE
		SYNTAX  Counter 
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION ""
		::= { fsChanEchoStatsEntry 10 }

	fsEchoForeSwitchCnt OBJECT-TYPE
		SYNTAX  Counter 
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION ""
		::= { fsChanEchoStatsEntry 11 }

	fsEchoBackSwitchCnt OBJECT-TYPE
		SYNTAX  Counter 
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION ""
		::= { fsChanEchoStatsEntry 12 }

	fsEchoXIDLECnt OBJECT-TYPE
		SYNTAX  Counter 
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION ""
		::= { fsChanEchoStatsEntry 13 }

	fsEchoOtherBypassCnt OBJECT-TYPE
		SYNTAX  Counter 
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION ""
		::= { fsChanEchoStatsEntry 14 }

-- ***************************************************************************
-- CDR Stats

fsCdrStats OBJECT IDENTIFIER ::= { fsVoiceStats 10 }

fsCdrStatsTable OBJECT-TYPE
	SYNTAX  SEQUENCE OF FsCdrStatsEntry
	ACCESS  not-accessible
	STATUS  mandatory
	DESCRIPTION  ""
	::= { fsCdrStats 1 }

	FsCdrStatsEntry ::= SEQUENCE {
		fsCdrRecordNo	INTEGER,
		fsCdrBufLeft	DisplayString,
            fsCdrBufRight	DisplayString
		}

	fsCdrStatsEntry OBJECT-TYPE
		SYNTAX  FsCdrStatsEntry
		ACCESS  not-accessible
		STATUS  mandatory
		DESCRIPTION ""
		INDEX { fsCdrRecordNo }
		::= { fsCdrStatsTable 1 }

	fsCdrRecordNo OBJECT-TYPE
		SYNTAX  INTEGER 
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION ""
		::= { fsCdrStatsEntry 1 }

	fsCdrBufLeft OBJECT-TYPE
		SYNTAX  DisplayString
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION ""
		::= { fsCdrStatsEntry 2 }

	fsCdrBufRight OBJECT-TYPE
		SYNTAX  DisplayString
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION ""
		::= { fsCdrStatsEntry 3 }
END

