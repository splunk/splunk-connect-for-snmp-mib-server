-- ***************************************************************************
-- Loader Stats

fsNetLoaderStatsTable OBJECT-TYPE
	SYNTAX  SEQUENCE OF FsNetLoaderStatsTableEntry
	ACCESS  not-accessible
	STATUS  mandatory
	DESCRIPTION  ""
	::= { fsNetLoaderStats 1 }

	FsNetLoaderStatsTableEntry ::= SEQUENCE {
		fsNetLoaderStatsTableIndex    INTEGER,
		fsNetLoaderState              INTEGER,
		fsNetLoaderAttemptTrans       Counter,
		fsNetLoaderSuccConn           Counter,
		fsNetLoaderFailTrans          Counter,
		fsNetLoaderCompleteTrans      Counter,
		fsNetLoaderFileTransState     INTEGER,
		fsNetLoaderByteReceived       Counter,
		fsNetLoaderStartRegAddr       DisplayString,
		fsNetLoaderFlashWriteAddr     DisplayString,
		fsNetLoaderEndRegAddr         DisplayString
		}

	fsNetLoaderStatsTableEntry OBJECT-TYPE	
		SYNTAX  FsNetLoaderStatsTableEntry
		ACCESS  not-accessible
		STATUS  mandatory
		DESCRIPTION ""
		INDEX { fsNetLoaderStatsTableIndex }
		::= { fsNetLoaderStatsTable 1 }

	fsNetLoaderStatsTableIndex OBJECT-TYPE
		SYNTAX  INTEGER
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION ""
		::=  { fsNetLoaderStatsTableEntry 1 }

	fsNetLoaderState OBJECT-TYPE
		SYNTAX  INTEGER {
			idle		(0),
			attempt	(1),
			program	(2)
			}
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION "Current state of the loader (idle, attempt, 
				 programming)."
		::=  { fsNetLoaderStatsTableEntry 2 }

	fsNetLoaderAttemptTrans OBJECT-TYPE
		SYNTAX  Counter
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION "Number of time the loader attempted a transmission."
		::= { fsNetLoaderStatsTableEntry 3 }

	fsNetLoaderSuccConn OBJECT-TYPE
		SYNTAX  Counter
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION "Number of successful connections to the loader."
		::= { fsNetLoaderStatsTableEntry 4 }

	fsNetLoaderFailTrans OBJECT-TYPE
		SYNTAX  Counter
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION "Number of time the transmission of data failed 
				 in the loader."
		::= { fsNetLoaderStatsTableEntry 5 }

	fsNetLoaderCompleteTrans OBJECT-TYPE
		SYNTAX  Counter
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION "Number of time the loader completed."
		::= { fsNetLoaderStatsTableEntry 6 }

	fsNetLoaderFileTransState OBJECT-TYPE
		SYNTAX  INTEGER {
			none              (0),
			active            (1),
			err-region        (2),
			err-parsing       (3),
			err-tftp          (4),
			err-unknown       (5),
			complete          (6)
			}
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION "Current state of the file being loaded (none, 
				 active, err-region, err-parsing, err-tftp, 
				 err-unknown, complete)."
		::=  { fsNetLoaderStatsTableEntry 7 }

	fsNetLoaderByteReceived OBJECT-TYPE
		SYNTAX  Counter
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION "Number of bytes received by the loader."
		::= { fsNetLoaderStatsTableEntry 8 }

	fsNetLoaderStartRegAddr OBJECT-TYPE
		SYNTAX  DisplayString (SIZE (0..8))
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION "Starting register address of the loader."
		::= { fsNetLoaderStatsTableEntry 9 }

	fsNetLoaderFlashWriteAddr OBJECT-TYPE
		SYNTAX  DisplayString (SIZE (0..8))
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION "Flash write address."
		::= { fsNetLoaderStatsTableEntry 10 }

	fsNetLoaderEndRegAddr OBJECT-TYPE
		SYNTAX  DisplayString (SIZE (0..8))
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION "Ending address of the Flash loader."
		::= { fsNetLoaderStatsTableEntry 11 }

