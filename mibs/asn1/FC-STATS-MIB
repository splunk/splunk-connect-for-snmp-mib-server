		fsX25NewCallRecCalledAddress                DisplayString,
		fsX25NewCallRecCallDirection                DisplayString,
		fsX25NewCallRecClearDirection               DisplayString,
		fsX25NewCallRecClearCause                   INTEGER,
		fsX25NewCallRecDiagCode                     INTEGER,
		fsX25NewCallRecDuration                     DisplayString,
		fsX25NewCallRecInPDUs                       Counter,
		fsX25NewCallRecOutPDUs                      Counter
		}

	fsX25NewCallRecStatsEntry OBJECT-TYPE
		SYNTAX  FsX25NewCallRecStatsEntry
		ACCESS  not-accessible
		STATUS  mandatory
		DESCRIPTION ""
		INDEX   { fsX25NewCallRecStatsPort, fsX25NewCallRecStatsCirNum }
		::= { fsX25NewCallRecStatsTable  1 }

	fsX25NewCallRecStatsPort OBJECT-TYPE
		SYNTAX  INTEGER {
			portN		(1),
			port1       (2),
			port2       (3),
			port3       (4),
			port4       (5),
			port10      (10),
			port11	(11),
			port12      (12),
			port13      (13),
			port14      (14),
			port15      (15),
			port16	(16),
			port17      (17),
			port18      (18),
			port19      (19),
                  port20      (20),
                  port21      (21),
                  port22      (22),
                  port23      (23),
                  port24      (24),
                  port25      (25),
                  port26      (26),
                  port27      (27),
                  port28      (28),
                  port29      (29)
			}
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			" " 
		::= { fsX25NewCallRecStatsEntry 1 }

	fsX25NewCallRecStatsCirNum OBJECT-TYPE
		SYNTAX  INTEGER 
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
                      "The value of ifIndex for the port which cleared the
                      circuit that created the entry."
		::= { fsX25NewCallRecStatsEntry  2 }

	fsX25NewCallRecChnl OBJECT-TYPE
      	SYNTAX  INTEGER
            ACCESS  read-only
            STATUS  mandatory
            DESCRIPTION
                      "The channel number for this circuit.
                      The valid values range from 0 to 4095."
            ::= { fsX25NewCallRecStatsEntry 3 }

      fsX25NewCallRecCallingAddress OBJECT-TYPE
            SYNTAX  DisplayString
            ACCESS  read-only
            STATUS  mandatory
            DESCRIPTION
                      "The calling address from the cleared circuit.
                      The value is shown as an ASCII string of length 0-14."
            ::= { fsX25NewCallRecStatsEntry 4 }

      fsX25NewCallRecCalledAddress OBJECT-TYPE
            SYNTAX  DisplayString 
            ACCESS  read-only
            STATUS  mandatory
            DESCRIPTION
                      "The called address from the cleared circuit.
                      The value is shown as an ASCII string of length 0-15."
            ::= { fsX25NewCallRecStatsEntry 5 }

      fsX25NewCallRecCallDirection OBJECT-TYPE
            SYNTAX DisplayString
            ACCESS  read-only
            STATUS  mandatory
            DESCRIPTION
                       "Call initiator of the cleared VC ."
            ::= { fsX25NewCallRecStatsEntry 6 }

      fsX25NewCallRecClearDirection OBJECT-TYPE
            SYNTAX DisplayString
            ACCESS  read-only
            STATUS  mandatory
            DESCRIPTION
                       "End which initiated the clearing of the VC."
            ::= { fsX25NewCallRecStatsEntry 7 }

      fsX25NewCallRecClearCause OBJECT-TYPE
            SYNTAX INTEGER
            ACCESS  read-only
            STATUS  mandatory
            DESCRIPTION
                      "The Clearing Cause from the clear request or clear
                      indication packet that cleared the circuit.

                      The valid values range from 0 to 255."
            ::= { fsX25NewCallRecStatsEntry 8 }

      fsX25NewCallRecDiagCode OBJECT-TYPE
            SYNTAX  INTEGER
            ACCESS  read-only
            STATUS  mandatory
            DESCRIPTION
                      "The Diagnostic Code from the clear request or clear
                      indication packet that cleared the circuit.

                      The valid values range from 0 to 255."
            ::= { fsX25NewCallRecStatsEntry 9 }

      fsX25NewCallRecDuration OBJECT-TYPE
            SYNTAX  DisplayString 
            ACCESS  read-only
            STATUS  mandatory
            DESCRIPTION
                      "The value of sysUpTime when the circuit was cleared.
                      For locally initiated clears, this will be the time
                      when the clear confirmation was received. For remotely
                      initiated clears, this will be the time when the clear
                      indication was received."
            ::= { fsX25NewCallRecStatsEntry 10 }

      fsX25NewCallRecInPDUs OBJECT-TYPE
            SYNTAX  Counter
            ACCESS  read-only
            STATUS  mandatory
            DESCRIPTION
                      "The number of PDUs received on the circuit."
            ::= { fsX25NewCallRecStatsEntry 11 }
	
	fsX25NewCallRecOutPDUs OBJECT-TYPE
            SYNTAX  Counter
            ACCESS  read-only
            STATUS  mandatory
            DESCRIPTION
                      "The number of PDUs transmitted on the circuit."
            ::= { fsX25NewCallRecStatsEntry 12 }

