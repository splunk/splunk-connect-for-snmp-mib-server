fsTrigStatsTable OBJECT-TYPE
	SYNTAX  SEQUENCE OF FsTrigStatsEntry
	ACCESS  not-accessible
	STATUS  mandatory
	DESCRIPTION  ""
	::= { fsTrigStats 1 }

	FsTrigStatsEntry ::= SEQUENCE {
		fsTrigIndex		  INTEGER,
		fsTrigStatus	 	  DisplayString,
		fsTrigNoTriggers          Counter
		}

