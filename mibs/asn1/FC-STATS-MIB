			port4       (5),
			port10      (10),
			port11	(11),
			port12      (12),
			port13      (13),
			port14      (14),
			port15      (15),
			port16	(16),
			port17      (17),
			port18      (18),
			port19      (19),
                  port20      (20),
                  port21      (21),
                  port22      (22),
                  port23      (23),
                  port24      (24),
                  port25      (25),
                  port26      (26),
                  port27      (27),
                  port28      (28),
                  port29      (29)
			}
         	ACCESS  read-only
            STATUS  mandatory
            DESCRIPTION ""
            ::= { fsALCEntry 1 }

     	fsALCResToPoll OBJECT-TYPE
         	SYNTAX  INTEGER {
            	no    (0),
                  yes   (1)
                  }
         	ACCESS  read-only
            STATUS  mandatory
            DESCRIPTION "Is there a response to the poll."
            ::= { fsALCEntry 2 }

    	fsALCNumPolls OBJECT-TYPE
 		SYNTAX  Counter
         	ACCESS  read-only
            STATUS  mandatory
            DESCRIPTION "Number of polls."
            ::= { fsALCEntry 3 }

    	fsALCResTimeOut OBJECT-TYPE
 		SYNTAX  Counter
          	ACCESS  read-only
            STATUS  mandatory
            DESCRIPTION "Number of response timeouts."
            ::= { fsALCEntry 4 }

    	fsALCFrameRec OBJECT-TYPE
 		SYNTAX  Counter
          	ACCESS  read-only
            STATUS  mandatory
            DESCRIPTION "Number of frames received."
            ::= { fsALCEntry 5 }

    	fsALCFrameTrans OBJECT-TYPE
 		SYNTAX  Counter
          	ACCESS  read-only
            STATUS  mandatory
            DESCRIPTION "Number of frames sent."
            ::= { fsALCEntry 6 }

    	fsALCCCCErrors OBJECT-TYPE
 		SYNTAX  Counter
            ACCESS  read-only
            STATUS  mandatory
            DESCRIPTION "Number of ALC CCC errors."
            ::= { fsALCEntry 7 }

   	fsALCStreamDetect OBJECT-TYPE
       	SYNTAX  INTEGER {
            	no    (0),
                  yes   (1)
                  }
            ACCESS  read-only
            STATUS  mandatory
            DESCRIPTION "Is the device streaming."
            ::= { fsALCEntry 8 }

   	fsALCAttemPollWhileRTS OBJECT-TYPE
 		SYNTAX  Counter
            ACCESS  read-only
            STATUS  mandatory
            DESCRIPTION "Streaming indication if yes."
            ::= { fsALCEntry 9 }

   	fsALCLargeFrameRec OBJECT-TYPE
 		SYNTAX  Counter
            ACCESS  read-only
            STATUS  mandatory
            DESCRIPTION "Number of frames received that were too big."
