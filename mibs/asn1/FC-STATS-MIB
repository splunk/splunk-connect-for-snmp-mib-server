-- ***************************************************************************
-- SDLC2 Port Stats

fsSdlc2PortStatsTable OBJECT-TYPE
   	SYNTAX  SEQUENCE OF FsSdlc2PortStatsEntry
    	ACCESS  not-accessible
    	STATUS  mandatory
    	DESCRIPTION
	   	"Each entry in this table contains statistics
	    	for a specific SDLC port."
    	::= { fsSdlc2Stats  1 }

	FsSdlc2PortStatsEntry ::= SEQUENCE {
		fsSdlc2PortIndex                    INTEGER,
		fsSdlc2PortStatsPhysicalFailures    Counter,
		fsSdlc2PortStatsInvalidAddresses    Counter,
	 	fsSdlc2PortStatsDwarfFrames         Counter,
		fsSdlc2PortStatsPollsIn             Counter,
		fsSdlc2PortStatsPollsOut            Counter,
		fsSdlc2PortStatsPollRspsIn          Counter,
		fsSdlc2PortStatsPollRspsOut         Counter,
		fsSdlc2PortStatsLocalBusies         Counter,
		fsSdlc2PortStatsRemoteBusies        Counter,
		fsSdlc2PortStatsIFramesIn           Counter,
		fsSdlc2PortStatsIFramesOut          Counter,
		fsSdlc2PortStatsOctetsIn            Counter,
		fsSdlc2PortStatsOctetsOut           Counter,
		fsSdlc2PortStatsProtocolErrs        Counter, 
		fsSdlc2PortStatsActivityTOs         Counter,
		fsSdlc2PortStatsRetriesExps         Counter,
		fsSdlc2PortStatsRetransmitsIn       Counter,
		fsSdlc2PortStatsRetransmitsOut      Counter
    		}

	fsSdlc2PortStatsEntry OBJECT-TYPE
    		SYNTAX  FsSdlc2PortStatsEntry
    		ACCESS  not-accessible
    		STATUS  mandatory
    		DESCRIPTION
	   		"A list of statistics for an SDLC port."
    		INDEX   { fsSdlc2PortIndex }
    		::= { fsSdlc2PortStatsTable  1 }

	fsSdlc2PortIndex OBJECT-TYPE
		SYNTAX  INTEGER {
			portN		(1),
			port1       (2),
			port2       (3),
			port3       (4),
			port4       (5),
			port10      (10),
			port11	(11),
			port12      (12),
			port13      (13),
			port14      (14),
			port15      (15),
			port16	(16),
			port17      (17),
			port18      (18),
			port19      (19),
                  port20      (20),
                  port21      (21),
                  port22      (22),
                  port23      (23),
                  port24      (24),
                  port25      (25),
                  port26      (26),
                  port27      (27),
                  port28      (28),
                  port29      (29)
			}
    		ACCESS  read-only
    		STATUS  mandatory
    		DESCRIPTION
	   		"This object reflects the total number of times
	    		this port has failed due to its physical media
	    		since port startup.  At port startup time,
	    		this object must be initialized to zero."
    		::= { fsSdlc2PortStatsEntry  1 }

	fsSdlc2PortStatsPhysicalFailures OBJECT-TYPE
    		SYNTAX  Counter
    		ACCESS  read-only
    		STATUS  mandatory
    		DESCRIPTION
	   		"This object reflects the total number of times
	    		this port has failed due to its physical media
	    		since port startup.  At port startup time,
	    		this object must be initialized to zero."
    		::= { fsSdlc2PortStatsEntry  2 }

	fsSdlc2PortStatsInvalidAddresses OBJECT-TYPE
    		SYNTAX  Counter
    		ACCESS  read-only
    		STATUS  mandatory
    		DESCRIPTION
	   		"This object reflects the total number of
	    		frames received by this port with invalid link
	    		station addresses."
    		::= { fsSdlc2PortStatsEntry  3 }

	fsSdlc2PortStatsDwarfFrames OBJECT-TYPE
    		SYNTAX  Counter
    		ACCESS  read-only
    		STATUS  mandatory
    		DESCRIPTION
	   		"This object reflects the total number of
	    		frames received by this port which were
	    		delivered intact by the physical layer but were
	    		too short to be legal."
    		::= { fsSdlc2PortStatsEntry  4 }

	fsSdlc2PortStatsPollsIn OBJECT-TYPE
    		SYNTAX  Counter
    		ACCESS  read-only
    		STATUS  mandatory
    		DESCRIPTION
	   		"This object reflects the total number of polls
	    		received by this port since the port was
	    		created."
    		::= { fsSdlc2PortStatsEntry  5 }

	fsSdlc2PortStatsPollsOut OBJECT-TYPE
    		SYNTAX  Counter
    		ACCESS  read-only
    		STATUS  mandatory
    		DESCRIPTION
	   		"This object reflects the total number of polls
	    		sent by this port since the port was created."
    		::= { fsSdlc2PortStatsEntry  6 }

	fsSdlc2PortStatsPollRspsIn OBJECT-TYPE
    		SYNTAX  Counter
    		ACCESS  read-only
    		STATUS  mandatory
    		DESCRIPTION
	   		"This object reflects the total number of poll
	    		responses received by this port since the port
	    		was created."
    		::= { fsSdlc2PortStatsEntry  7 }

	fsSdlc2PortStatsPollRspsOut OBJECT-TYPE
    		SYNTAX  Counter
    		ACCESS  read-only
    		STATUS  mandatory
    		DESCRIPTION
	   		"This object reflects the total number of poll
	    		responses sent by this port since the port was
	    		created."
    		::= { fsSdlc2PortStatsEntry  8 }

	fsSdlc2PortStatsLocalBusies OBJECT-TYPE
    		SYNTAX  Counter
    		ACCESS  read-only
    		STATUS  mandatory
    		DESCRIPTION
	   		"This object reflects the total number of
	    		times that the local SDLC link stations on
	    		this port have entered a busy state (RNR).
	    		This object is initialized to zero when the
	    		port is created."
    		::= { fsSdlc2PortStatsEntry  9 }

	fsSdlc2PortStatsRemoteBusies OBJECT-TYPE
    		SYNTAX  Counter
    		ACCESS  read-only
    		STATUS  mandatory
    		DESCRIPTION
	   		"This object reflects the total number of
	    		times that the adjacent (i.e., remote) SDLC
	    		link stations on this port have entered a busy
	    		state (RNR).  This object is initialized to
	    		zero when the port is created."
    		::= { fsSdlc2PortStatsEntry  10 }

	fsSdlc2PortStatsIFramesIn OBJECT-TYPE
		SYNTAX  Counter
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"This object reflects the total number of
			I-Frames that have been received by SDLC link
			stations on this port.  This object is
			initialized to zero when the port is created."
		::= { fsSdlc2PortStatsEntry  11 }

	fsSdlc2PortStatsIFramesOut OBJECT-TYPE
		SYNTAX  Counter
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"This object reflects the total number of
			I-Frames that have been transmitted by SDLC
			link stations on this port.  This object is
			initialized to zero when the port is created."
		::= { fsSdlc2PortStatsEntry  12 }

	fsSdlc2PortStatsOctetsIn OBJECT-TYPE
		SYNTAX  Counter
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"This object reflects the total octets
			received from adjacent SDLC link stations on
			this port.  This object covers the address,
			control, and information field of I-Frames
			only.  This object is initialized to zero when
			the port is created."
		::= { fsSdlc2PortStatsEntry  13 }

	fsSdlc2PortStatsOctetsOut OBJECT-TYPE
		SYNTAX  Counter
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"This object reflects the total octets
			transmitted to adjacent SDLC link stations on
			this port.  This object covers the address,
			control, and information field of I-Frames
			only.  This object is initialized to zero when
			the port is created."
		::= { fsSdlc2PortStatsEntry  14 }

	fsSdlc2PortStatsProtocolErrs OBJECT-TYPE
		SYNTAX  Counter
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"This object reflects the total number of
			times that the SDLC link stations on this port
			have deactivated the link as a result of
			having received a protocol violation from the
			adjacent link station.  This object is
			initialized to zero when the port is created."
		::= { fsSdlc2PortStatsEntry  15 }

	fsSdlc2PortStatsActivityTOs OBJECT-TYPE
		SYNTAX  Counter
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"This object reflects the total number of
			times that the SDLC link stations on this port
			have deactivated the link as a result of no
			activity on the link.  This object is
			initialized to zero when the port is created."
		::= { fsSdlc2PortStatsEntry  16 }

	fsSdlc2PortStatsRetriesExps OBJECT-TYPE
		SYNTAX  Counter
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"This object reflects the total number of
			times that the SDLC link stations on this port
			have deactivated the link as a result of a
			retry sequence being exhausted.  This object
			is initialized to zero when the port is
			created."
		::= { fsSdlc2PortStatsEntry  17 }

	fsSdlc2PortStatsRetransmitsIn OBJECT-TYPE
		SYNTAX  Counter
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"This object reflects the total number of
			I-Frames retransmitted by remote link stations
			for all SDLC link stations on this port.  This
			object is initialized to zero when the port is
			created."
		::= { fsSdlc2PortStatsEntry  18 }

	fsSdlc2PortStatsRetransmitsOut OBJECT-TYPE
		SYNTAX  Counter
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"This object reflects the total number of
			I-Frames retransmitted by all local SDLC link
			stations on this port.  This object is
			initialized to zero when the port is created."
		::= { fsSdlc2PortStatsEntry  19 }

-- ***************************************************************************
-- SDLC2 Stn Stats

fsSdlc2LSStatsTable OBJECT-TYPE
	SYNTAX  SEQUENCE OF FsSdlc2LSStatsEntry
	ACCESS  not-accessible
	STATUS  mandatory
	DESCRIPTION
		"Each entry in this table contains statistics
		for a specific SDLC link station."
	::= { fsSdlc2Stats  2 }

	FsSdlc2LSStatsEntry ::= SEQUENCE {
		fsSdlc2LSPortIndex            INTEGER,
		fsSdlc2LSStnRecid             INTEGER,
		fsSdlc2LSStnAddress           DisplayString,
		fsSdlc2LSStatsBLUsIn          Counter,
		fsSdlc2LSStatsBLUsOut         Counter,
		fsSdlc2LSStatsOctetsIn        Counter,
		fsSdlc2LSStatsOctetsOut       Counter,
		fsSdlc2LSStatsPollsIn         Counter,
		fsSdlc2LSStatsPollsOut        Counter,
		fsSdlc2LSStatsPollRspsIn      Counter,
		fsSdlc2LSStatsPollRspsOut     Counter,
		fsSdlc2LSStatsLocalBusies     Counter,
		fsSdlc2LSStatsRemoteBusies    Counter,
		fsSdlc2LSStatsIFramesIn       Counter,
		fsSdlc2LSStatsIFramesOut      Counter,
		fsSdlc2LSStatsXIDsIn          Counter,
		fsSdlc2LSStatsXIDsOut         Counter,
		fsSdlc2LSStatsREJsIn          Counter,
		fsSdlc2LSStatsFRMRsIn         Counter,
		fsSdlc2LSStatsFRMRsOut        Counter
		}

	fsSdlc2LSStatsEntry OBJECT-TYPE
		SYNTAX  FsSdlc2LSStatsEntry
		ACCESS  not-accessible
		STATUS  mandatory
		DESCRIPTION
			"A list of statistics for an SDLC link station."
		INDEX   { fsSdlc2LSPortIndex, fsSdlc2LSStnRecid }
		::= { fsSdlc2LSStatsTable  1 }

	fsSdlc2LSPortIndex OBJECT-TYPE
		SYNTAX  INTEGER {
			portN		(1),
			port1       (2),
			port2       (3),
			port3       (4),
			port4       (5),
			port10      (10),
			port11	(11),
			port12      (12),
			port13      (13),
			port14      (14),
			port15      (15),
			port16	(16),
			port17      (17),
			port18      (18),
			port19      (19),
                  port20      (20),
                  port21      (21),
                  port22      (22),
                  port23      (23),
                  port24      (24),
                  port25      (25),
                  port26      (26),
                  port27      (27),
                  port28      (28),
                  port29      (29)
			}
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"This is the Port number of the port which the station 
			 belongs to."
		::= { fsSdlc2LSStatsEntry  1 }

	fsSdlc2LSStnRecid OBJECT-TYPE
		SYNTAX  INTEGER
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"This is the Record id the station record."
		::= { fsSdlc2LSStatsEntry  2 }

	fsSdlc2LSStnAddress OBJECT-TYPE
		SYNTAX  DisplayString (SIZE(2))
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"This is the address of the SDLC station."
		::= { fsSdlc2LSStatsEntry  3 }

	fsSdlc2LSStatsBLUsIn OBJECT-TYPE
		SYNTAX  Counter
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"This object reflects the total basic link
			units (BLUs; frames) received from an adjacent
			SDLC link station since link station startup.
			At link station startup time, this object must
			be initialized to zero."
		::= { fsSdlc2LSStatsEntry  4 }

	fsSdlc2LSStatsBLUsOut OBJECT-TYPE
		SYNTAX  Counter
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"This object reflects the total basic link
			units (BLUs; frames), transmitted to an
			adjacent SDLC link station since link station
			startup.  At link station startup time, this
			object must be initialized to zero."
		::= { fsSdlc2LSStatsEntry  5 }

	fsSdlc2LSStatsOctetsIn OBJECT-TYPE
		SYNTAX  Counter
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"This object reflects the total octets received
			from an adjacent SDLC link station since link
			station startup.  This object covers the
			address, control, and information field of
			I-Frames only.  At link station startup time,
			this object must be initialized to zero."
		::= { fsSdlc2LSStatsEntry  6 }

	fsSdlc2LSStatsOctetsOut OBJECT-TYPE
		SYNTAX  Counter
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"This object reflects the total octets
			transmitted to an adjacent SDLC link station
			since link station startup.  This object covers
			the address, control, and information field of
			I-Frames only.  At link station startup time,
			this object must be initialized to zero."
		::= { fsSdlc2LSStatsEntry  7 }

	fsSdlc2LSStatsPollsIn OBJECT-TYPE
		SYNTAX  Counter
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"This object reflects the total polls received
			from an adjacent SDLC link station since link
			station startup.  At link station startup time,
			this object must be initialized to zero."
		::= { fsSdlc2LSStatsEntry  8 }

	fsSdlc2LSStatsPollsOut OBJECT-TYPE
		SYNTAX  Counter
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"This object reflects the total polls sent to
			an adjacent SDLC link station since link
			station startup.  At link station startup time,
			this object must be initialized to zero."
		::= { fsSdlc2LSStatsEntry  9 }

	fsSdlc2LSStatsPollRspsIn OBJECT-TYPE
		SYNTAX  Counter
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"This object reflects the total number of poll
			responses received from the adjacent SDLC link
			station since station startup.  This value
			includes I-frames that are received in response
			to a poll.
			At link station startup time, this object must
			be initialized to zero."
		::= { fsSdlc2LSStatsEntry  10 }

	fsSdlc2LSStatsPollRspsOut OBJECT-TYPE
		SYNTAX  Counter
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"This object reflects the total number of poll
			responses sent to the adjacent SDLC link
			station since link station startup.  This value
			includes I-frames that are sent in response to
			a poll.
			At link station startup time, this object must
			be initialized to zero."
		::= { fsSdlc2LSStatsEntry  11 }

	fsSdlc2LSStatsLocalBusies OBJECT-TYPE
		SYNTAX  Counter
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"This object reflects the total number of times
			that the local SDLC link station has entered a
			busy state (RNR) since link station startup.
			At link station startup time, this object must
			be initialized to zero."
		::= { fsSdlc2LSStatsEntry  12 }

	fsSdlc2LSStatsRemoteBusies OBJECT-TYPE
		SYNTAX  Counter
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"This object reflects the total number of times
			that an adjacent ( remote) SDLC link station
			has entered a busy state (RNR) since link
			station startup.  At link station startup time,
			this object must be initialized to zero."
		::= { fsSdlc2LSStatsEntry  13 }

	fsSdlc2LSStatsIFramesIn OBJECT-TYPE
		SYNTAX  Counter
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"This object reflects the total I-frames
			received from an adjacent SDLC link station
			since link station startup.  At link station
			startup time, this object must be initialized
			to zero."
		::= { fsSdlc2LSStatsEntry  14 }

	fsSdlc2LSStatsIFramesOut OBJECT-TYPE
		SYNTAX  Counter
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"This object reflects the total I-frames
			transmitted to an adjacent SDLC link station
			since link station startup.  At link station
			startup time, this object must be initialized
			to zero."
		::= { fsSdlc2LSStatsEntry  15 }

	fsSdlc2LSStatsXIDsIn OBJECT-TYPE
		SYNTAX  Counter
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"This object reflects the total XID frames
			received from an adjacent SDLC link station
			since link station startup."
		::= { fsSdlc2LSStatsEntry  16 }

	fsSdlc2LSStatsXIDsOut OBJECT-TYPE
		SYNTAX  Counter
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"This object reflects the total XID frames
			transmitted to an adjacent SDLC link station
			since link station startup."
		::= { fsSdlc2LSStatsEntry  17 }

	fsSdlc2LSStatsREJsIn OBJECT-TYPE
		SYNTAX  Counter
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"This object reflects the total REJ frames
			received from an adjacent SDLC link station
			since link station startup."
		::= { fsSdlc2LSStatsEntry  18 }

	fsSdlc2LSStatsFRMRsIn OBJECT-TYPE
		SYNTAX  Counter
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
	   		"This object reflects the total frame reject
	    		(FRMR) frames received from an adjacent SDLC
	    		link station since link station startup."
		::= { fsSdlc2LSStatsEntry  19 }

	fsSdlc2LSStatsFRMRsOut OBJECT-TYPE
		SYNTAX  Counter
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"This object reflects the total frame reject
			(FRMR) frames transmitted to an adjacent SDLC
			link station since link station startup."
		::= { fsSdlc2LSStatsEntry  20 }

-- ***************************************************************************
-- LLC Stn Stats

fsLlcStationStatisticsTable OBJECT-TYPE
	SYNTAX  SEQUENCE OF FsLlcStationStatisticsEntry
	ACCESS  not-accessible
	STATUS  mandatory
	DESCRIPTION
		"Statistics Table for Station Component. Each entry
		would be accessed serially by the object
		'fsLlcStationStatisticsIndex'."
	::= { fsLlcStats 1 }

	FsLlcStationStatisticsEntry ::= SEQUENCE {
		fsLlcStationStatisticsIndex         INTEGER,
		fsLlcStationStatisticsIfId          INTEGER,
		fsLlcStationStatisticsCircuit       INTEGER,
		fsLlcStationNumActiveSAPs           INTEGER,
		fsLlcStationNumActiveConns          INTEGER
		}

	fsLlcStationStatisticsEntry OBJECT-TYPE
		SYNTAX  FsLlcStationStatisticsEntry
		ACCESS  not-accessible
		STATUS  mandatory
		DESCRIPTION
			"Entries of the fsLlcStationStatisticsTable."
		INDEX   { fsLlcStationStatisticsIndex }
		::= { fsLlcStationStatisticsTable 1 }

	fsLlcStationStatisticsIndex OBJECT-TYPE
		SYNTAX  INTEGER
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"The index value for the Llc Station Table."
		::= { fsLlcStationStatisticsEntry 1 }

	fsLlcStationStatisticsIfId OBJECT-TYPE
		SYNTAX  INTEGER
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"The Port Number for this LLC Station."
		::= { fsLlcStationStatisticsEntry 2 }

	fsLlcStationStatisticsCircuit OBJECT-TYPE
		SYNTAX  INTEGER
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"The circuit number for Frame Relay interface.
			The circuit number is set to 0 for Ethernet and Token
			Ring interfaces."
		::= { fsLlcStationStatisticsEntry 3 }

	fsLlcStationNumActiveSAPs OBJECT-TYPE
		SYNTAX  INTEGER (1..7)
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"The number of active LSAPs under this Station
			Component.
			The valid values range from 1 to 7."
		::= { fsLlcStationStatisticsEntry 4 }

	fsLlcStationNumActiveConns OBJECT-TYPE
		SYNTAX  INTEGER (0..512)
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
			"The number of active connections under this Station
			Component.
			The valid values range from 0 to 512."
		::= { fsLlcStationStatisticsEntry 5 }

