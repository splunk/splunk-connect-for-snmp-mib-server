-- Vr/<string> Ip Nhrp Nhs EgressPolicy/n
-- This component represents an egress policy on this server.
-- The server has the following default egress policies which may be 
-- overridden by any provisioned egress policies: 1. NAK resolution 
-- request for any remotely attached destination regardless of source, 
-- 2. ACK resolution request for any locally attached destination 
-- regardless of source.

mscVrIpNhrpNhsEp OBJECT IDENTIFIER ::= { mscVrIpNhrpNhs 2 }



mscVrIpNhrpNhsEpRowStatusTable OBJECT-TYPE
   SYNTAX     SEQUENCE OF MscVrIpNhrpNhsEpRowStatusEntry
   ACCESS     not-accessible
   STATUS     mandatory
   DESCRIPTION
     "This entry controls the addition and deletion of 
      mscVrIpNhrpNhsEp components."
::= { mscVrIpNhrpNhsEp 1 }

mscVrIpNhrpNhsEpRowStatusEntry OBJECT-TYPE
   SYNTAX     MscVrIpNhrpNhsEpRowStatusEntry
   ACCESS     not-accessible
   STATUS     mandatory
   DESCRIPTION
     "A single entry in the table represents a single 
      mscVrIpNhrpNhsEp component."
   INDEX {
      mscVrIndex,
      mscVrIpIndex,
      mscVrIpNhrpIndex,
      mscVrIpNhrpNhsIndex,
      mscVrIpNhrpNhsEpIndex }
::= { mscVrIpNhrpNhsEpRowStatusTable 1 }

MscVrIpNhrpNhsEpRowStatusEntry ::= SEQUENCE {
   mscVrIpNhrpNhsEpRowStatus
      RowStatus,
   mscVrIpNhrpNhsEpComponentName
      DisplayString,
   mscVrIpNhrpNhsEpStorageType
      StorageType,
   mscVrIpNhrpNhsEpIndex
      Integer32 }

mscVrIpNhrpNhsEpRowStatus OBJECT-TYPE
   SYNTAX     RowStatus 
   ACCESS     read-write
   STATUS     mandatory
   DESCRIPTION
     "This variable is used as the basis for SNMP naming of
      mscVrIpNhrpNhsEp components.
      These components can be added and deleted."
::= { mscVrIpNhrpNhsEpRowStatusEntry 1 }

mscVrIpNhrpNhsEpComponentName OBJECT-TYPE
   SYNTAX     DisplayString 
   ACCESS     read-only
   STATUS     mandatory
   DESCRIPTION
     "This variable provides the component's string name for
      use with the ASCII Console Interface"
::= { mscVrIpNhrpNhsEpRowStatusEntry 2 }

mscVrIpNhrpNhsEpStorageType OBJECT-TYPE
   SYNTAX     StorageType 
   ACCESS     read-only
   STATUS     mandatory
   DESCRIPTION
     "This variable represents the storage type value for the
      mscVrIpNhrpNhsEp tables."
::= { mscVrIpNhrpNhsEpRowStatusEntry 4 }

mscVrIpNhrpNhsEpIndex OBJECT-TYPE
   SYNTAX     Integer32 (0..65535)
   ACCESS     not-accessible
   STATUS     mandatory
   DESCRIPTION
     "This variable represents the index for the mscVrIpNhrpNhsEp tables."
::= { mscVrIpNhrpNhsEpRowStatusEntry 10 }

mscVrIpNhrpNhsEpProvTable OBJECT-TYPE
   SYNTAX     SEQUENCE OF MscVrIpNhrpNhsEpProvEntry
   ACCESS     not-accessible
   STATUS     mandatory
   DESCRIPTION
     "This group contains the provisionable attributes of the EgressPolicy 
      component."
::= { mscVrIpNhrpNhsEp 10 }

mscVrIpNhrpNhsEpProvEntry OBJECT-TYPE
   SYNTAX     MscVrIpNhrpNhsEpProvEntry
   ACCESS     not-accessible
   STATUS     mandatory
   DESCRIPTION
     "An entry in the mscVrIpNhrpNhsEpProvTable."
   INDEX {
      mscVrIndex,
      mscVrIpIndex,
      mscVrIpNhrpIndex,
      mscVrIpNhrpNhsIndex,
      mscVrIpNhrpNhsEpIndex }
::= { mscVrIpNhrpNhsEpProvTable 1 }

MscVrIpNhrpNhsEpProvEntry ::= SEQUENCE {
   mscVrIpNhrpNhsEpAction
      INTEGER,
   mscVrIpNhrpNhsEpProtocol
      INTEGER }

mscVrIpNhrpNhsEpAction OBJECT-TYPE
   SYNTAX     INTEGER  {
                 ack(1),
                 nak(2) }
   ACCESS     read-write
   STATUS     mandatory
   DESCRIPTION
     "This attribute specifies how the server should handle the NHRP 
      Resolution Request if it matches this policy. The possible actions 
      are:
      ack - Send a positive acknowledgement (ACK Reply) for the 
      request.
      nak - Send a negative acknowledgement (NAK Reply) for the 
      request."
   DEFVAL     { nak }
::= { mscVrIpNhrpNhsEpProvEntry 1 }

mscVrIpNhrpNhsEpProtocol OBJECT-TYPE
   SYNTAX     INTEGER  {
                 default(0),
                 staticLocal(1) }
   ACCESS     read-write
   STATUS     mandatory
   DESCRIPTION
     "This attribute species the reachability type (that is, protocol) of the 
      destination in the NHRP Resolution Request. The possible 
      protocols:
      default - This value is a 'wildcard' and matches any protocol. A 
      policy with a 'wildcard' protocol is less preferred than a policy with 
      a protocol which matches exactly.
      staticLocal - The destination is on a locally attached network."
   DEFVAL     { default }
::= { mscVrIpNhrpNhsEpProvEntry 2 }


