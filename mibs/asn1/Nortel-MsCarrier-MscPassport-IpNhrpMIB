      Counter32,
   mscVrIpNhrpNhsTxPurgeReq
      Counter32,
   mscVrIpNhrpNhsRxPurgeReply
      Counter32,
   mscVrIpNhrpNhsTxPurgeReply
      Counter32,
   mscVrIpNhrpNhsRxErrUnrecognizedExtension
      Counter32,
   mscVrIpNhrpNhsRxErrLoopDetected
      Counter32,
   mscVrIpNhrpNhsRxErrProtoAddrUnreachable
      Counter32,
   mscVrIpNhrpNhsRxErrProtoError
      Counter32,
   mscVrIpNhrpNhsRxErrSduSizeExceeded
      Counter32,
   mscVrIpNhrpNhsRxErrInvalidExtension
      Counter32,
