      Counter32,
   mscVrIpNhrpNhsTxErrLoopDetected
      Counter32,
   mscVrIpNhrpNhsTxErrProtoAddrUnreachable
      Counter32,
   mscVrIpNhrpNhsTxErrProtoError
      Counter32,
   mscVrIpNhrpNhsTxErrSduSizeExceeded
      Counter32,
   mscVrIpNhrpNhsTxErrInvalidExtension
      Counter32,
   mscVrIpNhrpNhsTxErrAuthenticationFailure
      Counter32,
   mscVrIpNhrpNhsTxErrHopCountExceeded
      Counter32,
   mscVrIpNhrpNhsFwdResolveReq
      Counter32,
   mscVrIpNhrpNhsFwdResolveReply
      Counter32,
   mscVrIpNhrpNhsFwdRegisterReq
      Counter32,
   mscVrIpNhrpNhsFwdRegisterReply
      Counter32,
   mscVrIpNhrpNhsFwdPurgeReq
      Counter32,
   mscVrIpNhrpNhsFwdPurgeReply
      Counter32,
   mscVrIpNhrpNhsFwdErrorIndication
      Counter32 }

mscVrIpNhrpNhsRxResolveReq OBJECT-TYPE
   SYNTAX     Counter32 
   ACCESS     read-only
   STATUS     mandatory
   DESCRIPTION
     "This attribute counts the NHRP Resolution Requests received by 
      this server. 
      This counter wraps to zero when the maximum value is exceeded."
::= { mscVrIpNhrpNhsStatsEntry 1 }

mscVrIpNhrpNhsTxResolveReplyAck OBJECT-TYPE
   SYNTAX     Counter32 
   ACCESS     read-only
   STATUS     mandatory
   DESCRIPTION
     "This attribute counts the positively acknowledged NHRP Resolution 
      Replies transmitted by this server. 
      This counter wraps to zero when the maximum value is exceeded."
::= { mscVrIpNhrpNhsStatsEntry 2 }

mscVrIpNhrpNhsTxResolveReplyNakProhibited OBJECT-TYPE
   SYNTAX     Counter32 
   ACCESS     read-only
   STATUS     mandatory
   DESCRIPTION
     "This attribute counts the NAK NHRP Resolution Replies 
      transmitted by this server with the code 'Administratively 
      Prohibited'. 
      This counter wraps to zero when the maximum value is exceeded."
::= { mscVrIpNhrpNhsStatsEntry 3 }

mscVrIpNhrpNhsTxResolveReplyNakInsufResources OBJECT-TYPE
   SYNTAX     Counter32 
   ACCESS     read-only
   STATUS     mandatory
   DESCRIPTION
     "This attribute counts the NAK NHRP Resolution Replies 
      transmitted by this server with the code 'Insufficient Resources'. 
      This counter wraps to zero when the maximum value is exceeded."
::= { mscVrIpNhrpNhsStatsEntry 4 }

mscVrIpNhrpNhsTxResolveReplyNakNoBinding OBJECT-TYPE
   SYNTAX     Counter32 
   ACCESS     read-only
   STATUS     mandatory
   DESCRIPTION
     "This attribute counts the NAK NHRP Resolution Replies 
      transmitted by this server with the code 'No Internetworking Layer 
      Address to NBMA Address Binding Exists'. 
      This counter wraps to zero when the maximum value is exceeded."
::= { mscVrIpNhrpNhsStatsEntry 5 }

mscVrIpNhrpNhsTxResolveReplyNakNotUnique OBJECT-TYPE
   SYNTAX     Counter32 
   ACCESS     read-only
   STATUS     mandatory
   DESCRIPTION
     "This attribute counts the NAK NHRP Resolution Replies 
      transmitted by this server with the code 'Binding Exists But Is Not 
      Unique'. 
      This counter wraps to zero when the maximum value is exceeded."
::= { mscVrIpNhrpNhsStatsEntry 6 }

