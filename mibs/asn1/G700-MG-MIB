				{
				active(1),
				notConfigured(2),
				notArmed(3),
				armedNotConfigured(4),
				notSupported(255)				
				}			
			MAX-ACCESS read-only			
			STATUS current 			
			DESCRIPTION 
			"Dynamic CAC status"
			::= { cmgDynamicCAC 1 }
			
