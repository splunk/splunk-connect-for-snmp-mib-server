        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION
                "The transceiver type of the unit."
        ::= { pgRemUnitEpEntry 4 }

pgRemUnitEpAFE OBJECT-TYPE
        SYNTAX  INTEGER
        ACCESS  read-write
        STATUS  mandatory
        DESCRIPTION
