        SYNTAX  INTEGER
        ACCESS  read-write
        STATUS  mandatory
        DESCRIPTION
                "Number of DS0 channels allocated to Ethernet port."
        ::= { pgRemUnitEpEntry 8 }

pgRemUnitEpPOTSType OBJECT-TYPE
        SYNTAX  INTEGER {
                noPOT(1),
                fxO1(2),
                fxS1(3),
                fxO2(4),
                fxS2(5)
                }
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION
                "This object indicates what kind of POTS card is installed,
                 and how many POTS lines are available."
        ::= { pgRemUnitEpEntry 9 }

pgRemUnitEpPOTSEnable1 OBJECT-TYPE
        SYNTAX  INTEGER {
                disable(1),
                enable(2)
                }
        ACCESS  read-write
        STATUS  mandatory
        DESCRIPTION
                "This object enables or disables POTS 1 line."
        ::= { pgRemUnitEpEntry 10 }

