                    SYNTAX      INTEGER
                    {
                    setTime (1)
                    }
                    ACCESS      write-only
                    STATUS      mandatory
                    DESCRIPTION
                            "Broadcast the time to all nodes."
                    ::= { tyviewBroadcastTable 1 }

