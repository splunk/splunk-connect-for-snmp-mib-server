                    SYNTAX      INTEGER
                    {
                    forwardAlarms (1)
                    }
                    ACCESS      write-only
                    STATUS      mandatory
                    DESCRIPTION
                            "Broadcast alarm forwarding."
                    ::= { tyviewBroadcastTable 2 }

