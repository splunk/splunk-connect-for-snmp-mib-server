       DEFVAL { 1 } 
       ::= { cabhPsDevBpProfileEntry 15 } 
    
   cabhPsDevBpPhysicalLocation       OBJECT-TYPE 
       SYNTAX      SnmpAdminString (SIZE(0..32)) 
