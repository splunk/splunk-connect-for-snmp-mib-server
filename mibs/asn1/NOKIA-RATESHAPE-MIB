IMPORTS

    IpAddress, enterprises, Integer32, Counter32,
    OBJECT-TYPE, MODULE-IDENTITY
            FROM SNMPv2-SMI
--    ifIndex
--            FROM IF-MIB
    RowStatus, DisplayString, TEXTUAL-CONVENTION
            FROM SNMPv2-TC
    OBJECT-GROUP
            FROM SNMPv2-CONF;

ntcRS MODULE-IDENTITY
        LAST-UPDATED "0001300000Z"
        ORGANIZATION "Nokia"
	CONTACT-INFO
