      PassportCounter64,
   mscAtmIfVpcPmFwdLastSampleClr0
      FixedPoint9,
   mscAtmIfVpcPmFwdLastSampleUserCellsRx01
      PassportCounter64,
   mscAtmIfVpcPmFwdLastSampleUserCellsTx01
      PassportCounter64,
   mscAtmIfVpcPmFwdLastSampleClr01
      FixedPoint9,
   mscAtmIfVpcPmFwdTotalClrInterval
      Unsigned32,
   mscAtmIfVpcPmFwdTotalUserCellsRx0
      PassportCounter64,
   mscAtmIfVpcPmFwdTotalUserCellsTx0
      PassportCounter64,
   mscAtmIfVpcPmFwdAverageClr0
      FixedPoint9,
   mscAtmIfVpcPmFwdMaximumClr0
      FixedPoint9,
   mscAtmIfVpcPmFwdMinimumClr0
      FixedPoint9,
   mscAtmIfVpcPmFwdTotalUserCellsRx01
      PassportCounter64,
   mscAtmIfVpcPmFwdTotalUserCellsTx01
      PassportCounter64,
   mscAtmIfVpcPmFwdAverageClr01
      FixedPoint9,
   mscAtmIfVpcPmFwdMaximumClr01
      FixedPoint9,
   mscAtmIfVpcPmFwdMinimumClr01
      FixedPoint9,
   mscAtmIfVpcPmFwdPmCellDiscardCount
      Unsigned32 }

