   mscAtmIfVpcVpdTmRowStatus
      RowStatus,
   mscAtmIfVpcVpdTmComponentName
      DisplayString,
   mscAtmIfVpcVpdTmStorageType
      StorageType,
   mscAtmIfVpcVpdTmIndex
      NonReplicated }

