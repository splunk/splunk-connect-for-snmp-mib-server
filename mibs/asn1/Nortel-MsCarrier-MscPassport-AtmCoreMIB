   mscAtmIfVptVpdRowStatus
      RowStatus,
   mscAtmIfVptVpdComponentName
      DisplayString,
   mscAtmIfVptVpdStorageType
      StorageType,
   mscAtmIfVptVpdIndex
      NonReplicated }

