   DEFVAL     { 63 }
::= { mscAtmIfCAProvEntry 127 }

mscAtmIfCAOperTable OBJECT-TYPE
   SYNTAX     SEQUENCE OF MscAtmIfCAOperEntry
   ACCESS     not-accessible
   STATUS     mandatory
   DESCRIPTION
     "This group contains operational attributes giving statistics about the 
      ATM connections configured on an ATM interface."
::= { mscAtmIfCA 110 }

mscAtmIfCAOperEntry OBJECT-TYPE
   SYNTAX     MscAtmIfCAOperEntry
   ACCESS     not-accessible
   STATUS     mandatory
   DESCRIPTION
     "An entry in the mscAtmIfCAOperTable."
   INDEX {
      mscAtmIfIndex,
      mscAtmIfCAIndex }
::= { mscAtmIfCAOperTable 1 }

