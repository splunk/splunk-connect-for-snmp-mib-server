   SYNTAX     MscAtmIfVptVccTmAbrOperEntry
   ACCESS     not-accessible
   STATUS     mandatory
   DESCRIPTION
     "An entry in the mscAtmIfVptVccTmAbrOperTable."
   INDEX {
      mscAtmIfIndex,
      mscAtmIfVptIndex,
      mscAtmIfVptVccIndex,
      mscAtmIfVptVccTmIndex,
      mscAtmIfVptVccTmAbrIndex }
::= { mscAtmIfVptVccTmAbrOperTable 1 }

MscAtmIfVptVccTmAbrOperEntry ::= SEQUENCE {
   mscAtmIfVptVccTmAbrInitialCellRate
      Unsigned32,
   mscAtmIfVptVccTmAbrTransientBufferExposure
      Unsigned32,
   mscAtmIfVptVccTmAbrFixedRoundTripTime
      Unsigned32,
   mscAtmIfVptVccTmAbrRateDecreaseFactor
      Unsigned32,
   mscAtmIfVptVccTmAbrRateIncreaseFactor
      Unsigned32,
