      FixedPoint9,
   mscAtmIfVccPmFwdLastSampleUserCellsRx01
      PassportCounter64,
   mscAtmIfVccPmFwdLastSampleUserCellsTx01
      PassportCounter64,
   mscAtmIfVccPmFwdLastSampleClr01
      FixedPoint9,
   mscAtmIfVccPmFwdTotalClrInterval
      Unsigned32,
   mscAtmIfVccPmFwdTotalUserCellsRx0
      PassportCounter64,
   mscAtmIfVccPmFwdTotalUserCellsTx0
      PassportCounter64,
   mscAtmIfVccPmFwdAverageClr0
      FixedPoint9,
   mscAtmIfVccPmFwdMaximumClr0
      FixedPoint9,
   mscAtmIfVccPmFwdMinimumClr0
      FixedPoint9,
   mscAtmIfVccPmFwdTotalUserCellsRx01
      PassportCounter64,
   mscAtmIfVccPmFwdTotalUserCellsTx01
      PassportCounter64,
   mscAtmIfVccPmFwdAverageClr01
      FixedPoint9,
   mscAtmIfVccPmFwdMaximumClr01
      FixedPoint9,
   mscAtmIfVccPmFwdMinimumClr01
      FixedPoint9,
   mscAtmIfVccPmFwdPmCellDiscardCount
      Unsigned32 }

