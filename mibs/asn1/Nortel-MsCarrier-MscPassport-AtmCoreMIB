      FixedPoint9,
   mscAtmIfVpcPmBwdMaximumClr0
      FixedPoint9,
   mscAtmIfVpcPmBwdMinimumClr0
      FixedPoint9,
   mscAtmIfVpcPmBwdTotalUserCellsRx01
      PassportCounter64,
   mscAtmIfVpcPmBwdTotalUserCellsTx01
      PassportCounter64,
   mscAtmIfVpcPmBwdAverageClr01
      FixedPoint9,
   mscAtmIfVpcPmBwdMaximumClr01
      FixedPoint9,
   mscAtmIfVpcPmBwdMinimumClr01
      FixedPoint9,
   mscAtmIfVpcPmBwdPmCellDiscardCount
      Unsigned32 }

