-- Optical Layer Event Traps.
-----------------------------------------------------------------------


olOSCStatusChangeEvt NOTIFICATION-TYPE
    OBJECTS      { olOSCCpTypeIdx,
                   olOSCShelfIdx,
                   olOSCSlotIdx,
                   olOSCLineIdx,
                   olOSCOperStatus,
                   olOSCOperStatQlfr,
                   evtDateAndTime,
                   trapSeqNum,
                   evtDescription,
                   evtObjectType,
                   evtCodeType }
    STATUS       current
    DESCRIPTION  "Notification of a change in the operational status or
                  operational status qualifier or an optical layer OSC
                  facility."
    ::= { olOSCEvtNotifications 0 1 }


