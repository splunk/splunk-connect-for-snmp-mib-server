    OBJECTS      { olOSCCpTypeIdx,
                   olOSCShelfIdx,
                   olOSCSlotIdx,
                   olOSCLineIdx,
                   tcaIntervalType,
                   tcaDateAndTime,
                   tcaMontype,
                   tcaValue,
                   tcaThreshold,
                   evtDateAndTime,
                   trapSeqNum,
                   evtDescription,
                   evtObjectType,
                   evtCodeType }
    STATUS       current
    DESCRIPTION  "This trap provides a threshold crossing alert for a
                  performance monitoring statistic on an optical layer
                  OSC facility."
    ::= { olOSCEvtNotifications 0 2 }



