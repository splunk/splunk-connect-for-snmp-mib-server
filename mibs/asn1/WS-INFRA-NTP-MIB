		-- 1.3.6.1.4.1.388.14.1.10.1.6
		wsInfraNtpCurrentDateTime OBJECT-TYPE
			SYNTAX DateAndTime
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"Current Date and Time. If NTP is not running, or has not 
				yet synchronized time, this value will be 0."
			::= { wsInfraNTPMIB 6 }

		-- 1.3.6.1.4.1.388.14.1.10.1.7
		wsInfraNtpAllTimeZones OBJECT-TYPE
			SYNTAX OCTET STRING (SIZE (0..5000))
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"Lists all possible timezones separated by newlines."
			::= { wsInfraNTPMIB 7 }

