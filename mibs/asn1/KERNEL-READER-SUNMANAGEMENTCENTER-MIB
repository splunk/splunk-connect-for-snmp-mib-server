    SYNTAX      DisplayString
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
                "Zone's Status."
    ::= { krZoneListEntry 3 }

krPath OBJECT-TYPE
    SYNTAX      DisplayString
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
                "Logical PATH for the zone."
    ::= { krZoneListEntry 4 }

krIp OBJECT-TYPE
    SYNTAX      DisplayString
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
                "Zone's IP address."
    ::= { krZoneListEntry 5 }

krAutoboot OBJECT-TYPE
    SYNTAX      DisplayString
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
                "Autoboot Status for the zone."
    ::= { krZoneListEntry 6 }


