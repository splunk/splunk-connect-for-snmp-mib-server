krZoneListTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF KrZoneListEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
                "MIB tree branch"
    ::= { krZoneList 1 }

krZoneListEntry OBJECT-TYPE
    SYNTAX      KrZoneListEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
                "MIB tree branch"
    INDEX  { krZoneName }
    ::= { krZoneListTable 1 }

KrZoneListEntry ::=
    SEQUENCE {
        krZoneId
            DisplayString,
        krZoneName
            DisplayString,
        krStatus
            DisplayString,
        krPath
            DisplayString,
        krIp
            DisplayString,
        krAutoboot
            DisplayString
    }

