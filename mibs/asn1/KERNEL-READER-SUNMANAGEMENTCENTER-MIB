krDeviceErrorTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF KrDeviceErrorEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
                "This table displays device error summary statistics."
    ::= { krIOErrorStatsGroup 1 }

krDeviceErrorEntry OBJECT-TYPE
    SYNTAX      KrDeviceErrorEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
                "Error information for a particular device."
    INDEX  { krDevDevice }
    ::= { krDeviceErrorTable 1 }

KrDeviceErrorEntry ::=
    SEQUENCE {
        krDevDevice
            DisplayString,
        krDevSoftwareErrors
            Integer32,
        krDevHardwareErrors
            Integer32,
        krDevTransportErrors
            Integer32,
        krDevTotalErrors
            Integer32
    }

