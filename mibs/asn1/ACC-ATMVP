	 oVpAtm1483RetryCount    	INTEGER,
	 oVpAtm1483Vpi           	INTEGER,
	 oVpAtm1483Vci           	INTEGER,
	 oVpAtm1483CongTimeOut   	INTEGER,
	 oVpAtm1483MuxType       	INTEGER
}
