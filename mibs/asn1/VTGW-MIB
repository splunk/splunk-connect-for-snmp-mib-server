        -- Traps section

        setParameterFailure	TRAP-TYPE
                ENTERPRISE	vtg
                DESCRIPTION  
                        "Setting parameter failed."
        ::= 1

        haspExistance		TRAP-TYPE
                ENTERPRISE	vtg
                DESCRIPTION  
                        "HASP existance failed."
        ::= 2

        cti2ServiceFailure	TRAP-TYPE
                ENTERPRISE	vtg
                DESCRIPTION  
                        "CTI2 service failed."
        ::= 3

        vgkLoginFailure		TRAP-TYPE
                ENTERPRISE	vtg
                DESCRIPTION  
                        "VTG has failed to login to VGK."
        ::= 4

        vgkLogoutFailure	TRAP-TYPE
                ENTERPRISE	vtg
                DESCRIPTION 
                        "VTG has failed to logout from VGK."
        ::= 5

        vgkDisconnection	TRAP-TYPE
                ENTERPRISE	vtg
                DESCRIPTION 
                        "VTG disconnected from VGK."
        ::= 6

        telephonyError		TRAP-TYPE
                ENTERPRISE	vtg
                VARIABLES   { telephonyErrorDescription }
                DESCRIPTION 
                        "Telephony error."
        ::= 7

        incorrectPasswordCalls	TRAP-TYPE
                ENTERPRISE	vtg
                DESCRIPTION  
                        "Incorrect password call."
        ::= 8

        disconnectedCalls	TRAP-TYPE
                ENTERPRISE	vtg
                DESCRIPTION  
                        "Disconnected call."
        ::= 9

        notSupportedCalls	TRAP-TYPE
                ENTERPRISE	vtg
                DESCRIPTION 
                        "Not supported Call."
        ::= 10

-------------------------------------------------------------------------------

END

