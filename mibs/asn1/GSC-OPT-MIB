        -- Control objects

        cdx6500ContGSC  OBJECT IDENTIFIER ::= { cdx6500Controls 12 }

        cdx6500ContGSCPTable OBJECT-TYPE
            SYNTAX SEQUENCE OF Cdx6500ContGSCPTableEntry
            ACCESS not-accessible
            STATUS mandatory
            DESCRIPTION
                "This table contains the port control objects"
        ::= { cdx6500ContGSC 1 }

        cdx6500ContGSCPTableEntry OBJECT-TYPE
            SYNTAX Cdx6500ContGSCPTableEntry
            ACCESS not-accessible
            STATUS mandatory
            DESCRIPTION
                "Each entry represents a port control parameter."
            INDEX { cdx6500ContGSCPPortNum }
        ::= { cdx6500ContGSCPTable 1 }

        Cdx6500ContGSCPTableEntry ::= 
            SEQUENCE {
                cdx6500ContGSCPPortNum
                    INTEGER,
                cdx6500ContGSCPBootPort
                    INTEGER,
                cdx6500ContGSCPEnablePort
                    INTEGER,
                cdx6500ContGSCPDisablePort
                    INTEGER
                }
            
        cdx6500ContGSCPPortNum  OBJECT-TYPE
            SYNTAX INTEGER (1..54)
            ACCESS not-accessible
            STATUS mandatory
            DESCRIPTION
                "GSC port Number."
        ::= { cdx6500ContGSCPTableEntry 1 }

        cdx6500ContGSCPBootPort OBJECT-TYPE
            SYNTAX INTEGER {
                    boot   (1),
                    noBoot (2)
                    }
            ACCESS write-only
            STATUS mandatory
            DESCRIPTION
                "Boot the specified GSC port.
                 boot   : Boot the port
                 noBoot : Do not boot the port."
        ::= { cdx6500ContGSCPTableEntry 2 }

        cdx6500ContGSCPEnablePort OBJECT-TYPE
            SYNTAX INTEGER {
                    enable   (1),
                    noEnable (2)
                    }
            ACCESS write-only
            STATUS mandatory
            DESCRIPTION
                "Enable the specified GSC port.
                 enable   : Enable the GSC port.
                 noEnable : Do not enable the GSC port."
        ::= { cdx6500ContGSCPTableEntry 3 }

        cdx6500ContGSCPDisablePort OBJECT-TYPE
            SYNTAX INTEGER {
                    disable   (1),
                    noDisable (2)
                    }
            ACCESS write-only
            STATUS mandatory
            DESCRIPTION
                "Disable the specified GSC port.
                 disable   : Disable the GSC port.
                 noDisable : Do not disable the GSC port."
        ::= { cdx6500ContGSCPTableEntry 4 }

        cdx6500ContGSCSTable OBJECT-TYPE
            SYNTAX SEQUENCE OF Cdx6500ContGSCSTableEntry
            ACCESS not-accessible
            STATUS mandatory
            DESCRIPTION
                "This table contains the station control objects."
        ::= { cdx6500ContGSC 2 }

        cdx6500ContGSCSTableEntry OBJECT-TYPE
            SYNTAX Cdx6500ContGSCSTableEntry
            ACCESS not-accessible
            STATUS mandatory
            DESCRIPTION
                "Each entry represents a station control parameter."
            INDEX { cdx6500ContGSCSPortNum, cdx6500ContGSCSStnNum }
        ::= { cdx6500ContGSCSTable 1 }

        Cdx6500ContGSCSTableEntry ::= 
            SEQUENCE {
                cdx6500ContGSCSPortNum
                    INTEGER,
                cdx6500ContGSCSStnNum
                    INTEGER,
                cdx6500ContGSCSBootStation
                    INTEGER,
                cdx6500ContGSCSEnableStation
                    INTEGER,
                cdx6500ContGSCSDisableStation
                    INTEGER
                }

        cdx6500ContGSCSPortNum  OBJECT-TYPE
            SYNTAX INTEGER (1..54)
            ACCESS not-accessible
            STATUS mandatory
            DESCRIPTION
                "The GSC Port Number."
        ::= { cdx6500ContGSCSTableEntry 1 }

        cdx6500ContGSCSStnNum OBJECT-TYPE
            SYNTAX INTEGER (1..24)
            ACCESS not-accessible
            STATUS mandatory
            DESCRIPTION
                "The GSC Station Number."
        ::= { cdx6500ContGSCSTableEntry 2 }

        cdx6500ContGSCSBootStation OBJECT-TYPE
            SYNTAX INTEGER {
                    boot   (1),
                    noBoot (2)
                    }
            ACCESS write-only
            STATUS mandatory
            DESCRIPTION
                "Boot the specified GSC station
                 boot   : Boot the station.
                 noBoot : Do not boot the station."
        ::= { cdx6500ContGSCSTableEntry 3 }

        cdx6500ContGSCSEnableStation OBJECT-TYPE
            SYNTAX INTEGER {
                    enable   (1),
                    noEnable (2)
                    }
            ACCESS write-only
            STATUS mandatory
            DESCRIPTION
                "Enable the specified GSC station.
                 enable   : Enable the Station.
                 noEnable : Do not enable the Station."
        ::= { cdx6500ContGSCSTableEntry 4 }

        cdx6500ContGSCSDisableStation OBJECT-TYPE
            SYNTAX INTEGER {
                    disable   (1),
                    noDisable (2) }
            ACCESS write-only
            STATUS mandatory
            DESCRIPTION
                "Disable the specified GSC station.
                 disable   : Disable the station.
                 noDisable : Do not disable the station."
        ::= { cdx6500ContGSCSTableEntry 5 }
          
        END
