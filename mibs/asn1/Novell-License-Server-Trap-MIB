Novell-License-Server-Trap-MIB DEFINITIONS ::= BEGIN

IMPORTS
	enterprises
		FROM RFC1155-SMI

	InternationalDisplayString
		FROM HOST-RESOURCES-MIB

	TRAP-TYPE
		FROM RFC-1215

	OBJECT-TYPE
		FROM RFC-1212;


novell				OBJECT IDENTIFIER ::= { enterprises 23 }

mibDoc				OBJECT IDENTIFIER ::= { novell 2 }

nlstrap-mib			OBJECT IDENTIFIER ::= { mibDoc 73 }

trapinfo			OBJECT IDENTIFIER ::= { nlstrap-mib 1 }


--trapinfo variables


serverName		OBJECT-TYPE
			SYNTAX		InternationalDisplayString(SIZE(1..48))
			ACCESS		not-accessible
			STATUS		mandatory
			DESCRIPTION	""
			::= {trapinfo 1}


trapTime		OBJECT-TYPE
			SYNTAX		INTEGER
			ACCESS		not-accessible
			STATUS		mandatory
			DESCRIPTION	"The number of seconds since midnight
						of January 1,1970 GMT."
			::= {trapinfo 2}


appName			OBJECT-TYPE
			SYNTAX		InternationalDisplayString(SIZE(1..256)) 
			ACCESS		not-accessible
			STATUS		mandatory
			DESCRIPTION	"The name of the licensed application"
			::= {trapinfo 3}

userName		OBJECT-TYPE
			SYNTAX		InternationalDisplayString(SIZE(1..256))
			ACCESS		not-accessible
			STATUS		mandatory
			DESCRIPTION	"The user requesting or releasing application Licenses"
			::= {trapinfo 4}

networkAddress	OBJECT-TYPE
			SYNTAX		OCTET STRING(SIZE(4))
			ACCESS		not-accessible
			STATUS		mandatory
			DESCRIPTION "Four byte internal network number."
			::= {trapinfo 5}

macAddress       	OBJECT-TYPE
			SYNTAX		OCTET STRING(SIZE(6))
			ACCESS		not-accessible
			STATUS		mandatory
			DESCRIPTION "Six byte MAC address."
			::= {trapinfo 6}


--traps generated by NLS

lsRequestSuccess		TRAP-TYPE
            ENTERPRISE		nlstrap-mib
			VARIABLES		{ serverName, trapTime, appName, userName, networkAddress, macAddress }
			DESCRIPTION		"NetWare License Server: LSRequest was completed successfully."
			--#TYPE 	"NLS: Request was successful"
			--#SUMMARY 	"A license request for %s was completed successfully by "
			--#SUMMARY 	"%s at %x:%x on server %s. "
			--#ARGUMENTS {2,3,4,5,0}
            --#SEVERITY INFORMATIONAL
            --#TIMEINDEX 1        
            --#STATE OPERATIONAL
			::= 1

lsRequestSystemError	TRAP-TYPE   
            ENTERPRISE      nlstrap-mib
			VARIABLES		{ serverName, trapTime, appName, userName, networkAddress, macAddress }
			DESCRIPTION		"NetWare License Server: LSRequest failed due to
							 system unavailable."
			--#TYPE 	"NLS: Request failed - System unavailable"
			--#SUMMARY 	"A license request for %s by %s at %x:%x failed due to system" 
			--#SUMMARY 	" unavailable on server %s. "
			--#ARGUMENTS {2,3,4,5,0}
            --#SEVERITY MINOR
            --#TIMEINDEX 1        
            --#STATE  OPERATIONAL
			::= 2

lsRequestResourceError	TRAP-TYPE   
            ENTERPRISE      nlstrap-mib
			VARIABLES		{ serverName, trapTime, appName, userName, networkAddress, macAddress }
			DESCRIPTION		"NetWare License Server: LSRequest failed due to 
							resources unavailable."
			--#TYPE 	"NLS: Request failed resources unavailable"
			--#SUMMARY 	"A license request for %s by %s at %x:%x failed due to resources"
			--#SUMMARY	" unavailable on server %s. "
			--#ARGUMENTS {2,3,4,5,0}
            --#SEVERITY MINOR
            --#TIMEINDEX 1        
            --#STATE  OPERATIONAL
			::= 3

lsRequestAuthError		TRAP-TYPE   
            ENTERPRISE      nlstrap-mib
			VARIABLES		{ serverName, trapTime, appName, userName, networkAddress, macAddress }
			DESCRIPTION		"NetWare License Server: LSRequest failed due to 
							security error."
			--#TYPE 	"NLS: Request failed security error"
			--#SUMMARY 	"A license request for %s by %s at %x:%x failed, current user was"
			--#SUMMARY	" not security-equivalent to original creator of the"
			--#SUMMARY	" license handle. "
			--#ARGUMENTS {2,3,4,5}
            --#SEVERITY MINOR
            --#TIMEINDEX 1        
            --#STATE  OPERATIONAL
			::= 4

lsRequestBadArg			TRAP-TYPE   
            ENTERPRISE      nlstrap-mib
			VARIABLES		{ serverName, trapTime, appName, userName, networkAddress, macAddress }
			DESCRIPTION		"NetWare License Server: LSRequest failed due to
							 bad parameter(s)."
			--#TYPE 	"NLS: Request failed - bad parameter."
			--#SUMMARY 	"A license request for %s by %s at %x:%x failed, one or more"
			--#SUMMARY	" parameter are invalid."
			--#ARGUMENTS {2,3,4,5}
            --#SEVERITY MINOR
            --#TIMEINDEX 1        
            --#STATE  OPERATIONAL
			::= 5

lsRequestInsuffUnits	TRAP-TYPE   
            ENTERPRISE      nlstrap-mib
			VARIABLES		{ serverName, trapTime, appName, userName, networkAddress, macAddress }
			DESCRIPTION		"NetWare License Server: LSRequest failed not 
							 enough installed license units."
			--#TYPE 	"NLS: Request failed - Insufficient Units"
			--#SUMMARY 	"A license request for %s by %s at %x:%x failed, could not find"
			--#SUMMARY	" enough installed license units to satisfy the"
			--#SUMMARY	" request at this time."
			--#ARGUMENTS {2,3,4,5}
            --#SEVERITY MINOR
            --#TIMEINDEX 1        
            --#STATE  OPERATIONAL
			::= 6

lsRequestLicUnavail		TRAP-TYPE   
            ENTERPRISE      nlstrap-mib
			VARIABLES		{ serverName, trapTime, appName, userName, networkAddress, macAddress }
			DESCRIPTION		"NetWare License Server: LSRequest failed due to 
							 license unavailable."
			--#TYPE 	"NLS: Request failed - license unavailable"
			--#SUMMARY 	"A license request for %s by %s at %x:%x failed, not enough license"
			--#SUMMARY	" units are available to fulfill the request, but"
			--#SUMMARY	" there are enough units installed to fulfill the"
			--#SUMMARY	" request."
			--#ARGUMENTS {2,3,4,5}
            --#SEVERITY MINOR
            --#TIMEINDEX 1        
            --#STATE  OPERATIONAL
			::= 7

lsRequestNetUnavail		TRAP-TYPE   
            ENTERPRISE      nlstrap-mib
			VARIABLES		{ serverName, trapTime, appName, userName, networkAddress, macAddress }
			DESCRIPTION		"NetWare License Server: LSRequest failed due to
							 network unavailable."
			--#TYPE 	"NLS: Request failed - network unavailable"
			--#SUMMARY 	"A license request for %s by %s at %x:%x failed, the network is"
			--#SUMMARY	" currently unavailable on server %s."
			--#ARGUMENTS {2,3,4,5,0}
            --#SEVERITY MINOR
            --#TIMEINDEX 1        
            --#STATE  OPERATIONAL
			::= 8

lsRelSuccess			TRAP-TYPE   
