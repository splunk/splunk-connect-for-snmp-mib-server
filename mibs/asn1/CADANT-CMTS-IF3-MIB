            cadIf3CsNumRfMuteAll                Integer32,
            cadIf3CsNumTotal                    Integer32,
            cadIf3CsNumRangingAborted           Integer32,
            cadIf3CsNumRangFlaps                Integer32,
            cadIf3CsNumProvFlaps                Integer32,
            cadIf3CsNumRegFlaps                 Integer32
        }

