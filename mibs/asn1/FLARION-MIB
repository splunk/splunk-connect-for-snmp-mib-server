    mobileActiveEui         MobileEui,
    mobileActiveIpAddr      IpAddress,
    mobileActiveVersion     DisplayString
}

mobileActiveSectorId	OBJECT-TYPE
    SYNTAX          INTEGER
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     "Sector ID."
    ::= { mobileActiveEntry 1 }

mobileActiveIndex   OBJECT-TYPE
    SYNTAX          INTEGER
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION     "Active mobile index."
    ::= { mobileActiveEntry 2 }

