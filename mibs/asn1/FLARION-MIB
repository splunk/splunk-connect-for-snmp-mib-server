        wanLbo              WanLineBuildOut,
        wanPolarity         WanPolarity,
        wanPulse            WanFeatureState,
        wanJitter           WanFeatureState,
        wanJitterSide       WanJitterAttenuation,
        wanChan1to8         WanFeatureState,
        wanChan9to16        WanFeatureState,
        wanChan17to24       WanFeatureState,
        wanChan25to32       WanFeatureState,
        wanPortValid        TruthValue
}

