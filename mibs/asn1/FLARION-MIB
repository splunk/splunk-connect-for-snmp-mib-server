        SYNTAX          INTEGER (0..65535)
        MAX-ACCESS      read-write
        STATUS          current
        DESCRIPTION     "L2 Page Timeout.  Timeout in seconds for which we will
                         continue to wait for the mobile to go through access
                         or to receive an L2 Page Response for presence test."
        DEFVAL          { 20 }
        ::= { pgLpaProv 1 }

pgLpaSpoofThreshold     OBJECT-TYPE
        SYNTAX          INTEGER (0..65535)
        MAX-ACCESS      read-write
        STATUS          current
