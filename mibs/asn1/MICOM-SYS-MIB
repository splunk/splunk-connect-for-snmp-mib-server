                voice-isdn-BRI    (42),
                voice-isdn-LAPD   (43),
                voice-isdn-D-ch   (44),
                voice-isdn-B1-ch  (45),
                voice-isdn-B2-ch  (46)
		}
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = The PPA device type of this interface.;
	   HELP = ;
