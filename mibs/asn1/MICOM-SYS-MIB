-- Traps Group

mcmSysTrapSubscriptionTable OBJECT-TYPE
	SYNTAX	SEQUENCE OF McmSysTrapSubscriptionEntry
	ACCESS	not-accessible
	STATUS	mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = This Table displays the System Trap \
                  subscription information of this \
                  access device.;
	   HELP = ;
	   CAPABILITIES = NET_CFG, VPN_DISP;"
	::= {sys-trap 1}

mcmSysTrapSubscriptionEntry OBJECT-TYPE
	SYNTAX	McmSysTrapSubscriptionEntry
	ACCESS	not-accessible
	STATUS	mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = Entry that pertains to a specific TRAP.;
	   HELP = ;
	   CAPABILITIES = NET_CFG, VPN_DISP;"
	INDEX		{ mcmSysTrapSubscriptionIPAddress }
	::= {mcmSysTrapSubscriptionTable 1}

McmSysTrapSubscriptionEntry ::= SEQUENCE {
	mcmSysTrapSubscriptionIPAddress		IpAddress,
	mcmSysTrapSubscriptionCommunityString		DisplayString,
	mcmSysTrapSubscriptionAction			INTEGER
	}

