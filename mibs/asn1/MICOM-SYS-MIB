--
-- Based on General System MIB dated: 06-10-96
-- MIB Revision Dated: 06-27-96 
-- Updated after meeting with OSCAR Development Team
-- MIB Revision Dated: 09-24-96 
-- 
-- MIB Revision : 1.0 dated 1-17-97
-- Change Desc. : None. 
-- 
-- MIB Revision : 1.1 dated 2-4-97
-- Change Desc. : Swapped object in Frimware Inf. group for cosmetic reasons.
-- 
-- 
-- MIB Revision : 1.2 dated 2-19-97
-- Change Desc. : Modified system configuration and smart config. objects
-- 
-- 
-- MIB Revision : 1.3 dated 2-25-97
-- Change Desc. : Modified sytem card table, system control group 
--                and system firmware group. -A.Llanes 
-- 
-- 
-- MIB Revision : 1.4 dated 3-10-97
-- Change Desc. : Fixed McmSysCardEntry by removing "String" from the object 
--                mcmSysCardPcbRevisionString. -A.Llanes 
-- 
-- MIB Revision : 1.5 dated 3-13-97
-- Change Desc. : Range check changes.
-- 
-- 
-- MIB Revision : 1.6 dated 3-24-97
-- Change Desc. : Add mcmSysValidateCommunityGroup, modify power supply types, and 
--                add a new field for receive bandwidth utilization and renamed the 
--                existing link bandwidth utilization to link transmit bandwidth 
--                utilization. -A.Llanes
-- 
-- MIB Revision : 1.7 dated 4-16-97
-- Change Desc. : Changes trap ENTERPRISE from micom-oscar to micom-system. A. Llanes
-- 
-- 
-- MIB Revision : 1.8 dated 4-17-97
-- Change Desc. : Change the Trap Subscription Action from 
--                write-only to read-write, as per Robert Thacker's request. A. Llanes
-- 
-- 
-- MIB Revision : 1.9 dated 4-25-97
-- Change Desc. : Correct range values in the mcmSysTimeOfDayGroup group
-- 
-- *** RELEASE 1.1 ***
-- MIB Revision : 1.10 dated 4-15-97
-- Change Desc. : Support rollback functionality and 
--                modify Console Port group. A. Llanes
--
-- MIB Revision : 1.11 dated 4-30-97
-- Change Desc. : Added FR DCE related changes as per FR DCE/HTDS MIB 
--		Req't Document (dated 4/20/97) - R. Carmona.
-- 
--
-- MIB Revision : 1.12 dated 5-07-97
-- Change Desc. : Changed Community table index to an enumeration 
--                to support rel 1.1 CAPABILITIES.  - S.Gaur/Shridhar K.
--
--
-- MIB Revision : 1.13 dated 5-8-97
-- Change Desc. : Rearranged FR DCE related objects for SNMP compliance
--		for modifications from 4400 1.0 to 4400 1.1.
--		Merged Community String's Range from 0..25 to 1..25 to
--		disallow NULL Community names. - R. Carmona
--
--
-- MIB Revision : 1.14 dated 5-13-97
-- Change Desc. : Modified the access level of the object 
--                LastCommunityPriviledgeLevel to six
--                member enumerated string to reflect the 
--                CAPABILITIES functionality-Shridhar
--
-- MIB Revision : 1.15 dated 6-03-97
-- Change Desc. : Added System Rollback Traps, added and 
--                renamed enumerations for mcmSysRestartReason,
--                added mcmSysRollbackStatus, and modified
--                mcmSysRollbackConfirm, as per Mario Cedillo's
--                request. - A. LLanes  
--
-- MIB Revision : 1.16 dated 6-26-97
-- Change Desc. : Added two extra objects to the end of the
--                system card table to support Nortel nomenclature
--                for the 4400 H/W. -A.Llanes
-- 
-- MIB Revision : 1.17 dated 6-27-97
-- Change Desc. : Added Trap Descriptions. -A.Llanes 
--
-- *** RELEASE 2.0 ***
-- MIB Revision : 2.0 dated 7-24-97
-- Change Desc. : Added enumerations for SysIfExtType objects
--		  for ISDN functionality as per 7/22/97 meeting
--		  with RC, Bhushan, Klaus S., Bernd S., Amparo,
--		  MG, DS, and TM.  -R. Carmona 
--
-- MIB Revision : 2.1 dated 8-26-97
-- Change Desc. : Modified mcmSysRestartReason object based on Brian P.'s request.
--                And, Introduction of DCE-Conn device type. - B. Jafari
--
-- MIB Revision : 2.2 dated 9-15-97
-- Change Desc. : Minor modification to enumerations requested by Brian P. (duplicates)
--                and other fixes to fix compilation problems       -S. Gaur
--
-- MIB Revision : 2.3 dated 10-8-97
-- Change Desc. : Replaced MPA with 4400, and other minor changes. - B. Jafari. 
--
-- MIB Revision : 2.4 dated 10-17-97
-- Change Desc. : Added isdn-LAPD and isdn-BRI device types. - B. Jafari. 
--
-- MIB Revision : 2.5 dated 10-28-97
-- Change Desc. : Introduced 'lim' (logical interface module) to reflect 
--                a card's former 'slot' notation; e.g. 'slotB' is 
--                now regarded as 'limB', and added four 'lim' types
--                for future expansion. - B. Jafari.
--
-- MIB Revision : 2.6 dated 10-31-97
-- Change Desc. : Modified CAPABILITIES for objects inside mcmSysCommitTrackingGroup. - B. Jafari. 
--
--
-- MIB Revision : 2.7 dated 11-06-97
-- Change Desc. : Changed all object names and descriptions that had "slot" to module. -S. Gaur 
--
--
-- MIB Revision : 2.8 dated 1-05-98
-- Change Desc. : Modified the System card table name to System LIM table.
--                Reorganized the (mcm/nvm)SysLimModuleAddress and (mcm/nvm)SysIfExtModule
--                enumerations to enforce better guidelines for LIM ordering on 
--                the Passport 4400.
--                Added the mcmSysPhysTable to map a particular physical slot
--                to the corresponding Logical Interface Module (LIM).
--                Changed all object names and descriptions that had card to LIM.
--                                                                                    - S. Gaur
--
-- MIB Revision : 2.9 dated 3-06-98
-- Change Desc. : Changed SYNTAX field for mcmSysCfgDRAMSize and mcmSysCfgFlashMemorySize 
--                objects.  The format was changed from bytes to megabytes.           - S. Gaur 
--
--*** RELEASE 2.1 ***
-- MIB Revision : 3.0 dated 4-09-98
-- Change Desc. : Added enumerations for (mcm/nvm)SysIfExtPPADeviceType for release
--                2.1 ISDN BRI voice card functionality.
--         
--
-- *** RELEASE 3.0 *** 
-- MIB Revision : 3.1 dated 6-25-98
-- Change Desc. : Deprecated the mcmSysHWTypeModelType object, replacing 
--                its functionality with the mcmSysHWTypeCPUType object.
--                Added the mcmSysHWTypeGenCfgType and  
--                mcmSysTimeOfLastReset objects. 
--                Added enumeration thirty seven to the
--                (mcm/nvm)SysIfExtPPADeviceType.        
--                Replaced all instances of the "4400" string with the 
--                generic string "access device".
--                                               -S. Gaur
--
-- MIB Revision : 3.2 dated 8-17-98
-- Change Desc. : Added enumeration 38 to 40 (mcm/nvm)SysIfExtPPADeviceType. 
--                Please note this enmumeration was wrongly defined for 
--                mcmIfextType in the previous revision. 
--                Changes per K. Seggelke.
--                                                              - S. Gaur
--
-- MIB Revision : 3.3 dated 8-24-98
-- Change Desc. : Changed enumeration 1 from "4300" to "reserved" for 
--                mcmSysHWTypeGenCfgType.
--                                                              - S. Gaur
-- MIB Revision : 3.2 dated 8-28-98
-- Change Desc. : Merged From BRI branch to 3.1 branch.
--                Moved down ISDN-Voice enumerations for 
--                (mcm/nvm)SysIfExtPPADeviceType.
--                                                              - S. Gaur
-- MIB Revision : 3.4 dated 11-02-98
-- Change Desc. : Severity Levels were added to Traps.
--						-F. Kottab
--
MICOM-SYS-MIB DEFINITIONS ::= BEGIN
	IMPORTS
                TimeTicks
                                FROM RFC1155-SMI 
		Counter, Gauge, IpAddress 
                   		FROM RFC1155-SMI
		OBJECT-TYPE
                    		FROM RFC-1212
		TRAP-TYPE
                		FROM RFC-1215
	    	micom-oscar 
		    		FROM MICOM-OSCAR-MIB
		DisplayString 
				FROM SNMPv2-TC;


-- group OBJECT IDENTIFIERs



	micom-system			OBJECT IDENTIFIER ::= {micom-oscar 1}

	sys-configuration		OBJECT IDENTIFIER ::= {micom-system 1}
	sys-control			OBJECT IDENTIFIER ::= {micom-system 2}
	sys-statistics			OBJECT IDENTIFIER ::= {micom-system 3}
	sys-trap			OBJECT IDENTIFIER ::= {micom-system 4}


mcmSysHWTypeGroup    OBJECT IDENTIFIER ::= {sys-configuration 1}

mcmSysHWTypeModelType OBJECT-TYPE
	SYNTAX	DisplayString (SIZE (50))
	ACCESS	read-only
	STATUS	deprecated
	DESCRIPTION
	  "NAME = ;
	   DESC = **NOTE: THIS OBJECT HAS BEEN  \
                  DEPRECATED. Its functionality \
                  has been replaced by the      \
                  mcmSysHWTypeCPUType object    \
                  located below in this group.**\
                                                \  
                  The model name/type of the \
                  access device.;
	   HELP = ;
	   CAPABILITIES = NET_CFG, VPN_DISP;"
	 ::= { mcmSysHWTypeGroup 1}

mcmSysHWTypeRevisionLevel OBJECT-TYPE
	SYNTAX	INTEGER
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = The revision level of the \
                  access device.;
	   HELP = ;
	   CAPABILITIES = NET_CFG, VPN_DISP;"
	 ::= { mcmSysHWTypeGroup 2}

mcmSysHWTypeManufRevDate OBJECT-TYPE
	SYNTAX	DisplayString (SIZE (25))
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = The manufacture/revision date \
                  of the access device .;
	   HELP = ;
	   CAPABILITIES = NET_CFG, VPN_DISP;"
	 ::= { mcmSysHWTypeGroup 3}

mcmSysHWTypeSerialNumber OBJECT-TYPE
	SYNTAX	DisplayString (SIZE (9))
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = The serial number of \
                  the access device..;
	   HELP = ;
	   CAPABILITIES = NET_CFG, VPN_DISP;"
	 ::= { mcmSysHWTypeGroup 4}

mcmSysHWTypeModuleID OBJECT-TYPE
	SYNTAX	INTEGER
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = The module ID of the access \
                  device.;
	   HELP = ;
	   CAPABILITIES = NET_CFG, VPN_DISP;"
	 ::= { mcmSysHWTypeGroup 5}

mcmSysHWTypeMACAddress OBJECT-TYPE
	SYNTAX	OCTET STRING (SIZE(6))
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = The base MAC address assigned \
                  to the access device.;
	   HELP = ;
	   CAPABILITIES = NET_CFG, VPN_DISP;"
	 ::= { mcmSysHWTypeGroup 6}

mcmSysHWTypeCPUType OBJECT-TYPE
	SYNTAX	DisplayString (SIZE (50))
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = Indicates the CPU type for this \
                  access device;
	   HELP = ;
	   CAPABILITIES = NET_CFG, VPN_DISP;"
	 ::= { mcmSysHWTypeGroup 7}

mcmSysHWTypeGenCfgType OBJECT-TYPE
	SYNTAX	INTEGER {
                reserved     (1),
                passport4400 (2)
                }
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = Indicates the model type for this \
                  access device.;
	   HELP = ;
	   CAPABILITIES = NET_CFG, VPN_DISP;"
	 ::= { mcmSysHWTypeGroup 8}

mcmSysCfgGroup    OBJECT IDENTIFIER ::= {sys-configuration 2}

mcmSysCfgCPUConfiguration OBJECT-TYPE
	SYNTAX	INTEGER {
		base (1),
		companion040 (2),
		companion060 (3)
		}
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = The CPU configuration for this
                  access device.;
	   HELP = ;
	   CAPABILITIES = NET_CFG, VPN_DISP;"
	 ::= { mcmSysCfgGroup 1}

mcmSysCfgPrimaryWANPort OBJECT-TYPE
	SYNTAX	INTEGER {
		present (1),
		notPresent (2)
		}
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = Indicates the presence of the \
                  Primary WAN port.;
	   HELP = ;
	   CAPABILITIES = NET_CFG, VPN_DISP;"
	 ::= { mcmSysCfgGroup 2}

mcmSysCfgPrimaryWANPortPhyMedia OBJECT-TYPE
	SYNTAX	INTEGER {
		none (1),
		unknown (2),
		link56k (3),
		generalserial (4),
		t1e1 (5),
		isdn (6)
		}
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = Indicates the physical media 
                  of Primary WAN port.;
	   HELP = ;
	   CAPABILITIES = NET_CFG, VPN_DISP;"
	 ::= { mcmSysCfgGroup 3}

mcmSysCfgDCESerialPortPhyMedia OBJECT-TYPE
	SYNTAX	INTEGER {
		none (1),
		unknown (2),
		rs232(3),
		v35 (4),
		v36 (5),
		x21 (6)
		}
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = Indicates the detected DCE \
                  serial port on the access \ 
                  device.;
	   HELP = ;
	   CAPABILITIES = NET_CFG, VPN_DISP;"
	 ::= { mcmSysCfgGroup 4}

mcmSysCfgQUICCExpansionModule1 OBJECT-TYPE
	SYNTAX	INTEGER {
		present (1),
		notPresent (2)
		}
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = QUICC Expansion module detected.;
	   HELP = ;
	   CAPABILITIES = NET_CFG, VPN_DISP;"
	 ::= { mcmSysCfgGroup 5}

mcmSysCfgQUICCExpansionModule2 OBJECT-TYPE
	SYNTAX	INTEGER {
		present (1),
		notPresent (2)
		}
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = QUICC Expansion module-2 is detected.;
	   HELP = ;
	   CAPABILITIES = NET_CFG, VPN_DISP;"
	 ::= { mcmSysCfgGroup 6}

mcmSysCfgFeatureRTCBatteryStatus OBJECT-TYPE
	SYNTAX	INTEGER {
		noBattery (1),
		batteryLow (2),
		batteryOK (3)
		}
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = TimeOfDay/SRAM module detected.;
	   HELP = ;
	   CAPABILITIES = NET_CFG, VPN_DISP;"
	 ::= { mcmSysCfgGroup 7}

mcmSysCfgDRAMSize OBJECT-TYPE
	SYNTAX	INTEGER {
                size4Mbytes  (1),
                size8Mbytes  (2),
                size16Mbytes (3),
                size32Mbytes (4),
                size64Mbytes (5)
                }
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = Total installed DRAM memory size in megabytes.;
	   HELP = ;
	   CAPABILITIES = NET_CFG, VPN_DISP;"
	 ::= { mcmSysCfgGroup 8}

mcmSysCfgFlashMemorySize OBJECT-TYPE
	SYNTAX	INTEGER {
                size4Mbytes  (1),
                size8Mbytes  (2),
                size16Mbytes (3),
                size32Mbytes (4),
                size64Mbytes (5)
                }
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = Total installed Flash memory size in megabytes.;
	   HELP = ;
	   CAPABILITIES = NET_CFG, VPN_DISP;"
	 ::= { mcmSysCfgGroup 9}


-- LIM Table

mcmSysLimTable OBJECT-TYPE
	SYNTAX	SEQUENCE OF McmSysLimEntry
	ACCESS	not-accessible
	STATUS	mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = Table to view an individual Logical \
                  Interface Modules(LIM) on the access \
                  device.;
	   HELP = Table to view the hardware \
	   CAPABILITIES = NET_CFG, VPN_DISP;"
	::= {sys-configuration 3}

mcmSysLimEntry OBJECT-TYPE
	SYNTAX	McmSysLimEntry
	ACCESS	not-accessible
	STATUS	mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = Entry pertaining to a specific logical module.;
	   HELP = This includes the Name, Part number, and rev. level.;
	   CAPABILITIES = ;"
	INDEX		{mcmSysLimModuleAddress}
	::= {mcmSysLimTable 1}

McmSysLimEntry ::= SEQUENCE {
	mcmSysLimModuleAddress		INTEGER,
	mcmSysLimModuleName		DisplayString,
	mcmSysLimPcbPartNumber	        DisplayString,
        mcmSysLimPcbRelease             DisplayString,
	mcmSysLimSerialNumber		DisplayString,
	mcmSysLimMfgDate		DisplayString
	}

mcmSysLimModuleAddress OBJECT-TYPE
	SYNTAX	INTEGER {
		limA (1),
		lim1 (2),
		lim2 (3),
		lim3 (4),
		lim4 (5),
		limB (6),
		limC (7),
		limD (8),
		limE (9)
		}
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = lim (logical interface module) identifies \
                  the address of the module within the unit;
	   HELP = ;
	   CAPABILITIES = ;"
	::= {mcmSysLimEntry 1}

mcmSysLimModuleName OBJECT-TYPE
	SYNTAX  DisplayString (SIZE(0..20))	
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = Displays the name of the module \
		  in the specific location;
                  NOTE : this object was previously named \
                  mcmSysCardModuleName.;
	   HELP = ;
           CAPABILITIES = ;"
	::= {mcmSysLimEntry 2}

mcmSysLimPcbPartNumber OBJECT-TYPE
	SYNTAX  DisplayString (SIZE(0..16))	
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = Displays the part number of the module in \
                  the specific location;
	   HELP = ;
           CAPABILITIES = ;"
	::= {mcmSysLimEntry 3}

mcmSysLimPcbRelease OBJECT-TYPE
	SYNTAX  DisplayString (SIZE(0..4))	
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = Displays the release level of the module in \
                  the specific location.;
	   HELP = ;
           CAPABILITIES = ;"
	::= {mcmSysLimEntry 4}

mcmSysLimSerialNumber OBJECT-TYPE
	SYNTAX  DisplayString (SIZE(0..13))	
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = Displays the serial number of the module in \
                  the specific location.;
	   HELP = ;
           CAPABILITIES = ;"
	::= {mcmSysLimEntry 5}

mcmSysLimMfgDate OBJECT-TYPE
	SYNTAX  DisplayString (SIZE(0..7))	
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = Displays the manufacturing date of the module in \
                  the specific location.;
	   HELP = ;

           CAPABILITIES = ;"
	::= {mcmSysLimEntry 6}

-- PHYS Table

mcmSysPhysTable OBJECT-TYPE
	SYNTAX	SEQUENCE OF McmSysPhysEntry
	ACCESS	not-accessible
	STATUS	mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = Table to map physical cards to   \
                  Logical Interface Modules (LIM) on \
                  the access device
	   HELP =  ;
	   CAPABILITIES = NET_CFG, VPN_DISP;"
	::= {sys-configuration 16}

mcmSysPhysEntry OBJECT-TYPE
	SYNTAX	McmSysPhysEntry
	ACCESS	not-accessible
	STATUS	mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = Entry pertaining to a specific physical module.;
	   HELP = ;
	   CAPABILITIES = ;"
	INDEX		{mcmSysPhysModuleAddress}
	::= {mcmSysPhysTable 1}

McmSysPhysEntry ::= SEQUENCE {
	mcmSysPhysModuleAddress		INTEGER,
	mcmSysLogModuleAddress		INTEGER
	}

mcmSysPhysModuleAddress OBJECT-TYPE
	SYNTAX	INTEGER (1..5)
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = The value of this object identifies \
                  the physical slot address of the module \
                  within the unit;
	   HELP = ;
	   CAPABILITIES = ;"
	::= {mcmSysPhysEntry 1}

mcmSysLogModuleAddress OBJECT-TYPE
        SYNTAX INTEGER {
		limA (1),
		lim1 (2),
		lim2 (3),
		lim3 (4),
		lim4 (5),
		limB (6),
		limC (7),
		limD (8),
		limE (9),
		none (10)
                }
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = The value of this object identifies \
                  the logical interface module (lim) address;
	   HELP = ;
           CAPABILITIES = ;"
	::= {mcmSysPhysEntry 2}


-- Chassis Group

mcmSysChassisGroup    OBJECT IDENTIFIER ::= {sys-configuration 4}

mcmSysChassisNumberOfModules OBJECT-TYPE
	SYNTAX	INTEGER {
		moduleThree (1),
		moduleFive (2)
		}
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = Chassis style: number of modules.;
                  NOTE : this object was previously named \
                  mcmSysChassisNumberOfSlots.;
	   HELP = ;
	   CAPABILITIES = NET_CFG, VPN_DISP;"
	::= { mcmSysChassisGroup 1}

mcmSysChassisCoolingFan OBJECT-TYPE
	SYNTAX	INTEGER {
		running (1),
		notRunning (2)
		}
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC =Chassis cooling fan status.;
	   HELP = ;
	   CAPABILITIES = NET_CFG, VPN_DISP;"
	::= { mcmSysChassisGroup 2}

mcmSysChassisPowerSupplyType OBJECT-TYPE
	SYNTAX	INTEGER {
		type-A (1),
		type-B (2)
		}
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = Specifies the type of power supply \
		  based on type of voltage;
	   HELP = ;
           CAPABILITIES = NET_CFG, VPN_DISP;"
	::= { mcmSysChassisGroup 3}

mcmSysChassisPowerSupplyStatus OBJECT-TYPE
	SYNTAX	INTEGER {
		normal (1),
		fault (2)
		}
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = Fault indicates that at least one \
		  power supply module is down;
	   HELP = ;
           CAPABILITIES = NET_CFG, VPN_DISP;"
	::= { mcmSysChassisGroup 4}

mcmSysChassisPowerSupplyModuleCount OBJECT-TYPE
	SYNTAX	INTEGER (1..5)
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = Specifies the number of power \
		  supply modules installed;
	   HELP = ;
           CAPABILITIES = NET_CFG, VPN_DISP;"
	::= { mcmSysChassisGroup 5}

mcmSysChassisPowerSupplyRedundant OBJECT-TYPE
	SYNTAX	INTEGER {
		yes (1),
		no (2)
		}
	ACCESS	read-only
	STATUS	mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = Indicates whether there are \
		  redundant power supply modules;
	   HELP = ;
           CAPABILITIES = NET_CFG, VPN_DISP;"
	::= { mcmSysChassisGroup 6}

-- Time of Day Group

mcmSysTimeOfDayGroup    OBJECT IDENTIFIER ::= {sys-configuration 5}

mcmSysTimeOfDay		OBJECT IDENTIFIER ::= {mcmSysTimeOfDayGroup 1}

mcmSysTimeOfDaySec OBJECT-TYPE
	SYNTAX	INTEGER (0..59)
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = System Time of day in seconds.;
	   HELP = ;
	   CAPABILITIES = NET_CFG, VPN_DISP;"
	 ::= { mcmSysTimeOfDay 1}

mcmSysTimeOfDayMin OBJECT-TYPE
	SYNTAX	INTEGER (0..59)
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = System Time of day in minutes.;
	   HELP = ;
	   CAPABILITIES = NET_CFG, VPN_DISP;"
	 ::= { mcmSysTimeOfDay 2}

mcmSysTimeOfDayHour OBJECT-TYPE
	SYNTAX	INTEGER (0..23)
	ACCESS	read-write
	STATUS	mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = System Time of day in hours.;
	   HELP = ;
	   CAPABILITIES = NET_CFG, VPN_DISP;"
	 ::= { mcmSysTimeOfDay 3}

mcmSysTimeOfDayDay OBJECT-TYPE
	SYNTAX	INTEGER {
		sunday (1),
		monday (2),
		tuesday (3),
		wednesday (4),
		thursday (5),
		friday (6),
		saturday (7)
	}
	ACCESS	read-write
