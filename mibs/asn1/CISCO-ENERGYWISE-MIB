ciscoEnergywiseEntityGroup OBJECT-GROUP
    OBJECTS         {
                        cewMaxImportance,
                        cewMaxImportanceId,
                        cewEntNeighborIndex,
                        cewEntNannyVector,
                        cewEntKeyword,
                        cewEntName,
                        cewEntRoleDescription,
                        cewEntFullName,
                        cewEntEnergyUnits,
                        cewEntEnergyUsage,
                        cewEntEnergyLevel,
                        cewEntEnergyUsageProvisioned,
                        cewEntImportanceInt,
                        cewEntImportanceExt,
                        cewEntImportanceParentId,
                        cewEntParentId,
                        cewEntImportanceRelative,
                        cewEntAdminStatus,
                        cewEntOperStatus,
                        cewLevelMaxUsage,
                        cewLevelDeltaUsage,
                        cewLevelUnits,
                        cewDeviceId,
                        cewDeviceNeighborCount,
                        cewEntEnergyUsageCaliber,
                        cewDomainName
                    }
    STATUS          current
    DESCRIPTION
        "This group contains the collection of all the objects
        related to a Cisco EnergyWise entity."
    ::= { ciscoEnergywiseMIBGroups 1 }

