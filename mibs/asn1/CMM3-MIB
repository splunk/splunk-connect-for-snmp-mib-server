        ::= {cmmConfig 22 }
        
         port6Config		OBJECT-TYPE
        	SYNTAX           INTEGER {
        			auto(1),
        			hundredFDX(2),
        			hundredHDX(3),
        			tenFDX(4),
        			tenHDX(5)
        			}
                MAX-ACCESS      read-write
                STATUS          current
                DESCRIPTION
                        "Link speed configuration on port 6."
                         
        ::= {cmmConfig 23 }
        
         port7Config		OBJECT-TYPE
        	SYNTAX           INTEGER {
        			auto(1),
        			hundredFDX(2),
        			hundredHDX(3),
        			tenFDX(4),
        			tenHDX(5)
